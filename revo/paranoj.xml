<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: paranoj.xml,v 1.20 2023/10/20 14:25:19 revo Exp $">
<kap>
  <rad>paranoj</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="paranoj.0o">
  <kap><tld/>o</kap>
  <snc mrk="paranoj.0o.PSI">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="lst" cel="psikoz.0o" lst="voko:malsanoj">Psikozo</ref>,
      karakteriza per sistemigita
      <ref tip="vid" cel="delir.0o">deliro</ref>, sufi&ccirc;e kohera,
      kiu tamen povas koncerni nur limigitan temon, lasante normala
      la menskapablon por la ordinaraj vivbezonoj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_n;&c_o;&c_ja;</trd>
  <trd lng="bg">&c_p;&c_a;&c_r;&c_a;&c_n;&c_o;&c_ja;</trd>
  <trd lng="ca">paranoia</trd>
  <trd lng="cs">paranoia</trd>
  <trd lng="de">Paranoia</trd>
  <trd lng="en">paranoia</trd>
  <trd lng="es">paranoia</trd>
  <trd lng="fr">parano&iuml;a</trd>
  <trdgrp lng="hu">
    <trd>t&eacute;boly</trd>,
    <trd>paranoia</trd>
  </trdgrp>
  <trd lng="id">paranoia</trd>
  <trdgrp lng="ja">
    <trd>&#x504F;&#x57F7;&#x75C5;  <pr>&#12408;&#12435;&#12375;&#12421;&#12358;&#12403;&#12423;&#12358;</pr></trd>,
    <trd>&#22916;&#x60F3;&#x75C7;  <pr>&#12418;&#12358;&#x305D;&#12358;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#x30D1;&#x30E9;&#x30CE;&#x30A4;&#x30A2;</trd>
  </trdgrp>
  <trd lng="nl">paranoia</trd>
  <trd lng="pl">paranoja</trd>
  <trd lng="pt">par&acirc;metro</trd>
  <trd lng="ro">paranoia</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_n;&c_o;&c_j;&c_ja;</trd>
  <trd lng="sk">paranoja</trd>
  <trd lng="uk">&#x43F;&c_a;&c_r;&c_a;&#x43D;&#x43E;&#x44F;</trd>
</drv>

<drv mrk="paranoj.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo havanta <tld/>on:
      <ekz>
        la <tld/>uloj misinterpretas &ccirc;iutagajn okaza&jcirc;ojn en la senco
        de sia deliro, &ccirc;u pri persekuti&gcirc;o, pri megalomanio, pri
        mistikeco a&ubreve; pri hipokondrio.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_n;&c_o;&c_ib;&c_k;</trd>
  <trd lng="ca">paranoic</trd>
  <trd lng="cs">paranoik</trd>
  <trd lng="es">paranoico</trd>
  <trd lng="fr">parano&iuml;aque<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>paranoi&aacute;s</trd>,
    <trd>t&eacute;bolyult</trd>
  </trdgrp>
  <trd lng="ja">&#x504F;&#x57F7;&#x75C5;&#32773;  <pr>&#12408;&#12435;&#12375;&#12421;&#12358;&#12403;&#12423;&#12358;&#12375;&#12419;</pr></trd>
  <trd lng="pl">paranoik</trd>
  <trd lng="ro">paranoic</trd>
  <trd lng="sk">paranoik</trd>
  <trd lng="uk">&#x43F;&c_a;&c_r;&c_a;&#x43D;&#x43E;&c_ji;&#x43A;</trd>
</drv>
</art>
