<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>paranoj</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="paranoj.0o">
  <kap><tld/>o</kap>
  <snc mrk="paranoj.0o.PSI">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="super" cel="psikoz.0o">Psikozo</ref>,
      karakteriza per sistemigita
      <ref tip="vid" cel="delir.0o">deliro</ref>, sufi&ccirc;e kohera,
      kiu tamen povas koncerni nur limigitan temon, lasante normala
      la menskapablon por la ordinaraj vivbezonoj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_n;&c_o;&c_ja;</trd>
  <trd lng="ca">paranoia</trd>
  <trd lng="en">paranoia</trd>
  <trd lng="es">paranoia</trd>
  <trd lng="fr">parano&iuml;a</trd>
  <trdgrp lng="hu">
    <trd>t&eacute;boly</trd>,
    <trd>paranoia</trd>
  </trdgrp>
  <trd lng="nl">paranoia</trd>
  <trd lng="pt">par&acirc;metro</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_n;&c_o;&c_j;&c_ja;</trd>
</drv>

<drv mrk="paranoj.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo havanta <tld/>on:
      <ekz>
        la <tld/>uloj misinterpretas &ccirc;iutagajn okaza&jcirc;ojn en la senco
        de sia deliro, &ccirc;u pri persekuti&gcirc;o, pri megalomanio, pri
        mistikeco a&ubreve; pri hipokondrio.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">paranoic</trd>
  <trd lng="es">paranoico</trd>
  <trd lng="fr">parano&iuml;aque<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>paranoi&aacute;s</trd>,
    <trd>t&eacute;bolyult</trd>
  </trdgrp>
</drv>
</art>
