<drv mrk="bril.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bril.0i.lumo">
    <dif>
      Eligi a&ubreve; resendi helan lumon:
      <ekz>
        la luno <tld/>is tra la fenestro sur la mezon de la planko
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        frue matene <klr>[&#x2026;]</klr> la suno ek<tld/>is super la akvo
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 3:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo <klr>[&#x2026;]</klr> <tld/>is kiel oro
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        altaj bluaj montoj, sur kies suproj <tld/>is la blanka ne&gcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ek<tld/>is fulmoj unu post alia
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj <tld/>is <klr>[&#x2026;]</klr>, li havis belegajn longajn harojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plandoj de iliaj piedoj <klr>[&#x2026;]</klr> <tld/>is kiel hela pura kupro
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 1:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        floroj <klr>[&#x2026;]</klr>, ru&gcirc;aj kaj bluaj, <tld/>is &ccirc;e la muroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la plafono <tld/>is lampiroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> <tld/>is kiel diamantoj
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>ne &ccirc;io <tld/>anta estas diamanto</ind>
        <trd lng="fr">tout ce qui brille n'est pas or</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne &ccirc;io utilas, kio brilas<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="blink.0i">blinki</ref>,
      <ref cel="fajr.0i">fajri</ref>,
      <ref cel="flagr.0i">flagri</ref>,
      <ref cel="flam.0i">flami</ref>,
      <ref cel="fulm.0i">fulmi</ref>,
      <ref cel="glim">glimi</ref>,
      <ref cel="hel">heli</ref>,
      <ref cel="lum.0i">lumi</ref>,
      <ref cel="radi.0i">radii</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>scheinen</trd>,
      <trd>glitzern</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>b&lstroke;yszcze&cacute;</trd>,
      <trd>l&sacute;ni&cacute;</trd>,
      <trd>ja&sacute;nie&cacute;</trd>
    </trdgrp>
    <trd lng="pt">brilhar</trd>
  </snc>
  <snc mrk="bril.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Altiri la atenton per sia prospereco, eleganteco, lukso, spriteco:
      <ekz>
        la sukceso estis <tld/>anta, preska&ubreve; &ccirc;iuj pagis
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la ruinigita nobelo <klr>[&#x2026;]</klr> revis pli <tld/>antan sorton
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flor.0i.prosperi">flori<sncref/></ref>,
      <ref cel="juvel.0o.FIG">juvelo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ib;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_z;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_ib;&c_h;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_ib;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_ib;&c_s;&c_k;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_b;&c_l;&c_je;&c_s;&c_t;&c_ja;</trd>,
    <trd>&c_l;&c_malmol;&c_shch;&c_ja;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_ja;</trd>,
    <trd>&c_s;&c_i;&c_ja;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bl&yacute;skat se</trd>,
    <trd>lesknout se</trd>,
    <trd>skv&ecaron;t se</trd>,
    <trd>skv&ecaron;t se</trd>,
    <trd>t&rcaron;pytit se</trd>,
    <trd>z&aacute;&rcaron;it</trd>,
    <trd>z&aacute;&rcaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gl&auml;nzen</trd>,
  </trdgrp>
  <trd lng="en">shine</trd>
  <trd lng="es">brillar</trd>
  <trdgrp lng="fr">
    <trd>briller</trd>,
    <trd>luire</trd>
  </trdgrp>
  <trd lng="he">&lamed;&zayin;&he;&vav;&resh;</trd>
  <trd lng="hu">ragyog</trd>
  <trdgrp lng="ja">
    <trd>&#x8F1D;&#x304F;  [<ind>&#x304B;&#x304C;&#x3084;&#x304F;</ind>]</trd>,
    <trd>&#x5149;&#x308B;  [<ind>&#x3072;&#x304B;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="ne">&#x918;&#x93E;&#x92E; &#x932;&#x93E;&#x917;&#x94B;&#x938;&#x94D;</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_je;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_i;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_i;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&zcaron;iari&tcaron;</trd>,
    <trd>ligota&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_l;&c_i;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_l;&c_i;&c_s;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_b;&c_l;&c_i;&c_s;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ja;&c_r;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_ja;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_v;&c_ib;&c_t;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_o;&c_r;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_ib;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="bril.0o">
  <kap><tld/>o</kap>
  <snc mrk="bril.0o.FIZ">
    <dif>
      Hellumo sendata de io:
      <ekz>
        la <tld/>o de la luno
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 60:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidis kvaza&ubreve; helegan <tld/>on, kvaza&ubreve; fajron interne kaj &ccirc;irka&ubreve;e
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 1:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lampo lumas al vi per sia <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Luko 11:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tutan tagon ni havis sun<tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        rigardu &ccirc;i tie la <tld/>on de la &ccirc;ielarko
        <fnt>
          <bib>Fab3</bib>
          <lok>Lasta perlo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno <klr>[&#x2026;]</klr> mallevi&gcirc;is kaj la roza <tld/>o estingi&gcirc;is sur la maro
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la norda <tld/>o <klr>(vd <ref cel="lum.nord0o">nordlumo</ref>)</klr>
        flamis super la glacimontoj de la nordo
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni eniris en pre&gcirc;ejon, kiu kun siaj pentra&jcirc;oj sur ora grundo radiis en plena
        <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la kandeloj ili lumis kiel folioj de tulipoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kia <tld/>o kaj kia beleco estis en la salonoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>!
      </ekz>
      <ekz>
        juna knabino kun lumantaj okuloj, kun la <tld/>o de saneco sur la vangoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="radi.0o.energielsendo">radio<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="af">skyn</trd>
  <trd lng="am">&#x12A0;&#x1265;&#x122B;</trd>
  <trd lng="ar">&a_t;&a_A_hamza_sure;&a_l;&a_q;</trd>
