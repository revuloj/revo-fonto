<drv mrk="bril.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="B"/>anta:
      <ekz>
        jen apud ili staris du viroj en <tld/>aj vestoj
        <fnt>
          <bib>NT</bib>
          <lok>Luko 24:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniam vidi la <tld/>an sunon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la adia&ubreve;o <klr>[li]</klr> donis al ili <tld/>an tran&ccirc;ilon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis forportonta la <tld/>an akvon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fidela Johano</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_l;&c_ib;&c_s;&c_k;&c_u;&c_ch;&c_y;</trd>,
      <trd>&c_b;&c_l;&c_ib;&c_sh;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>,
      <trd>&c_z;&c_ib;&c_h;&c_o;&c_t;&c_k;&c_ib;</trd>,
      <trd>&c_z;&c_ib;&c_h;&c_a;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="de">gl&auml;nzend</trd>
    <trd lng="es">brillante</trd>
    <trd lng="fr">brillant <klr tip="ind">(adj.)</klr></trd>
    <trd lng="hu">ragyog&oacute;</trd>
    <trdgrp lng="pl">
      <trd>b&lstroke;yszcz&aogonek;cy</trd>,
      <trd>l&sacute;ni&aogonek;cy</trd>,
      <trd>ja&sacute;niej&aogonek;cy</trd>
    </trdgrp>
    <trd lng="ro">str&abreve;lucitor</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_l;&c_je;&c_s;&c_t;&c_ja;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_i;&c_s;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_i;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="bril.0a.elstara">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="star.el0a.eminenta">Elstara</ref> pro prospereco, eleganteco, lukso, spriteco:
      <ekz>
        mi trovi&gcirc;as en la plej <tld/>a situacio
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi sen beda&ubreve;ro forlasos <klr>[&#8230;]</klr> la <tld/>an estontecon, kiu vin atendas
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>?
      </ekz>
      <ekz>
        tiu &ccirc;i infana gloramo, tiu &ccirc;i nevenkebla obstineco kaj &ccirc;iuj &ccirc;i tiuj belaj
        <tld/>aj virtoj, kiuj &gcirc;ermis en la pa&ccirc;ja fileto, faros el li iam
        <klr>[&#8230;]</klr> grandan homon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li imagis la <tld/>an gloron de liaj faroj
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la estonto promesis nur <tld/>ajn rezultatojn
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis en <tld/>a humoro, kiel &ccirc;iam, kiam li povis labori la&ubreve; propra deziro
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> provis trovi pli <tld/>ajn solvojn de la problemaj ol tiujn de
        la verkinto
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ivagin</nom> fari&gcirc;is subite la plej <tld/>a oratoro
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        altekulturita, <tld/>a homo, favorato kaj intimulo de l&#8217; &ccirc;efepiskopo
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eminen1.0a">eminenta</ref>,
      <ref cel="presti1.0a">presti&gcirc;a</ref>,
      <ref cel="renom.0a">renoma</ref>,
      <ref cel="sprit.0a">sprita</ref>
    </refgrp>
    <trd lng="de">brillant</trd>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_b;&c_l;&c_je;&c_s;&c_t;&c_ja;&c_shch;</trd>,
    <trd>&c_l;&c_malmol;&c_s;&c_k;&c_a;&c_v;</trd>,
