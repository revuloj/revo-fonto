  <trd lng="en">reflect</trd>
  <trdgrp lng="fr">
    <trd>se <ind>refl&eacute;ter</ind></trd>,
    <trd>se <ind>r&eacute;fl&eacute;chir</ind> <klr tip="ind">
    (se refl&eacute;ter)</klr></trd>,
    <trd>miroiter</trd>
  </trdgrp>
  <trd lng="hu">visszat&uuml;kr&ouml;z&odblac;dik</trd>
  <trdgrp lng="ja">
    <trd>&#21453;&#23556;&#12377;&#12427;  <pr>&#12399;&#12435;&#12375;&#12419;&#12377;&#12427;</pr></trd>,
    <trd>&#26144;&#12360;&#12427;  <pr>&#12399;&#12360;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odb&lstroke;yskiwa&cacute;</trd>,
    <trd>puszcza&cacute; zaj&aogonek;czki<klr> (lusterkiem)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; <ind>
  &c_o;&c_t;&c_b;&c_l;&c_je;&c_s;&c_k;</ind></trd>
  <trd lng="sk">odr&aacute;&zcaron;a&tcaron; sa</trd>
  <trd lng="uk">&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_v;&c_ib;&c_d;&c_b;&c_l;&c_i;&c_s;&c_k;</trd>
</drv>

<drv mrk="bril.trem0i">
  <kap>trem<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bril.trem0i.FIZ">
    <dif>
      Intermite &jcirc;eti lumbriletojn:
      <ekz>
        la sunlumo trem<tld/>is en la <klr>[&#8230;]</klr> kupoloj de la
        pre&gcirc;ejoj
        <fnt><bib>Voja&gcirc;Imp</bib>,
          <lok>&ccirc;e Lev Tolstoj en Jasnaja
          Poljana</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> direktis la okulojn al la malhela fenestro, sur kiu trem<tld/>is
        respeguli&gcirc;o de lia kandelo
        <fnt>
          <bib>LOdE</bib>
          <aut>Anton &Ccirc;e&hcirc;ov, trad. Valentin Melnikov</aut>
          <vrk>Iva&ccirc;jo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ambron de la malsanulo inundadis timiga trem<tld/>a lumo
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        senfina trem<tld/>a strio de &scirc;tofo pufi&gcirc;is kiel ondo
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="flagr.0i">flagri</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_g;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ib;&c_g;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_l;&c_je;&c_shch;&c_u;&c_k;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>blikat</trd>,
    <trd>mrkat</trd>,
    <trd>m&zcaron;ikat</trd>,
    <trd>t&rcaron;pytit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>flimmern</trd>,
    <trd>glitzern</trd>,
    <trd>flackern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sparkle</trd>,
    <trd>glitter</trd>
  </trdgrp>
  <trd lng="es">parpadear<klr> (una luz)</klr></trd>
  <trd lng="fr">scintiller</trd>
  <trdgrp lng="hu">
    <trd>villog</trd>,
    <trd>csillog</trd>,
    <trd>vill&oacute;dzik</trd>
