<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bril.xml,v 1.29 2021/03/20 13:52:59 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bril</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bril.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bril.0i.lumo">
    <dif>
      Eligi a&ubreve; resendi helan lumon:
      <ekz>
        la luno <tld/>is tra la fenestro sur la mezon de la planko
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        frue matene <klr>[&#8230;]</klr> la suno ek<tld/>is super la akvo
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 3:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo <klr>[&#8230;]</klr> <tld/>is kiel oro
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        altaj bluaj montoj, sur kies suproj <tld/>is la blanka ne&gcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ek<tld/>is fulmoj unu post alia
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj <tld/>is <klr>[&#8230;]</klr>, li havis belegajn longajn harojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plandoj de iliaj piedoj <klr>[&#8230;]</klr> <tld/>is kiel hela pura kupro
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 1:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        floroj <klr>[&#8230;]</klr>, ru&gcirc;aj kaj bluaj, <tld/>is &ccirc;e la muroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la plafono <tld/>is lampiroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> <tld/>is kiel diamantoj
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>ne &ccirc;io <tld/>anta estas diamanto</ind>
        <trd lng="fr">tout ce qui brille n'est pas or</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne &ccirc;io utilas, kio brilas<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="blink.0i">blinki</ref>,
      <ref cel="fajr.0i">fajri</ref>,
      <ref cel="flagr.0i">flagri</ref>,
      <ref cel="flam.0i">flami</ref>,
      <ref cel="fulm.0i">fulmi</ref>,
      <ref cel="glim">glimi</ref>,
      <ref cel="hel">heli</ref>,
      <ref cel="lum.0i">lumi</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>scheinen</trd>,
      <trd>glitzern</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>b&lstroke;yszcze&cacute;</trd>,
      <trd>l&sacute;ni&cacute;</trd>,
      <trd>ja&sacute;nie&cacute;</trd>
    </trdgrp>
    <trd lng="pt">brilhar</trd>
  </snc>
  <snc mrk="bril.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Altiri la atenton per sia prospereco, eleganteco, lukso, spriteco:
      <ekz>
        la sukceso estis <tld/>anta, preska&ubreve; &ccirc;iuj pagis
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la ruinigita nobelo <klr>[&#8230;]</klr> revis pli <tld/>antan sorton
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flor.0i.prosperi">flori<sncref/></ref>,
      <ref cel="juvel.0o.FIG">juvelo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ib;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_z;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_ib;&c_h;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_ib;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_ib;&c_s;&c_k;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_b;&c_l;&c_je;&c_s;&c_t;&c_ja;</trd>,
    <trd>&c_l;&c_malmol;&c_shch;&c_ja;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_ja;</trd>,
    <trd>&c_s;&c_i;&c_ja;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bl&yacute;skat se</trd>,
    <trd>lesknout se</trd>,
    <trd>skv&ecaron;t se</trd>,
    <trd>skv&ecaron;t se</trd>,
    <trd>t&rcaron;pytit se</trd>,
    <trd>z&aacute;&rcaron;it</trd>,
    <trd>z&aacute;&rcaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gl&auml;nzen</trd>,
  </trdgrp>
  <trd lng="en">to shine</trd>
  <trd lng="es">brillar</trd>
  <trdgrp lng="fr">
    <trd>briller</trd>,
    <trd>luire</trd>
  </trdgrp>
  <trd lng="he">&lamed;&zayin;&he;&vav;&resh;</trd>
  <trd lng="hu">ragyog</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_je;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_i;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_i;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&zcaron;iari&tcaron;</trd>,
    <trd>ligota&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="bril.0o">
  <kap><tld/>o</kap>
  <snc mrk="bril.0o.FIZ">
    <dif>
      Hellumo sendata de io:
      <ekz>
        <tld/>o de fajro, de kvarco;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>o de la luno;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni ne lasu nin trompi per la <tld/>o de la vortoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ja;&c_s;&c_k;</trd>,
    <trd>&c_z;&c_mol;&c_z;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_ib;&c_h;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_b;&c_l;&c_ja;&c_s;&c_malmol;&c_k;</trd>,
    <trd>&c_s;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>lesk</trd>,
    <trd>z&aacute;&rcaron;</trd>,
    <trd>z&aacute;&rcaron;e</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Glanz</trd>,
    <trd>Schein</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>light</trd>,
    <trd>brightness</trd>, <trd>brilliance</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>brillo</trd>,
    <trd>resplandor</trd>
  </trdgrp>
  <trd lng="fr">&eacute;clat <klr tip="ind">(lueur)</klr></trd>
  <trdgrp lng="he">
    <trd>&zayin;&vav;&he;&resh;</trd>,
    <trd>&bet;&resh;&qof;</trd>
  </trdgrp>
  <trd lng="hu">ragyog&aacute;s</trd>
  <trdgrp lng="pl">
    <trd>blask</trd>,
    <trd>l&sacute;nienie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_je;&c_s;&c_k;</trd>,
    <trd>&c_s;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&zcaron;iara</trd>,
    <trd>lesk</trd>,
    <trd>ligot</trd>
  </trdgrp>
</drv>

<drv mrk="bril.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="B"/>anta:
      <ekz>
        <tld/>a festo, kura&gcirc;a&jcirc;o, venko, stilo;
      </ekz>
      <ekz>
        <tld/>e glorigi iun<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_l;&c_ib;&c_s;&c_k;&c_u;&c_ch;&c_y;</trd>,
      <trd>&c_b;&c_l;&c_ib;&c_sh;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>,
      <trd>&c_z;&c_ib;&c_h;&c_o;&c_t;&c_k;&c_ib;</trd>,
      <trd>&c_z;&c_ib;&c_h;&c_a;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="de">gl&auml;nzend</trd>
    <trd lng="es">brillante</trd>
    <trd lng="fr">brillant <klr tip="ind">(adj.)</klr></trd>
    <trd lng="hu">ragyog&oacute;</trd>
    <trdgrp lng="pl">
      <trd>b&lstroke;yszcz&aogonek;cy</trd>,
      <trd>l&sacute;ni&aogonek;cy</trd>,
      <trd>ja&sacute;niej&aogonek;cy</trd>
    </trdgrp>
    <trd lng="ro">str&abreve;lucitor</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_l;&c_je;&c_s;&c_t;&c_ja;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_i;&c_s;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_i;&c_ja;&c_ju;&c_shch;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_b;&c_l;&c_je;&c_s;&c_t;&c_ja;&c_shch;</trd>,
    <trd>&c_l;&c_malmol;&c_s;&c_k;&c_a;&c_v;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_je;&c_shch;</trd>,
    <trd>&c_s;&c_i;&c_ja;&c_je;&c_shch;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>jiskrn&yacute;</trd>,
    <trd>n&aacute;dhern&yacute;</trd>,
    <trd>skv&ecaron;l&yacute;</trd>,
    <trd>skv&ecaron;l&yacute;</trd>,
    <trd>z&aacute;&rcaron;iv&yacute;</trd>
  </trdgrp>
   <trdgrp lng="en">
      <trd>bright</trd>,
      <trd>shiny</trd>, <trd>shining</trd>, <trd>brilliant</trd>
    </trdgrp>
  <trd lng="he">&zayin;&vav;&he;&resh;</trd>
  <trdgrp lng="sk">
    <trd>&zcaron;iariv&yacute;</trd>,
    <trd>leskl&yacute;</trd>,
    <trd>ligotav&yacute;</trd>,
    <trd>skvel&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="bril.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="bril.0eto.KOMUNE">
    <dif>
      Malgranda, malforta <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>eto de kontenteco &scirc;teliris trans lia viza&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">lueur</trd>
</drv>

<drv mrk="bril.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="bril.ma0a.lume">
    <dif>
      Kiu ne a&ubreve; malmulte <tld/>as:
      <ekz>
        la plej mal<tld/>ajn astrojn oni prefere observu
        per eta grandigo: alimaniere ili simple malaperas
        <fnt>
          <aut>D. Galad&iacute;-Enr&iacute;quez, A. Wandel</aut>,
          <vrk>La Kosmo kaj Ni</vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="bril.ma0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Maleminenta, senefekta, sobra:
      <ekz>
        mi &ccirc;iam konsideris lin mal<tld/>a homo:
        lerta malanta&ubreve; la kuliso, sed ne aparte rimarkinda
        <fnt>
          <aut>A. L&ouml;wenstein</aut>,
          <vrk>Morto de Artisto</vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>falot</trd>,
    <trd>terne</trd>
  </trdgrp>
</drv>

<drv mrk="bril.re0i">
  <kap>re<tld/>i</kap>
  <snc mrk="bril.re0i.FIZ">
    <dif>
      Lume speguli&gcirc;i:
      <ekz>
        la steloj re<tld/>is en la lago.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_d;&c_a;&c_c;&c_mol; <ind>
  &c_v;&c_o;&c_d;&c_b;&c_l;&c_je;&c_s;&c_k;</ind></trd>
  <trd lng="bg">&c_o;&c_t;&c_r;&c_a;&c_z;&c_ja;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  <trd lng="cs">odr&aacute;&zcaron;et se</trd>
  <trd lng="en">to reflect</trd>
  <trdgrp lng="fr">
    <trd>se <ind>refl&eacute;ter</ind></trd>,
    <trd>se <ind>r&eacute;fl&eacute;chir</ind> <klr tip="ind">
    (se refl&eacute;ter)</klr></trd>,
    <trd>miroiter</trd>
  </trdgrp>
  <trd lng="hu">visszat&uuml;kr&ouml;z&odblac;dik</trd>
  <trdgrp lng="pl">
    <trd>odb&lstroke;yskiwa&cacute;</trd>,
    <trd>puszcza&cacute; zaj&aogonek;czki<klr> (lusterkiem)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; <ind>
  &c_o;&c_t;&c_b;&c_l;&c_je;&c_s;&c_k;</ind></trd>
  <trd lng="sk">odr&aacute;&zcaron;a&tcaron; sa</trd>
</drv>

<drv mrk="bril.trem0i">
  <kap>trem<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bril.trem0i.FIZ">
    <dif>
      Intermite &jcirc;eti lumbriletojn:
      <ekz>
        la sunlumo trem<tld/>is en la <klr>[&#8230;]</klr> kupoloj de la 
        pre&gcirc;ejoj <fnt><bib>Voja&gcirc;Imp</bib>, 
        <lok>&ccirc;e Lev Tolstoj en Jasnaja
        Poljana</lok></fnt>;
      </ekz>
      <ekz>
        la steloj trem<tld/>is en la silenta nokto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_g;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ib;&c_g;&c_a;&c_c;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_l;&c_je;&c_shch;&c_u;&c_k;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>blikat</trd>,
    <trd>mrkat</trd>,
    <trd>m&zcaron;ikat</trd>,
    <trd>t&rcaron;pytit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>flimmern</trd>,
    <trd>glitzern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to sparkle</trd>,
    <trd>glitter</trd>
  </trdgrp>
  <trd lng="es">parpadear<klr> (una luz)</klr></trd>
  <trd lng="fr">scintiller</trd>
  <trdgrp lng="hu">
    <trd>villog</trd>,
    <trd>csillog</trd>,
    <trd>vill&oacute;dzik</trd>
  </trdgrp>
  <trd lng="nl">flikkeren</trd>
  <trdgrp lng="pl">
    <trd>migota&cacute;</trd>,
    <trd>b&lstroke;yska&cacute;</trd>,
    <trd>pe&lstroke;ga&cacute;<klr> (o ogniu)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_je;&c_r;&c_c;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kmita&tcaron;</trd>,
    <trd>trblieta&tcaron; sa</trd>
  </trdgrp>
</drv>
</art>
