<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grafis.xml,v 1.18 2024/04/09 06:29:05 revo Exp $">
<kap>
  <rad>grafism</rad>/o
</kap>

<drv mrk="grafis.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">LIN</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="sistem.skrib0o">Skribsistemo</ref>:
      <ekz>
        la antikva hebrea alfabeto estis forlasita flanke de judoj en la komenci&gcirc;o
        de la dua duono de la unua miljaro anta&ubreve; Jesuo Kristo <klr>[&#8230;]</klr>,
        sed &gcirc;i estis konservita &ccirc;e la samarianoj kaj, kvankam evoluinta,
        konigas per kiuj <tld/>oj estis antikve skribita la Tana&hcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=441158">Samaria alfabeto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sistem.son0o.FON">sonsistemo</ref>,
      <ref cel="foneti1.0o">fonetismo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_s;&c_t;&c_e;&c_m;&c_a; <ind>&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_o;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  <trd lng="ca">grafia</trd>
  <trdgrp lng="cs">
    <trd>grafika</trd>,
    <trd>pravopis</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schriftsystem</trd>,
    <trd>Graphismus</trd>,
  </trdgrp>
  <trd lng="es">graf&iacute;a</trd>
  <trd lng="fr">graphie</trd>
  <trdgrp lng="hu">
    <trd>grafikai rendszer</trd>,
    <trd>bet&udblac;rendszer</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#34920;&#x8A18;&#x6CD5;  <pr>&#12402;&#12423;&#12358;&#x304D;&#x307B;&#12358;</pr></trd>,
    <trd>&#x66F8;&#x8A18;&#x6CD5;  <pr>&#12375;&#12423;&#x304D;&#x307B;&#12358;</pr></trd>,
    <trd>&#34920;&#x8A18;&#x4E3B;&#x7FA9;  <pr>&#12402;&#12423;&#12358;&#x304D;&#12375;&#12421;&#x304E;</pr></trd>
  </trdgrp>
  <trd lng="pl">grafika</trd>
  <trd lng="pt">grafismo</trd>
  <trdgrp lng="sk">
    <trd>grafika</trd>,
    <trd>pravopis</trd>
  </trdgrp>
  <trd lng="uk">&#x43D;&c_a;&#x43F;&c_i;&c_s;&c_a;&#x43D;&#x43D;&#x44F; (&c_s;&#x43B;&#x43E;&c_v;&c_a;)</trd>
</drv>
</art>
