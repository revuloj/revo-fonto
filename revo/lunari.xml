<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lunari</rad>/o
</kap>
<drv mrk="lunari.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el <ref tip="super" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Lunaria</trd>),
      kun rondaj, papere maldikaj silikvoj, similaj al ar&gcirc;entmoneroj.
    </dif>
  </snc>
  <bld lok="../bld/lunari.jpg">
    <ind>jara <tld/>o</ind>
    <trd lng="la">Lunaria annua</trd>
  </bld>
  <trd lng="de">Silberblatt</trd>
  <trdgrp lng="en">
    <trd>lunaria</trd>,
    <trd>honesty</trd>,
    <trd>money plant</trd>
  </trdgrp>
  <trd lng="nl">judaspenning</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_u;&c_n;&c_n;&c_i;&c_k; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_g;&c_r;&c_o;&c_z;&c_d;&c_o;&c_v;&c_n;&c_i;&c_k; &c_l;&c_u;&c_n;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
