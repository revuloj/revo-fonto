<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lunari.xml,v 1.17 2020/02/28 21:10:41 revo Exp $">
<kap>
  <rad>lunari</rad>/o
</kap>
<drv mrk="lunari.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el <ref tip="malprt"
      cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Lunaria</trd>),
      <ref tip="lst" cel="herb.0o.BOT"
      lst="voko:brasikoparencoj">herbo</ref>
      kun rondaj, papere maldikaj silikvoj, similaj al ar&gcirc;entmoneroj:
      <ekz>
        la <tld/>o a&ubreve; judasmonero estas <klr>[&#8230;]</klr> indi&gcirc;ena
        en centra kaj suda E&ubreve;ropo, <klr>[&#8230;]</klr> la latina scienca
        nomo signifas lunplanton kaj aludas al la aspekto de la silikvoj
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=400098">Lunario</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/lunari.jpg">
    <ind>jara <tld/>o</ind>
    <trd lng="la">Lunaria annua</trd>
  </bld>
  <trd lng="ca">setins</trd>
  <trd lng="de">Silberblatt</trd>
  <trdgrp lng="en">
    <trd>lunaria</trd>,
    <trd>honesty</trd>,
    <trd>money plant</trd>
  </trdgrp>
  <trd lng="fr">lunaire<klr> (plante)</klr></trd>
  <trd lng="hu">holdviola</trd>
  <trd lng="nl">judaspenning</trd>
  <trd lng="pl">miesi&aogonek;cznica <klr>(ro&sacute;l.)</klr></trd>     
  <trd lng="ro">onestitate</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_u;&c_n;&c_n;&c_i;&c_k; <klr
tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_g;&c_r;&c_o;&c_z;&c_d;&c_o;&c_v;&c_n;&c_i;&c_k;
&c_l;&c_u;&c_n;&c_o;&c_v;&c_i;&c_d;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
