<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>lunari</rad>/o
</kap>
<drv mrk="lunari.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el <ref tip="super" cel="brasik.0acoj">brasikacoj</ref>
      (<trd lng="la">Lunaria</trd>),
      kun rondaj, papere maldikaj silikvoj, similaj al ar&gcirc;entmoneroj.   
    </dif>
  </snc>
  <bld lok="../bld/lunari.jpg">
    jara <tld/>o (<trd lng="la">Lunaria annua</trd>)
  </bld>
  <trd lng="de">Silberblatt</trd>
</drv>
</art>
