<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>lunari</rad>/o
</kap>
<drv mrk="lunari.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el <ref tip="super" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Lunaria</trd>),
      kun rondaj, papere maldikaj silikvoj, similaj al ar&gcirc;entmoneroj.
    </dif>
  </snc>
  <bld lok="../bld/lunari.jpg">
    <ind>jara <tld/>o</ind>
    <trd lng="la">Lunaria annua</trd>
  </bld>
  <trd lng="de">Silberblatt</trd>
  <trdgrp lng="en">
    <trd>lunaria</trd>,
    <trd>honesty</trd>,
    <trd>money plant</trd>
  </trdgrp>
  <trd lng="nl">judaspenning</trd>
  </drv>
</art>
