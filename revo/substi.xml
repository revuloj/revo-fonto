<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>substitu</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="substi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Anstata&ubreve;igi ion per io alia.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>substituieren</trd>,
    <trd>ersetzen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>remplacer</trd>,
    <trd>substituer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vervangen</trd>,
    <trd>substitueren</trd>
  </trdgrp>
</drv>

<drv mrk="substi.0o">
  <kap><tld/>o</kap>
  <snc mrk="substi.0o.KOMUNE">
    <dif>
      Ago a&ubreve; rezulto <tld/>i.
    </dif>
    <trdgrp lng="fr">
      <trd>remplacement</trd>,
      <trd>substitution</trd>
    </trdgrp>
  </snc>
  <snc mrk="substi.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 14</lok></fnt>
    <dif>
      <ref tip="super" cel="permut.0o">Permuto</ref> super
      finia aro.
    </dif>
  </snc>
</drv>
</art>
