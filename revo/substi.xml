<art mrk="$Id: substi.xml,v 1.25 2025/04/06 06:47:40 revo Exp $">
<kap>
  <rad>substitu</rad>/i
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="substi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Anstata&ubreve;igi ion per io alia:
      <ekz>
        vaste uzata simbolo por IJK &gcirc;enerale ne ekzistas,
        sed tiun funkcion almena&ubreve; dum la aran&gcirc;o mem <tld/>as uzo
        de la IJK-flago, kiu kutime pendas fone de la podio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1815">Internacia Junulara
          Kongreso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        por multaj el ili la problemo de la tutmondi&gcirc;o estas nur
        <tld/>a problemo, en ilia protesto evidenti&gcirc;as ilia radikalismo
        kaj la &gcirc;enerala malkontento de junaj homoj
        <fnt>
          <aut>jpa</aut>
            <vrk><url ref="&Monato;/2000/007093.php">Internacia kunsido de financistoj en
            Prago</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 9</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>anstata&#365;igi</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_m;&c_ja;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">substituir</trd>
  <trdgrp lng="cs">
    <trd>nahradit</trd>,
    <trd>nahrazovat</trd>,
    <trd>substituovat</trd>,
    <trd>zam&ecaron;nit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>substituieren</trd>,
    <trd>ersetzen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>reemplazar</trd>,
    <trd>sustituir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>remplacer</trd>,
    <trd>substituer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>helyettes&iacute;t</trd>,
    <trd>felcser&eacute;l</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>sostituire</trd>,
    <trd>rimpiazzare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20195;&#29992;&#12377;&#12427;  <pr>&#12384;&#12356;&#12424;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#32622;&#12365;&#25563;&#12360;&#12427;  <pr>&#12362;&#12365;&#12363;&#12360;&#12427;</pr></trd>,
    <trd>&#32622;&#25563;&#12377;&#12427;  <pr>&#12385;&#12363;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#20195;&#20837;&#12377;&#12427;  <pr>&#12384;&#12356;&#12395;&#12421;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vervangen</trd>,
    <trd>substitueren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>substytuowa&cacute;</trd>,
    <trd>zast&eogonek;powa&cacute;  <klr>(jedno drugim)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_je;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol; <klr tip="amb">(&c_v;&c_m;&c_je;&c_s;&c_t;&c_o;
    &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_p;&c_o;&c_d;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_v;&c_m;&c_je;&c_s;&c_t;&c_o;
    &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dosadi&tcaron;</trd>,
    <trd>nahradi&tcaron;</trd>,
    <trd>substituova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#1084;&c_ib;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&#1087;&c_ib;&c_d;&c_s;&c_t;&c_a;&c_v;&#1083;&#1103;&c_t;&c_i;</trd>,
    <trd>&#1087;&c_ib;&c_d;&#1084;&c_ib;&#1085;&#1102;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&#1084;&c_ib;&#1085;&#1103;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i; &#1085;&c_a; &c_ch;&c_i;&c_jeu;&c_s;&#1100; &#1084;&c_ib;&c_s;&c_c;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20195; <pr>d&#224;i</pr></trd>,
    <trd>&#21462;&#32780;&#20195;&#20043; <pr>q&#468;&#233;rd&#224;izh&#299;</pr></trd>,
    <trd>&#26367;&#20195; <pr>t&#236;d&#224;i</pr></trd>,
    <trd>&#20195;&#26367; <pr>d&#224;it&#236;</pr></trd>,
    <trd>&#21462;&#20195; <pr>q&#468;d&#224;i</pr></trd>,
    <trd>&#26367;&#25442; <pr>t&#236;hu&#224;n</pr></trd>,
    <trd>&#26367;&#25563; <pr>t&#236;hu&#224;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="substi.0o">
  <kap><tld/>o</kap>
  <snc mrk="substi.0o.KOMUNE">
    <dif>
      Ago <tld/>i a&ubreve; &gcirc;ia rezulto.
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_m;&c_ja;&c_sh;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_m;&c_je;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">substituci&oacute;</trd>
    <trdgrp lng="es">
      <trd>reemplazo</trd>,
      <trd>sustituto</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>remplacement</trd>,
      <trd>substitution</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>helyettes&iacute;t&eacute;s</trd>,
      <trd>felcser&eacute;l&eacute;s</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>sostituzione</trd>,
      <trd>rimpiazzo</trd>
    </trdgrp>
    <trd lng="pl">podmiana</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_m;&c_je;&c_n;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_m;&c_je;&c_n;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#25442;&#20154; <pr>hu&#224;nr&#233;n</pr></trd>,
    <trd>&#25563;&#20154; <pr>hu&#224;nr&#233;n</pr></trd>,
    <trd>&#26367;&#20195; <pr>t&#236;d&#224;i</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="substi.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 14</lok></fnt>
    <dif>
      <ref tip="super" cel="permut.0o.bijekcio">Permuto</ref> super
      finia aro.
    </dif>
    <rim>
      La montrita fonto ne indikas, en kio konsistas la nuanco inter
      permuto kaj <tld/>o. Cetere la nuanco estas malgranda kaj
      &scirc;ajnas, ke en multaj naciaj lingvoj oni preferas uzi
      nur la pli vastasencan <ctl>permuto</ctl>.
    </rim>
    <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_a; <klr>(&c_m;&c_a;&c_t;.)</klr></trd>
    <trd lng="ca">permutaci&oacute;</trd>
    <trdgrp lng="de">
      <trd>Substitution</trd>,
      <trd>Einsetzung</trd>,
      <trd>Vertauschung</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>substitution</trd>,
      <trd>permutation</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>substitution</trd>,
      <trd>permutation</trd>
    </trdgrp>
    <trd lng="hu">helyettes&iacute;t&eacute;s</trd>
    <trd lng="it">sostituzione</trd>
    <trd lng="pl">permutacja</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>anstata&#365;igado</ke>.
  </rim>

  <trdgrp lng="ja">
    <trd>&#20195;&#29992;  <pr>&#12384;&#12356;&#12424;&#12358;</pr></trd>,
    <trd>&#32622;&#12365;&#25563;&#12360;  <pr>&#12362;&#12365;&#12363;&#12360;</pr></trd>,
    <trd>&#32622;&#25563;  <pr>&#12385;&#12363;&#12435;</pr></trd>,
    <trd>&#20195;&#20837;  <pr>&#12384;&#12356;&#12395;&#12421;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#1084;&c_ib;&c_shch;&c_je;&#1085;&#1085;&#1103;</trd>,
    <trd>&#1087;&c_ib;&c_d;&#1084;&c_ib;&#1085;&c_a;</trd>,
    <trd>&c_z;&c_a;&#1084;&c_ib;&#1085;&c_a;</trd>
  </trdgrp>
</drv>
