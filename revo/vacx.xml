<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vacx.xml,v 1.8 2023/09/05 09:43:53 revo Exp $">
<kap>
  <rad>va&ccirc;</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="vacx.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vacx.0i.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="vid" cel="gvat.0i.dejxore">Gvati</ref>,
      de&jcirc;ore observi:
      <ekz>
        ni tri viroj, kiuj lo&gcirc;is en tiu kupeo, &ccirc;iuj samtempe <tld/>is, tiel, ke ni &ccirc;iuj
        en la sama tempo devis vesti kaj malvesti nin en tiu &ccirc;ambro
        <fnt>
          <aut>B. Traven El, trad. Hans Georg Kaiser </aut>
          <vrk><url ref="&Tekstaro;?nomo=mortula-shipo">Mortula &scirc;ipo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sur la anta&ubreve;ferdeko staris la <tld/>anaroj, kiuj havis liberan tempon
        <fnt>
          <aut>B. Traven, trad. Hans Georg Kaiser </aut>
          <vrk><url ref="&Tekstaro;?nomo=mortula-shipo">Mortula &scirc;ipo</url></vrk>
          <lok>&ccirc;apitro 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        do la <ind>rat<tld/>o</ind> <klr>(de noktomezo &gcirc;is la 4a horo, anka&ubreve; hunda <tld/>o)</klr>,
        tio ja anta&ubreve;videblis
        <fnt>
          <aut>B. Traven El, trad. Hans Georg Kaiser </aut>
          <vrk><url ref="&Tekstaro;?nomo=mortula-shipo">Mortula &scirc;ipo</url></vrk>
        </fnt>;
        <trd lng="de">Hundewache</trd>
        <trd lng="en">dog watch</trd>
        <trd lng="he">&mem;&shin;&mem;&resh;&tav; &kaf;&lamed;&bet;</trd>
      </ekz>
      <ekz>
        li indikis la pa&scirc;ojn tenante la takton per metronomo, sub la 
        afabla <tld/>o de <nom>&Uacute;rsula</nom>, kiu e&ccirc; ne unu momenton forlasis la 
        &ccirc;ambron dum &scirc;iaj filinoj ricevis la lecionojn
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>1992</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="gard.0estari">gardestari</ref>,
       <ref cel="gvardi.0o.MIL_gardistaro">gvardio<sncref/></ref>,
       <ref cel="posten.0i">posteni</ref>
    </refgrp>
    <trd lng="de">wachen</trd>
    <trd lng="en">watch</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_n;&c_a; <ind>&c_v;&c_a;&c_h;&c_c;&c_je;</ind></trd>,
    <trd>&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_v;&c_a;&c_h;&c_t;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="es">vigilar</trd>
  <trd lng="fr">surveiller</trd>
  <trd lng="ja">&#x5F53;&#x76F4;&#x306B;&#x3064;&#x304F;  [<ind>&#x3068;&#x3046;&#x3061;&#x3087;&#x304F;&#x306B;&#x3064;&#x304F;</ind>]</trd>
  <trd lng="nl">bewaken</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &#x43D;&c_a; &c_v;&c_a;&c_h;&c_t;&c_ib;</trd>,
    <trd>&#x43D;&c_je;&c_s;&c_t;&c_i; &c_v;&c_a;&c_h;&c_t;&c_u;</trd>
  </trdgrp>
</drv>
</art>

