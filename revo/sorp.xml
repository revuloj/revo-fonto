<drv mrk="sorp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Sorbus</trd>), kies fruktoj estas man&gcirc;eblaj nur
      kiam ili estas tromaturaj.
    </dif>
  </snc>
  <trd lng="de">Eberesche</trd>
  <trd lng="hu">berkenye</trd>
  <trd lng="nl">lijsterbes</trd>
  <trd lng="ru">&c_r;&c_ja;&c_b;&c_i;&c_n;&c_a;</trd>
  <trd lng="sv">oxel</trd>
</drv>
</art>
