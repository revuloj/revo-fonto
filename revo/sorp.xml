<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sorp.xml,v 1.24 2017/06/07 14:10:16 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>sorp</rad>/o
</kap>
<drv mrk="sorp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      La <ref tip="super" cel="drup.0o">drupo</ref>
      de (birda) <ref tip="malprt" cel="sorp.0ujo"><tld/>ujo</ref>:
      <ekz>&scirc;i deskuis <tld/>ojn, el kiuj &scirc;i faris kolierojn
        <fnt>
          <aut>Bo&zcaron;ena N&ecaron;mcov&aacute;, trad. Vl. Tobek kaj K. Proch&aacute;zka</aut>
          <vrk>Avinjo</vrk>
          <lok>&ccirc;apitro 11a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">je&rcaron;abina</trd>
  <trd lng="de">Vogelbeere</trd>
  <trdgrp lng="en">
    <trd>sorb</trd>,
    <trd>sorbus</trd>
  </trdgrp>
  <trd lng="fr">sorbe</trd>
  <trd lng="hu">berkenye<klr tip="amb"> (gy&uuml;m&ouml;lcs)</klr></trd>
  <trd lng="nl">lijsterbes<klr tip="ind"> (vrucht)</klr></trd>
  <trd lng="pl">jarz&aogonek;b</trd>
  <trd lng="ru">&c_r;&c_ja;&c_b;&c_i;&c_n;&c_a;<klr tip="amb"> (&c_ja;&c_g;&c_o;&c_d;&c_a;)</klr></trd>
  <trd lng="sk">jarabina</trd>
</drv>

<drv mrk="sorp.0ujo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Sorbus</trd>), de arbedoj kaj arboj sen pikiloj,
      simplaj
      a&ubreve;
      plumaj folioj, ofte kun lana malsupra flanko, kun floroj
      aran&gcirc;ataj
      en umbelaj panikloj kaj bero- a&ubreve; piroformaj drupoj:
      <ekz>
        e&ccirc; malgranda beleta sorparbo enigis tie radikojn kaj
        staris gracie kaj elegante alte supre en la mezo
        de la maljuna saliko
        <fnt><bib>Fab3</bib>, <lok>&Ccirc;io sur sian &gcirc;ustan
        lokon!</lok></fnt>;
      </ekz>
      <ekz>
        sub la ru&gcirc;a <tld/>ujo &scirc;i staris
        <fnt>
          <aut>Selma Lagerl&ouml;f, trad. John Lundgren</aut>
          <vrk>Uzino 2</vrk>
          <lok>La senpaculoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>doma <tld/>ujo</ind> <klr>(kun pirformaj fruktoj)</klr>;
        <trd lng="de">Speierling</trd>
        <trdgrp lng="fr">
          <trd>sorbier domestique</trd>,
          <trd>cormier</trd>
        </trdgrp>
          <trd lng="hu">kerti <ind>berkenye</ind></trd>
        <trd lng="la">Sorbus domestica</trd>
      </ekz>
      <ekz>
        <ind>birda <tld/>ujo</ind> <klr>(kun oran&gcirc;- &gcirc;is
        ru&gcirc;koloraj
        fruktoj uzeblaj por marmelado kaj kompoto)</klr>;
        <trdgrp lng="de">
          <trd>Eberesche</trd>, <trd>Vogelbeerbaum</trd>
        </trdgrp>
        <trd lng="fr">sorbier des oiseleurs</trd>
        <trd lng="hu">mad&aacute;rberkenye</trd>
        <trd lng="la">Sorbus aucuparia</trd>
      </ekz>
      <ekz>
        <ind>blanka <tld/>ujo</ind>.
        <trd lng="de">Gew&ouml;hnliche <ind>Mehlbeere</ind></trd>
        <trd lng="fr">alisier blanc</trd>
        <trd lng="hu">lisztes <ind>berkenye</ind></trd>
        <trd lng="la">Sorbus aria</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">je&rcaron;&aacute;b jedl&yacute;</trd>
  <trd lng="de">Mehlbeere</trd>
  <trdgrp lng="fr">
    <trd>sorbier<klr> (plumaj folioj)</klr></trd>,
    <trd>alisier<klr> (simplaj folioj)</klr></trd>
  </trdgrp>
  <trd lng="hu">berkenye<klr tip="amb"> (fa)</klr></trd>
  <trd lng="nl">lijsterbes<klr tip="ind"> (plant)</klr></trd>
  <trd lng="pl">jarz&aogonek;b</trd>
  <trd lng="ru">&c_r;&c_ja;&c_b;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>druh jarabiny</trd>,
    <trd>oskoru&scaron;a</trd>
  </trdgrp>
  <trd lng="sv">oxel</trd>
</drv>
</art>
