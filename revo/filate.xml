<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>filatel</rad>/o<fnt>Z</fnt>,
filateli/o<fnt>Z</fnt>
</kap>
<drv mrk="filate.0o">
   <kap><tld/>o, filatelio</kap>
   <uzo tip="fak">BELA</uzo>
   <uzo tip="fak">SPO</uzo>
   <snc>
     <dif>
       Konado k kolektado de po&scirc;tmarkoj.
     </dif>
   </snc>
   <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_t;&c_e;&c_l;&c_ib;&c_ja;</trd>
   <trdgrp lng="de">
      <trd>Philatelie</trd>,
      <trd>Briefmarkenkunde</trd>,
      <trd>Briefmarkensammeln</trd></trdgrp>
   <trd lng="es">filatelia</trd>
   <trd lng="fr">philat&eacute;lie</trd>
   <trdgrp lng="hu">
      <trd>b&eacute;lyeggy&udblac;jt&eacute;s</trd>,
      <trd>filat&eacute;lia</trd></trdgrp>
   <trd lng="nl">filatelie</trd>
   <trd lng="ru">&c_f;&c_i;&c_l;&c_a;&c_t;&c_je;&c_l;&c_i;&c_ja;</trd>
</drv>
<drv mrk="filate.0isto">
   <kap><tld/>isto</kap>
   <snc>
     <dif>
       Kolektanto de po&scirc;tmarkoj.
     </dif>
   </snc>
   <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_t;&c_e;&c_l;&c_ib;&c_s;&c_t;</trd>
   <trdgrp lng="de">
      <trd>Philatelist</trd>,
      <trd>Briefmarkensammler</trd></trdgrp>
   <trd lng="fr">philat&eacute;liste</trd>
   <trdgrp lng="hu">
      <trd>b&eacute;lyeggy&udblac;jt&odblac;</trd>,
      <trd>filat&eacute;lista</trd></trdgrp>
   <trdgrp lng="nl">
      <trd>filatelist</trd>,
      <trd>postzegelverzamelaar</trd></trdgrp>
   <trd lng="ru">&c_f;&c_i;&c_l;&c_a;&c_t;&c_je;&c_l;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
