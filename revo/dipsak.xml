<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>dipsak</rad>/o
</kap>
<drv mrk="dipsak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="dipsak.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Dipsacus</trd>), kies dornojn oni uzas por kardi &scirc;tofon.
    </dif>
  </snc>
  <trd lng="de">Karde</trd>
</drv>
<drv mrk="dipsak.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Dipsacaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Kardengew&auml;chse</trd>
</drv>
</art>
