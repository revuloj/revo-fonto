<vortaro>
<art mrk="$Id$">
<kap>
<rad>dipsak</rad>/o
</kap>
<drv mrk="dipsak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="dipsak1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Dipsacus</trd>), kies dornojn oni uzas por kardi
      &scirc;tofon.
    </dif>
  </snc>
  <trd lng="de">Karde</trd>
  <trd lng="fr">card&egrave;re</trd>
  <trd lng="hu">m&aacute;csonya</trd>
  <trdgrp lng="pt">
    <trd>d&#237;psaco</trd>,
    <trd>cardo</trd>,
    <trd>cardo-cardador</trd>,
    <trd>cardo-penteador</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_s;&c_ja;&c_n;&c_k;&c_a;</trd>
</drv>

</art>
