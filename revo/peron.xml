<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>peron</rad>/o
</kap>
<drv mrk="peron.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ekstera malalta &scirc;tuparo fini&gcirc;anta per plata&jcirc;o, sur kiu malfermi&gcirc;as
      pordo de domo:
      <ekz>
        vasta pina domo kun <tld/>eto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Freitreppe</trd>,
      <trd>Vortreppe</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>stoep</trd>,
      <trd>bordes</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_y;&c_l;&c_mol;&c_c;&c_o;</trd>,
      <trd>&c_p;&c_o;&c_d;&c_malmol;&c_je;&c_z;&c_d;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      Kajo 2.
    </dif>
    <trd lng="de">Bahnsteig</trd>
    <trd lng="ru">&c_p;&c_je;&c_r;&c_r;&c_o;&c_n;</trd>
  </snc>
</drv>
</art>
