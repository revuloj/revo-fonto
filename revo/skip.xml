<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skip</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="skip.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="grup.0o.personoj">Grupo</ref> de
      kunlaborantoj
      plenumanta komunajn taskojn:
      <ekz>
        Leopoldo komisiis al li <klr>...</klr> prepari la voja&gcirc;on de
        <tld/>o de esploristoj
        <fnt>
        <aut>Wim M. A. De Smet</aut>,
        <vrk>Anta&ubreve; cent jaroj</vrk>,
        <bib>Monato</bib>, <lok>jaro 1998a, numero 5a, p. 25a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="band.0o.KOMUNE">bando</ref>,
      <ref cel="op.0o.KOMUNE">opo</ref>,
      <ref cel="tacxme.0o">ta&ccirc;mento</ref>,
      <ref cel="team.0o.SPO">teamo</ref>,
      <ref cel="trup.0o.KOMUNE">trupo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_a;&c_n;&c_d;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_y;&c_g;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_je;&c_k;&c_t;&c_y;&c_w;</trd>,
    <trd>&c_z;&c_mol;&c_m;&c_je;&c_n;&c_a; <klr tip="amb">(&c_n;&c_a;
    &c_p;&c_r;&c_a;&c_d;&c_p;&c_r;&c_y;&c_je;&c_m;&c_s;&c_t;&c_v;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="en">team</trd>
  <trd lng="fr">&eacute;quipe</trd>
  <trdgrp lng="hu">
    <trd>csapat</trd>,
    <trd>brig&aacute;d</trd>,
    <trd>m&udblac;szak</trd>
  </trdgrp>
</drv>
</art>

