<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: minera1.xml,v 1.29 2024/01/26 17:40:06 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>mineralogi</rad>/o
</kap>
<drv mrk="minera1.0o">
  <kap><tld/>o</kap>
  <snc mrk="minera1.0o.SCI">
    <uzo tip="fak">MIN</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako" lst="voko:sciencoj">Scienco</ref> 
      pri la mineraloj:
      <ekz>
        en <nom>Freiberg</nom> kaj Lepsiko li studis juron, filozofion, lingvojn kaj &ccirc;efe <tld/>on,
        kiu fari&gcirc;is lia preferata scienco, en la jaro 1775 <nom>Werner</nom> eklaboris kiel
        instruisto por minekspluato kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=266">Abraham Gottlob Werner</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_e;&c_r;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="ca">mineralogia</trd>
  <trdgrp lng="cs">
    <trd>mineralogie</trd>,
    <trd>n. o nerostech</trd>
  </trdgrp>
  <trd lng="de">Mineralogie</trd>
  <trd lng="en">mineralogy</trd>
  <trd lng="es">mineralog&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&f_k;&a_A;&a_n;&f_y;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>,
    <trd>&a_m;&a_ayn;&a_d;&a_n;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">min&eacute;ralogie</trd>
  <trd lng="hu">&aacute;sv&aacute;nytan</trd>
  <trd lng="id">mineralogi</trd>
  <trd lng="ja">&#x9271;&#x7269;&#x5B66;  <pr>&#x3053;&#x3046;&#x3076;&#x3064;&#x304C;&#x304F;</pr></trd>
  <trd lng="nl">mineralogie</trd>
  <trd lng="pt">mineralogia</trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_je;&c_r;&c_a;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>mineral&oacute;gia</trd>,
    <trd>nerastopis</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x456;&#x43D;&#x435;&#x440;&#x430;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x77FF;&#x7269;&#x5B66; <pr>ku&#xE0;ngw&#xF9;xu&#xE9;</pr></trd>,
    <trd>&#x7926;&#x7269;&#x5B78; <pr>ku&#xE0;ngw&#xF9;xu&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="minera1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="minera1.0isto.SCI">
    <uzo tip="fak">MIN</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        <nom>Ign&aacute;c Born</nom> <klr>[&#x2026;]</klr> estis hungara <tld/>isto, geologo kaj framasono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=318709">Ign&aacute;c Born</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="mineralog.0o">mineralogo</ref>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_e;&c_r;&c_a;&c_l;&c_jo;&c_g;</trd>
  <trd lng="ca">mineralogista</trd>
  <trd lng="en">mineralogist</trd>
  <trd lng="fr">min&eacute;ralogiste</trd>
  <trd lng="id">mineralog</trd>
</drv>
</art>
