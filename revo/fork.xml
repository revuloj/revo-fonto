<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fork.xml,v 1.52 2023/12/12 19:24:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fork</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="fork.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fork.0o.mangxilo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="mangx.0ilo">Man&gcirc;ilo</ref> konsistanta
      el tenilo kaj plateto provizita per pluraj pikiloj, servanta por
      piki ion kaj levi &gcirc;in al la bu&scirc;o:
      <ekz>
        mi a&ccirc;etis dekduon da kuleroj kaj du dekduojn da <tld/>oj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni metis anta&ubreve; mi man&gcirc;ilaron, kiu konsistis el
        telero, <ref tip="vid" cel="kuler.0o">kulero</ref>,
        <ref tip="super" cel="trancx.0ilo">tran&ccirc;ilo</ref>,
        <tld/>o, glaseto por brando, glaso por vino kaj telertuketo
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        la ansero elsaltis el la plado kaj kun <tld/>o kaj tran&ccirc;ilo
        en sia dorso balanci&gcirc;ante ekiris sur la planko
        <fnt><bib>Fab2</bib>, <lok>knabineto kun alumetoj</lok></fnt>;
      </ekz>
      <ekz>
        se iu bu&ccirc;is oferon tiam venadis knabo de la pastro
        dum la kuirado de la viando havante en sia mano tridentan
        <tld/>on
        kaj li enpu&scirc;adis &gcirc;in en la kaldronon
        a&ubreve; en la poton kaj &ccirc;ion, kion trafis
        la <tld/>o prenadis la pastro
        <fnt><bib>MT</bib>, <lok>&Sa1; 2:13</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fruktodona valo kiu havis formon de <tld/>o, kies dentoj
        estis ka&scirc;itaj inter la &scirc;tonoj
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 3a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la infanteria kapitano forte min <ind><tld/>i</ind>s <klr>(kvaza&ubreve; pikis kaj levis)</klr>:
        mirinde lerte li, kanajlo, manovras
        la kartojn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>.
        <trdgrp lng="de">
          <trd>aufgabeln</trd>,
          <trd>aufspie&szlig;en</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_ib;&c_d;&c_e;&c_l;&c_je;&c_c;</trd>
    <trd lng="hu">villa</trd>
    <trd lng="id">garpu</trd>
    <trd lng="pl">widelec</trd>
    <trd lng="ru">&c_v;&c_i;&c_l;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="fork.0o.objekto">
    <dif>
      Samforma objekto:
      <ekz>
        kaj ili <klr>(<ref tip="malprt" cel="forfik.0o">forfikuloj</ref>)</klr>
        uzis la <tld/>on, kiun ili havis malanta&ubreve;e &ccirc;e la vosto
        por pin&ccirc;i la skarabon je lia barbo
        <fnt><bib>Fab4</bib>, <lok>sterkskarabo</lok></fnt>;
      </ekz>
      <ekz>
        la karbujoj, la <tld/>oj kaj la &scirc;oveliloj
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 4:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        forna <tld/>o kaj sitelo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> nerveme pu&scirc;is <tld/>on de la telefono kaj balbutis <klr>[&#8230;]</klr>
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>son<tld/>eto</ind>
        <klr>(<ref cel="diapaz.0o">diapazono</ref>)</klr>;
        <trd lng="be">&c_k;&c_a;&c_m;&c_e;&c_r;&c_t;&c_o;&c_n;</trd>
        <trd lng="de">Stimmgabel</trd>
        <trd lng="en">tuning fork</trd>
        <trd lng="fa">&a_d;&f_y;&a_A;&f_p;&a_A;&a_z;&a_w;&a_n;</trd>
        <trd lng="fr">diapason</trd>
        <trd lng="hu">hangvilla</trd>
        <trd lng="id"><ind>garpu</ind> tala</trd>
        <trd lng="nl">stemvork</trd>
        <trd lng="pt">diapas&atilde;o</trd>
        <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_t;&c_o;&c_n;</trd>
        <trd lng="sv">st&auml;mgaffel</trd>
        <trd lng="tr">diyapazon</trd>
      </ekz>
      <ekz>
        la anta&ubreve;tubo enhavas la aparataron kiu ebligas ke la <tld/>o
        <klr>(de biciklo)</klr>
        turni&gcirc;u milde por stirado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=420">Biciklo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ib;&c_l;&c_y;</trd>,
      <trd>&c_v;&c_ib;&c_d;&c_e;&c_l;&c_je;&c_c;</trd>
    </trdgrp>
    <trd lng="hu">villa</trd>
    <trd lng="nl">vork</trd>
    <trd lng="pl">widelec</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_i;&c_l;&c_y;</trd>,
      <trd>&c_v;&c_i;&c_l;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="sv">gaffel</trd>
  </snc>
  <snc mrk="fork.0o.disbrancxigxo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Disbran&ccirc;i&gcirc;o:
      <ekz>
        <tld/>o de arbo, bran&ccirc;o;
      </ekz>
      <ekz>
        mi iris en la mal&gcirc;usta direkto &ccirc;e <tld/>o de la monteta vojo
        <fnt>
          <bib>Sol</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>voj<tld/>o</ind> <klr>(kie vojo pluiras al du
        direktoj)</klr>;
        <trd lng="be">&c_r;&c_o;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>
        <trd lng="de">Weggabelung</trd>
        <trd lng="fr">bifurcation</trd>
        <trdgrp lng="id">
          <trd>bifurkasi</trd>,
          <trd>per<ind>cabang</ind>an dua</trd>
        </trdgrp>
        <trd lng="hu">&uacute;tel&aacute;gaz&aacute;s</trd>
        <trd lng="nl">splitsing</trd>
      </ekz>
      <ekz>
        sekvu la kablon &gcirc;is la <tld/>o kaj diru al mi &ccirc;u &gcirc;i estas enigita en la muron
        <fnt>
          <bib>LOdE</bib>
          <lok> 1999, No 4 (54)</lok>
        </fnt>;
      </ekz>
      <ekz>
        tipaj manovroj <klr>[de &scirc;akludo inkluzivas]</klr> <tld/>on (atako, kiam unu figuro kreas
        samtempe du minacojn)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4074">&Scirc;ako</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;o de komputila programo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brak.0o">brako</ref>,
      <ref cel="brancx.0o">bran&ccirc;o</ref>,
      <ref cel="kruc.0o">kruco</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_l;&c_k;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_g;&c_a;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_r;&c_o;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>
    </trdgrp>
    <trd lng="es">bifurcaci&oacute;n</trd>
    <trd lng="fa">&a_A;&a_n;&a_sh;&a_ayn;&a_A;&a_b;</trd>
    <trd lng="hu">el&aacute;gaz&aacute;s</trd>
    <trd lng="pl">ga&lstroke;&aogonek;&zacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_l;&c_k;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_t;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_v;&c_i;&c_l;&c_i;&c_c;&c_a;</trd>
  <trd lng="bo">&t_ka;&t_nga;&t_tsheg;&t_da;&t_rata;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>vidle</trd>,
    <trd>vidli&ccaron;ka</trd>,
    <trd>vidlice</trd>
  </trdgrp>
  <trd lng="de">Gabel</trd>
  <trd lng="en">fork</trd>
  <trd lng="es">tenedor</trd>
  <trd lng="fa">&f_ch;&a_n;&f_g;&a_A;&a_l;</trd>
  <trd lng="fr">fourchette</trd>
  <trdgrp lng="ja">
    <trd>&#12501;&#12457;&#12540;&#12463;&#29366;&#12398;&#36947;&#20855;
    <pr>&#12405;&#12361;&#12540;&#12367;&#12376;&#12423;&#12358;&#12398;&#12393;&#12358;&#12368;</pr></trd>,
    <trd>&#12501;&#12457;&#12540;&#12463;  <pr>&#12405;&#12361;&#12540;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="pt">garfo</trd>
  <trd lng="sk">predn&aacute; vidlica</trd>
  <trd lng="sw">uma (nyuma)</trd>
  <trd lng="tr">&ccedil;atal</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_d;&c_je;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_g;&c_a;&c_l;&c_u;&c_zh;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_v;&c_i;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_v;&c_i;&c_l;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21449;&#23376; <pr>ch&#257;zi</pr></trd>,
    <trd>&#21449; <pr>ch&#257;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fork.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="fork.0eto.KUI">
    <dif>
      Malgranda <tld/>o, uzata por man&gcirc;i kukon kaj similajn:
      <ekz>
        sep malgrandaj teleroj, &ccirc;iu telereto kun sia <tld/>eto
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>;
      </ekz>
      <ekz>
        sendu al mi prunte dekduon da <tld/>etoj
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i fine havigis al si <tld/>eton kaj reatingis sian man&gcirc;olokon
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2006/008905.php">Negro
          i&gcirc;is nigrulo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_d;&c_e;&c_l;&c_je;&c_c;</trd>
  <trd lng="cs">j&iacute;deln&iacute; vidli&ccaron;ka</trd>
  <trd lng="de">G&auml;belchen</trd>
  <trd lng="hu">kisvilla</trd>
  <trd lng="id"><ind>garpu</ind> kecil</trd>
  <trdgrp lng="ja">
    <trd>&#12501;&#12457;&#12540;&#12463;  <pr>&#12405;&#12361;&#12540;&#12367;</pr></trd>,
    <trd>&#20108;&#21448;  <pr>&#12405;&#12383;&#12414;&#12383;</pr></trd>
  </trdgrp>
  <trd lng="nl">vorkje</trd>
  <trd lng="pl">widelczyk</trd>
  <trd lng="ru">&c_v;&c_i;&c_l;&c_k;&c_a;</trd>
  <trd lng="sk">vidli&ccaron;ka</trd>
  <trd lng="sv">gaffel</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_d;&c_je;&c_l;&c_mol;&c_c;&c_je;</trd>,
    <trd>&c_k;&c_o;&c_ch;&c_je;&c_t; (&c_g;&c_n;&c_ib;&c_z;&c_d;&c_o; &c_d;&c_l;&c_ja; &c_v;&c_je;&c_s;&c_l;&c_a;)</trd>
  </trdgrp>
</drv>

<drv mrk="fork.0ego">
  <kap><tld/>ego</kap>
  <uzo tip="fak">AGR</uzo>
  <snc mrk="fork.0ego.AGR">
    <dif>
      Granda <tld/>o, kun fortikaj dentoj, uzata por levi fojnon,
      garbon, herbofaskon kc:
      <ekz>
        kiam foje leprulo venis al ni kaj volis perforte eniri en la
        jurton,
        <klr>[&#8230;]</klr> la patro elpu&scirc;is lin per <tld/>ego
        <fnt>
          <vrk><url ref="&EduInf;lern/librejo/0023/0007.php">La
          fundo de l' mizero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kolekti&gcirc;is la kamparanoj kun <tld/>egoj, fal&ccirc;iloj kaj aliaj iloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=47531">Franca
          revolucio de 1789</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_h;&c_o;&c_r; <klr>(&c_d;&c_l;&c_ja;
    &c_n;&c_a;&c_v;&c_o;&c_z;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="cs">vidle</trd>
  <trdgrp lng="de">
    <trd>Forke</trd>,
    <trd>Heugabel</trd>,
    <trd>Mistgabel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pitch fork</trd>,
    <trd>hay fork</trd>
  </trdgrp>
  <trd lng="es">trinche</trd>
  <trd lng="fa">&f_ch;&a_n;&f_g;&f_k;</trd>
  <trd lng="fr">fourche</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;navilla</trd>,
    <trd>vasvilla</trd>
  </trdgrp>
  <trd lng="id">garu</trd>
  <trd lng="ja">&#12367;&#12414;&#12391;</trd>
  <trd lng="nl">riek</trd>
  <trd lng="pl">wid&lstroke;y</trd>
  <trdgrp lng="pt">
   <trd>forcado</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_l;&c_y;</trd>
  <trd lng="sk">vidly</trd>
  <trdgrp lng="sv">
    <trd>h&ouml;gaffel</trd>,
    <trd>grep</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_i;&c_l;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#31914;&#32793; <pr>f&egrave;nb&agrave;</pr></trd>,
    <trd>&#25506;&#21548;&#19985;&#38395; <pr>t&agrave;nt&imacron;ngch&#466;uw&eacute;n</pr></trd>,
    <trd>&#31914;&#21449;&#23376; <pr>f&egrave;nch&#257;zi</pr></trd>,
    <trd>&#22534;&#32933;&#29992;&#32793; <pr>du&imacron;f&eacute;iy&ograve;ngb&agrave;</pr></trd>,
    <trd>&#25581;&#24320;&#19985;&#38395; <pr>ji&#275;k&#257;ich&#466;uw&eacute;n</pr></trd>,
    <trd>&#25581;&#21457;&#19985;&#38395; <pr>ji&#275;f&#257;ch&#466;uw&eacute;n</pr></trd>,
    <trd>&#33609;&#32793; <pr>c&#462;ob&agrave;</pr></trd>,
    <trd>&#24178;&#33609;&#21449; <pr>g&agrave;nc&#462;och&#257;</pr></trd>,
    <trd>&#26440; <pr>ch&#257;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fork.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fork.0igxi.disbrancxigxi">
    <dif>
      <ref tip="sin" cel="brancx.dis0igxi">Disbran&ccirc;i&gcirc;i</ref> kiel <tld/>o:
      <ekz>
        la kanalo dis<tld/>i&gcirc;is la&ubreve; du direktoj;
      </ekz>
      <ekz>
        viroj normale havas pli grandajn tra&hcirc;eojn kaj <tld/>i&gcirc;antajn
        bronkojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24230">Sekso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        river<tld/>i&gcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3641">Rivero</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_g;&c_a;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
    <trd lng="de"><klr>sich </klr>gabeln</trd>
    <trdgrp lng="en">
      <trd>bifurcate</trd>,
      <trd>branch (off)</trd>,
      <trd>split (off)</trd>
    </trdgrp>
    <trd lng="es">bifurcarse</trd>
    <trdgrp lng="fa">
      <trd>&a_d;&a_w;&a_sh;&a_A;&a_kh;&a_h; &a_sh;&a_d;&a_n;</trd>,
      <trd>&a_m;&a_n;&a_sh;&a_ayn;&a_b; &a_sh;&a_d;&a_n;</trd>,
      <trd>&a_A;&a_n;&a_sh;&a_ayn;&a_A;&a_b; &f_y;&a_A;&a_f;&a_t;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>bifurquer</trd>,
      <trd>fourcher<klr tip="ind"> (intr.)</klr></trd>
    </trdgrp>
    <trd lng="hu">el&aacute;gazik</trd>
    <trd lng="nl">afsplitsen</trd>
    <trdgrp lng="pl">
      <trd>rozwidla&cacute; si&eogonek;</trd>,
      <trd>rozwidli&cacute; si&eogonek;</trd>,
      <trd>rozga&lstroke;&eogonek;zia&cacute; si&eogonek;</trd>,
      <trd>rozga&lstroke;&eogonek;zi&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_t;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_d;&c_v;&c_o;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_g;&c_a;&c_l;&c_u;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#21457;&#26525; <pr>f&#257;zh&imacron;</pr></trd>
</drv>

<drv mrk="fork.trak0o">
  <kap>
    trak<tld/>o
  </kap>
  <snc mrk="fork.trak0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Komutebla parto de <ref tip="super" cel="trak.0o.FER">trako</ref>
      ebliganta konduki veturilon al alia trako:
      <ekz>
        sur la 109-hektarojn granda tereno esti&gcirc;os la nova &ccirc;efa stacidomo
        <klr>[&#8230;kun]</klr> 100&#8239;km da trakoj, 300 trakforkoj kaj 10 kajoj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2011/010483.php"
              >Trajnojn trak-tante</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
