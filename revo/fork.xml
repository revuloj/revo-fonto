<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fork</rad>/o
</kap>
<drv mrk="fork.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Ilo provizita &ccirc;e sia ekstrema&jcirc;o per du a&ubreve; pluraj longaj dentoj,
      k servanta por piki ion, kion oni volas preni k levi. 
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ib;&c_l;&c_y;</trd>,
      <trd>&c_v;&c_ib;&c_d;&c_e;&c_l;&c_je;&c_c;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_i;&c_l;&c_y;</trd>,
      <trd>&c_v;&c_i;&c_l;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Parto de io, kio disbran&ccirc;i&gcirc;as, kiel du dentoj de forko:
      <ekz>
        <tld/>o de arbo, bran&ccirc;o, relo;
      </ekz>
      <ekz>
        voj<tld/>o <klr>(vojkruci&gcirc;o)</klr>
        <trd lng="be">&c_r;&c_o;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="brak.0o">brako</ref>,
      <ref cel="brancx.0o">bran&ccirc;o</ref>,
      <ref cel="krucx.0o">kru&ccirc;o</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_ib;&c_l;&c_k;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_g;&c_a;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_r;&c_o;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_l;&c_k;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_t;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">vidli&ccaron;ka</trd>
  <trd lng="en">fork</trd>
  <trd lng="fr">fourche</trd>
  <trd lng="de">Gabel</trd>
  <trd lng="nl">vork</trd>
  <trd lng="pl">widelec</trd>
  <trd lng="tr">&ccedil;atal</trd>
</drv>
<drv mrk="fork.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Sama, pli malgranda, ilo, uzata por man&gcirc;ado. 
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_d;&c_e;&c_l;&c_je;&c_c;</trd>
  <trd lng="fr">fourchette</trd>
  <trd lng="nl">vorkje</trd>
  <trd lng="ru">&c_v;&c_i;&c_l;&c_k;&c_a;</trd>
</drv>
<drv mrk="fork.0ego">
  <kap><tld/>ego</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Sama ilo, kun fortikaj dentoj, uzata por levi fojnon, garbon,
      herbofaskon kc.  
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>pitch fork</trd>,
    <trd>hay fork</trd>
  </trdgrp>
  <trd lng="fr">fourche</trd>
  <trd lng="nl">riek</trd>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_h;&c_o;&c_r; <klr>(&c_d;&c_l;&c_ja; &c_n;&c_a;&c_v;&c_o;&c_z;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_l;&c_y;</trd>
</drv>
<drv mrk="fork.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Disbran&ccirc;i&gcirc;i kiel <tld/>o:
      <ekz>la kanalo dis<tld/>i&gcirc;is la&ubreve; du direktoj.</ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_g;&c_a;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
    <trdgrp lng="en">
      <trd>bifurcate</trd>,
      <trd>branch (off)</trd>,
      <trd>split (off)</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_t;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
</drv>
<drv mrk="fork.son0eto">
  <kap>son<tld/>eto</kap>
  <snc>
    <dif>
      Diapazono.  
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_e;&c_r;&c_t;&c_o;&c_n;</trd>
  <trd lng="de">Stimmgabel</trd>
  <trd lng="en">tuning fork</trd>
  <trd lng="nl">stemvork</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_r;&c_t;&c_o;&c_n;</trd>
  <trd lng="tr">diyapazon</trd>
</drv>
</art>
