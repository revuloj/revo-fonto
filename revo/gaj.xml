<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>gaj</rad>/a</kap>
<drv mrk="gaj.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Estanta en viva, facilanima kaj bonhumora stato, kaj montranta
      sian kontentecon per eksteraj signoj; ridhumora, &gcirc;ojmiena:
      <ekz>
        <tld/>a homo, <tld/>a urbo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e vivi;
      </ekz>
      <ekz>
        <ind>mal<tld/>a</ind>
        <trdgrp lng="be">
          <trd>&c_zh;&c_u;&c_r;&c_b;&c_o;&c_t;&c_n;&c_y;</trd>,
          <trd>&c_s;&c_u;&c_m;&c_n;&c_y;</trd>,
          <trd>&c_s;&c_m;&c_u;&c_t;&c_n;&c_y;</trd>,
          <trd>&c_m;&c_a;&c_r;&c_k;&c_o;&c_t;&c_n;&c_y;</trd>,
          <trd>&c_n;&c_u;&c_d;&c_n;&c_y;</trd>,
          <trd>&c_t;&c_u;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>,
          <trd>&c_p;&c_a;&c_n;&c_y;&c_l;&c_y;</trd>,
          <trd>&c_p;&c_a;&c_n;&c_u;&c_r;&c_y;</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>triste</trd>,
          <trd>attrist&eacute;</trd>,
          <trd>morose</trd>,
          <trd>pein&eacute;</trd>
        </trdgrp>
        <trdgrp lng="nl">
          <trd>triest</trd>,
          <trd>bedroefd</trd>,
          <trd>verdrietig</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_p;&c_je;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
        spirito;
      </ekz>
      <ekz>
        morte mal<tld/>a<fnt>Z</fnt>
      </ekz>
      .
    </dif>
    <refgrp tip="vid">
      <ref cel="gxoj.0a">&gcirc;oja</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Ka&ubreve;zanta tian staton, <tld/>iga:
      <ekz>
        <tld/>a kanto, vetero, koloro, rakonto, spektaklo;
      </ekz>
      <ekz>
        sur la kameno brulis <tld/>a fajro<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_s;&c_jo;&c_l;&c_y;</trd>
  <trd lng="cs">vesel&yacute;</trd>
  <trdgrp lng="de">
    <trd>fr&ouml;hlich</trd>,
    <trd>heiter</trd>,
    <trd>lustig</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>blij</trd>,
    <trd>vrolijk</trd>
  </trdgrp>
  <trd lng="en">gay</trd>
  <trdgrp lng="fr">
    <trd>gai</trd>,
    <trd>enjou&eacute;</trd>,
    <trd>joyeux</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_jo;&c_l;&c_y;&c_j;</trd>
  <trd lng="sv">glad</trd>
</drv>

<drv mrk="gaj.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a.
    </dif>
    <refgrp tip="vid">
      <ref cel="rid.0i">ridi</ref>,
      <ref cel="plezur">plezuri</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_s;&c_ja;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="fr">&ecirc;tre <ind>gai</ind></trd>
  <trd lng="nl">blij zijn</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_v; &c_h;&c_o;&c_r;&c_o;&c_sh;&c_je;&c_m; &c_n;&c_a;&c_s;&c_t;&c_r;&c_o;&c_je;&c_n;&c_i;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="gaj.mal0i">
  <kap>mal<tld/>i</kap>
  <snc>
    <dif>
      Esti mal<tld/>a.
    </dif>
    <refgrp tip="vid">
      <ref cel="funebr">funebri</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_m;&c_u;&c_t;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_k;&c_o;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_n;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>triste</ind></trd>,
    <trd>s'<ind>attrister</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bedroefd zijn</trd>,
    <trd>treurig zijn</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_ch;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="gaj.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari iun a&ubreve; ion <tld/>a:
      <ekz>
        <tld/>igi &ccirc;iujn per spritaj &scirc;ercoj;
      </ekz>
      <ekz>
        <tld/>igi la lecionon;
      </ekz>
      <ekz>
        <tld/>igu vin<fnt>Z</fnt>;
      </ekz>
      <ekz>
        grimaca <tld/>igista viza&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_s;&c_ja;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;gayer<klr tip="ind"> (rendre gai)</klr></trd>,
    <trd>distraire</trd>,
    <trd>divertir</trd>,
    <trd>r&eacute;jouir</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>blij maken</trd>,
    <trd>leuk maken</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="gaj.0iga">
  <kap><tld/>iga</kap>
  <snc>
    <dif>
      <tld lit="G"/>iganta:
      <ekz>
        <tld/>iga spektaklo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komik.0a">komika</ref>,
      <ref cel="burles.0a">burleska</ref>,
      <ref cel="rid">ridiga</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>distrayant</trd>,
    <trd>divertissant</trd>,
    <trd>r&eacute;jouissant</trd>
  </trdgrp>
  <trd lng="nl">leuk</trd>
</drv>
</art>
