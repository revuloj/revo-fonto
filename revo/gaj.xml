<drv mrk="gaj.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      En malbona humoro, ne &gcirc;uanta la cirkonstancojn:
      <ekz>
        senforti&gcirc;intaj korpoj kaj morte mal<tld/>aj spiritoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu rigardis sin per tiel neordinare mal<tld/>aj
        okuloj, ke la koro ne permesis al &scirc;i ekbati
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
        <trd>&c_zh;&c_u;&c_r;&c_b;&c_o;&c_t;&c_n;&c_y;</trd>,
        <trd>&c_s;&c_u;&c_m;&c_n;&c_y;</trd>,
        <trd>&c_s;&c_m;&c_u;&c_t;&c_n;&c_y;</trd>,
        <trd>&c_m;&c_a;&c_r;&c_k;&c_o;&c_t;&c_n;&c_y;</trd>,
        <trd>&c_n;&c_u;&c_d;&c_n;&c_y;</trd>,
        <trd>&c_t;&c_u;&c_zh;&c_l;&c_ib;&c_v;&c_y;</trd>,
        <trd>&c_p;&c_a;&c_n;&c_y;&c_l;&c_y;</trd>,
        <trd>&c_p;&c_a;&c_n;&c_u;&c_r;&c_y;</trd>
      </trdgrp>
  <trd lng="cs">smutn&yacute;</trd>
  <trd lng="es">triste</trd>
  <trdgrp lng="fr">
        <trd>triste</trd>,
        <trd>attrist&eacute;</trd>,
        <trd>morose</trd>,
        <trd>pein&eacute;</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>szomor&uacute;</trd>,
        <trd>b&uacute;s</trd>,
        <trd>b&aacute;natos</trd>
      </trdgrp>
  <trdgrp lng="id">
    <trd>gundah</trd>,
    <trd>masygul</trd>,
    <trd>muram</trd>,
    <trd>murung</trd>,
    <trd>pilu</trd>,
    <trd>sedih</trd>,
    <trd>sendu</trd>
  </trdgrp>
  <trdgrp lng="it">
        <trd>triste</trd>,
        <trd>corrucciato</trd>,
        <trd>fosco</trd>,
        <trd>funereo</trd>,
       </trdgrp>
  <trdgrp lng="ja">
