  <trdgrp lng="id">
    <trd><baz>gembira</baz>menggembirakan <klr>(adj)</klr></trd>,
    <trd><baz>senang</baz>menyenangkan <klr>(adj)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
        <trd>gaio</trd>,
        <trd>gioioso</trd>
       </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x697D;&#12375;&#x304F;&#12377;&#x308B;  <pr>&#x305F;&#x306E;&#12375;&#x304F;&#12377;&#x308B;</pr></trd>,
    <trd>&#x6C17;&#26228;&#12425;&#12375;&#x306B;&#x306A;&#x308B;  <pr>&#x304D;&#12400;&#12425;&#12375;&#x306B;&#x306A;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">leuk</trd>
  <trd lng="pt">alegre <klr tip="amb"> (que causa alegria)</klr><klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="ru">
        <trd>&c_v;&c_je;&c_s;&c_je;&c_l;&c_ja;&c_shch;&c_i;&c_j;</trd>,
        <trd>&c_u;&c_v;&c_je;&c_s;&c_je;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_r;&c_a;&c_z;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
  <trd lng="sk">rozvese&lcaron;uj&uacute;ci</trd>
  <trdgrp lng="tr">
       <trd>ne&scedil;elendiren</trd>,
       <trd>e&gbreve;lendiren</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_v;&c_je;&c_s;&c_je;&#x43B;&#x44F;&#x44E;&c_ch;&c_i;&c_j;</trd>
