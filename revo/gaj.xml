<drv mrk="gaj.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="gaj.0a.sente">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Estanta en viva, facilanima kaj bonhumora stato, kaj
      montranta sian kontentecon per eksteraj signoj;
      ridhumora, &gcirc;ojmiena:
      <ekz>
        la studento estis <tld/>a kaj &gcirc;oja, &ccirc;armega homo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinjoro kun tia <tld/>a viza&gcirc;o, ke <klr>[...]</klr>
        li devis esti la plej feli&ccirc;a homo sur la tero
        <fnt>
          <bib>Fab4</bib>
          <lok>Pupludisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun <tld/>a rideto sur la bu&scirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi ricevis plej <tld/>an humoron
        <fnt>
          <bib>Fab4</bib>
          <lok>Pupludisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a urbo
        <fnt>
          <bib>MT</bib>
          <lok>Cefanja 2:15</lok>
        </fnt>
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 49:25</lok>
        </fnt>
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 23:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni estu <tld/>aj, ni uzu bone la vivon, &ccirc;ar la
        vivo ne estas longa
        <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>;
      </ekz>
      <ekz>
        li <klr>[...]</klr> vivis tie lukse kaj <tld/>e
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxoj.0a">&gcirc;oja</ref>.
    </refgrp>
   <trdgrp lng="it">
    <trd>gaio</trd>,
    <trd>allegro</trd>,
    <trd>gioviale</trd>
   </trdgrp>
    <trdgrp lng="pt">
      <trd>alegre</trd>,
      <trd>contente</trd>,
      <trd>feliz</trd>
    </trdgrp>
  </snc>
  <snc mrk="gaj.0a.gajiga">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="klr">(io)</uzo>
    <dif>
      <ref tip="dif" cel="gaj.0iga"><tld lit="G"/>iga</ref>:
      <ekz>
        <tld/>a vetero, koloro, spektaklo;
      </ekz>
      <ekz>
        pro forta brak', / pro <tld/>a kant' / volonte akceptata
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rakonto ne estas <tld/>a; kaj tio mal&gcirc;ojigis
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la kameno brulis <tld/>a fajro
          <fnt>
            <bib>Marta</bib>
          </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="it">
    <trd>gaio</trd>,
    <trd>gioioso</trd>
   </trdgrp>
    <trd lng="pt">alegre</trd>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_s;&c_jo;&c_l;&c_y;</trd>
  <trd lng="bo">&#xF41;&#xFB1;&#xF51;&#xF0B;&#xF58;&#xF5A;&#xF62;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trd lng="cs">vesel&yacute;</trd>
  <trdgrp lng="de">
        <trd>fr&ouml;hlich</trd>,
        <trd>heiter</trd>,
        <trd>lustig</trd>
      </trdgrp>
  <trd lng="en">gay</trd>
  <trd lng="es">alegre</trd>
  <trdgrp lng="fr">
        <trd>gai</trd>,
        <trd>enjou&eacute;</trd>,
        <trd>joyeux</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>v&iacute;g</trd>,
        <trd>vid&aacute;m</trd>
      </trdgrp>
  <trdgrp lng="id">
    <trd>ceria</trd>,
    <trd>gembira</trd>,
    <trd>girang</trd>,
    <trd>ria</trd>,
    <trd>riang</trd>,
    <trd>senang</trd>,
    <trd><ind>senang</ind> hati</trd>,
    <trd><ind>suka</ind> hati</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x967D;&#x6C17;&#x306A;  <pr>&#12424;&#12358;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x5FEB;&#x6D3B;&#x306A;  <pr>&#x304B;&#12356;&#x304B;&#12388;&#x306A;</pr></trd>,
    <trd>&#24841;&#x5FEB;&#x306A;  <pr>&#12422;&#x304B;&#12356;&#x306A;</pr></trd>,
    <trd>&#x306B;&#x304E;&#12420;&#x304B;&#x306A;</trd>,
    <trd>&#x6D6E;&#x304D;&#x6D6E;&#x304D;&#12375;&#x305F;  <pr>&#12358;&#x304D;&#12358;&#x304D;&#12375;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
        <trd>blij</trd>,
        <trd>vrolijk</trd>
      </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_jo;&c_l;&c_y;&c_j;</trd>
  <trd lng="sk">vesel&yacute;</trd>
  <trd lng="sv">glad</trd>
  <trdgrp lng="sw">
    <trd>-enye furaha</trd>,
    <trd>-enye hali ya kuchekacheka</trd>
  </trdgrp>
  <trd lng="tr">ne&scedil;eli</trd>
  <trd lng="uk">&c_v;&c_je;&c_s;&c_je;&#x43B;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#24660; <pr>xi&#xE0;o</pr></trd>,
    <trd>&#x59C1; <pr>x&#x1D4;</pr></trd>,
    <trd>&#24709; <pr>yu&#xE8;</pr></trd>,
    <trd>&#x6B22;&#x4E50; <pr>hu&amacron;nl&#xE8;</pr></trd>,
    <trd>&#26210; <pr>zh&#xE9;</pr></trd>,
    <trd>&#26129; <pr>q&#x1D0;n</pr></trd>,
    <trd>&#20809;&#20809; <pr>gu&amacron;nggu&amacron;ng</pr></trd>,
    <trd>&#x4E50;&#x4E50;&#21621;&#21621; <pr>l&#xE8;lehehe</pr></trd>,
    <trd>&#x9AD8;&#20852;&#30340; <pr>g&amacron;ox&#xEC;ngde</pr></trd>,
    <trd>&#x61FD; <pr>hu&amacron;n</pr></trd>,
    <trd>&#x548D; <pr>h&amacron;i</pr></trd>,
    <trd>&#x65E0;&#x5FE7;&#x65E0;&#34385; <pr>w&#xFA;y&#x14D;uw&#xFA;l&#xFC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gaj.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a:
      <ekz>
        <klr>[ili]</klr> bu&ccirc;is tie pacoferojn anta&ubreve; la Eternulo,
        kaj forte <tld/>is tie Saul kaj &ccirc;iuj Izraelidoj
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 11:15</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rid.0i">ridi</ref>,
      <ref cel="plezur">plezuri</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_s;&c_ja;&c_l;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="cs">veselit se</trd>
  <trdgrp lng="de">
        <trd>fr&ouml;hlich sein</trd>,
        <trd>heiter sein</trd>,
        <trd>sich <ind>freuen</ind></trd>
      </trdgrp>
  <trd lng="es">estar <ind>alegre</ind></trd>
  <trd lng="fr">&ecirc;tre <ind>gai</ind></trd>
  <trdgrp lng="hu">
        <trd>vid&aacute;m</trd>,
        <trd>&ouml;rvendezik</trd>
      </trdgrp>
  <trdgrp lng="id">
    <trd><baz>gembira</baz>bergembira</trd>,
    <trd><baz>girang</baz>bergirang</trd>,
    <trd><baz>ria</baz>beria</trd>,
    <trd><baz>senang</baz>bersenang hati</trd>,
    <trd><baz>suka</baz>bersuka hari</trd>,
    <trd><baz>sukacita</baz>bersukacita</trd>,
    <trd><baz>sukaria</baz>bersukaria</trd>
  </trdgrp>
  <trdgrp lng="it">
        <trd>gioire</trd>,
        <trd>essere <ind>allegro</ind></trd>
      </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24841;&#x5FEB;&#x306A;&#x6C17;&#25345;&#12385;&#12391;&#12356;&#x308B; 
    <pr>&#12422;&#x304B;&#12356;&#x306A;&#x304D;&#12418;&#12385;&#12391;&#12356;&#x308B;</pr></trd>,
    <trd>&#26228;&#x308C;&#26228;&#x308C;&#12375;&#12390;&#12356;&#x308B;  <pr>&#x306F;&#x308C;&#12400;&#x308C;&#12375;&#12390;&#12356;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">blij zijn</trd>
  <trdgrp lng="pt">
        <trd>estar <ind>alegre</ind></trd>,
        <trd>estar de <ind>bom humor</ind></trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_v;&c_je;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_b;&c_y;&c_t;&c_mol; &c_v; &c_h;&c_o;&c_r;&c_o;&c_sh;&c_je;&c_m;
          <ind>&c_n;&c_a;&c_s;&c_t;&c_r;&c_o;&c_je;&c_n;&c_i;&c_i;</ind></trd>
      </trdgrp>
  <trd lng="sk">by&tcaron; vesel&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_s;&c_je;&#x43B;&c_i;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_v; &c_g;&c_a;&c_r;&#x43D;&#x43E;&#x43C;&c_u; &#x43D;&c_a;&c_s;&c_t;&c_r;&#x43E;&c_ji;</trd>
  </trdgrp>
</drv>

<drv mrk="gaj.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari iun a&ubreve; ion <tld/>a:
      <ekz>
        <tld/>igi &ccirc;iujn per spritaj &scirc;ercoj;
      </ekz><ekz>
        <tld/>igi la lecionon;
      </ekz>
      <ekz>
        mi volas <tld/>igi aliajn kaj anka&ubreve; min
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi konsolos ilin kaj <tld/>igos ilin post ilia aflikto
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 31:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        se iu anta&ubreve; nelonge edzi&gcirc;is, li ne iru en militon <klr>[...]</klr>
        li restu libera en sia domo dum unu jaro, kaj li <tld/>igu sian edzinon
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 24:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        por plezuro oni aran&gcirc;as festenojn, kaj vino <tld/>igas la vivon
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 10:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        grimaca <tld/>igista viza&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_s;&c_ja;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>bavit</trd>,
    <trd>rozveselovat</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>erheitern</trd>,
        <trd>aufheitern</trd>
      </trdgrp>
  <trd lng="es">alegrar</trd>
  <trdgrp lng="fr">
        <trd>&eacute;gayer<klr tip="ind"> (rendre gai)</klr></trd>,
        <trd>divertir</trd>,
        <trd>r&eacute;jouir</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>felvid&iacute;t</trd>,
        <trd>vid&aacute;m&iacute;t</trd>
      </trdgrp>
  <trdgrp lng="id">
    <trd><baz>gembira</baz>menggembirakan</trd>,
    <trd><baz>girang</baz>menggirangkan</trd>,
    <trd><baz>riang</baz>meriangkan</trd>,
    <trd><baz>senang</baz>menyenangkan</trd>
  </trdgrp>
  <trdgrp lng="it">
        <trd>allietare</trd>,
        <trd>rallegrare</trd>
      </trdgrp>
  <trd lng="ja">&#x697D;&#12375;&#x304F;&#12377;&#x308B;  <pr>&#x305F;&#x306E;&#12375;&#x304F;&#12377;&#x308B;</pr></trd>
  <trdgrp lng="nl">
        <trd>blij maken</trd>,
        <trd>leuk maken</trd>
      </trdgrp>
  <trd lng="pt">alegrar</trd>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">rozveseli&tcaron;</trd>
  <trdgrp lng="tr">
        <trd>ne&scedil;elendirmek</trd>,
        <trd>e&gbreve;lendirmek</trd>
      </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_s;&c_je;&#x43B;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_v;&c_je;&c_s;&c_je;&#x43B;&#x44F;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_v;&c_je;&c_s;&c_je;&#x43B;&#x44F;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x70ED;&#x95F9; <pr>r&#xE8;nao</pr></trd>
</drv>

<drv mrk="gaj.0iga">
  <kap><tld/>iga</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta <tld/>on, bonan humoron, ridon:
      <ekz>
        <tld/>iga spekta&jcirc;o;
      </ekz>
      <ekz>
        trinki <klr>[...]</klr> <tld/>igajn trinka&jcirc;ojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>.
      </ekz>

    </dif>
    <refgrp tip="vid">
      <ref cel="burles.0a">burleska</ref>,
      <ref cel="komik.0a">komika</ref>,
      <ref cel="rid">ridiga</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_je;&c_sh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_ja;&c_s;&c_jo;&c_l;&c_y;</trd>
