<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: septem.xml,v 1.35 2021/05/29 14:33:13 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>Septembr</rad>/o,
  <var><kap><rad var="s">septembr</rad>/o</kap></var>
</kap>
<drv mrk="septem.0o">
  <kap><ofc>*</ofc><tld/>o,
  <var><kap><tld var="s"/>o</kap></var></kap>
  <snc mrk="septem.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Na&ubreve;a <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj" val="9">monato</ref> de la <ref tip="malprt" cel="jar.0o.KAL">jaro</ref> la&ubreve; la Gregoria kalendaro, inter
      <ref cel="auxgus.0o">A&ubreve;gusto</ref> kaj <ref cel="oktobr.0o">Oktobro</ref>:
      <ekz>
        la dek du monatoj de l' kristana jaro estas: Januaro, Februaro,
        <klr>[&#x2026;]</klr> A&ubreve;gusto, <tld/>o, Oktobro, Novembro, Decembro
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la 25-an de <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        estis fino de <tld/>o, estis diman&ccirc;o kaj suna vetero
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis bela stelluma nokto, la aero ankora&ubreve; varma kaj milda, &ccirc;ar
        tiam estis ankora&ubreve; <tld var="s"/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Infano en la tombo</lok>
        </fnt>;
      </ekz>
      <ekz>
        de junio &gcirc;is la fino de <tld var="s"/>o, Nilo levi&gcirc;as kaj superver&scirc;as
        preska&ubreve; la tutan Egipton
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">Januaro, Rim.</ref>
  <trd lng="af">September</trd>
  <trd lng="be">&c_v;&c_je;&c_r;&c_a;&c_s;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_s;&c_je;&c_p;&c_t;&c_je;&c_m;&c_v;&c_r;&c_i;</trd>
  <trd lng="bo">&#xF5F;&#xFB3;&#xF0B;&#xF56;&#xF0B;&#xF51;&#xF42;&#xF74;&#xF0B;&#xF54;&#xF0B;</trd>
  <trd lng="br">Gwengolo</trd>
  <trd lng="cs">z&aacute;&rcaron;&iacute;</trd>
  <trd lng="de">September</trd>
  <trd lng="en">September</trd>
  <trdgrp lng="es">
    <trd>septiembre</trd>,
    <trd>setiembre</trd>
  </trdgrp>
  <trd lng="fr">septembre</trd>
  <trd lng="fy">septimber</trd>
  <trd lng="he">&samekh;&pe;&tet;&mem;&bet;&resh;</trd>
  <trd lng="hu">szeptember</trd>
  <trd lng="id">September</trd>
  <trd lng="io">septembro</trd>
  <trd lng="it">settembre</trd>
  <trd lng="ja">&#x4E5D;&#x6708; [<ind>&#x304F;&#x304C;&#x3064;</ind>]</trd>
  <trdgrp lng="jbo">
    <trd>sozma'i</trd>,
    <trd>xlima'i</trd>
  </trdgrp>
  <trd lng="nl">september</trd>
  <trd lng="pl">wrzesie&nacute;</trd>
  <trd lng="pt">setembro</trd>
  <trd lng="ro">septembrie</trd>
  <trd lng="ru">&c_s;&c_je;&c_n;&c_t;&c_ja;&c_b;&c_r;&c_mol;</trd>
  <trd lng="sk">september</trd>
  <trd lng="sv">september</trd>
  <trd lng="tp">tenpo mun nanpa luka tu tu</trd>
  <trd lng="tr">eyl&uuml;l</trd>
  <trd lng="vo">setul</trd>
</drv>

<drv mrk="septem.0a">
  <kap><tld lit="s"/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o:
      <ekz>
        en iu trankvila <tld lit="s"/>a tago
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007215.html">La peko
          de Ros</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="s"/>a akceptovizito de nova kroata ambasadoro
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="https://www.monato.be/2004/008453.html">Prezidanto
          postulas, prezidanto rifuzas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        leginte en la a&ubreve;gusta-<tld lit="s"/>a numero
        <fnt>
          <bib>Monato</bib>
          <aut>Manolo Parra</aut>
          <vrk><url ref="&Monato;/2010/010184.php">Ne tiel
          eksterordinara</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        atendi la rezultojn de la <tld lit="s"/>a rikolto
        <fnt>
          <bib>Monato</bib>
          <aut>Joyce Bunting</aut>
          <vrk><url ref="&Monato;/2013/010846.php">Taktiko
          por noveca tritiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="io">septembrala</trd>
  <trd lng="ja">&#x4E5D;&#x6708;&#x306E; [<ind>&#x304F;&#x304C;&#x3064;&#x306E;</ind>]</trd>
  <trd lng="vo">setulik</trd>
</drv>
</art>
