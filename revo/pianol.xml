<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pianol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pianol.0o">
  <kap><tld/>o</kap>
  <snc mrk="pianol.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.kord0o">Kordinstrumento</ref>,
      a&ubreve;tomata piano sonigata de me&hcirc;anismo:
      <ekz>
        li okupi&gcirc;u munti kaj agordi la <tld/>on kaj instrui kiel &gcirc;i 
        funkcias kaj kiel oni dancu la&ubreve; la enmoda muziko presita sur 
        ses paper-ruloj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gurd.0o.MUZ">gurdo</ref>
    </refgrp>
  </snc>
  <trd lng="fr">piano m&eacute;canique</trd>
  <trd lng="it">pianola</trd>
  <trd lng="nl">pianola</trd>
</drv>
</art>

