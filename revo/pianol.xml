<art mrk="$Id: pianol.xml,v 1.10 2024/02/17 06:43:07 revo Exp $">
<kap>
  <rad>pianol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pianol.0o">
  <kap><tld/>o</kap>
  <snc mrk="pianol.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.kord0o.MUZ" lst="voko:kordinstrumentoj">Kordinstrumento</ref>,
      a&ubreve;tomata piano sonigata de me&hcirc;anismo:
      <ekz>
        li okupi&gcirc;u munti kaj agordi la <tld/>on kaj instrui kiel &gcirc;i 
        funkcias kaj kiel oni dancu la&ubreve; la enmoda muziko presita sur 
        ses paper-ruloj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gurd.0o.MUZ">gurdo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_ja;&c_n;&c_o;&c_l;&c_a;</trd>
  <trd lng="cs">pianola</trd>
  <trd lng="de">Pianola</trd>
  <trd lng="fr">piano m&eacute;canique</trd>
  <trd lng="it">pianola</trd>
  <trd lng="nl">pianola</trd>
  <trd lng="pl">pianola</trd>
  <trd lng="ro">pianol&abreve;</trd>
  <trd lng="sk">pianola</trd>
  <trd lng="uk">&#x43F;&#x456;&#x430;&#x43D;&#x43E;&#x43B;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x81EA;&#x52A8;&#x94A2;&#x7434; <pr>z&#xEC;d&#xF2;ngg&#x101;ngq&#xED;n</pr></trd>,
    <trd>&#x81EA;&#x52D5;&#x92FC;&#x7434; <pr>z&#xEC;d&#xF2;ngg&#x101;ngq&#xED;n</pr></trd>
  </trdgrp>
</drv>
</art>

