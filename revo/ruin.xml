<drv mrk="ruin.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="ruin.0ajxo.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      <ref tip="dif" cel="ruin.0o.ARKE">Ruino<sncref/></ref>:
      <ekz>
        Jerusalem fari&gcirc;os <tld/>a&jcirc;o
        <fnt><bib>MT</bib>, <lok>&Mih; 3:12</lok></fnt>;
      </ekz>
      <ekz>
         tertremaj <tld/>a&jcirc;o
        <fnt>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2007/009090.php">Kion signifas la nocio la&ubreve;nature?</url></vrk>
          <bib>Monato</bib>
          <lok>2007/01, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> ser&ccirc;is por si vojon tra la <tld/>a&jcirc;oj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Vo&ccirc;o de la Kvieto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_u;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">ruine<klr> (b&acirc;timent)</klr></trd>
  <trd lng="hu">omlad&eacute;k</trd>
  <trd lng="ja">&#x6B8B;&#x9AB8;  <pr>&#x3056;&#x3093;&#x304C;&#x3044;</pr></trd>
  <trd lng="pl">zwalisko</trd>
  <adm>
    Komparu: detruita&jcirc;oj (Z), ruin&scirc;tonoj (Z)
  </adm>
  <trdgrp lng="zh">
    <trd>&#x5E9F;&#x589F; <pr>f&#xE8;ix&#x16B;</pr></trd>,
    <trd>&#x5EE2;&#x589F; <pr>f&#xE8;ix&#x16B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="ruin.0igi">
  <kap><ofc>*</ofc><tld/>igi <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5186">ruin'ig'</url></lok></fnt></kap>
  <snc mrk="ruin.0igi.ARKI">
    <uzo tip="klr">(pri konstrua&jcirc;o)</uzo>
    <dif>
      Disfaligi en <tld/>ojn, renversi, duondetrui:
      <ekz>
        tertremo <tld/>igis la urbon;
      </ekz>
      <ekz>
        mia tendo estas <tld/>igita
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 10:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        pa&scirc;tistoj <tld/>igis mian vinber&gcirc;ardenon
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 12:10</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">lerombol</trd>
  </snc>
  <snc mrk="ruin.0igi.detrui">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Detrui, pereigi:
      <ekz>
        per interna malpaco ni <tld/>igus nian aferon
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni decidis malhonorigi kaj <tld/>igi Egipton
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akcepto de via tre duba kaj balda&ubreve; siavice kritikota plibona&jcirc;o
        <tld/>igus la laboron de dudekjara disciplina kaj sukcesa laborado
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igantoj de virina honoro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni ne uzas ma&scirc;inojn, kiuj tiel terure <tld/>igas la fortojn
        kaj difektas la sanon de la laborantoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la konstantaj ribeloj <tld/>igis la a&ubreve;toritaton de la sultano;
      </ekz>
      <ekz>
        kun nervoj <tld/>igitaj de la &ccirc;iutaga eksciti&gcirc;o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekster1.0i">ekstermi</ref>,
      <ref cel="konsum.0i">konsumi</ref>,
      <ref cel="renver.0i">renversi</ref>,
      <ref cel="fos.sub0i">subfosi</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>t&ouml;nkretesz</trd>,
      <trd>leromlaszt</trd>,
      <trd>lez&uuml;lleszt</trd>
    </trdgrp>
  </snc>
  <snc mrk="ruin.0igi.senmonigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senmonigi, senhavigi, senri&ccirc;igi:
      <ekz>
        kiu <tld/>igas patron estas filo hontinda
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 19:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        tian sumon mi ne povas elspezi ne <tld/>igante min
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;tato estas <tld/>igita kaj la faraono estas en
        mizero
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igita nobelo.
      </ekz>
      <trdgrp lng="hu">
        <trd>t&ouml;nkretesz</trd>,
        <trd>cs&odblac;dbe visz</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_ja;&c_t;&c_mol;</trd>,
        <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
    </dif>
    <refgrp tip="vid">
      <ref cel="bankro.0o">bankroto</ref>,
      <ref cel="sucx.el0i">elsu&ccirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>p&rcaron;iv&eacute;st na mizinu</trd>,
    <trd>zbo&rcaron;it</trd>,
    <trd>zni&ccaron;it</trd>,
    <trd>zpusto&scaron;it</trd>,
    <trd>zruinovat</trd>
  </trdgrp>
  <trd lng="de">ruinieren</trd>
  <trd lng="fr">ruiner</trd>
  <trdgrp lng="ja">
    <trd>&#x5EC3;&#x589F;&#x306B;&#x3059;&#x308B;  <pr>&#x306F;&#x3044;&#x304D;&#x3087;&#x306B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8352;&#x5EC3;&#x3055;&#x305B;&#x308B;  <pr>&#x3053;&#x3046;&#x306F;&#x3044;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x5D29;&#x58CA;&#x3055;&#x305B;&#x308B;  <pr>&#x307B;&#x3046;&#x304B;&#x3044;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x7834;&#x7523;&#x3055;&#x305B;&#x308B;  <pr>&#x306F;&#x3055;&#x3093;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x7834;&#x6EC5;&#x3055;&#x305B;&#x308B;  <pr>&#x306F;&#x3081;&#x3064;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">ru&iuml;neren</trd>
  <trdgrp lng="pl">
    <trd>rujnowa&cacute;</trd>,
    <trd>demolowa&cacute;</trd>,
    <trd>zburzy&cacute;</trd>,
    <trd>niszczy&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol; &c_v; &c_r;&c_u;&c_i;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ni&ccaron;i&tcaron;</trd>,
    <trd>privies&tcaron; na mizinu</trd>,
    <trd>zni&ccaron;i&tcaron;</trd>,
    <trd>zruinova&tcaron;</trd>,
    <trd>zr&uacute;ca&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_j;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_i;&c_shch;&c_i;&c_t;&c_i;</trd>,
    <trd>(&#x43F;&c_r;&#x43E; &c_s;&#x43F;&#x43E;&c_r;&c_u;&c_d;&c_u; &c_shch;&c_je;) &c_v;&c_a;&#x43B;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x507E; <pr>f&#xE8;n</pr></trd>,
    <trd>&#x50E8; <pr>f&#xE8;n</pr></trd>,
