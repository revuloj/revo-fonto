<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ruin</rad>/o
</kap>
<drv mrk="ruin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="ruin.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      Resta&jcirc;o el renversita a&ubreve; disfalinta konstrua&jcirc;o a&ubreve; urbo:
      <ekz>
        sur la monteto alte stari&gcirc;as la <tld/>oj de kastelo;
      </ekz>
      <ekz>
        disfali en <tld/>ojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        mi faros la urbojn de Judujo <tld/>oj, lo&gcirc;ejo de
        &scirc;akaloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ca">ru&iuml;na</trd>
    <trd lng="de">Ruine</trd>
    <trd lng="en">ruin</trd>
    <trd lng="es">ruina</trd>
    <trd lng="hu">rom</trd>    
  </snc>
  <snc mrk="ruin.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pereo, detruiteco, senmoneco, senri&ccirc;eco:
      <ekz>
        la mondo falis de la <tld/>o de kredo<fnt>B</fnt>;
      </ekz>
      <ekz>
        tio estus nur prokrasti la <tld/>on de la &scirc;tato<fnt>B</fnt>;
      </ekz>
      <ekz>
        <ctl>Naskitaj sur la <tld/>o</ctl>
        <klr>(vivrakonto de Miyamoto Masao)</klr>
        <fnt><aut>Miyamoto Masao</aut>, <vrk>Naskitaj sur la ruino. -
        Librejo Pirato, 1976</vrk></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Ruin</trd>
    <trdgrp lng="hu">
      <trd>roml&aacute;s</trd>,
      <trd>t&ouml;nkremenetel</trd>,
      <trd>cs&odblac;d</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_u;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ruina</trd>,
    <trd>troska</trd>
  </trdgrp>
  <trd lng="es">ruina</trd>
  <trd lng="fr">ruine</trd>
  <trd lng="nl">ru&iuml;ne</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">ruin</trd>
</drv>

<drv mrk="ruin.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Disfalanta en <tld/>ojn, <tld/>igita:
      <ekz>
        <tld/>a &scirc;tuparo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="detru">duondetruita</ref>,
      <ref cel="kaduk.0a">kaduka</ref>,
      <ref cel="putr.0a">putra</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>en <ind>ruine</ind></trd>,
    <trd>ruin&eacute;<klr> (en ruine)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>romos</trd>,
    <trd>t&ouml;nkrement</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_i;&c_v;&c_sh;&c_i;&c_j;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="ruin.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="ruin.0ajxo.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <ref tip="dif" cel="ruin.0o.ARKE">ruino <sncref/></ref>
    <ekz>
      Jerusalem fari&gcirc;os <tld/>a&jcirc;o
      <fnt><bib>MT</bib>, <lok>&Mih; 3:12</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="fr">ruine<klr> (b&acirc;timent)</klr></trd>
  <trd lng="hu">omlad&eacute;k</trd>  
  <adm>
    Komparu: detruita&jcirc;oj (Z), ruin&scirc;tonoj (Z)
  </adm>
</drv>

<drv mrk="ruin.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="ruin.0igi.ARKI">
    <uzo tip="klr">(pri konstrua&jcirc;o)</uzo>
    <dif>
      Disfaligi en <tld/>ojn, renversi, duondetrui:
      <ekz>
        tertremo <tld/>igis la urbon;
      </ekz>
      <ekz>
        mia tendo estas <tld/>igita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pa&scirc;tistoj <tld/>igis mian vinber&gcirc;ardenon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">lerombol</trd>    
  </snc>
  <snc mrk="ruin.0igi.detrui">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Detrui, pereigi:
      <ekz>
        per interna malpaco ni <tld/>igus nian aferon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni decidis malhonorigi kaj <tld/>igi Egipton<fnt>B</fnt>;
      </ekz>
      <ekz>
        tio <tld/>igus la laboron de 20 jaroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igi la sanon de iu, la honoron de virino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la konstantaj ribeloj <tld/>igis la a&ubreve;toritaton de la sultano;
      </ekz>
      <ekz>
        <tld/>igitaj fortoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kun nervoj <tld/>igitaj de la &ccirc;iutaga eksciti&gcirc;o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekster1.0i">ekstermi</ref>,
      <ref cel="konsum.0i">konsumi</ref>,
      <ref cel="renver.0i">renversi</ref>,
      <ref cel="fos.sub0i">subfosi</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>t&ouml;nkretesz</trd>,
      <trd>leromlaszt</trd>,
      <trd>lez&uuml;lleszt</trd>
    </trdgrp>
  </snc>
  <snc mrk="ruin.0igi.senmonigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senmonigi, senhavigi, senri&ccirc;igi:
      <ekz>
        kiu <tld/>igas patron estas filo hontinda<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tia elspezo <tld/>igus min<fnt>B</fnt>
      </ekz>
      <ekz>
        la &scirc;tato estas <tld/>igita kaj la faraono estas en
        mizero<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>igita nobelo.
      </ekz>
      <trdgrp lng="hu">
        <trd>t&ouml;nkretesz</trd>,
        <trd>cs&odblac;dbe visz</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_ja;&c_t;&c_mol;</trd>,
        <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
    </dif>
    <refgrp tip="vid">
      <ref cel="bankro.0o">bankroto</ref>,
      <ref cel="sucx.el0i">elsu&ccirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">ruinieren</trd>
  <trd lng="fr">ruiner</trd>
  <trd lng="nl">ru&iuml;neren</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol; &c_v; &c_r;&c_u;&c_i;&c_n;&c_y;</trd>
  </trdgrp>
</drv>

<drv mrk="ruin.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      Pereo, detrui&gcirc;o materia, institucia, financa:
      <ekz>
        la malamikoj &scirc;in rigardas kaj ridas pri &scirc;ia <tld/>i&gcirc;o
        <fnt><bib>MT</bib>, <lok>&Plo; 1:7</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_d;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_h;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_sh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="de">Ruin</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;labrement</trd>,
    <trd>&eacute;croulement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;sszeoml&aacute;s</trd>,
    <trd>leroml&aacute;s</trd>,
    <trd>lez&uuml;ll&eacute;s</trd>,
    <trd>t&ouml;nkremenetel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_d;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_h;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
