<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ruin</rad>/o
</kap>
<drv mrk="ruin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Resta&jcirc;o el renversita a&ubreve; disfalinta konstruo:
      <ekz>
        sur la monteto alte stari&gcirc;as la <tld/>oj de kastelo;
      </ekz>
      <ekz>
        disfali en <tld/>ojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        mi faros la urbojn de Judujo <tld/>oj, lo&gcirc;ejo de
        &scirc;akaloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Ruine</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pereo, detrui&gcirc;o:
      <ekz>
        la mondo falis de (pro) la <tld/>o de kredo<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abism.0o">abismo</ref>.
    </refgrp>
    <trd lng="de">Ruin</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senmoni&gcirc;o, senri&ccirc;eco:
      <ekz>
        tio estus nur prokrasti la <tld/>on de la &scirc;tato<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Ruin</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>ruina</trd>,
    <trd>troska</trd>
  </trdgrp>
  <trd lng="nl">ru&iuml;ne</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">ruin</trd>
</drv>

<drv mrk="ruin.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld lit="R"/>igita, disfalanta en <tld/>ojn:
      <ekz>
        <tld/>a &scirc;tuparo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kaduk.0a">kaduka</ref>,
      <ref cel="detru">duondetruita</ref>,
      <ref cel="putr.0a">putra</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_i;&c_v;&c_sh;&c_i;&c_j;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="ruin.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <uzo tip="klr">(pp konstruo)</uzo>
    <dif>
      Disfaligi en <tld/>ojn, renversi, duondetrui:
      <ekz>
        tertremo <tld/>igis la urbon;
      </ekz>
      <ekz>
        mia tendo estas <tld/>igita<fnt>Z</fnt>;
      </ekz>
      <ekz>
        pa&scirc;tistoj <tld/>igis mian vinber&gcirc;ardenon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Detrui, pereigi:
      <ekz>
        per interna malpaco ni <tld/>igus nian aferon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni decidis malhonorigi kaj <tld/>igi Egipton<fnt>B</fnt>;
      </ekz>
      <ekz>
        tio <tld/>igus la laboron de 20 jaroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igi la sanon de iu, la honoron de virino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la konstantaj ribeloj <tld/>igis la a&ubreve;toritaton de la sultano;
      </ekz>
      <ekz>
        <tld/>igitaj fortoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kun nervoj <tld/>igitaj de la &ccirc;iutaga eksciti&gcirc;o
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ekster1.0i">ekstermi</ref>,
      <ref cel="konsum.0i">konsumi</ref>,
      <ref cel="fos.sub0i">subfosi</ref>,
      <ref cel="renver.0i">renversi</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senmonigi, senhavigi, senri&ccirc;igi:
      <ekz>
        kiu <tld/>igas patron estas filo hontinda<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tia elspezo <tld/>igus min<fnt>B</fnt>
      </ekz>
      <ekz>
        la &scirc;tato estas <tld/>igita kaj la faraono estas en
        mizero<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>igita nobelo.
      </ekz>
      <trdgrp lng="ru">
        <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_ja;&c_t;&c_mol;</trd>,
        <trd>&c_r;&c_a;&c_z;&c_o;&c_r;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
    </dif>
    <refgrp tip="vid">
      <ref cel="sucx.el0i">elsu&ccirc;i</ref>,
      <ref cel="bankro.0o">bankroto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">ruinieren</trd>
  <trd lng="nl">ru&iuml;neren</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol; &c_v; &c_r;&c_u;&c_i;&c_n;&c_y;</trd>
  </trdgrp>
</drv>

<drv mrk="ruin.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="ruin.0o">ruino 1</ref>:
      <ekz>
        Jeruzalem fari&gcirc;os <tld/>a&jcirc;o<fnt>Z</fnt>
        (Kp detruita&jcirc;oj<fnt>Z</fnt>,
        ruin&scirc;tonoj<fnt>Z</fnt>)
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="ruin.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      = <tld/>o 2 kaj 3:
      <ekz>
        kompleta financa <tld/>i&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Ruin</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_a;&c_l;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_d;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_h;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
