<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>moru</rad>/o
</kap>
<drv mrk="moru.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="gad.0o">gado</ref>
      (<trd lng="la">Gadus morhua</trd>),
      marfi&scirc;o, kiun oni man&gcirc;as precipe peklitan
      a&ubreve; sekigitan.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Kabeljau <klr>(plenkreska, nordamara)</klr></trd>,
    <trd>Dorsch <klr>(juna, baltamara)</klr></trd>
  </trdgrp>
  <trd lng="en">cod</trd>
  <trdgrp lng="fr">
    <trd>morue</trd>,
    <trd>cabillaud</trd>
  </trdgrp>
  <trd lng="es">bacalao</trd>
  <trd lng="it">merluzzo</trd>
  <trd lng="nl">kabeljauw</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_s;&c_k;&c_a;</trd>
  <trd lng="pt">bacalhau</trd>
</drv>
</art>
