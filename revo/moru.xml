<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: moru.xml,v 1.23 2024/11/11 01:53:42 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>moru</rad>/o
</kap>
<drv mrk="moru.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="gad.0o.ZOO">gado</ref>
      (<trd lng="la">Gadus morhua</trd>),
      <ref tip="lst" cel="fisx.mar0o" lst="voko:fi&scirc;oj">marfi&scirc;o</ref>
      &gcirc;is 2&#x202F;m longa kun tri dorsaj kaj du anusaj na&gcirc;iloj,
      anta&ubreve;en &scirc;ovita supra makzelo kaj fadeno &ccirc;e la malsupra makzelo
      kaj kun hela flanka linio:
      <ekz>
        <tld/>on oni man&gcirc;as precipe peklitan a&ubreve; sekigitan;
      </ekz>
      <ekz>
        sur sia vendobenko li havis kapon de <tld/>o
        <fnt>
          <bib>KrM</bib>
          <lok>18. Gaja Duopo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fi&scirc;istoj venis tien kaj pu&scirc;e hokis <tld/>ojn
        <fnt>
          <bib>VRA</bib>
          <lok>Eyvind Johnson</lok>
        </fnt>;
      </ekz>
      <ekz>
        norvegoj produktas pli ol 700&#x202F;000 tunojn da salmo, 70&#x202F;000 tunojn da truto,
        malpli grandajn kvantojn da <tld/>o, angilo, aliaj fi&scirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2015/011433.php">Krozi la&ubreve; la norvega
          marbordo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        muta kiel morta <tld/>o
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi min na&ubreve;zas, vi senmorala <tld/>o
        <fnt>
          <bib>ChV</bib>
          <lok>16</lok>
        </fnt>!
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/0f/Gadus.JPG/640px-Gadus.JPG" prm="CC BY-SA 3.0">kaptitaj <tld/>oj &ccirc;e
    Rams&oslash;y, Norvegujo
        <fnt>
          <aut>Arnstein R&oslash;nning, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Gadus.JPG">https://commons.wikimedia.org/wiki/File:Gadus.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>
  <trd lng="bg">&c_t;&c_r;&c_je;&c_s;&c_k;&c_a;</trd>
  <trd lng="cs">treska obecn&aacute;</trd>
  <trdgrp lng="de">
    <trd>Kabeljau <klr>(plenkreska, nordamara)</klr></trd>,
    <trd>Dorsch <klr>(juna, baltamara)</klr></trd>
  </trdgrp>
  <trd lng="en">Atlantic cod</trd>
  <trd lng="es">bacalao</trd>
  <trdgrp lng="fr">
    <trd>morue</trd>,
    <trd>cabillaud</trd>
  </trdgrp>
  <trd lng="hu">t&odblac;kehal</trd>
  <trd lng="io">moruo</trd>
  <trd lng="it">merluzzo</trd>
  <trdgrp lng="ja">
    <trd>&#x30BF;&#x30A4;&#x30BB;&#x30A4;&#x30E8;&#x30A6;&#x30C0;&#x30E9;</trd>,
    <trd>&#x30BF;&#x30E9;</trd>
  </trdgrp>
  <trd lng="nl">kabeljauw</trd>
  <trd lng="pl">dorsz</trd>
  <trd lng="pt">bacalhau</trd>
  <trd lng="ro">cod</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_s;&c_k;&c_a;</trd>
  <trd lng="sk">treska</trd>
  <trd lng="sv">torsk</trd>
  <trd lng="uk">&c_t;&c_r;&c_ib;&c_s;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x9C48; <pr>xu&#x11B;</pr></trd>,
    <trd>&#x9CD5; <pr>xu&#x11B;</pr></trd>
  </trdgrp>
</drv>
</art>
