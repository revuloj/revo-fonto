<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>vermut</rad>/o
</kap>
<drv mrk="vermut.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Blanka vino, en kiu estis infuzitaj amaraj vegeta&jcirc;oj.
    </dif>
    <trd lng="be">&c_v;&c_e;&c_r;&c_m;&c_u;&c_t;</trd>
    <trd lng="ca">vermut</trd>
    <trd lng="de">Wermut</trd>
    <trd lng="el">&beta;&epsilon;&rho;&my;&omikron;&ypsilon_ton;&tau;</trd>
    <trdgrp lng="en">
      <trd>vermuth</trd>,
      <trd>vermouth</trd>
    </trdgrp>
    <trd lng="fa">&a_w;&a_r;&a_m;&a_w;&a_t;</trd>
    <trd lng="fr">vermouth</trd>
    <trd lng="hu">vermut</trd>
    <trd lng="nl">vermout</trd>
    <trd lng="pl">wermut</trd>
    <trd lng="pt">vermute</trd>
    <trd lng="ru">&c_v;&c_je;&c_r;&c_m;&c_u;&c_t;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Maldol&ccirc;a&jcirc;o:
      <ekz>
        la ju&gcirc;on vi faras <tld/>o, kaj la veron vi &jcirc;etas sur
        teron<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_o;&c_r;&c_y;&c_ch;</trd>
    <trd lng="ca">amargor</trd>
    <trdgrp lng="fa">
      <trd>&a_t;&a_l;&a_kh;&f_y;</trd>,
      <trd>&a_t;&a_l;&a_kh;&zwnj;&f_k;&a_A;&a_m;&f_y;</trd>
    </trdgrp>
    <trd lng="fr">amertume</trd>
    <trd lng="hu">keser&udblac;s&eacute;g</trd>
    <trdgrp lng="pl">
    <trd>gorycz</trd>,
    <trd>ocet</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_je;&c_ch;&c_mol;</trd>
  </snc>
</drv>
</art>
