<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hihi.xml,v 1.15 2023/11/15 17:08:00 revo Exp $">
<kap>
  <rad>hihi</rad>/i <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="hihi.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="rid.0i.vizagxo">Ridi</ref> kun akuta sono,
      pro bridata amuzo a&ubreve; moko:
      <ekz>
        la knabino returnis sian kapon, kun unu fingro en la bu&scirc;o, kaj
        <tld/>is
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 83a, volumo 3a, p.
        32a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_h;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">riure <klr>(burleta, ir&ograve;nicament)</klr></trd>
  <trd lng="fa">&a_h;&a_kasra;&a_r;&a_h;&a_kasra;&a_r; &a_kh;&a_n;&a_d;&f_y;&a_d;&a_n;</trd>
  <trd lng="fr">glousser<klr> (rire)</klr></trd>
  <trdgrp lng="hu">
    <trd>kuncog</trd>,
    <trd>vihog</trd>,
    <trd>heher&eacute;szik</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>ridere<klr> (in modo acuto e rumoroso)</klr></trd>,
    <trd>sghignazzare</trd>
  </trdgrp>
  <trd lng="pt">rir abafadamente</trd>
  <trd lng="ru">&c_h;&c_i;&c_h;&c_i;&c_k;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_i;&c_h;&c_i;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_ib;&c_h;&c_ib;&#x43A;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="hihi.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc>
    <dif>
      <ref tip="lst" cel="bru.0o" lst="voko:sonoj">Bruo</ref>
      de iu ridanta, subridanta.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_ib;-&c_h;&c_ib;</trd>
  <trd lng="ca">hi! hi! hi!</trd>
  <trdgrp lng="cs">
    <trd>hehe</trd>,
    <trd>hihi</trd>
  </trdgrp>
  <trd lng="fr">hihi</trd>
  <trdgrp lng="hu">
    <trd>kuncog&aacute;s</trd>,
    <trd>vihog&aacute;s</trd>,
    <trd>heher&eacute;sz&eacute;s</trd>
  </trdgrp>
  <trd lng="it"><ind>hi</ind> hi</trd>
  <trd lng="ru">&c_h;&c_i;-&c_h;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>hehe</trd>,
    <trd>hihi</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_h;&c_i;-&c_h;&c_i;</trd>,
    <trd>&c_h;&c_ib;-&c_h;&c_ib;</trd>,
    <trd>&c_h;&c_i;-&c_h;&c_i;-&c_h;&c_i;</trd>,
    <trd>&c_h;&c_ib;-&c_h;&c_ib;-&c_h;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5475;&#x5475; <pr>h&#x113;h&#x113;</pr></trd>,
    <trd>&#x563B;&#x563B; <pr>x&#x12B;x&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>

