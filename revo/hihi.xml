<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hihi</rad>/i <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="hihi.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="rid.0i.vizagxo">Ridi</ref> kun akuta sono, pro
      bridata amuzo a&ubreve; moko:
      <ekz>
        la knabino returnis sian kapon, kun unu fingro en la bu&scirc;o, kaj
        <tld/>is
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 83a, volumo 3a, p.
        32a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_h;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="fr">glousser<klr> (rire)</klr></trd>
  <trdgrp lng="hu">
    <trd>kuncog</trd>,
    <trd>vihog</trd>,
    <trd>heher&eacute;szik</trd>
  </trdgrp>
  <trd lng="pt">rir abafadamente</trd>
  <trd lng="ru">&c_h;&c_i;&c_h;&c_i;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="hihi.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc>
    <dif>
      Bruo de iu ridanta, subridanta.
    </dif>
  </snc>
  <trd lng="fr">hihi</trd>
  <trdgrp lng="hu">
    <trd>kuncog&aacute;s</trd>,
    <trd>vihog&aacute;s</trd>,
    <trd>heher&eacute;sz&eacute;s</trd>
  </trdgrp>  
  <trd lng="ru">&c_h;&c_i;-&c_h;&c_i;</trd>
</drv>
</art>

