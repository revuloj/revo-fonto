<vortaro>
<art mrk="$Id: boleslav.xml,v 1.7 2022/08/19 15:37:47 revo Exp $">
<kap>
  <rad>Boleslav</rad>/o
</kap>
<drv mrk="boleslav.0o">
  <kap><tld/>o</kap>
  <snc mrk="boleslav.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> slavdevena:
      <ekz>
        <tld/>o da&ubreve;rigis lernadon &ccirc;e estro de la Konservatorio 
        orgenisto Nikolao Vanadzins
        <fnt>
          <aut>J. Slars</aut>,
          <vrk>Dua Dinastio de la Orgenistoj en Latvio</vrk>,
          <lok>
<url ref="http://www.b-eichkorn.homepage.t-online.de/dibe48.htm">
          Dio Benu, 2002:3</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_l;&c_ja;&c_s;&c_l;&c_a;&c_w;</trd>
  <trdgrp lng="de">
    <trd>Boleslaw</trd>,
    <trd>Boles&lstroke;aw</trd>
  </trdgrp>
  <trd lng="fr">Boleslav</trd>
  <trd lng="hu">Boleszl&aacute;v</trd>
  <trd lng="it">Boleslav</trd>
  <trd lng="pl">Boles&lstroke;aw</trd>
</drv>
</art>

