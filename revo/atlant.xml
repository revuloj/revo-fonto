<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Atlantik</rad>/o
</kap>
<drv mrk="atlant.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o">oceano</ref>
      inter E&ubreve;ropo, Afriko kaj Ameriko:
      <ekz>
        Kirkwall, &ccirc;efurbo de la Orkadoj, trovi&gcirc;as meze de insula regiono,
        inter <tld/>o kaj la Norda Maro<fnt><aut>W. H. Simcock</aut>:
        <vrk>Vizito al la nordaj insuloj</vrk>, <bib>Monato</bib><lok>jaro
        1999a, numero 9a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Atlantischer Ozean</trd>,
    <trd>Atlantik</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>Oceano Atl&aacute;ntico</trd>,
    <trd>Atl&aacute;ntico</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Atlantique</trd>,
    <trd>Oc&eacute;an atlantique</trd>
  </trdgrp>
  <trd lng="hu">Atlanti-&oacute;ce&aacute;n</trd>
  <trd lng="nl">Atlantische oceaan</trd>
  <trdgrp lng="ru">
    <trd>&c_A;&c_t;&c_l;&c_a;&c_n;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_A;&c_t;&c_l;&c_a;&c_n;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_o;&c_k;&c_je;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">Atlantick&yacute; Oce&aacute;n</trd>
</drv>
</art>
