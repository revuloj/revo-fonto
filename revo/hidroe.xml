  <trd lng="pl">hydroelektryczny</trd>
  <trd lng="pt">hidroel&eacute;(c)trico</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_o;&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
