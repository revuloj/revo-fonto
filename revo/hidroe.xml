  <trd lng="de">hydroelektrisch</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&omikron;&eta;&lambda;&epsilon;&kappa;&tau;&rho;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
