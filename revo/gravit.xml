<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gravit</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gravit.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="gravit.0i.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Celi, pezi al centra punkto.
    </dif>
    <adm>
      Dubinda difino. <aut>MB</aut>
    </adm>
    <trd lng="de">gravitieren</trd>
    <trd lng="ru">&c_t;&c_ja;&c_g;&c_o;&c_t;&c_je;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="gravit.0i.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sin movi konforme al la le&gcirc;o de la reciproka altiro de
      korpoj en la universo.
    </dif>
    <trd lng="de">gravitieren</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_ib;&c_m;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_ib;&c_m;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">graviter</trd>
  <trd lng="hu">gravit&aacute;l</trd>
  <trd lng="nl">graviteren</trd>
  <trd lng="pt">gravitar</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="gravit.0o">
  <kap><tld/>o</kap>
  <snc mrk="gravit.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Forto kuntiranta al iu maso alian mason, proporcie je la produto
      de amba&ubreve; kaj inverse je la kvadrato de ilia distanco.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_c;&c_ja;&c_g;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schwerkraft</trd>,
    <trd>Gravitation</trd>
  </trdgrp>
  <trd lng="es">gravedad</trd>
  <trd lng="fr">gravitation</trd>
  <trdgrp lng="hu">
    <trd>gravit&aacute;ci&oacute;</trd>,
    <trd>t&ouml;megvonz&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zwaartekracht</trd>,
    <trd>gravitatie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>gravita&ccedil;&atilde;o</trd>,
    <trd>gravidade<klr tip="amb"> (for&ccedil;a)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_a;&c_v;&c_i;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_t;&c_ja;&c_g;&c_o;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="gravit.sen0o">
  <kap>sen<tld/>o</kap>
  <snc mrk="gravit.sen0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &Scirc;veba stato de objekto, kiun tiras nenia <tld/>o:
      <ekz>
        &leftquot;mi supozas ke sen<tld/>o ne estas nova sperto por vi?&rightquot;
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
        <lok>tk-stafeto, 1982</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">apesanteur</trd>
</drv>
</art>
