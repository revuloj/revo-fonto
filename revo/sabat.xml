</kap>

<drv mrk="sabat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc>
    <dif>
      Sesa <ref tip="lst" cel="tag.0o.diurn" lst="voko:semajntagoj"
      val="6">tago<sncref ref="tag.0o.diurn"/></ref> de la <ref
      tip="malprt" cel="semajn.0o">semajno</ref> la&ubreve; la internacia normo
      ISO 8601, inter vendredo kaj diman&ccirc;o:
      <ekz>
        &scirc;tate deciditaj ferioj (<tld/>o, diman&ccirc;o,
        festotagoj)<fnt><aut>Huang Tingsheng</aut>: <vrk>La &ccirc;ina
        bilomerkato</vrk>, <bib>Monato</bib>, <lok>jaro 2001a, numero
        8a,
        p.
        10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">Saterdag</trd>
  <trd lng="be">&c_s;&c_u;&c_b;&c_o;&c_t;&c_a;</trd>
  <trd lng="bg">&c_s;&c_malmol;&c_b;&c_o;&c_t;&c_a;</trd>
  <trdgrp lng="br">
    <trd>Sadorn</trd>,
    <trd>disadorn</trd>
  </trdgrp>
  <trd lng="ca">dissabte</trd>
  <trd lng="cs">sobota</trd>
  <trdgrp lng="de">
    <trd>Samstag</trd>,
    <trd>Sonnabend</trd>,
    <trd>Sabbat<klr> [biblia]</klr></trd>
  </trdgrp>
  <trd lng="en">Saturday</trd>
  <trd lng="es">s&aacute;bado</trd>
  <trd lng="id">Sabtu</trd>
  <trd lng="it">sabato</trd>
  <trd lng="fa">&a_sh;&a_n;&a_b;&a_h;</trd>
  <trd lng="fr">samedi</trd>
  <trd lng="fy">sneon</trd>
  <trd lng="he">&shin;&bet;&tav;</trd>
  <trd lng="hu">szombat</trd>
  <trd lng="io">saturdio</trd>
  <trd lng="ja">&#22303;&#26332;&#26085; [<ind>&#12393;&#12424;&#12358;&#12403;</ind>]</trd>
  <trdgrp lng="jbo">
    <trd>xavdei</trd>,
    <trd>tedydei</trd>
  </trdgrp>
  <trd lng="ko">&#53664;&#50836;&#51068;</trd>
  <trd lng="nl">zaterdag</trd>
  <trd lng="pl">sobota</trd>
  <trd lng="pt">s&aacute;bado</trd>
  <trd lng="ro">s&acirc;mb&abreve;t&abreve;</trd>
  <trd lng="ru">&c_s;&c_u;&c_b;&c_b;&c_o;&c_t;&c_a;</trd>
  <trd lng="sk">sobota</trd>
  <trd lng="sv">l&ouml;rdag</trd>
  <trd lng="tp">tenpo suno nanpa luka wan</trd>
  <trd lng="vo">z&auml;del</trd>
</drv>
