    <trd>aer&oacute;dromo</trd>
    <trd>aerodromo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>a&eacute;rodrome</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>a&euml;rodroom</trd>,
    <trd>vliegveld</trd>
  </trdgrp>
  <trd lng="hu">rep&uuml;l&odblac;t&eacute;r</trd>
</drv>
