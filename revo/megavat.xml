<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: megavat.xml,v 1.15 2025/10/07 16:38:10 revo Exp $">
<kap>
  <rad>megavat</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="megavat.0o">
  <kap><tld/>o</kap>
  <mlg kod="SI">MW</mlg>
  <snc mrk="megavat.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="unu.mezur0o.SCI">Mezurunuo</ref> de
      <ref cel="pov.0o.FIZ">povo</ref>, egala al
      unu miliono da <ref tip="prt" cel="vat1.v0o.FIZ">vatoj</ref>:
      <ekz>
        en <nom>Ignalina</nom> funkcias unu reaktoro kun potenco de 1350 <tld/>oj
        <fnt>
          <aut>Last</aut>,
          <vrk>Nuklea energio restu en Litovio</vrk>,
          <bib>Monato</bib>,
          <lok>2007/1, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>megawatt</trd>,
    <trd>MW</trd>
  </trdgrp>
  <trd lng="be">&c_m;&c_e;&c_g;&c_a;&c_v;&c_a;&c_t;</trd>
  <trd lng="cs">megawatt</trd>
  <trd lng="de">Megawatt</trd>
  <trd lng="en">megawatt</trd>
  <trd lng="es">megavatio</trd>
  <trd lng="fr">m&eacute;gawatt</trd>
  <trd lng="id">megawatt</trd>
  <trd lng="it">megawatt</trd>
  <trd lng="ja">&#x30E1;&#x30AC;&#x30EF;&#x30C3;&#x30C8;  <pr>&#12417;&#x304C;&#x308F;&#12387;&#12392;</pr></trd>
  <trd lng="pl">megawat</trd>
  <trd lng="sk">megawatt</trd>
  <trd lng="zh">&#20806;&#x74E6; <pr>zh&#xE0;ow&#x1CE;</pr></trd>
</drv>
</art>

