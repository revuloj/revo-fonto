<art mrk="$Id$">
<kap>
  <rad>megavat</rad>/o
</kap>
<drv mrk="megavat.0o">
  <kap><tld/>o</kap>
  <mlg>MW</mlg>
  <snc mrk="megavat.0o.FIZ">
