<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>atmosfer</rad>/o
</kap>
<drv mrk="atmosf.0o">
  <kap><tld/>o</kap>
  <snc mrk="atmosf.0o.AST">
    <uzo tip="fak">AST</uzo>
    <uzo tip="fak">MET</uzo>
    <dif>
      Gasa maso &ccirc;irka&ubreve;anta astron, kaj interalie Teron.
    </dif>
    <trdgrp lng="hu">
      <trd>l&eacute;gk&ouml;r</trd>,
      <trd>atmoszf&eacute;ra</trd>
    </trdgrp>
  </snc>
  <snc mrk="atmosf.0o.etoso">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;irka&ubreve;antaj cirkonstancoj, medio:
      <ekz>
        vivi en <tld/>o de malvirto;
      </ekz>
      <ekz>
        la traktadoj okazas en favora <tld/>o;
      </ekz>
      <ekz>
        la sufoka <tld/>o de homa malamo;
      </ekz>
      <ekz>
        krei ne&ubreve;tralan <tld/>on de tutmonda homareco
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 2a, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="etos.0o">etoso</ref>,
      <ref cel="sfer.0o">sfero</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>k&ouml;zeg</trd>,
      <trd>l&eacute;gk&ouml;r</trd>
    </trdgrp>
  </snc>
  <snc mrk="atmosf.0o.unuo">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      <ref cel="prem.0o.FIZ">premo</ref>, egala al la premo de kolono el
      hidrargo, alta je 0,76 m, &ccirc;e normala graviteco kaj
      temperaturo 0 &ring;C; simb. atm:
      <ekz>
        unu <tld/>o egalas al 1,013.10<sup>5</sup>
        <ref cel="paskal.p0o">paskaloj</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_m;&c_a;&c_s;&c_f;&c_e;&c_r;&c_a;</trd>
  <trd lng="de">Atmosph&auml;re</trd>
  <trd lng="en">atmosphere</trd>
  <trd lng="es">atm&oacute;sfera</trd>
  <trd lng="fr">atmosph&egrave;re</trd>
  <trd lng="nl">atmosfeer</trd>
  <trd lng="pl">atmosfera</trd>
  <trd lng="ru">&c_a;&c_t;&c_m;&c_o;&c_s;&c_f;&c_je;&c_r;&c_a;</trd>
  <trd lng="sk">atmosf&eacute;ra</trd>
  <trd lng="sv">atmosf&auml;r</trd>
</drv>
</art>
