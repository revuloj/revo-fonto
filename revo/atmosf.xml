<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: atmosf.xml,v 1.33 2023/10/28 08:33:10 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>atmosfer</rad>/o
</kap>
<drv mrk="atmosf.0o">
  <kap><tld/>o</kap>
  <snc mrk="atmosf.0o.AST">
    <uzo tip="fak">AST</uzo>
    <uzo tip="fak">MET</uzo>
    <dif>
      Gasa maso &ccirc;irka&ubreve;anta astron, kaj interalie Teron.
    </dif>
    <trdgrp lng="hu">
      <trd>l&eacute;gk&ouml;r</trd>,
      <trd>atmoszf&eacute;ra</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x6C14;&#x6C1B; <pr>q&#xEC;f&#x113;n</pr></trd>,
    <trd>&#x6C23;&#x6C1B; <pr>q&#xEC;f&#x113;n</pr></trd>,
    <trd>&#x5305;&#x56F4;&#x5929;&#x4F53;&#x7684;&#x6C14;&#x4F53; <pr>b&#x101;ow&#xE9;iti&#x101;nt&#x1D0;deq&#xEC;t&#x1D0;</pr></trd>,
    <trd>&#x5305;&#x570D;&#x5929;&#x9AD4;&#x7684;&#x6C23;&#x9AD4; <pr>b&#x101;ow&#xE9;iti&#x101;nt&#x1D0;deq&#xEC;t&#x1D0;</pr></trd>,
    <trd>&#x5927;&#x6C14; <pr>d&#xE0;q&#xEC;</pr></trd>,
    <trd>&#x5927;&#x6C23; <pr>d&#xE0;q&#xEC;</pr></trd>,
    <trd>&#x5927;&#x6C14;&#x5708; <pr>d&#xE0;q&#xEC;qu&#x101;n</pr></trd>,
    <trd>&#x5927;&#x6C23;&#x5708; <pr>d&#xE0;q&#xEC;qu&#x101;n</pr></trd>,
    <trd>&#x5730;&#x7403;&#x5927;&#x6C14;&#x5C42; <pr>d&#xEC;qi&#xFA;d&#xE0;q&#xEC;c&#xE9;ng</pr></trd>,
    <trd>&#x5730;&#x7403;&#x5927;&#x6C23;&#x5C64; <pr>d&#xEC;qi&#xFA;d&#xE0;q&#xEC;c&#xE9;ng</pr></trd>,
    <trd>&#x5730;&#x7403;&#x5927;&#x6C14; <pr>d&#xEC;qi&#xFA;d&#xE0;q&#xEC;</pr></trd>,
    <trd>&#x5730;&#x7403;&#x5927;&#x6C23; <pr>d&#xEC;qi&#xFA;d&#xE0;q&#xEC;</pr></trd>,
    <trd>&#x592A;&#x7A7A; <pr>t&#xE0;ik&#x14D;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="atmosf.0o.etoso">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;irka&ubreve;antaj cirkonstancoj, medio:
      <ekz>
        vivi en <tld/>o de malvirto;
      </ekz>
      <ekz>
        la traktadoj okazas en favora <tld/>o;
      </ekz>
      <ekz>
        la sufoka <tld/>o de homa malamo;
      </ekz>
      <ekz>
        krei ne&ubreve;tralan <tld/>on de tutmonda homareco
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 2a, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="etos.0o">etoso</ref>,
      <ref cel="sfer.0o">sfero</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>k&ouml;zeg</trd>,
      <trd>l&eacute;gk&ouml;r</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x6C1B;&#x56F4; <pr>f&#x113;nw&#xE9;i</pr></trd>,
    <trd>&#x6C1B;&#x570D; <pr>f&#x113;nw&#xE9;i</pr></trd>,
    <trd>&#x8272; <pr>s&#xE8;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="atmosf.0o.unuo">
    <uzo tip="fak">FIZ</uzo>
    <mlg>atm</mlg>
    <dif>
      Komunuza <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">mezurunuo</ref> de
      <ref cel="prem.0o.FIZ">premo</ref>, egala al la premo de kolono el
      hidrargo, alta je 0,76 m, &ccirc;e normala graviteco kaj
      temperaturo 0 &#x2DA;C:
      <ekz>
        unu <tld/>o egalas al 1,013.10<sup>5</sup>
        <ref cel="paskal.p0o">paskaloj</ref>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x6C14;&#x538B;&#x5355;&#x4F4D; <pr>q&#xEC;y&#x101;d&#x101;nw&#xE8;i</pr></trd>,
    <trd>&#x6C23;&#x58D3;&#x55AE;&#x4F4D; <pr>q&#xEC;y&#x101;d&#x101;nw&#xE8;i</pr></trd>,
    <trd>&#x5927;&#x6C14;&#x538B;&#x529B; <pr>d&#xE0;q&#xEC;y&#x101;l&#xEC;</pr></trd>,
    <trd>&#x5927;&#x6C23;&#x58D3;&#x529B; <pr>d&#xE0;q&#xEC;y&#x101;l&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_t;&c_m;&c_a;&c_s;&c_f;&c_e;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>atmosf&eacute;ra</trd>,
    <trd>ovzdu&scaron;&iacute;</trd>,
    <trd>pov&ecaron;t&rcaron;&iacute;</trd>
  </trdgrp>
  <trd lng="de">Atmosph&auml;re</trd>
  <trd lng="en">atmosphere</trd>
  <trd lng="es">atm&oacute;sfera</trd>
  <trd lng="fr">atmosph&egrave;re</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5D8;&#x5DE;&#x5D5;&#x5B9;&#x5E1;&#x5E4;&#x5B5;&#x5E8;&#x5B8;&#x5D4;</trd>
  <trd lng="id">atmosfer</trd>
  <trd lng="it">atmosfera</trd>
  <trdgrp lng="ja">
    <trd>&#x5927;&#x6C17;  <pr>&#x305F;&#x3044;&#x304D;</pr></trd>,
    <trd>&#x96F0;&#x56F2;&#x6C17;  <pr>&#x3075;&#x3093;&#x3044;&#x304D;</pr></trd>,
    <trd>&#x6C17;&#x5727;  <pr>&#x304D;&#x3042;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">atmosfeer</trd>
  <trd lng="pl">atmosfera</trd>
  <trd lng="pt">atmosfera</trd>
  <trd lng="ru">&c_a;&c_t;&c_m;&c_o;&c_s;&c_f;&c_je;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>atmosf&eacute;ra</trd>,
    <trd>ovzdu&scaron;ie</trd>,
    <trd>povetrie</trd>
  </trdgrp>
  <trd lng="sv">atmosf&auml;r</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x442;&#x43C;&#x43E;&#x441;&#x444;&#x435;&#x440;&#x430;</trd>,
    <trd>&#x43E;&#x431;&#x441;&#x442;&#x430;&#x43D;&#x43E;&#x432;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
