<art mrk="$Id: skoter.xml,v 1.4 2015/07/04 08:37:43 revo Exp $">
<kap>
  <rad>skoter</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="skoter.0o">
  <kap><tld/>o</kap>
  <snc mrk="skoter.0o.CIK">
    <dif>
      <ref tip="super" cel="cikl.motor0o.CIK">Motorciklo</ref>
      kun du malgrandaj radoj kaj platformeto por la piedoj,
      faciliganta sidadon:
      <ekz>
        &scirc;i alvenis sur la <tld/>o kun la sako malanta&ubreve;e
        <fnt>
          <aut>S. Ponte Preta, trad. P. S. Viana</aut>,
          <vrk><url
          ref="http://esperantaretradio.blogspot.fr/2013/05/maljuna-kontrabandistino.html">
          Maljuna kontrabandistino</url></vrk>,
          <lok>2013-05-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">skútr</trd>
  <trd lng="en">scooter</trd>
  <trdgrp lng="es">
    <trd>motoneta</trd>,
    <trd>esc&uacute;ter</trd>,
    <trd>scooter</trd>
  </trdgrp>
  <trd lng="fr">scouteur</trd>
  <trd lng="sk">skúter</trd>
</drv>
</art>

