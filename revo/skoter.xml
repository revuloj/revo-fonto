<kap>
  <rad>skoter</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="skoter.0o">
  <kap><tld/>o</kap>
  <snc mrk="skoter.0o.CIK">
    <dif>
      <ref tip="lst" lst="voko:veturiloj" cel="motoci.0o">Motorciklo</ref>
      kun du malgrandaj radoj kaj platformeto por la piedoj,
      faciliganta sidadon:
      <ekz>
        &scirc;i alvenis sur la <tld/>o kun la sako malanta&ubreve;e
        <fnt>
          <aut>S. Ponte Preta, trad. P. S. Viana</aut>,
          <vrk><url ref="http://esperantaretradio.blogspot.fr/2013/05/maljuna-kontrabandistino.html">
          Maljuna kontrabandistino</url></vrk>,
          <lok>2013-05-24</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rapidega <tld/>o sulkas la kultivkampojn
        <fnt>
          <bib>MD08</bib>
          <vrk>La enigmo de la relikvoj en Kompostelo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="moped.0o">mopedo</ref>,
       <ref cel="skut.0ilo">skutilo</ref>,
       <ref cel="bicikl.motor0o">zumbiciklo</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/f/fb/Vespa_2.JPG/320px-Vespa_2.JPG" prm="CC BY-SA 3.0"><tld/>o &leftquot;Vespa&rightquot;
        <fnt>
          <aut>Werner100359, propra foto</aut>
          <lok><url ref="&WCW;/File:Vespa_2.JPG">&WCW;/File:Vespa_2.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_u;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_m;&c_a;&c_t;&c_a;&c_r;&c_o;&c_l;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="cs">sk&uacute;tr</trd>
  <trd lng="de">Motorroller</trd>
  <trd lng="en">scooter</trd>
  <trdgrp lng="es">
    <trd>motoneta</trd>,
    <trd>esc&uacute;ter</trd>,
    <trd>scooter</trd>
  </trdgrp>
  <trd lng="fr">scouteur</trd>
  <trd lng="ja">&#x30B9;&#x30AF;&#x30FC;&#x30BF;&#x30FC;</trd>
  <trd lng="pl">skuter</trd>
  <trd lng="sk">sk&uacute;ter</trd>
  <trd lng="uk">&#x43C;&#x43E;&c_t;&#x43E;&c_r;&#x43E;&#x43B;&c_je;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x8E0F;&#x677F;&#x6469;&#x6258;&#x8ECA; <pr>t&#xE0;b&#x1CE;nm&#xF3;tu&#x14D;ch&#x113;</pr></trd>,
    <trd>&#x8E0F;&#x677F;&#x6469;&#x6258;&#x8F66; <pr>t&#xE0;b&#x1CE;nm&#xF3;tu&#x14D;ch&#x113;</pr></trd>
  </trdgrp>
