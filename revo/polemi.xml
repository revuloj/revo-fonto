<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>polemik</rad>/o
</kap>
<drv mrk="polemi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Skriba publika diskutado:
      <ekz>
        <tld/>a verko;
      </ekz>
      <ekz>
        mi ne volis entiri min en <tld/>on kun certa kategorio da
        personoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_je;&c_m;&c_ib;&c_k;&c_a;</trd>
  <trd lng="de">Polemik</trd>
  <trd lng="es">pol&eacute;mica</trd>
  <trd lng="fr">pol&eacute;mique</trd>
  <trdgrp lng="hu">
    <trd>nyilv&aacute;nos <ind>vita</ind></trd>,
    <trd>pol&eacute;mia</trd>
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>polemiek</trd>,
    <trd>pennenstrijd</trd>
  </trdgrp>
  <trd lng="pt">pol&ecirc;mica</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je;&c_m;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="polemi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Skribe publike diskuti:
      <ekz>
        mi ne <tld/>os kontra&ubreve; ili e&ccirc; per unu vorto<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_je;&c_m;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">polemisieren</trd>
  <trd lng="es">polemizar</trd>
  <trd lng="fr">pol&eacute;miquer</trd>
  <trdgrp lng="hu">
    <trd>vit&aacute;zik</trd>,
    <trd>polemiz&aacute;l</trd>
  </trdgrp>  
  <trd lng="nl">polemiseren</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je;&c_m;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
