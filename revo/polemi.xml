<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>polemik</rad>/o
</kap>
<drv mrk="polemi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Skriba publika diskutado:
      <ekz>
        <tld/>a verko;
      </ekz>
      <ekz>
        mi ne volis entiri min en <tld/>on kun certa kategorio da
        personoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Polemik</trd>
  <trdgrp lng="nl">
    <trd>polemiek</trd>,
    <trd>pennenstrijd</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je;&c_m;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="polemi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Skribe publike diskuti:
      <ekz>
        mi ne <tld/>os kontra&ubreve; ili e&ccirc; per unu vorto<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">polemisieren</trd>
  <trd lng="nl">polemiseren</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je;&c_m;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
