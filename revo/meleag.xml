<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>meleagr</rad>/o
</kap>
<drv mrk="meleag.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fazane.0oj">fazanedoj</ref>
      (<trd lng="la">Meleagris</trd>),
      kortobirdo pli granda ol koko kun vosto ventumilforme etendebla.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_y;&c_k;</trd>
  <trdgrp lng="br">
    <trd>kilhog-Indez<klr> (virsekso)</klr></trd>,  
    <trd>yar-Indez<klr> (insekso)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Truthahn (masklo)</trd>,
    <trd>Pute (femalo)</trd>
  </trdgrp>
  <trd lng="en">turkey</trd> 
  <trdgrp lng="fr">
    <trd>dinde<klr> (insekso)</klr></trd>,  
    <trd>dindon<klr> (virsekso)</klr></trd>
  </trdgrp>
  <trd lng="hu">pulyka</trd>
  <trd lng="nl">kalkoen</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_d;&c_ju;&c_k; <klr>(masklo)</klr></trd>,
    <trd>&c_i;&c_n;&c_d;&c_je;&c_j;&c_k;&c_a; <klr>(femalo)</klr></trd>,
    <trd>&c_i;&c_n;&c_d;&c_ju;&c_sh;&c_k;&c_a; <klr>(femalo)</klr></trd>
  </trdgrp>
  <trd lng="tr">hindi</trd>
</drv>
</art>
