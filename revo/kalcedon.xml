<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kalcedon.xml,v 1.4 2023/12/02 21:20:04 afido Exp $">
    <kap>
      <rad>kalcedon</rad>/o <fnt><bib>FdO</bib></fnt>,
      <var><kap><rad var="i">kalcedoni</rad>/o  <fnt><bib>NT</bib></fnt></kap></var>
    </kap>

<drv mrk="kalcedon.0o">
  <kap><tld/>o, <var><kap><tld var="i"/>o</kap></var></kap>
  <snc mrk="kalcedon.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Fibreca mikrokristala varia&jcirc;o de <ref tip="super" cel="kvarc.0o">kvarco</ref>:
      <ekz>
        la akva kolono brilis kiel oro, rubeno, <tld/>o kaj smeraldo
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fundamentoj de la muro <klr>[&#x2026;]</klr> estis ornamitaj per altvaloraj &scirc;tonoj
        <klr>[&#x2026;]</klr> la tria, <tld var="i"/>o, la kvara, smeraldo
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 21:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj <klr>[estas]</klr> valoraj ornamaj &scirc;tonoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=214268">Kalcedono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_h;&c_a;&c_l;&c_c;&c_e;&c_d;&c_o;&c_n;</trd>
    <trd lng="de">Chalcedon</trd>
    <trd lng="en">Chalcedony</trd>
    <trd lng="fr">Calc&eacute;doine</trd>
    <trd lng="grc">&chi;&alfa;&lambda;&kappa;&eta;&delta;&omega_ton;&ny;</trd>
    <trd lng="zh">&#x7389;&#x9AD3; <pr>y&#xF9;su&#x1D0;</pr></trd>
</snc>
  <trd lng="uk">&c_h;&c_a;&#x43B;&c_c;&c_je;&c_d;&#x43E;&#x43D;</trd>
</drv>

  </art>

