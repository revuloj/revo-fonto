<kap>
  <rad>politikologi</rad>/o <fnt><bib>PIV2</bib></fnt>,
  <var><kap><rad var="t">politologi</rad>/o
  <fnt><bib>PIV2</bib></fnt></kap></var>
</kap>
<drv mrk="politikologi.0o">
  <kap><tld/>o, <var><kap><tld var="t"/>o</kap></var></kap>
  <snc mrk="politikologi.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="vid" cel="politi.0o.POL">Politiko</ref>scienco
      kiu studas la teorion de &scirc;tatoj kaj regsistemoj kaj
      la efektivan politikan vivon de socioj:
      <ekz>
        mi pagus unue kelkajn specialistojn pri
        sociologio kaj <tld/>o kaj starigus jenajn
        demandojn <klr>[&#8230;]</klr>
        <fnt>
          <aut>W. Zelazny</aut>,
          <vrk>Pri la rilatoj inter inteligencio...</vrk>,
          <bib>LOdE</bib>,
          <lok>2000:4 (66)</lok>
        </fnt>;
      </ekz>  
      <ekz>
        <tld var="t"/>aj skoloj    
        <fnt>
          <aut>J. Stan&ecaron;k</aut>,
          <vrk>Senvualita Sekreto de nia Historio</vrk>,
          <lok><url ref="http://www.ikue.org/cz/arkivo/a-61-1-06.htm">
          Dio Benu, 2006:1 (61)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">politologie</trd>
  <trd lng="de">Politologie</trd>
  <trdgrp lng="fr">
    <trd>politologie</trd>,
    <trd>science politique</trd>
  </trdgrp>
  <trd lng="hu">politol&oacute;gia</trd>
  <trd lng="pl">politologia</trd>
  <trd lng="sk">politol&oacute;gia</trd>
</drv>
</art>

