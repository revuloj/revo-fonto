        mi pagus unue kelkajn specialistojn pri
        sociologio kaj <tld/>o kaj starigus jenajn
        demandojn <klr>[&#x2026;]</klr>
        <fnt>
          <aut>W. Zelazny</aut>,
          <vrk>Pri la rilatoj inter inteligencio...</vrk>,
          <bib>LOdE</bib>,
          <lok>2000:4 (66)</lok>
        </fnt>;
      </ekz>  
      <ekz>
        <tld var="t"/>aj skoloj    
        <fnt>
          <aut>J. Stan&ecaron;k</aut>,
          <vrk>Senvualita Sekreto de nia Historio</vrk>,
          <lok><url ref="http://www.ikue.org/cz/arkivo/a-61-1-06.htm">
          Dio Benu, 2006:1 (61)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_t;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>,
