<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ski.xml,v 1.24 2024/03/25 06:40:50 revo Exp $">
<kap>
   <ofc>8</ofc>
   <rad>ski</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ski.0o">
   <kap><tld/>o</kap>
   <snc mrk="ski.0o.SPO">
     <uzo tip="fak">SPO</uzo>
     <dif>
       Longa, plata tabulo kun supren kurbita ekstremo,
       kiun oni alligas al &scirc;uoj kaj
       per kiu oni glitas sur ne&gcirc;o.
     </dif>
   </snc>
  <trd lng="be">&c_l;&c_y;&c_zh;&c_y;</trd>
  <trd lng="ca">esqu&iacute;</trd>
  <trdgrp lng="cs">
    <trd>ly&#x17E;e</trd>,
    <trd>ski</trd>
  </trdgrp>
  <trd lng="de">Ski</trd>
  <trd lng="en">ski</trd>
  <trd lng="es">esqu&iacute;<klr> (pat&iacute;n para esquiar)</klr></trd>
  <trd lng="fr">ski<klr> (objet)</klr></trd>
  <trd lng="hu">s&iacute;</trd>
  <trd lng="ja">&#x30B9;&#x30AD;&#x30FC;  <pr>&#x3059;&#x304D;&#x30FC;</pr></trd>
  <trd lng="nl">ski</trd>
  <trd lng="pl">narta</trd>
  <trd lng="ru">&c_l;&c_y;&c_zh;&c_a;</trd>
  <trd lng="sk">ly&#x17E;a</trd>
  <trd lng="sv">skida</trd>
  <trd lng="uk">&#x43B;&c_i;&c_zh;&c_a;</trd>
  <trd lng="zh">&#x6ED1;&#x96EA;&#x677F; <pr>hu&#xE1;xu&#x11B;b&#x1CE;n</pr></trd>
</drv>

<drv mrk="ski.0i">
  <kap><tld/>i</kap>
  <snc mrk="ski.0i.SPO">
    <dif>
      Iri per <tld/>oj:
      <ekz>
        ili en prujnaj arbaroj <tld/>is sur alta&jcirc;oj supren kaj malsupren
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_g;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_l;&c_y;&c_zh;&c_a;&c_h;</ind></trd>,
    <trd>&c_k;&c_a;&c_t;&c_a;&c_c;&c_c;&c_a; &c_n;&c_a; <ind>&c_l;&c_y;&c_zh;&c_a;&c_h;</ind></trd>
  </trdgrp>
  <trd lng="cs">ly&#x17E;ovat</trd>
  <trd lng="fr">skier</trd>
  <trd lng="ja">&#x30B9;&#x30AD;&#x30FC;&#x3092;&#x3059;&#x308B;  <pr>&#x3059;&#x304D;&#x30FC;&#x3092;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="pl">
    <trd>je&zacute;dzi&cacute; na  <ind>nartach</ind></trd>,
    <trd>uprawia&cacute;  <ind>narciarstwo</ind></trd>
  </trdgrp>
  <trd lng="sk">ly&#x17E;ova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&c_t;&c_a;&c_t;&c_i;&c_s;&#x44F;/&c_h;&#x43E;&c_d;&c_i;&c_t;&c_i; &#x43D;&c_a; &#x43B;&c_i;&c_zh;&c_a;&c_h;</trd>,
    <trd>&c_z;&c_a;&c_j;&#x43C;&c_a;&c_t;&c_i;&c_s;&#x44F; &#x43B;&c_i;&c_zh;&#x43D;&c_i;&#x43C; &c_s;&#x43F;&#x43E;&c_r;&c_t;&#x43E;&#x43C;</trd>
  </trdgrp>
  <trd lng="zh">&#x6ED1;&#x96EA; <pr>hu&#xE1;xu&#x11B;</pr></trd>
</drv>

<drv mrk="ski.0isto">
   <kap><tld/>isto</kap>
   <snc mrk="ski.0isto.SPO">
     <uzo tip="fak">SPO</uzo>
     <dif>
       Tiu, kiu sportas sur <tld/>oj.
     </dif>
   </snc>
  <trd lng="be">&c_l;&c_y;&c_zh;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">esquiador</trd>
  <trd lng="cs">ly&#x17E;a&rcaron;</trd>
  <trd lng="de">Skil&auml;ufer</trd>
  <trd lng="en">skier</trd>
  <trd lng="es">esquiador</trd>
  <trd lng="fr">skieur</trd>
  <trdgrp lng="hu">
     <trd>s&iacute;el&odblac;</trd>,
     <trd>s&iacute;z&odblac;</trd>
   </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30AD;&#x30FC;&#x30E4;&#x30FC;  <pr>&#x3059;&#x304D;&#x30FC;&#x3084;&#x30FC;</pr></trd>,
    <trd>&#x30B9;&#x30AD;&#x30FC;&#x9078;&#x624B;  <pr>&#x3059;&#x304D;&#x30FC;&#x305B;&#x3093;&#x3057;&#x3085;</pr></trd>
  </trdgrp>
  <trd lng="nl">ski&euml;r</trd>
  <trd lng="pl">narciarz</trd>
  <trd lng="ru">&c_l;&c_y;&c_zh;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">ly&#x17E;iar</trd>
  <trd lng="sv">skid&aring;kare</trd>
  <trd lng="uk">&#x43B;&c_i;&c_zh;&#x43D;&c_i;&#x43A;</trd>
  <trd lng="zh">&#x6ED1;&#x96EA;&#x8005; <pr>hu&#xE1;xu&#x11B;zh&#x11B;</pr></trd>
</drv>
</art>
