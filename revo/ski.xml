<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>ski</rad>/o
</kap>
<drv mrk="ski.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Speco de longa, plata kaj ligna glitilo kiun oni aligas al &scirc;uoj kaj
      per kiu oni glitas sur ne&gcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_y;&c_zh;&c_y;</trd>
  <trd lng="de">Ski</trd>
  <trd lng="en">ski</trd>
  <trd lng="fr">ski</trd>
  <trd lng="nl">ski</trd>
  <trd lng="ru">&c_l;&c_y;&c_zh;&c_a;</trd>
</drv>

<drv mrk="ski.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Tiu, kiu sportas sur <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_y;&c_zh;&c_n;&c_ib;&c_k;</trd>  
  <trd lng="de">Skil&auml;ufer</trd>
  <trd lng="en">skier</trd>
  <trd lng="nl">ski&euml;r</trd>
  <trd lng="ru">&c_l;&c_y;&c_zh;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
