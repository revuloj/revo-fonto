<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>avid</rad>/a
</kap>
<drv mrk="avid.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Fortege deziranta ion:
      <ekz>
        <tld/>a je gloro, al la&ubreve;doj, pri
        ri&ccirc;a&jcirc;oj;
      </ekz>
      <ekz>
        donu fingron al <tld/>ulo, li tutan manon
        postulas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sang<tld/>a
        <fnt><bib>MT</bib>,
        <lok>&Nah; 3, 1</lok></fnt>;
      </ekz>
      <ekz>
        nobla <tld/>eco<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mon<tld/>a.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">gierig</trd>
  <trd lng="es">&aacute;vido</trd>
  <trd lng="fr">avide</trd>
  <trdgrp lng="hu">
    <trd>moh&oacute;</trd>,
    <trd>s&oacute;v&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_l;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_zh;&c_a;&c_zh;&c_d;&c_u;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="avid.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fortege <ref tip="super" cel="dezir.0i">deziri</ref> ion:
      <ekz>
        <tld/>i ripozon post longa penado;
      </ekz>
      <ekz>
        kiu <tld/>as pli bonan, perdas plej
        bezonan<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ambici">ambicii</ref>,
      <ref cel="avar.0a">avara</ref>,
      <ref cel="kapt.0i">kapti</ref>,
      <ref cel="marcip.0o">marcipano</ref>,
      <ref cel="sving.0i">svingi</ref>,
      <ref cel="ventr.0o">ventro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">gieren</trd>
  <trd lng="fr">&ecirc;tre <ind>avide</ind> (de)</trd>
  <trdgrp lng="hu">
    <trd>s&oacute;v&aacute;rog</trd>,
    <trd>&aacute;h&iacute;tozik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_zh;&c_a;&c_d;&c_n;&c_y;&c_m; <klr>(&c_d;&c_o; &c_ch;&c_je;&c_g;&c_o;-&c_l;&c_i;&c_b;&c_o;, &c_k; &c_ch;&c_je;&c_m;&c_u;-&c_l;&c_i;&c_b;&c_o;)</klr></trd>,
    <trd>&c_zh;&c_a;&c_d;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_zh;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
