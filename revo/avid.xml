<drv mrk="avid.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="avid.0i.KOMUNE">
    <dif>
      Forte, tro <ref tip="super" cel="dezir.0i.porSiMem">deziri</ref>:
      <ekz>
        <tld/>i ripozon post longa penado;
      </ekz><ekz>
        <ind>kiu <tld/>as pli bonan, perdas plej bezonan</ind>
        <trd lng="fr">le mieux est l'ennemi du bien</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ambici">ambicii</ref>,
      <ref cel="avar.0a">avara</ref>,
      <ref cel="kapt.0i">kapti</ref>,
      <ref cel="marcip.0o">marcipano</ref>,
      <ref cel="sving.0i">svingi</ref>,
      <ref cel="ventr.0o">ventro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_v;&c_a;&c_p;&c_ib;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
  <trd lng="bo">&#xF51;&#xF7C;&#xF51;&#xF0B;&#xF46;&#xF42;&#xF66;&#xF0B;&#xF63;&#xF44;&#xF0B;</trd>
  <trdgrp lng="br">
      <trd>c'hoantaat</trd>,
      <trd>kaout c'hoant</trd>
    </trdgrp>
  <trdgrp lng="cs">
  <trd>ba&zcaron;it</trd>,
  <trd>dychtit</trd>,
  <trd>hn&aacute;t se <klr>(za n&ecaron;&ccaron;&iacute;m)</klr></trd>,
  <trd>la&ccaron;nit</trd>,
  <trd>tou&zcaron;it po</trd>,
  <trd>usilovat</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>gieren</trd>,
      <trd>begehren</trd>,
      <trd>begierig sein</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>be avid</trd>,
      <trd>covet</trd>,
      <trd>crave</trd>,
      <trd>desire</trd>,
      <trd>lust</trd>,
      <trd>yearn</trd>,
      <trd>want greatly</trd>
    </trdgrp>
  <trd lng="fr">&ecirc;tre <ind>avide</ind> (de)</trd>
  <trd lng="he">&#x5E8;&#x5D5;&#x5D3;&#x5E3; &#x5D1;&#x5E6;&#x5E2;</trd>
  <trdgrp lng="hu">
      <trd>s&oacute;v&aacute;rog</trd>,
      <trd>&aacute;h&iacute;tozik</trd>
    </trdgrp>
  <trdgrp lng="it">
      <trd>essere <ind>avido</ind></trd>,
      <trd>agognare</trd>,
      <trd>bramare</trd>,
      <trd>concupire</trd>
    </trdgrp>
  <trd lng="ja">&#x3057;&#x304D;&#x308A;&#x306B;&#x6B32;&#x3057;&#x304C;&#x308B;  <pr>&#x3057;&#x304D;&#x308A;&#x306B;&#x307B;&#x3063;&#x3057;&#x304C;&#x308B;</pr></trd>
  <trdgrp lng="ru">
      <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_zh;&c_a;&c_d;&c_n;&c_y;&c_m;</ind><klr>
          (&c_d;&c_o; &c_ch;&c_je;&c_g;&c_o;-&c_l;&c_i;&c_b;&c_o;, &c_k; &c_ch;&c_je;&c_m;&c_u;-&c_l;&c_i;&c_b;&c_o;)</klr></trd>,
      <trd>&c_a;&c_l;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_zh;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol;<klr> (&c_p;&c_je;&c_r;.)</klr></trd>
    </trdgrp>
  <trdgrp lng="sk">
  <trd>ba&zcaron;i&tcaron;</trd>,
  <trd>by&tcaron; &zcaron;iadostiv&yacute;</trd>,
  <trd>dychti&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_zh;&c_a;&c_d;&c_ib;&c_b;&#x43D;&c_i;&#x43C; <klr>(&c_d;&#x43E; &c_ch;&#x43E;&c_g;&#x43E;-&#x43D;.,
        &c_d;&#x43E; &c_ch;&#x43E;&c_g;&#x43E;-&#x43D;.)</klr></trd>,
    <trd>&c_zh;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_zh;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x773C;&#x70ED; <pr>y&#x1CE;nr&#xE8;</pr></trd>,
    <trd>&#x773C;&#x71B1; <pr>y&#x1CE;nr&#xE8;</pr></trd>,
    <trd>&#x89AC; <pr>j&#xEC;</pr></trd>,
    <trd>&#x89CA; <pr>j&#xEC;</pr></trd>,
    <trd>&#x55DC;&#x597D; <pr>sh&#xEC;h&#xE0;o</pr></trd>
  </trdgrp>
</drv>

