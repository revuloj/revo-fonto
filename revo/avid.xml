<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: avid.xml,v 1.31 2021/05/29 10:56:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>avid</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="avid.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="avid.0a.KOMUNE">
    <dif>
      Havanta nesatigeblan deziron &gcirc;ui a&ubreve; posedi ion:
      <ekz>
        popolamaso <tld/>a je spektakloj
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 10a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a pri gloro, al la&ubreve;doj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_k;&c_v;&c_a;&c_p;&c_n;&c_y;</trd>
    </trdgrp>
  <trd lng="bo">&#xF60;&#xF51;&#xF7C;&#xF51;&#xF0B;&#xF54;&#xF0B;&#xF46;&#xF7A;&#xF53;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trd lng="br">c'hoantek</trd>
  <trdgrp lng="cs">
  <trd>chtiv&yacute;</trd>,
  <trd>dychtiv&yacute;</trd>,
  <trd>la&ccaron;n&yacute;</trd>,
  <trd>&zcaron;&aacute;dostiv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>gierig</trd>,
      <trd>begierig</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>avid</trd>,
      <trd>eager</trd>,
      <trd>greedy</trd>,
      <trd>covetous</trd>,
    </trdgrp>
  <trd lng="es">&aacute;vido</trd>
  <trd lng="fr">avide</trd>
  <trd lng="he">&#x5E8;&#x5D5;&#x5D3;&#x5E3; &#x5D1;&#x5E6;&#x5E2;</trd>
  <trdgrp lng="hu">
      <trd>moh&oacute;</trd>,
      <trd>s&oacute;v&aacute;r</trd>
    </trdgrp>
  <trd lng="it">avido<klr> (agg.)</klr></trd>
  <trd lng="pt">&aacute;vido</trd>
  <trdgrp lng="ru">
      <trd>&c_zh;&c_a;&c_d;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_a;&c_l;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_zh;&c_a;&c_zh;&c_d;&c_u;&c_shch;&c_i;&c_j;<klr> (&c_p;&c_je;&c_r;.)</klr></trd>
    </trdgrp>
  <trdgrp lng="sk">
  <trd>dychtiv&yacute;</trd>,
  <trd>&zcaron;iadostiv&yacute;</trd>
</trdgrp>
</drv>

<drv mrk="avid.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="avid.0i.KOMUNE">
    <dif>
      Forte, tro <ref tip="super" cel="dezir.0i.porSiMem">deziri</ref>:
      <ekz>
        <tld/>i ripozon post longa penado;
      </ekz><ekz>
        <ind>kiu <tld/>as pli bonan, perdas plej bezonan</ind>
        <trd lng="fr">le mieux est l'ennemi du bien</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ambici">ambicii</ref>,
      <ref cel="avar.0a">avara</ref>,
      <ref cel="kapt.0i">kapti</ref>,
      <ref cel="marcip.0o">marcipano</ref>,
      <ref cel="sving.0i">svingi</ref>,
      <ref cel="ventr.0o">ventro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_v;&c_a;&c_p;&c_ib;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
  <trd lng="bo">&#xF51;&#xF7C;&#xF51;&#xF0B;&#xF46;&#xF42;&#xF66;&#xF0B;&#xF63;&#xF44;&#xF0B;</trd>
  <trdgrp lng="br">
      <trd>c'hoantaat</trd>,
      <trd>kaout c'hoant</trd>
    </trdgrp>
  <trdgrp lng="cs">
  <trd>ba&zcaron;it</trd>,
  <trd>dychtit</trd>,
  <trd>hn&aacute;t se <klr>(za n&ecaron;&ccaron;&iacute;m)</klr></trd>,
  <trd>la&ccaron;nit</trd>,
  <trd>tou&zcaron;it po</trd>,
  <trd>usilovat</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>gieren</trd>,
      <trd>begehren</trd>,
      <trd>begierig sein</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>be avid</trd>,
      <trd>covet</trd>,
      <trd>crave</trd>,
      <trd>desire</trd>,
      <trd>lust</trd>,
      <trd>yearn</trd>,
      <trd>want greatly</trd>
    </trdgrp>
  <trd lng="fr">&ecirc;tre <ind>avide</ind> (de)</trd>
  <trd lng="he">&#x5E8;&#x5D5;&#x5D3;&#x5E3; &#x5D1;&#x5E6;&#x5E2;</trd>
  <trdgrp lng="hu">
      <trd>s&oacute;v&aacute;rog</trd>,
      <trd>&aacute;h&iacute;tozik</trd>
    </trdgrp>
  <trdgrp lng="it">
      <trd>essere <ind>avido</ind></trd>,
      <trd>agognare</trd>,
      <trd>bramare</trd>,
      <trd>concupire</trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_b;&c_y;&c_t;&c_mol; <ind>&c_zh;&c_a;&c_d;&c_n;&c_y;&c_m;</ind><klr>
          (&c_d;&c_o; &c_ch;&c_je;&c_g;&c_o;-&c_l;&c_i;&c_b;&c_o;, &c_k; &c_ch;&c_je;&c_m;&c_u;-&c_l;&c_i;&c_b;&c_o;)</klr></trd>,
      <trd>&c_a;&c_l;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_zh;&c_a;&c_zh;&c_d;&c_a;&c_t;&c_mol;<klr> (&c_p;&c_je;&c_r;.)</klr></trd>
    </trdgrp>
  <trdgrp lng="sk">
  <trd>ba&zcaron;i&tcaron;</trd>,
  <trd>by&tcaron; &zcaron;iadostiv&yacute;</trd>,
  <trd>dychti&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="avid.0o">
  <kap><tld/>o</kap>
  <snc mrk="avid.0o.KOMUNE">
    <dif>
      Dezirego de akiro:
      <ekz>
        mon<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&#xF51;&#xF7C;&#xF51;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="cs">
  <trd>ba&zcaron;en&iacute;</trd>,
  <trd>chtivost</trd>,
  <trd>touha</trd>
  </trdgrp>
  <trd lng="de">Gier</trd>
  <trdgrp lng="en">
      <trd>avidity</trd>,
      <trd>greed</trd>
    </trdgrp>
  <trd lng="fr">avidit&eacute;</trd>
  <trd lng="he">&#x5E8;&#x5D5;&#x5D3;&#x5E3; &#x5D1;&#x5E6;&#x5E2;</trd>
  <trd lng="it">avidit&agrave;</trd>
  <trdgrp lng="ru">
      <trd>&c_a;&c_l;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_zh;&c_a;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  <trdgrp lng="sk">
  <trd>dychtivos&tcaron;</trd>,
  <trd>&zcaron;iadostivos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="avid.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="avid.0inda.KOMUNE">
    <dif>
      Kiun oni spontanee deziregas:
      <ekz>
        la cenzuro faras tenton nur pli interesa kaj <tld/>inda en la 
        okuloj de la tentato
        <fnt>
          <aut>V. O&scaron;lak</aut>,
          <vrk><url ref="http://www.kristana-misio-en-esperanto.si/administrator/upload/Oslak,%20Sentencoj.pdf">
          Foje e&ccirc; pensi estas &gcirc;uo</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">tr&egrave;s <ind>d&eacute;sirable</ind></trd>
</drv>

<drv mrk="avid.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="avid.0ulo.KOMUNE">
    <dif>
      Homo tre dezirema, posedema, nesatigebla:
      <ekz>
        donu fingron al <tld/>ulo, li tutan manon postulas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>chamtivec</trd>,
    <trd>chtivec</trd>,
    <trd>mamon&aacute;&rcaron;</trd>
  </trdgrp>
  <trd lng="de">gieriger Mensch</trd>
  <trdgrp lng="fr">
      <trd>homme <ind>cupide</ind></trd>,
      <trd>homme <ind>rapace</ind></trd>
    </trdgrp>
  <trd lng="he">&#x5E8;&#x5D5;&#x5D3;&#x5E3; &#x5D1;&#x5E6;&#x5E2;</trd>
  <trd lng="it">avido<klr> (sost.)</klr></trd>
  <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_ja;&c_zh;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_a;&c_l;&c_ch;&c_n;&c_y;&c_j; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;</trd>
    </trdgrp>
  <trd lng="sk">chamtivec</trd>
</drv>

<drv mrk="avid.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="avid.mal0a.KOMUNE">
    <dif>
      Kiu ne <tld/>as, ne deziras multon por si:
      <ekz>
        kiu estas senpasia, mal<tld/>a kaj krome rezona kaj erudicia, 
        meritas la nomon de granda sa&gcirc;ulo; li pasigas la lastan vivon
        <fnt>
          <aut>-, trad. Seiho Nishi</aut>
          <vrk><url ref="http://www001.upp.so-net.ne.jp/jble/sutro/darmapado.html">
          Darmapado, 352</url></vrk>
          <lok>La Japana Budhano, 1974-1975 (162-169)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;sinteress&eacute;</trd>
</drv>

<drv mrk="avid.glor0a">
  <kap>glor<tld/>a</kap>
  <snc mrk="avid.glor0a.KOMUNE">
    <dif>
      Tro deziranta gloron, famon, estimon a&ubreve; admiron de aliaj
      homoj:
      <ekz>
        li estas perfortema homo <klr>...</klr> e&ccirc; glor<tld/>a kaj
        tromemfida
        <fnt><bib>SatirRak</bib>, <lok>supersti&ccirc;o</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ambici.0a">ambicia</ref>,
      <ref cel="humil.mal0a">malhumila</ref>
    </refgrp>
  </snc>
  <trd lng="cs">cti&zcaron;&aacute;dostiv&yacute;</trd>
  <trd lng="en">thirsting for glory</trd>
  <trd lng="fr">orgueilleux</trd>
  <trdgrp lng="it">
      <trd>ambizioso</trd>,
      <trd><ind>avido</ind> di gloria</trd>,
      <trd><ind>assetato</ind> di gloria</trd>,
      <trd><ind>affamato</ind> di gloria</trd>
    </trdgrp>
  <trd lng="ru">&c_t;&c_shch;&c_je;&c_s;&c_l;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">ctiba&zcaron;n&yacute;</trd>
</drv>

<drv mrk="avid.profit0a">
  <kap>profit<tld/>a</kap>
  <snc mrk="avid.profit0a.KOMUNE">
    <dif>
      Tro ser&ccirc;anta nur sian profiton:
      <ekz>
        &ccirc;iuj, de la malgrandaj &gcirc;is la grandaj, estas
        profit<tld/>aj
        <fnt><bib>MT</bib>, <lok>&Jer; 6:13</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
  <trd>chamtiv&yacute;</trd>,
  <trd>ziskuchtiv&yacute;</trd>,
  <trd>zi&scaron;tn&yacute;</trd>
  </trdgrp>
  <trd lng="en">rapacious</trd>
  <trdgrp lng="fr">
      <trd>&acirc;pre au gain</trd>,
      <trd>cupide</trd>
    </trdgrp>
  <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_r;&c_y;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_k;&c_o;&c_r;&c_y;&c_s;&c_t;&c_o;&c_l;&c_ju;&c_b;&c_i;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
  <trdgrp lng="sk">
  <trd>chamtiv&yacute;</trd>,
  <trd>ziskuchtiv&yacute;</trd>,
  <trd>zi&scaron;tn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="avid.sang0a">
  <kap>sang<tld/>a</kap>
  <snc mrk="avid.sang0a.KOMUNE">
    <dif>
      Deziranta kruela&jcirc;ojn, amanta la vidon de sango:
      <ekz>
        eksplodas milito, li fari&gcirc;as sang<tld/>a mortigemulo
        <fnt><bib>Lanti</bib>, <lok>nia &ccirc;efa tasko</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kruel.0a">kruela</ref>
    </refgrp>
  </snc>
  <trd lng="cs">krvela&ccaron;n&yacute;</trd>
  <trd lng="en">bloodthirsty</trd>
  <trd lng="fr">sanguinaire</trd>
  <trdgrp lng="it">
    <trd>sanguinario</trd>,
    <trd><ind>avido</ind> di sangue</trd>,
    <trd><ind>assetato</ind> di sangue</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_v;&c_o;&c_zh;&c_a;&c_d;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">krvila&ccaron;n&yacute;</trd>
</drv>

<drv mrk="avid.vid0a">
  <kap>vid<tld/>a</kap>
  <snc mrk="avid.vid0a.KOMUNE">
    <dif>
      Tre deziranta &gcirc;ui la vidon de io:
      <ekz>
        la vid<tld/>a homamaso elstre&ccirc;is la kolojn, por ne
        maltrafi iajn vidinda&jcirc;ojn
        <fnt><bib>I&scirc;tar</bib>,
          <lok>&ccirc;apitro 16a, heroa sinofero</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>trop <ind>curieux</ind></trd>,
    <trd>voyeur</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_h;&c_o;&c_ch;&c_i;&c_j; &c_d;&c_o; &c_z;&c_r;&c_je;&c_l;&c_i;&c_shch;</trd>
</drv>
</art>
