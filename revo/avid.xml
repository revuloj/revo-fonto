<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>avid</rad>/a
</kap>
<drv mrk="avid.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc>
     <dif>
       Fortege deziranta ion:
       <ekz>
         <tld/>a je gloro, al la&ubreve;doj, pri
         ri&ccirc;a&jcirc;oj;
       </ekz>
       <ekz>
         donu fingron al <tld/>ulo, li tutan manon postulas
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
       </ekz>
       <ekz>
         sang<tld/>a
         <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
         <lok>Na&hcirc;um 3, 1</lok>)</fnt>;
       </ekz>
       <ekz>
         nobla <tld/>eco<fnt>Z</fnt>;
       </ekz>
       <ekz>
         mon<tld/>a.
       </ekz>
     </dif>
   </snc>
   <trd lng="de">gierig</trd>
   <trd lng="fr">avide</trd>
   <trdgrp lng="ru">
     <trd>&#1078;&#1072;&#1076;&#1085;&#1099;&#1081;</trd>,
     <trd>&#1072;&#1083;&#1095;&#1085;&#1099;&#1081;</trd>,
     <trd>&#1078;&#1072;&#1078;&#1076;&#1091;&#1097;&#1080;&#1081;</trd>
   </trdgrp>
</drv>
<drv mrk="avid.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Esti <tld/>a je:
       <ekz>
         <tld/>i ripozon post longa penado;
       </ekz>
       <ekz>
         kiu <tld/>as pli bonan, perdas plej bezonan
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="avar.0a">avara</ref>,
       <ref cel="dezir.0i">deziri</ref>,
       <ref cel="sopir.0i">sopiri</ref>,
       <ref cel="ambici">ambicii</ref>.
     </refgrp>
     <refgrp tip="vid">
       <ref cel="ventr.0o">ventro</ref>,
       <ref cel="lepor.0o">leporo</ref>,
       <ref cel="marcip.0o">marcipano</ref>,
       <ref cel="sving.0i">svingi</ref>,
       <ref cel="kapt.0i">kapti</ref>.
     </refgrp>
   </snc>
   <trd lng="de">gieren</trd>
   <trd lng="fr">&ecirc;tre <ind>avide</ind> (de)</trd>
   <trdgrp lng="ru">
     <trd>&#1073;&#1099;&#1090;&#1100; 
&#1078;&#1072;&#1076;&#1085;&#1099;&#1084; <klr>(&#1076;&#1086; 
&#1095;&#1077;&#1075;&#1086;-&#1083;&#1080;&#1073;&#1086;, &#1082; 
&#1095;&#1077;&#1084;&#1091;-&#1083;&#1080;&#1073;&#1086;)</klr></trd>,
     <trd>&#1078;&#1072;&#1076;&#1085;&#1080;&#1095;&#1072;&#1090;&#1100;</trd>,
     <trd>&#1078;&#1072;&#1078;&#1076;&#1072;&#1090;&#1100;</trd>
   </trdgrp>
</drv>
</art>
