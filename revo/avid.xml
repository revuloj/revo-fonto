<drv mrk="avid.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="avid.0a.KOMUNE">
    <dif>
      Havanta nesatigeblan deziron &gcirc;ui a&ubreve; posedi ion:
      <ekz>
        popolamaso <tld/>a je spektakloj
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 10a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a pri gloro, al la&ubreve;doj.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x6081; <pr>ju&#xE0;n</pr></trd>,
    <trd>&#x8CAA; <pr>t&#x101;n</pr></trd>,
    <trd>&#x8D2A; <pr>t&#x101;n</pr></trd>,
    <trd>&#x6C38;&#x4E0D;&#x6EE1;&#x8DB3; <pr>y&#x1D2;ngb&#xF9;m&#x1CE;nz&#xFA;</pr></trd>,
    <trd>&#x6C38;&#x4E0D;&#x6EFF;&#x8DB3; <pr>y&#x1D2;ngb&#xF9;m&#x1CE;nz&#xFA;</pr></trd>,
    <trd>&#x617E;&#x58D1;&#x96E3;&#x586B; <pr>y&#xF9;h&#xE8;n&#xE1;nti&#xE1;n</pr></trd>,
    <trd>&#x6B32;&#x58D1;&#x96BE;&#x586B; <pr>y&#xF9;h&#xE8;n&#xE1;nti&#xE1;n</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_k;&c_v;&c_a;&c_p;&c_n;&c_y;</trd>
    </trdgrp>
  <trd lng="bo">&#xF60;&#xF51;&#xF7C;&#xF51;&#xF0B;&#xF54;&#xF0B;&#xF46;&#xF7A;&#xF53;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trd lng="br">c'hoantek</trd>
  <trdgrp lng="cs">
  <trd>chtiv&yacute;</trd>,
  <trd>dychtiv&yacute;</trd>,
  <trd>la&ccaron;n&yacute;</trd>,
  <trd>&zcaron;&aacute;dostiv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>gierig</trd>,
      <trd>begierig</trd>
    </trdgrp>
  <trdgrp lng="en">
      <trd>avid</trd>,
      <trd>eager</trd>,
      <trd>greedy</trd>,
      <trd>covetous</trd>,
    </trdgrp>
  <trd lng="es">&aacute;vido</trd>
  <trd lng="fr">avide</trd>
  <trd lng="he">&#x5E8;&#x5D5;&#x5D3;&#x5E3; &#x5D1;&#x5E6;&#x5E2;</trd>
  <trdgrp lng="hu">
      <trd>moh&oacute;</trd>,
      <trd>s&oacute;v&aacute;r</trd>
    </trdgrp>
  <trd lng="it">avido<klr> (agg.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6E07;&#x671B;&#x3057;&#x3066;&#x3044;&#x308B;  <pr>&#x304B;&#x3064;&#x307C;&#x3046;&#x3057;&#x3066;&#x3044;&#x308B;</pr></trd>,
    <trd>&#x3080;&#x3055;&#x307C;&#x308B;&#x3088;&#x3046;&#x306A;</trd>
  </trdgrp>
  <trd lng="pt">&aacute;vido</trd>
  <trdgrp lng="ru">
      <trd>&c_zh;&c_a;&c_d;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_a;&c_l;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_zh;&c_a;&c_zh;&c_d;&c_u;&c_shch;&c_i;&c_j;<klr> (&c_p;&c_je;&c_r;.)</klr></trd>
    </trdgrp>
  <trdgrp lng="sk">
  <trd>dychtiv&yacute;</trd>,
  <trd>&zcaron;iadostiv&yacute;</trd>
