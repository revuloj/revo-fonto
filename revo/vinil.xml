<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vinil</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vinil.0o">
  <kap><tld/>o</kap>
  <snc mrk="vinil.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Baza substanco de diversaj <ref tip="super"
      cel="plast.0o.TEK">plastoj</ref> por izoliloj, &scirc;tofoj,
      memglua&jcirc;oj, lakoj... (&hcirc;emie CH<sub>2</sub>=CH-):
      <ekz>
        li portas <tld/>sakon
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;ardanta taglibro en
        Mogadi&scirc;o&rightquot;, p. 131a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">vinyle</trd>
  <trd lng="hu">vinil</trd>
  <trdgrp lng="pt">
   <trd>vinil</trd>,
   <trd>vinila</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_i;&c_l;</trd>
</drv>
</art>

