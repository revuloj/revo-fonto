<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kamerun</rad>/o
</kap>

<drv mrk="kamerun.0o">
  <kap>
    <tld lit="K"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="kamerun.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Jaundeo.
      Teritorio de Respubliko Kameruno.
      ISO-3166 kodo: CM.
      &Gcirc;i limas kun
      <ref tip="vid" cel="afrik.centr0o.GEOG">Centr-Afriko</ref>,
      <ref tip="vid" cel="cxad.0o.GEOG">&Ccirc;ado</ref>,
      <ref tip="vid" cel="gvine.ekvatora0o.GEOG">Ekvatora Gvineo</ref>,
      <ref tip="vid" cel="gabon.0o.GEOG">Gabono</ref>,
      <ref tip="vid" cel="kong.0o.brazavila.GEOG">Kongo 
      Brazavila</ref>,
      <ref tip="vid" cel="nigxeri.0o.GEOG">Ni&gcirc;erio</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref> (Gvinea 
      Golfo).
    </dif>
    <trdgrp lng="pl">
      <trd>Kamerun</trd>,
      <trd>Republika <ind>Kamerunu</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

