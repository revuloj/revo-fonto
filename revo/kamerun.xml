<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>kamerun</rad>/o
    </kap>

    <drv mrk="kamerun.K0o">
      <kap>
        <tld lit="K"/>o
      </kap>
      <snc mrk="kamerun.0o.monto">
        <uzo tip="fak">GEOG</uzo>
        <fnt><bib>PIV2</bib></fnt>
        <dif>
          La plej alta monto &mdash; fakte vulkano &mdash; de la Okcidenta
          Afriko
          (9&ring;10'E 4&ring;12'N), surloke nomita 
          <ctl>Monto de grandeco</ctl> (Mongo ma Ndemi).
        </dif>
        <trd lng="fr">Mont Cameroun</trd>
        <trd lng="nl">Kameroenberg</trd>
      </snc>

      <snc mrk="kamerun.K0o.GEOG">
        <uzo tip="fak">GEOG</uzo>
        <uzo tip="fak">POL</uzo>
        <fnt>
          <vrk>Oficiala Informo de AdE</vrk>,
          <lok>numero 12</lok>
        </fnt>
        <dif>
          Lando en la centra <ref tip="malprt"
          cel="afrik.A0o">Afriko</ref>
          kun &ccirc;efurbo Jaundeo,
          teritorio de Respubliko Kameruno,
          kunlima kun
          <ref tip="vid" cel="afrik.centr0o.GEOG">Centr-Afriko</ref>,
          <ref tip="vid" cel="cxad.CX0o.GEOG">&Ccirc;ado</ref>,
          <ref tip="vid" cel="gvine.ekvatora0o.GEOG">Ekvatora
          Gvineo</ref>,
          <ref tip="vid" cel="gabon.G0o.GEOG">Gabono</ref>,
          <ref tip="vid" cel="kong.K0o.brazavila.GEOG">Kongo 
            Brazavila</ref>,
          <ref tip="vid" cel="nigxeri.N0o.GEOG">Ni&gcirc;erio</ref>
          kaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantiko</ref>
          (Gvinea Golfo); 
          ISO-3166 kodo: CM.
        </dif>
        <ref tip="sin" cel="kameruni.K0o">Kamerunio</ref>
         <trd lng="nl">Kameroen</trd>
      </snc>
      <trd lng="bg">&c_K;&c_a;&c_m;&c_je;&c_r;&c_u;&c_n;</trd>
      <trd lng="de">Kamerun</trd>
      <trd lng="en">Cameroon</trd>
      <trd lng="fr">Cameroun</trd>
      <trdgrp lng="pl">
        <trd>Kamerun</trd>,
        <trd>Republika <ind>Kamerunu</ind></trd>
      </trdgrp>
      <trd lng="ru">&c_K;&c_a;&c_m;&c_je;&c_r;&c_u;&c_n;</trd>
    </drv>

  </art>

