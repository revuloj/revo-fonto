<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: linac.xml,v 1.12 2022/06/24 12:57:44 revo Exp $">
<kap>
  <rad>linac</rad>/oj
</kap>
<drv mrk="linac.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Linaceae</trd>),
      kun nedividitaj sentigaj alterne sidantaj folioj 
      kaj kvinnombraj floroj:
      <ekz>
        <ref tip="prt" cel="lin.0o">lino</ref>
        estas genro <klr>[&#8230;el]</klr> la familio de <tld/>oj, 
        indi&gcirc;ena al moderaj klimatoj de la norda hemisfero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18584">Lino</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        lina&jcirc;o (tolo farita el lino-planto t.e. <tld/>oj)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=69637">&Scirc;tofo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_jo;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="en">Linaceae</trd>
  <trd lng="ca">lin&agrave;cies</trd>
  <trd lng="de">Leingew&auml;chse</trd>
  <trd lng="fr">Linac&eacute;es</trd>
  <trd lng="hu">lenf&eacute;l&eacute;k</trd>
  <trd lng="nl">vlasfamilie</trd>
  <trd lng="pl">lnowate</trd>
  <trdgrp lng="pt">
    <trd>humiri&aacute;ceas</trd>,
    <trd>lin&aacute;ceas</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_mol;&c_n;&c_o;&c_v;&c_y;&c_je;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_l;&c_jo;&c_n;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>
