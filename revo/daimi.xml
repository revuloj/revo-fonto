<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: daimi.xml,v 1.7 2023/11/04 13:16:26 revo Exp $">
    <kap>
      <rad>daimi</rad>/o
    </kap>

<drv mrk="daimi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc mrk="daimi.0o.japana_princo">
    <dif>
      Japana <ref tip="super" cel="feuxd.0estro">fe&ubreve;destro</ref>:
      <ekz>
        <nom>Yuki Hideyasu</nom> estis japana <tld/>o, kiu vivis dum la <klr>[&#x2026;]</klr> komenco de
        la Edo-epoko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=212443">Yuki Hideyasu</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1615 <nom>Hasekura Cunenaga</nom>, vasalo de <nom>Date Masamune</nom>, potenca <tld/>o en
        la
        nord-orienta Japanio, marveturis tra Meksiko al Hispanio kaj Vatikano, kie
        lin a&ubreve;dience ricevis la papo Pa&ubreve;lo la 5a
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2015/011398.php">Sekretaj kristanoj, &ccirc;u veraj
          kristanoj?</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="princ.0o.nobelo">princo<sncref/></ref>
    <trd lng="de">Daimy&#x14D;</trd>
    <trd lng="en">daimyo</trd>
    <trd lng="fr">da&iuml;mio</trd>
    <trd lng="ja">&#x5927;&#x540D; <pr>&#x3060;&#x3044;&#x307F;&#x3087;&#x3046;</pr></trd>
    <trd lng="zh">&#x9999;&#x6E2F;&#x65E5;&#x4F54;&#x6642;&#x671F; <pr>xi&#x101;ngg&#x1CE;ngr&#xEC;zh&#xE0;nsh&#xED;q&#x12B;</pr></trd>
</snc>
  <trd lng="be">&c_d;&c_a;&c_j;&c_m;&c_jo;</trd>
  <trd lng="uk">&c_d;&c_a;&c_j;&#x43C;&#x44C;&#x43E;</trd>
</drv>

  </art>

