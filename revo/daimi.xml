<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: daimi.xml,v 1.3 2022/04/26 17:11:47 revo Exp $">
    <kap>
      <rad>daimi</rad>/o
    </kap>

<drv mrk="daimi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc mrk="daimi.0o.japana_princo">
    <dif>
      Japana <ref tip="super" cel="feuxd.0estro">fe&ubreve;destro</ref>:
      <ekz>
        <nom>Yuki Hideyasu</nom> estis japana <tld/>o, kiu vivis dum la <klr>[&#x2026;]</klr> komenco de
        la Edo-epoko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=212443">Yuki Hideyasu</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en 1615 <nom>Hasekura Cunenaga</nom>, vasalo de <nom>Date Masamune</nom>, potenca <tld/>o en la
        nord-orienta Japanio, marveturis tra Meksiko al Hispanio kaj Vatikano, kie
        lin a&ubreve;dience ricevis la papo Pa&ubreve;lo la 5a
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2015/011398.php">Sekretaj kristanoj, &ccirc;u veraj kristanoj?</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="princ.0o.nobelo">princo<sncref/></ref>
    <trd lng="de">Daimy&#x14D;</trd>
    <trd lng="en">daimyo</trd>
    <trd lng="fr">da&iuml;mio</trd>
    <trd lng="ja">&#x5927;&#x540D;</trd>
  </snc>
  <trd lng="uk">&#x434;&#x430;&#x439;&#x43C;&#x44C;&#x43E;</trd>
</drv>

  </art>

