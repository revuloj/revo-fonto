<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kontin3.xml,v 1.29 2020/02/15 11:10:20 revo Exp $">
<kap>
 <ofc>4</ofc>
 <rad>kontinu</rad>/a <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kontin3.0a">
  <kap><tld/>a</kap>
  <snc mrk="kontin3.0a.KOMUNE">
    <dif>
      Konsistanta el eroj, kiuj formas seninterrompan, senmankan vicon:
    </dif>
    <subsnc mrk="kontin3.0a.tempo">
      <dif>
        En tempo:
        <ekz>
          <tld/>a pluvo, elektra fluo, febro, bruo;
        </ekz>
        <ekz>
          fulmoj <tld/>e zigzagas.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="kontin3.0a.spaco">
      <dif>
        En spaco:
        <ekz>
          <tld/>a muro;
        </ekz>
        <ekz>
          <tld/>a linio;
        </ekz>
        <ekz>
          <tld/>aj rimoj <klr>(<tld/>a
          <ref cel="rim.0arangxo">rimaran&gcirc;o</ref>)</klr>.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="dauxr.0a">da&ubreve;ra</ref>,
      <ref cel="konsta.0a">konstanta</ref>,
      <ref cel="cxes.sen0a">sen&ccirc;esa</ref>,
      <ref cel="mank.seninter0a.KOMUNE">senintermanka</ref>,
      <ref cel="pec.unu0a.KOMUNE">unupeca</ref>,
    </refgrp>
    <trdgrp lng="de">
      <trd>ununterbrochen</trd>,
      <trd>st&auml;ndig</trd>,
      <trd>best&auml;ndig</trd>,
      <trd>kontinuierlich</trd>
    </trdgrp>
    <trd lng="en">continuous</trd>
    <trdgrp lng="hu">
      <trd>folyamatos</trd>,
      <trd>szakadatlan</trd>,
      <trd>folytonos</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ci&aogonek;g&lstroke;y</trd>,
      <trd>nieprzerwany</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_je;&c_s;&c_p;&c_r;&c_je;&c_r;&c_y;&c_v;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_p;&c_l;&c_o;&c_sh;&c_n;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kontin3.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>EKV</bib><lok>&para;223</lok></fnt>
    <subsnc mrk="kontin3.0a.cxepunkto">
      <dif>
        <klr>(p.p. <ref cel="bild.0igo.MAT">bildigo</ref> 
        <frm am="f"><k>f</k></frm>
        de <ref cel="spac.topologia0o">topologia spaco</ref> 
        <frm am="bb E"><g>E</g></frm>
        al topologia spaco <frm am="bb F"><g>F</g></frm>,
        &ccirc;e punkto <frm am="a in bb E"><k>a</k>&#8712;<g>E</g></frm>)</klr>
        Tia, ke &ccirc;iu ajn
        <ref cel="cxirka.0ajxo.topologio">&ccirc;irka&ubreve;a&jcirc;o</ref>
        de <frm am="f(a)"><k>f</k>(<k>a</k>)</frm> inkluzivas la bildon
        per <frm am="f"><k>f</k></frm> de iu &ccirc;irka&ubreve;a&jcirc;o
        de <frm am="a"><k>a</k></frm>.
      </dif>
      <rim>
        En malpli faka lingva&jcirc;o: <frm am="f(x)"><k>f</k>(<k>x</k>)</frm>
        senlime proksimi&gcirc;as al <frm am="f(a)"><k>f</k>(<k>a</k>)</frm>,
        kiam <frm am="x"><k>x</k></frm> senlime proksimi&gcirc;as al
        <frm am="a"><k>a</k></frm>.
      </rim>
    </subsnc>
    <subsnc mrk="kontin3.0a.cxie">
      <fnt><bib>MatTerm</bib><lok>p. 20</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="bild.0igo.MAT">bildigo</ref>
        <frm am="f"><k>f</k></frm> de
        <ref cel="spac.topologia0o">topologia spaco</ref>
        <frm am="bb E"><g>E</g></frm> al topologia spaco
        <frm am="bb F"><g>F</g></frm>)</klr>
        Tia, ke &gcirc;i estas
        <ref cel="kontin3.0a.cxepunkto"><tld/>a <sncref/></ref>
        &ccirc;e &ccirc;iuj punktoj de <frm am="bb E"><g>E</g></frm>:
        <ekz>
          <frm am="f"><k>f</k></frm> estas <tld/>a, se kaj nur se la
          inversa
          bildo
          de &ccirc;iu ajn
          <ref cel="ferm.mal0ita.topologio">malfermita subaro</ref> de
          <frm am="bb F"><g>F</g></frm> estas malfermita subaro de
          <frm am="bb E"><g>E</g></frm>;
        </ekz>
        <ekz>
          la idento-bildigo de <frm am="bb E"><g>E</g></frm> al si mem
          (kun la
          sama topologio) estas <tld/>a;
        </ekz>
        <ekz>
          la kunliga&jcirc;o de du <tld/>aj bildigoj estas mem <tld/>a;
        </ekz>
        <ekz>
          kiu ajn bildigo inter topologiaj spacoj kun diskreta topologio
          estas <tld/>a.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="homeom.0o">homeomorfio</ref>.
      </refgrp>
    </subsnc>
    <trd lng="de">stetig</trd>
    <trd lng="en">continuous</trd>
    <trd lng="pl">ci&aogonek;g&lstroke;y</trd>
  </snc>
  <trd lng="ca">continu</trd>
  <trdgrp lng="cs">
    <trd>kontinu&aacute;ln&iacute;</trd>,
    <trd>nep&rcaron;etr&zcaron;it&yacute;</trd>,
    <trd>plynul&yacute;</trd>,
    <trd>souvisl&yacute;</trd>,
    <trd>spojit&yacute;</trd>,
    <trd>trval&yacute;</trd>,
    <trd>ustavi&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="es">continuo, -a</trd>
  <trd lng="fr">continu</trd>
  <trd lng="hu">folytonos</trd>
  <trdgrp lng="nl">
    <trd>onafgebroken</trd>,
    <trd>continu</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ci&aogonek;g&lstroke;y</trd>,
    <trd>nieprzerwany</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_p;&c_r;&c_je;&c_r;&c_y;&c_v;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>kontinu&aacute;lny</trd>,
    <trd>nepretr&zcaron;it&yacute;</trd>,
    <trd>s&uacute;visl&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="kontin3.0i">
 <kap><tld/>i</kap>
 <gra><vspec>ntr</vspec></gra>
 <snc mrk="kontin3.0i.KOMUNE">
   <dif>
     Esti <tld/>a, okazi senrompe:
     <ekz>
       la kolekto <tld/>as de la unua jaro &gcirc;is hodia&ubreve;.
     </ekz>
   </dif>
 </snc>
  <trd lng="ca">continuar <klr>(intr.)</klr></trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t souvisl&yacute;</trd>,
    <trd>pokra&ccaron;ovat</trd>,
    <trd>trvat</trd>
  </trdgrp>
  <trd lng="en">be continuous</trd>
  <trd lng="es">ser continuo, -a</trd>
  <trdgrp lng="fr">
   <trd>se <ind>poursuivre</ind></trd>,
   <trd>continuer<klr tip="ind"> (intr.)</klr></trd>
 </trdgrp>
  <trd lng="hu">folytonos</trd>
  <trd lng="nl">doorlopen</trd>
  <trd lng="pl">ci&aogonek;gn&aogonek;&cacute; si&eogonek;</trd>
  <trdgrp lng="sk">
    <trd>by&tcaron; kontinu&aacute;lny</trd>,
    <trd>nepretr&zcaron;it&yacute;</trd>,
    <trd>trva&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kontin3.0eco">
 <kap><tld/>eco</kap>
 <snc mrk="kontin3.0eco.KOMUNE">
   <dif>
     Eco de io <tld/>a; neinterrompita interligo
     de partoj a&ubreve; periodoj:
     <ekz>
       <tld/>eco de la spino;
     </ekz>
     <ekz>
       <uzo tip="fak">MAT</uzo>
       esplori la <tld/>econ de funkcio;
     </ekz>
     <ekz>
       rompo de <tld/>eco;
     </ekz>
     <ekz>
       la <tld/>eco de tiu bruo estas netolerebla;
     </ekz>
     <ekz>
       nia lingvo, dank' al la reguleco de sia progresado, neniam
       perdas la <tld/>econ kun la lingvo de tempo pli frua<fnt>Z</fnt>.
     </ekz>
   </dif>
 </snc>
  <trdgrp lng="ca">
   <trd>continu&iuml;tat</trd>,
   <trd>persist&egrave;ncia</trd>
 </trdgrp>
  <trdgrp lng="cs">
    <trd>kontinuita</trd>,
    <trd>kontinu&aacute;lnost</trd>,
    <trd>souvislost</trd>,
    <trd>spojitost</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kontinuit&auml;t</trd>,
     <trd>Best&auml;ndigkeit</trd>,
     <trd>Stetigkeit</trd>
 </trdgrp>
  <trd lng="en">continuity</trd>
  <trd lng="es">continuidad</trd>
  <trd lng="fr">continuit&eacute;</trd>
  <trdgrp lng="hu">
   <trd>folyamatoss&aacute;g</trd>,
   <trd>folytonoss&aacute;g</trd>,
   <trd>szakadatlans&aacute;g</trd>,
   <trd>kontinuit&aacute;s</trd>
 </trdgrp>
  <trd lng="nl">continu&iuml;teit</trd>
  <trd lng="pl">ci&aogonek;g&lstroke;o&sacute;&cacute;</trd>
  <trd lng="ru">&c_n;&c_je;&c_p;&c_r;&c_je;&c_r;&c_y;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kontinuita</trd>,
    <trd>nepretr&zcaron;itos&tcaron;</trd>,
    <trd>spojitos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kontin3.0ega">
 <kap><tld/>ega</kap>
 <uzo tip="fak">MAT</uzo>
 <fnt><bib>EKV</bib><lok>&para;225</lok></fnt>
 <snc mrk="kontin3.0ega.MAT">
   <dif>
     <klr>(p.p. <ref cel="bild.0igo.MAT">bildigo</ref>
     <frm am="f"><k>f</k></frm> de
     <ref cel="spac.metrika0o">metrika spaco</ref> <frm am="bb
     E"><g>E</g></frm>
     al metrika spaco <frm am="bb F"><g>F</g></frm>)</klr>
     Tia, ke por &ccirc;iu pozitiva reelo <frm am="epsilon">&epsilon;</frm> ekzistas
     tia
     reelo <frm am="alpha">&alfa;</frm>, ke se la distanco inter ajnaj
     punktoj
     <frm am="x"><k>x</k></frm> kaj <frm am="y"><k>y</k></frm> 
     en <frm am="bb E"><g>E</g></frm> estas
     pli malgranda ol <frm am="alpha">&alfa;</frm>, tiam la distanco inter la
     
     bildoj de
     <frm am="x"><k>x</k></frm> kaj <frm am="y"><k>y</k></frm> per 
     <frm am="f"><k>f</k></frm>
     estas pli malgranda ol <frm am="epsilon">&epsilon;</frm>:
     <ekz>
       &ccirc;iu <tld/>ega bildigo estas anka&ubreve;
       <ref cel="kontin3.0a.cxie"><tld/>a <sncref/></ref>, sed la malo
       ne veras.
     </ekz>
   </dif>
 </snc>
 <trd lng="ca">uniformement continu</trd>
 <trd lng="de">gleichm&auml;&szlig;ig <ind>stetig</ind></trd>
 <trd lng="en">uniformly <ind>continous</ind></trd>
 <trd lng="es"><ind>continuidad</ind> uniforme</trd>
 <trd lng="fr">uniform&eacute;ment <ind>continu</ind></trd>
 <trd lng="hu">egyenletesen <ind>folytonos</ind></trd>
 <trd lng="pl">jednostajnie <ind>ci&aogonek;g&lstroke;y</ind></trd>
 <trd lng="ru">&c_r;&c_a;&c_v;&c_n;&c_o;&c_m;&c_je;&c_r;&c_n;&c_o; <ind>&c_n;&c_je;&c_p;&c_r;&c_je;&c_r;&c_y;&c_v;&c_n;&c_y;&c_j;</ind></trd>
</drv>

<drv mrk="kontin3.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <snc mrk="kontin3.0igi.KOMUNE">
    <dif>
      Fari, ke io estu <tld/>a:
      <ekz>
        li <tld/>igis la laboron de siaj
        anta&ubreve;intoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dauxr.0igi">da&ubreve;rigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>continuar <klr>(tr.)</klr></trd>,
    <trd>prosseguir</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nechat trvat</trd>,
    <trd>u&ccaron;init spojit&yacute;m</trd>
  </trdgrp>
  <trd lng="de">fortsetzen</trd>
  <trd lng="es">continuar</trd>
  <trd lng="fr">continuer<klr tip="ind"> (tr.)</klr></trd>
  <trdgrp lng="hu">
    <trd>folytat</trd>,
    <trd>folyamatoss&aacute; tesz</trd>
  </trdgrp>
  <trd lng="nl">voortzetten</trd>
  <trd lng="pl">kontynuowa&cacute;</trd>
  <trd lng="ro">a continua</trd>
  <trd lng="sk">necha&tcaron; trva&tcaron;</trd>
</drv>

<drv mrk="kontin3.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="kontin3.mal0a.KOMUNE">
    <dif>
      Prezentanta &ccirc;esojn, rompojn, interspacojn:
      <ekz>
        la geografiaj kaj klimataj &scirc;an&gcirc;i&gcirc;oj, kiuj
        certe okazis ene de lastaj geologiaj tempoj,
        sendube interrompis a&ubreve; faris mal<tld/>aj
        la anta&ubreve;e <tld/>ajn etendi&gcirc;ojn de multaj specioj
        <fnt>
          <bib>OriginoDeSpecioj</bib>,
          <lok>&ccirc;apitro 11a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="diskre.0a.nombrebla">diskreta<sncref/></ref>,
    <ref cel="romp.inter0i.cxesigi">interrompi</ref>
  </refgrp>
  <trdgrp lng="fr">
    <trd>discret<klr> (non continu)</klr></trd>,
    <trd>interrompu</trd>
  </trdgrp>
  <trd lng="sk">pretr&zcaron;it&yacute;</trd>
  <trd lng="cs">p&rcaron;etr&zcaron;it&yacute;</trd>
</drv>
</art>
