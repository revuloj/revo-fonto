<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kantar1.xml,v 1.20 2022/03/21 17:51:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kantarid</rad>/o
</kap>
<drv mrk="kantar1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kantar1.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio el la ordo <ref tip="super" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Lytta vesicatoria</trd>),
      metale verda skarabo, kies veneno <klr>(kantaridino)</klr>
      vezikigas la ha&ubreve;ton kaj estis uzata kiel afrodiziiga&jcirc;o
      kaj por vezikplastroj:
      <ekz>
        li almetis al si sur la nukon kaj sur la dorson po unu granda
        <tld/>a plastro, por eltiri la frenezecon
        <fnt><bib>Fab1</bib>, <lok>galo&scirc;oj de feli&ccirc;o, 4</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Mankas dif de "vezikplastro".
    <aut>MB</aut>
  </adm>
  <trd lng="br">c'hwil-glas</trd>
  <trd lng="ca">cant&agrave;rida</trd>
  <trd lng="de">Spanische Fliege</trd>
  <trdgrp lng="en">
    <trd>sailor beetle</trd>,
    <trd>soldier beetle</trd>
  </trdgrp>
  <trd lng="fr">cantharide</trd>
  <trd lng="hu">k&odblac;risbog&aacute;r</trd>
  <trd lng="nl">Spaanse <ind>vlieg</ind></trd>
  <trdgrp lng="pl">
    <trd>kantaryda</trd>,
    <trd>pryszczel lekarski</trd>
  </trdgrp>   
  <trd lng="ro">cantarid&abreve;</trd>
  <trd lng="ru">&c_sh;&c_p;&c_a;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_u;&c_h;&c_a;</trd>
  <trd lng="sk">&scaron;panielska mu&scaron;ka</trd>
  <trd lng="cs">moucha &scaron;pan&ecaron;lsk&aacute;</trd>
  <trd lng="uk">&#x448;&#x43F;&#x430;&#x43D;&#x441;&#x44C;&#x43A;&#x430; &#x43C;&#x443;&#x445;&#x430;</trd>
</drv>
</art>
