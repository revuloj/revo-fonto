<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kantarid</rad>/o
</kap>
<drv mrk="kantar1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio el la ordo <ref tip="super" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Lytta vesicatoria</trd>),
      metale verda skarabo, kies veneno <klr>(kantaridino)</klr>
      vezikigas la ha&ubreve;ton kaj estis uzata kiel afrodiziiga&jcirc;o
      kaj por vezikplastroj.
    </dif>
  </snc>
  <adm>
    Mankas dif de "vezikplastro".
    <aut>MB</aut>
  </adm>
  <trd lng="de">Spanische Fliege</trd>
  <trdgrp lng="en">
    <trd>sailor beetle</trd>,
    <trd>soldier beetle</trd>
  </trdgrp>
  <trd lng="fr">cantharide</trd>
  <trd lng="hu">k&odblac;risbog&aacute;r</trd>
  <trd lng="nl">Spaanse <ind>vlieg</ind></trd>
  <trd lng="ru">&c_sh;&c_p;&c_a;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_u;&c_h;&c_a;</trd>
</drv>
</art>

