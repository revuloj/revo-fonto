<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: haleba.xml,v 1.18 2019/03/27 19:10:18 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>halebard</rad>/o
</kap>
<drv mrk="haleba.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:bataliloj" cel="lanc.0o">Lanco</ref>
      kun duflanka ekstrema&jcirc;o, similanta al hakila klingo:
      <ekz>
        en sia spirito li povis vidi soldatojn kun <tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        malamikaj soldatoj kun brilantaj bataliloj kaj en diverskoloraj uniformoj, kun
        lancoj kaj <tld/>oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Lasta son&gcirc;o de maljuna kverko</lok>
        </fnt>;
      </ekz>
      <ekz>
        aperas <klr>[&#8230;]</klr> ia ombro, <klr>[&#8230;]</klr> anta&ubreve; la teruritaj oficiroj,
        proksime tiel, ke per <tld/>o &gcirc;in ili povus tu&scirc;i
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>                      
  <trd lng="be">&c_a;&c_l;&c_je;&c_b;&c_a;&c_r;&c_d;&c_a;</trd>
  <trd lng="bg">&c_a;&c_l;&c_je;&c_b;&c_a;&c_r;&c_d;&c_a;</trd>
  <trd lng="br">halabardenn</trd>
  <trd lng="ca">alabarda</trd>
  <trd lng="de">Hellebarde</trd>
  <trd lng="el">&alfa;&lambda;&alfa;&beta;&alfa_ton;&rho;&delta;&alfa;</trd>
  <trd lng="en">halberd</trd>
  <trd lng="fr">hallebarde</trd>
  <trd lng="hu">alab&aacute;rd</trd>
  <trd lng="it">alabarda</trd>
  <trd lng="nl">hellebaard</trd>
  <trdgrp lng="pl">
    <trd>halabarda</trd>,
    <trd>berdysz</trd>
  </trdgrp>
  <trd lng="pt">alabarda</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_je;&c_b;&c_a;&c_r;&c_d;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_r;&c_d;&c_y;&c_sh;</trd>
  </trdgrp>
</drv>
</art>
