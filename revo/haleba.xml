<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>halebard</rad>/o
</kap>
<drv mrk="haleba.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="arm.0ilo">Armilo</ref>, konsistanta el
      <ref cel="lanc.0o">lanco</ref> kun duflanka ekstrema&jcirc;o,
      similanta al hakila klingo.
    </dif>
  </snc>
  <trd lng="de">Hellebarde</trd>
  <trd lng="el">&alfa;&lambda;&alfa;&beta;&alfa_ton;&rho;&delta;&alfa;</trd>
  <trd lng="fr">hallebarde</trd>
  <trd lng="nl">hellebaard</trd>
  <trd lng="pt">alabarda</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_je;&c_b;&c_a;&c_r;&c_d;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_r;&c_d;&c_y;&c_sh;</trd>
  </trdgrp>
</drv>
</art>
