<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: soroban.xml,v 1.10 2024/03/25 06:40:52 revo Exp $">
<kap>
<rad>soroban</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="soroban.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Japana <ref tip="super" cel="abak.0o.kalkulilo">abako</ref>:
      <ekz>
        <klr>[li]</klr> instruis kalkuli per <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>2004, &#x2116; 2 (112)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kun globetoj povantaj repreni &ccirc;iuj sian originan lokon
        en duonsekundo
        <fnt>"El Popola &Ccirc;inio", 8/1994, 
          <lok>"Kolektanto de abakoj", p.50</lok>
        </fnt>;
      </ekz>
      <ekz>
        membroj de la japana delegacio prezentis interesajn aspektojn de japana hejma
        arto, precipe pri &ccirc;anojo, ikebano, <tld/>o, paperfalda arto kaj belskribado
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n; <klr>(&c_ja;&c_p;&c_o;&c_n;&c_s;&c_k;&c_ib;&c_ja; &c_l;&c_ib;&c_ch;&c_y;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_ib;)</klr></trd>
  <trd lng="cs">japonsk&eacute; po&ccaron;&iacute;tadlo</trd>
  <trdgrp lng="de">
    <trd>Abakus</trd>,
    <trd>Soroban</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>soroban</trd>,
    <trd>abacus</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>soroban</trd>,
    <trd>boulier</trd>
  </trdgrp>
  <trd lng="pl">soroban</trd>
  <trd lng="sk">japonsk&eacute; po&ccaron;&iacute;tadlo</trd>
  <trd lng="uk">(&#x44F;&#x43F;&#x43E;&#x43D;&c_s;&#x44C;&#x43A;&c_a;) &c_a;&c_b;&c_a;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x7B97;&#x76D8; <pr>su&#xE0;np&#xE1;n</pr></trd>,
    <trd>&#x7B97;&#x76E4; <pr>su&#xE0;np&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>

