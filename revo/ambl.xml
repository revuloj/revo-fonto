<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ambl</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ambl.0i">
  <kap><tld/>i</kap>
  <snc mrk="ambl.0i.RAJ">
    <dif>
      Iri en aparta maniero de kvarpieduloj,
      levante samtempe amba&ubreve; piedojn de sama flanko:
      <ekz>
        ni suden <tld/>is sur sableca voja&ccirc;o
        <fnt><aut>J. Ribillard</aut>, <vrk>Vagado sub palmoj</vrk>, 
        <lok>UEA, 1956</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="galop.0i.RAJ">galopi <sncref/></ref>,
      <ref cel="pasx.0i">pa&scirc;i</ref>,
      <ref cel="trot.0i.RAJ">troti <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="fr">ambler</trd>
</drv>

<drv mrk="ambl.0o">
  <kap><tld/>o</kap>
  <snc mrk="ambl.0o.RAJ">
    <dif>
      Irmaniero de kvarpiedulo, kiu <tld/>as.
    </dif>
  </snc>
  <trd lng="de">Passgang</trd>
  <trd lng="en">amble</trd>
  <trd lng="fr">amble</trd>
  <trd lng="nl">telgang</trd>
  <trd lng="ru">&c_i;&c_n;&c_o;&c_h;&c_o;&c_d;&c_mol;</trd>
  <trd lng="sk">inochod</trd>
</drv>
</art>
