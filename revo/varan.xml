<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: varan.xml,v 1.11 2024/04/12 18:25:40 revo Exp $">
<kap>
  <rad>varan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="varan.0o">
  <kap><tld/>o</kap>
  <snc mrk="varan.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda karnoman&gcirc;a rampulo
      vivanta en tropikaj regionoj de Oceanio,
      Sudazio a&ubreve; Afriko;
      genro el familio varanedoj
      (<trd lng="la">Varanus</trd>):
      <ekz>
        la komoda <tld/>o ofte fosas kavon, profundan je 9 m kaj tie &gcirc;i
        demetas la ovojn
        <fnt>
          <bib>Viki</bib>,
          <vrk><url ref="&Viki;/Komoda_varano">
          komoda varano</url></vrk>,
          <lok>2011-06-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_r;&c_a;&c_n;</trd>
  <trd lng="bg">&c_v;&c_a;&c_r;&c_a;&c_n;</trd>
  <trd lng="ca">var&agrave;</trd>
  <trd lng="cs">varan</trd>
  <trd lng="de">Waran</trd>
  <trd lng="fr">varan</trd>
  <trd lng="ja">&#x30AA;&#x30AA;&#x30C8;&#x30AB;&#x30B2;</trd>
  <trd lng="pl">waran</trd>
  <trd lng="sk">varan n&#xED;lsky</trd>
  <trd lng="uk">&c_v;&c_a;&c_r;&c_a;&#x43D;</trd>
</drv>
</art>

