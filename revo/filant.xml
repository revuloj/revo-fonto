<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>filant</rad>/o
</kap>
<drv mrk="filant.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="euxfor1.0oj">e&ubreve;forbiacoj</ref>
      (<trd lng="la">Phyllantus</trd>);
      <ref tip="lst" cel="veget.0ajxo" 
      lst="voko:malpigioparencoj">vegeta&jcirc;o</ref>,
      kies folioj situas spirale a&ubreve; duvice sur tigoj similantaj
      plumojn; kies virseksaj floroj estas senkorolaj, havantaj
      du &gcirc;is ses stamenojn;
      kaj kun globformaj <ref tip="prt" cel="kapsul.0o">kapsuloj</ref>
      disi&gcirc;antaj maturi&gcirc;inte en tri dufakajn partojn:
      <ekz>
        filanto<klr>[j&#8230;]</klr> estas <esc>plejparte</esc> da&ubreve;rafoliaj arboj kaj arbedoj,
        &scirc;ajne havantaj kunmetajn foliojn pro &scirc;osetoj plati&gcirc;intaj al simplaj kaj stipulohavaj kladodioj 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=606717">Filanto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Blattblume</trd>,
     <trd>Phylantus</trd>
  </trdgrp>
  <trd lng="fr">girembellier</trd>
  <trd lng="hu">vir&aacute;glev&eacute;l<klr tip="ind"> (cserje)</klr></trd>
  <trd lng="pl">li&sacute;ciokwiat</trd>
  <trd lng="pt">filanto</trd>
</drv>
</art>

