<vortaro>
  <art mrk="$Id: stres.xml,v 1.7 2023/11/08 21:24:15 afido Exp $">
    <kap>
      <rad>stres</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="stres.0o">
  <kap><tld/>o</kap>
  <snc mrk="stres.0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Psika kaj fizilogia <ref tip="super" cel="ag.re0o.respondaefiko">reago</ref>
      al aparte postulaj eksteraj cirkonstancoj;
      <ref tip="vid" cel="strecx.anim0o.PSI">animstre&ccirc;o</ref>:
      <ekz>
        en kondi&ccirc;oj de dan&gcirc;ero, <tld/>o a&ubreve; hasto, <klr>[ili]</klr> havas inklinon
        senpripense refoje stumbli
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007159.php">Flugilhavaj vortoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 22</lok>
          </fnt>;
      </ekz>
      <ekz>
        la stre&ccirc;o kaj <tld/>o por unu sola kosmona&ubreve;to fermita en kvaza&ubreve;a sardinskatolo
        dum tiom longa soleca voja&gcirc;o povus esti netolerebla
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2010/009989.php"
              >Voja&gcirc;o unudirekta</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[oni]</klr> postulas <tld/>-teston por &ccirc;iuj atomelektraj centraloj
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2011/010418.php"
              >Adia&ubreve;o al nukleo</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        katoj povas kadre de t.n. felinoterapio nuligi <tld/>on kaj kuraci sangopremon
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2015/011483.php"
              >Kafo kun kato</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strecx.0o">stre&ccirc;o</ref>,
      <ref cel="tensi.0o.strecxo">tensio<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Stress</trd>,
      <trd>Anspannung</trd>
    </trdgrp>
    <trd lng="en">stress</trd>
    <trd lng="fr">stress</trd>
    <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_s;&c_s;</trd>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_e;&c_s;</trd>
  <trd lng="af">stres</trd>
  <trd lng="bg">&c_s;&c_t;&c_r;&c_je;&c_s;</trd>
