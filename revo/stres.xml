<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: stres.xml,v 1.1 2022/06/10 12:54:40 afido Exp $">
    <kap>
      <rad>stres</rad>/o
    </kap>

<drv mrk="stres.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">PSI</uzo>
    <dif>
      Psika kaj fizilogia <ref tip="super" cel="ag.re0o.respondaefiko">reago</ref> 
      al aparte postulaj eksteraj cirkonstancoj:
      <ekz>
        en kondi&ccirc;oj de dan&gcirc;ero, <tld/>o a&ubreve; hasto, <klr>[ili]</klr> havas inklinon
        senpripense refoje stumbli
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007159.html"
              >Flugilhavaj vortoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la stre&ccirc;o kaj <tld/>o por unu sola kosmona&ubreve;to fermita en kvaza&ubreve;a sardinskatolo
        dum tiom longa soleca voja&gcirc;o povus esti netolerebla
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="https://www.monato.be/2010/009989.php"
              >Voja&gcirc;o unudirekta</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[oni]</klr> postulas <tld/>-teston por &ccirc;iuj atomelektraj centraloj
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="https://www.monato.be/2011/010418.php"
              >Adia&ubreve;o al nukleo</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        katoj povas kadre de t.n. felinoterapio nuligi <tld/>on kaj kuraci sangopremon
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="https://www.monato.be/2015/011483.php"
              >Kafo kun kato</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strecx.0o">stre&ccirc;o</ref>,
      <ref cel="tensi.0o.strecxo">tensio<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Stress</trd>,
      <trd>Anspannung</trd>
    </trdgrp>
    <trd lng="en">stress</trd>
    <trd lng="fr">stress</trd>
    <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_s;&c_s;</trd>
  </snc>
</drv>

<drv mrk="stres.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Ka&ubreve;zanta <tld/>on:
      <ekz>
        oni trovi&gcirc;as en <tld/>a situacio
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007159.html"
              >Flugilhavaj vortoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tien ili rifu&gcirc;as <klr>[&#8230;]</klr>, se urba vivo i&gcirc;as netolereble <tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007162.html"
              >&Gcirc;ojoj kaj &gcirc;uoj de finna somero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">stressig</trd>
</drv>




  </art>

