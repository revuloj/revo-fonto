<drv mrk="stres.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="stres.0a.PSI">
    <dif>
      Ka&ubreve;zanta <tld/>on; animstre&ccirc;a:
      <ekz>
        oni trovi&gcirc;as en <tld/>a situacio
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007159.php">Flugilhavaj vortoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 22</lok>
          </fnt>;
      </ekz>
      <ekz>
        tien ili rifu&gcirc;as <klr>[&#8230;]</klr>, se urba vivo i&gcirc;as netolereble <tld/>a
        <fnt><aut>Saliko</aut>
        <vrk><url ref="&Monato;/2000/007162.html">&Gcirc;ojoj kaj
        &gcirc;uoj de finna somero</url></vrk>
        <bib>Monato</bib>
        <lok>2000/12, p. 12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_e;&c_s;&c_a;&c_v;&c_y;</trd>
  <trd lng="de">stressig</trd>
  <trd lng="pl">stresuj&aogonek;cy</trd>
</drv>
