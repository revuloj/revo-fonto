<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arabes.xml,v 1.23 2023/10/28 08:33:08 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>arabesk</rad>/o
</kap>
<drv mrk="arabes.0o">
  <kap><tld/>o</kap>
  <snc mrk="arabes.0o.ornamo">
    <dif>
      Ornama <ref tip="super" cel="desegn.0ajxo">desegna&jcirc;o</ref>, la&ubreve; la araba maniero, konsistanta
      el
      kunplektitaj linioj:
      <ekz>
        la planko de la halo estis pavimita per lerte kunmetitaj platoj kun desegna&jcirc;oj,
        jen geometriaj, jen <tld/>aj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        masiva pordo, <klr>[&#x2026;]</klr> ornamita de rondkapaj najloj desegnantaj <tld/>ojn
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekoka</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la tonoj de la muziko kune kun la bruo de la vento formas iajn aerajn <tld/>ojn
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro IV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="volut.0o">voluto</ref>,
      <ref cel="volv.0ajxo">volva&jcirc;o</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x963F;&#x62C9;&#x4F2F;&#x5F0F;&#x82B1;&#x7D0B; <pr>&#x101;l&#x101;b&#xF3;sh&#xEC;hu&#x101;w&#xE9;n</pr></trd>,
    <trd>&#x963F;&#x62C9;&#x4F2F;&#x5F0F;&#x82B1;&#x7EB9; <pr>&#x101;l&#x101;b&#xF3;sh&#xEC;hu&#x101;w&#xE9;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_r;&c_a;&c_b;&c_e;&c_s;&c_k;&c_a;</trd>
  <trd lng="bg">&c_a;&c_r;&c_a;&c_b;&c_je;&c_s;&c_k;&c_a;</trd>
  <trd lng="ca">arabesc</trd>
  <trd lng="cs">arabeska</trd>
  <trdgrp lng="de">
    <trd>Arabeske</trd>,
    <trd>Ranken<ind>ornament</ind></trd>
  </trdgrp>
  <trd lng="en">arabesque</trd>
  <trd lng="es">arabesco</trd>
  <trd lng="fa">&a_A;&a_s;&a_l;&f_y;&a_m;&f_y;</trd>
  <trd lng="fr">arabesque</trd>
  <trd lng="he">&ayin;&resh;&bet;&samekh;&qof;&he;</trd>
  <trd lng="hu">arabeszk</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30E9;&#x30D9;&#x30B9;&#x30AF;</trd>,
    <trd>&#x5510;&#x8349;&#x6A21;&#x69D8;  <pr>&#x304B;&#x3089;&#x304F;&#x3055;&#x3082;&#x3088;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">arabesk</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_a;&c_b;&c_je;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_z;&c_o;&c_r;</trd>,
    <trd>&c_o;&c_r;&c_n;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">arabeska</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_r;&c_a;&c_b;&c_je;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_z;&c_o;&c_r;</trd>
  </trdgrp>
</drv>
</art>
