<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pekl</rad>/i
</kap>
<drv mrk="pekl.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Meti krudajn man&gcirc;a&jcirc;ojn (ekz. viandon, fi&scirc;on, legomojn) en salon
      por konservi ilin man&gcirc;eblaj kaj doni al ili specifan guston:
      <ekz>
        <tld/>ita porko, moruo, kukumo;
      </ekz>
      <ekz>
        <ind><tld/>a&jcirc;o</ind>.
        <trdgrp lng="ru">
          <trd>&c_s;&c_o;&c_l;&c_je;&c_n;&c_mol;&c_je;</trd>,
          <trd>&c_s;&c_o;&c_l;&c_o;&c_n;&c_i;&c_n;&c_a;</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_k;&c_a;&c_n;&c_s;&c_e;&c_r;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_s;&c_o;&c_l;&c_mol;&c_l;&c_ju;)</klr></trd>
  <trd lng="de">p&ouml;keln</trd>
  <trd lng="en">pickle</trd>
  <trd lng="nl">pekelen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_l;&c_i;&c_t;&c_mol;<klr tip="ind"> (&c_d;&c_l;&c_ja; &c_k;&c_o;&c_n;&c_s;&c_je;&c_r;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_ja;)</klr></trd>,
    <trd>&c_z;&c_a;&c_s;&c_a;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_k;&c_o;&c_n;&c_s;&c_je;&c_r;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_ja;)</klr></trd>
  </trdgrp>
</drv>
</art>
