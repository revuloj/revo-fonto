<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ignam.xml,v 1.15 2019/06/30 18:21:02 revo Exp $">
<kap>
  <rad>ignam</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="ignam.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="radik.0o.BOT">radiko</ref>
      de <ref tip="malprt" cel="dioskore.0o">dioskoreoj</ref>:
      <ekz>
        la kamparanoj produktas <klr>[&#8230;]</klr> <tld/>on, milion, fazeolon a&ubreve; sorgon
        <fnt>
          <bib>Monato</bib>
          <aut>Sagbadjelou Kokou/pg</aut>
          <vrk><url ref="&Monato;/2005/008658.php"
              >Pro pluvo plu</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la tubero de <tld/>o enhavas multan karbonhidraton, &gcirc;i estas la terpomo de la
        tropikoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27012"
              >Ignamo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_m;&c_s;</trd>
  <trd lng="de">Yamswurzel</trd>
  <trd lng="en">yam</trd>
  <trd lng="fr">igname</trd>
  <trdgrp lng="hu">
    <trd>jamgy&ouml;k&eacute;r</trd>,
    <trd>jamgum&oacute;</trd>
  </trdgrp>
  <trd lng="nl">broodvrucht</trd>
  <trdgrp lng="pl">
    <trd>jama</trd>,
    <trd>ziemniak japo&nacute;ski</trd>,
    <trd>bulwa pochrzynu</trd>
  </trdgrp>
  <trd lng="pt">inhame</trd>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_m;&c_s;</trd>,
    <trd>&c_i;&c_n;&c_mol;&c_ja;&c_m;</trd>
  </trdgrp>
  <trd lng="sv">jamsrot</trd>
  <trd lng="uk">&c_ja;&c_m;&c_s;</trd>
</drv>
</art>
