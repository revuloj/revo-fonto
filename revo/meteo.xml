<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>mete</rad>/o
</kap>
<drv mrk="mete.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      &Ccirc;ia atmosfera fenomeno, precipe la nekutimaj. Ekz.: fulmo, pluvo,
      &ccirc;ielarko.
    </dif>
  </snc>
</drv>

<drv mrk="mete.0ologio">
  <kap><tld/>ologio</kap>
  <snc>
    <uzo tip="fak">MET</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref>, kiu studas la
      atmosferajn fenomenojn kaj provas konjekti la estontan veteron.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="meteor2.0o">meteorologio</ref>
  </refgrp>
  <trd
lng="be">&c_m;&c_e;&c_t;&c_e;&c_a;&c_r;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="de">Meteorologie</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;t&eacute;orologie<klr> (science)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meteorol&oacute;gia</trd>,
    <trd>id&odblac;j&aacute;r&aacute;stan</trd>,
    <trd>l&eacute;gk&ouml;rtan</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>weerkunde</trd>,
    <trd>meteorologie</trd>
  </trdgrp>
  <trd lng="pt">meteorologia</trd>
  <trd
lng="ru">&c_m;&c_je;&c_t;&c_je;&c_o;&c_r;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>
</art>

