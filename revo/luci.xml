<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: luci.xml,v 1.1 2024/10/06 19:22:14 afido Exp $">
    <kap>
      <ofc>10</ofc><rad>Luci</rad>/o
    </kap>

<drv mrk="luci.0o">
  <kap><ofc>10</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">NOM</uzo>
    <dif>
      Roma <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">nomo</ref>:
      <ekz>
        en <nom>Antio&hcirc;ia</nom>, en la tiea eklezio, estis profeto <klr>[&#8230;]</klr> <tld/>o, la Kirenano
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 13:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        Seneko ne gvidas <tld/>on, li indulgas lin
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Lucius</trd>,
    <trd>Luzius</trd>,
    <trd>Lucia</trd>
  </trdgrp>
</drv>

<drv mrk="luci.sankta0a">
  <kap>
    <ofc>10</ofc>sankta<tld lit="l"/>a
  </kap>
  <snc>
    <dif>
      Rilata al (la insulo) Sankta <tld/>o.
    </dif>
  </snc>
</drv>

<drv mrk="luci.sankta0ano">
  <kap>
    <ofc>10</ofc>sankta<tld lit="l"/>ano
  </kap>
  <snc>
    <dif>
      Iu el (la insulo) Sankta <tld/>o.
    </dif>
  </snc>
<trd lng="de">St. Lucianer</trd>
</drv>

<drv mrk="luci.Sankta_0o">
  <kap>
    <ofc>10</ofc>Sankta <tld/>o
  </kap>
  <snc mrk="luci.Sankta_0o.GEOG">
    <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato de la Malgrandaj Antiloj sur la samnoma insulo, 50&#8239;km norde de Sankta
      Vincento:
      <ekz>
        la insulo Sankta <tld/>o estas nomita la&ubreve; la kristana martirino <tld/>o de Sirakuso.
      </ekz>
    </dif>
    <trd lng="de">St. Lucia</trd>
    <trd lng="en">Saint Lucia</trd>
    <trd lng="fr">Sainte-Lucie</trd>
  </snc>
  <snc mrk="luci.Sankta_0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <tld/>o de Sirakuso 283&#8239;&ndash;&#8239;304), frua kristana martirino.
    </dif>
    <trd lng="de">Lucia von Syrakus</trd>
    <trd lng="en">Saint Lucy</trd>
    <trd lng="fr">Lucie de Syracuse</trd>
  </snc>
</drv>

  </art>

