<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mahago.xml,v 1.27 2024/01/25 20:10:33 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>mahagon</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="mahago.0o">
  <kap><tld/>o</kap>
  <snc mrk="mahago.0o.arbo">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Tropika arbo liveranta la
      <ref tip="prt" cel="mahago.0o.ligno"><tld/>on<sncref ref="mahago.0o.ligno"/></ref>;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="meliac.0oj">meliacoj</ref>
      (<trd lng="la">Swietenia</trd>):
      <ekz>
        en la mano li portis bastonegon el <tld/>a ligno
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la raketo flugos en la direkton de la alta <tld/>o sur la alia riverbordo
        <fnt>
          <aut>T. Steele</aut>
          <vrk>La raketo de Freddy revenas</vrk>
          <lok>en: Australia Felix, 1999</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/4/4c/Swietenia_mahagoni_Boynton_Beach_starr-090924-5920.jpg/640px-Swietenia_mahagoni_Boynton_Beach_starr-090924-5920.jpg" prm="CC BY 2.0">bran&ccirc;oj kun fruktoj de <tld/>o (Swietenia mahagoni), Florido, Usono
        <fnt>
          <aut>Forest kaj Kim Starr</aut>
          <lok><url ref="&WCW;/File:Swietenia_mahagoni_Boynton_Beach_starr-090924-5920.jpg">https://commons.wikimedia.org/wiki/File:Swietenia_mahagoni_Boynton_Beach_starr-090924-5920.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="en">mahogany</trd>
    <trd lng="hu">mahag&oacute;ni(fa)</trd>
    <trd lng="nl">mahonie</trd>
    <trd lng="pl">mahoniowiec</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_a;&c_h;&c_a;&c_g;&c_o;&c_n;&c_i;&c_je;&c_v;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
      <trd>&c_s;&c_v;&c_i;&c_t;&c_je;&c_n;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="mahago.0o.ligno">
    <uzo tip="fak">TEK</uzo>
    <dif>
      La bela brunru&gcirc;a ligno de
      <ref tip="malprt" cel="mahago.0o.arbo"><tld/>o<sncref ref="mahago.0o.arbo"/></ref>
      kaj parencaj specioj, uzata en meblofarado kaj konstruado:
      <ekz>
        super la ronda <tld/>a tablo jam brulis la rozokolora lampo
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        en la eleganta restoracio li ser&ccirc;is al si <tld/>-koloran, duonferman lo&gcirc;ion
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">mahogany</trd>
    <trd lng="hu">mahag&oacute;ni<klr tip="ind"> (faanyag)</klr></trd>
    <trd lng="nl">mahoniehout</trd>
    <trd lng="pl">maho&nacute;</trd>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_h;&c_a;&c_g;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_m;&c_a;&c_h;&c_a;&c_g;&c_o;&c_n;</trd>
  <trd lng="br">akajou</trd>
  <trd lng="ca">caoba</trd>
  <trd lng="cs">mahagon</trd>
  <trd lng="de">Mahagoni</trd>
  <trd lng="fr">acajou<klr tip="amb"> (subst.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30DB;&#x30AC;&#x30CB;&#x30FC;</trd>,
    <trd>&#x30DE;&#x30DB;&#x30AC;&#x30CB;&#x30FC;&#x6750;  <pr>&#x30DE;&#x30DB;&#x30AC;&#x30CB;&#x30FC;&#x3056;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>mogno</trd>,
    <trd>acaiacatinga</trd>,
    <trd>acaju</trd>,
    <trd>magano</trd>,
    <trd>m&oacute;gono</trd>,
    <trd>mogno-verdadeiro</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>mahon</trd>,
    <trd>acaju</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sk">mahag&oacute;n</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_je;&c_r;&c_v;&c_o;&c_n;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_a;&c_gu;&c_o;&c_n;&c_ib;&c_jeu;&c_v;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_m;&c_a;&c_h;&c_a;&c_gu;&c_o;&c_n;&c_ib;</trd>
  </trdgrp>
  <trd lng="zh">&#x6843;&#x82B1;&#x5FC3;&#x6728; <pr>t&#xE1;ohu&#x101;x&#x12B;nm&#xF9;</pr></trd>
</drv>
</art>
