<drv mrk="kant.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kant.0i.homo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="muzik.0i">Muziki</ref> per sia vo&ccirc;o:
      <ekz>
	    <tld/>i arion;
      </ekz>
      <ekz>
        &scirc;i dancas, &scirc;i tintas, &scirc;i <tld/>as kun
        &gcirc;ojo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia onklo ek<tld/>is kaj tuj &ccirc;esis, sed mia frato
        <tld/>adis la
        tutan vesperon
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
	    ofte <tld/>as la bu&scirc;o kiam ploras la
	    koro<fnt><bib>PrV</bib></fnt>
      </ekz>
   </dif>
  <refgrp tip="vid">
	  <ref cel="deklam.0i">deklami</ref>.
  </refgrp>
	<trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
	<trd lng="de">singen</trd>
	<trd lng="nl">zingen</trd>
	<trd lng="pl">&sacute;piewa&cacute;</trd>
    <trd lng="sv">sjunga</trd>
  </snc>
  <snc mrk="kant.0i.birdo">
    <uzo tip="klr">(pri birdoj kaj kelkaj aliaj bestoj)</uzo>
    <dif>
       Eligi kriojn a&ubreve; sonojn harmonie modulitajn:
       <ekz>
	 la najtingalo <tld/>as sian amdoloron
          <fnt>
            <bib>Fab2</bib>
            <lok>Rozo de la tombo de Homero</lok>
          </fnt>.
       </ekz>
    </dif>
    <refgrp tip="vid">
	  <ref cel="blek.0i">bleki</ref>,
	  <ref cel="cxirp.0i">&ccirc;irpi</ref>,
	  <ref cel="pep.0i">pepi</ref>.
    </refgrp>
    <trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>parrupar</trd>,
      <trd>refilar</trd>,
      <trd>escatainar</trd>,
      <trd>escotxegar</trd>
    </trdgrp>
	<trd lng="de">singen</trd>
	<trd lng="nl">zingen</trd>
	<trd lng="pl">&sacute;piewa&cacute;</trd>
    <trd lng="sv">sjunga</trd>
  </snc>
  <snc mrk="kant.0i.FIG">
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Estigi mildan harmonian murmuron (p.p. a&jcirc;oj):
	  <ekz>
	    la akvo <tld/>as boli&gcirc;ante;
	  </ekz><ekz>
	    la rad&scirc;pinilo <tld/>is en la silenta &ccirc;ambro;
	  </ekz>
      <ekz>
        io <tld/>is kaj sonis en la koro de la knabo
        <fnt>
          <bib>Fab4</bib>
          <lok>Du fratoj</lok>
        </fnt>.
      </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="flustr.0i">flustri</ref>,
	  <ref cel="susur.0i">susuri</ref>
	</refgrp>
	<trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>rondinejar</trd>,
      <trd>murmurar</trd>
    </trdgrp>
	<trd lng="de">singen</trd>
	<trd lng="nl">zingen</trd>
	<trd lng="pl">&sacute;piewa&cacute;</trd>
    <trd lng="sv">sjunga</trd>
  </snc>
  <snc mrk="kant.0i.lauxdi">
     <dif>
      <ref tip="dif" cel="kant.pri0i.MUZ">Pri<tld/>i</ref>:
      <ekz>
	    mi <tld/>os Vian forton
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 59:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <klr>[&#x2026;]</klr> ja ne konas tiujn, kiuj <tld/>as nin
        konstante
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
	    Vergilio <tld/>is Eneon <klr>(abs.)</klr>;
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>lloar</trd>,
      <trd>encomiar</trd>
    </trdgrp>
