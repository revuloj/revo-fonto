<drv mrk="kant.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="kant.0ado.MUZ">
    <dif>
      Homvo&ccirc;a muzikado;
      <ref tip="sin" cel="kant.0o.kantado">kanto<sncref/></ref>:
      <ekz>
        mortintoj kun <tld/>ado estis enterigataj
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ado de la sklavinoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ado de la birdoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Peco da perlovico</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &gcirc;i a&ubreve;dis la <tld/>adon kaj sibladon de kugloj
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;dis muzikon kaj <tld/>adon
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
	<trd>&c_p;&c_je;&c_s;&c_mol;&c_n;&c_ja;</trd>,
	<trd>&c_s;&c_mol;&c_p;&c_je;&c_w;</trd>,
	<trd>&c_s;&c_mol;&c_p;&c_je;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">cant</trd>
  <trdgrp lng="cs">
    <trd>p&iacute;se&ncaron;</trd>,
    <trd>zp&iacute;v&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Gesang</trd>
  <trd lng="es">canto</trd>
  <trdgrp lng="fr">
    <trd>chanson</trd>,
    <trd>chant</trd>
  </trdgrp>
  <trd lng="hu">&eacute;nekl&eacute;s</trd>
  <trdgrp lng="id">
    <trd>lagu</trd>,
    <trd><ind>nyanyi</ind>an</trd>
  </trdgrp>
  <trd lng="it">canto</trd>
  <trd lng="nl">lied</trd>
  <trd lng="no">sang</trd>
  <trd lng="pl">&sacute;piew</trd>
  <trd lng="pt">canto</trd>
  <trdgrp lng="ru">
	<trd>&c_p;&c_je;&c_s;&c_n;&c_ja;</trd>,
	<trd>&c_p;&c_je;&c_s;&c_n;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">spev</trd>
  <trd lng="sv">s&aring;ng</trd>
  <trd lng="th">&#xE01;&#xE32;&#xE23;&#xE23;&#xE49;&#xE2D;&#xE07;&#xE40;&#xE1E;&#xE25;&#xE07;</trd>
</drv>

<drv mrk="kant.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="kant.0ajxo.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="muzik.0ajxo">Muzika&jcirc;o</ref>
      kun paroloj eldirataj la&ubreve; melodio;
      <ref tip="sin" cel="kant.0o.kantajxo">kanto<sncref/></ref>:
      <ekz>
        li estis glitinta en dol&ccirc;an duondormon, en kiu li
        perceptis la
        vortojn kiel <tld/>a&jcirc;o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Sinjorino Ajuolo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_s;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="ca">can&ccedil;&oacute;</trd>
  <trd lng="cs">p&iacute;se&ncaron;</trd>
  <trdgrp lng="es">
	<trd>canci&oacute;n</trd>,
	<trd>canto</trd>
  </trdgrp>
  <trdgrp lng="fr">
	<trd>chanson</trd>,
	<trd>chant</trd>
  </trdgrp>
  <trd lng="hu">&eacute;neksz&aacute;m</trd>
  <trdgrp lng="id">
    <trd>lagu</trd>,
    <trd><ind>nyanyi</ind>an</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>canto</trd>, <trd>canzone</trd>
  </trdgrp>
  <trd lng="nl">lied</trd>
  <trd lng="no">sang</trd>
  <trdgrp lng="pt">
	<trd>canto</trd>,
	<trd>can&ccedil;&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="pl">
	<trd>pie&sacute;&nacute;</trd>,
	<trd>piosenka</trd>
  </trdgrp>
