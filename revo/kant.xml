          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2010/010074.php">&Scirc;tupoj
          al malplenaj po&scirc;oj</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
	<trd>&c_h;&c_ib;&c_t;</trd>,
	<trd>&c_sh;&c_l;&c_ja;&c_g;&c_je;&c_r;</trd>
