<drv mrk="kant.0o">
  <kap><tld/>o</kap>
  <snc mrk="kant.0o.kantado">
    <dif>
      <ref tip="dif" cel="kant.0ado">Kantado</ref>:
      <ekz>
        <klr>[ili]</klr> per <tld/>oj pacigis la dion
        <fnt><bib>FK</bib></fnt>;
      </ekz><ekz>
        reiras herooj <tld/>antaj, al <tld/>oj iliaj ali&gcirc;as
        sopiroj
        <fnt><bib>FK</bib></fnt>;
      </ekz><ekz>
        kiuj semas kun larmoj, tiuj rikoltos kun <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 126:5</lok>
        </fnt>;
      </ekz><ekz>
        resonante al la <tld/>o de l' mar' senfunda kaj bruanta
        <fnt><bib>FK</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_mol;&c_p;&c_je;&c_w;</trd>,
      <trd>&c_s;&c_mol;&c_p;&c_je;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="ca">cant</trd>
    <trd lng="hu">&eacute;nekl&eacute;s</trd>
    <trd lng="de">Gesang</trd>
    <trd lng="pl">&sacute;piew</trd>
    <trd lng="sv">s&aring;ng</trd>
  </snc>
  <snc mrk="kant.0o.kantajxo">
    <dif>
      <ref tip="dif" cel="kant.0ajxo">Kanta&jcirc;o</ref>:
      <ekz>
	en la oreloj la malnova <tld/>o jam forgesita al
	ni resonas
        <fnt><bib>Ifigenio</bib></fnt>;
      </ekz><ekz>
        amiko <klr>[&#8230;]</klr> nela&ubreve;te <tld/>adis la <tld/>on
        de <nom>Heine</nom>:
        &leftquot;Mi ne scias, kion signifas...&rightquot;
        <fnt><bib>FK</bib></fnt>;
      </ekz><ekz>
        amikino en la rondo <tld/>as <tld/>on pri l' espero
        <fnt><bib>FK</bib></fnt>;
      </ekz><ekz>
        el <tld/>o oni vortojn ne el&jcirc;etas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_je;&c_s;&c_mol;&c_n;&c_ja;</trd>
    <trd lng="ca">can&ccedil;&oacute;</trd>
    <trd lng="pl">pie&sacute;&nacute;</trd>
    <trd lng="sv">s&aring;ng</trd>
  </snc>
  <snc mrk="kant.0o.cxapitro">
    <uzo tip="fak">POE</uzo>
    <dif>
      <ref tip="sub" cel="cxapit.0o">&Ccirc;apitro</ref> de
      <ref tip="malprt" cel="poem.0o.POE">poemo<sncref/></ref> resp.
      poemaro:
      <ekz>
         Duli&hcirc;io estas insulo kiu aperas en la unua <tld/>o de
         Odiseado de Homero, kaj plue en la na&ubreve;a <tld/>o
	 <fnt><bib>Viki</bib>,
          <lok><url ref="&Viki;/Duli%C4%A5io">Duli&hcirc;io</url></lok></fnt>;
      </ekz><ekz>
        Alta <tld/>o de Salomono
        <fnt><bib>MT</bib> <lok>&Alt; 1:1</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_je;&c_s;&c_mol;&c_n;&c_ja;</trd>
    <trd lng="ca">cant</trd>
    <trd lng="pl">pie&sacute;&nacute;</trd>
  </snc>
