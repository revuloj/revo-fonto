<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kant</rad>/i
</kap>
<drv mrk="kant.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kant.0i.homo">
    <dif>
      Eligi vo&ccirc;modulitajn harmoniajn sonojn a&ubreve; vortojn, ofte kiel
      signo de &gcirc;ojo:
      <ekz>la &scirc;uisto <tld/>is la tutan tagon;</ekz>
      <ekz><tld/>i arion;</ekz>
      <ekz>ofte <tld/>as la bu&scirc;o kiam ploras la koro<fnt>Z</fnt></ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dolor.0i">dolori</ref>.
    </refgrp>
    <refgrp tip="sin">
      <ref cel="deklam.0i">deklami</ref>.
    </refgrp>
    <trd lng="nl">zingen</trd>
  </snc>
  <snc mrk="kant.0i.birdo">
    <dif>
      Eligi kriojn a&ubreve; sonojn harmonie modulitajn (p.p. birdoj): 
      <ekz>la najtingalo <tld/>as sian amdoloron<fnt>Z</fnt>.</ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="blek.0i">bleki</ref>,
      <ref cel="cxirp.0i">&ccirc;irpi</ref>,
      <ref cel="pep.0i">pepi</ref>.
    </refgrp>
    <trd lng="nl">zingen</trd>
  </snc>
  <snc mrk="kant.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Estigi mildan harmonian murmuron (p.p. a&jcirc;oj):
      <ekz>la akvo <tld/>as boli&gcirc;ante;</ekz>
      <ekz>la rad&scirc;pinilo <tld/>is en la silenta &ccirc;ambro.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="susur.0i">susuri</ref>,
      <ref cel="flustr.0i">flustri</ref>.
    </refgrp>
    <trd lng="nl">zingen</trd>
  </snc>
  <snc mrk="kant.0i.lauxdi">
    <dif>
      Glori per versoj; versfaradi honore al iu a&ubreve; io:
      <ekz>mi <tld/>os Vian forton<fnt>Z</fnt>;</ekz>
      <ekz>mi <tld/>os al Vi per harpo<fnt>Z</fnt>;</ekz>
      <ekz>Vergilio <tld/>is Eneon <klr>(abs.)</klr>;</ekz>
      <ekz>amo kaj printempo igas la poetojn <tld/>i.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="lauxd.0i">la&ubreve;di</ref>.
    </refgrp>
    <trd lng="nl">bezingen</trd>
    <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="cs">zp&iacute;vat</trd>
  <trd lng="en">sing</trd>
  <trd lng="ru">&c_p;&c_je;&c_t;&c_mol;</trd>
</drv>
<drv mrk="kant.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i a&ubreve; <tld/>ita&jcirc;o:
      <ekz>
        la malnova <tld/>o jam forgesita en la oreloj al
        ni resonas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la libro de la <tld/>oj de Heine;
      </ekz>
      <ekz>la Alta <tld/>o <klr>(ama poemo el la biblio)</klr>.</ekz> 
    </dif>
  </snc>
  <trd lng="cs">p&iacute;se&ncaron;</trd>
  <trd lng="nl">lied</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_s;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_s;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="kant.0eti">
  <kap><tld/>eti</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="K"/>i duonvo&ccirc;e, malla&ubreve;te, ne klare prononcante la silabojn.
    </dif>
  </snc>
  <trd lng="nl">neuri&euml;n</trd>
  <trd lng="ru">&c_n;&c_a;&c_p;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="kant.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kant.0isto.profesio">
    <dif>
      Tiu, kiu profesie <tld/>as. 
    </dif>
    <trd lng="nl">zanger</trd>
  </snc>
  <snc mrk="kant.0isto.poeto">
    <dif>
      Poeto. 
    </dif>
  </snc>
  <trd lng="ru">&c_p;&c_je;&c_v;&c_je;&c_c;</trd>
</drv>
<drv mrk="kant.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="K"/>i 4. 
    </dif>
  </snc>
  <trd lng="nl">bezingen</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="kant.re0ajxo">
  <kap>re<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Parto de <tld/>o, kiun oni re<tld/>as post &ccirc;iu strofo.  
    </dif>
  </snc>
  <trd lng="nl">refrein</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_p;&c_je;&c_v;</trd>,
    <trd>&c_r;&c_je;&c_f;&c_r;&c_je;&c_n;</trd>
  </trdgrp>
</drv>
<drv mrk="kant.furor0o">
  <kap>furor<tld/>o, mod<tld/>o <fnt><bib>HejmVort</bib></fnt></kap>
  <snc>
    <dif>
      Dumtempe tre populara <tld/>o.
    </dif> 
    <ref tip="sin" cel="sxlagr.0o">&scirc;lagro</ref> 
  </snc>
  <trd lng="en">hit<klr tip="ind"> (song)</klr></trd>
  <trdgrp lng="de">
     <trd>Schlager</trd>,
     <trd>Hit</trd></trdgrp>
  <trdgrp lng="es">
    <trd>&eacute;xito<klr tip="ind"> (canci&oacute;n)</klr></trd>,
    <trd>hit</trd>
  </trdgrp>
  <trd lng="fi">hitti</trd>
  <trdgrp lng="fr">
    <trd>chanson &agrave; succ&egrave;s</trd>,
    <trd>hit<klr tip="ind"> (chanson)</klr></trd>,
    <trd>succ&egrave;s<klr tip="ind"> (chanson)</klr></trd>,
    <trd>tube<klr tip="ind"> (chanson)</klr></trd>
  </trdgrp>
  <trd lng="it">canzone di successo</trd>
  <trdgrp lng="nl">
    <trd>schlager</trd>,
    <trd>hit</trd>
  </trdgrp>
  <trd lng="pl">hit</trd>
  <trdgrp lng="pt">
    <trd>top</trd>,
    <trd>sucesso da temporada</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>schlager</trd>,
    <trd>hit</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_l;&c_ja;&c_g;&c_je;&c_r;</trd>,
    <trd>&c_h;&c_i;&c_t;</trd>
  </trdgrp>
</drv>

</art>
