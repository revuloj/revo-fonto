  <gra><vspec>tr</vspec></gra>
  <snc mrk="kant.0i.homo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="muzik.0i">Muziki</ref> per sia vo&ccirc;o:
      <ekz>
        fra&ubreve;lino <nom>Delna</nom> <tld/>is sian faman arion de &leftquot;la Kantinistino&rightquot;
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz><ekz>
        &scirc;i dancas, &scirc;i tintas, &scirc;i <tld/>as kun
        &gcirc;ojo
        <fnt><bib>FK</bib></fnt>;
      </ekz><ekz>
        mia onklo ek<tld/>is kaj tuj &ccirc;esis, sed mia frato
        <tld/>adis la tutan vesperon
        <fnt><bib>DL</bib></fnt>;
      </ekz><ekz>
        &ccirc;u iu estas gaja? li psalme <tld/>u
        <fnt>
          <bib>NT</bib>
          <lok>Jakobo 5:13</lok>
        </fnt>;
      </ekz><ekz>
        mi <tld/>is pre&gcirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>u dorman <tld/>on
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>!
      </ekz><ekz>
        &scirc;i povosciis anka&ubreve; <tld/>i, tiel <klr>[&#8230;]</klr> &ccirc;arme, kiel oni neniam anta&ubreve;e
        a&ubreve;dis
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz><ekz>
	ofte <tld/>as la bu&scirc;o kiam ploras la
	koro<fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        facile estas danci, se la feli&ccirc;o <tld/>as
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
   </dif>
    <refgrp tip="vid">
      <ref cel="deklam.0i">deklami</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">singen</trd>
    <trd lng="nl">zingen</trd>
    <trd lng="pl">&sacute;piewa&cacute;</trd>
    <trd lng="sv">sjunga</trd>
  </snc>
  <snc mrk="kant.0i.birdo">
    <uzo tip="klr">(pri birdoj kaj kelkaj aliaj bestoj)</uzo>
    <dif>
      Eligi kriojn a&ubreve; sonojn harmonie modulitajn:
      <ekz>
	la najtingalo <tld/>as sian amdoloron
        <fnt>
          <bib>Fab2</bib>
          <lok>Rozo de la tombo de Homero</lok>
        </fnt>;
      </ekz><ekz>
        la koko <tld/>is sian kikeriki
        <fnt>
          <bib>ElektFab</bib>
          <lok>La bremenaj muzikistoj</lok>
        </fnt>;
      </ekz><ekz>
        en la &ccirc;ina lingvo <klr>[&#8230;]</klr> la koko <tld/>as <nac>kiao-kiao</nac>
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz><ekz>
        birdo <tld/>as la&ubreve; sia beko
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="blek.0i">bleki</ref>,
      <ref cel="cxirp.0i">&ccirc;irpi</ref>,
      <ref cel="pep.0i">pepi</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>parrupar</trd>,
      <trd>refilar</trd>,
      <trd>escatainar</trd>,
      <trd>escotxegar</trd>
