<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>jes</rad>
</kap>
<drv mrk="jes.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Interjekcio uzata por montri konsenton, aprobon, permeson:
      <ekz>
        &dash; &ccirc;u vi venos? &dash; <tld/>,
        mi venos;
      </ekz>
      <ekz>
        &dash; &ccirc;u mi povas eliri? &dash; <tld/>;
      </ekz>
      <ekz>
        &ccirc;iuj estis <tld/>opiniaj pri tio<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_k;</trd>
  <trd lng="bg">&c_d;&c_a;</trd>
  <trd lng="cs">ano</trd>
  <trd lng="de">ja</trd>
  <trd lng="el">&ny;&alfa;&jota;</trd>
  <trd lng="en">yes</trd>
  <trd lng="es">s&iacute;</trd>
  <trd lng="fr">oui</trd>
  <trd lng="hu">igen</trd>
  <trd lng="it">s&igrave;</trd>
  <trd lng="nl">ja</trd>
  <trd lng="pl">tak</trd>
  <trd lng="pt">sim</trd>
  <trd lng="ru">&c_d;&c_a;</trd>
  <trd lng="sv">ja</trd>
  <trd lng="tr">evet</trd>
  <trd lng="vo">si</trd>
</drv>

<drv mrk="jes.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Konsentanta, aprobanta, permesanta:
      <ekz>
        &ccirc;u li donis al vi <tld/>an respondon
        a&ubreve; nean?
        <fnt><bib>F</bib><lok>&FE; 31</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_n;&c_o;&c_w;&c_ch;&c_y;</trd>
  <trd lng="de">bejahend</trd>
  <trd lng="en">affirmative</trd>
  <trd lng="es">afirmativo</trd>
  <trdgrp lng="fr">
    <trd>affirmatif<klr tip="ind"> (positif)</klr></trd>,
    <trd>positif<klr tip="ind"> (affirmatif)</klr></trd>
  </trdgrp>
  <trd lng="hu">igenl&odblac;</trd>
  <trd lng="nl">bevestigend</trd>
  <trd lng="pt">afirmativo</trd>
  <trd lng="ru">&c_u;&c_t;&c_v;&c_je;&c_r;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">jakande</trd>
</drv>

<drv mrk="jes.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Diri <tld/>, respondi <tld/>:
      <ekz>
        <tld/>i demandon;
      </ekz>
      <ekz>
        la mortintino klinis sian kapon <tld/>ante kaj metis la manon sur la bruston
        <fnt><bib>Fab3</bib><lok>Historio</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">bejahen</trd>
  <trd lng="en">say yes</trd>
  <trd lng="es">afirmar</trd>
  <trdgrp lng="fr">
    <trd>dire <ind>oui</ind></trd>,
    <trd>r&eacute;pondre par <ind>oui</ind></trd>
  </trdgrp>
  <trd lng="hu">igenel</trd>
  <trdgrp lng="nl">
    <trd>ja zeggen</trd>,
    <trd>toestemmen</trd>
  </trdgrp>
  <trd lng="pt">consentir</trd>
  <trd lng="sv">jaka</trd>
</drv>

<drv mrk="jes.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        li kapbalancis sian <tld/>on
        <fnt><bib>Fab2</bib><lok>Pa&scirc;tistino kaj kamentubisto</lok></fnt>;
      </ekz>
      <ekz>
        Antef balancis kaj turnis la kapon, kio povis same bone signifi
        <tld/>on a&ubreve; neon
        <fnt><bib>Far3</bib><lok>&ccirc;ap. 12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>oui<klr tip="ind"> (subst.)</klr></trd>,
    <trd>r&eacute;ponse affirmative</trd>
  </trdgrp>
</drv>

<drv mrk="jes.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <snc>
    <ref tip="dif" cel="konfir.0i">konfirmi</ref>
    <ekz>
      li <tld/>igis al mi sian alvenon
      <fnt><bib>PV</bib></fnt>;
    </ekz>
    <ekz>
      kamaradoj, li mensogas <klr>[...:]</klr> in&gcirc;eniero
      Bobrov <tld/>igos tion, &ccirc;u ne?
      <fnt><bib>Metrop</bib><lok>&ccirc;ap. 12</lok></fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="cert.0igi">certigi</ref>,
      <ref cel="asert.0i">aserti</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_c;&c_mol;&c_v;&c_je;&c_r;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="en">
    <trd>certify</trd>,
    <trd>confirm</trd>
  </trdgrp>
  <trd lng="fr">confirmer</trd>
  <trd lng="nl">bevestigen</trd>
  <trd lng="pt">certificar</trd>
</drv>

<drv mrk="jes.mal0i">
  <kap>mal<tld/>i</kap>
  <fnt><bib>PV</bib></fnt>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="ne.0i.0diri">nei <sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;
    &c_p;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_m;&c_o;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">verneinen</trd>
  <trd lng="en">say no</trd>
  <trd lng="es">negar</trd>
  <trd lng="hu">tagad</trd>
  <trdgrp lng="fr">
    <trd>dire <ind>non</ind></trd>,
    <trd>r&eacute;pondre par <ind>non</ind></trd>
  </trdgrp>
  <trd lng="nl">ontkennen</trd>
  <trd lng="pt">negar</trd>
  <trd lng="sv">neka</trd>
</drv>

<drv mrk="jes.kap0i">
  <kap>kap<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Signi sian <tld/>adon per movo de la kapo,
      ofte per klino de &gcirc;i malsupren kaj supren:
      <ekz>
        al mia demando, li kap<tld/>is
        <fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        la enveninto <klr>[...]</klr> demandis: <ctl>&Ccirc;u vi permesas,
        ke mi sidi&gcirc;u?</ctl>, kaj ricevinte <ind>kap<tld/>o</ind>n li
        okupis la brakse&gcirc;on
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 13</lok></fnt>.
        <trdgrp lng="fr">
          <trd>oui de la t&ecirc;te</trd>,
          <trd>r&eacute;ponse affirmative<klr tip="ind"> (d'un signe de t&ecirc;te)</klr></trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>dire <ind>oui</ind><klr tip="ind"> (d'un signe de t&ecirc;te)</klr></trd>,
    <trd>r&eacute;pondre par <ind>oui</ind><klr tip="ind"> (d'un signe de t&ecirc;te)</klr></trd>,
    <trd>opiner du chef</trd>
  </trdgrp>
  <trd lng="nl">ja knikken</trd>
</drv>
</art>
