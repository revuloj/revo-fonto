<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>jes</rad>
</kap>
<drv mrk="jes.0">
  <kap>
    <ofc>*</ofc><tld/>
  </kap>
  <snc>
    <dif>
      Interjekcio uzata por konsenti, aprobi, permesi:
      <ekz>
        &ccirc;u vi venos? <tld/>,
        mi venos;
      </ekz>
      <ekz>
        &ccirc;u mi povas eliri? <tld/>;
      </ekz>
      <ekz>
        &ccirc;iuj estis <tld/>opiniaj pri tio<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_d;&c_a;</trd>
  <trd lng="cs">ano</trd>
  <trd lng="de">ja</trd>
  <trd lng="en">yes</trd>
  <trd lng="es">s&iacute;</trd>
  <trd lng="fr">oui</trd>
  <trd lng="nl">ja</trd>
  <trd lng="pl">tak</trd>
  <trd lng="ru">&c_d;&c_a;</trd>
  <trd lng="sv">ja</trd>
  <trd lng="tr">evet</trd>
</drv>

<drv mrk="jes.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Konsentanta, aprobanta, permesanta:
      <ekz>
        <tld/>a respondo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">bejahend</trd>
  <trd lng="nl">bevestigend</trd>
  <trd
  lng="ru">&c_u;&c_t;&c_v;&c_je;&c_r;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">jakande</trd>
</drv>

<drv mrk="jes.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Diri <tld/>, konsenti:
      <ekz>
        <tld/>i demandon;
      </ekz>
      <ekz>
        al mia demando, li kap<tld/>is.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">bejahen</trd>
  <trdgrp lng="nl">
    <trd>ja zeggen</trd>,
    <trd>toestemmen</trd>
  </trdgrp>
  <trd lng="sv">jaka</trd>
</drv>

<drv mrk="jes.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Konfirmi:
      <ekz>
        li <tld/>igis al mi sian alvenon
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cert.0igi">certigi</ref>,
      <ref cel="asert.0i">aserti</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">bevestigen</trd>
</drv>

<drv mrk="jes.mal0i">
  <kap>
    mal<tld/>i
  </kap>
  <snc>
    <dif>
      Nei.
    </dif>
  </snc>
  <trd lng="de">verneinen</trd>
  <trd lng="nl">ontkennen</trd>
  <trd lng="sv">neka</trd>
</drv>
</art>
