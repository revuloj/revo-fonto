  <trdgrp lng="cs">
    <trd>p&rcaron;ivolen&iacute;</trd>,
    <trd>souhlas</trd>
  </trdgrp>
  <trd lng="de">Ja</trd>
  <trdgrp lng="en">
    <trd>affirmation</trd>,
    <trd>yes</trd>
  </trdgrp>
  <trdgrp lng="fi">
    <trd>my&ouml;nt&auml;v&auml; vastaus</trd>,
    <trd>suostumus</trd>
