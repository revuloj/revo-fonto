<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plusx.xml,v 1.22 2024/02/17 07:52:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>plu&scirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="plusx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="plusx.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxtof.0o.TEKS">&Scirc;tofo</ref> similanta
      <ref cel="velur.0o">veluro</ref>n, sed kun pli longaj haroj:
      <ekz>
        mi por la honorario tuj a&ccirc;etos al mi <tld/>an matenveston kun pelta &ccirc;irka&ubreve;kudro
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tablo estis tegita per malhelru&gcirc;a <tld/>o
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o kaj oro, gigantaj lustroj, molaj se&gcirc;oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        Vinokurov sidigis &scirc;in en verdan <tld/>an apogse&gcirc;on apud malgranda lakkovrita
        tableto
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a urseto</ind>
        <fnt>
          <bib>CKv</bib>
          <lok>1</lok>
        </fnt>.
        <trd lng="de">Teddy</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ju;&c_sh;</trd>
  <trd lng="de"><ofc>*</ofc>Pl&uuml;sch</trd>
  <trd lng="en"><ofc>*</ofc>plush</trd>
  <trdgrp lng="es">
    <trd>felpa</trd>,
    <trd>peluche</trd>
  </trdgrp>
  <trd lng="fr"><ofc>*</ofc>peluche</trd>
  <trd lng="hu">pl&uuml;ss</trd>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30E9;&#x30B7;&#x5929;  <pr>&#x30D5;&#x30E9;&#x30B7;&#x3066;&#x3093;</pr></trd>,
    <trd>&#x30D7;&#x30E9;&#x30C3;&#x30B7;&#x30E5;</trd>
  </trdgrp>
  <trd lng="nl">pluche</trd>
  <trd lng="pl"><ofc>*</ofc>plusz</trd>
  <trd lng="pt">pel&uacute;cia</trd>
  <trd lng="ru"><ofc>*</ofc>&c_p;&c_l;&c_ju;&c_sh;</trd>
  <trd lng="uk">&c_p;&c_l;&c_ju;&c_sh;</trd>
  <trdgrp lng="zh">
    <trd>&#x6BDB;&#x7D68; <pr>m&#xE1;or&#xF3;ng</pr></trd>,
    <trd>&#x6BDB;&#x7ED2; <pr>m&#xE1;or&#xF3;ng</pr></trd>,
    <trd>&#x9577;&#x6BDB;&#x7D68; <pr>ch&#xE1;ngm&#xE1;or&#xF3;ng</pr></trd>,
    <trd>&#x957F;&#x6BDB;&#x7ED2; <pr>ch&#xE1;ngm&#xE1;or&#xF3;ng</pr></trd>
  </trdgrp>
</drv>
</art>
