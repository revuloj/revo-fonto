  <art mrk="$Id: oksford.xml,v 1.9 2024/01/30 16:09:26 revo Exp $">
    <kap>
      <rad>Oksford</rad>/o
    </kap>

<drv mrk="oksford.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="oksford.0o.urbo">
    <dif>
      <ref tip="malprt" cel="brit.B0ujo">Brita</ref> universitata
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      &ccirc;e <ref cel="tamiz.0o">Tamizo</ref>, 90&#x202F;km nordokcidente de
      Londono:
      <ekz>
        tamen la kongreso ... okazos en <tld/>o
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Sibilla Orakolas</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Tolkien</nom> mem diris ke lin siatempe ege fascinis la
        finna <nom>Kalevala</nom> kaj aliaj
        legendaroj, kiujn li studis kiel filologo en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Vja&ccirc;eslav (Slavik) Ivanov</aut>
          <vrk><url ref="&Monato;/2003/007865.html">La oseta lingvo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        proksime al la urbocentro, trovi&gcirc;as nova konstrua&jcirc;o: la <tld/>a
        Centro pri
        Islamaj Studoj
        <fnt>
          <bib>Monato</bib>
          <aut>Jens Spillner</aut>
          <vrk><url ref="&Monato;/2015/011406.php">Kunfandi&gcirc;o de okcidento kaj oriento</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_O;&c_k;&c_s;&c_f;&c_a;&c_r;&c_d;</trd>
    <trd lng="bg">&c_O;&c_k;&c_s;&c_f;&c_o;&c_r;&c_d;</trd>
    <trd lng="de">Oxford</trd>
    <trd lng="en">Oxford</trd>
    <trd lng="fr">Oxford</trd>
    <trd lng="pl">Oksford</trd>
    <trd lng="ro">Oxford</trd>
    <trd lng="ru">&c_O;&c_k;&c_s;&c_f;&c_o;&c_r;&c_d;</trd>
    <trd lng="zh">&#x725B;&#x6D25; <pr>Ni&#xFA;j&#x12B;n</pr></trd>
</snc>
  <trd lng="ja">&#x30AA;&#x30C3;&#x30AF;&#x30B9;&#x30D5;&#x30A9;&#x30FC;&#x30C9;</trd>
  <trd lng="uk">&#x41E;&#x43A;&#x441;&#x444;&#x43E;&#x440;&#x434;</trd>
</drv>

