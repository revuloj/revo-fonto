<kap>
  <ofc>9</ofc><rad>virtual</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="virtua.0a">
  <kap><ofc>9</ofc><tld/>a</kap>
  <snc mrk="virtua.0a.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Ebla sed ankora&ubreve; ne efektivi&gcirc;inta
    </dif>
    <refgrp tip="vid">
      <ref cel="kontin1.0a">kontingenca</ref>,
      <ref cel="latent.0a">latenta</ref>
    </refgrp>
    <trd lng="hu">virtu&aacute;lis</trd>    
  </snc>
  <snc>
    <dif>
      Nur &scirc;ajne ekzistanta:
      <ekz>
        Russel Tyalor kaj Warren Robinett, &ccirc;e la usona Universitato
        Chapel
        Hill (Norda Karolino), parigante la tunelefikan mikroskopon kaj
        la
        teknikojn de <tld/>a realo, fabrikis instrumenton baptitan
        nanomanipulatoro
        <fnt><aut>G. Cazzaro</aut>:
        <vrk><url ref="&Monato;/1997/005286.html">Mikroskope vastaj
        horizontoj</url></vrk>,
        <bib>Monato</bib>,
        <lok>1997/02</lok>.
        </fnt>
      </ekz>
    </dif>
    <trd lng="hu">l&aacute;tsz&oacute;lagos</trd>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_r;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="ca">virtual</trd>
  <trd lng="de">virtuell</trd>
  <trd lng="en">virtual</trd>
  <trd lng="es">virtual</trd>
  <trd lng="fr">virtuel</trd>
  <trd lng="nl">virtueel</trd>
  <trd lng="pt">virtual</trd>
  <trd lng="ru">&c_v;&c_i;&c_r;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_r;&c_t;&c_u;&c_a;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&#x43E;&c_t;&c_je;&#x43D;&c_c;&c_ib;&c_j;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_h;&#x43E;&c_v;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_d;&c_a;&c_t;&#x43D;&c_i;&c_j; &#x43F;&c_r;&#x43E;&#x44F;&c_v;&c_i;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>
