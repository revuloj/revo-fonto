<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: hefej.xml,v 1.5 2023/08/31 15:46:15 afido Exp $">
    <kap>
      <rad>Hefej</rad>/o
    </kap>

<drv mrk="hefej.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la oriento de <ref tip="malprt" cel="cxin.CX0ujo">&Ccirc;inujo</ref>,
      &ccirc;. 200&#x202F;km okcidente de <ref cel="nankin.0o">Nankino</ref>:
      <ekz>
        de la 8-a &gcirc;is la 6-a jarcento a.K. <tld/>o estis loko de multaj malgrandaj
        &scirc;tatoj, kiuj ofte bataladis
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Hefejo">Hefejo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        filmrakonto pri la &ccirc;ina urbo <tld/>o kaj la &ccirc;iutaga vivo de tieaj esperantistoj
        <fnt>
          <vrk><url ref="https://katalogo.uea.org/katalogo.php?kateg=dvd">UEA Katalogo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_H;&c_e;&c_f;&c_e;&c_j;</trd>
    <trd lng="de">Hefei</trd>
    <trd lng="en">Hefei</trd>
    <trd lng="fr">Hefei</trd>
    <trd lng="zh">&#x5408;&#x80A5;&#x5E02;</trd>
  </snc>
  <trd lng="ja">&#x30DB;&#x30FC;&#x30D5;&#x30A7;&#x30A4;</trd>
  <trd lng="uk">&c_H;&c_je;&c_f;&c_je;&c_j;</trd>
</drv>

  </art>

