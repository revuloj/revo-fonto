<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: furgon.xml,v 1.5 2022/06/09 22:31:56 afido Exp $">
<kap>
  <rad>furgon</rad>/o  <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="furgon.0o">
  <kap><tld/>o</kap>
  <snc mrk="furgon.0o.TRA">
    <dif>
      Radveturilo kun tegita platformo por transporto de
      kargo, paka&jcirc;oj, komerca&jcirc;oj:
    </dif>
    <subsnc mrk="furgon.0o.AUT">
      <uzo tip="fak">AUT</uzo>
      <dif>
        Tia stratveturilo:
      <ekz>
        la&ubreve; la malplena strato traveturis en
        la kontra&ubreve;a direkto grandega <tld/>o,
        &scirc;ar&gcirc;ita per kestoj
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/vojo.html">
          La vojo nenien</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
      </dif>
      <ref tip="vid" cel="kamion.0o">kamiono</ref>
      <trdgrp lng="de">
        <trd>Transporter</trd>,
        <trd>Kastenwagen</trd>,
        <trd>Lieferwagen</trd>
      </trdgrp>
      <trd lng="en">van</trd>
    </subsnc>
    <subsnc mrk="furgon.0o.FER">
      <uzo tip="fak">FER</uzo>
      <dif>
        Tia vagono.
      </dif>
      <trdgrp lng="de">
        <trd>G&uuml;terwagen</trd>,
        <trd>Gep&auml;ckwagen</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="fr">fourgon</trd>
  <trd lng="uk">&c_f;&c_u;&c_r;&c_gu;&#x43E;&#x43D;</trd>
</drv>
</art>

