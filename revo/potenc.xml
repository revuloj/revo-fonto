<drv mrk="potenc.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="potenc.0i.POL">
    <dif>
      Havi <tld/>on super:
      <ekz>
        la tutan Mediteraneon iam <tld/>is Romo;
      </ekz>
      <ekz>
        vidis neniam mi kaj ne ekvidos jam virojn similajn, kiel Pirito
        kaj Drio, kiu <tld/>is popolojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kvardek falsistoj sendepende unu de la alia prezentis sin kiel
        Petro la 3-a kaj provis <tld/>i en iuj provincoj de Rusio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17923">Petro la
          3-a (Rusio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_l;&c_a;&c_d;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>die <ind>Macht</ind> aus&uuml;ben &uuml;ber</trd>,
      <trd>die <ind>Gewalt</ind> haben &uuml;ber</trd>,
      <trd>beherrschen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ural</trd>,
      <trd>hatalm&aacute;ban tart</trd>
    </trdgrp>
    <trd lng="nl">macht hebben</trd>
    <trdgrp lng="pl">
      <trd>mie&cacute;  <ind>w&lstroke;adz&eogonek;</ind>  nad kim&sacute;</trd>,
      <trd>posiada&cacute;  <ind>moc</ind>  nad kim&sacute;</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>putea</trd>,
      <trd>fi  <ind>capabil</ind></trd>,
      <trd>avea  <ind>putere</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_l;&c_a;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trdgrp lng="zh">
    <trd>&#x80FD;&#x591F;&#x81EA;&#25105;&#x63A7;&#21046;
    <pr>n&#xE9;ngg&#xF2;uz&#xEC;w&#x1D2;k&#xF2;ngzh&#xEC;</pr></trd>,
    <trd>&#x80FD;&#22816;&#x81EA;&#25105;&#x63A7;&#21046;
    <pr>n&#xE9;ngg&#xF2;uz&#xEC;w&#x1D2;k&#xF2;ngzh&#xEC;</pr></trd>
  </trdgrp>
</snc>

  <snc mrk="potenc.0i.algebra">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="potenc.0igi.algebra">Potencigi</ref>:
      <ekz>
        eksponento, per kiu oni devas <tld/>i b por ekhavi a
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3243">Operacioj
          per nombroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
        <trdgrp lng="be">
      <trd>&c_u;&c_z;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_u;
<ind>&c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_mol;</ind></trd>,
      <trd>&c_v;&c_y;&c_l;&c_ib;&c_ch;&c_a;&c_c;&c_mol;
<ind>&c_s;&c_t;&c_u;&c_p;&c_je;&c_n;&c_mol;</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>hatv&aacute;nyoz</trd>,
      <trd>hatv&aacute;nyra emel</trd>
    </trdgrp>
  </snc>
  <trd lng="es">potenciar</trd>
  <trdgrp lng="ja">
    <trd>&#x652F;&#x914D;&#12377;&#x308B;  <pr>&#12375;&#x306F;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x6A29;&#x529B;&#12434;&#12405;&#x308B;&#12358;  <pr>&#12369;&#12435;&#x308A;&#12423;&#x304F;&#12434;&#12405;&#x308B;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">macht geven</trd>
  <trdgrp lng="sk">
    <trd>ma&tcaron; moc</trd>,
    <trd>vl&aacute;dnu&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&#x43E;&#x43B;&#x43E;&c_d;&c_a;&c_r;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
