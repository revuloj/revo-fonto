<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: euxzebi.xml,v 1.7 2024/03/10 21:20:02 revo Exp $">
<kap>
  <rad>E&ubreve;zebi</rad>/o,
  <var><kap><rad var="bo">E&ubreve;zeb</rad>/o <fnt><bib>NePiv</bib></fnt></kap></var>
</kap>
<drv mrk="euxzebi.0o">
  <kap><tld/>o, <var><kap><tld var="bo"/>o</kap></var></kap>
  <snc mrk="euxzebi.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        historiisto de tiuj tempoj <tld/>o skribis, ke anta&ubreve; la
        ekzekuto, Jakobo kisis sian ekzekutiston
        <fnt>
          <aut>J. Zielonka</aut>
          <vrk><url ref="http://esperanto.pl/page.php?tid=310400">
          Predikoj en Kromie&jcirc;i&scirc;</url></vrk>
          <lok>esperanto.pl, 2002</lok>
        </fnt>;
      </ekz>
      <ekz>
        fine de tiu &ccirc;i strato, kaj meze de tiu, kiu bordas la
        kanalon,
        staris kolono kun kruco, nomita kruco de sankta <tld/>o
        <fnt><aut>A. Manzoni, trad. B. Cadei</aut> <vrk>La
        Gefian&ccirc;oj</vrk> <lok>2006</lok></fnt>;
      </ekz>
      <ekz>
        <tld var="bo"/>o de Cezareo diras ke li estis la unua episkopo de Kreto
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Apostolaj_sidejoj">
          Apostolaj sidejoj</url></vrk>
          <lok>Vikipedio, 2023-08-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Eusebius</trd>
  <trd lng="fr">Eus&egrave;be<klr> (pr&eacute;nom masculin)</klr></trd>
  <trd lng="it">Eusebio</trd>
  <trdgrp lng="pl">
    <trd>Euzebiusz</trd>,
    <trd>Zibi<klr> (zdrobnienie)</klr></trd>
  </trdgrp>
  <trd lng="pt">Eus&eacute;bio</trd>
</drv>
</art>

