<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vin</rad>/o
</kap>
<drv mrk="vin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Alkohola trinka&jcirc;o, ekstraktita el fermentintaj vinberoj:
      <ekz>
        blanka, ru&gcirc;a, juna<fnt>Z</fnt> vino;
      </ekz>
      <ekz>
        la vino gajigas la koron de la homo<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="most.0o">mosto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_n;&c_o;</trd>
  <trd lng="cs">v&iacute;no</trd>
  <trd lng="de">Wein</trd>
  <trd lng="el">&kappa;&rho;&alfa;&sigma;&jota_ton;</trd>
  <trd lng="en">wine</trd>
  <trd lng="fr">vin</trd>
  <trd lng="hu">bor</trd>
  <trd lng="hr">vino</trd>
  <trd lng="nl">wijn</trd>
  <trd lng="pl">vino</trd>
  <trd lng="pt">vinho</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_o;</trd>
  <trd lng="sv">vin</trd>
  <trd lng="tr">&scedil;arap</trd>
</drv>

<drv mrk="vin.0bero">
  <kap><tld/>bero</kap>
  <snc>
    <dif>
      Frukto de <tld/>berujo, uzata por fari <tld/>on:
      <ekz>
        li lavas en sango de <tld/>beroj sian mantelon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
lng="be">&c_v;&c_ib;&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd
>
  <trd lng="de">Weinbeere</trd>
  <trd lng="el">&sigma;&tau;&rho;&alfa;&phi;&ypsilon_ton;&lambda;&jota;</trd>
  <trd lng="fr">raisin</trd>
  <trd lng="hu">sz&odblac;l&odblac;</trd>
  <trd lng="nl">druif</trd>
  <trd lng="pt">uva</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="sv">vindruva</trd>
  <trd lng="tr">&uuml;z&uuml;m</trd>
</drv>

<drv mrk="vin.0beraro">
  <kap><tld/>beraro</kap>
  <snc>
    <dif>
      Aro da <tld/>beroj, pendantaj de sama trunketo
    </dif>
    <refgrp tip="sin">
      <ref cel="grapol.0o">grapolo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_o;&c_n;&c_k;&c_a;
<klr>(&c_v;&c_ib;&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_u;)</klr></trd>
  <trd lng="de">Weintraube</trd>
  <trd lng="el">&tau;&sigma;&alfa;&my;&pi;&jota_ton;</trd>
  <trd lng="fr">grappe</trd>
  <trd lng="hu">sz&odblac;l&odblac;f&uuml;rt</trd>
  <trd lng="nl">druiventros</trd>
  <trd lng="pt">cacho</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_n;&c_a;&c_ja;
<ind>&c_g;&c_r;&c_o;&c_z;&c_d;&c_mol;</ind></trd>
</drv>

<drv mrk="vin.0berarbo0berujo">
  <kap><tld/>berarbo, <tld/>berujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="vitac.0oj">vitacoj</ref>
      (<trd lng="la">Vitis</trd>), arbeto produktanta
      <tld/>berojn; <ref tip="sin" cel="vit.0o">vito</ref>.
    </dif>
  </snc>
  <trd lng="de">Weinrebe</trd>
  <trd lng="el">&kappa;&lambda;&eta;&my;&alfa;&tau;&alfa;&rho;&jota;&alfa_ton;</trd>
  <trd lng="fr">vigne</trd>
  <trd lng="hu">sz&odblac;l&odblac;t&odblac;ke</trd>
  <trd lng="nl">wijnstok</trd>
  <trd lng="pt">parreira</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="tr">asma</trd>
</drv>

<drv mrk="vin.0bergxardeno">
  <kap><tld/>ber&gcirc;ardeno</kap>
  <snc>
    <dif>
      Loko plantita per <tld/>berujoj:
      <ekz>
        Noa plantis <tld/>ber&gcirc;ardenon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
lng="be">&c_v;&c_ib;&c_n;&c_a;&c_g;&c_r;&c_a;&c_d;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Weinberg</trd>
  <trd lng="hu">sz&odblac;l&odblac;skert</trd>
  <trd lng="nl">wijngaard</trd>
  <trd
lng="ru">&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
