<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>flut</rad>/o</kap>
<drv mrk="flut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Iu el diversaj
      <ref tip="super" cel="instru2.blov0o">blovinstrumentoj</ref>
      konsistantaj el solida, rekta tubo
      kun fingre a&ubreve; klave &scirc;topeblaj truoj,
      kiun oni sonigas blovante al e&gcirc;o de la ekstremo
      au de flanka truo.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_je;&c_j;&c_t;&c_a;</trd>
  <trd lng="de">Fl&ouml;te</trd>
  <trd lng="es">flauta</trd>
  <trd lng="fr">fl&ucirc;te</trd>
  <trd lng="hu">fuvola</trd>
  <trd lng="nl">fluit</trd>
  <trd lng="ru">&c_f;&c_l;&c_je;&c_j;&c_t;&c_a;</trd>
</drv>

<drv mrk="flut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Muziki per <tld/>o a&ubreve; kvaza&ubreve; per <tld/>o:
      <ekz>
        la merlo <tld/>as sian kanton.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_f;&c_l;&c_je;&c_j;&c_c;&c_je;</ind></trd>
  <trdgrp lng="de">
    <trd>Fl&ouml;te spielen</trd>,
    <trd>fl&ouml;ten</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>jouer de la <ind>fl&ucirc;te</ind></trd>,
    <trd>siffler<klr> (ex. en p. d'un oiseau)</klr></trd>
  </trdgrp>
  <trd lng="hu">fuvol&aacute;zik</trd>
  <trdgrp lng="nl">
    <trd>fluit spelen</trd>,
    <trd>fluiten</trd>
  </trdgrp>
</drv>

<drv mrk="flut.bas0o">
  <kap>bas<tld/>o</kap> <fnt><bib>MuzTerm</bib></fnt>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Longa <ref tip="super" cel="instru2.blov0o">blovinstrumento</ref>,
      basa <ref tip="vid" cel="flut.transversa0o">transversa fluto</ref>
      kun hoka ekstremo flanke de la blovtruo.
    </dif>
  </snc>
  <trd lng="fr">fl&ucirc;te basse</trd>
  <trd lng="hu">basszusfurulya</trd>
  <trd lng="nl">basfluit</trd>
</drv>

<drv mrk="flut.bek0o">
  <kap>bek<tld/>o</kap> <fnt><bib>MuzTerm</bib></fnt>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      &Gcirc;enerale ligna <ref tip="super"
      cel="instru2.blov0o">blovinstrumento</ref>,
      mallonga rekta <ref tip="vid" cel="flut.0o">fluto</ref>
      en kiun oni blovas per ekstrema bu&scirc;peco.
    </dif>
  </snc>
  <trd lng="fr">fl&ucirc;te &agrave; bec</trd>
  <trdgrp lng="hu">
    <trd>cs&odblac;rfuvola</trd>,
    <trd>c&ouml;l&ouml;pfuvola</trd>,
    <trd>blockfl&ouml;te</trd>
  </trdgrp>
  <trd lng="nl">blokfluit</trd>
</drv>

<drv mrk="flut.transversa0o">
  <kap>transversa <tld/>o</kap> <fnt><bib>RokPop</bib></fnt>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Alflanke tenata <ref tip="super"
      cel="instru2.blov0o">blovinstrumento</ref>,
      <ref tip="vid" cel="flut.0o">fluto</ref> kun unu fermita ekstrema&jcirc;o
      apud kiu estas blovtruo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Querfl&ouml;te</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>flute traversi&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">har&aacute;ntfuvola</trd>
  <trd lng="nl">dwarsfluit</trd>
</drv>
</art>
