<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: flut.xml,v 1.37 2022/04/27 17:19:11 revo Exp $">
<kap><ofc>*</ofc>
<rad>flut</rad>/o</kap>

<drv mrk="flut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="flut.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Iu el diversaj
      <ref tip="lst" cel="instru2.blov0o" lst="voko:blovinstrumentoj">blovinstrumentoj</ref>
      konsistantaj el solida, rekta tubo
      kun fingre a&ubreve; klave &scirc;topeblaj truoj,
      kiun oni sonigas blovante al e&gcirc;o de la ekstremo
      a&ubreve; de flanka truo:
      <ekz>
        manovru sur tiuj &ccirc;i truoj per viaj fingroj, donu al la <tld/>o spiron per via
        bu&scirc;o, kaj la <tld/>o eldonos tre bonan muzikon
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_je;&c_j;&c_t;&c_a;</trd>
  <trd lng="cs">fl&eacute;tna</trd>
  <trd lng="de">Fl&ouml;te</trd>
  <trd lng="en">flute</trd>
  <trd lng="es">flauta</trd>
  <trd lng="fr">flute</trd>
  <trd lng="hu">fuvola</trd>
  <trdgrp lng="id">
    <trd>seruling</trd>,
    <trd>suling</trd>
  </trdgrp>
  <trd lng="it">flauto</trd>
  <trd lng="nl">fluit</trd>
  <trd lng="pl">flet</trd>
  <trd lng="pt">flauta</trd>
  <trd lng="ru">&c_f;&c_l;&c_je;&c_j;&c_t;&c_a;</trd>
  <trd lng="sk">flauta</trd>
  <trd lng="uk">&c_f;&#x43B;&c_je;&c_j;&c_t;&c_a;</trd>
</drv>

<drv mrk="flut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="flut.0i.MUZ">
    <dif>
      Muziki per <tld/>o a&ubreve; kvaza&ubreve; per <tld/>o:
      <ekz>
        la kantoturdoj senlace <tld/>is siajn ton&ccirc;enojn sur la suproj de altaj piceoj en
        la ardo de la subiranta suno
        <fnt>
          <bib>ViV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_f;&c_l;&c_je;&c_j;&c_c;&c_je;</ind></trd>
  <trd lng="cs">hr&aacute;t na fl&eacute;tnu</trd>
  <trdgrp lng="de">
    <trd>Fl&ouml;te spielen</trd>,
    <trd>fl&ouml;ten</trd>
  </trdgrp>
  <trd lng="en">play (the) flute</trd>
  <trd lng="es">tocar la flauta</trd>
  <trdgrp lng="fr">
    <trd>jouer de la <ind>flute</ind></trd>,
    <trd>siffler<klr> (p. ex. en p. d'un oiseau)</klr></trd>
  </trdgrp>
  <trd lng="hu">fuvol&aacute;zik</trd>
  <trd lng="id"><baz>suling</baz>menyuling</trd>
  <trd lng="id">ber<ind>suling</ind></trd>
  <trd lng="it">suonare con il <ind>flauto</ind></trd>
  <trdgrp lng="nl">
    <trd>fluit spelen</trd>,
    <trd>fluiten</trd>
  </trdgrp>
  <trd lng="pl">gra&cacute; na <ind>flecie</ind></trd>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a; <ind>&c_f;&c_l;&c_je;&c_j;&c_t;&c_je;</ind></trd>
  <trd lng="sk">hra&tcaron; na flaute</trd>
  <trd lng="uk">&c_g;&c_r;&c_a;&c_t;&c_i; &#x43D;&c_a; &c_f;&#x43B;&c_je;&c_j;&c_t;&c_ib;</trd>
</drv>

<drv mrk="flut.bas0o">
  <kap>bas<tld/>o</kap> <fnt><bib>MuzTerm</bib></fnt>
  <snc mrk="flut.bas0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Longa <ref tip="lst" cel="instru2.blov0o" lst="voko:blovinstrumentoj">blovinstrumento</ref>,
      basa <ref tip="vid" cel="flut.transversa0o">transversa fluto</ref>
      kun hoka ekstremo flanke de la blovtruo.
    </dif>
  </snc>
  <trd lng="de">Bassfl&ouml;te</trd>
  <trd lng="en">bass flute</trd>
  <trd lng="es">flauta bajo</trd>
  <trd lng="fr">flute basse</trd>
  <trd lng="hu">basszusfurulya</trd>
  <trd lng="it">flauto basso</trd>
  <trd lng="nl">basfluit</trd>
</drv>

<drv mrk="flut.bek0o">
  <kap>bek<tld/>o</kap> <fnt><bib>MuzTerm</bib></fnt>
  <snc mrk="flut.bek0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.blov0o" lst="voko:blovinstrumentoj">Blovinstrumento</ref>,
      mallonga rekta <ref tip="vid" cel="flut.0o">fluto</ref>
      en kiun oni blovas per ekstrema bu&scirc;peco:
      <ekz>
        Meven esprimas sian voja&gcirc;emon kaj inter&scirc;an&gcirc;emon invitante nin <klr>[&#8230;]</klr>
        danci je ritmoj de tradiciaj instrumentoj kiaj violono, bek<tld/>o, a&ubreve;
        akordiono
        <fnt>
          <aut>-</aut>
          <vrk><url ref="http://artista.ikso.net/eurokka/novajx/auxg04_2.php">Plu Meven - Ondumoj
          tra la mond'</url></vrk>
          <lok>Informletero de Vinilkosmo, 2004-08 (8)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">zobcov&aacute; fl&eacute;tna</trd>
  <trd lng="de">Blockfl&ouml;te</trd>
  <trd lng="en">recorder</trd>
  <trd lng="es">flauta de pico</trd>
  <trd lng="fr">flute &agrave; bec</trd>
  <trd lng="he">&samekh;&vav;&gimel; &het;&lamed;&yod;&lamed; </trd>
  <trdgrp lng="hu">
    <trd>cs&odblac;rfuvola</trd>,
    <trd>c&ouml;l&ouml;pfuvola</trd>,
    <trd>blockfl&ouml;te</trd>
  </trdgrp>
  <trd lng="id">rekorder</trd>
  <trdgrp lng="it">
    <trd>flauto dolce</trd>,
    <trd>flauto a becco</trd>,
    <trd>flauto diritto</trd>
  </trdgrp>
  <trd lng="nl">blokfluit</trd>
  <trdgrp lng="pl">
    <trd>fujarka</trd>,
    <trd>flet prosty</trd>
  </trdgrp>
  <trd lng="sk">zobcov&aacute; flauta</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_u;&c_v;&c_i;&c_c;&c_ib;</trd>,
    <trd>&c_f;&#x43B;&#x43E;&#x44F;&c_r;&c_a;</trd>,
    <trd>&c_s;&c_v;&c_i;&c_r;&c_ib;&#x43B;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="flut.pajno0o">
  <kap>
    pajno<tld/>o,
    <var><kap>pajn<tld/>o</kap></var>
  </kap>
  <snc mrk="flut.pajno0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.blov0o" lst="voko:blovinstrumentoj">Blovinstrumento</ref>
      konsistanta el serio de
      diverse longaj, sentruaj flutoj, kiuj sonigas la diversajn tonojn:
      <ekz>
        pajno<tld/>oj el bambuo longaj &gcirc;is 2,20 metroj
        <fnt>
          <aut>-</aut>
          <vrk><url ref="&Viki;/Muziko_de_Brazilo">Muziko de
          Brazilo</url></vrk>
          <lok>Vikipedio, 2022-01-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Panfl&ouml;te</trd>,
    <trd>Pfozhobel<klr> [ark]</klr></trd>
  </trdgrp>
<trdgrp lng="fr">
  <trd>flute de Pan</trd>,
  <trd>syrinx</trd>
</trdgrp></drv>

<drv mrk="flut.sak0o">
  <kap>
    sak<tld/>o
  </kap>
  <snc mrk="flut.sak0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.blov0o" lst="voko:blovinstrumentoj">Blovinstrumento</ref>
      konsistanta el aersako,
      kiun la ludanto plenblovas, kaj kiu elpelas la aeron tra unu a&ubreve; pluraj flutoj;
      <ref cel="sxalm.sak0o.MUZ">sak&scirc;almo</ref>:
      <ekz>
        Ride ili rigardadas / Sur la vivan foirejon, / Kie dancas la geursoj / &Ccirc;e la
        sonoj de sak<tld/>o
        <fnt>
          <aut>H. Heine, trad. F. Pillath</aut>
          <vrk><url ref="https://eo.wikisource.org/wiki/Atta_Troll/I">Atta Troll</url></vrk>
          <lok>1925</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Sackpfeife</trd>,
    <trd>Dudelsack</trd>
  </trdgrp>
  <trd lng="en">bagpipes</trd>
  <trd lng="fr">cornemuse</trd>
  <trd lng="hu">duda</trd> 
  <trdgrp lng="it">
    <trd>cornamusa</trd>, 
    <trd>zampogna</trd>, 
    <trd>piva</trd>
  </trdgrp> 
  <trd lng="nl">doedelzak</trd>
  <trdgrp lng="pl">
    <trd>dudy</trd>,
    <trd>koza</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_y;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&#x43E;&#x43B;&c_i;&#x43D;&#x43A;&c_a;</trd>,
    <trd>&#x43A;&#x43E;&c_z;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="flut.transversa0o">
  <kap>transversa <tld/>o</kap> <fnt><bib>RokPop</bib></fnt>
  <snc mrk="flut.transversa0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Alflanke tenata <ref tip="lst" cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>,
      <ref tip="vid" cel="flut.0o">fluto</ref> kun unu fermita
      ekstrema&jcirc;o apud kiu estas blovtruo:
      <ekz>
        <nom>Jaques de Vaucanson</nom> <klr>[&#8230;]</klr> studis la konsiston
        kaj la ludmanieron de transversa <tld/>o
        <fnt>
          <aut>K. Podak, trad. D. kaj H.-G. Kaiser</aut>
          <vrk><url ref="https://satesperanto.org/spip.php?article3109">La mekanika anaso de Jaques
          de Vaucanson (1709-1782)</url></vrk>
          <lok>SAT Kulturo, [2008?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">p&rcaron;&iacute;&ccaron;n&aacute; fl&eacute;tna</trd>
  <trd lng="de">Querfl&ouml;te</trd>
  <trd lng="en">transverse flute</trd>
  <trd lng="es">flauta travesera</trd>
  <trd lng="fr">flute traversi&egrave;re</trd>
  <trd lng="hu">har&aacute;ntfuvola</trd>
  <trd lng="it">flauto traverso</trd>
  <trd lng="nl">dwarsfluit</trd>
  <trd lng="pl">flet poprzeczny</trd>
  <trd lng="ru">&c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_f;&c_l;&c_je;&c_j;&c_t;&c_a;</ind></trd>
  <trd lng="sk">prie&ccaron;na flauta</trd>
</drv>
</art>
