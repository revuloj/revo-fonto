<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>flut</rad>/o
</kap>
<drv mrk="flut.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.blov0o">Blovinstrumento</ref> konsistanta el ligna
      a&ubreve; metala tubo kun truoj kaj
      eventuale &scirc;topaj klavetoj, en
      kiun oni lipblovas per flanka truo.
    </dif>
    <adm>
    Lipblovas? <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="de">Fl&ouml;te</trd>
  <trd lng="fr">fl&ucirc;te</trd>
  <trd lng="ru">&c_f;&c_l;&c_je;&c_j;&c_t;&c_a;</trd>
</drv>

<drv mrk="flut.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Muziki per <tld/>o a&ubreve; kvaza&ubreve; per <tld/>o:
      <ekz>
        la merlo <tld/>as sian kanton.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Fl&ouml;te spielen</trd>,
    <trd>fl&ouml;ten</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>jouer de la <ind>fl&ucirc;te</ind></trd>,
    <trd>siffler<klr tip="ind"> (ex. en p. d'un oiseau)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="flut.0eto">
  <kap>
    <tld/>eto
  </kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Mallonga <ref tip="super" cel="flut.0o">fluto</ref>
      kun alta registro.
      <ref tip="sin" cel="fifr.0o">fifro</ref>.
    </dif>
  </snc>
  <trd lng="de">Querpfeife</trd>
  <trdgrp lng="fr">
    <trd>fifre</trd>,
    <trd>piccolo</trd>,
    <trd>picolo</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_i;&c_r;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="flut.bas0o">
  <kap>
    bas<tld/>o
  </kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Longa <ref tip="super" cel="flut.0o">fluto</ref>
      kun hoka ekstremo por pli facila teno
      kaj malalta registro.
    </dif>
  </snc>
  <fnt>
    <aut>M. C. Butler, F. Merrick, ind. David G. Hill</aut>:
    <vrk>Muzika terminaro; Sofia, 1992</vrk>,
    <lok>p. 24</lok>
  </fnt>
  <trd lng="fr">fl&ucirc;te basse</trd>
</drv>

<drv mrk="flut.bek0o">
  <kap>
    bek<tld/>o
  </kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      &Gcirc;enerale ligna <ref tip="super" cel="flut.0o">fluto</ref>
      rekte tenata,
      en kiun oni blovas per ekstrema beko.
    </dif>
  </snc>
  <fnt>
    <aut>M. C. Butler, F. Merrick, ind. David G. Hill</aut>:
    <vrk>Muzika terminaro; Sofia, 1992</vrk>,
    <lok>p. 24</lok>
  </fnt>
  <trd lng="fr">fl&ucirc;te &agrave; bec</trd>
</drv>
</art>
