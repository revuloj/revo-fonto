<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>flut</rad>/o</kap>

<drv mrk="flut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Iu el diversaj
      <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumentoj</ref>
      konsistantaj el solida, rekta tubo
      kun fingre a&ubreve; klave &scirc;topeblaj truoj,
      kiun oni sonigas blovante al e&gcirc;o de la ekstremo
      a&ubreve; de flanka truo.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_je;&c_j;&c_t;&c_a;</trd>
  <trd lng="de">Fl&ouml;te</trd>
  <trd lng="en">flute</trd>
  <trd lng="es">flauta</trd>
  <trd lng="fr">fl&ucirc;te</trd>
  <trd lng="hu">fuvola</trd>
  <trdgrp lng="id">
    <trd>seruling</trd>,
    <trd>suling</trd>
  </trdgrp>
  <trd lng="it">flauto</trd>
  <trd lng="nl">fluit</trd>
  <trd lng="pl">flet</trd>
  <trd lng="pt">flauta</trd>
  <trd lng="ru">&c_f;&c_l;&c_je;&c_j;&c_t;&c_a;</trd>
</drv>

<drv mrk="flut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Muziki per <tld/>o a&ubreve; kvaza&ubreve; per <tld/>o:
      <ekz>
        la merlo <tld/>as sian kanton.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_f;&c_l;&c_je;&c_j;&c_c;&c_je;</ind></trd>
  <trdgrp lng="de">
    <trd>Fl&ouml;te spielen</trd>,
    <trd>fl&ouml;ten</trd>
  </trdgrp>
  <trd lng="en">play (the) flute</trd>
  <trd lng="es">tocar la flauta</trd>
  <trdgrp lng="fr">
    <trd>jouer de la <ind>fl&ucirc;te</ind></trd>,
    <trd>siffler<klr> (ex. en p. d'un oiseau)</klr></trd>
  </trdgrp>
  <trd lng="hu">fuvol&aacute;zik</trd>
  <trd lng="id">[<ind>suling</ind>] menyuling</trd>
  <trd lng="it">suonare con il <ind>flauto</ind></trd>
  <trdgrp lng="nl">
    <trd>fluit spelen</trd>,
    <trd>fluiten</trd>
  </trdgrp>
  <trd lng="pl">gra&cacute; na <ind>flecie</ind></trd>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a; <ind>&c_f;&c_l;&c_je;&c_j;&c_t;&c_je;</ind></trd>
</drv>

<drv mrk="flut.bas0o">
  <kap>bas<tld/>o</kap> <fnt><bib>MuzTerm</bib></fnt>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Longa <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>,
      basa <ref tip="vid" cel="flut.transversa0o">transversa fluto</ref>
      kun hoka ekstremo flanke de la blovtruo.
    </dif>
  </snc>
  <trd lng="de">Bassfl&ouml;te</trd>
  <trd lng="en">bass flute</trd>
  <trd lng="es">flauta bajo</trd>
  <trd lng="fr">fl&ucirc;te basse</trd>
  <trd lng="hu">basszusfurulya</trd>
  <trd lng="it">flauto basso</trd>
  <trd lng="nl">basfluit</trd>
</drv>

<drv mrk="flut.bek0o">
  <kap>bek<tld/>o</kap> <fnt><bib>MuzTerm</bib></fnt>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      &Gcirc;enerale ligna <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>,
      mallonga rekta <ref tip="vid" cel="flut.0o">fluto</ref>
      en kiun oni blovas per ekstrema bu&scirc;peco.
    </dif>
  </snc>
  <trd lng="de">Blockfl&ouml;te</trd>
  <trd lng="en">recorder</trd>
  <trd lng="es">flauta de pico</trd>
  <trd lng="fr">fl&ucirc;te &agrave; bec</trd>
  <trdgrp lng="hu">
    <trd>cs&odblac;rfuvola</trd>,
    <trd>c&ouml;l&ouml;pfuvola</trd>,
    <trd>blockfl&ouml;te</trd>
  </trdgrp>
  <trd lng="id">rekorder</trd>
  <trdgrp lng="it">
    <trd>flauto dolce</trd>,
    <trd>flauto a becco</trd>,
    <trd>flauto diritto</trd>
  </trdgrp>
  <trd lng="nl">blokfluit</trd>
  <trdgrp lng="pl">
    <trd>fujarka</trd>,
    <trd>flet prosty</trd>
  </trdgrp>
</drv>

<drv mrk="flut.transversa0o">
  <kap>transversa <tld/>o</kap> <fnt><bib>RokPop</bib></fnt>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Alflanke tenata <ref tip="lst"
      cel="instru2.blov0o"
      lst="voko:blovinstrumentoj">blovinstrumento</ref>,
      <ref tip="vid" cel="flut.0o">fluto</ref> kun unu fermita
      ekstrema&jcirc;o
      apud kiu estas blovtruo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Querfl&ouml;te</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>transverse flute</trd>
  </trdgrp>
  <trd lng="es">flauta travesera</trd>
  <trdgrp lng="fr">
    <trd>flute traversi&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">har&aacute;ntfuvola</trd>
  <trd lng="it">flauto traverso</trd>
  <trd lng="nl">dwarsfluit</trd>
  <trd lng="pl">flet poprzeczny</trd>
  <trd lng="ru">&c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_f;&c_l;&c_je;&c_j;&c_t;&c_a;</ind></trd>
</drv>
</art>
