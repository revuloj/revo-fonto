<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vibr.xml,v 1.28 2024/09/23 01:55:53 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>vibr</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vibr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vibr.0i.tremmove">
    <dif>
      Pri <ref cel="elast.0a.FIZ">elasta</ref> korpo: regule kaj rapide <ref tip="super"
      cel="trem.0i">tremi</ref>,
      tiel ke oni povas apena&ubreve; vidi, sed senti &gcirc;in kaj eventuale a&ubreve;di samfrekvencan sonon:
      <ekz>
        la metala bruo de sonorilo <tld/>anta
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="altern.0i">alterni</ref>,
        <ref cel="sving.0igxi">svingi&gcirc;i</ref>,
      <ref cel="tint.0i">tinti</ref>,
      <ref cel="sonor.0i">sonori</ref>
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_v;&c_i;&c_b;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vibr.0i.sono">
    <dif>
      Pri <ref cel="son.0o">sono</ref>: <ref tip="super" cel="sving.0igxi">svingi&gcirc;i</ref> regule
      inter du proksimaj
      frekvencoj tiel, ke esti&gcirc;as tremanta nefirma, <ref cel="sxveb.0i.hezite">&scirc;vebanta</ref> sono:
      <ekz>
        tiuj vortoj, elparolitaj per vo&ccirc;o <tld/>anta, aliris rekte al la koro de la
        maljuna nobelulo
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        vo&ccirc;o, en kiu <tld/>is la energio de roma soldato
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        per vo&ccirc;o en kiu <tld/>is korpremo kaj timo, &scirc;i petegis, ke <klr>[&#8230;]</klr> li
        kompatu &scirc;in
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la birdoj kaskade plenigis la aeron per <tld/>antaj tonoj
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj okuloj brilis melankolie, kaj &scirc;ia vo&ccirc;o <tld/>etis de pli intima kamaradeco
        ol ordinare
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="interf.0i">interferi</ref>,
      <ref cel="modul1.0i">moduli&gcirc;i</ref>,
      <ref cel="oscil.0i">oscili</ref>
    </refgrp>
    <trd lng="de">zittern <klr>(Ton)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_r;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_i;&c_b;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vibr.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="stl">POE</uzo>
    <dif>
      <ref cel="sent.0o">Sento</ref>, <ref cel="emoci.0o">emocio</ref>
      similanta fizikan <tld/>adon, alterne rapide &scirc;veletanta kaj mal&scirc;veletanta:
      <ekz>
        kontra&ubreve; beleco tiel <tld/>anta de pasio, kiel estas la &scirc;ia, viro <klr>[ne]</klr>
        povas kontra&ubreve;batali
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        en li <tld/>as printempo, lia koro tamburas
        <fnt>
          <bib>LGA</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ebri.0a.spontanee">ebria<sncref/></ref>,
      <ref cel="euxfori.0o">e&ubreve;forio</ref>,
      <ref cel="flagr.0i">flagri</ref>,
      <ref cel="fluktu.0i">fluktui</ref>,
      <ref cel="sku.0igxi">skui&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_y;&c_m;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_ib;&c_b;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_r;&c_y;&c_zh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">vibrar</trd>
  <trdgrp lng="cs">
    <trd>chv&ecaron;t se</trd>,
    <trd>kmitat</trd>,
    <trd>vibrovat</trd>,
    <trd>zn&iacute;t</trd>
  </trdgrp>
  <trd lng="de">vibrieren</trd>
  <trd lng="en">vibrate</trd>
  <trd lng="es">vibrar</trd>
  <trd lng="fr">vibrer</trd>
  <trdgrp lng="hu">
    <trd>rezeg</trd>,
    <trd>vibr&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x632F;&#x52D5;&#12377;&#x308B;  <pr>&#12375;&#12435;&#12393;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x63FA;&#x308C;&#x308B;  <pr>&#12422;&#x308C;&#x308B;</pr></trd>,
    <trd>&#38663;&#x52D5;&#12377;&#x308B;  <pr>&#12375;&#12435;&#12393;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#38663;&#12360;&#x308B;  <pr>&#12405;&#x308B;&#12360;&#x308B;</pr></trd>,
    <trd>&#x9CF4;&#x308A;&#x97FF;&#x304F;  <pr>&#x306A;&#x308A;&#12402;&#12403;&#x304F;</pr></trd>,
    <trd>&#12358;&#12385;&#38663;&#12360;&#x308B;  <pr>&#12358;&#12385;&#12405;&#x308B;&#12360;&#x308B;</pr></trd>,
    <trd>&#x308F;&#x304F;&#x308F;&#x304F;&#12377;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">trillen</trd>
  <trdgrp lng="pl">
    <trd>wibrowa&cacute;</trd>,
    <trd>dr&zdot;e&cacute;</trd>,
    <trd>oscylowa&cacute;</trd>,
    <trd>rezonowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">vibrar</trd>
  <trdgrp lng="sk">
    <trd>chvie&tcaron; sa</trd>,
    <trd>kmita&tcaron;</trd>,
    <trd>vibrova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_b;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43A;&#x43E;&#x43B;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_t;&c_r;&c_je;&#x43C;&c_t;&c_ib;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x986B;&#x52D5; <pr>ch&#xE0;nd&#xF2;ng</pr></trd>,
    <trd>&#x98A4;&#x52A8; <pr>ch&#xE0;nd&#xF2;ng</pr></trd>,
    <trd>&#x632F;&#x52A8; <pr>zh&#xE8;nd&#xF2;ng</pr></trd>,
    <trd>&#x632F;&#x52D5; <pr>zh&#xE8;nd&#xF2;ng</pr></trd>,
    <trd>&#x632F;&#33633; <pr>zh&#xE8;nd&#xE0;ng</pr></trd>,
    <trd>&#x632F;&#34153; <pr>zh&#xE8;nd&#xE0;ng</pr></trd>,
    <trd>&#38663; <pr>zh&#xE8;n</pr></trd>,
    <trd>&#25238;&#x52A8; <pr>d&#x1D2;ud&#xF2;ng</pr></trd>,
    <trd>&#25238;&#x52D5; <pr>d&#x1D2;ud&#xF2;ng</pr></trd>,
    <trd>&#38663;&#x76EA; <pr>zh&#xE8;nd&#xE0;ng</pr></trd>,
    <trd>&#38663;&#33633; <pr>zh&#xE8;nd&#xE0;ng</pr></trd>,
    <trd>&#38663;&#33633; <pr>zh&#xE8;nd&#xE0;ng</pr></trd>,
    <trd>&#38663;&#34153; <pr>zh&#xE8;nd&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vibr.0o">
  <kap><tld/>o</kap>
  <snc mrk="vibr.0o.MOV">
    <dif>
      Iu el la alternaj movoj de io <tld/>anta:
      <ekz>
        radioaparatoj kreas, la&ubreve; sia grandeco, anka&ubreve; mem<tld/>adon, la tuboj <tld/>igas
        sin mem, kaj tiuj <tld/>oj, sen la necesa &scirc;irmado, influas anka&ubreve; la antenon,
        kiu tiamaniere kaptas groteskajn ondojn: la aparato fajfas, knaras, bruas
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> vidis <klr>[&#8230;]</klr> tremeton &ccirc;irka&ubreve; &scirc;iaj <klr>[&#8230;]</klr> lipoj,
        kaj rimarkis <tld/>eton en &scirc;ia vo&ccirc;o
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu ekscito estis la <tld/>o de la feli&ccirc;o
        <fnt>
          <bib>LGA</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="disona.0o">disonanco</ref>,
      <ref cel="interf.0o">interfero</ref>,
      <ref cel="frekve.0o.TEK">frekvenco<sncref/></ref>,
      <ref cel="trem.0o.MOV">tremo</ref>
    </refgrp>
  </snc>
  <rim>
    &Z; en unu verko uzis la vorton &leftquot;<esc>vibracio</esc>&rightquot;:
    <ekz>
      artritaj sentadoj estas &ccirc;iam akompanataj de malakordo de la me&hcirc;anikaj <esc>vibracioj</esc>
      <fnt>
        <bib>Rabistoj</bib>
        <lok>Akto Dua</lok>
      </fnt>.
    </ekz>
  </rim>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_y;&c_m;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_d;&c_r;&c_y;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_ib;&c_b;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">vibraci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>chv&ecaron;n&iacute; t&ecaron;les</trd>,
    <trd>kmit&aacute;n&iacute;</trd>,
    <trd>st&rcaron;&aacute;s&aacute;n&iacute;</trd>,
    <trd>vibrace</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schwingung</trd>,
    <trd>Vibration</trd>
  </trdgrp>
  <trd lng="es">vibraci&oacute;n</trd>
  <trd lng="fr">vibration</trd>
  <trd lng="hu">rezg&eacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#12402;&#12392;&#x63FA;&#x308C;  <pr>&#12402;&#12392;&#12422;&#x308C;</pr></trd>,
    <trd>&#38663;&#12360;  <pr>&#12405;&#x308B;&#12360;</pr></trd>
  </trdgrp>
  <trd lng="nl">trilling</trd>
  <trdgrp lng="pl">
    <trd>wibracja</trd>,
    <trd>dr&zdot;enie</trd>,
    <trd>drganie</trd>
  </trdgrp>
  <trd lng="pt">vibra&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_v;&c_i;&c_b;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>chvenie</trd>,
    <trd>kmitanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_b;&c_r;&c_a;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&#x43A;&#x43E;&#x43B;&c_i;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_t;&c_r;&c_je;&#x43C;&c_t;&c_ib;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x632F;&#33633; <pr>zh&#xE8;nd&#xE0;ng</pr></trd>,
    <trd>&#x632F;&#34153; <pr>zh&#xE8;nd&#xE0;ng</pr></trd>,
    <trd>&#38663;&#x76EA; <pr>zh&#xE8;nd&#xE0;ng</pr></trd>,
    <trd>&#38663;&#33633; <pr>zh&#xE8;nd&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vibr.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>adon de io:
      <ekz>
        la aero, eligata forte el la pulmoj tra la gloto <tld/>igas la vo&ccirc;ajn kordojn,
        tiam ili ritme ekmovi&gcirc;as kaj pro siaj longeco, lar&gcirc;eco kaj stre&ccirc;iteco &ndash;
        principe tute same kiel &ccirc;e la muzikaj instrumentoj &ndash; produktas diversajn
        sonojn
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        kvar elektraj motoroj trovi&gcirc;as sub la &scirc;ipo: rezulte, neniu stirilo kaj neniu
        helico&scirc;afto povas <tld/>igi kaj bruigi la &scirc;ipon
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="&Monato;/2004/008255.html">Re&gcirc;ino de la maroj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol; <ind>&c_d;&c_r;&c_y;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</ind></trd>,
    <trd>&c_u;&c_z;&c_b;&c_u;&c_d;&c_zh;&c_a;&c_c;&c_mol; &c_v;&c_ib;&c_b;&c_r;&c_a;&c_c;&c_y;&c_ju;</trd>
  </trdgrp>
  </snc>
  <snc mrk="vibr.0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Igi <ref cel="vibr.0i.FIG">vibri<sncref/></ref> la sentojn, tu&scirc;i emocie:
      <ekz>
        la infano alkuris al &scirc;i, <klr>[&#8230;]</klr> balbutante tiun &ccirc;arman vorton, kiu
        <tld/>igas la koron de patrinoj
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        varmega tagmeza suno brile lumigis la placon, la sekvantan tagon, kaj <tld/>igis
        <klr>(<ref cel="bril.trem0i">trembrili</ref>gis)</klr> la aeron super &gcirc;ia brika pavimo
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ricevis filinon kelkajn jarojn post la edzi&gcirc;o <klr>[sed]</klr> tio ne tre
        <tld/>igis <klr>(<ref cel="sxance.0i.PSI">&scirc;ancelis</ref>)</klr> liajn sentojn
        <fnt>
          <bib>HsT</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_u;&c_z;&c_r;&c_u;&c_sh;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">r&uuml;hren</trd>
  </snc>
  <trdgrp lng="de">
    <trd>zum <ind>Vibrieren</ind> bringen</trd>,
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x632F;&#x52D5;&#12373;&#x305B;&#x308B;  <pr>&#12375;&#12435;&#12393;&#12358;&#12373;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x63FA;&#12425;&#12377;  <pr>&#12422;&#12425;&#12377;</pr></trd>,
    <trd>&#38663;&#x52D5;&#12373;&#x305B;&#x308B;  <pr>&#12375;&#12435;&#12393;&#12358;&#12373;&#x305B;&#x308B;</pr></trd>,
    <trd>&#38663;&#x308F;&#x305B;&#x308B;  <pr>&#12405;&#x308B;&#x308F;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">wprawi&cacute; w  <ind>drgania</ind></trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43B;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#38663;&#x52A8; <pr>zh&#xE8;nd&#xF2;ng</pr></trd>,
    <trd>&#38663;&#x52D5; <pr>zh&#xE8;nd&#xF2;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vibr.post0i">
  <kap>post<tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Plu <tld/>i post la &ccirc;esigo de la impulso estiginta la
      <tld/>on:
      <ekz>
        liaj timpanoj &scirc;ajnis post<tld/>i <klr>[pro la
        alarmilo]</klr>
        <fnt><bib>UrdHadda</bib><lok>p. 5</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fine li ekdormas, kaj en liaj son&gcirc;oj post<tld/>as
        lernejaj bildoj, kiaj &jcirc;us okupis lian maldorman cerbon
        <fnt>
        <aut>N. Barthelmess</aut>,
        <vrk>Ne plu ludo</vrk>,
        <lok><url
        ref="http://esperanto.org/literaturo/geja/NPL.html">http://esperanto.org/literaturo/geja/NPL.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sonor.0i">sonori</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol; <ind>&c_v;&c_ib;&c_b;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</ind></trd>,
    <trd>&c_r;&c_e;&c_z;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">reverberar</trd>
  <trd lng="de">nachschwingen</trd>
  <trd lng="es">reverberar</trd>
  <trdgrp lng="hu">
    <trd>ut&oacute;rezeg</trd>,
    <trd>ut&oacute;rezg&eacute;st v&eacute;gez</trd>
  </trdgrp>
  <trd lng="pl">rezonowa&cacute;</trd>
</drv>
</art>
