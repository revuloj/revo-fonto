<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rostr</rad>/o
</kap>
<drv mrk="rostr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Tre longa tubforma nazo de elefanto kaj de kelkaj mambestoj:
      <ekz>
        <tld/>o <klr>(nazego)</klr> de porko<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_h;&c_o;&c_b;&c_a;&c_t;</trd>
    <trd lng="en">trunk</trd>
    <trd lng="hu">orm&aacute;ny</trd>
    <trd lng="pt">tromba</trd>
    <trd lng="ru">&c_h;&c_o;&c_b;&c_o;&c_t;</trd>
  </snc>
  <snc>
    <dif>
      Su&ccirc;ilo de diversaj insektoj.
    </dif>
    <trd lng="be">&c_h;&c_a;&c_b;&c_a;&c_t;&c_o;&c_k;</trd>
    <trdgrp lng="en">
      <trd>proboscis</trd>,
      <trd>sucker</trd>
    </trdgrp>
    <trd lng="ru">&c_h;&c_o;&c_b;&c_o;&c_t;&c_o;&c_k;</trd>
  </snc>
  <trd lng="de">R&uuml;ssel</trd>
  <trd lng="fr">trompe</trd>
  <trd lng="hu">sz&iacute;v&oacute;szerv</trd>
  <trd lng="tr">hortum</trd>
  <trd lng="nl">slurf</trd>
</drv>

<drv mrk="rostr.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Proboscidea</trd>).
    </dif>
    <ref tip="sub" cel="elefan2.0oj">elefantedoj</ref>
  </snc>
  <trd lng="de">R&uuml;sseltiere</trd>
  <trd lng="hu">orm&aacute;nyos <ind>eml&odblac;s&ouml;k</ind></trd>
  <trd lng="ru">&c_h;&c_o;&c_b;&c_o;&c_t;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="tr">hortumlular</trd>
</drv>
</art>
