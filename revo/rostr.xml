<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>rostr</rad>/o
</kap>
<drv mrk="rostr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc num="1">
    <dif>
      Tre longa tubforma nazo de elefanto k de kelkaj mambestoj:
      <ekz>
        <tld/>o <klr>(nazego)</klr> de porko<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Su&ccirc;ilo de diversaj insektoj.  
    </dif>
  </snc>
  <trd lng="de">R&uuml;ssel</trd>
  <trd lng="fr">trompe</trd>
  <trd lng="tr">hortum</trd>
</drv>
<drv mrk="rostr.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Proboscidea</trd>).
    </dif>
    <ref tip="sub" cel="elefan.0edoj">elefantedoj</ref>
  </snc>
  <trd lng="de">R&uuml;sseltiere</trd>
  <trd lng="tr">hortumlular</trd>
</drv>
</art>
