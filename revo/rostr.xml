<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rostr.xml,v 1.34 2024/10/06 18:34:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rostr</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="rostr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rostr.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Tre longa tubforma <ref tip="super" cel="naz.0o.ZOO">nazo</ref> de elefanto kaj de kelkaj
      mambestoj:
      <ekz>
        elefanto tremis de teruro kaj maltrankvile movadis la <tld/>on
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        perfekte artiste farita porko el kupro <klr>[&#8230;]</klr> pro maljuneco aspektas
        tute mallume verda, nur la <tld/>o brilas
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; 40 mil jaroj vivis la vira mamutido <nom>Dima</nom>, <klr>[&#8230;]</klr> konservi&gcirc;is &gcirc;iaj
        oreloj, <tld/>o, peniso kaj parto de la felo
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2015/011414.php"
              >Glaciaj mumioj sor&ccirc;as A&ubreve;strion</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_h;&c_o;&c_b;&c_a;&c_t;</trd>
    <trd lng="en">trunk</trd>
    <trdgrp lng="fr">
      <trd>groin</trd>,
      <trd>hure</trd>
    </trdgrp>
    <trd lng="hu">orm&aacute;ny</trd>
    <trd lng="id">belalai</trd>
    <trd lng="pl">tr&aogonek;ba</trd>
    <trd lng="pt">tromba</trd>
    <trd lng="ru">&c_h;&c_o;&c_b;&c_o;&c_t;</trd>
  </snc>
  <snc mrk="rostr.0o.ZOO_insekto">
    <dif>
      Su&ccirc;ilo de diversaj insektoj.
    </dif>
    <trd lng="be">&c_h;&c_a;&c_b;&c_a;&c_t;&c_o;&c_k;</trd>
    <trdgrp lng="en">
      <trd>proboscis</trd>,
      <trd>sucker</trd>
    </trdgrp>
    <trd lng="pl">tr&aogonek;bka</trd>
    <trd lng="ru">&c_h;&c_o;&c_b;&c_o;&c_t;&c_o;&c_k;</trd>
  </snc>
  <trd lng="ca">trompa</trd>
  <trd lng="cs">obrn&ecaron;n&aacute; p&rcaron;&iacute;&dcaron; voj.
  lodi</trd>
  <trd lng="de">R&uuml;ssel</trd>
  <trdgrp lng="es">
    <trd>prob&oacute;scide</trd>,
    <trd>trompa</trd>
  </trdgrp>
  <trd lng="fr">trompe</trd>
  <trd lng="hu">sz&iacute;v&oacute;szerv</trd>
  <trdgrp lng="ja">
    <trd>&#40763; <pr>&#12399;&#12394;</pr></trd>,
    <trd>&#21475;&#21563; <pr>&#12371;&#12358;&#12405;&#12435;</pr></trd>,
    <trd>&#33337;&#22068; <pr>&#12379;&#12435;&#12399;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">slurf</trd>
  <trd lng="sk">obrnen&aacute; predn&aacute; &ccaron;as&tcaron;
  vojenskej lode</trd>
  <trd lng="sv">snabel</trd>
  <trd lng="tr">hortum</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_o;&c_b;&c_o;&c_t;</trd>,
    <trd>&c_r;&c_i;&c_l;&c_mol;&c_c;&c_je;</trd>,
    <trd>&c_r;&c_o;&c_s;&c_t;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#23574;&#24418;&#22068; <pr>ji&amacron;nx&iacute;ngzu&#464;</pr></trd>,
    <trd>&#38263;&#40763; <pr>ch&aacute;ngb&iacute;</pr></trd>,
    <trd>&#38271;&#40763; <pr>ch&aacute;ngb&iacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rostr.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="rostr.0ulo.ZOO">
    <dif>
      Iu el <ref tip="malprt" cel="rostr.0uloj"><tld/>uloj</ref>:
      <ekz>
        li jam preska&ubreve; vidis <klr>(&#8230;)</klr> la pigrajn, grandajn <tld/>ulojn
        <fnt>
          <aut>M. Babits, trad. K. Kalocsay kaj P. A. Rados</aut>
          <vrk>La An&gcirc;elo</vrk>
          <lok><url
          ref="https://bibliotekoj.org/literaturamondo/PDF/1936/LM%201936-01c.pdf">Literatura Mondo,
          1936/01</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_o;&c_b;&c_a;&c_t;&c_a;&c_n;&c_o;&c_s;&c_a;&c_ja; &c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;</trd>
  <trd lng="es">probosc&iacute;deo</trd>
  <trd lng="pl">s&lstroke;o&nacute; np.</trd>
</drv>

<drv mrk="rostr.0uloj">
  <kap><tld/>uloj</kap>
  <snc mrk="rostr.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref>
      de <ref tip="malprt" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Proboscidea</trd>).
    </dif>
    <lstref lst="voko:rostruloj"><tld/>uloj</lstref>
    <ref tip="sub" cel="elefan2.0oj">elefantedoj</ref>
  </snc>
  <trd lng="be">&c_h;&c_o;&c_b;&c_a;&c_t;&c_a;&c_n;&c_o;&c_s;&c_y;&c_ja;</trd>
  <trd lng="ca">proboscidis</trd>
  <trd lng="de">R&uuml;sseltiere</trd>
  <trd lng="es">probosc&iacute;deos</trd>
  <trd lng="fr">Proboscidiens</trd>
  <trd lng="hu">orm&aacute;nyos <ind>eml&odblac;s&ouml;k</ind></trd>
  <trd lng="ja">&#38263;&#40763;&#39006;  <pr>&#12385;&#12423;&#12358;&#12403;&#12427;&#12356;</pr></trd>
  <trd lng="pl">tr&aogonek;bowce</trd>
  <trd lng="ru">&c_h;&c_o;&c_b;&c_o;&c_t;&c_n;&c_y;&c_je; <klr
  tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="tr">hortumlular</trd>
  <trd lng="uk">&c_h;&c_o;&c_b;&c_o;&c_t;&c_n;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#38263;&#40763;&#30446; <pr>ch&aacute;ngb&iacute;m&ugrave;</pr></trd>,
    <trd>&#38271;&#40763;&#30446; <pr>ch&aacute;ngb&iacute;m&ugrave;</pr></trd>
  </trdgrp>
</drv>
</art>
