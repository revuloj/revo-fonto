<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Bu&hcirc;or</rad>/o
    </kap>

    <uzo tip="fak">GEOG</uzo>

    <drv mrk="buhxor.B0o">
      <kap>
        <tld/>o
      </kap>
      <snc>
        <dif>
          Provinca &ccirc;efurbo en Uzbekujo; historie, grava centro
          budaismo, poste kaj precipe, granda kaj fama urbo de la
          islama mondo; iama &ccirc;efurbo de la regno Samanida, poste de
          <tld/>aj &hcirc;anujo kaj emirujo.
        </dif>
      </snc>
      <trdgrp lng="en">
        <trd>Bukhxara</trd>,
        <trd>Buxoro</trd>
      </trdgrp>
      <trd lng="ru">&c_B;&c_u;&c_h;&c_a;&c_r;&c_a;</trd>
    </drv>

  </art>

