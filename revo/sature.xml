<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sature</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sature.0o">
  <kap><tld/>o</kap>
  <snc mrk="sature.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Herbo kreskanta en sekaj, sunaj lokoj de la norda terduono
      kun du specoj uzataj kiel <ref tip="super" 
      cel="spic.0o.KUI">spico</ref>,
      genro el familio <ref tip="super" 
      cel="lamiac.0oj.BOT">lamiacoj</ref> (<trd 
      lng="la">Satureja</trd>):
      <ekz>
        bran&ccirc;o de <tld/>o
        <fnt>
          <aut>Jefo</aut>,
          <vrk><url ref="http://fagot.alain.free.fr/kuirado/Rpf01/html/CxS11.html">
          Bukedo de verda&jcirc;oj kun kuniklo</url></vrk>,
          <lok>en: Kelkaj receptoj, 2005-11</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Bohnenkraut</trd>
  <trd lng="en">savory</trd>
  <trd lng="fr">sarriette</trd>
  <trd lng="it">santoreggia</trd>
</drv>
</art>

