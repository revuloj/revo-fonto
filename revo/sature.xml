<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sature</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sature.0o">
  <kap><tld/>o</kap>
  <snc mrk="sature.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Aroma <ref tip="lst" cel="herb.0o"
      lst="voko:lamioparencoj">herbo</ref>
      a&ubreve; arbusto kreskanta en sekaj, sunaj lokoj de la norda terduono,
      kun mallar&gcirc;aj folioj kaj kun malgrandaj palaj dulipaj floroj,
      kies malsupra estas triloba, tiel similante kvarfolian korolon,
      la du longaj kaj du mallongaj fekundaj stamenoj apudas la supran
      lipon;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="lamiac.0oj.BOT">lamiacoj</ref> (<trd 
      lng="la">Satureja</trd>), el kiu
      du specioj uzataj kiel <ref tip="lst" 
      cel="spic.0o.KUI" lst="voko:spicoj">spico</ref>:
      <ekz>
        bran&ccirc;o de <tld/>o
        <fnt>
          <aut>Jefo</aut>,
          <vrk><url
          ref="http://fagot.alain.free.fr/kuirado/Rpf01/html/CxS11.html">
          Bukedo de verda&jcirc;oj kun kuniklo</url></vrk>,
          <lok>en: Kelkaj receptoj, 2005-11</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_G;&c_r;&c_a;&c_d;&c_i;&c_n;&c_s;&c_k;&c_a;&c_t;&c_a; &c_ch;&c_u;&c_b;&c_r;&c_i;&c_c;&c_a;</trd>
  <trd lng="de">Bohnenkraut</trd>
  <trd lng="en">savory</trd>
  <trd lng="es">ajedrea de jard&iacute;n</trd>
  <trd lng="fr">sarriette</trd>
  <trd lng="hu">borsikaf&udblac;</trd>
  <trd lng="it">santoreggia</trd>
  <trd lng="no">sar</trd>
  <trd lng="sv">sommarkyndel</trd>
</drv>
</art>

