<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sature.xml,v 1.14 2024/03/24 20:00:00 revo Exp $">
<kap>
  <rad>sature</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sature.0o">
  <kap><tld/>o</kap>
  <snc mrk="sature.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Aroma <ref tip="lst" cel="herb.0o" lst="voko:lamioparencoj">herbo</ref>
      a&ubreve; arbusto kreskanta en sekaj, sunaj lokoj de la norda terduono,
      kun mallar&gcirc;aj folioj kaj kun malgrandaj palaj dulipaj floroj,
      kies malsupra estas triloba, tiel similante kvarfolian korolon,
      la du longaj kaj du mallongaj fekundaj stamenoj apudas la supran
      lipon;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="lamiac.0oj.BOT">lamiacoj</ref> (<trd lng="la">Satureja</trd>), el kiu
      du specioj uzataj kiel <ref tip="lst" cel="spic.0o.KUI" lst="voko:spicoj">spico</ref>:
      <ekz>
        bran&ccirc;o de <tld/>o
        <fnt>
          <aut>Jefo</aut>,
          <vrk><url ref="http://fagot.alain.free.fr/kuirado/Rpf01/html/CxS11.html">
          Bukedo de verda&jcirc;oj kun kuniklo</url></vrk>,
          <lok>en: Kelkaj receptoj, 2005-11</lok>
        </fnt>
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/64/Satureja_hortensis_Prague_2011_3.jpg/485px-Satureja_hortensis_Prague_2011_3.jpg" prm="CC BY-SA 3.0">somera <tld/>o (Satureja hortensis), Botanika &Gcirc;ardeno de Karola Universitato, Prago
        <fnt>
          <aut>Karelj, propra verko</aut>
          <lok><url ref="&WCW;/File:Satureja_hortensis_Prague_2011_3.jpg">&WCW;/File:Satureja_hortensis_Prague_2011_3.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_t;&c_u;&c_r;&c_e;&c_ja;</trd>
  <trd lng="bg">&c_g;&c_r;&c_a;&c_d;&c_i;&c_n;&c_s;&c_k;&c_a;&c_t;&c_a; <ind>&c_ch;&c_u;&c_b;&c_r;&c_i;&c_c;&c_a;</ind></trd>
  <trd lng="cs">saturejka</trd>
  <trd lng="de">Bohnenkraut</trd>
  <trd lng="en">savory</trd>
  <trd lng="es">ajedrea de jard&iacute;n</trd>
  <trd lng="fa">&a_m;&a_r;&a_z;&a_h;</trd>
  <trd lng="fr">sarriette</trd>
  <trd lng="hu">borsikaf&udblac;</trd>
  <trd lng="it">santoreggia</trd>
  <trd lng="mk">&c_ch;&c_u;&c_b;&c_r;&c_i;&c_k;&c_a;</trd>
  <trd lng="nl">bonenkruid</trd>
  <trd lng="no">sar</trd>
  <trd lng="pl">cz&aogonek;ber ogrodowy</trd>
  <trd lng="ru">&c_ch;&c_a;&c_b;&c_jo;&c_r;</trd>
  <trd lng="sk">saturejka</trd>
  <trd lng="sv">sommarkyndel</trd>
  <trd lng="uk">&c_ch;&c_a;&c_b;&c_jo;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x53EF;&#x53E3;&#x98EF;&#x83DC; <pr>k&#x11B;k&#x1D2;uf&#xE0;nc&#xE0;i</pr></trd>,
    <trd>&#x53EF;&#x53E3;&#x996D;&#x83DC; <pr>k&#x11B;k&#x1D2;uf&#xE0;nc&#xE0;i</pr></trd>,
    <trd>&#x5473;&#x9999;&#x597D;&#x5403; <pr>w&#xE8;ixi&#x101;ngh&#x1CE;och&#x12B;</pr></trd>,
    <trd>&#x5F00;&#x80C3;&#x7684;&#x83DC;&#x80B4; <pr>k&#x101;iw&#xE8;idec&#xE0;iy&#xE1;o</pr></trd>,
    <trd>&#x958B;&#x80C3;&#x7684;&#x83DC;&#x80B4; <pr>k&#x101;iw&#xE8;idec&#xE0;iy&#xE1;o</pr></trd>,
    <trd>&#x98A8;&#x5473;&#x6975;&#x4F73;&#x7684;&#x83DC;&#x80B4; <pr>f&#x113;ngw&#xE8;ij&#xED;ji&#x101;dec&#xE0;iy&#xE1;o</pr></trd>,
    <trd>&#x98CE;&#x5473;&#x6781;&#x4F73;&#x7684;&#x83DC;&#x80B4; <pr>f&#x113;ngw&#xE8;ij&#xED;ji&#x101;dec&#xE0;iy&#xE1;o</pr></trd>
  </trdgrp>
</drv>
