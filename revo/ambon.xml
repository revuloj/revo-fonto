<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ambon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ambon.0o">
  <kap><tld/>o</kap>
  <snc mrk="ambon.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">REL</uzo>
    <dif>
      Alta <ref tip="malprt" cel="pregx.0ejo">pre&gcirc;eja</ref> <ref tip="super"
      cel="tribun.0o">tribuno</ref>, kelkfoje lo&gcirc;ia, de kiu oni vo&ccirc;legas
      a&ubreve; predikas:
      <ekz>
        sur la <tld/>o estas aran&gcirc;ita estrado
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ambon</trd>,
    <trd>chaire<klr> (d'&eacute;glise)</klr></trd>,
    <trd>minbar</trd>
  </trdgrp>
</drv>
</art>

