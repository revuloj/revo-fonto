<drv mrk="ide.sam0ano">
  <kap>sam<tld/>ano</kap>
  <snc mrk="ide.sam0ano.gxenerale">
    <dif>
      Tiu, kiu havas komunan <tld/>on, komunan celon kun aliaj.
    </dif>
    <trdgrp lng="pt">
      <trd>co-idealista</trd>,
      <trd>correligion&aacute;rio</trd>
    </trdgrp>
  </snc>
  <snc mrk="ide.sam0ano.ESP">
    <uzo tip="fak">ESP</uzo>
    <dif>
      Afabla &gcirc;entiltitolo uzata inter esperantistoj, konsiderataj
      kiel kunlaborantoj pro la Interna <tld lit="I"/>o:
      <ekz>
        la esperantistoj sin salutas ordinare per la vorto
        &leftquot;sam<tld/>ano&rightquot;;
      </ekz>
      <ekz>
        estimata sam<tld/>ano <klr>(formulo komence de
        letero)</klr>;
      </ekz>
      <ekz>
        sam<tld/>ane via <klr>(formulo fine de letero)</klr>.
      </ekz>
    </dif>
    <trd lng="it">samideano</trd>
    <trd lng="pt">samideano</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_n;&c_a;&c_d;&c_u;&c_m;&c_je;&c_c;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a;&c_d;&c_u;&c_m;&c_c;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a;&c_m;&c_y;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>samide&aacute;n</trd>,
    <trd>&ccaron;l. stejn&eacute; my&scaron;lenky</trd>
  </trdgrp>
  <trd lng="de">Gleichgesinnter</trd>
  <trd lng="fr">partisan<klr tip="ind"> (d'un mouvement d'id&eacute;es, de
  l'esp&eacute;ranto)</klr></trd>
  <trd lng="hu">eszmet&aacute;rs</trd>
  <trd lng="pl">wsp&oacute;&lstroke;ideowiec
  <klr> (tytu&lstroke; u&zdot;ywany mi&eogonek;dzy esperantystami)
  </klr></trd>
  <trd lng="ru">&c_je;&c_d;&c_i;&c_n;&c_o;&c_m;&c_y;&c_sh;&c_l;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">samide&aacute;n <klr>(st&uacute;penec tej istej my&scaron;lienky)</klr></trd>
  <trd lng="uk">&#x43E;&c_d;&#x43D;&#x43E;&c_d;&c_u;&#x43C;&c_je;&c_c;&#x44C;</trd>
</drv>

<drv mrk="ide.fiksa0o">
  <kap>fiksa <tld/>o</kap>
  <snc mrk="ide.fiksa0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="vid" cel="obsed.0o.PSI">Obsedo</ref>,
      penso, kiu konstante revenas al la animo:
      <ekz>
        vi havas fiksan <tld/>on, revon, kiu tute plenigas vin
        <fnt>
          <aut>H. Conscience, trad. M. Posenaer</aut>,
          <vrk><url ref="http://eo.wikisource.org/wiki/Rikke-tikke-tak/II">
          Rikke-Tikke-Tak</url></vrk>,
          <lok>1912</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_d;&c_e;&c_f;&c_ib;&c_k;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>fixn&iacute; idea</trd>,
    <trd>id&eacute;e fixe</trd>,
    <trd>utkv&ecaron;l&aacute; my&scaron;lenka</trd>
  </trdgrp>
  <trd lng="fr">id&eacute;e fixe</trd>
  <trd lng="sk">fixn&aacute; idea</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_d;&c_je;&#x44F; &c_f;&c_ib;&#x43A;&c_s;</trd>,
    <trd>&#x43D;&c_a;&c_s;&c_t;&c_i;&c_r;&#x43B;&c_i;&c_v;&c_a; &c_ib;&c_d;&c_je;&#x44F;</trd>
  </trdgrp>
</drv>
<adm>
