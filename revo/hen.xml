<art mrk="$Id: hen.xml,v 1.26 2025/07/21 08:00:21 revo Exp $">
<kap>
  <rad>hen</rad>/i
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="hen.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="hen.0i.bleki">
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleki</ref> kiel
      <ref cel="cxeval.0o">&ccirc;evalo</ref>:
      <ekz>
        &Ccirc;e pordego <tld/>as &#11005; &Ccirc;evaloj, kaj &ccirc;asistoj bruas
        <fnt>
          <bib>SrT</bib>
          <lok>Libro V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ante terurate la besto falis en la abismon, kuntirante la
        kale&scirc;on
        <fnt><bib>DOz</bib>, <lok>&leftquot;la tertremo&rightquot;</lok></fnt>;
      </ekz>
      <ekz>
        kiam &scirc;i a&ubreve;dis la <tld/>on, <nom>Veronika</nom> tuj haltis kaj rigardis malanta&ubreve;en,
        unu rigardo sufi&ccirc;is por ke &scirc;i <klr>[&#8230;]</klr> pali&gcirc;u al mort-aspekta blanko
        <fnt>
          <bib>ChV</bib>
          <lok>18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru (&ccirc;evale) <ke>bleki</ke>.
  </rim>

  <trd lng="be">&c_ib;&c_r;&c_zh;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>renillar</trd>,
    <trd>a&iuml;nar</trd>,
    <trd>assa&iuml;nar</trd>,
    <trd>arniar</trd>,
    <trd>eguinar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>r&zcaron;&aacute;t</trd>,
    <trd>&rcaron;ehtat</trd>
  </trdgrp>
  <trd lng="de">wiehern</trd>
  <trdgrp lng="en">
    <trd>neigh</trd>,
    <trd>whinny</trd>
  </trdgrp>
  <trd lng="es">relinchar</trd>
  <trd lng="fr">hennir</trd>
  <trd lng="hu">nyer&iacute;t</trd>
  <trd lng="it">nitrire</trd>
  <trdgrp lng="ja">
    <trd>&#12356;&#12394;&#12394;&#12367;</trd>,
    <trd>&#12498;&#12498;&#12531;&#12392;&#12394;&#12367;</trd>
  </trdgrp>
  <trd lng="nl">hinniken</trd>
  <trd lng="pl">r&zdot;e&cacute;</trd>
  <trdgrp lng="pt">
    <trd>rinchar</trd>,
    <trd>relinchar</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_zh;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">erd&zcaron;a&tcaron;</trd>
  <trd lng="sv">gn&auml;gga</trd>
  <trd lng="uk">&c_ib;&c_r;&c_zh;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#22070;&#40180; <pr>s&imacron;m&iacute;ng</pr></trd>,
    <trd>&#20687;&#39340;&#19968;&#27171;&#22070;&#21483; <pr>xi&agrave;ngm&#462;y&iacute;y&agrave;ngs&imacron;ji&agrave;o</pr></trd>,
    <trd>&#22070;&#21483; <pr>s&imacron;ji&agrave;o</pr></trd>
  </trdgrp>
</drv>
</art>
