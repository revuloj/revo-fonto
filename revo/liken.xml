<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>liken</rad>/o
</kap>
<drv mrk="liken.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="liken.0o.BOT">
    <dif>
      Iu el grupo de vegeta&jcirc;oj (<trd lng="la">Lichenes</trd>)
      formitaj per simbiozo de fungo kaj algo; kreskantaj sur muroj, arboj,
      &scirc;tonegoj.
    </dif>
    <trd lng="el">&lambda;&epsilon;&jota;&chi;&eta_ton;&ny;&alfa;</trd>
    <trd lng="en">lichen</trd>
    <trd lng="hu">zuzm&oacute;</trd>
    <trd lng="nl">korstmos</trd>
    <trd lng="ru">&c_l;&c_i;&c_sh;&c_a;&c_j;&c_n;&c_i;&c_k;
<klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </snc>
  <snc mrk="liken.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Ha&ubreve;ta malsano karakterizata de ha&ubreve;tdifektoj
      <tld/>&scirc;ajnaj.
    </dif>
    <trd lng="hu">s&ouml;m&ouml;r</trd>
    <trd lng="ru">&c_l;&c_i;&c_sh;&c_a;&c_j;
<klr>(&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;)</klr></trd>
  </snc>
  <trd lng="de">Flechte</trd>
  <trd lng="pt">l&iacute;quen</trd>
</drv>
</art>
