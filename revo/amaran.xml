<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: amaran.xml,v 1.19 2021/02/28 16:09:06 revo Exp $">
<kap>
  <rad>amarant</rad>/o
  <fnt><bib>Fab1</bib></fnt>
</kap>
<drv mrk="amaran.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <tld/>acoj (<trd lng="la">Amarantus</trd>),
      kies floroj estas malhele karminru&gcirc;aj:
      <ekz>
        venis multaj aliaj floroj <klr>[&#x2026;]</klr> la bluaj violoj kaj la ru&gcirc;aj <tld/>oj,
        la lekantoj kaj la konvaloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estis unu el la &ccirc;efaj man&gcirc;a&jcirc;oj de la inkaoj, kaj &gcirc;i ankora&ubreve; estas uzata
        en la regiono hodia&ubreve;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28657">Amaranto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fa/Cadolzburg_Burggarten_-_Amaranth.jpg/576px-Cadolzburg_Burggarten_-_Amaranth.jpg" prm="CC BY-SA 4.0"><tld/>o &leftquot;Algeria&rightquot; en la &gcirc;ardeno de Cadolzburg, Germanujo
        <fnt>
          <aut>Wolfgang Sauber, propra verko</aut>
          <lok><url ref="&WCW;/File:Cadolzburg_Burggarten_-_Amaranth.jpg">https://commons.wikimedia.org/wiki/File:Cadolzburg_Burggarten_-_Amaranth.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_a;&c_k;&c_s;&c_a;&c_m;&c_ib;&c_t;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_shch;&c_i;&c_r;</trd>,
    <trd>&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>amarant</trd>,
    <trd>laskavec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Amarant</trd>, <trd>Fuchsschwanz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amaranth</trd>,
    <trd>love-lies-bleeding</trd>,
    <trd>Prince's feather</trd>,
    <trd>pigweed</trd>
  </trdgrp>
  <trd lng="es">amaranto</trd>
  <trdgrp lng="fr">
    <trd>amarante</trd>,
    <trd>amaranthe</trd>
  </trdgrp>
  <trd lng="he">&#x5D9;&#x5B7;&#x5E8;&#x5D1;&#x5BC;&#x5D5;&#x5BC;&#x5D6;</trd>
  <trdgrp lng="hu">
    <trd>diszn&oacute;par&eacute;j</trd>,
    <trd>amar&aacute;nt</trd>
  </trdgrp>
  <trd lng="nl">amarant</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_a;&c_k;&c_s;&c_a;&c_m;&c_i;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_shch;&c_i;&c_r;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_s;&c_v;&c_je;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">l&aacute;skavec</trd>
</drv>
</art>
