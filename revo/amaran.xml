<vortaro>
<art mrk="$Id$">
<kap>
<rad>amarant</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="amaran.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Genro el familio <tld/>acoj (<trd lng="la">Amarantus</trd>),
       kies floroj estas malhele karminru&gcirc;aj.
     </dif>
   </snc>
   <trdgrp lng="de"><trd>Amarant</trd>, <trd>Fuchsschwanz</trd></trdgrp>
   <trdgrp lng="en">
     <trd>amaranth</trd>,
     <trd>love-lies-bleeding</trd>,
     <trd>Prince's feather</trd>,
     <trd>pigweed</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>amarante</trd>,
     <trd>amaranthe</trd></trdgrp>
   <trd lng="nl">amarant</trd>
   <trdgrp lng="ru">
     <trd>&#1072;&#1084;&#1072;&#1088;&#1072;&#1085;&#1090;</trd>,
     <trd>&#1072;&#1082;&#1089;&#1072;&#1084;&#1080;&#1090;&#1085;&#1080;&#1082;</trd>,
     <trd>&#1097;&#1080;&#1088;&#1080;&#1094;&#1072;</trd>,
     <trd>&#1087;&#1086;&#1076;&#1089;&#1074;&#1077;&#1082;&#1086;&#1083;&#1100;&#1085;&#1080;&#1082;</trd>
   </trdgrp>
</drv>
</art>
