<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: amaran.xml,v 1.24 2023/08/30 18:23:33 revo Exp $">
<kap>
  <rad>amarant</rad>/o
  <fnt><bib>Fab1</bib></fnt>
</kap>
<drv mrk="amaran.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <tld/>acoj (<trd lng="la">Amarantus</trd>),
      kies floroj estas malhele karminru&gcirc;aj:
      <ekz>
        venis multaj aliaj floroj <klr>[&#x2026;]</klr> la bluaj violoj kaj la ru&gcirc;aj <tld/>oj,
        la lekantoj kaj la konvaloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        li rigardis <klr>[&#x2026;]</klr> la ru&gcirc;ajn florojn de l&#x2019; <tld/>o, kies etajn semojn,
        kiuj estas nek greneroj nek legomoj, &ccirc;iu Hindo povas senpeke man&gcirc;i dum la
        fasto
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estis unu el la &ccirc;efaj man&gcirc;a&jcirc;oj de la inkaoj, kaj &gcirc;i ankora&ubreve; estas uzata
        en la regiono hodia&ubreve;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28657">Amaranto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fa/Cadolzburg_Burggarten_-_Amaranth.jpg/576px-Cadolzburg_Burggarten_-_Amaranth.jpg" prm="CC BY-SA 4.0"><tld/>o &leftquot;Algeria&rightquot; en la &gcirc;ardeno de Cadolzburg, Germanujo
        <fnt>
          <aut>Wolfgang Sauber, propra verko</aut>
          <lok><url ref="&WCW;/File:Cadolzburg_Burggarten_-_Amaranth.jpg">&WCW;/File:Cadolzburg_Burggarten_-_Amaranth.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_a;&c_k;&c_s;&c_a;&c_m;&c_ib;&c_t;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_shch;&c_i;&c_r;</trd>,
    <trd>&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>amarant</trd>,
    <trd>laskavec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Amarant</trd>, <trd>Fuchsschwanz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amaranth</trd>,
    <trd>love-lies-bleeding</trd>,
    <trd>Prince's feather</trd>,
    <trd>pigweed</trd>
  </trdgrp>
  <trd lng="es">amaranto</trd>
  <trdgrp lng="fr">
    <trd>amarante</trd>,
    <trd>amaranthe</trd>
  </trdgrp>
  <trd lng="he">&yod;&#x5B7;&resh;&bet;&#x5BC;&vav;&#x5BC;&zayin;</trd>
  <trdgrp lng="hu">
    <trd>diszn&oacute;par&eacute;j</trd>,
    <trd>amar&aacute;nt</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30DE;&#x30E9;&#x30F3;&#x30B5;&#x30B9;</trd>,
    <trd>&#x30CF;&#x30B2;&#x30A4;&#x30C8;&#x30A6;</trd>,
    <trd>&#x30D2;&#x30E6;</trd>
  </trdgrp>
  <trd lng="nl">amarant</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_m;&c_a;&c_r;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_a;&c_k;&c_s;&c_a;&c_m;&c_i;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_shch;&c_i;&c_r;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_s;&c_v;&c_je;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">l&aacute;skavec</trd>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_i;&c_r;&c_i;&c_c;&#x44F;</trd>,
    <trd>&c_a;&#x43C;&c_a;&c_r;&c_a;&#x43D;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="amaran.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Malhele <ref tip="super" cel="rugx.0a">ru&gcirc;a</ref> kiel la floroj de <tld/>o:
      <ekz>
        anta&ubreve; la kameno staris kanapeto, tegita per <tld/>a damasko
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_jo;&c_m;&c_n;&c_a;-&c_m;&c_a;&c_l;&c_ib;&c_n;&c_a;&c_v;&c_y; <klr>(&c_k;&c_o;&c_l;&c_je;&c_r;)</klr></trd>
  <trd lng="de">amarant</trd>
</drv>


</art>
