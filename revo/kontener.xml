<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kontener.xml,v 1.15 2023/12/03 09:58:57 revo Exp $">
<kap>
  <rad>kontener</rad>/o  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kontener.0o">
  <kap><tld/>o</kap>
  <fnt><bib>TK</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc mrk="kontener.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Normita <ref tip="super" cel="kest.0o">kestego</ref> por transporto de varoj,
      sen apogaj gamboj,  kiu povas esti staplata:
      <ekz>
        la preferata <tld/>o havas alton de 2,44&#x202F;m, longon de 12,20&#x202F;m
        kaj kapaciton de 2 TEU;
      </ekz>
      <ekz>
        la Roterdama <tld/>-haveno
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;6</lok>
        </fnt>;
      </ekz>
      <ekz>
        kompanio <klr>[&#x2026;]</klr> ricevis permeson de la japana registaro eksporti 2700
        tunojn da uzita papero en 122 konteneroj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006893.html">Japanio: Rubo-ruzo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tendo kaj kontenero provizore servas kiel ambasado
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007216.html">Universitatoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la eksploda&jcirc;oj estis konservataj en 98 konteneroj stakigitaj unu sur la alia
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2011/010459.php">Eksplodas popola kolero</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_e;&c_j;&c_n;&c_je;&c_r;</trd>
  <trd lng="bg">&c_k;&c_o;&c_n;&c_t;&c_je;&c_j;&c_n;&c_je;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>kontejner</trd>,
    <trd>p&rcaron;epravn&iacute; sk&rcaron;&iacute;&ncaron;</trd>,
    <trd>p&rcaron;epravn&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Container</trd>
  <trd lng="en">container</trd>
  <trd lng="es">contenedor</trd>
  <trd lng="fr">conteneur</trd>
  <trd lng="nl">container</trd>
  <trd lng="pl">kontener</trd>
  <trdgrp lng="pt">
      <trd>cont&ecirc;iner</trd>,
      <trd>container</trd>,
      <trd>cofre de carga</trd>
  </trdgrp>
  <trd lng="ro">container</trd>
  <trd lng="sk">kontajner</trd>
  <trdgrp lng="zh">
    <trd>&#x76E6; <pr>&#x101;n</pr></trd>,
    <trd>&#x96C6;&#x88C5;&#x7BB1; <pr>j&#xED;zhu&#x101;ngxi&#x101;ng</pr></trd>,
    <trd>&#x8CA8;&#x6AC3; <pr>hu&#xF2;gu&#xEC;</pr></trd>
  </trdgrp>
</drv>
  </art>

