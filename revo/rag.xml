<art mrk="$Id$">
<kap>
  <rad>rag</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="rag.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Hind-muzika <ref tip="super" cel="tonal.0o">tonalo</ref>
      kun propraj skalo, baza tono, &ccirc;ef-tonoj, 
      ornamaro, kaj humora nuanco konvena por iu parto de diurno.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>raga</trd>
  </trdgrp>
  <trd lng="hu">raga</trd>
</drv>
</art>

