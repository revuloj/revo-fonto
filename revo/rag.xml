<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rag.xml,v 1.12 2023/12/08 21:24:22 revo Exp $">
<kap>
  <rad>rag</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="rag.0o">
  <kap><tld/>o</kap>
  <snc mrk="rag.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Hind-muzika <ref tip="super" cel="tonal.0o">tonalo</ref>
      kun propraj skalo, baza tono, melodiaj linioj,
      ornamaro, kaj humora nuanco konvena por iu parto de diurno:
      <ekz>
        la &leftquot;<tld/>o&rightquot; baza modelo melodia en hinda muziko
        <fnt>
        <aut>E. Smailus</aut>,
        <vrk>la &leftquot;rago&rightquot;...</vrk>,
        <lok>Scienca Revuo, 2001, numero 3a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_g;&c_a; <klr tip="amb">(&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;)</klr></trd>
  <trdgrp lng="en">
    <trd>raga</trd>,
    <trd>rag</trd>, <trd>ragam</trd>
  </trdgrp>
  <trd lng="cs">raga</trd>
  <trdgrp lng="de">
    <trd>Raga</trd>,
    <trd>Rag</trd>
  </trdgrp>
  <trd lng="fr">raga</trd>
  <trd lng="hu">raga</trd>
  <trd lng="nl">raga</trd>
  <trd lng="pl">raga  <klr>(muz.)</klr></trd>
  <trd lng="pt">raga</trd>
  <trd lng="sk">raga</trd>
  <trd lng="uk">&c_r;&c_a;&c_g;&c_a;</trd>
</drv>
</art>
