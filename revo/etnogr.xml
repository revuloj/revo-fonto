<vortaro>
  <art mrk="$Id$">
    <kap>
<ofc>2</ofc>
      <rad>etnografi</rad>/o
    </kap>
    <uzo tip="fak">SCI</uzo>
    <drv mrk="etnogr.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Priskriba scienco pri konkretaj
          <ref tip="prt" cel="etn.0o">etnoj</ref>,
          iliaj moroj, vivmaniero,
          <ref tip="prt" cel="folklo.0o">folkloro</ref> ktp;
          priskriba <ref tip="super" cel="etnolo1.0o">etnologio</ref>.
        </dif>
      </snc>
      <trd lng="en">ethnography</trd>
      <trd lng="ru">&c_e;&c_t;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
    </drv>
  </art>

