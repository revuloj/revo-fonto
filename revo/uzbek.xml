<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>uzbek</rad>/o
    </kap>

    <drv mrk="uzbek.U0o">
      <kap><tld lit="U"/>o</kap>
      <snc>
	<uzo tip="fak">PERS</uzo>
	<dif>
	  Tjurka nomo, interalie de la &hcirc;ano (m. 1342) kiu enkondukis islamon en
	  la Ora Hordo.
	</dif>
      </snc>
      <trdgrp lng="ru">
         <trd>&c_U;&c_z;&c_b;&c_je;&c_k;</trd>,
         <trd>&c_O;&c_z;&c_b;&c_ja;&c_k;</trd>
      </trdgrp>
    </drv>

    <drv mrk="uzbek.0o">
      <kap><tld/>o</kap>
      <snc>
	<dif>
	  Ano de <ref tip="super" cel="tjurk.0o">tjurka</ref> popolo,
	  la plej multenombra en <tld lit="U"/>ujo.
	</dif>
      </snc>
      <trd lng="en">Uzbek</trd>
      <trd lng="ru">&c_u;&c_z;&c_b;&c_je;&c_k;</trd>
    </drv>

    <drv mrk="uzbek.0ujo">
      <kap><tld lit="U"/>ujo</kap>
      <snc>
	<uzo tip="fak">GEOG</uzo>
	<uzo tip="fak">POL</uzo>
	<dif>
	  &Scirc;tato en la
          <ref tip="malprt" cel="azi.InternaA0o">Interna Azio</ref>,
          en la baseno de <ref cel="aral.A0o">Aralo</ref>,
          inter kaj &ccirc;irka&ubreve; la riveroj
          <ref cel="sirdarj.0o">Sirdarjo</ref> kaj
          <ref cel="amudarj.0o">Amudarjo</ref>.
	</dif>
      </snc>
      <trd lng="en">Uzbekistan</trd>
      <trd lng="ru">&c_U;&c_z;&c_b;&c_je;&c_k;&c_i;&c_s;&c_t;&c_a;&c_n;</trd>
    </drv>

  </art>

  <!--
