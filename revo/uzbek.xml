<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap>
       <rad>uzbek</rad>/o
     </kap>

     <drv mrk="uzbek.U0o">
       <kap><tld lit="U"/>o</kap>
       <snc>
         <uzo tip="fak">PERS</uzo>
         <dif>
           Tjurka nomo, interalie de la &hcirc;ano (m. 1342) kiu
           enkondukis islamon en
           la Ora Hordo.
         </dif>
       </snc>
       <trd lng="be">&c_U;&c_z;&c_mol;&c_b;&c_je;&c_k;</trd>
       <trd lng="de">Usbek</trd>
       <trd lng="fi">Uzbek(-kaani)</trd>
       <trd lng="hu">&Ouml;zbek</trd>
       <trd lng="it">Uzbek<klr> Khan</klr></trd>
       <trdgrp lng="pt">
          <trd>Uzbeque</trd>
       </trdgrp>
       <trdgrp lng="ru">
          <trd>&c_U;&c_z;&c_b;&c_je;&c_k;</trd>,
          <trd>&c_O;&c_z;&c_b;&c_ja;&c_k;</trd>
       </trdgrp>
     </drv>

     <drv mrk="uzbek.0o">
       <kap><tld/>o</kap>
       <snc>
       <dif>
         Ano de <ref tip="super" cel="tjurk.0o">tjurka</ref> popolo,
         la plej multenombra en <tld lit="U"/>ujo.
       </dif>
       </snc>
       <trd lng="be">&c_u;&c_z;&c_mol;&c_b;&c_je;&c_k;</trd>
       <trd lng="de">Usbeke</trd>
       <trd lng="en">Uzbek</trd>
       <trd lng="fi">uzbekki</trd>
       <trd lng="fr">Ouzbek<klr> (subst.)</klr></trd>
       <trd lng="hu">&uuml;zb&eacute;g</trd>
       <trd lng="id">Uzbek</trd>
       <trdgrp lng="it">
       <trd>usbeco<klr> (sost.)</klr></trd>, 
       <trd>uzbeko<klr> (sost.)</klr></trd>
       </trdgrp>
       <trd lng="pl">Uzbek</trd>
       <trd lng="pt">uzbeque <klr tip="ind"> (subst.)</klr></trd>
       <trd lng="ru">&c_u;&c_z;&c_b;&c_je;&c_k;</trd>
     </drv>
     
     <drv mrk="uzbek.0a">
       <kap><tld/>a</kap>
       <snc>
         <dif>
           Rilatanta al <tld lit="U"/>oj, ilia nacieco a&ubreve; &scirc;tato.
         </dif>
       </snc>
       <trd lng="fr">ouzbek<klr> (adj.)</klr></trd>
       <trd lng="pt">uzbeque <klr tip="ind"> (adj.)</klr></trd>
     </drv>


     <drv mrk="uzbek.U0ujo">
       <kap><tld lit="U"/>ujo, <var><kap><tld
       lit="U"/>io</kap></var></kap>
       <snc>
        <uzo tip="fak">GEOG</uzo>
        <uzo tip="fak">POL</uzo>
        <dif>
          &Scirc;tato en la
           <ref tip="malprt" cel="azi.InternaA0o">Interna Azio</ref>,
           en la baseno de <ref cel="aral.A0o">Aralo</ref>,
           inter kaj &ccirc;irka&ubreve; la riveroj
           <ref cel="sirdarj.S0o">Sirdarjo</ref> kaj
           <ref cel="amudarj.A0o">Amudarjo</ref>.
        </dif>
       </snc>
       <trd lng="be">&c_U;&c_z;&c_mol;&c_b;&c_je;&c_k;&c_ib;&c_s;&c_t;&c_a;&c_n;</trd>
       <trd lng="bg">&c_U;&c_z;&c_b;&c_je;&c_k;&c_i;&c_s;&c_t;&c_a;&c_n;</trd>
       <trd lng="de">Usbekistan</trd>
       <trd lng="en">Uzbekistan</trd>
       <trd lng="fi">Uzbekistan</trd>
       <trd lng="fr">Ouzb&eacute;kistan</trd>
       <trd lng="hu">&Uuml;zbegiszt&aacute;n</trd>
       <trd lng="id">Uzbekistan</trd>
       <trd lng="it">Uzbekistan</trd>
       <trd lng="pl">Uzbekistan</trd>
       <trd lng="pt">Uzbequist&atilde;o</trd>
       <trd lng="ru">&c_U;&c_z;&c_b;&c_je;&c_k;&c_i;&c_s;&c_t;&c_a;&c_n;</trd>
     </drv>

   </art>

