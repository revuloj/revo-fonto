<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
  <art mrk="$Id$">
    <kap><rad>&hcirc;asid</rad>/o</kap>
 
    <drv mrk="hxasid.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">REL</uzo>
      <snc mrk="hxasid.0o.HIST">
 <uzo tip="fak">HIST</uzo>
 <dif>
          En la antikva historio, ano de judisma sekto kiu kun la
          Makabeoj batalis kontra&ubreve; la sirianoj regataj de Antio&hcirc;o
          Epifano.
 </dif>
      </snc>
      <snc mrk="hxasid.0o.ismano">
 <dif>
          <tld lit="&Hcirc;"/>ismano, ano de <tld/>ismo.
 </dif>
      </snc>
      <trdgrp lng="en">
        <trd>Chasid</trd>, <trd>Hasid</trd>, <trd>Hassid</trd>
      </trdgrp>
      <trd lng="pl">Chasyd</trd>
      <trd lng="ru">&c_h;&c_a;&c_s;&c_i;&c_d;</trd>
    </drv>
 
    <drv mrk="hxasid.0ismo">
      <kap><tld/>ismo</kap>
      <snc>
 <uzo tip="fak">REL</uzo>
 <dif>
          En <ref tip="malprt" cel="jud.0ismo">judismo</ref>, mistika
          movado kaj vivmaniero bazita sur &gcirc;oje fida sindono al la Dia
          volo, esti&gcirc;inta en la 18a jc inter la judoj polaj kaj
          ruslandaj.
 </dif>
      </snc>
      <trdgrp lng="en">
        <trd>Chassidim</trd>, <trd>Hasidim</trd>, <trd>Hassidim</trd>
      </trdgrp>
      <trd lng="pl">chasydyzm</trd>
      <trd lng="ru">&c_h;&c_a;&c_s;&c_i;&c_d;&c_i;&c_z;&c_m;</trd>
    </drv>
 
  </art>

<!--
$Log$
-->
</vortaro>