        </trdgrp>
        <trd lng="ru">&c_p;&c_o;&c_d;&c_r;&c_a;&c_z;&c_u;&c_m;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
      </ekz>
      <ekz>
        <tld/>krii
        <klr>(detene krieti)</klr>;
