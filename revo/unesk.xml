<kap>
    <rad>Unesk</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="unesk.0o">
  <kap><tld/>o</kap>
  <snc mrk="unesk.0o.organizo">
    <uzo tip="fak">POL</uzo>
    <dif>
      Organiza&jcirc;o de Unui&gcirc;intaj Nacioj pri Edukado, Scienco kaj Kulturo:
      <ekz>
        <tld/>o fondi&gcirc;is la 16-an de novembro 1945, &gcirc;i havas centran
        oficejon en 
        Parizo, Francio, kaj 73 sub-oficejojn en diversaj partoj de la
        mondo
        <fnt><bib>Viki</bib>, <lok>Unesko</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">UNESCO</trd>
  <trd lng="fr">UNESCO</trd>
  <trdgrp lng="ja">
    <trd>&#x30E6;&#x30CD;&#x30B9;&#x30B3;  [<ind>&#x3086;&#x306D;&#x3059;&#x3053;</ind>]</trd>,
    <trd>Scienco kaj Kulturo  [<ind>SciencokajKulturo</ind>]</trd>
  </trdgrp>
  <trd lng="uk">&#x42E;&#x43D;&#x435;&#x441;&#x43A;&#x43E;</trd>
</drv>
</art>

