<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>perpendikular</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="perpen1.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>VdE</bib></fnt>
  <snc>
    <ref tip="dif" cel="ort.0a.figuroj">orta.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>perpendikular</trd>,
    <trd>senkrecht</trd>
  </trdgrp>
  <trd lng="fr">perpendiculaire<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="nl">loodrecht</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_p;&c_je;&c_n;&c_d;&c_i;&c_k;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="perpen1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>VdE</bib></fnt>
  <snc>
    <ref tip="dif" cel="ort.0anto">ortanto.</ref>
  </snc>
  <trd lng="de">Senkrechte</trd>
  <trd lng="en">perpendicular</trd>
  <trd lng="fr">perpendiculaire<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_p;&c_je;&c_n;&c_d;&c_i;&c_k;&c_u;&c_l;&c_ja;&c_r;</trd>
</drv>
</art>
