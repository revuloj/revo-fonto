<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: perpen1.xml,v 1.21 2023/09/03 12:45:07 revo Exp $">
<kap>
  <rad>perpendikular</rad>/a
  <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="perpen1.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>VdE</bib></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="ort.0a.figuroj">Orta</ref>:
      <ekz>
        la pupiloj de la okuloj de la katoj &gcirc;is tagmezo iom post iom malgrandi&gcirc;as kaj
        tiam atingas sian plej mallar&gcirc;an kuntiri&gcirc;on en formo de maldika streko,
        metita, kiel hareto, <tld/>e sur la mezon de la okulo
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_t;&c_a;&c_g;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_e;&c_r;&c_p;&c_e;&c_n;&c_d;&c_y;&c_k;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">perpendicular <klr>(adj.)</klr></trd>
  <trdgrp lng="cs">
    <trd>kolm&yacute;</trd>,
    <trd>perpendikul&aacute;rn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>perpendikular</trd>,
    <trd>orthogonal</trd>,
    <trd>senkrecht</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>orthogonal</trd>,
    <trd>perpendicular</trd>
  </trdgrp>
  <trd lng="es">perpendicular</trd>
  <trdgrp lng="fr">
    <trd>orthogonal</trd>,
    <trd>perpendiculaire<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">mer&odblac;leges</trd>
  <trd lng="ja">&#x5782;&#x76F4;&#x306A;  [<ind>&#x3059;&#x3044;&#x3061;&#x3087;&#x304F;&#x306A;</ind>]</trd>
  <trd lng="nl">loodrecht</trd>
  <trdgrp lng="pl">
    <trd>prostopad&lstroke;y</trd>,
    <trd>ortogonalny</trd>
  </trdgrp>
  <trd lng="pt">perpendicular <klr tip="ind">(adj.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_r;&c_t;&c_o;&c_g;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_p;&c_je;&c_n;&c_d;&c_i;&c_k;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j;
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kolm&yacute;</trd>,
    <trd>strm&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x435;&#x440;&#x43F;&#x435;&#x43D;&#x434;&#x438;&#x43A;&#x443;&#x43B;&#x44F;&#x440;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x440;&#x44F;&#x43C;&#x43E;&#x432;&#x438;&#x441;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="perpen1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>VdE</bib></fnt>
  <snc>
    <ref tip="dif" cel="ort.0anto">ortanto</ref>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_p;&c_e;&c_n;&c_d;&c_y;&c_k;&c_u;&c_l;&c_ja;&c_r;</trd>
  <trd lng="ca">perpendicular <klr>(subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>kolmice</trd>,
    <trd>perpendikul&aacute;ra</trd>,
    <trd>svislice</trd>
  </trdgrp>
  <trd lng="de">Senkrechte</trd>
  <trd lng="en">perpendicular</trd>
  <trd lng="es">perpendicular</trd>
  <trd lng="fr">perpendiculaire<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">mer&odblac;leges<klr tip="ind"> (egyenes)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x5782;&#x76F4;&#x7DDA;  [<ind>&#x3059;&#x3044;&#x3061;&#x3087;&#x304F;&#x305B;&#x3093;</ind>]</trd>,
    <trd>&#x5782;&#x7DDA;  [<ind>&#x3059;&#x3044;&#x305B;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">loodlijn</trd>
  <trd lng="pl">prosta prostopad&lstroke;a</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_p;&c_je;&c_n;&c_d;&c_i;&c_k;&c_u;&c_l;&c_ja;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>kolmica</trd>,
    <trd>zvislica</trd>
  </trdgrp>
</drv>
</art>
