<drv mrk="auxt.sxargx0o">
  <kap>&scirc;ar&gcirc;<tld/>o</kap>
  <snc mrk="auxt.sxargx0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="dif" cel="kamion.0o">Kamiono</ref>:
      <ekz>
        &scirc;ar&gcirc;<tld/>ojn plenajn de laborvestitaj fabrik-laboristoj
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Horv&aacute;th</aut>
          <vrk><url ref="https://www.monato.net/2006/009060.php">Tra
          infanaj okuloj: memoroj pri la eventoj de '56</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sur ponto trans la kanalo staris la &scirc;ar&gcirc;<tld/>o de la laboristoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="https://www.monato.net/2003/008139.html">Babel-insulo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        organizitaj grupoj el diversaj urboj de Litovio ka&scirc;as
        kontrabandajn varojn en
        &scirc;ar&gcirc;<tld/>oj kaj &scirc;ipkonteneroj
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Straznickas</aut>
          <vrk><url ref="&Steloj;monato2/006830.html">Litovio &#8208;
          Britio: Kontrabanditaj cigaredoj invadas Brition</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        bulgaraj firmaoj elspezis pli ol 8 milionojn da e&ubreve;roj por
        trans&scirc;ar&gcirc;igo de varoj
        al &scirc;ar&gcirc;<tld/>oj kaj trajnoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Steloj;monato2/006857.html">Trafiko:
          Danuba &scirc;iptrafiko paralizita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi <klr>[&#8230;]</klr> kun kortu&scirc;o memoras pri amiko mia, 
        portugala &scirc;ar&gcirc;<tld/>o-kondukisto
        <fnt><bib>Plumamikoj</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_z;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_i;&c_o;&c_n;</trd>
