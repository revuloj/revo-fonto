<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>a&ubreve;t</rad>/o
</kap>

<drv mrk="auxt.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxt.0o.AUT">
    <tezrad fak="AUT"/>
    <uzo tip="fak">AUT</uzo>
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="vetur.0ilo">Veturilo</ref> por surgrunda
      transporto de varoj a&ubreve; homoj, movata de motoro:
      <ekz>
        kelkaj <tld/>oj kuris sur la stratoj
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, 2</lok></fnt>;
      </ekz>
      <ekz>
        dekono de la familioj balda&ubreve; a&ccirc;etos <tld/>on
        <fnt><aut>Mu Binghua</aut>, <vrk>Dekono de la familioj 
        balda&ubreve; a&ccirc;etos a&ubreve;ton</vrk>, 
        <bib>Monato</bib>, <lok>jaro 2001a, numero 6a,
        p. 6a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="auxtom1.0o">a&ubreve;tomobilo</ref>.
    </refgrp>
    <rim>
      La radiko <ctl><tld/></ctl> estas nun pli ofte uzata en
      &ccirc;iutaga lingva&jcirc;o, ol
      <ctl>a&ubreve;tomobil</ctl>, kies mallongigo &gcirc;i
      estas. Pro imito al nacilingvaj kutimoj oni emas konservi
      la fina&jcirc;on &leftquot;o&rightquot; en kunmeta&jcirc;oj (ekz.
      a&ubreve;tovojo) kaj oni ankora&ubreve; ne ofte uzas la
      adjektivon <ctl><tld/>a</ctl>.
    </rim>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_mol;
  </trd>
  <trd lng="bg">&c_k;&c_o;&c_l;&c_a;</trd>
  <trd lng="de">Auto</trd>
  <trdgrp lng="en">
    <trd>car</trd>,
    <trd>auto</trd>,
    <trd>automobile</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>auto</trd>,
    <trd>coche</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>auto</trd>,
    <trd>voiture</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kocsi</trd>,
    <trd>aut&oacute;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>auto<klr> (vettura)</klr></trd>,
    <trd>macchina<klr> (vettura)</klr></trd>
  </trdgrp>
  <trd lng="nl">auto</trd>
  <trdgrp lng="pl">
    <trd>a&ubreve;to</trd>,
    <trd>samoch&oacute;d</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_mol;
  </trd>
  <trd lng="sk">auto</trd>
  <trd lng="sv">bil</trd>
  <trd lng="tr">oto</trd>
</drv>

<drv mrk="auxt.0a">
  <kap><tld/>a</kap>
  <snc mrk="auxt.0a.AUT">
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        parkinto po&scirc;telefone sendas sian <tld/>an identigilon
        <fnt><aut>Marija Belosevi&ccirc;</aut>, 
        <vrk>Parkilo en la po&scirc;o</vrk>,
        <bib>Monato</bib>, <lok>jaro 2002a, numero 5a, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="de">Auto-</trd>
  <trd lng="en">automotive</trd>
  <trdgrp lng="fr">
    <trd>automobile<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>dell'<ind>automobile</ind></trd>,
    <trd>automobilistico</trd>
  </trdgrp>
  <trd lng="nl">auto-</trd>
  <trd lng="pl">samochodowy</trd>
</drv>

<drv mrk="auxt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="auxt.0i.AUT">
    <dif>
      Veturi per <tld/>o:
      <ekz>
        vi <tld/>us en Finnlando la&ubreve; la &scirc;oseo E75
        <fnt><aut>Saliko</aut>, <vrk>Kolekti sonorilojn</vrk>,
        <bib>Monato</bib>, <lok>jaro 2001a, numero 2a, p.
        24a</lok></fnt>...
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_je;&c_h;&c_a;&c_c;&c_mol; <ind>
  &c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_je;&c_m;</ind></trd>
  <trd lng="de">autofahren</trd>
  <trd lng="en">drive</trd>
  <trd lng="fr">rouler<klr tip="ind"> (en voiture)</klr></trd>
  <trd lng="hu">aut&oacute;zik</trd>
  <trdgrp lng="it">
    <trd>guidare<klr> (viaggiare in automobile)</klr></trd>,
    <trd>viaggiare in <ind>automobile</ind></trd>
  </trdgrp>
  <trd lng="nl">rijden<klr tip="ind"> (per auto)</klr></trd>
  <trdgrp lng="pl">
   <trd>jecha&cacute;<klr tip="ind"> autem</klr></trd>,
   <trd>jecha&cacute;<klr tip="ind"> samochodem</klr></trd>
  </trdgrp>
  <trd lng="sv">&aring;ka<klr tip="ind"> (bil)</klr></trd>
</drv>

<drv mrk="auxt.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="auxt.0ejo.AUT">
    <dif>
      <ref tip="super" cel="remiz.0o">Remizo</ref> por <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_a;&c_zh;</trd>
  <trd lng="de">Garage</trd>
  <trdgrp lng="en">
    <trd>garage</trd>,
    <trd>carport</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>box<klr tip="ind"> (pour voiture)</klr></trd>,
    <trd>garage<klr tip="ind"> (pour voiture)</klr></trd>
  </trdgrp>
  <trd lng="hu">gar&aacute;zs</trd>
  <trdgrp lng="it">
    <trd>autorimessa</trd>,
    <trd>garage</trd>, <trd>box</trd>
  </trdgrp>
  <trd lng="nl">garage</trd>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_a;&c_zh;</trd>
  <trdgrp lng="pl">
    <trd>gara&zdot;</trd>,
    <trd>parking</trd>
  </trdgrp>
  <trd lng="sk">gar&aacute;&zcaron;</trd>
  <trd lng="sv">garage</trd>
</drv>

<drv mrk="auxt.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="auxt.0isto.AUT">
    <dif>
      Tiu, kiu profesie funkciigas kaj uzas <tld/>on.
    </dif>
  </snc>
  <trd lng="be">
  &c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">automobilista</trd>
  <trd lng="de">Autofahrer</trd>
  <trdgrp lng="en">
    <trd>driver</trd>,
    <trd>motorist</trd>
  </trdgrp>
  <trd lng="es">automovilista</trd>
  <trd lng="fr">automobiliste</trd>
  <trd lng="hu">aut&oacute;s</trd>
  <trd lng="it">autista</trd>
  <trd lng="nl">chauffeur</trd>
  <trdgrp lng="pl">
    <trd>kierowca<klr tip="ind"> samochodu</klr></trd>,
    <trd>kierowca<klr tip="ind"> auta</klr></trd>,
    <trd>szofer</trd>,
    <trd>automobilista<klr> (przestarz.)</klr></trd>
  </trdgrp>
  <trd lng="ru">
  &c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="sv">bilf&ouml;rare</trd>
  <adm>
    La originala redakto difinis "~isto" per "motoristo", kio la&ubreve;
    mi estas anglismo. En la &scirc;an&gcirc;ita difino mi preferus 
    skribi "stiras", sed la koncerna radiko ankora&ubreve; ne ekzistas. 
    &Scirc;ajnas, ke la uzo multe hezitis : konduki/direkti/stiri ~on.
    <aut>MB</aut>
  </adm>
</drv>

<drv mrk="auxt.logx0o">
  <kap>lo&gcirc;<tld/>o</kap>
  <snc mrk="auxt.logx0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      <ref tip="super" cel="auxt.0o.AUT">A&ubreve;to</ref> kun platformo
      aran&gcirc;ita por lo&gcirc;ado:
      <ekz>
        viziti E&ubreve;ropajn landojn per lo&gcirc;<tld/>o
        <fnt>
        <aut>M. Claude</aut>,
        <vrk>8 julio 2005, Esperanto Koresponda Servo, Listo de 
        respondintoj</vrk>,
        <lok>soc.culture.esperanto, 2005-07-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Wohnmobil</trd>
  <trdgrp lng="en">
    <trd>camper</trd>,
    <trd>motor home</trd>
  </trdgrp>
  <trd lng="fr">camping-car</trd>
  <trd lng="hu">lak&oacute;aut&oacute;</trd>
   <trdgrp lng="it">
    <trd>camper</trd>,
    <trd>caravan</trd>
  </trdgrp>
  <trd lng="nl">motorhome</trd>
  <trd lng="pl">kampobus</trd>
</drv>

<drv mrk="auxt.sxargx0o">
  <kap>&scirc;ar&gcirc;<tld/>o</kap>
  <snc mrk="auxt.sxargx0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <uzo tip="fak">TRA</uzo>
    <ref tip="dif" cel="kamion.0o">kamiono</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_u;&c_z;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_i;&c_o;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Lastwagen</trd>,
    <trd>Lkw</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lorry</trd>,
    <trd>truck</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>camion</trd>
  </trdgrp>
  <trd lng="hu">kamion</trd>
  <trdgrp lng="it">
    <trd>camion</trd>,
    <trd>furgone</trd>
  </trdgrp>
  <trd lng="nl">vrachtwagen</trd>
  <trd lng="pl">ci&eogonek;&zdot;ar&oacute;wka</trd>
  <trd lng="ru">&c_g;&c_r;&c_u;&c_z;&c_o;&c_v;&c_i;&c_k;</trd>
</drv>

<drv mrk="auxt.sxargx0isto">
  <kap>&scirc;ar&gcirc;<tld/>isto</kap>
  <snc mrk="auxt.sxargx0isto.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="sxofor.0o.TRA">&Scirc;oforo</ref>, kiu 
      kondukas &scirc;ar&gcirc;<tld/>ojn:
      <ekz>
        mi <klr>...</klr> kun kortu&scirc;o memoras pri amiko mia, 
        portugala &scirc;ar&gcirc;<tld/>o-kondukisto
        <fnt><bib>Plumamikoj</bib>, <lok>tradukoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">truck driver</trd>
  <trd lng="fr">camionneur</trd>
  <trd lng="hu">kamionsof&odblac;r</trd>
  <trd lng="it">camionista</trd>
  <trd lng="nl">vrachtwagenchauffeur</trd>
  <trd lng="pl">kierowca<klr tip="ind"> 
  ci&eogonek;&zdot;ar&oacute;wki</klr></trd>
</drv>
</art>
