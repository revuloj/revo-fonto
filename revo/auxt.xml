<drv mrk="auxt.0a">
  <kap><tld/>a</kap>
  <snc mrk="auxt.0a.AUT">
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        parkinto po&scirc;telefone sendas sian <tld/>an identigilon
        <fnt><aut>M. Belosevi&ccirc;</aut>
        <vrk>Parkilo en la po&scirc;o</vrk>
        <bib>Monato</bib> <lok>2002:5, p. 17a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj a&ubreve; busaj paneoj
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="&Monato;/2003/007662.html">Nesolidaj
          soldatoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="de">Auto-</trd>
  <trd lng="en">automotive</trd>
  <trdgrp lng="fr">
    <trd>automobile<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>automotif</trd>,
    <trd>otomotif</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>dell'<ind>automobile</ind></trd>,
    <trd>automobilistico</trd>
  </trdgrp>
  <trd lng="nl">auto-</trd>
  <trd lng="pl">samochodowy</trd>
  <trdgrp lng="tr">
    <trd>otomotiv</trd>,
    <trd>arabayla ilgili</trd>
  </trdgrp>
</drv>

<drv mrk="auxt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="auxt.0i.AUT">
    <dif>
      <ref tip="super" cel="vetur.0i.TRA">Veturi<sncref/></ref> per <tld/>o:
      <ekz>
        vi <tld/>us en Finnlando la&ubreve; la &scirc;oseo E75
        <fnt><aut>Saliko</aut> <vrk>Kolekti sonorilojn</vrk>
        <bib>Monato</bib> <lok>2001:2, p.
        24a</lok></fnt>;
      </ekz>
      <ekz>
        se &ccirc;i-somere vi turisme <tld/>as, &ccirc;u en Britio a&ubreve; alilande,
        &scirc;oforu sekure
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2011/010446.php">Lingvo
          a&ubreve;to-didakta</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la tagon anta&ubreve;e <nom>Albert</nom> sola <tld/>is de
        <klr>[&#8230;]</klr> lia lo&gcirc;loko en la regiono
        de Nurenbergo, al Hamburgo
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2011/010500.php">Kiam
          montopado fari&gcirc;is mortopado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_je;&c_h;&c_a;&c_c;&c_mol; <ind>
  &c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_b;&c_ib;&c_l;&c_je;&c_m;</ind></trd>
  <trd lng="de">Auto <ind>fahren</ind></trd>
  <trdgrp lng="en">
    <trd>drive<klr tip="ind"> (in a car)</klr></trd>,
    <trd><ind>go</ind> somewhere by car</trd>
  </trdgrp>
  <trd lng="fr">rouler<klr tip="ind"> (en voiture)</klr></trd>
  <trd lng="hu">aut&oacute;zik</trd>
  <trdgrp lng="id">
    <trd><baz>kemudi</baz> mengemudikan<klr tip="amb"> (mobil
    dsb.)</klr></trd>,
    <trd><baz>kendara</baz> mengendarai<klr tip="amb"> (mobil
    dsb.)</klr></trd>,
    <trd><baz>setir</baz> menyetir<klr tip="amb"> (mobil dsb.)</klr><klr tip="amb"/></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>guidare<klr> (viaggiare in automobile)</klr></trd>,
    <trd>viaggiare in <ind>automobile</ind></trd>
  </trdgrp>
  <trd lng="ja">&#36554;&#12391;&#34892;&#12367;
  <ind>&#12367;&#12427;&#12414;&#12391;&#12356;&#12367;</ind>]</trd>
  <trd lng="nl">rijden<klr tip="ind"> (per auto)</klr></trd>
  <trdgrp lng="pl">
   <trd>jecha&cacute;<klr tip="ind"> autem</klr></trd>,
   <trd>jecha&cacute;<klr tip="ind"> samochodem</klr></trd>
  </trdgrp>
