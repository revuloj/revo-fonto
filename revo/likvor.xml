<art mrk="$Id: likvor.xml,v 1.24 2024/01/15 19:02:14 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>likvor</rad>/o
</kap>
<drv mrk="likvor.0o">
  <kap>
  <ofc>*</ofc><tld/>o
  </kap>
  <snc>
  <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" lst="voko:trinka&jcirc;oj" cel="alkoho.0ajxo">Alkohola&jcirc;o</ref>
      farita el <ref tip="prt" cel="brand.0o">brando</ref>, 
      aroma&jcirc;oj kaj sukero:
      <ekz>
        <ind>ovo<tld/>o</ind>;
        <trd lng="bg">&c_ja;&c_j;&c_ch;&c_je;&c_n;</trd>
        <trd lng="de">Eierlik&ouml;r</trd>
        <trd lng="en">eggnog</trd>
        <trd lng="nl">advocaat</trd>
      </ekz>
      <ekz>
        aniza <tld/>o;
      </ekz>
      <ekz>
        kasiso estas franca <tld/>o farita el nigraj riboj
        <fnt>
          
          <aut>Ste&ccirc;jo Norvell</aut>
          <vrk><url ref="&Monato;/2001/007157.html">Vivo: Riboj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        tekilo estas speco de brando a&ubreve; <tld/>o produktata
        <klr>[...]</klr> en Meksiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4252">Tekilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_k;&c_jo;&c_r;</trd>
  <trd lng="bg">&c_l;&c_i;&c_k;&c_mol;&c_o;&c_r;</trd>
  <trd lng="bo">&#xF68;&#xF0B;&#xF62;&#xF42;&#xF0B;</trd>
  <trd lng="de">Lik&ouml;r</trd>
  <trd lng="en">liqueur</trd>
  <trd lng="es">licor</trd>
  <trd lng="fr">liqueur</trd>
  <trd lng="hu">lik&ouml;r</trd>
  <trd lng="ja">&#x30EA;&#x30AD;&#x30E5;&#x30FC;&#x30EB;</trd>
  <trd lng="nl">likeur</trd>
  <trd lng="pl">likier</trd> 
  <trd lng="pt">licor</trd>
  <trd lng="ro">lichior</trd>
  <trd lng="ru">&c_l;&c_i;&c_k;&c_jo;&c_r;</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_b;&c_s;&c_je;&#x43D;&c_t;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&c_i;&#x43D;&#x43E;&c_v;&c_a; &#x43D;&c_a;&c_s;&c_t;&#x43E;&c_j;&#x43A;&c_a;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&c_i;&#x43D;&#x43E;&c_v;&c_a; &c_g;&#x43E;&c_r;&c_ib;&#x43B;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x919D; <pr>cu&#xF3;</pr></trd>,
    <trd>&#x529B;&#x5A07;&#x9152; <pr>l&#xEC;ji&#x101;oji&#x1D4;</pr></trd>,
    <trd>&#x529B;&#x5B0C;&#x9152; <pr>l&#xEC;ji&#x101;oji&#x1D4;</pr></trd>,
    <trd>&#x5229;&#x53E3;&#x9152; <pr>l&#xEC;k&#x1D2;uji&#x1D4;</pr></trd>,
    <trd>&#x751C;&#x70E7;&#x9152; <pr>ti&#xE1;nsh&#x101;oji&#x1D4;</pr></trd>,
    <trd>&#x751C;&#x71D2;&#x9152; <pr>ti&#xE1;nsh&#x101;oji&#x1D4;</pr></trd>,
    <trd>&#x871C;&#x9152; <pr>m&#xEC;ji&#x1D4;</pr></trd>
  </trdgrp>
</drv>
