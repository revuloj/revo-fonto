<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>alpinist</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="alpinist.0o">
  <kap><tld/>o</kap>
  <snc mrk="alpinist.0o.SPO">
    <dif>
      <ref tip="dif" cel="alp.0isto.SPO">Alpisto</ref>:
      <ekz>
        &ccirc;iuj niaj <tld/>aj akrobata&jcirc;oj &scirc;ajnas esti infana ludo
        <fnt>
          <aut>Vladimir Glazunov</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Cxarmo.html"
              >La &ccirc;armo de &Ccirc;ielmontaro</url></vrk>,
          <lok>Fonto, numero 37a</lok>
        </fnt>;
      </ekz>
      <ekz>
        li konati&gcirc;is kun Linko, grava monda <tld/>o
        <fnt>
          <aut>P. Mladenovic</aut>,
          <vrk><url ref="http://www.china.org.cn/EL/EL/ElPopolaCxinio/bd98-8-12.html"
              >Tibor Sekelj &ndash; filo de la naturo</url></vrk>,
          <lok>El popola &Ccirc;inio, 1998-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>alpinista</trd>,
    <trd>horolezec</trd>
  </trdgrp>
  <trd lng="de">Alpinist</trd>
  <trd lng="sk">horolezec</trd>
</drv>
</art>

