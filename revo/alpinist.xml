<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alpinist.xml,v 1.7 2023/10/28 08:33:05 revo Exp $">
<kap>
  <rad>alpinist</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="alpinist.0o">
  <kap><tld/>o</kap>
  <snc mrk="alpinist.0o.SPO">
    <dif>
      <ref tip="dif" cel="alp.0isto.SPO">Alpisto</ref>:
      <ekz>
        &ccirc;iuj niaj <tld/>aj akrobata&jcirc;oj &scirc;ajnas esti infana ludo
        <fnt>
          <aut>Vladimir Glazunov</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/Cxarmo.html">La &ccirc;armo de &Ccirc;ielmontaro</url></vrk>,
          <lok>Fonto, numero 37a</lok>
        </fnt>;
      </ekz>
      <ekz>
        li konati&gcirc;is kun Linko, grava monda <tld/>o
        <fnt>
          <aut>P. Mladenovic</aut>,
          <vrk><url ref="http://www.china.org.cn/EL/EL/ElPopolaCxinio/bd98-8-12.html">Tibor Sekelj &ndash; filo de la naturo</url></vrk>,
          <lok>El popola &Ccirc;inio, 1998-08</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x767B;&#x5C71;&#x5BB6; <pr>d&#x113;ngsh&#x101;nji&#x101;</pr></trd>,
    <trd>&#x963F;&#x5C14;&#x5351;&#x65AF;&#x767B;&#x5C71;&#x5BB6; <pr>&#x101;&#x11B;rb&#x113;is&#x12B;d&#x113;ngsh&#x101;nji&#x101;</pr></trd>,
    <trd>&#x963F;&#x723E;&#x5351;&#x65AF;&#x767B;&#x5C71;&#x5BB6; <pr>&#x101;&#x11B;rb&#x113;is&#x12B;d&#x113;ngsh&#x101;nji&#x101;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_p;&c_ib;&c_n;&c_ib;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>alpinista</trd>,
    <trd>horolezec</trd>
  </trdgrp>
  <trd lng="de">Alpinist</trd>
  <trd lng="sk">horolezec</trd>
</drv>
</art>

