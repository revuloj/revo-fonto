<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dizert.xml,v 1.22 2020/12/02 14:37:51 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>dizert</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dizert.0i">
  <kap><tld/>i</kap>
  <snc mrk="dizert.0i.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="fugx.0i">Fu&gcirc;i</ref> el <ref cel="serv.milit0o">militservo</ref> kun la
      intenco ne plu reveni al &gcirc;i:
      <ekz>
        li <tld/>is, ricevis pro tio mortokondamnon kaj dum la tuta cetero de sia vivo
        li estis fervora pacifisto
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.be/2006/008886.php"
              >Prisilentita heroo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="disert.0i">diserti</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_z;&c_e;&c_r;&c_t;&c_y;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_je;&c_z;&c_je;&c_r;&c_t;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>desertovat</trd>,
    <trd>dezertovat</trd>,
    <trd>uprchnout</trd>,
    <trd>ut&eacute;ci</trd>,
    <trd>zb&ecaron;hnout</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>desertieren</trd>,
    <trd>fahnenfl&uuml;chtig werden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>desert</trd>,
    <trd>run away from</trd>
  </trdgrp>
  <trd lng="es">desertar</trd>
  <trd lng="fr">d&eacute;serter</trd>
  <trd lng="it">disertare</trd>
  <trd lng="nl">deserteren</trd>
  <trd lng="pl">dezerterowa&cacute;</trd>
  <trd lng="pt">desertar</trd>
  <trd lng="ru">&c_d;&c_je;&c_z;&c_je;&c_r;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>dezertova&tcaron;</trd>,
    <trd>zbehn&uacute;&tcaron;</trd>
  </trdgrp>
</drv>
<drv mrk="dizert.0o">
  <kap><tld/>o</kap>
  <snc mrk="dizert.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Ago <tld/>i:
      <ekz>
        post <tld/>o el sia ta&ccirc;mento li estis kaptita kaj kondamnita al mortopuno
        <fnt>
          <vrk><url ref="https://www.esperanto.be/fel/publika/012249p.php"
              >Leginda malperfekta&jcirc;o - Esperanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="desert.0o">deserto</ref>,
      <ref cel="dezert.0o">dezerto</ref>,
    </refgrp>
  </snc>
   <trdgrp lng="de">
    <trd>Fahnenflucht</trd>,
    <trd>Desertation</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;sertion</trd>
</drv>

<drv mrk="dizert.0into">
  <kap>
    <tld/>into, <var><kap><tld/>ulo</kap></var>
  </kap>
  <snc>
    <dif>
      Tiu, kiu <tld/>is:
      <ekz>
        <tld/>into el la turka armeo <klr>[&#8230;]</klr> petis azilon
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2006/009092.php"
              >Pordo al dan&gcirc;ero</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        Japanio devus liveri la <tld/>inton al la usonaj instancoj
        <fnt>
          <bib>Monato</bib>
          <aut>Igarasi Takeo</aut>
          <vrk><url ref="https://www.monato.be/2005/008407.php"
              >Komplika familia reunui&gcirc;o</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        junaj serboj, militrifuzantoj kaj <tld/>uloj <klr>[&#8230;]</klr> for&jcirc;etis siajn
        armilojn kaj fu&gcirc;is eksterlanden, precipe al najbara Hungario
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007190.html">Trompitaj dizertintoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Svisoj <klr>[&#8230;]</klr> &scirc;an&gcirc;is siajn bankpreskribojn, tiel ke e&ubreve;ropaj
        financ<tld/>intoj <klr>(impostfu&gcirc;intoj)</klr> nun devas ser&ccirc;i aliajn impost-oazojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2009/009929.php"
              >Du mil na&ubreve;</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Deserteur</trd>,
    <trd>Fahnenfl&uuml;chtiger</trd>
  </trdgrp>
</drv>

</art>
