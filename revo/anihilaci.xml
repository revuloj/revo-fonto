<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>anihilaci</rad>/o
</kap>
<drv mrk="anihilaci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Unu el specoj de transformi&gcirc;o de
      <ref tip="vid" cel="partik2.0o">partikloj</ref>,
      okazanta dum kunpu&scirc;i&gcirc;o de partiklo kun
      <ref tip="vid" cel="partik2.kontraux0o">kontra&ubreve;partiklo</ref>.
      Dum <tld/>o la partiklo kaj la kontra&ubreve;partiklo malaperas,
      transformi&gcirc;ante
      en aliajn partiklojn, kies nombro kaj specoj estas limigataj de la
      le&gcirc;oj de konservado.
      <ekz>
        En kunpu&scirc;i&gcirc;oj kun malgrandaj energioj dum <tld/>o de paro
        negatrono-pozitrono aperas fotonoj, kaj de paro
        nukleono-kontra&ubreve;nukleono
        aperas precipe pi-mezonoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_ib;&c_g;&c_ib;&c_l;&c_ja;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Zerstrahlung</trd>
  <trd lng="en">annihilation</trd>
  <trd lng="es">aniquilaci&oacute;n<klr> (f&iacute;sica)</klr></trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;tsug&aacute;rz&aacute;s</trd>,
    <trd>annihili&aacute;ci&oacute;</trd></trdgrp>
  <trd lng="nl">annihilatie</trd>
  <trd lng="ru">&c_a;&c_n;&c_n;&c_i;&c_g;&c_i;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">anihil&aacute;cia</trd>
</drv>

<drv mrk="anihilaci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Malaperi en <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_ib;&c_g;&c_ib;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">zerstrahlen</trd>
  <trd lng="en">annihilate</trd>
  <trd lng="es">aniquilar</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;tsug&aacute;rzik</trd>,
    <trd>annihili&aacute;l</trd></trdgrp>
  <trd lng="nl">annihileren</trd>
  <trd lng="ru">&c_a;&c_n;&c_n;&c_i;&c_g;&c_i;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">anihilova&tcaron;</trd>
</drv>
</art>
