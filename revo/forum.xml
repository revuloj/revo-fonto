<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: forum.xml,v 1.23 2023/11/10 06:08:00 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>forum</rad>/o
</kap>
<drv mrk="forum.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Placo de antikva Romo, kie la popolo pritraktis la publikajn
      aferojn.
    </dif>
    <trd lng="ru">&c_f;&c_o;&c_r;&c_u;&c_m;</trd>
    <trd lng="nl">forum romanum</trd>
    <trdgrp lng="pt">
      <trd>foro</trd>,
      <trd>f&oacute;rum</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Loko, kie oni diskutas pri publikaj aferoj; centro de la publika
      vivo.
    </dif>
    <trdgrp lng="ru">
      <trd>&c_f;&c_o;&c_r;&c_u;&c_m;</trd>, <trd>&c_t;&c_o;&c_r;&c_zh;&c_i;&c_shch;&c_je;</trd>,
      <trd>&c_v;&c_je;&c_ch;&c_je;</trd>
    </trdgrp>
    <trd lng="nl">forum</trd>
  <trdgrp lng="pt">
    <trd>foro</trd>,
    <trd>tribuna</trd>,
    <trd>f&oacute;rum</trd>
  </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ju&gcirc;opovo de la publika opinio:
      <ekz>
        sin prezenti anta&ubreve; la <tld/>o de la scienco.
      </ekz>
    </dif>
  <trdgrp lng="pt">
    <trd>foro</trd>,
    <trd>tribunal</trd>,
    <trd>f&oacute;rum</trd>,
    <trd>ju&iacute;zo</trd>,
    <trd>circunscri&ccedil;&atilde;o judici&aacute;ria</trd>,
  </trdgrp>
    <trd lng="ru">&c_s;&c_u;&c_d; <klr>(&c_o;&c_b;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_g;&c_o; &c_m;&c_n;&c_je;&c_n;&c_i;&c_ja;, &c_p;&c_u;&c_b;&c_l;&c_i;&c_k;&c_i;)</klr></trd>
    <trd lng="nl">forum</trd>
  </snc>
  <snc mrk="forum.0o.komp">
    <uzo tip="fak">KOMP</uzo>
    <fnt><lok><url ref="&KompLeks;/F.html#FORUMO">KompLeks</url></lok></fnt>
    <dif>
      Nefermita diskutgrupo en
      <ref tip="malprt" cel="ret.malloka_0o.KOMP">malloka komputila
      reto</ref>.
    </dif>
    <ref tip="prt" cel="afisx.0i">afi&scirc;i</ref>
    <trdgrp lng="en">
      <trd>forum</trd>, <trd>newsgroup</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_je;&c_l;&c_je;&c_k;&c_o;&c_n;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>,
      <trd>&c_d;&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_i;&c_o;&c_n;&c_n;&c_a;&c_ja; &c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</trd>
    </trdgrp>
    <trd lng="nl">nieuwsgroep<klr tip="ind"> (Internet)</klr></trd>
  <trdgrp lng="pt">
    <trd>f&oacute;rum</trd>,
    <trd>newsgroup</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_f;&c_o;&c_r;&c_u;&c_m;</trd>
  <trd lng="cs">f&oacute;rum</trd>
  <trd lng="de">Forum</trd>
  <trd lng="es">foro</trd>
  <trdgrp lng="fr">
    <trd>forum</trd>,
    <trd>groupe de discussion<klr tip="ind"> (Internet)</klr></trd>,
    <trd>lieu de d&eacute;bat</trd>,
    <trd>tribunal<klr tip="ind"> (jugement de
    l'opinion)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>f&oacute;rum</trd>,
    <trd>vitaf&oacute;rum<klr tip="ind"> (Internet)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x30D5;&#x30A9;&#x30FC;&#x30E9;&#x30E0;  <pr>&#x3075;&#x3049;&#x30FC;&#x3089;&#x3080;</pr></trd>
  <trd lng="sk">f&oacute;rum</trd>
  <trd lng="uk">&#x444;&#x43E;&#x440;&#x443;&#x43C;</trd>
  <trdgrp lng="zh">
    <trd>&#x8AD6;&#x58C7; <pr>l&#xF9;nt&#xE1;n</pr></trd>,
    <trd>&#x8A0E;&#x8AD6;&#x6703; <pr>t&#x1CE;ol&#xF9;nhu&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
