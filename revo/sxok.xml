          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=214781">Elektro&scirc;oka armilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_sh;&c_o;&c_k;</trd>
