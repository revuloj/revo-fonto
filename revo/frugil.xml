<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: frugil.xml,v 1.19 2022/04/27 17:19:11 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>frugileg</rad>/o
</kap>
<drv mrk="frugil.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">Specio</ref>
      de la genro <ref tip="super" cel="korv.0o.genro">korvo</ref>
      (<trd lng="la">Corvus frugilegus</trd>):
      <ekz>
        li kontaktis du maljunajn &ccirc;asistojn, kiuj memoris kiel mortigi la <tld/>ojn kaj ilin man&gcirc;ocele prepari
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2003/007964.html">Frua frugilega tradicio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="kornik.0o">korniko</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_k;</trd>
  <trd lng="bg">&c_p;&c_o;&c_s;&c_je;&c_v;&c_n;&c_a; <ind>&c_v;&c_r;&c_a;&c_n;&c_a;</ind></trd>
  <trdgrp lng="cs">
    <trd>havran polni</trd>,
    <trd>havran poln&iacute;</trd>
  </trdgrp>
  <trd lng="de">Saatkr&auml;he</trd>
  <trd lng="en">rook</trd>
  <trd lng="fr">freux</trd>
  <trd lng="hu">vet&eacute;si varj&uacute;</trd>
  <trd lng="nl">roek</trd>
  <trd lng="pl">gawron</trd>
  <trdgrp lng="pt">
    <trd>gralha-calva</trd>,
    <trd>frouva</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_ch;</trd>
  <trd lng="sk">havran po&lcaron;n&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x440;&#x430;&#x43A;</trd>,
    <trd>&#x433;&#x430;&#x439;&#x432;&#x43E;&#x440;&#x43E;&#x43D;</trd>
  </trdgrp>
</drv>
</art>
