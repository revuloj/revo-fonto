<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">

<kap>
 <ofc>8</ofc> <rad>Brazil</rad>/o
 </kap>

<drv mrk="brazil.B0o">
  <kap><ofc>8</ofc> <tld/>o
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
</kap> 
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      Sudamerika <ref tip="lst" cel="sxtat.0o"
      lst="voko:&scirc;tatoj">&scirc;tato</ref> kun &ccirc;efurbo <ref tip="dif"
      cel="brazilj.0o">Braziljo</ref>, limanta al <ref
      tip="vid"
      cel="urugvaj.U0o">Urugvajo</ref>, 
      <ref tip="vid" cel="paragvaj.P0o">Paragvajo</ref>,
      <ref tip="vid" cel="argentin.A0o">Argentino</ref>, 
      <ref tip="vid" cel="bolivi.B0o">Bolivio</ref>,
      <ref tip="vid" cel="peru.P0o">Peruo</ref>, 
      <ref tip="vid" cel="kolombi.K0o">Kolombio</ref>,
      <ref tip="vid" cel="venezuel.V0o">Venezuelo</ref>, 
      <ref tip="vid" cel="gujan.G0o.sxtato">Gujano</ref>,
      <ref tip="vid" cel="surinam.S0o">Surinamo</ref> kaj 
      <ref tip="vid" cel="gujan.FrancaG0o.sxtato">Franca Gujano</ref>,
      kaj anka&ubreve; al
      <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>.
      La kvina plej granda lando en la mondo la&ubreve; surfaco kaj
      la&ubreve; lo&gcirc;antaro.
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_r;&c_a;&c_z;&c_ib;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_B;&c_r;&c_a;&c_z;&c_i;&c_l;&c_i;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>el Brasil</trd>,
    <trd>Rep&uacute;blica Federal del Brasil</trd>
  </trdgrp>
  <trd lng="cs">Braz&iacute;lie <klr>(st&aacute;t)</klr></trd>
  <trd lng="da">Brasilien</trd>
  <trd lng="de">Brasilien</trd>
  <trd lng="en">Brazil</trd>
  <trd lng="es">Brasil</trd>
  <trd lng="fi">Brasilia</trd>
  <trd lng="fr">Br&eacute;sil</trd>
  <trd lng="he">&bet;&resh;&zayin;&yod;&lamed;</trd>
  <trd lng="hu">Braz&iacute;lia</trd>
  <trd lng="id">Brazil</trd>
  <trd lng="ja">&#12502;&#12521;&#12472;&#12523;</trd>
  <trd lng="nl">Brazili&euml;</trd>
  <trd lng="pl">Brazylia</trd>
  <trd lng="pt">Brasil</trd>
  <trd lng="ru">&c_B;&c_r;&c_a;&c_z;&c_i;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">Braz&iacute;lia <klr>(&scaron;t&aacute;t)</klr></trd>
  <trd lng="uk">&c_B;&c_r;&c_a;&c_z;&c_ib;&c_l;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="brazil.0a">
  <kap><tld lit="b"/>a</kap>
  <snc>
    <dif>
      Rilata al la &scirc;tato <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">brasiler <klr>(adj.)</klr></trd>
  <trd lng="fr">br&eacute;silien<klr> (adj.)</klr></trd>
  <trd lng="pt">brasileiro <klr tip="amb"> (adj.)</klr></trd>
</drv>

<drv mrk="brazil.0ano">
  <kap><tld lit="b"/>ano,
  <var><kap><tld lit="b"/>anino</kap></var></kap>
  <snc>
    <dif>
      &Scirc;tatano, &scirc;tatanino de <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">brasiler <klr>(individu)</klr></trd>
  <trd lng="fr">Br&eacute;silien<klr> (subst.)</klr></trd>
  <trd lng="pt">brasileiro<klr tip="amb"> (subs.)</klr></trd>
</drv>

<drv mrk="brazil.0ana">
  <kap><tld lit="b"/>ana</kap>
  <snc>
    <dif>
      Rilata al la <tld lit="b" />anoj.
    </dif>
  </snc>
  <trd lng="pt">brasileiro<klr tip="amb"> (adj.)</klr></trd>
</drv>

<drv mrk="brazil.0ajxo">
  <kap><tld lit="b"/>a&jcirc;o</kap>
  <snc mrk="brazil.0ajxo.ago">
    <dif>
      Ago a&ubreve; maniero propra de <tld lit="b"/>anoj.
    </dif>
  </snc>
  <snc mrk="brazil.0ajxo.ajxo">
    <dif>
      A&jcirc;o propra de <tld/>o.
    </dif>
  </snc>
  <trd lng="pt">brasileirice</trd>
</drv>

<drv mrk="brazil.0eco">
  <kap><tld lit="b"/>eco</kap>
  <snc>
    <dif>
      Eco aparta de <tld lit="b"/>anoj a&ubreve; <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">"brasilitat"</trd>
  <trd lng="pt">brasilidade</trd>
</drv>

<drv mrk="brazil.0ismo">
  <kap><tld lit="b"/>ismo</kap>
  <snc mrk="brazil.0ismo.LIN">
  <uzo tip="fak">LIN</uzo>
    <dif>
      <tld/>ana aparta dirmaniero.
    </dif>
  </snc>
  <snc mrk="brazil.0ismo.amo">
    <dif>
      Sento de amo al <tld/>o.
    </dif>
  </snc>
  <trd lng="ca">"brasilerisme"</trd>
  <trd lng="pt">brasileirismo</trd>
</drv>

</art>

