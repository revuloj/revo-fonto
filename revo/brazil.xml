<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>8</ofc><rad>Brazil</rad>/o</kap>
<drv mrk="brazil.B0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Sudamerika <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>,
      limanta
      al <ref tip="vid" cel="urugvaj.U0o">Urugvajo</ref>, 
      <ref tip="vid" cel="paragvaj.P0o">Paragvajo</ref>,
      <ref tip="vid" cel="argentin.A0o">Argentino</ref>, 
      <ref tip="vid" cel="bolivi.B0o">Bolivio</ref>,
      <ref tip="vid" cel="peru.P0o">Peruo</ref>, 
      <ref tip="vid" cel="kolombi.K0o">Kolombio</ref>,
      <ref tip="vid" cel="venezuel.V0o">Venezuelo</ref>, 
      <ref tip="vid" cel="gujan.G0o.sxtato">Gujano</ref>,
      <ref tip="vid" cel="surinam.S0o">Surinamo</ref> kaj 
      <ref tip="vid" cel="gujan.FrancaG0o.sxtato">Franca Gujano</ref>,
      kaj anka&ubreve; al
      <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>.
      La kvina plej granda lando en la mondo la&ubreve; surfaco kaj
      la&ubreve; lo&gcirc;antaro.
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_r;&c_a;&c_z;&c_ib;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_B;&c_r;&c_a;&c_z;&c_i;&c_l;&c_i;&c_ja;</trd>
  <trd lng="da">Brasilien</trd>
  <trd lng="de">Brasilien</trd>
  <trd lng="en">Brazil</trd>
  <trd lng="es">Brasil</trd>
  <trd lng="fi">Brasilia</trd>
  <trd lng="fr">Br&eacute;sil</trd>
  <trd lng="he">&bet;&resh;&zayin;&yod;&lamed;</trd>
  <trd lng="hu">Braz&iacute;lia</trd>
  <trd lng="ja">&#12502;&#12521;&#12472;&#12523;</trd>
  <trd lng="nl">Brazili&euml;</trd>
  <trd lng="pl">Brazylia</trd>
  <trd lng="pt">Brasil</trd>
  <trd lng="ru">&c_B;&c_r;&c_a;&c_z;&c_i;&c_l;&c_i;&c_ja;</trd>
  <trd lng="uk">&c_B;&c_r;&c_a;&c_z;&c_ib;&c_l;&c_ib;&c_ja;</trd>
</drv>
</art>

