<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: brazil.xml,v 1.31 2021/04/12 07:07:58 revo Exp $">

<kap>
 <ofc>8</ofc> <rad>Brazil</rad>/o <fnt><bib>PIV1</bib></fnt>
 </kap>

<drv mrk="brazil.B0o">
  <kap><ofc>8</ofc> <tld/>o
    <fnt><bib>RLN</bib></fnt>
  </kap> 
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="malprt" cel="amerik.Sud0o">Sudamerika</ref>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&scirc;tato</ref> kun
      &ccirc;efurbo <ref tip="dif" cel="brazilj.0o">Braziljo</ref>, limanta
      kun
      <ref tip="vid" cel="urugvaj.U0o">Urugvajo</ref>, 
      <ref tip="vid" cel="paragvaj.P0o">Paragvajo</ref>,
      <ref tip="vid" cel="argentin.A0o">Argentino</ref>, 
      <ref tip="vid" cel="bolivi.B0o">Bolivio</ref>,
      <ref tip="vid" cel="peru.P0o">Peruo</ref>, 
      <ref tip="vid" cel="kolombi.K0o">Kolombio</ref>,
      <ref tip="vid" cel="venezuel.V0o">Venezuelo</ref>, 
      <ref tip="vid" cel="gujan.G0o.sxtato">Gujano</ref>,
      <ref tip="vid" cel="surinam.S0o">Surinamo</ref> kaj 
      <ref tip="vid" cel="gujan.FrancaG0o.sxtato">Franca Gujano</ref>,
      kaj anka&ubreve; al
      <ref tip="vid" cel="atlant.A0o">Atlantiko</ref>,
      kvina plej granda lando en la mondo la&ubreve; surfaco kaj
      la&ubreve; lo&gcirc;antaro:
      <ekz>
        en julio okazis en <nom>Recife</nom>, <tld/>o, la 45a jarkunsido
        de la <tld/>a Societo por la Progresigo de la Scienco
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="http://steloj.de/esperanto/monato1/monato-9309-noticoj.txt">Esperanto
          sen peno</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toroj, nome, <nom>Kent Jones</nom> el Usono kaj
        <nom>Gilbert Ledon</nom> el <tld/>o,
        asertas, ke la&ubreve; statistikoj, 11&nbsp;% de la aviadilaj kra&scirc;oj okazas 
        pro miskomprenoj inter pilotoj kaj flughavenaj kontrolistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007159.html">Flugilhavaj
          vortoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o, Kolombio, Peruo la &scirc;akristoj pri drogoj, la
        banditoj, jam montras pli da povo ol la &scirc;tato
        <fnt>
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_r;&c_a;&c_z;&c_ib;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_B;&c_r;&c_a;&c_z;&c_i;&c_l;&c_i;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>el Brasil</trd>,
    <trd>Rep&uacute;blica Federal del Brasil</trd>
  </trdgrp>
  <trd lng="cs">Braz&iacute;lie <klr>(st&aacute;t)</klr></trd>
  <trd lng="da">Brasilien</trd>
  <trd lng="de">Brasilien</trd>
  <trd lng="en">Brazil</trd>
  <trd lng="es">Brasil</trd>
  <trd lng="fi">Brasilia</trd>
  <trd lng="fr">Br&eacute;sil</trd>
  <trd lng="he">&bet;&resh;&zayin;&yod;&lamed;</trd>
  <trd lng="hu">Braz&iacute;lia</trd>
  <trd lng="id">Brazil</trd>
  <trd lng="ja">&#12502;&#12521;&#12472;&#12523; [<ind>&#12406;&#12425;&#12376;&#12427;</ind>]</trd>
  <trd lng="nl">Brazili&euml;</trd>
  <trd lng="pl">Brazylia</trd>
  <trd lng="pt">Brasil</trd>
  <trd lng="ru">&c_B;&c_r;&c_a;&c_z;&c_i;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">Braz&iacute;lia <klr>(&scaron;t&aacute;t)</klr></trd>
  <trd lng="tp">ma Pasila</trd>
  <trd lng="uk">&c_B;&c_r;&c_a;&c_z;&c_ib;&c_l;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="brazil.0a">
  <kap><tld lit="b"/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        nia <tld lit="b"/>a samideano
        <fnt>
          <bib>Monato</bib>
          <aut>Claudinei Ribeiro</aut>
          <vrk><url ref="https://www.monato.be/2003/008125.html">Memore
          al Gilbert Ledon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni havas kelkajn germandevenajn <tld lit="b"/>ajn amikojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url
          ref="https://www.monato.be/2005/008798.php">Migrobirdoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="b"/>a ekvatora &gcirc;angalo
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="https://www.monato.be/2003/008015.html">Misa
          mondo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        fojfoje la muziko estas vigla, foje iom trena &#8208; la&ubreve; <tld
        lit="b"/>a stilo
        <fnt>
          <bib>Monato</bib>
          <aut>Grigori Arosev</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007106.html">&Ccirc;armaj
          kantoj por infanoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="b"/>a kajpirinjo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul </aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007121.html">Malbona
          venko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a Esperanto-Ligo
        <fnt>
          <bib>Monato</bib>
          <aut>Grigori Arosev</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007106.html">&Ccirc;armaj
          kantoj por infanoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_z;&c_ib;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">brasiler <klr>(adj.)</klr></trd>
  <trd lng="de">brasilianisch</trd>
  <trd lng="en">Brazilian<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="fr">br&eacute;silien<klr> (adj.)</klr></trd>
  <trd lng="ja">&#12502;&#12521;&#12472;&#12523;&#12398; [<ind>&#12406;&#12425;&#12376;&#12427;&#12398;</ind>]</trd>
  <trd lng="nl">Braziliaans</trd>
  <trd lng="pt">brasileiro <klr tip="amb"> (adj.)</klr></trd>
  <trd lng="tp">pi ma Pasila</trd>
</drv>

<drv mrk="brazil.0ano">
  <kap><tld lit="b"/>ano</kap>
  <snc>
    <dif>
      &Scirc;tatano de <tld/>o:
      <ekz>
        se ne ekzistus la &scirc;tataj entreprenoj <nom>Riocine</nom> kaj
        <nom>Petrobras</nom> 
        la <tld lit="b"/>anoj neniam povus spekti <tld lit="b"/>ajn
        filmojn en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Miguel Boeira Vianna</aut>
          <vrk><url ref="https://www.monato.be/2003/008118.html">El
          fatraso ekstremdekstremula</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sub nivelo de plena malri&ccirc;eco vivas 54&nbsp;% el la 175 milionoj da
        <tld lit="b"/>anoj
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="https://www.monato.be/2003/008040.html">Kvina
          povo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_z;&c_ib;&c_l;&c_je;&c_c;</trd>
  <trd lng="ca">brasiler <klr>(individu)</klr></trd>
  <trd lng="de">Brasilianer</trd>
  <trd lng="en">Brazilian<klr tip="ind"> (person)</klr></trd>
  <trd lng="fr">Br&eacute;silien<klr> (subst.)</klr></trd>
  <trd lng="ja">&#12502;&#12521;&#12472;&#12523;&#20154; [<ind>&#12406;&#12425;&#12376;&#12427;&#12376;&#12435;</ind>]</trd>
  <trd lng="nl">Braziliaan</trd>
  <trd lng="pt">brasileiro<klr tip="amb"> (subs.)</klr></trd>
  <trd lng="tp">jan pi ma Pasila</trd>
</drv>


</art>

