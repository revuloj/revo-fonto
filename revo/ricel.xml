<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: ricel.xml,v 1.1 2026/01/04 20:34:47 afido Exp $">
    <kap>
      <rad>ricel</rad>/i
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="ricel.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kasx.0i">Ka&scirc;i</ref> &scirc;tela&jcirc;on a&ubreve;
      <ref tip="super" cel="sxakr.0i">&scirc;akri</ref> per &scirc;tela&jcirc;o:
      <ekz>
        konscie a&ccirc;eti &scirc;tela&jcirc;on estas <tld/>ado, kaj anka&ubreve; ricelado estas krimo
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2010/010008.php"
              >Melkante la &scirc;taton</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        trovi <tld/>anton, kiu pagos plej altan prezon por &scirc;telitaj varoj
        <fnt>
          <vrk><url ref="https://archive.org/details/sociolekta-triopo/Gavaro/"
              >La Romp&scirc;telistoj</url></vrk>
          <lok>La verda slango 2006, p. 39</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kasx.sxtel0i">&scirc;tel(a&jcirc;)ka&scirc;i</ref>
    <trd lng="de">hehlen</trd>
    <trdgrp lng="en">
      <trd>fence <klr>(something)</klr></trd>,
      <trd>handle stolen goods</trd>
    </trdgrp>
    <trd lng="fr">receler</trd>
  </snc>
</drv>

  </art>

