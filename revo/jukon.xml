<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: jukon.xml,v 1.4 2022/03/21 17:23:46 afido Exp $">
    <kap>
      <rad>Jukon</rad>/o
    </kap>

<drv mrk="jukon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">rivero</ref>, 3120&#x202F;km longa, 
      fontanta en nordokcidenta Kanado kaj trafluinte <ref tip="malprt" cel="alask.A0o">Alasko</ref>n 
      enfluanta en lar&gcirc;a delto, formita kune kun Kuskokvimo, en Beringan Maron; 
      la <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">regiono</ref>
      &ccirc;irka&ubreve; la rivero:
      <ekz>
        trans-Beringe, tra-<tld/>e, la&ubreve;-Rok-Montare &gcirc;is Monument-Valo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Poemoj de Edwin Grobe &dash; Ok sonetoj el Navaholando</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        grandaj gregoj de libere migrantaj bizonoj en <tld/>o kaj la Nordokcidentaj
        Teritorioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=128195">Arbara bizono</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        parolantoj de la centra alaska jupika lingvo lo&gcirc;as en <klr>[&#x2026;]</klr> inter
        <klr>[la]</klr> valoj de riveroj <ind>Kuskokvimo</ind> kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=622673">Eskima skribo</url></lok>
        </fnt>.
        <trd lng="de">Kuskokwim-Fluss</trd>
        <trd lng="en">Kuskokwim River</trd>
        <trd lng="fr">Kuskokwim <klr>(fleuve)</klr></trd>
      </ekz>
    </dif>
  </snc>
    <trd lng="be">&c_Ju;&c_k;&c_a;&c_n; <klr>(&c_r;&c_a;&c_k;&c_a;)</klr></trd>
    <trd lng="de">Yukon-Fluss</trd>
    <trd lng="en">Yukon River</trd>
    <trd lng="fr">Yukon <klr>(fleuve)</klr></trd>
  <trd lng="uk">&#x42E;&#x43A;&#x43E;&#x43D;</trd>
</drv>

  </art>

