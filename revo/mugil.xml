<art mrk="$Id$">
<kap>
  <rad>mugil</rad>/o
</kap>
<drv mrk="mugil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mugile.0oj"><tld/>edoj</ref>
      (<trd lng="la">Mugil</trd>), tre &scirc;atata fi&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Meer&auml;sche</trd>
  <trd lng="en">mullet</trd>
  <trdgrp lng="es">
    <trd>mujol</trd>,
    <trd>lisa</trd>,
    <trd>galupe</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>muge</trd>,
    <trd>mulet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>muggine</trd>,
    <trd>cefalo</trd>
  </trdgrp>
  <trd lng="nl">harder</trd>
  <trdgrp lng="pt">
    <trd>tainha</trd>,
    <trd>mugem</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_je;&c_f;&c_a;&c_l;&c_mol;</trd>
</drv>
</art>
