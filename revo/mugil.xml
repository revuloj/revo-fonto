<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>mugil</rad>/o
</kap>
<drv mrk="mugil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mugil.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Mugil</trd>), tre &scirc;atata fi&scirc;o .
    </dif>
  </snc>
  <trd lng="de">Meer&auml;sche</trd>
  <trd lng="en">mullet</trd>
  <trdgrp lng="es">
    <trd>mujol</trd>,
    <trd>lisa</trd>,
    <trd>galupe</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>muge</trd>,
    <trd>mulet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>muggine</trd>,
    <trd>cefalo</trd>
  </trdgrp>
  <trd lng="nl">harder</trd>
  <trdgrp lng="pt">
    <trd>tainha</trd>,
    <trd>mugem</trd>
  </trdgrp>
</drv>
<drv mrk="mugil.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="perk.0oformaj">perkoformaj
      fi&scirc;oj</ref> (<trd lng="la">Mugilidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Meer&auml;schen <klr tip="ind">(Familie)</klr></trd>
  <trd lng="en">mullets <klr tip="ind">(family)</klr></trd>
</drv>
</art>
