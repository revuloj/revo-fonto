<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mugil</rad>/o
</kap>
<drv mrk="mugil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mugile.0oj"><tld/>edoj</ref>
      (<trd lng="la">Mugil</trd>), tre &scirc;atata fi&scirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_f;&c_a;&c_l;&c_mol;</trd>
  <trd lng="de">Meer&auml;sche</trd>
  <trd lng="en">mullet</trd>
  <trdgrp lng="es">
    <trd>mujol</trd>,
    <trd>lisa</trd>,
    <trd>galupe</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>muge</trd>,
    <trd>mulet</trd>
  </trdgrp>
  <trd lng="hu">p&eacute;rhal</trd>
  <trdgrp lng="it">
    <trd>muggine</trd>,
    <trd>cefalo</trd>
  </trdgrp>
  <trd lng="nl">harder</trd>
  <trdgrp lng="pt">
    <trd>tainha <klr tip="amb">(zool.)</klr></trd>,
    <trd>cambira <klr tip="amb">(zool.)</klr></trd>,
    <trd>curim&atilde; <klr tip="amb">(zool.)</klr></trd>,
    <trd>curum&atilde; <klr tip="amb">(zool.)</klr></trd>,
    <trd>mugem <klr tip="amb">(zool.)</klr></trd>,
    <trd>tapiara <klr tip="amb">(zool.)</klr></trd>,    
    <trd>targana <klr tip="amb">(zool.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_je;&c_f;&c_a;&c_l;&c_mol;</trd>
</drv>
</art>
