<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>paelj</rad>/o</kap>
<drv mrk="paelj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Hispana <ref tip="super" cel="plad.0o.mangx">plado</ref> konsistanta
      el fritita rizo kaj legomoj, jen kun frititaj
      fi&scirc;a&jcirc;oj, jen kun fritita viando:
      <ekz>
        kiel jam estas tradicio en la
        Esperanto-aran&gcirc;oj en la valencia regiono, &ccirc;iuj &ccirc;eestantoj havis la
        plezuron senpage ricevi <tld/>on kuiritan en granda ujo por 500
        homoj
        <fnt><aut>Augusto Casquero de la Cruz</aut>:
        <bib>LOdE</bib>,
        <lok>n-ro 8-9/1999</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_e;&c_l;&c_mol;&c_ja;</trd>
  <trd lng="de">Paella</trd>
  <trd lng="en">paella</trd>
  <trd lng="es">paella</trd>
  <trd lng="hu">paella</trd>
  <trd lng="nl">paella</trd>
  <trd lng="ru">&c_p;&c_a;&c_e;&c_l;&c_mol;&c_ja;</trd>
</drv>
</art>
