<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ies</rad>
</kap>
<drv mrk="ies.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Senfleksia pronomo, signifanta &leftquot;de iu&rightquot;, &leftquot;apartenanta al iu&rightquot;:
      <ekz>
        subite li a&ubreve;dis <tld/> vo&ccirc;on;
      </ekz>
      <ekz>
        kiu &gcirc;ojas pri ies malfeli&ccirc;o, tiu ne restos sen puno
        <fnt><bib>MT</bib>, <lok>&Sen; 17:5</lok></fnt>;
      </ekz>
      <ekz>
        esti objekto de <tld/> atento<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sie&gcirc;i <tld/> pordon
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_y;&c_j;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_ch;&c_y;&c_j;-&c_n;&c_je;&c_b;&c_u;&c_d;&c_z;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>jemandes</trd>,
    <trd>irgendjemandes</trd>
  </trdgrp>
  <trd lng="fr">de <ind>quelqu'un</ind></trd>
  <trd lng="hu">valakinek a</trd>
  <trd lng="nl">iemands</trd>
  <trd lng="pt">de <ind>algu&eacute;m</ind><klr tip="amb"> (propriedade)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_j;-&c_t;&c_o;</trd>,
    <trd>&c_ch;&c_je;&c_j;-&c_n;&c_i;&c_b;&c_u;&c_d;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">n&aring;gons</trd>
</drv>
</art>
