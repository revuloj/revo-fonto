<kap>
  <ofc>*</ofc>
  <rad>ies</rad>
  <fnt> <bib>UV</bib></fnt></kap>
<drv mrk="ies.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Senfleksia pronomo, signifanta &leftquot;de iu&rightquot;, &leftquot;apartenanta al iu&rightquot;:
      <ekz>
        subite &scirc;i eka&ubreve;dis <tld/> fortan malagrablan vo&ccirc;on
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu &gcirc;ojas pri ies malfeli&ccirc;o, tiu ne restos sen puno
        <fnt><bib>MT</bib>, <lok>&Sen; 17:5</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Marta</nom> ne rimarkis, ke &scirc;i estas objekto de <tld/> atento
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sie&gcirc;i <tld/> pordon
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_y;&c_j;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_ch;&c_y;&c_j;-&c_n;&c_je;&c_b;&c_u;&c_d;&c_z;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&ecaron;&ccaron;&iacute;</trd>,
    <trd>&ccaron;&iacute;si</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>jemandes</trd>,
    <trd>irgendjemandes</trd>
  </trdgrp>
  <trd lng="en">someone's</trd>
  <trd lng="fr">de <ind>quelqu'un</ind></trd>
  <trd lng="hu">valakinek a</trd>
  <trdgrp lng="ja">
    <trd>&#x3060;&#x308C;&#x304B;&#x306E;</trd>,
    <trd>&#x3042;&#x308B;&#x4EBA;&#x306E;  <pr>&#x3042;&#x308B;&#x3058;&#x3093;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">iemands</trd>
  <trd lng="pl">czyj&sacute;</trd>
  <trd lng="pt">de <ind>algu&eacute;m</ind><klr tip="amb">
(propriedade)</klr></trd>
  <trdgrp lng="ro">
    <trd>a</trd>,
    <trd>ai</trd>,
    <trd>al</trd>,
    <trd>ale cuiva</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_j;-&c_t;&c_o;</trd>,
    <trd>&c_ch;&c_je;&c_j;-&c_n;&c_i;&c_b;&c_u;&c_d;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nie&ccaron;&iacute;</trd>,
    <trd>vo&lcaron;a&ccaron;&iacute;</trd>,
    <trd>&ccaron;&iacute;si</trd>
  </trdgrp>
  <trd lng="sv">n&aring;gons</trd>
  <trd lng="tok">jan</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_i;&c_j;&c_s;&c_mol;</trd>,
    <trd>&c_ch;&c_i;&c_j;-&c_n;&c_je;&c_b;&c_u;&c_d;&c_mol;</trd>,
    <trd>&c_b;&c_o;&c_z;&c_n;&c_a;-&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_a;&c_z;&c_n;&c_a;-&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_t;&c_o;&c_z;&c_n;&c_a;-&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
