<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ies</rad></kap>
<drv mrk="ies.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Nevariebla pronomo, signifanta "de iu", "apartenanta al iu":
      <ekz>
        subite li a&ubreve;dis <tld/> vo&ccirc;on;
      </ekz>
      <ekz>
        kiu &gcirc;ojas pri ies malfeli&ccirc;o, tiu ne restos sen puno
        <fnt><bib>M</bib>, <lok>Sentencoj 17:5</lok></fnt>;
      </ekz>
      <ekz>
        esti objekto de <tld/> atento<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sie&gcirc;i <tld/> pordon
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>jemandes</trd>,
      <trd>irgendjemandes</trd>
    </trdgrp>
  </snc>
</drv>
</art>
