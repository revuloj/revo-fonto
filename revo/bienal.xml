<kap>
  <rad>bienal</rad>/o <fnt><bib>PGl</bib></fnt>
</kap>
<drv mrk="bienal.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="bienal.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Granda kultura <ref tip="super" cel="ven.kun0o">kunveno</ref>
      a&ubreve;
      <ref tip="super" cel="ekspoz1.0o.prezentado">ekspozicio</ref>
      normale okazanta &ccirc;iun duan jaron:
      <ekz>
        li estis denove invitita al la Venecia <tld lit="B"/>o en 1972
        <fnt>
        <aut>M. Menghini</aut>,
        <vrk>Pri Valeriano Trubbiani la a&ubreve;toro de
        &leftquot;Fu&gcirc;i el
        Babelturo&rightquot;</vrk>,
        <lok>soc.culture.esperanto, 2002-03-14</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="festival.0o">festivalo</ref>
    </refgrp>
    <trd lng="de">Biennale</trd>
    <trd lng="fr">biennale</trd>
    <trd lng="hu">bienn&aacute;l&eacute;</trd>
    <trd lng="nl">bi&euml;nnale</trd>
    <trd lng="pl">biennale</trd>
    <trdgrp lng="zh">
    <trd>&#x4E24;&#x5E74;&#x671F; <pr>li&#x1CE;ngni&#xE1;nq&#x12B;</pr></trd>,
    <trd>&#x4E24;&#x5E74;&#x671F;&#x95F4; <pr>li&#x1CE;ngni&#xE1;nq&#x12B;ji&#x101;n</pr></trd>,
    <trd>&#x53CC;&#x5E74;&#x5C55; <pr>shu&#x101;ngni&#xE1;nzh&#x1CE;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_ib;&c_je;&c_n;&c_a;&c_l;&c_je;</trd>
  <trd lng="cs">bien&aacute;le</trd>
  <trd lng="sk">bien&aacute;le</trd>
  <trd lng="uk">&#x431;&#x456;&#x454;&#x43D;&#x430;&#x43B;&#x435;</trd>
