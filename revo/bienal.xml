<drv mrk="bienal.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="bienal.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Granda kultura <ref tip="super" cel="ven.kun0o">kunveno</ref> a&ubreve;
      <ref tip="super" cel="ekspoz1.0o.prezentado">ekspozicio</ref>
      normale okazanta &ccirc;iun duan jaron:
      <ekz>
        li estis denove invitita al la Venecia <tld lit="B"/>o en 1972
        <fnt>
        <aut>M. Menghini</aut>,
        <vrk>Pri Valeriano Trubbiani la a&ubreve;toro de &leftquot;Fu&gcirc;i el
        Babelturo&rightquot;</vrk>,
        <lok>soc.culture.esperanto, 2002-03-14</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="festival.0o">festivalo</ref>
    </refgrp>
  </snc>
  <trd lng="fr">biennale</trd>
</drv>
