<vortaro>
<art mrk="$Id$">
<kap>
  <rad>alternator</rad>/o
</kap>
<drv mrk="altern3.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">ELE</uzo>
    <dif>
      Mekanika generatoro de alterna kurento per elektromagneta indukto.
    </dif>
  <ref tip="vid" cel="dinam.0o">dinamo</ref>  
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_mol;&c_t;&c_e;&c_r;&c_n;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_g;&c_je;&c_n;&c_e;&c_r;&c_a;&c_t;&c_a;&c_r; &c_z;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_a;&c_g;&c_a; &c_t;&c_o;&c_k;&c_u;</trd>
  </trdgrp>
  <trd lng="de">Wechselstromgenerator</trd>
  <trd lng="en">alternator</trd>
  <trd lng="es">alternador</trd>
  <trd lng="fr">alternateur</trd>
  <trd lng="hu">v&aacute;ltkoz&oacute; &aacute;ram&uacute; <ind>gener&aacute;tor</ind></trd>
  <trd lng="nl">alternator</trd>
  <trdgrp lng="pl">
    <trd>alternator</trd>,
    <trd>pr&aogonek;dnica pr&aogonek;du przemiennego</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_mol;&c_t;&c_je;&c_r;&c_n;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_g;&c_je;&c_n;&c_je;&c_r;&c_a;&c_t;&c_o;&c_r; &c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_o;&c_g;&c_o; &c_t;&c_o;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">altern&aacute;tor</trd>
</drv>
</art>
