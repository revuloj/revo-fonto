<drv mrk="altern3.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">ELE</uzo>
    <dif>
      Me&hcirc;anika <ref tip="dif" cel="genera2.0o">generatoro</ref> de
      alterna kurento per elektromagneta indukto:
      <ekz>
        1872 pluraj alzacaj industriaj societoj instalas al <nom>Belfort</nom> establojn, kiuj
        ebligos al ili konservi la aliron al la franca merkato: <klr>[&#8230;]</klr> Alzaca
        Societo de Mekanikaj Konstrua&jcirc;oj (lokomotivoj, motoroj kaj <tld/>oj...)
        i&gcirc;inta poste <klr>[&#8230;]</klr> <nom>Alstom</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=48212">Belfort</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="dinam.0o">dinamo</ref>  
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>alternigilo</ke>, <ke>alternelektrigilo</ke>. 
  </rim>

  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_mol;&c_t;&c_e;&c_r;&c_n;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_g;&c_je;&c_n;&c_e;&c_r;&c_a;&c_t;&c_a;&c_r; &c_z;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_a;&c_g;&c_a; &c_t;&c_o;&c_k;&c_u;</trd>
  </trdgrp>
  <trd lng="cs">altern&aacute;tor</trd>
  <trd lng="de">Wechselstromgenerator</trd>
  <trd lng="en">alternator</trd>
  <trd lng="es">alternador</trd>
  <trd lng="fr">alternateur</trd>
  <trd lng="he">&#1488;&#1463;&#1500;&#1456;&#1496;&#1462;&#1512;&#1456;&#1504;&#1464;&#1496;&#1493;&#1465;&#1512;</trd>
  <trd lng="hu">v&aacute;ltkoz&oacute; &aacute;ram&uacute; <ind>gener&aacute;tor</ind></trd>
  <trd lng="ja">&#20132;&#27969;&#30330;&#38651;&#27231;  <pr>&#12371;&#12358;&#12426;&#12421;&#12358;&#12399;&#12388;&#12391;&#12435;&#12365;</pr></trd>
  <trd lng="nl">alternator</trd>
  <trdgrp lng="pl">
    <trd>alternator</trd>,
    <trd>pr&aogonek;dnica pr&aogonek;du przemiennego</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_mol;&c_t;&c_je;&c_r;&c_n;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_g;&c_je;&c_n;&c_je;&c_r;&c_a;&c_t;&c_o;&c_r; &c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_o;&c_g;&c_o; &c_t;&c_o;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">altern&aacute;tor</trd>
