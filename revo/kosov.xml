<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Kosov</rad>/o
</kap>

<drv mrk="kosov.K0o">
  <kap>
    <tld/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="kosov.K0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref> kun &ccirc;efurbo Pri&scirc;tino.
      Teritorio de Respubliko Kosovo.
      Kelkaj &scirc;tatoj uzas nomon A&ubreve;tonoma Provinco Kosovo kaj Meto&hcirc;io.
      &Gcirc;i limas kun
      <ref tip="vid" cel="alban.A0ujo">Albanujo</ref>,
      <ref tip="vid" cel="makedon.NordM0ujo.GEOG">Nord-Makedonujo</ref>,
      <ref tip="vid" cel="monteneg.M0o.GEOG">Montenegro</ref>
      kaj <ref tip="vid" cel="serb.S0ujo.GEOG">Serbujo</ref>.
    </dif>
    <trd lng="bg">&c_K;&c_o;&c_s;&c_o;&c_v;&c_o;</trd>
    <trd lng="de">Kosovo</trd>
    <trd lng="en">Kosovo</trd>
    <trd lng="nl">Kosovo</trd>
    <trdgrp lng="pl">
      <trd>Kosowo</trd>
    </trdgrp>
    <trd lng="pt">Kosovo</trd>
    <trd lng="fr">Kosovo</trd>
  </snc>
</drv>

<drv mrk="kosov.0a">
   <kap><tld lit="k"/>a</kap>
   <snc mrk="kosov.0a.GOEG">
     <dif>
       Rilata al <tld/>o.
     </dif>
   </snc>
   <trdgrp lng="en">
     <trd>Kosovan</trd>,
     <trd>Kosovar</trd>
   </trdgrp>
   <trd lng="nl">Kosovaars</trd>
</drv>

<drv mrk="kosov.0ano">
   <kap><tld lit="k"/>ano</kap>
   <snc mrk="kosov.0ano.GEOG">
     <dif>
       Civitano de <tld/>o.
     </dif>
   </snc>
   <trdgrp lng="en">
     <trd>Kosovan</trd>,
     <trd>Kosovar</trd>
   </trdgrp>
   <trd lng="nl">Kosovaar</trd>
</drv>

</art>

