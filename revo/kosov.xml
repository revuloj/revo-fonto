<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kosov.xml,v 1.14 2023/12/03 09:58:59 revo Exp $">
<kap>
  <rad>Kosov</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="kosov.K0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="kosov.K0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&Scirc;tato</ref>
      en la suda <ref tip="malprt" cel="euxrop1.E0o.geog">E&ubreve;ropo</ref> kun &ccirc;efurbo Pri&scirc;tino;
      teritorio de Respubliko <tld/>o,
      limanta kun
      <ref tip="vid" cel="alban.A0ujo">Albanujo</ref>,
      <ref tip="vid" cel="makedon.NordM0ujo.GEOG">Nord-Makedonujo</ref>,
      <ref tip="vid" cel="monteneg.M0o.GEOG">Montenegro</ref>
      kaj <ref tip="vid" cel="serb.S0ujo.GEOG">Serbujo</ref>:
      <ekz>
        kelkaj &scirc;tatoj uzas la nomon A&ubreve;tonoma Provinco <tld/>o kaj Meto&hcirc;io;
      </ekz>
      <ekz>
        <tld/>o estis konkerita de serboj nur ekde la 12a&#x202F;-&#x202F;13a&#x202F;jc
        <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio: 
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>;
      </ekz>      
      <ekz>
        fronte al la serbaj terora&jcirc;oj en <tld/>o la albana gerilo, kaj nun anka&ubreve; NATO,
        sentis sin devigataj reagi per milit-rimedoj
        <fnt>
       <aut>Douglas Draper</aut>
          <vrk><url ref="&Monato;/2000/006834.php">Ne&ubreve;trala</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 4</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_K;&c_o;&c_s;&c_a;&c_v;&c_a;</trd>
    <trd lng="bg">&c_K;&c_o;&c_s;&c_o;&c_v;&c_o;</trd>
    <trd lng="de">Kosovo</trd>
    <trd lng="en">Kosovo</trd>
    <trd lng="nl">Kosovo</trd>
    <trdgrp lng="pl">
      <trd>Kosowo</trd>
    </trdgrp>
    <trd lng="pt">Kosovo</trd>
    <trd lng="fr">Kosovo</trd>
    <trdgrp lng="zh">
    <trd>&#x79D1;&#x7D22;&#x6C83; <pr>K&#x113;su&#x1D2;w&#xF2;</pr></trd>,
    <trd>&#x79D1;&#x7D22;&#x6C83; <pr>k&#x113;su&#x1D2;w&#xF2;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ja">&#x30B3;&#x30BD;&#x30DC;</trd>
</drv>

<drv mrk="kosov.0a">
  <kap><tld lit="k"/>a</kap>
  <snc mrk="kosov.0a.GOEG">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld lit="k"/>aj civitanoj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/007925.html">Voja&gcirc;impresoj, somero 2002: Kosovo anta&ubreve;eniras</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la elstara <tld lit="k"/>a poeto, kiun la popolo konas per la kromnomo
        <nom>Sat Nok&scaron;iki</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/007925.html">Voja&gcirc;impresoj, somero 2002: Kosovo anta&ubreve;eniras</url></vrk>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_s;&c_a;&c_w;&c_s;&c_k;&c_ib;</trd>
  <trdgrp lng="en">
     <trd>Kosovan</trd>,
     <trd>Kosovar</trd>
  </trdgrp>
  <trd lng="nl">Kosovaars</trd>
</drv>

<drv mrk="kosov.0ano">
  <kap><tld lit="k"/>ano</kap>
  <snc mrk="kosov.0ano.GEOG">
    <dif>
      Civitano de <tld/>o:
      <ekz>
        la albanaj <tld lit="k"/>anoj ja a&ubreve;dacis protesti kontra&ubreve; la serba regado
        <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio: 
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_s;&c_a;&c_v;&c_a;&c_r;</trd>
  <trdgrp lng="en">
     <trd>Kosovan</trd>,
     <trd>Kosovar</trd>
  </trdgrp>
  <trd lng="nl">Kosovaar</trd>
</drv>

</art>

