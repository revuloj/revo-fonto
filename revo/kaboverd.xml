<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaboverd.xml,v 1.17 2026/01/04 02:41:03 revo Exp $">
<kap><ofc>10</ofc>
  <rad>Kaboverd</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="kaboverd.K0o">
  <kap>
    <ofc>10</ofc><tld lit="K"/>o
  </kap>
  <mlg kod="ISO-3166">CV</mlg>
  <snc mrk="kaboverd.K0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="land.0o" lst="voko:&scirc;tatoj">Lando</ref>
      en la samnomaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantikaj</ref>
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">insuloj</ref>
      okcidente de norda <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref> Prajo,
      teritorio de Respubliko <tld/>o:
      <ekz>
        la Ru&gcirc;aj milvoj en <tld/>o estas (a&ubreve; pli bone estis) tre distingaj la&ubreve;
        morfologio, iome intermezaj kun la Nigraj milvoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16444">Ru&gcirc;a milvo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la Nacia flago de <tld/>o estis adoptita la 22-an de septembro 1992
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=349410">Flago de Kaboverdo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_K;&c_a;&c_b;&c_a;-&c_V;&c_e;&c_r;&c_d;&c_e;</trd>
    <trd lng="bg">&c_K;&c_a;&c_b;&c_o; &c_V;&c_je;&c_r;&c_d;&c_je;</trd>
    <trd lng="ca">Cap Verd</trd>
    <trd lng="de">Kap Verde</trd>
    <trd lng="en">Cape Verde</trd>
    <trd lng="fr">Cap-Vert</trd>
    <trd lng="hu">Z&ouml;ld-foki K&ouml;zt&aacute;rsas&aacute;g</trd>
    <trd lng="id">Tanjung Verde</trd>
    <trd lng="ja">&#12459;&#12540;&#12508;&#12505;&#12523;&#12487;</trd>
  <trdgrp lng="pl">
      <trd>Wyspy <ind>Zielonego Przyl&aogonek;dka</ind></trd>,
      <trd>Republika <ind>Zielonego Przyl&aogonek;dka</ind></trd>
    </trdgrp>
    <trd lng="pt">Cabo Verde</trd>
  <trdgrp lng="zh">
    <trd>&#20315;&#24471;&#35282; <pr>F&oacute;d&eacute;ji&#462;o</pr></trd>,
    <trd>&#32173;&#24503;&#35282; <pr>W&eacute;id&eacute;ji&#462;o</pr></trd>,
    <trd>&#20315;&#24471;&#35282; <pr>f&oacute;d&eacute;ji&#462;o</pr></trd>,
    <trd>&#20315;&#24471;&#35282;&#20849;&#21644;&#22269; <pr>f&oacute;d&eacute;ji&#462;og&ograve;ngh&eacute;gu&oacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kaboverd.0a">
  <kap>
    <ofc>10</ofc><tld lit="k"/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o, &gcirc;iaj lo&gcirc;antoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_b;&c_a;&c_v;&c_e;&c_r;&c_d;&c_e;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="pl">dot. Wysp Zielonego Przyl&aogonek;dka</trd>
</drv>

<drv mrk="kaboverd.0ano">
  <kap>
    <ofc>10</ofc><tld lit="k"/>ano
  </kap>
  <snc>
    <dif>
      Iu el <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_b;&c_a;&c_v;&c_e;&c_r;&c_d;&c_e;&c_ja;&c_n;&c_je;&c_c;</trd>
  <trd lng="pl">mieszkaniec Wysp Zielonego Przyl&aogonek;dka</trd>
</drv>
</art>

