<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kaboverd</rad>/o
</kap>

<drv mrk="kaboverd.0o">
  <kap>
    <tld lit="K"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="kaboverd.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la samnomaj <ref tip="vid"
      cel="atlant.0o.GEOG">Atlantikaj</ref> insuloj 
      okcidente de norda <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Prajo.
      Teritorio de Respubliko Kaboverdo.
      ISO-3166 kodo: CV.
    </dif>
    <trd lng="bg">&c_K;&c_a;&c_b;&c_o; &c_V;&c_je;&c_r;&c_d;&c_je;</trd>
    <trd lng="de">Kap Verde</trd>
    <trd lng="en">Cape Verde</trd>
    <trdgrp lng="pl">
      <trd>Wyspy <ind>Zielonego Przyl&aogonek;dka</ind></trd>,
      <trd>Republika <ind>Zielonego Przyl&aogonek;dka</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

