<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaboverd.xml,v 1.10 2019/02/11 17:10:24 revo Exp $">
<kap>
  <rad>Kaboverd</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="kaboverd.K0o">
  <kap>
    <tld lit="K"/>o
  </kap>
  <mlg kod="ISO-3166">CV</mlg>
  <snc mrk="kaboverd.K0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="land.0o" lst="voko:&scirc;tatoj">Lando</ref>
      en la samnomaj <ref tip="vid"
      cel="atlant.A0o.GEOG">Atlantikaj</ref> 
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">insuloj</ref> 
      okcidente de norda <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Prajo,
      teritorio de Respubliko <tld/>o:
      <ekz>
        la Ru&gcirc;aj milvoj en <tld/>o estas (a&ubreve; pli bone estis) tre distingaj la&ubreve;
        morfologio, iome intermezaj kun la Nigraj milvoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16444"
              >Ru&gcirc;a milvo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la Nacia flago de <tld/>o estis adoptita la 22-an de septembro 1992
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=349410"
              >Flago de Kaboverdo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="bg">&c_K;&c_a;&c_b;&c_o; &c_V;&c_je;&c_r;&c_d;&c_je;</trd>
    <trd lng="ca">Cap Verd</trd>
    <trd lng="de">Kap Verde</trd>
    <trd lng="en">Cape Verde</trd>
    <trd lng="fr">Cap-Vert</trd>
    <trd lng="hu">Z&ouml;ld-foki K&ouml;zt&aacute;rsas&aacute;g</trd>
    <trd lng="id">Tanjung Verde</trd>
    <trdgrp lng="pl">
      <trd>Wyspy <ind>Zielonego Przyl&aogonek;dka</ind></trd>,
      <trd>Republika <ind>Zielonego Przyl&aogonek;dka</ind></trd>
    </trdgrp>
    <trd lng="pt">Cabo Verde</trd>
</drv>

</art>

