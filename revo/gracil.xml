<vortaro>
<art mrk="$Id: gracil.xml,v 1.16 2021/07/28 13:35:36 revo Exp $">
<kap>
  <rad>gracil</rad>/a
</kap>
<drv mrk="gracil.0a">
  <kap><tld/>a</kap>
  <snc mrk="gracil.0a.maldika">
    <dif>
      Tre maldika, malforta pro malsano a&ubreve; pro patologiaj ka&ubreve;zoj:
      <ekz>
        <tld/>a bildo en spegulo; / la bild' kompatu min, kaj mi vin
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 89a, volumo 3a, p.
        142a</lok></fnt>!
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_ib;&c_z;&c_e;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_z;&c_mol;&c_m;&c_ib;&c_z;&c_a;&c_r;&c_n;&c_je;&c_l;&c_y;</trd>,
      <trd>&c_k;&c_a;&c_sh;&c_ch;&c_a;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="ca">escanyolit</trd>
    <trdgrp lng="fr">
      <trd>ch&eacute;tif</trd>,
      <trd>malingre</trd>
    </trdgrp>
  </snc>
  <snc mrk="gracil.0a.svelta">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="svelt.0a">svelta</ref>
    <trd lng="ca">abrinat</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>graciln&iacute;</trd>,
    <trd>nedorostl&yacute;</trd>,
    <trd>tenk&yacute;</trd>,
    <trd>&uacute;tl&yacute;</trd>,
    <trd>&scaron;t&iacute;hl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>grazil</trd>,
    <trd>zartgliedrig</trd>,
    <trd>feingliedrig</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>betegesen sov&aacute;ny</trd>,
    <trd>v&eacute;kony</trd>,
    <trd>cing&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">slank</trd>
  <trdgrp lng="pt">
   <trd>d&eacute;bil</trd>, 
   <trd>raqu&iacute;tico</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_r;&c_u;&c_p;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_o;&c_n;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>krehk&yacute;</trd>,
    <trd>&uacute;tly</trd>,
    <trd>&scaron;t&iacute;hly</trd>
  </trdgrp>
</drv>
</art>
