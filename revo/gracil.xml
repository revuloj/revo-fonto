<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gracil.xml,v 1.24 2025/09/02 22:28:44 revo Exp $">
<kap>
  <rad>gracil</rad>/a
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gracil.0a">
  <kap><tld/>a</kap>
  <snc mrk="gracil.0a.maldika">
    <dif>
      Tre maldika, kies korpopartoj aperas
      <ref tip="sin" cel="romp.0ebla">rompeblaj</ref>,
      <ref tip="sin" cel="fortik.mal0a">malfortikaj</ref>:
      <ekz>
        iliaj korpoj aspektas tiom <tld/>aj, ke oni havis la impreson, ke la plej
        malforta vento forblovus ilin
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2015/011355.php">Komunika&jcirc;o de Ikaro</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a bildo en spegulo / la bild' kompatu min, kaj mi vin
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 89a, volumo 3a, p.
        142a</lok></fnt>!
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="delika.0a.io_malbona">delikata<sncref/></ref>,
      <ref cel="fragil.0a">fragila</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_ib;&c_z;&c_e;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_z;&c_mol;&c_m;&c_ib;&c_z;&c_a;&c_r;&c_n;&c_je;&c_l;&c_y;</trd>,
      <trd>&c_k;&c_a;&c_sh;&c_ch;&c_a;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="ca">escanyolit</trd>
    <trdgrp lng="fr">
      <trd>ch&eacute;tif</trd>,
      <trd>malingre</trd>
    </trdgrp>
  </snc>
  <snc mrk="gracil.0a.svelta">
    <uzo tip="stl">EVI</uzo>
    <refgrp tip="dif">
      <ref cel="svelt.0a">svelta</ref>,
      <ref cel="graci.0a">gracia</ref>
    </refgrp>
    <trd lng="ca">abrinat</trd>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>malforta</ke>, <ke>malfortika</ke>,
    <ke>maldikega</ke>.
  </rim>

  <trdgrp lng="cs">
    <trd>graciln&iacute;</trd>,
    <trd>nedorostl&yacute;</trd>,
    <trd>tenk&yacute;</trd>,
    <trd>&uacute;tl&yacute;</trd>,
    <trd>&scaron;t&iacute;hl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>grazil</trd>,
    <trd>zartgliedrig</trd>,
    <trd>feingliedrig</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>betegesen sov&aacute;ny</trd>,
    <trd>v&eacute;kony</trd>,
    <trd>cing&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12363;&#32048;&#12356;  <pr>&#12363;&#12412;&#12381;&#12356;</pr></trd>,
    <trd>&#12365;&#12419;&#12375;&#12419;&#12394;</trd>
  </trdgrp>
  <trd lng="nl">slank</trd>
  <trdgrp lng="pl">
    <trd>cienki</trd>,
    <trd>wysmuk&lstroke;y</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>d&eacute;bil</trd>,
   <trd>raqu&iacute;tico</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_r;&c_u;&c_p;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_o;&c_n;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>krehk&yacute;</trd>,
    <trd>&uacute;tly</trd>,
    <trd>&scaron;t&iacute;hly</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_o;&c_n;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_je;&c_n;&c_d;&c_ib;&c_t;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#24494;&#22937; <pr>w&eacute;imi&agrave;o</pr></trd>,
    <trd>&#32654;&#22937; <pr>m&ecaron;imi&agrave;o</pr></trd>
  </trdgrp>
</drv>
</art>
