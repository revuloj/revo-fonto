<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: placx.xml,v 1.27 2022/11/19 23:30:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pla&ccirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="placx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="placx.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="konten.0igi.PSI">Kontentigi</ref> a&ubreve; 
      simpatiigi iun per sia &ccirc;armo:
      <ekz>
        lia heroeco tre <tld/>is al mi
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz>
      <ekz>
        kie <tld/>as al vi, tie lo&gcirc;i&gcirc;u
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 20:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rakontoj de fratoj <nom>Grimm</nom> <klr>[&#8230;]</klr> multe <tld/>as 
        al infanaj legantoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la oferto]</klr> tiel <tld/>is al la knabino, ke &scirc;i lasis sin trompi
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knaboj pla&ccirc;is al la bona homo kaj &ccirc;ar li ne havis proprajn infanojn, li
        prenis ilin en sian domon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej bona man&gcirc;o ne <tld/>as al mi sen salo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        al mi tre <tld/>as via senka&scirc;eco kaj bonkoreco
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu flatas al &ccirc;iu, <tld/>as al neniu
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        al Dio <tld/>u, sed sur diablon ne kra&ccirc;u
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        al &ccirc;iu besto <tld/>as &gcirc;ia nesto
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxarm.0i">&ccirc;armi</ref>,
      <ref cel="felicx.0igi">feli&ccirc;igi</ref>,
      <ref cel="gxoj.0igi">&gcirc;ojigi</ref>,
      <ref cel="log.0i">logi</ref>,
      <ref cel="tent.0i.inspiri_deziron">tenti<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_b;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="bo">&t_sa;&t_kata;&t_shabchu;&t_tsheg;&t_ma;&t_kha;&t_yata;&t_drengbu;&t_na;&t_tsheg;</trd>
  <trd lng="ca">agradar</trd>
  <trd lng="cs">l&iacute;bit se</trd>
  <trd lng="de">gefallen</trd>
  <trdgrp lng="en">
    <trd>please</trd>,
    <trd>be pleasing</trd>
  </trdgrp>
  <trd lng="es">agradar</trd>
  <trd lng="fr">plaire</trd>
  <trd lng="hu">tetszik</trd>
  <trd lng="io">plezar</trd>
  <trdgrp lng="nl">
    <trd>behagen</trd>,
    <trd>bevallen<klr tip="ind"> (behagen)</klr></trd>
  </trdgrp>
  <trd lng="oc">agradar</trd>
  <trd lng="pl">podoba&cacute; si&eogonek;</trd>
  <trd lng="pt">agradar</trd>
  <trd lng="ro">place</trd>
  <trd lng="ru">&c_n;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_d;&#x43E;&c_b;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
</drv>

<drv mrk="placx.0a">
  <kap><tld/>a</kap>
  <snc mrk="placx.0a.KOMUNE">
    <dif>
      Alloge <ref tip="vid" cel="agrabl.0a">agrabla</ref>, 
      <ref tip="vid" cel="cxarm.0a">&ccirc;arma</ref>:
      <ekz>
        ne faru ion ne <tld/>an al la estroj
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 29:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        ioma <tld/>a diketeco
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>&ccirc;e Lev Tolstoj en Jasnaja
        Poljana</lok></fnt>;
      </ekz>
      <ekz>
        vi povos admiri la <tld/>ajn bildojn, desegnitajn speciale por vi
        <fnt>
          <bib>LSB</bib>
          <lok>La Stranga Butiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i babilis kun la patrino en same <tld/>a maniero
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kanceliero estis <tld/>a homo
        <fnt>
          <bib>IKr</bib>
          <lok>Tri Musketeroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia aspekto estis <tld/>a kaj milda
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        pla&ccirc;a, alloga vida&jcirc;o sin prezentis al la okuloj de la lacaj viroj, vigligante
        ilin per promeso pri dol&ccirc;a ripozo
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_je;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_a;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_a;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_n;&c_a;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_y;&c_m;&c_p;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_da;&t_ga;&t_-a;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trd lng="cs">l&iacute;biv&yacute;</trd>
  <trdgrp lng="de">
    <trd>gef&auml;llig</trd>,
    <trd>angenehm</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pleasing</trd>,
    <trd>agreeable</trd>,
    <trd>pleasant</trd>,
    <trd>nice</trd>
  </trdgrp>
  <trd lng="es">agradable</trd>
  <trd lng="fr">plaisant</trd>
  <trdgrp lng="hu">
    <trd>tetszet&odblac;s</trd>,
    <trd>csinos</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aangenaam</trd>,
    <trd>aantrekkelijk</trd>,
    <trd>bekoorlijk</trd>,
    <trd>fijn</trd>,
    <trd>plezant</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>cacany</trd>,
    <trd>podobaj&aogonek;cy si&eogonek;</trd>,
    <trd>przyjemny</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>dr&abreve;gu&#539;</trd>,
    <trd>pl&abreve;cut</trd>,
    <trd>savurabil</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_i;&c_m;&c_p;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_i;&c_jeu;&#x43C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_i;&#x43C;&#x43F;&c_a;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_v;&c_a;&c_b;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="placx.0o">
  <kap><tld/>o</kap>
  <snc mrk="placx.0o.KOMUNE">
    <dif>
      Allogeco de tio, kio kontentigas ies deziron a&ubreve; guston:
      <ekz>
        se mi trovis <tld/>on en viaj okuloj, ne pasu preter vian
        sklavon
        <fnt><bib>MT</bib>, <lok>&Gen; 18:3</lok></fnt>;
      </ekz>
      <ekz>
        <ind>la&ubreve; via <tld/>o</ind>
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;APITRO XLIX</lok>
        </fnt>;
        <trd lng="de">wie du <ind>m&ouml;chtest</ind></trd>
        <trd lng="en">as you please</trd>
        <trd lng="nl">zoals je wil</trd>
      </ekz>
      <ekz>
        <ind>kiel pla&ccirc;as al vi</ind>
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 19:27</lok>
        </fnt>;
        <trd lng="de">wie es dir <ind>gef&auml;llt</ind></trd>
      </ekz>
      <ekz>
        la urso <klr>[&#8230;]</klr> permesis al la infanoj ludi kun si la&ubreve; ilia <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne havis la intencon <klr>[&#8230;]</klr> krei 
        la&ubreve; mia persona <tld/>o la tutan
        lingvon de l' kapo &gcirc;is la piedoj
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i akceptis &scirc;in kun <tld/>o, enamigite tiel de &scirc;ia beleco kiel de &scirc;ia diskreteco
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ajnas, ke mi iel trovis <tld/>on en la okuloj de la re&gcirc;o
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiom ajn oni penas, per forto <tld/>o ne venas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arbitr.0o">arbitro</ref>,
      <ref cel="trov.bon0o">bontrovo</ref>,
      <ref cel="favor.0o">favoro</ref>,
      <ref cel="gust.0o.karaktero">gusto<sncref/></ref>,
      <ref cel="prefer.0o.KOMUNE">prefero</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_ja;&c_z;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_y;&c_m;&c_p;&c_a;&c_t;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Gefallen</trd>
  <trdgrp lng="en">
    <trd>pleasure</trd>,
    <trd>delight</trd>
  </trdgrp>
  <trd lng="hu">tetsz&eacute;s</trd>
  <trdgrp lng="nl">
    <trd>behagen</trd>,
    <trd>welbehagen</trd>,
    <trd>genoegen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>upodobanie</trd>,
    <trd>uznanie</trd>,
    <trd>widzimisi&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pl&abreve;cere</trd>,
    <trd>stim&abreve;</trd>,
    <trd>capriciu</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_ja;&c_z;&c_n;&c_mol;</trd>,
    <trd>&c_s;&c_i;&c_m;&c_p;&c_a;&c_t;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_h;&c_v;&c_a;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_s;&c_i;&#x43C;&#x43F;&c_a;&c_t;&c_ib;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="placx.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="placx.0ema.vanta">
    <dif>
      Vola, deziranta <tld/>i,
      <ref tip="sin" cel="vant.0a">vanta</ref>:
      <ekz>
        viaj komizoj kiuj <tld/>eme sin turnadas kaj simile al papagoj 
        babila&ccirc;as pri flora&jcirc;oj kaj papilioj aspektas treege 
        ridinde  
        <fnt><bib>Marta</bib></fnt>                
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="galant1.0">galanta</ref>,
      <ref cel="koket.0a">koketa</ref>,
      <ref cel="vant.0a.iu">vanta<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>gefalls&uuml;chtig</trd>,
    <trd>eitel</trd>,
    <trd>kokett</trd>
  </trdgrp>
  <trd lng="es">seductor<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="fr">s&eacute;ducteur<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="hu">tetszelg&odblac;</trd>
  <trd lng="nl">behaagziek</trd>
  <trd lng="pl">chc&aogonek;cy si&eogonek; przypodoba&cacute; </trd>
  <trd lng="ro">dorind s&abreve; te rog</trd>
</drv>

<drv mrk="placx.laux0e">
  <kap>la&ubreve;<tld/>e</kap>
  <snc mrk="placx.laux0e.KOMUNE">
    <dif>
      Kiel oni volas, obeante nur la propran preferon:
      <ekz>
        &scirc;i ja estas libera en la vesperoj kaj povos iri danci kaj amuzi
        sin tute la&ubreve;<tld/>e
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        3a</lok></fnt>;
      </ekz>
      <ekz>
        estas privilegio de la homaro kiel de la bestaro, utiligi la&ubreve;<tld/>e siajn
        krurojn
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru la&ubreve;<tld/>e
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi pagu, kiom vi povas kaj volas poste vi la&ubreve;<tld/>e depagos posemajne
        <fnt>
          <bib>Tor</bib>
          <lok>III.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>nach <ind>Belieben</ind></trd>,
    <trd>nach <ind>Gefallen</ind></trd>,
    <trd>beliebig</trd>
  </trdgrp>
  <trd lng="en">as one pleases</trd>
  <trdgrp lng="es">
    <trd>a voluntad</trd>,
    <trd>al libre albedr&iacute;o<klr tip="amb"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&agrave; son gr&eacute;</trd>,
    <trd>&agrave; volont&eacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>naar wens</trd>,
    <trd>naar believen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wedle woli</trd>,
    <trd>wed&lstroke;ug upodobania</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>dup&abreve; plac</trd>,
    <trd>dupa bunul plac</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a; &c_v;&c_i;&c_b;&c_ib;&c_r;</trd>,
    <trd>&c_z;&c_a; &c_b;&c_a;&c_zh;&c_a;&#x43D;&#x43D;&#x44F;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="placx.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="placx.mal0i.KOMUNE">
    <dif>
      Ne kontentigi, estigi malagrablan senton &ccirc;e iu:
      <ekz>
        la lo&gcirc;ejo tute mal<tld/>is al li
        <fnt><bib>ElektFab</bib>, <lok>Fingreto</lok></fnt>;
      </ekz>
      <ekz>
        la afero forte mal<tld/>is al <nom>Abraham</nom> pro lia filo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 21:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estas nur ripeta&jcirc;o, kaj tio mal<tld/>is al li
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni mal<tld/>os al diversaj personoj, kiuj ne amas tiun ideon
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en 
          Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>!
      </ekz>
      <ekz>
        kelke da ostoj kun viando sur ili ne mal<tld/>us al mi
        <fnt>
          <bib>ElektFab</bib>
          <lok>La bremenaj muzikistoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia propono &scirc;ajnas vin mal<tld/>i
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>as nenio, se taksas pasio
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">missfallen</trd>
  <trdgrp lng="en">
    <trd>displease</trd>,
    <trd>be displeasing</trd>
  </trdgrp>
  <trd lng="es">desagradar</trd>
  <trd lng="fr">d&eacute;plaire</trd>
  <trd lng="io">desplezar</trd>
  <trd lng="pl">nie podoba&cacute; si&eogonek;</trd>
  <trd lng="ro">s&abreve; nu plac&abreve;</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je; &#x43F;&#x43E;&c_d;&#x43E;&c_b;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &#x43D;&c_je;&#x43F;&c_r;&c_i;&c_jeu;&#x43C;&#x43D;&c_i;&#x43C;</trd>,
    <trd>&c_v;&c_i;&#x43A;&#x43B;&c_i;&#x43A;&c_a;&c_t;&c_i; &#x43D;&c_je;&c_h;&c_ib;&c_t;&#x44C;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_a;&c_z;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="placx.okul0a">
  <kap>okul<tld/>a</kap>
  <snc mrk="placx.okul0a.KOMUNE">
    <dif>
      <tld lit="P"/>a por la okuloj, agrabla al vidanto:
      <ekz>
        &ccirc;irka&ubreve;rigardante kaj &gcirc;uante la okul<tld/>an vida&jcirc;on, li 
        eksentis facilkorecon
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 10a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="graci.0a">gracia</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>den Augen <ind>schmeicheln</ind>d</trd>,
    <trd>den Augen <ind>gef&auml;llig</ind></trd>
  </trdgrp>
  <trd lng="es">hermoso</trd>
  <trd lng="fr">joli</trd>
  <trdgrp lng="pl">
