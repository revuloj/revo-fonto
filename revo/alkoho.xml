<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>alkohol</rad>/o
</kap>
<drv mrk="alkoho.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      C<sub>2</sub>H<sub>5</sub>OH. Flua&jcirc;o
      ricevita el distilado de vino, greno kaj diversaj fermentinta&jcirc;oj.
    </dif>
  </snc>
  <trd lng="de">Alkohol</trd>
  <trd lng="es">alcohol</trd>
  <trd lng="fr">alcool</trd>
  <trd lng="it">alcool</trd> 
  <trd lng="tr">alkol</trd>
  <trd lng="nl">alcohol</trd>
  <trd lng="pt">&aacute;lcool</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_i;&c_r;&c_t;</trd>,
    <trd>&c_a;&c_l;&c_k;&c_o;&c_g;&c_o;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="alkoho.0ajo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Trinka&jcirc;o enhavanta pli a&ubreve; malpli da <tld/>o (= brando).
    </dif>
    <refgrp tip="sin">
      <ref cel="ebri.0igajxo">ebriiga&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>alkoholisches Getr&auml;nk</trd>,
    <trd>Spirituose</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>alcool</trd>,
    <trd>spiritueux</trd>
  </trdgrp>
  <trd lng="nl">alcoholische drank</trd>
  <trd lng="pt">bebida alco&oacute;lica</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_i;&c_r;&c_t;&c_n;&c_o;&c_je;</trd>,
    <trd>&c_a;&c_l;&c_k;&c_o;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_n;&c_a;&c_p;&c_i;&c_t;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="tr">i&ccedil;ki</trd>
</drv>

<drv mrk="alkoho.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <dif>
      Enmeti <tld/>on en ion:
      <ekz>
        delikate <tld/>igita krema&jcirc;o, kuko.
      </ekz>
    </dif>
    <trd lng="pt">alcoolizar</trd>
    <trd lng="es">alcoholizar</trd>
  </snc>
</drv>

<drv mrk="alkoho.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Malsana stato ka&ubreve;zita de trouzado de <tld/>a&jcirc;oj.
    </dif>
  </snc>
  <trd lng="de">Alkoholismus</trd>
  <trd lng="fr">alcoolisme</trd>
  <trd lng="es">alcoholismo</trd>
  <trd lng="it">alcolismo</trd>
  <trd lng="nl">alcoholisme</trd>
  <trd lng="pt">alcoolismo</trd>
  <trd lng="ru">&c_a;&c_l;&c_k;&c_o;&c_g;&c_o;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
