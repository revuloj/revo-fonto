<art mrk="$Id: bihar.xml,v 1.6 2024/01/02 19:21:54 revo Exp $">
<kap>
  <rad>Bihar</rad>/o <fnt><bib>LanLin</bib></fnt>
</kap>
<drv mrk="bihar.0o">
  <kap><tld/>o</kap>
  <snc mrk="bihar.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Gubernio en la orienta parto de
      <ref tip="malprt" cel="hind.H0ujo.POL">Hindujo</ref>:
      <ekz>
        en la &scirc;tato <tld/>o, &ccirc;e la limo kun Nepalo,
        <klr>...</klr> la sendependec-postuloj ampleksi&gcirc;as
        <fnt>
          <aut>M. Bulard, trad. V. Lutermano</aut>,
          <vrk><url ref="&Mondediplo;article1177.html">
          Barato re-alprenas sian rangon</url></vrk>,
          <lok>Le Monde diplomatique, 2007-01-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_ib;&c_g;&c_a;&c_r;</trd>
  <trd lng="fr">Bihar</trd>
  <trd lng="ja">&#x30D3;&#x30CF;&#x30FC;&#x30EB;&#x5DDE; 
  <pr>&#x30D3;&#x30CF;&#x30FC;&#x30EB;&#12375;&#12421;&#12358;</pr></trd>
  <trd lng="uk">&c_B;&c_ib;&c_h;&c_a;&c_r;</trd>
</drv>
