  <trdgrp lng="es">
    <trd>sustantivo</trd>, 
    <trd>nombre</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>substantif</trd>,
    <trd>nom</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>f&odblac;n&eacute;v</trd>,
    <trd>substantivum</trd>
  </trdgrp>
