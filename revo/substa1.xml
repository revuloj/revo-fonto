<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>substantiv</rad>/o
</kap>
<drv mrk="substa1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <dif>
      Vorto esprimanta personon, estulon, objekton aferon a&ubreve; ideon:
      <ekz>
        en Esperanto &ccirc;iu <tld/>o ricevas la fini&gcirc;on o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_z;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Substantiv</trd>,
    <trd>Nomen</trd>,
    <trd>Hauptwort</trd>
  </trdgrp>
  <trd lng="en">noun</trd>
  <trdgrp lng="es">
    <trd>sustantivo</trd>, 
    <trd>nombre</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>substantif</trd>,
    <trd>nom</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>f&odblac;n&eacute;v</trd>,
    <trd>substantivum</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>substantief</trd>,
    <trd>zelfstandig <ind>naamwoord</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;</trd>
</drv>
</art>
