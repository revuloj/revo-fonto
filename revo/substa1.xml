    <trd>Nomen</trd>,
    <trd>Hauptwort</trd>
  </trdgrp>
  <trd lng="en">noun</trd>
  <trd lng="es">sustantivo, nombre</trd>
  <trdgrp lng="nl">
    <trd>substantief</trd>,
    <trd>zelfstandig <ind>naamwoord</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je;</trd>
