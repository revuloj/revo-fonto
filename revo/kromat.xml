<art mrk="$Id: kromat.xml,v 1.16 2023/12/03 09:59:01 revo Exp $">
<kap>
  <rad>kromat</rad>/a
</kap>
<drv mrk="kromat.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Rilatanta al la koloroj de la spektro:
      <ekz>
        <tld/>a aberacio.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>chromatisch</trd>,
      <trd>farbig</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kromatikus</trd>,
      <trd>sz&iacute;n-</trd>,
      <trd>sz&iacute;nes</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Konsistanta el notoj kun intervalo de duontono:
      <ekz>
        <tld/>a gamo povas esti kun diesoj suprenirante a&ubreve; kun bemoloj
        malsuprenirante.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diaton.0a">diatona</ref>.
    </refgrp>
    <trd lng="de">chromatisch</trd>
    <trd lng="hu">kromatikus</trd>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_a;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="en">chromatic</trd>
  <trd lng="it">cromatico</trd>
  <trdgrp lng="ja">
    <trd>&#x8272;&#x5F69;&#x306E;  <pr>&#x3057;&#x304D;&#x3055;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x534A;&#x97F3;&#x968E;&#x306E;  <pr>&#x306F;&#x3093;&#x304A;&#x3093;&#x304B;&#x3044;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">chromatisch</trd>
  <trd lng="pl">chromatyczny</trd>
  <trd lng="pt">crom&aacute;tico</trd>
  <trd lng="ro">cromatic</trd>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&#x445;&#x440;&#x43E;&#x43C;&#x430;&#x442;&#x438;&#x447;&#x43D;&#x438;&#x439; (&#x43F;&#x440;&#x43E; &#x43A;&#x43E;&#x43B;&#x456;&#x440;</trd>,
    <trd>&#x437;&#x432;&#x443;&#x43A;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6613;&#x67D3; <pr>y&#xEC;r&#x1CE;n</pr></trd>,
    <trd>&#x4E0A;&#x8272; <pr>sh&#xE0;ngsh&#x1CE;i</pr></trd>,
    <trd>&#x67D3;&#x8272;&#x6027; <pr>r&#x1CE;ns&#xE8;x&#xEC;ng</pr></trd>
  </trdgrp>
</drv>
</art>
