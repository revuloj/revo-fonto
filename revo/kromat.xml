<kap>
  <rad>kromat</rad>/a
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="kromat.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Rilatanta al la koloroj de la spektro:
      <ekz>
        <tld/>a aberacio.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>chromatisch</trd>,
      <trd>farbig</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kromatikus</trd>,
      <trd>sz&iacute;n-</trd>,
      <trd>sz&iacute;nes</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Konsistanta el notoj kun intervalo de duontono:
      <ekz>
        <tld/>a gamo povas esti kun diesoj suprenirante a&ubreve; kun bemoloj
        malsuprenirante.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diaton.0a">diatona</ref>.
    </refgrp>
    <trd lng="de">chromatisch</trd>
    <trd lng="hu">kromatikus</trd>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_a;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">crom&agrave;tic</trd>
  <trd lng="en">chromatic</trd>
  <trd lng="es">crom&aacute;tico</trd>
  <trd lng="it">cromatico</trd>
  <trdgrp lng="ja">
    <trd>&#33394;&#24425;&#12398;  <pr>&#12375;&#12365;&#12373;&#12356;&#12398;</pr></trd>,
    <trd>&#21322;&#38899;&#38542;&#12398;  <pr>&#12399;&#12435;&#12362;&#12435;&#12363;&#12356;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">chromatisch</trd>
  <trd lng="pl">chromatyczny</trd>
  <trd lng="pt">crom&aacute;tico</trd>
  <trd lng="ro">cromatic</trd>
  <trd lng="ru">&c_h;&c_r;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_r;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j; (&c_p;&c_r;&c_o; &c_k;&c_o;&c_l;&c_ib;&c_r;</trd>,
    <trd>&c_z;&c_v;&c_u;&c_k;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26131;&#26579; <pr>y&igrave;r&#462;n</pr></trd>,
    <trd>&#19978;&#33394; <pr>sh&agrave;ngsh&#462;i</pr></trd>,
    <trd>&#26579;&#33394;&#24615; <pr>r&#462;ns&egrave;x&igrave;ng</pr></trd>
  </trdgrp>
</drv>
</art>
