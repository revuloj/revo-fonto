<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hejt</rad>/i
</kap>
<drv mrk="hejt.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Varmigi per tiucela aparato fornon, &ccirc;ambron, lo&gcirc;ejon:
      <ekz>
        en la vintro oni <tld/>as la fornojn;
        <fnt><bib>F</bib>, <lok>&FE; 16</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i kamenon;
      </ekz>
      <ekz>
        forno forte <tld/>ita de la bakisto
        <fnt><bib>MT</bib>, <lok>Ho&scirc;ea 7:4</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i lokomotivon;
      </ekz>
      <ekz>
        <klr>(abs.)</klr> la forno &ccirc;esas <tld/>i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tuj post la <ind><tld/>o</ind> la forno estis varmega
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
        <trd lng="fr">chauffe<klr tip="ind"> (action de chauffer)</klr></trd>
      </ekz>
      <ekz>
        la <ind>centra <tld/>ado</ind>
        <klr>(kun unu fajrujo kaj pluraj <tld/>otubaroj)</klr>.
        <trd lng="fr">chauffage central</trd>
        <trd lng="nl">centrale <ind>verwarming</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">heizen</trd>
  <trd lng="en">heat</trd>
  <trd lng="es">calentar</trd>
  <trd lng="fr">chauffer<klr tip="ind"> (tr., une machine, une maison)</klr></trd>
  <trd lng="nl">verwarmen</trd>
  <trd lng="pt">aquecer</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_a;&c_p;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="hejt.0ilo">
  <kap>
    <tld/>ilo
  </kap>
  <snc>
    <dif>
      Aparato, per kiu oni <tld/>as lo&gcirc;ejon:
      <ekz>
        <tld/>i apartamenton per gasaj <tld/>iloj.
      </ekz>
    </dif>
  </snc>
  <adm>
  Enkonduki "radiatoro" k referenci al gxi de
  cxi tie. <aut>MB</aut>
  </adm>
  <trd lng="es">calentador</trd>
  <trd lng="en">heater</trd>
  <trdgrp lng="fr">
    <trd>appareil de chauffage</trd>,
    <trd>calorif&egrave;re</trd>,
    <trd>chauffage<klr tip="ind"> (calorif&egrave;re)</klr></trd></trdgrp>
  <trd lng="pt">aquecedor</trd>
</drv>

<drv mrk="hejt.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Homo, kies metio estas <tld/>i lo&gcirc;ejon a&ubreve; ma&scirc;inon.
    </dif>
  </snc>
  <trd lng="de">Heizer</trd>
  <trdgrp lng="fr">
    <trd>chauffeur<klr tip="ind"> (celui qui chauffe une machine)</klr></trd>,
    <trd>chauffagiste</trd></trdgrp>
  <trd lng="nl">stoker</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_t;&c_o;&c_p;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_ch;&c_je;&c_g;&c_a;&c_r;</trd>
  </trdgrp>
</drv>
</art>
