<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>hejt</rad>/i
</kap>
<drv mrk="hejt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Varmigi per tiucela aparato fornon, &ccirc;ambron, lo&gcirc;ejon:
      <ekz>
        <tld/>i kamenon;
      </ekz>
      <ekz>
        <tld/>i apartamenton per gasaj <tld/>iloj;
      </ekz>
      <ekz>
        la forno &ccirc;esas <tld/>i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        forno forte <tld/>ita de la bakisto
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Ho&scirc;ea 7:4</lok>)</fnt>;
      </ekz>
      <ekz>
        <tld/>i lokomotivon;
      </ekz>
      <ekz>
        la centra <tld/>ado
        <klr>(kun unu fajrujo kaj pluraj <tld/>otubaroj)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">heizen</trd>
  <trd lng="en">heat</trd>
</drv>
<drv mrk="hejt.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kies metio estas <tld/>i lo&gcirc;ejon a&ubreve; ma&scirc;inon.
    </dif>
  </snc>
  <trd lng="de">Heizer</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
