<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hejt.xml,v 1.24 2022/03/20 15:11:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hejt</rad>/i
</kap>
<drv mrk="hejt.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Varmigi per tiucela aparato fornon, &ccirc;ambron, lo&gcirc;ejon:
      <ekz>
        en la vintro oni <tld/>as la fornojn;
        <fnt><bib>F</bib>, <lok>&FE; 16</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i kamenon;
      </ekz>
      <ekz>
        forno forte <tld/>ita de la bakisto
        <fnt><bib>MT</bib>, <lok>Ho&scirc;ea 7:4</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i lokomotivon;
      </ekz>
      <ekz>
        <klr>(abs.)</klr> la forno &ccirc;esas <tld/>i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tuj post la <ind><tld/>o</ind> la forno estis varmega
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
        <trd lng="fr">chauffe<klr tip="ind"> (action de chauffer)</klr>
        </trd>
      </ekz>
      <ekz>
        la <ind>centra <tld/>ado</ind>
        <klr>(kun unu fajrujo kaj pluraj <tld/>otubaroj)</klr>.
        <trd lng="br">tommerezh kreiz</trd>
        <trd lng="ca">calefacci&oacute; central</trd>
        <trd lng="fr">chauffage central</trd>
        <trd lng="hu">k&ouml;zponti <ind>f&udblac;t&eacute;s</ind></trd>
        <trd lng="nl">centrale <ind>verwarming</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_c;&c_ja;&c_p;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol;
    <klr>(&c_u; &c_p;&c_je;&c_ch;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="br">tomma&ntilde;<klr tip="ind"> (ul lojeiz, ur mekanik)
  </klr></trd>
  <trdgrp lng="ca">
    <trd>escalfar</trd>,
    <trd>encendre</trd>
  </trdgrp>
  <trd lng="de">heizen</trd>
  <trd lng="en">heat</trd>
  <trd lng="es">calentar</trd>
  <trd lng="fr">chauffer<klr tip="ind"> (tr., une machine, une maison)
  </klr></trd>
  <trd lng="hu">f&udblac;t</trd>
  <trdgrp lng="it">
  <trd>scaldare<klr tip="amb"> (con combustione)</klr></trd>, 
  <trd>accendere<klr tip="amb"> (un forno o sim.)</klr>
  </trd>
  </trdgrp>
  <trd lng="nl">verwarmen</trd>
  <trdgrp lng="pl">
    <trd>pali&cacute;<klr> w piecu</klr></trd>,
    <trd>opala&cacute;</trd>,
    <trd>grza&cacute;</trd>,
    <trd>ogrzewa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">aquecer<klr tip="amb"> (um forno, um recinto)</klr>
  </trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_a;&c_p;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x43E;&#x43F;&#x438;&#x442;&#x438; (&#x43F;&#x456;&#x447;)</trd>,
    <trd>&#x43E;&#x43F;&#x430;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x431;&#x456;&#x433;&#x440;&#x456;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x433;&#x440;&#x456;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="hejt.0ilo">
  <kap>
    <tld/>ilo
  </kap>
  <snc>
    <dif>
      Aparato, per kiu oni <tld/>as lo&gcirc;ejon:
      <ekz>
        <tld/>i apartamenton per gasaj <tld/>iloj.
      </ekz>
    </dif>
  </snc>
  <adm>
  Enkonduki "radiatoro" k referenci al gxi de
  cxi tie. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_a;&c_c;&c_ja;&c_p;&c_l;&c_ja;&c_l;&c_mol;&c_n;&c_ib;&c_k;
    </trd>,
    <trd>&c_r;&c_a;&c_d;&c_y;&c_ja;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_je;&c_ch;</trd>
  </trdgrp>
  <trd lng="bo">&#xF5A;&#xF0B;&#xF42;&#xFB3;&#xF7C;&#xF42;&#xF0B;</trd>
  <trd lng="br">tommerez</trd>
  <trdgrp lng="ca">
    <trd>estufa</trd>,
    <trd>calefactor</trd>,
    <trd>radiador</trd>
  </trdgrp>
  <trd lng="de">Heizung</trd>
  <trd lng="es">calentador</trd>
  <trd lng="en">heater</trd>
  <trdgrp lng="fr">
    <trd>appareil de chauffage</trd>,
    <trd>calorif&egrave;re</trd>,
    <trd>chauffage<klr tip="ind"> (calorif&egrave;re)</klr></trd>
  </trdgrp>
  <trd lng="hu">f&udblac;t&odblac;berendez&eacute;s</trd>
  <trd lng="it">stufa</trd>
  <trdgrp lng="pl">
    <trd>grzejnik</trd>,
    <trd>kaloryfer</trd>,
    <trd>piec<klr> grzewczy</klr></trd>
  </trdgrp>
  <trd lng="pt">aquecedor<klr tip="amb"> (aparelho)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_ch;&c_mol;</trd>,
    <trd>
    &c_o;&c_b;&c_o;&c_g;&c_r;&c_je;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;
    </trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43F;&#x430;&#x43B;&#x44E;&#x432;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439; &#x43F;&#x440;&#x438;&#x43B;&#x430;&#x434;</trd>,
    <trd>&#x43E;&#x431;&#x456;&#x433;&#x440;&#x456;&#x432;&#x430;&#x447;</trd>
  </trdgrp>
</drv>

<drv mrk="hejt.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Homo, kies metio estas <tld/>i lo&gcirc;ejon a&ubreve;
      ma&scirc;inon.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_l;&c_mol;&c_sh;&c_ch;&c_y;&c_k;</trd>
  <trd lng="br">paotr an <ind>tommerezh</ind></trd>
  <trd lng="ca">fogoner</trd>
  <trd lng="de">Heizer</trd>
  <trdgrp lng="fr">
    <trd>chauffeur<klr tip="ind"> (celui qui chauffe une machine)</klr>
    </trd>,
    <trd>chauffagiste</trd></trdgrp>
  <trd lng="hu">f&udblac;t&odblac;</trd>
  <trd lng="it">fuochista</trd>
  <trd lng="nl">stoker</trd>
  <trd lng="pl">palacz</trd>
  <trdgrp lng="pt">
   <trd>aquecedor<klr tip="amb"> (pessoa)</klr></trd>, 
   <trd>fornalheiro</trd>,
   <trd>foguista</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_t;&c_o;&c_p;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_ch;&c_je;&c_g;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&#x43F;&#x430;&#x43B;&#x44E;&#x432;&#x430;&#x447;</trd>
</drv>
</art>
