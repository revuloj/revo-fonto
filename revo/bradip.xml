<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc>
  <rad>bradip</rad>/o
</kap>
<drv mrk="bradip.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Centr-Amerika kaj Sud-Amerika mamulo (<trd
      lng="la">Bradypus</trd>),
      kiu pendas de arbobran&ccirc;oj
      per la hokaj ungegoj de siaj tre longaj dufingraj a&ubreve; trifingraj
      membroj, kaj sin movas tre malrapide.
    </dif>
  </snc>
  <trd lng="de">Dreifinger-Faultier</trd>
  <trd lng="en">three-toed sloth</trd>
  <trdgrp lng="fr">
    <trd>bradype</trd>
    <trd>a&iuml;</trd>
    <trd>paresseux<klr tip="ind"> (bradype)</klr></trd>
  </trdgrp>
</drv>
</art>

