<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc>
  <rad>bradip</rad>/o
</kap>
<drv mrk="bradip.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Centr-Amerika kaj Sud-Amerika <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> de 
      <ref tip="malprt" cel="artik.krom0uloj">kromartikuloj</ref> (<trd
      lng="la">Bradypus</trd>),
      kiu pendas de arbobran&ccirc;oj
      per la hokaj ungegoj de siaj tre longaj dufingraj a&ubreve;
      trifingraj
      membroj, kaj sin movas tre malrapide.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>peres&oacute;s</trd>,
    <trd>peres&oacute;s de tres dits</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Dreifinger-Faultier</trd>,
     <trd>Faultier</trd>
  </trdgrp>
  <trd lng="en">three-toed sloth</trd>
  <trdgrp lng="fr">
    <trd>bradype</trd>
    <trd>a&iuml;</trd>
    <trd>paresseux<klr tip="ind"> (bradype)</klr></trd>
  </trdgrp>
  <trd lng="hu">lajh&aacute;r</trd>
  <trd lng="pl">leniwiec</trd>
</drv>
</art>

