<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>karikatur</rad>/o
</kap>
<drv mrk="karika.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Groteska desegna&jcirc;o, prezentanta la &ccirc;efajn trajtojn de iu facile
      rekoneblajn sed malbeligitajn kaj difektitajn per komika troigo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_y;&c_k;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="ca">caricatura</trd>
  <trd lng="de">Karikatur</trd>
  <trdgrp lng="en">
    <trd>caricature</trd>
  </trdgrp>
  <trd lng="es">caricatura</trd>
  <trd lng="fr">caricature</trd>
  <trd lng="hu">karikat&uacute;ra</trd>
  <trd lng="nl">karikatuur</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_i;&c_k;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="sv">karikatyr</trd>
</drv>

<drv mrk="karika.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Desegni ies viza&gcirc;on per <tld/>o.
    </dif>
  </snc>
  <trd lng="en">caricature</trd>
  <trd lng="fr">caricaturer</trd>
  <trd lng="de">karikieren</trd>
  <trd lng="hu">karik&iacute;roz</trd>
</drv>
</art>
