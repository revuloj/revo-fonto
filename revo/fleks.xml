<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fleks</rad>/i
</kap>
<drv mrk="fleks.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Kurbigi, arkigi:
      <ekz>
        <tld/>i ferfadenon;
      </ekz>
      <ekz>
        la feino <tld/>is flanken la bran&ccirc;ojn
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        &Gcirc;ardeno de la paradizo</vrk>)</fnt>;
      </ekz>
      <ekz><tld/>u arbon dum &gcirc;ia juneco
        <fnt><bib>PrV</bib></fnt>
        <klr>(ne lasu pasi la &gcirc;ustan momenton!)</klr></ekz>
      <ekz>
        <tld/>i la korpon, la brakon;
      </ekz>
      <ekz>
        li malrapide pa&scirc;is, <tld/>ita de malespero;
      </ekz>
      <ekz>
        (mult)<tld/>ita <klr>(multkurba, serpentuma)</klr> vojo<fnt>Z</fnt>.
      </ekz>  
    </dif>
    <trdgrp lng="de">
      <trd>biegen</trd>,
      <trd>beugen</trd>,
      <trd>kr&uuml;mmen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Cedigi, obeigi:
      <ekz>
        <tld/>i la junajn spiritojn sub disciplino;
      </ekz>
      <ekz>
        <tld/>i la venkiton sub severa le&gcirc;o;
      </ekz>
      <ekz>
        vian obstinecon mi <tld/>os
        (<refgrp tip="sin">
          <ref cel="humil.0igi">humiligi</ref>,
          <ref cel="fier">malfierigi</ref>)
        </refgrp>;
      </ekz>
      <ekz>
        <tld/>i la lingvon la&ubreve; sia gusto. 
      </ekz>
    </dif>
    <trd lng="de">beugen</trd>
  </snc>
  <trd lng="en">bend</trd>
  <trdgrp lng="fr">
    <trd>fl&eacute;chir<klr tip="ind"> (tr.)</klr></trd>,
    <trd>infl&eacute;chir</trd>,
    <trd>faire <ind>c&eacute;der</ind></trd>,
    <trd>plier<klr tip="ind"> (tr.)</klr></trd></trdgrp> 
</drv>
<drv mrk="fleks.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <snc num="1">
    <dif>
      <tld lit="F"/>ita. 
    </dif>
    <adm>
    Mankas en PIV1. Ekzemploj bonvenaj. <aut>MB</aut>
    </adm>
  </snc>
  <snc num="2">
    <dif>
      <ref tip="dif" cel="fleks.0igxema"><tld lit="F"/>i&gcirc;ema</ref>:
      <ekz>
        <tld/>aj vermoj
        <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>aj brakoj
        <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>.
      </ekz> 
    </dif>
    <trd lng="fr">flexible</trd>
  </snc>
</drv>
<drv mrk="fleks.0o0ajxo">
  <kap><tld/>o, <tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld lit="F"/>ita parto de io:
      <ekz>
        la rivero havas multe da <tld/>oj
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Malgranda Niko kaj granda Niko</vrk>)</fnt>;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;oj de la vojo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Biegung</trd>
  <trdgrp lng="fr">
    <trd>coude<klr tip="ind"> (tournant)</klr></trd>,
    <trd>sinuosit&eacute;</trd>,
    <trd>tournant</trd></trdgrp> 
  <trd lng="ru">&c_i;&c_z;&c_g;&c_i;&c_b;</trd>
</drv>
<drv mrk="fleks.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Tia, ke oni povas &gcirc;in <tld/>i (p.p. io), a&ubreve; lin cedigi 
      (p.p. iu):
      <ekz>
        kano estas <tld/>ebla
        <klr>(<refgrp tip="vid">
          <ref cel="mol.0a">mola</ref>,
          <ref cel="elast.0a">elasta</ref>
        </refgrp>)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>ebla lingvo, stilo; 
      </ekz>
      <ekz>
        <ind>ne<tld/>ebla</ind> karaktero
        <klr>(<ref cel="fer.0a">fera</ref>)</klr>.
        <trd lng="fr">inflexible</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>biegsam</trd>,
    <trd>beugsam <klr>(karaktero)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>flexible<klr tip="ind"> (adj.)</klr></trd>,
    <trd>souple</trd></trdgrp> 
  <trd lng="ru">&c_g;&c_i;&c_b;&c_k;&c_i;&c_j;</trd>
</drv>
<drv mrk="fleks.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc num="1">
    <dif>
      Kurbi&gcirc;i:
      <ekz>pli bone <tld/>i&gcirc;i ol rompi&gcirc;i
        <fnt><bib>PrV</bib></fnt></ekz>
      <ekz>
        <tld/>i&gcirc;i sub peza &scirc;ar&gcirc;o;
      </ekz>
      <ekz>
        bran&ccirc;oj <tld/>i&gcirc;as pro pezo de la fruktoj;
      </ekz>
      <ekz>
        tie <tld/>i&gcirc;as la vojo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>sich <ind>biegen</ind></trd>,
      <trd>sich <ind>beugen</ind></trd>,
      <trd>sich <ind>kr&uuml;mmen</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_s;&c_o;&c_g;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_i;&c_z;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_i;&c_z;&c_o;&c_g;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Cedi, submeti&gcirc;i:
      <ekz>
        <tld/>i&gcirc;i anta&ubreve; la forto sub la destino;
      </ekz>
      <ekz>
        lia kura&gcirc;o <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        E-o kreskis regule, neniam <tld/>i&gcirc;ante
        <klr>(deklini&gcirc;ante, misformi&gcirc;ante)</klr> a&ubreve;
        kripli&gcirc;ante<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>beugen</ind></trd>
  </snc>
  <trdgrp lng="fr">
    <trd>fl&eacute;chir<klr tip="ind"> (intr.)</klr></trd>,
    <trd>c&eacute;der<klr tip="ind"> (accepter)</klr></trd>,
    <trd>s'<ind>infl&eacute;chir</ind></trd>,
    <trd>tourner<klr tip="ind"> (pr&eacute;senter un tournant)</klr></trd>,
    <trd>plier<klr tip="ind"> (intr.)</klr></trd></trdgrp>
</drv>
<drv mrk="fleks.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      Movo, per kiu io <tld/>i&gcirc;as; formo de io <tld/>ita:
      <ekz>
        <tld/>i&gcirc;o de risorto;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>i&gcirc;o de la rivero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Biegung</trd>,
    <trd>Beugung</trd>,
    <trd>Kr&uuml;mmung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fl&eacute;chissement</trd>,
    <trd>flexion<klr tip="ind"> (fait de fl&eacute;chir)</klr></trd>,
    <trd>inflexion</trd></trdgrp> 
  <trd lng="ru">&c_i;&c_z;&c_g;&c_i;&c_b;</trd>
</drv>
<drv mrk="fleks.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc>
    <dif>
      Kiu facile <tld/>i&gcirc;as; facile <tld/>ebla:
      <ekz>
        longa <tld/>i&gcirc;ema kolo de cigno<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>flexible</trd>,
    <trd>accommodant</trd>,
    <trd>mall&eacute;able</trd>,
    <trd>souple</trd></trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_b;&c_k;&c_i;&c_j;</trd>
</drv>
<drv mrk="fleks.genu0i">
  <kap>genu<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Surgenui&gcirc;i:
      <ekz>
        anta&ubreve; Li genu<tld/>os &ccirc;iuj forirantaj al la tombo
        <fnt><bib>MT</bib>, <lok>&Psa; 22:29</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
  PIV1 mencias tiun verbon nek sub fleks, nek sub genu. Cetere
  gxi uzas aliajn derivajxojn de genu (genui, ~igxi), dum PV
  atribuas ambaux sencojn al "surgenuigxi"... Cetere staras la
  demando, cxu "genufleksi" signifas simple "fleksi genuon" (en
  signo de respekto) aux "starigxi sur la genuoj". En la koncerna
  ekz-o mi supozas, ke temas pri la dua senco, sed la samforma
  derivajxo en la franca (genuflexion) montras la unuan sencon.
  <aut>MB</aut>
  </adm>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol; &c_k;&c_o;&c_l;&c_je;&c_n;&c_i;</trd>
</drv>
</art>
