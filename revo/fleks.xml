<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fleks</rad>/i
</kap>
<drv mrk="fleks.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fleks.0i.kurbigi">
    <dif>
      Kurbigi, arkigi:
      <ekz>
        <tld/>i ferfadenon;
      </ekz>
      <ekz>
        la feino <tld/>is flanken la bran&ccirc;ojn
        <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la paradizo</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>u arbon dum &gcirc;ia juneco
        <fnt><bib>PrV</bib></fnt>
        <klr>(ne lasu pasi la &gcirc;ustan momenton!)</klr>
      </ekz>
      <ekz>
        <tld/>i la korpon, la brakon;
      </ekz>
      <ekz>
        li malrapide pa&scirc;is, <tld/>ita de malespero;
      </ekz>
      <ekz>
        (mult)<tld/>ita <klr>(multkurba, serpentuma)</klr>
        vojo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>biegen</trd>,
      <trd>beugen</trd>,
      <trd>kr&uuml;mmen</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>buigen</trd>,
      <trd>krommen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fleks.0i.cedigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Cedigi, obeigi:
      <ekz>
        <tld/>i la junajn spiritojn sub disciplino;
      </ekz>
      <ekz>
        <tld/>i la venkiton sub severa le&gcirc;o;
      </ekz>
      <ekz>
        vian obstinecon mi <tld/>os
        <klr>
        <refgrp tip="vid">
          (<ref cel="humil.0igi">humiligi</ref>,
          <ref cel="fier">malfierigi</ref>)
        </refgrp>
        </klr>;
      </ekz>
      <ekz>
        <tld/>i la lingvon la&ubreve; sia gusto.
      </ekz>
    </dif>
    <trd lng="de">beugen</trd>
    <trd lng="nl">buigen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_g;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">bend</trd>
  <trdgrp lng="fr">
    <trd>fl&eacute;chir<klr tip="ind"> (tr.)</klr></trd>,
    <trd>arquer</trd>,
    <trd>courber</trd>,
    <trd>couder</trd>,
    <trd>infl&eacute;chir</trd>,
    <trd>faire <ind>c&eacute;der</ind></trd>,
    <trd>faire <ind>plier</ind></trd>,
    <trd>plier<klr tip="ind"> (tr.)</klr></trd>,
    <trd>ployer<klr tip="ind"> (tr.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="fleks.0o">
  <kap><tld/>o</kap>
  <snc mrk="fleks.0o.0ita_parto">
    <dif>
      <tld lit="F"/>ita parto de io:
      <ekz>
        la rivero havas multe da <tld/>oj
        <fnt><bib>Fab1</bib>, <lok>Malgranda Niko kaj granda
        Niko</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="fleks.0ajxo"><tld/>a&jcirc;o</ref>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_g;&c_ib;&c_n;</trd>,
      <trd>&c_z;&c_g;&c_ib;&c_n;</trd>
    </trdgrp>
    <trd lng="de">Biegung</trd>
    <trdgrp lng="fr">
      <trd>coude<klr tip="ind"> (tournant)</klr></trd>,
      <trd>pli<klr tip="ind"> (partie fl&eacute;chie de qc)</klr></trd>
      <trd>infl&eacute;chissement</trd>
      <trd>inflexion<klr tip="ind"> (partie fl&eacute;chie de qc)</klr></trd>
      <trd>tournant</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bocht</trd>,
      <trd>kromming</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_z;&c_g;&c_i;&c_b;</trd>,
      <trd>&c_s;&c_g;&c_i;&c_b;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fleks.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="fort.0o.FIZ">Forto</ref>, kiu <tld/>as.
    </dif>
    <trd lng="fr">flexion<klr tip="ind"> (phys.)</klr></trd>
  </snc>
  <snc mrk="fleks.0o.ago0i">
    <dif>
      Ago <tld/>i.
    </dif>
    <rim>
      &Ccirc;i tiu signifo estas postulata de la
      radika karaktero, kaj supozeble tial &gcirc;i
      estis enkondukita en <fnt><bib>PIV2</bib></fnt>,
      sed &gcirc;i restas neuzata.
    </rim>
  </snc>
</drv>

<drv mrk="fleks.0a">
  <kap><tld/>a</kap>
  <snc mrk="fleks.0a.0orilata">
    <uzo tip="stl">RAR</uzo>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a momanto, rigideco
        <fnt>
        <aut>J. Werner</aut>
        <vrk>Terminaro de betono kaj de betonistaj laboroj</vrk>
        <lok><url ref="http://eventoj.freeweb.hu/steb/ingxenierado/konstruado/beton-termin.htm">http://eventoj.freeweb.hu/steb/ingxenierado/konstruado/beton-termin.htm</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">de <ind>flexion</ind></trd>
  </snc>
  <snc mrk="fleks.0a.0igxema">
    <ref tip="dif" cel="fleks.0igxema"><tld/>i&gcirc;ema</ref>
    <ekz>
      <tld/>aj vermoj
      <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>;
    </ekz>
    <ekz>
      <tld/>aj brakoj
      <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>.
    </ekz>
    <trd lng="be">&c_g;&c_n;&c_u;&c_t;&c_k;&c_ib;</trd>
    <trd lng="fr">flexible<klr tip="ind"> (adj.)</klr></trd>
    <trd lng="nl">buigzaam</trd>
    <trd lng="ru">&c_g;&c_i;&c_b;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="fleks.0a.0ita">
    <fnt><bib>PV</bib></fnt>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <tld lit="F"/>ita.
    </dif>
    <adm>
      Tute mankas ekzemploj de tiu senco. Nur PV gxin mencias.
      <aut>MB</aut>
    </adm>
    <trd lng="be">&c_s;&c_a;&c_g;&c_n;&c_u;&c_t;&c_y;</trd>
    <trd lng="nl">gebogen</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_n;&c_u;&c_t;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_o;&c_g;&c_n;&c_u;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="fleks.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>flexion<klr tip="ind"> (action de courber)</klr></trd>,
    <trd>arcure<klr tip="ind"> (action de fl&eacute;chir une branche)</klr></trd>,
    <trd>courbure<klr tip="ind"> (action de courber)</klr></trd>,
    <trd>coudage</trd>,
    <trd>inflexion<klr tip="ind"> (action d'infl&eacute;chir)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="fleks.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <ref tip="dif" cel="fleks.0o.0ita_parto"><tld/>o <sncref/></ref>
    <ekz>
      la <tld/>a&jcirc;oj de la vojo
      <fnt><bib>PV</bib></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_g;&c_ib;&c_n;</trd>,
    <trd>&c_z;&c_g;&c_ib;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Biegung</trd>
  <trdgrp lng="fr">
    <trd>coude<klr tip="ind"> (tournant)</klr></trd>,
    <trd>pli<klr tip="ind"> (partie fl&eacute;chie de qc)</klr></trd>
    <trd>infl&eacute;chissement</trd>
    <trd>inflexion<klr tip="ind"> (partie fl&eacute;chie de qc)</klr></trd>
    <trd>tournant</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bocht</trd>,
    <trd>kromming</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_g;&c_i;&c_b;</trd>,
    <trd>&c_s;&c_g;&c_i;&c_b;</trd>
  </trdgrp>
</drv>

<drv mrk="fleks.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Kiun oni povas <tld/>i:
      <ekz>
        kano estas <tld/>ebla;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>ebla lingvo, stilo;
      </ekz>
      <ekz>
        <ind>ne<tld/>ebla</ind> karaktero
        <klr>(<ref cel="fer.0a">fera</ref>)</klr>.
        <trdgrp lng="fr">
          <trd>inflexible</trd>
          <trd>intransigeant</trd>,
          <trd>intraitable</trd>,
          <trd>implacable</trd>,
          <trd>irr&eacute;ductible</trd>,
          <trd>rigide</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mol.0a">mola</ref>,
      <ref cel="elast.0a">elasta</ref>
    </refgrp>
    <rim>
      Ial neniu vortaro a&ubreve;dacas mencii la Fundamentan
      ekzemplon:
      <ekz>
        &scirc;talo estas <tld/>ebla, sed fero ne estas <tld/>ebla
        <fnt><bib>F</bib><lok>&FE; 41</lok></fnt>.
      </ekz>
      Kabe preferas diri
      <ekz>
        fero estas <tld/>ebla, sed &scirc;tono ne estas <tld/>ebla
        <fnt><bib>VdE</bib></fnt>.
      </ekz>
    </rim>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_u;&c_t;&c_k;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>biegsam</trd>,
    <trd>beugsam <klr>(karaktero)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>flexible<klr tip="ind"> (adj.)</klr></trd>,
    <trd>souple</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>buigzaam</trd>,
    <trd>soepel</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_b;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="fleks.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="fleks.0igxi.kurbigxi">
    <dif>
      Kurbi&gcirc;i:
      <ekz>
        pli bone <tld/>i&gcirc;i ol rompi&gcirc;i
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ekz>
        <tld/>i&gcirc;i sub peza &scirc;ar&gcirc;o;
      </ekz>
      <ekz>
        bran&ccirc;oj <tld/>i&gcirc;as pro pezo de la fruktoj;
      </ekz>
      <ekz>
        tie <tld/>i&gcirc;as la vojo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_v;&c_y;&c_g;&c_ib;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_z;&c_g;&c_ib;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>sich <ind>biegen</ind></trd>,
      <trd>sich <ind>beugen</ind></trd>,
      <trd>sich <ind>kr&uuml;mmen</ind></trd>
    </trdgrp>
    <trd lng="nl">buigen</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_s;&c_o;&c_g;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_i;&c_z;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_i;&c_z;&c_o;&c_g;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fleks.0igxi.cedi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Cedi, submeti&gcirc;i:
      <ekz>
        <tld/>i&gcirc;i anta&ubreve; la forto sub la destino;
      </ekz>
      <ekz>
        lia kura&gcirc;o <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        Esperanto kreskis regule, neniam <tld/>i&gcirc;ante
        <klr>(deklini&gcirc;ante, misformi&gcirc;ante)</klr> a&ubreve;
        kripli&gcirc;ante<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_g;&c_ib;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_s;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_c;&c_a; <klr tip="amb">(&c_p;&c_je;&c_r;&c_a;&c_d; &c_k;&c_ib;&c_m;&c_s;&c_mol;&c_c;&c_ib;/&c_ch;&c_y;&c_m;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>,
      <trd>&c_s;&c_a;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">sich <ind>beugen</ind></trd>
    <trd lng="nl">zich <ind>buigen</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_p;&c_je;&c_r;&c_je;&c_n;., &c_p;&c_je;&c_r;&c_je;&c_d; &c_k;&c_je;&c_m;/&c_ch;&c_je;&c_m;-&c_l;.)</klr></trd>,
      <trd>&c_u;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>fl&eacute;chir<klr tip="ind"> (intr.)</klr></trd>,
    <trd>c&eacute;der<klr tip="ind"> (accepter)</klr></trd>,
    <trd>se <ind>couder</ind></trd>,
    <trd>s'<ind>infl&eacute;chir</ind></trd>,
    <trd>tourner<klr tip="ind"> (pr&eacute;senter un tournant)</klr></trd>,
    <trd>plier<klr tip="ind"> (intr.)</klr></trd>
    <trd>ployer<klr tip="ind"> (intr.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="fleks.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      Movo, per kiu io <tld/>i&gcirc;as; formo de io <tld/>ita:
      <ekz>
        <tld/>i&gcirc;o de risorto;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>i&gcirc;o de la rivero.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_g;&c_ib;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Biegung</trd>,
    <trd>Beugung</trd>,
    <trd>Kr&uuml;mmung</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fl&eacute;chissement</trd>,
    <trd>flexion<klr tip="ind"> (fait de fl&eacute;chir)</klr></trd>,
    <trd>inflexion</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>buiging</trd>,
    <trd>bocht</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_z;&c_g;&c_i;&c_b;</trd>
</drv>

<drv mrk="fleks.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc>
    <dif>
      Kiu facile <tld/>i&gcirc;as; facile <tld/>ebla:
      <ekz>
        longa <tld/>i&gcirc;ema kolo de cigno<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_u;&c_t;&c_k;&c_ib;</trd>
  <trdgrp lng="fr">
    <trd>flexible<klr tip="ind"> (adj.)</klr></trd>,
    <trd>mall&eacute;able</trd>,
    <trd>souple</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>soepel</trd>,
    <trd>flexibel</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_b;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="fleks.genu0i">
  <kap>genu<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="F"/>i sian korpon &ccirc;e la 
      <ref cel="genu.0o.ANA">genuo</ref>, ofte
      por esprimi respekton al la persono, anta&ubreve; kiu
      oni staras:
      <ekz>
        anta&ubreve; Li genu<tld/>os &ccirc;iuj forirantaj al la tombo
        <fnt><bib>MT</bib><lok>&Psa; 22:29</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i estis &ccirc;iam kutiminta, ke oni &scirc;in starigu
        sur piedestalon, &ccirc;irka&ubreve;atan de genu<tld/>antaj kavaliroj
        <fnt><bib>FK</bib><lok>La porcio de glacia&jcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        en tiu senkompare vasta baziliko ja povas okdek mil kreduloj
        respektege genu<tld/>i anta&ubreve; la papo
        <fnt><bib>Lanti</bib><lok>&Ccirc;e la lando de Nigra&ccirc;emizuloj</lok></fnt>;
      </ekz>
      <ekz>
        <ind>genu<tld/>o</ind>.
        <trd lng="fr">g&eacute;nuflexion</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="genu.0i">genui</ref>,
      <ref cel="genu.0igxi">genui&gcirc;i</ref>
    </refgrp>
    <rim>
      Genu<tld/>adon foje akompanas klinado de la busto. La kruroj
      povas resti paralelaj a&ubreve; ne. Oni kutime
      subkomprenas, ke la fleksado estas mallonga kaj tuj sekvota de
      rerektigo, sed la etiketo povas postuli, ke unu genuo
      malalti&gcirc;u &gcirc;is la planko kaj ke oni restu staranta
      unuegenue sur la planko.
    </rim>
    <rim>
      La vorto aperas en <fnt><bib>VdE</bib></fnt> kaj
      <fnt><bib>PV</bib></fnt> kun la iom alia signifo
      &leftquot;<ref cel="genu.sur0igxi">surgenui&gcirc;i</ref>&rightquot;
      kaj tute
      malaperis el <fnt><bib>PIV1</bib></fnt>, supozeble pro la fakto,
      ke, en la koncerna kunmeta&jcirc;o, la flankelemento &scirc;ajnas
      roli kiel objekto de la &ccirc;efelemento.
    </rim>
  </snc>
  <adm>
    Kontroli la tradukojn (nl) post modifo de la difino.
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_k;&c_l;&c_je;&c_n;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>fl&eacute;chir le <ind>genou</ind></trd>,
    <trd>faire une <ind>g&eacute;nuflexion</ind></trd>
  </trdgrp>
  <trd lng="nl">knielen</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol; &c_k;&c_o;&c_l;&c_je;&c_n;&c_i;</trd>
</drv>

<drv mrk="fleks.trans0igxa">
  <kap>trans<tld/>i&gcirc;a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <klr>(p.p. punkto de ebena <ref cel="kurb.0o.MAT">kurbo</ref>)</klr>
      Tia, ke la <ref cel="sign.0o.MAT">signo</ref> de la
      <ref cel="kurb.0eco.de_ebena_kurbo">kurbeco</ref>
      &scirc;an&gcirc;i&gcirc;as, kiam la kurbo &gcirc;in trairas;
      intuicie: la kurbo trairas sian tan&gcirc;anton &ccirc;e tiu punkto:
      <ekz>
        por ke estu trans<tld/>i&gcirc;a la punkto
        <frm>(<k>x</k><sub>0</sub>,<k>y</k><sub>0</sub>)</frm> de kurbo
        difinita per kartezia ekvacio
        <frm><k>y</k> = <k>f</k>(<k>x</k>)</frm>, kie <frm><k>f</k></frm>
        estas almena&ubreve; dufoje kontinue derivebla
        <ref cel="funkci.0o.MAT">funkcio</ref>, necesas, ke la dua
        deriva&jcirc;o de <frm><k>f</k></frm> nuli&gcirc;u &ccirc;e
        <frm><k>x</k><sub>0</sub></frm> <klr>(tiam, oni povas diri, ke
        la koncerna punkto estas trans<tld/>i&gcirc;a punkto
        de funkcio <frm><k>f</k></frm>)</klr>.
      </ekz>
    </dif>
    <ref tip="sin" cel="inflek.0a">infleksa.</ref>
    <rim>
      Tiu termino ne estas tre kontentiga: &gcirc;i malprecize
      elvokas la ideon, ke la kurbo transiras la koncernan punkton,
      a&ubreve; sian tan&gcirc;anton &ccirc;e tiu punkto,
      <tld/>i&gcirc;ante (al alia direkto). &Ccirc;iuj aliaj fontoj &dash;
      krom <fnt><bib>PIV2</bib></fnt>, kiu konas nur la substantivon
      <ctl>trans<tld/>i&gcirc;ejo</ctl> &dash; preferas la formon
      <ctl>infleksa</ctl>, kies radiko restas ignorata de la
      &gcirc;eneralaj vortaroj.
    </rim>
  </snc>
  <bld tip="svg" lok="../bld/f_transfleksigxa_punkto.svg" alt="300" lrg="300">
    trans<tld/>i&gcirc;a punkto
  </bld>
  <trd lng="be"><klr tip="amb">(&c_p;&c_u;&c_n;&c_k;&c_t;)</klr> &c_p;&c_je;&c_r;&c_a;&c_g;&c_ib;&c_b;&c_u;</trd>
  <trd lng="cs">inflexn&iacute;<klr tip="amb"> (bod)</klr></trd>
  <trdgrp lng="de">
    <trd>Inflexions-<klr tip="amb">(punkt)</klr></trd>,
    <trd>Wende-<klr tip="amb">(punkt)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd><klr tip="amb">(point) </klr>of <ind>inflection</ind></trd>,
    <trd>flex<klr tip="amb"> (point)</klr></trd>
  </trdgrp>
  <trd lng="fr"><klr tip="amb">(point) </klr>d'<ind>inflexion</ind></trd>
  <trd lng="pl"><klr tip="amb">(punkt) </klr>przegi&eogonek;cia</trd>
  <trd lng="ru"><klr tip="amb">(&c_t;&c_o;&c_ch;&c_k;&c_a;) </klr>&c_p;&c_je;&c_r;&c_je;&c_g;&c_i;&c_b;&c_a;</trd>
</drv>

<drv mrk="fleks.trans0igxejo">
  <kap>trans<tld/>i&gcirc;ejo</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      <ref cel="fleks.trans0igxa">Trans<tld/>i&gcirc;a</ref> punkto.
    </dif>
    <rim>
      Pri tia uzo de sufikso <ctl>-ejo</ctl>,
      vd rimarkon sub <ref cel="maksim1.0iganto">maksimumiganto</ref>.
    </rim>
  </snc>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_k;&c_a; <ind>&c_p;&c_je;&c_r;&c_je;&c_g;&c_i;&c_b;&c_a;</ind></trd>
</drv>
