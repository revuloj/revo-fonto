<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fleks.xml,v 1.41 2021/05/29 10:56:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fleks</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fleks.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fleks.0i.kurbigi">
    <dif>
      <ref tip="super" cel="kurb.0igi">Kurbigi</ref>, 
      <ref cel="ark.0igi">arkigi</ref>:
      <ekz>
        la feino <tld/>is flanken la bran&ccirc;ojn
        <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Stella</nom> sidis <klr>[kun la]</klr> kapo en la manoj, &scirc;ia blanka kolo estis
        <tld/>ita
        <fnt>
          <bib>FK</bib>
          <vrk>La Porcio da Glacia&jcirc;o</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is siajn genuojn kaj pre&gcirc;is
        <fnt>
          <bib>Fab4</bib>
          <lok>Infano en la tombo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li eltiris el la ingo sufi&ccirc;e longan glavon 
        <klr>[&#x2026;]</klr> kaj komencis <tld/>i
        &gcirc;in per la manoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        la faraono sin <tld/>is &gcirc;is la tero
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la aliaj friponoj kaptis lin, luktis kun li, <tld/>is lin tiel, ke lia kapo
        atingis liajn piedojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vinicius</nom> staras anta&ubreve; li senarma, kun mizera viza&gcirc;o kaj kun figuro, <tld/>ita de
        sufero
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        genuojn homoj <tld/>as ja nur tiam, / se la rampad&#x2019; profiton donas
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        povi <tld/>i per la fingroj ferajn bastonojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sin <tld/>inte trans la tablon, li kriis en la orelon mem de la vizitanto
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ita strio de la vojo
        <klr>(multkurba, serpentuma)</klr>
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u arbon dum &gcirc;ia juneco
        <fnt><bib>PrV</bib></fnt>
        <klr>(ne lasu pasi la &gcirc;ustan momenton)</klr>!
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>biegen</trd>,
      <trd>beugen</trd>,
      <trd>kr&uuml;mmen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>arquear</trd>,
      <trd>doblar</trd>,
      <trd>flexionar</trd>,
      <trd>curvar</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>buigen</trd>,
      <trd>krommen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>gi&aogonek;&cacute;</trd>,
      <trd>zgina&cacute;</trd>,
      <trd>nagina&cacute;</trd>,
      <trd>wygina&cacute;</trd>,
      <trd>przegina&cacute;</trd>,
      <trd>dogina&cacute;</trd>,
      <trd>ugina&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fleks.0i.cedigi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Cedigi, <ref tip="super" cel="obe.0igi">obeigi</ref>:
      <ekz>
        <tld/>i la junajn spiritojn sub disciplino;
      </ekz>
      <ekz>
        <tld/>i la venkiton sub severa le&gcirc;o;
      </ekz>
      <ekz>
        &ccirc;i tiujn mi <tld/>os per nenio pensis la faraono
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi scias ankora&ubreve; rimedojn, kiuj tre bone povas <tld/>i 
        <klr><refgrp tip="vid">
          (<ref cel="humil.0igi">humiligi</ref>,
          <ref cel="fier">malfierigi</ref>)
        </refgrp>
        </klr>
        la fierecon de malhumila obstinulino
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">beugen</trd>
    <trd lng="es">doblegar</trd>
    <trd lng="nl">buigen</trd>
    <trdgrp lng="pl">
      <trd>nagina&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fleks.0i.adapti">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="konfor.0igi">Konformigi</ref>, 
      <ref tip="super" cel="adapt.0i">adapti</ref>:
      <ekz>
        lingvoj naturaj kreskas tute trankvile, &ccirc;ar kun tia lingvo neniu kura&gcirc;as fari
        iajn eksperimentojn a&ubreve; <tld/>i &gcirc;in la&ubreve; sia gusto
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        nia lingvo estas ankora&ubreve; juna kaj nepotenca kaj devas sin <tld/>i, se &gcirc;i ne
        volas perei
        <fnt>
          <bib>LR</bib>
          <vrk>Pri la elekto inter &leftquot;h&rightquot; kaj supersignitaj literoj</vrk>
          <lok>La Esperantisto, 1891, p; 15.</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ne povas en tia direkto <tld/>i la amatan virinon
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>anpassen</trd>,
      <trd>zurecht <ind>biegen</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_g;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">oh&yacute;bat</trd>
  <trd lng="en">bend</trd>
  <trdgrp lng="fr">
    <trd>fl&eacute;chir<klr tip="ind"> (tr.)</klr></trd>,
    <trd>arquer</trd>,
    <trd>courber</trd>,
    <trd>couder</trd>,
    <trd>infl&eacute;chir</trd>,
    <trd>faire <ind>c&eacute;der</ind></trd>,
    <trd>faire <ind>plier</ind></trd>,
    <trd>plier<klr tip="ind"> (tr.)</klr></trd>,
    <trd>ployer<klr tip="ind"> (tr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>dobrar</trd>,
    <trd>curvar</trd>,
    <trd>vergar</trd>,
    <trd>torcer</trd>
  </trdgrp>
  <trd lng="sk">oh&yacute;ba&tcaron;</trd>
</drv>

<drv mrk="fleks.0o">
  <kap><tld/>o</kap>
  <snc mrk="fleks.0o.0ita_parto">
    <dif>
      <tld lit="F"/>ita parto de io:
      <ekz>
        la rivero havas multe da <tld/>oj
        <fnt><bib>Fab1</bib>, <lok>Malgranda Niko kaj granda
        Niko</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;ia okulo sen malfacileco rimarkas la plej komplikitajn
        <tld/>ojn de la linioj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li malaperis malanta&ubreve; <tld/>o de la muro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        grandaj bran&ccirc;oj <klr>[kun]</klr> kurba&jcirc;oj kaj <tld/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="fleks.0ajxo"><tld/>a&jcirc;o</ref>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_g;&c_ib;&c_n;</trd>,
      <trd>&c_z;&c_g;&c_ib;&c_n;</trd>
    </trdgrp>
    <trd lng="de">Biegung</trd>
    <trdgrp lng="fr">
      <trd>coude<klr tip="ind"> (tournant)</klr></trd>,
      <trd>pli<klr tip="ind"> (partie fl&eacute;chie de qc)</klr></trd>
      <trd>infl&eacute;chissement</trd>
      <trd>inflexion<klr tip="ind"> (partie fl&eacute;chie de qc)</klr>
      </trd>
      <trd>tournant</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bocht</trd>,
      <trd>kromming</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>zgi&eogonek;cie</trd>,
      <trd>zagi&eogonek;cie</trd>,
      <trd>wygi&eogonek;cie</trd>,
      <trd>ugi&eogonek;cie</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_z;&c_g;&c_i;&c_b;</trd>,
      <trd>&c_s;&c_g;&c_i;&c_b;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fleks.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="fort.0o.FIZ">Forto</ref>, kiu <tld/>as:
      <ekz>
        la frontaj cilindraj pivotoj estas stre&ccirc;ataj per <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=133362">Pivoto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">flexion<klr tip="ind"> (phys.)</klr></trd>
  </snc>
  <snc mrk="fleks.0o.ago0i">
    <dif>
      Ago <tld/>i.
    </dif>
    <rim>
      &Ccirc;i tiu signifo estas postulata de la
      radika karaktero, kaj supozeble tial &gcirc;i
      estis enkondukita en <fnt><bib>PIV2</bib></fnt>,
      sed &gcirc;i restas neuzata.
    </rim>
    <trdgrp lng="pl">
      <trd>gi&eogonek;cie</trd>,
      <trd>wyginanie</trd>,
      <trd>zginanie</trd>,
      <trd>zaginanie</trd>,
      <trd>doginanie</trd>,
      <trd>przyginanie</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>flexe</trd>,
    <trd>ohnut&iacute; <klr>(nap&rcaron;. pa&zcaron;e)</klr> pohybem v kloubu</trd>,
    <trd>oh&yacute;b&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>flexi&oacute;n</trd>,
    <trd>curva</trd>,
    <trd>doblez</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>flexia</trd>,
    <trd>oh&yacute;banie</trd>
  </trdgrp>
</drv>

<drv mrk="fleks.0a">
  <kap><tld/>a</kap>
  <snc mrk="fleks.0a.0orilata">
    <uzo tip="stl">RAR</uzo>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a momanto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=501516">Stre&ccirc;o
          (mekaniko)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a rigideco
        <fnt>
          <aut>J. Werner</aut>
          <vrk>Terminaro de betono kaj de betonistaj laboroj</vrk>
          <lok><url ref="http://eventoj.freeweb.hu/steb/ingxenierado/                     konstruado/beton-termin.htm">
          beton-terminaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">de <ind>flexion</ind></trd>
    <trd lng="pl">gi&eogonek;tki</trd>
  </snc>
  <snc mrk="fleks.0a.0igxema">
    <dif>
      <ref tip="dif" cel="fleks.0igxema"><tld lit="F"/>i&gcirc;ema</ref>:
      <ekz>
        <tld/>a serpento
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        teruraj polipoj, kiuj etendadis post &scirc;i siajn <tld/>ajn brakojn
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
         kun fingroj kiel <tld/>aj vermoj
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_n;&c_u;&c_t;&c_k;&c_ib;</trd>
    <trd lng="de">biegsam</trd>
    <trd lng="es">flexible</trd>
    <trd lng="fr">flexible<klr tip="ind"> (adj.)</klr></trd>
    <trd lng="nl">buigzaam</trd>
    <trdgrp lng="pl">
      <trd>gi&eogonek;tki</trd>,
      <trd>gibki</trd>,
      <trd>wiotki</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_i;&c_b;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="fleks.0a.0ita">
    <fnt><bib>PV</bib></fnt>
    <uzo tip="stl">EVI</uzo>
    <dif>
      <tld lit="F"/>ita.
    </dif>
    <adm>
      Tute mankas ekzemploj de tiu senco. Nur PV gxin mencias.
      <aut>MB</aut>
    </adm>
    <trd lng="be">&c_s;&c_a;&c_g;&c_n;&c_u;&c_t;&c_y;</trd>
    <trd lng="nl">gebogen</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_n;&c_u;&c_t;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_o;&c_g;&c_n;&c_u;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">ohebn&yacute;</trd>
  <trdgrp lng="sk">
    <trd>ohybn&yacute;</trd>,
    <trd>pru&zcaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="fleks.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="fleks.0ado.KOMUNE">
    <dif>
      Ago <tld/>i:
      <ekz>
        al la pasivaj movoj, dum kiuj la masa&gcirc;ita persono mem faras
        neniun stre&ccirc;adon, apartenas <klr>[...]</klr> <tld/>ado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=54921">Masa&gcirc;o</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; principo de piezoelektra efekto <klr>[...]</klr> per premado
        a&ubreve; <tld/>ado de iuj materialoj ekestas elektra tensio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=113719">Mikrofono</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_g;&c_ib;&c_n;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="cs">oh&yacute;b&aacute;n&iacute;</trd>
  <trdgrp lng="fr">
    <trd>flexion<klr tip="ind"> (action de courber)</klr></trd>,
    <trd>arcure<klr tip="ind"> (action de fl&eacute;chir une branche)
    </klr></trd>,
    <trd>courbure<klr tip="ind"> (action de courber)</klr></trd>,
    <trd>coudage</trd>,
    <trd>inflexion<klr tip="ind"> (action d'infl&eacute;chir)
    </klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gi&eogonek;cie</trd>,
    <trd>wyginanie</trd>,
    <trd>zginanie</trd>,
    <trd>zaginanie</trd>,
    <trd>doginanie</trd>,
    <trd>przyginanie</trd>
  </trdgrp>
  <trd lng="sk">oh&yacute;banie</trd>
</drv>

<drv mrk="fleks.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="fleks.0ajxo.KOMUNE">
    <dif>
      <ref tip="dif" cel="fleks.0o.0ita_parto"><tld lit="F"/>o<sncref/></ref>:
      <ekz>
        la <tld/>a&jcirc;oj de la vojo
        <fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        &gcirc;is <tld/>a&jcirc;o de sia kubuto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=267740">Seksa
          pozicio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la vo&ccirc;o kaj la gitaro de <nom>Ale</nom> <klr>[&#x2026;]</klr> en siaj plej subtilaj <tld/>a&jcirc;oj,
        donis al mi la necesan materialon por mia laboro
        <fnt>
          <bib>Kon11</bib>
          <vrk>Intervjuo &dash; Ale Kossabela &ndash; Kameleono de la E-muziko</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>flexi&oacute;n</trd>,
      <trd>curva</trd>,
      <trd>recodo</trd>,
      <trd>doblez</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_g;&c_ib;&c_n;</trd>,
    <trd>&c_z;&c_g;&c_ib;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Biegung</trd>
  <trdgrp lng="fr">
    <trd>coude<klr tip="ind"> (tournant)</klr></trd>,
    <trd>pli<klr tip="ind"> (partie fl&eacute;chie de qc)</klr></trd>
    <trd>infl&eacute;chissement</trd>
    <trd>inflexion<klr tip="ind"> (partie fl&eacute;chie de qc)</klr>
    </trd>
    <trd>tournant</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bocht</trd>,
    <trd>kromming</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zgi&eogonek;cie</trd>,
    <trd>zagi&eogonek;cie</trd>,
    <trd>wygi&eogonek;cie</trd>,
    <trd>ugi&eogonek;cie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_g;&c_i;&c_b;</trd>,
    <trd>&c_s;&c_g;&c_i;&c_b;</trd>
  </trdgrp>
</drv>

<drv mrk="fleks.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="fleks.0ebla.KOMUNE">
    <dif>
      Kiun oni povas <tld/>i:
      <ekz>
        &scirc;talo estas <tld/>ebla, sed fero ne estas <tld/>ebla
        <fnt>
          <bib>F</bib>
          <lok>&FE; 41</lok>
        </fnt>;
      </ekz>
      <ekz>
        la trunketo kaj folioj estas tiel <tld/>eblaj kaj elastaj, ke
        ili &ccirc;e la plej malgranda fluo de la akvo sin movas, kiel vivaj
        esta&jcirc;oj
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havas fingrojn delikatajn kaj <tld/>eblajn
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili &scirc;miris al si la angulojn de la bu&scirc;o per fi&scirc;oleo, por ke ili
        fari&gcirc;u pli <tld/>eblaj
        <fnt>
          <bib>Fab3</bib>
          <lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok>
