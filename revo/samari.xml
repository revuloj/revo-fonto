  <trd lng="fr">samarium</trd>
  <trd lng="hu">szam&aacute;rium</trd>
  <trd lng="it">samario</trd>
  <trd lng="la">Samarium</trd>
  <trd lng="nl">Samarium</trd>
  <trd lng="pl">samar</trd>
  <trd lng="pt">sam&aacute;rio</trd>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_a;&c_r;&c_i;&c_j;</trd>
</drv>
</art>
