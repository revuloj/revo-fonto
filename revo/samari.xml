<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: samari.xml,v 1.21 2024/03/24 19:59:59 revo Exp $">
<kap>
  <rad>samari</rad>/o
</kap>
<drv mrk="samari.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Sm</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="62">elemento</ref> kun
      atomnumero 62, atompezo 150,35;
      ar&gcirc;entgriza <ref tip="super" cel="metal.0o">metalo</ref>,
      lantanoido:
      <ekz>
        <tld/>o estas <klr>[&#x2026;]</klr> bluar&gcirc;ent-kolora lantanoida elemento, kiu ekbrulos
        en aero &ccirc;e 150&#x202F;&deg;C
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=203990">Samario (kemio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Anka&ubreve; regiono nomi&gcirc;as <ref tip="hom" cel="samari1.0o">Samario</ref>.
  </rim>
  <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_r;&c_y;&c_j;</trd>
  <trd lng="bg">&c_s;&c_a;&c_m;&c_a;&c_r;&c_i;&c_j;</trd>
  <trd lng="br">samariom</trd>
  <trd lng="ca">samari</trd>
  <trdgrp lng="cs">
    <trd>Sm</trd>,
    <trd>samarium</trd>
  </trdgrp>
  <trd lng="de">Samarium</trd>
  <trd lng="en">samarium</trd>
  <trd lng="es">samario</trd>
  <trd lng="fr">samarium</trd>
  <trd lng="hu">szam&aacute;rium</trd>
  <trd lng="id">samarium</trd>
  <trd lng="it">samario</trd>
  <trd lng="la">Samarium</trd>
  <trd lng="nl">Samarium</trd>
  <trd lng="pl">samar</trd>
  <trd lng="pt">sam&aacute;rio</trd>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_a;&c_r;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Sm</trd>,
    <trd>sam&aacute;rium</trd>
  </trdgrp>
  <trd lng="sv">samarium</trd>
  <trd lng="uk">&#x441;&#x430;&#x43C;&#x430;&#x440;&#x456;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x91E4; <pr>sh&#x101;n</pr></trd>,
    <trd>&#x9490; <pr>sh&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>
