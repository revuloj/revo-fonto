<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolost.xml,v 1.16 2018/12/13 23:10:17 revo Exp $">
<kap>
  <rad>kolostr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kolost.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      La unua <ref tip="super" cel="lakt.0o.mambesto">lakto</ref>
      de aku&scirc;intino:
      <ekz>
        <tld/>o (konata anka&ubreve; kiel unua lakto) estas formo de lakto produktita de la
        laktoglandoj de mamuloj (inklude homojn) fine de la gravedeco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=543791"
              >Kolostro</url></lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_o;&c_d;&c_z;&c_ib;&c_v;&c_a;</trd>
  <trd lng="ca">calostre</trd>
  <trdgrp lng="cs">
    <trd>kolostrum</trd>,
    <trd>mlezivo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Colostrum</trd>,
    <trd>Vormilch</trd>,
    <trd>Kolostralmilch</trd>,
    <trd>Biestmilch<klr tip="amb"> (bei K&uuml;hen)</klr></trd>,
    <trd>Erstmilch</trd>
  </trdgrp>
  <trd lng="en">colostrum</trd>
  <trd lng="es">calostro</trd>
  <trd lng="fr">colostrum</trd>
  <trd lng="it">colostro</trd>
  <trd lng="nl">biest</trd>
  <trd lng="pl">siara</trd>
  <trd lng="pt">colostro</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_o;&c_z;&c_i;&c_v;&c_o;</trd>
  <trd lng="sk">mledzivo</trd>
</drv>
</art>
