<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>grumbl</rad>/i
</kap>
<drv mrk="grumbl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Obtuze murmuri pro malkontenteco. 
    </dif>
    <refgrp tip="vid">
      <ref cel="grunt.0i">grunti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>brummen</trd>,
     <trd>murren</trd>,
     <trd>n&ouml;rgeln</trd>,
     <trd>knurren</trd>,
     <trd>kn&ouml;ren [sudgermana]</trd></trdgrp>
  <trd lng="en">grumble</trd>
  <trdgrp lng="fr">
    <trd>grommeler</trd>,
    <trd>grogner<klr tip="ind"> (r&eacute;criminer)</klr></trd>,
    <trd>r&acirc;ler<klr tip="ind"> (r&eacute;criminer)</klr></trd>,
    <trd>r&eacute;criminer</trd>,
    <trd>ronchonner</trd>,
    <trd>rousp&eacute;ter</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_z;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="grumbl.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo, kiu kutimas <tld/>i.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>brummiger Mensch</trd>,
     <trd>m&uuml;rrischer Mensch</trd>,
     <trd>N&ouml;rgler</trd>,
     <trd>Griesgram</trd>,
     <trd>Kn&ouml;rer [sudgermana]</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>grognon<klr tip="ind"> (subst.)</klr></trd>,
    <trd>r&acirc;leur</trd>,
    <trd>ronchonneur</trd>,
    <trd>rousp&eacute;teur</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_ch;&c_u;&c_n;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_z;&c_g;&c_a;</trd>
  </trdgrp>
</drv>
</art>
