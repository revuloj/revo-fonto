<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grumbl.xml,v 1.27 2024/11/07 07:24:02 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>grumbl</rad>/i
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="grumbl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="grumbl.0i.SON">
    <dif>
      Vo&ccirc;<ref tip="super" cel="bru.0i.soni">brui</ref> obtuze,
      malkontente:
      <ekz>
        hejme la filo <tld/>is, kie &scirc;i restas tiom longe
        <fnt>
          <aut>S. &Scirc;timec</aut>
          <vrk>Morto en Melburna tramo</vrk>
          <bib>Monato</bib>
          <lok>1997:8, p. 24a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la laboristoj vivis en plena nescio pri la estonteco
        <klr>[&#8230;]</klr>, <tld/>is
        kaj blasfemis kaj &ccirc;iutage centfoje rema&ccirc;is la samajn
        frazojn pri senlaboreco
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li forte <tld/>us al mi, se li kaptus min sen laboro en la mano
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 2</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial iuj maljunuloj <tld/>as kaj trudas sin al pli
        junaj
        <fnt>
          <aut>I. Gon&ccirc;arova</aut>
          <vrk>Vivi plu: kial?</vrk>
          <bib>Monato</bib>,
          <lok>2002:6, p. 20a</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="murmur.0i.protesti">murmuri <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>rondinar</trd>,
    <trd>remugar</trd>,
    <trd>murmurar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>brblat</trd>,
    <trd>bru&ccaron;et</trd>,
    <trd>hu&ccaron;et</trd>,
    <trd>mru&ccaron;et</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>brummen</trd>,
    <trd>murren</trd>,
    <trd>n&ouml;rgeln</trd>,
    <trd>knurren</trd>,
    <trd>kn&ouml;ren [sudgermana]</trd>
  </trdgrp>
  <trd lng="en">grumble</trd>
  <trdgrp lng="fa">
    <trd>&a_gh;&a_r;&a_w;&a_l;&a_n;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_n;&a_q; &a_z;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bougonner</trd>,
    <trd>grommeler</trd>,
    <trd>grogner<klr tip="ind"> (r&eacute;criminer)</klr></trd>,
    <trd>r&acirc;ler<klr tip="ind"> (r&eacute;criminer)</klr></trd>,
    <trd>r&eacute;criminer</trd>,
    <trd>rousp&eacute;ter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>morog</trd>,
    <trd>d&ouml;rm&ouml;g</trd>,
    <trd>z&uacute;gol&oacute;dik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12412;&#12420;&#12367;</trd>,
    <trd>&#12406;&#12388;&#12406;&#12388;&#19981;&#24179;&#12434;&#35328;&#12358;  <pr>&#12406;&#12388;&#12406;&#12388;&#12405;&#12408;&#12356;&#12434;&#12356;&#12358;</pr></trd>,
    <trd>&#24858;&#30196;&#12434;&#12371;&#12412;&#12377;  <pr>&#12368;&#12385;&#12434;&#12371;&#12412;&#12377;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mopperen</trd>,
    <trd>brommen</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>resmungar</trd>,
   <trd>rosnar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_z;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hundra&tcaron;</trd>,
    <trd>mrmla&tcaron;</trd>,
    <trd>&scaron;omra&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_r;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_o;&c_r;&c_k;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_u;&c_r;&c_k;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22228; <pr>n&amacron;ng</pr></trd>,
    <trd>&#21501; <pr>j&imacron;</pr></trd>,
    <trd>&#39723; <pr>k&agrave;n</pr></trd>,
    <trd>&#21872;&#21766; <pr>lu&omacron;su&omacron;</pr></trd>,
    <trd>&#30952;&#35910;&#33104; <pr>m&ograve;d&ograve;ufu</pr></trd>,
    <trd>&#22047;&#21725; <pr>d&umacron;nong</pr></trd>
  </trdgrp>
</drv>

<drv mrk="grumbl.0ema">
  <kap>
    <tld/>ema
  </kap>
  <snc mrk="grumbl.0ema.SON">
    <dif>
      Kiu volonte <tld/>as, a&ubreve;deble
      <ref cel="humor.malbon0a">mishumoras</ref>:
      <ekz>
        ili estis <tld/>emaj, &ccirc;ar li timigis ilin
        <fnt>
          <aut>J. R. R. Tolkien, trad. C. Gledhill</aut>
          <vrk>La Hobito</vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        haveno, profunda nokto, la <tld/>emaj arganoj
        &ccirc;esas interparoli per knarantaj
        vo&ccirc;oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fotita junulo esprimis &ccirc;iutrajte gajecon,
        kio ege kontrastis kun la <tld/>ema
        urso de hodia&ubreve;
        <fnt>
          <bib>ChV</bib>
          <lok>17</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="konten.mal0a">malkontenta</ref>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_k;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trdgrp lng="de">
    <trd>brummig</trd>,
    <trd>n&ouml;rgelig</trd>,
    <trd>m&uuml;rrisch</trd>,
    <trd>misslaunig</trd>,
    <trd>griesgr&auml;mig</trd>,
    <trd>gnatzig</trd>,
    <trd>missmutig</trd>,
    <trd>verdrossen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>grognon<klr> (adj.)</klr></trd>,
    <trd>ronchon</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#32118;&#12360;&#12378;&#19981;&#24179;&#12434;&#35328;&#12358;  <pr>&#12383;&#12360;&#12378;&#12405;&#12408;&#12356;&#12434;&#12356;&#12358;</pr></trd>,
    <trd>&#19981;&#24179;&#19981;&#28288;&#12398;&#22810;&#12356;  <pr>&#12405;&#12408;&#12356;&#12405;&#12414;&#12435;&#12398;&#12362;&#12362;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_r;&c_k;&c_o;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_r;&c_k;&c_o;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_u;&c_r;&c_k;&c_o;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21741;&#20007;&#30528;&#33080; <pr>k&umacron;sangzheli&#462;n</pr></trd>,
    <trd>&#33086;&#27668;&#22351; <pr>p&iacute;q&igrave;hu&agrave;i</pr></trd>,
    <trd>&#19981;&#39640;&#20852;&#22320; <pr>b&ugrave;g&amacron;ox&igrave;ngde</pr></trd>
  </trdgrp>
</drv>

<drv mrk="grumbl.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="grumbl.0ulo.SON">
    <dif>
      Homo, kiu kutimas <tld/>i:
      <ekz>
        oni satigis min &ccirc;i tie &gcirc;is abomeno per
        kontra&ubreve;sovetaj anekdotoj kaj mi mem
        fari&gcirc;is iu <tld/>ulo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        miaopinie estus tre bonvena iu klariga piednoto
        tiurilate <klr>[&#8230;]</klr> sed nu,
        nur <tld/>ulo tion menciu
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="https://www.monato.be/2015/011307.php">Rekomendinda romano en matura lingva&jcirc;o</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_r;&c_ch;&c_u;&c_n;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_k;&c_u;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>rondinaire</trd>,
    <trd>remugador</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bru&ccaron;oun</trd>,
    <trd>bub&aacute;k</trd>,
    <trd>kakabus</trd>,
    <trd>morous</trd>,
    <trd>protiva</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>brummiger Mensch</trd>,
    <trd>m&uuml;rrischer Mensch</trd>,
    <trd>N&ouml;rgler</trd>,
    <trd>Griesgram</trd>,
    <trd>Kn&ouml;rer <klr>(sudgermana)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>grognon<klr tip="ind"> (subst.)</klr></trd>,
    <trd>r&acirc;leur</trd>,
    <trd>ronchonneur</trd>,
    <trd>rousp&eacute;teur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>morg&oacute;s ember</trd>,
    <trd>zs&eacute;mbes ember</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#19981;&#24179;&#23478;  <pr>&#12405;&#12408;&#12356;&#12363;</pr></trd>,
    <trd>&#19981;&#24179;&#23627;  <pr>&#12405;&#12408;&#12356;&#12420;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mopperaar</trd>,
    <trd>brombeer</trd>
  </trdgrp>
  <trd lng="pt">resmungador</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_ch;&c_u;&c_n;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_z;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hundro&scaron;</trd>,
    <trd>mrmlo&scaron;</trd>
  </trdgrp>
  <trd lng="zh">&#26080;&#31934;&#25171;&#37319;&#30340;&#20154; <pr>w&uacute;j&imacron;ngd&#462;c&#462;ider&eacute;n</pr></trd>
</drv>

<drv mrk="grumbl.post0i">
  <kap>post<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="grumbl.post0i.SON">
    <dif>
      <tld lit="G"/>i de malanta&ubreve;e, al iu malproksimi&gcirc;anta:
      <ekz>
        belan taskon mi havas! &mdash; ofendite ekkriis la rufulo, kaj
        post<tld/>is en &scirc;ian dorson: &mdash; stulta ino!
        <fnt><bib>MkM</bib> <lok>&ccirc;ap. 19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_ch;&c_a;&c_c;&c_mol; &c_u;&c_s;&c_mol;&c_l;&c_je;&c_d;</trd>
  <trd lng="ca">rondinar a l'esquena d'alg&uacute;</trd>
  <trd lng="de">hinterher brummen</trd>
  <trd lng="fr">ronchonner<klr> (par derri&egrave;re)</klr></trd>
</drv>
</art>
