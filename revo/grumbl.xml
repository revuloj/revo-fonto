<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>grumbl</rad>/i
</kap>
<drv mrk="grumbl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Obtuze murmuri pro malkontenteco. 
    </dif>
    <refgrp tip="sin">
      <ref cel="grunt.0i">grunti</ref>.
    </refgrp>
  </snc>
  <trd lng="en">grumble</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_z;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="grumbl.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo, kiu kutimas <tld/>i.  
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_ch;&c_u;&c_n;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_z;&c_g;&c_a;</trd>
  </trdgrp>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
