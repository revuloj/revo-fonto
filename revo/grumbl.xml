<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>grumbl</rad>/i
</kap>
<drv mrk="grumbl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Audigi obtuzan vo&ccirc;bruon signe pri malkontento:
      <ekz>
        hejme la filo <tld/>is kie &scirc;i restas tiom longe<fnt><aut>Spomenka
        &Scirc;timec</aut>: <vrk>Morto en Melburna tramo</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 8a, p. 24a</lok></fnt>;
      </ekz>
      <ekz>
        kial iuj maljunuloj <tld/>as kaj trudas sin al pli
        junaj<fnt><aut>Irina Gon&ccirc;arova</aut>: <vrk>Vivi plu: kial? (Gudrun
        Pausewang: La avo en la &ccirc;areto)</vrk>, <bib>Monato</bib><lok>jaro
        2002a, numero 6a, p. 20a</lok></fnt>?
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="murmur.0i.protesti">mumuri <sncref/></ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>brummen</trd>,
    <trd>murren</trd>,
    <trd>n&ouml;rgeln</trd>,
    <trd>knurren</trd>,
    <trd>kn&ouml;ren [sudgermana]</trd>
  </trdgrp>
  <trd lng="en">grumble</trd>
  <trdgrp lng="fr">
    <trd>bougonner</trd>,
    <trd>grommeler</trd>,
    <trd>grogner<klr tip="ind"> (r&eacute;criminer)</klr></trd>,
    <trd>r&acirc;ler<klr tip="ind"> (r&eacute;criminer)</klr></trd>,
    <trd>r&eacute;criminer</trd>,
    <trd>ronchonner</trd>,
    <trd>rousp&eacute;ter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>morog</trd>,
    <trd>d&ouml;rm&ouml;g</trd>,
    <trd>z&uacute;gol&oacute;dik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mopperen</trd>,
    <trd>brommen</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>resmungar</trd>, 
   <trd>rosnar</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_z;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="grumbl.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo, kiu kutimas <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_r;&c_ch;&c_u;&c_n;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_k;&c_u;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>brummiger Mensch</trd>,
    <trd>m&uuml;rrischer Mensch</trd>,
    <trd>N&ouml;rgler</trd>,
    <trd>Griesgram</trd>,
    <trd>Kn&ouml;rer [sudgermana]</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>grognon<klr tip="ind"> (subst.)</klr></trd>,
    <trd>r&acirc;leur</trd>,
    <trd>ronchonneur</trd>,
    <trd>rousp&eacute;teur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>morg&oacute;s ember</trd>,
    <trd>zs&eacute;mbes ember</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>mopperaar</trd>,
    <trd>brombeer</trd>
  </trdgrp>
  <trd lng="pt">resmungador</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_ch;&c_u;&c_n;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_z;&c_g;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="grumbl.post0i">
  <kap>post<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="G"/>i de malanta&ubreve;e, al iu malproksimi&gcirc;anta:
      <ekz>
        &mdash; Belan taskon mi havas! &mdash; ofendite ekkriis la rufulo, kaj
        post<tld/>is en &scirc;ian dorson: &mdash; Stulta ino!
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 19</lok></fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
