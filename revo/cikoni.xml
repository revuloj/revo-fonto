<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cikoni.xml,v 1.30 2023/10/31 13:37:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>cikoni</rad>/o
</kap>

<drv mrk="cikoni.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="super" cel="cikoni1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Ciconia</trd>), granda longpieda
      <ref tip="lst" lst="voko:birdoj" cel="bird.vad0oj">vadbirdo</ref>,
      ofte nestanta sur tegmentoj kaj kamentuboj:
      <ekz>
        tie nestas birdoj, <tld/>o havas sian lo&gcirc;ejon sur abioj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 104:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o staris supre sur la tegmento, kie &gcirc;i havis sian neston
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj kaj pelikanoj flugis en vicoj simile al flugetantaj rubandoj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>oj iom post iom kolekti&gcirc;is, por ekflugi anta&ubreve; la vintro al la varmaj
        landoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Cikonioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni &gcirc;enerale konsideras la <tld/>on simbolo de feli&ccirc;o kaj fekundeco
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010711.php">Cikonioj interrete</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/f/fb/Ciconia_ciconia_-_01.jpg/640px-Ciconia_ciconia_-_01.jpg" prm="CC BY-SA 3.0"><ind>Blanka <tld/>o</ind> (Ciconia ciconia), Madrido, Hispanujo
      <fnt>
        <aut>Carlos Delgado, propra verko</aut>
        <lok><url ref="&WCW;/File:Ciconia_ciconia_-_01.jpg">&WCW;/File:Ciconia_ciconia_-_01.jpg</url></lok>
      </fnt>
    <trd lng="ca">cigonya blanca</trd>
    <trd lng="la">Ciconia ciconia</trd>
  </bld>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_s;&c_je;&c_l;</trd>,
    <trd>&c_b;&c_a;&c_c;&c_ja;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_shch;&c_malmol;&c_r;&c_k;&c_je;&c_l;</trd>
  <trd lng="br">c'hwibon</trd>
  <trd lng="cs">&ccaron;&aacute;p</trd>
  <trd lng="ca">cigonya</trd>
  <trd lng="de">Storch</trd>
  <trd lng="el">&pi;&epsilon;&lambda;&alfa;&rho;&gamma;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">stork</trd>
  <trd lng="es">cig&uuml;e&ntilde;a</trd>
  <trd lng="fr">cigogne</trd>
  <trd lng="he">&het;&samekh;&yod;&dalet;&he;</trd>
  <trd lng="hu">g&oacute;lya</trd>
  <trd lng="ja">&#x30B3;&#x30A6;&#x30CE;&#x30C8;&#x30EA;</trd>
  <trd lng="nl">ooievaar</trd>
  <trd lng="pl">bocian</trd>
  <trd lng="pt">cegonha</trd>
  <trd lng="ru">&c_a;&c_i;&c_s;&c_t;</trd>
  <trd lng="tr">leylek</trd>
  <trd lng="sk">bocian</trd>
  <trd lng="uk">&#x43B;&#x435;&#x43B;&#x435;&#x43A;&#x430;</trd>
  <trd lng="zh">&#x9E1B; <pr>gu&#xE0;n</pr></trd>
</drv>
</art>
