<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cikoni</rad>/o
</kap>
<drv mrk="cikoni.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cikoni.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Ciconia</trd>), granda longpieda vadbirdo,
      ofte nestanta sur tegmentoj kaj kamentuboj.
    </dif>
  </snc>
  <bld lok="../bld/cikoni.jpg">
    blankaj <tld/>oj (<trd lng="la">Ciconia ciconia)</trd>
  </bld>
  <trd lng="de">Storch</trd>
  <trd lng="fr">cigogne</trd>
  <trd lng="nl">ooievaar</trd>
  <trd lng="tr">leylek</trd>
</drv>
<drv mrk="cikoni.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo vadbirdoj
      (<trd lng="la">Ciconiidae</trd>).
    </dif>
  </snc>
  <trd lng="de">St&ouml;rche <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
