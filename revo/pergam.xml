<kap>
  <ofc>*</ofc>
  <rad>pergamen</rad>/o
</kap>
<drv mrk="pergam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Felo preparita por skribado kaj uzata precipe dum e&ubreve;ropa mezepoko:
      <ekz>
        Svisa pasporto en tiu tempo 1910 kaj 1911 estis ankora&ubreve; granda
        <tld/>a folio
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 17a, p. 47a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_r;&c_g;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_e;&c_r;&c_g;&c_a;&c_m;&c_e;&c_n;</trd>
  </trdgrp>
  <trd lng="br">parch</trd>
  <trd lng="ca">pergam&iacute;</trd>
  <trd lng="cs">pergamen</trd>
  <trd lng="de">Pergament</trd>
  <trd lng="en">parchment</trd>
  <trd lng="es">pergamino</trd>
  <trd lng="fr">parchemin</trd>
  <trd lng="hu">pergamen</trd>
  <trd lng="ja">&#x7F8A;&#x76AE;&#x7D19;  <pr>&#12424;&#12358;&#12402;&#12375;</pr></trd>
  <trd lng="nl">perkament</trd>
  <trd lng="pl">pergamin</trd>
  <trd lng="pt">pergaminho</trd>
  <trd lng="ro">pergament</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_g;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  <trd lng="sk">vyhladen&aacute; ov&ccaron;ia ko&zcaron;a <klr>(pou&zcaron;&iacute;van&aacute; na p&iacute;sanie| hlavne v stredoveku)</klr></trd>
  <trd lng="uk">&#x43F;&c_je;&c_r;&c_g;&c_a;&#x43C;&c_je;&#x43D;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x7F8A;&#x76AE;&#x7D19; <pr>y&#xE1;ngp&#xED;zh&#x1D0;</pr></trd>,
    <trd>&#x7F8A;&#x76AE;&#x7EB8; <pr>y&#xE1;ngp&#xED;zh&#x1D0;</pr></trd>,
    <trd>&#x7F8A;&#x76AE;&#x7D19;&#25991;&#x4EF6;
    <pr>y&#xE1;ngp&#xED;zh&#x1D0;w&#xE9;nji&#xE0;n</pr></trd>,
    <trd>&#x7F8A;&#x76AE;&#x7EB8;&#25991;&#x4EF6;
    <pr>y&#xE1;ngp&#xED;zh&#x1D0;w&#xE9;nji&#xE0;n</pr></trd>,
    <trd>&#x7F8A;&#x76AE;&#21367; <pr>y&#xE1;ngp&#xED;ju&#xE0;n</pr></trd>,
    <trd>&#x7F8A;&#x76AE;&#25458; <pr>y&#xE1;ngp&#xED;ju&#xE0;n</pr></trd>
  </trdgrp>
