<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: dioksin.xml,v 1.4 2023/11/04 14:29:02 afido Exp $">
    <kap>
      <rad>dioksin</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="dioksin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="dioksin.0o.molekulo">
    <dif>
      <frm>C<sub>4</sub>H<sub>4</sub>O<sub>2</sub></frm>, Organika
      oksigenhava heterocikla <ref tip="super" cel="moleku.0o">molekulo</ref>,
      konsiderebla kiel cikle fermita duobla <ref cel="eter.0o.klaso">etero</ref>
      de etendiolo (<frm>HO&ndash;CH=CH&ndash;OH</frm>).
    </dif>
  </snc>
  <snc mrk="dioksin.0o.klaso">
    <dif>
      Klaso de halogenhavaj triciklaj &hcirc;emiaj <ref tip="super" cel="kombin.0ajxo.KEM">kombina&jcirc;o</ref>j,
      enhavantaj tian molekulon, kaj el kiuj pluraj estas dan&gcirc;eraj por la naturo kaj homoj:
      <ekz>
        pro la <tld/>a skandalo en majo 1999 la belga terkultura industrio forte suferis
        <fnt>
          <aut>Guido van Damme</aut>
          <vrk><url ref="&Monato;/2001/007272.php">Reen al la edena paradizo</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        dioksin-komisiono <klr>[&#x2026;]</klr> esploras la fifaman krizon pri belgaj ovoj,
        kokinvianda&jcirc;oj, ktp
        <fnt>

          <aut>Guido van Damme</aut>
          <vrk><url ref="&Monato;/2000/006921.php">Belgio: Malvuali nigran fantomon</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_k;&c_s;&c_y;&c_n;</trd>
  <trd lng="de">Dioxin</trd>
  <trd lng="en">dioxin</trd>
  <trd lng="fr">dioxine</trd>
  <trd lng="nl">dioxine</trd>
  <trd lng="zh">&#x6234;&#x5965;&#x8F9B; <pr>d&#xE0;i&#xE0;ox&#x12B;n</pr></trd>
</drv>

  </art>

