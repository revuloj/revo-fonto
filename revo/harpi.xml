<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>harpi</rad>/o
</kap>
<drv mrk="harpi.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="harpi.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Fabela monstro, simbolanta ventegon kaj kiun oni prezentis kun virina
      viza&gcirc;o kaj vultura korpo.
    </dif>
    <trd lng="de">Harpyie</trd>
    <trd lng="es">harp&iacute;a</trd>
    <trd lng="nl">harpij</trd>
  </snc>
  <snc mrk="harpi.0o.virino">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Virino kun treege malakordi&gcirc;ema karaktero.
    </dif>
    <trd lng="es">harp&iacute;a</trd>
    <trd lng="nl">harpij</trd>
  </snc>
  <snc mrk="harpi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="akcipi1.0oj">akcipitredoj</ref>
      (<trd lng="la">Harpia</trd>), granda sudamerika rabobirdo,
      vivanta en pluvarbaro.
    </dif>
    <trd lng="de">Harpyie</trd>
    <trd lng="en">harpy eagle</trd>
    <trd lng="nl">Zuid-Amerikaanse <ind>arend</ind></trd>
  </snc>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_p;&c_i;&c_ja;</trd>
</drv>
</art>
