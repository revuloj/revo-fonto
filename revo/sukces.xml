<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sukces.xml,v 1.29 2021/05/30 09:52:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sukces</rad>/i
</kap>
<drv mrk="sukces.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Havi feli&ccirc;an rezultaton.
    </dif>
    <trd lng="ca">resultar</trd>
    <trdgrp lng="de">
      <trd>gelingen</trd>,
      <trd>gl&uuml;cken</trd>,
      <trd>erfolgreich verlaufen</trd>
    </trdgrp>
    <trd lng="hu">siker&uuml;l</trd>    
    <trdgrp lng="ru">
      <trd>&c_i;&c_m;&c_je;&c_t;&c_mol; <ind>&c_u;&c_s;&c_p;&c_je;&c_h;</ind></trd>,
      <trd>&c_u;&c_d;&c_a;&c_s;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Atingi feli&ccirc;an deziratan rezultaton en entrepreno:
      <ekz>
        se malri&ccirc;ulo <tld/>as, li &ccirc;iujn forgesas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kiun la sorto karesos tiu <tld/>os
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i en, pri io;
      </ekz>
      <ekz>
        fine li <tld/>is altiri la atenton.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="prospe1.0i">prosperi</ref>,
      <ref cel="stat">bonstati</ref>,
      <ref cel="flor.0i">flori</ref>.
    </refgrp>
    <trdgrp lng="ca">
      <trd>aconseguir de</trd>    
      <trd>reeixir</trd>,
      <trd>sortir-se'n</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Erfolg haben</trd>,
      <trd>erfolgreich sein</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>tener feliz resultado</trd>,
      <trd>ocurrir felizmente</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sikeres</trd>,
      <trd>siker&uuml;l neki</trd>
    </trdgrp>
    <trdgrp lng="oc">
      <trd>arribar</trd>,
      <trd>capitar</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_je;&c_u;&c_s;&c_p;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_o;&c_s;&c_t;&c_i;&c_g;&c_n;&c_u;&c_t;&c_mol; <ind>&c_u;&c_s;&c_p;&c_je;&c_h;&c_a;</ind></trd>,
      <trd>&c_s;&c_m;&c_o;&c_ch;&c_mol;</trd>,
      <trd>&c_s;&c_u;&c_m;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_c;&c_mol; <ind>&c_p;&c_o;&c_s;&c_mol;&c_p;&c_je;&c_h;</ind></trd>,
    <trd>&c_d;&c_a;&c_s;&c_ja;&c_g;&c_a;&c_c;&c_mol; <ind>&c_p;&c_o;&c_s;&c_mol;&c_p;&c_je;&c_h;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dok&aacute;zat</trd>,
    <trd>m&iacute;t &uacute;sp&ecaron;ch</trd>,
    <trd>sta&ccaron;it n&ecaron;co ud&ecaron;lat</trd>,
    <trd>um&ecaron;t</trd>,
    <trd>usp&ecaron;t</trd>
  </trdgrp>
  <trd lng="en">succeed</trd>
  <trd lng="fr">r&eacute;ussir</trd>
  <trd lng="ne">&#x938;&#x92B;&#x932;&#x924;&#x93E;</trd>
  <trdgrp lng="nl">
    <trd>slagen</trd>,
    <trd>succes hebben</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>docieli&tcaron;</trd>,
    <trd>dok&aacute;za&tcaron;</trd>,
    <trd>ma&tcaron; &uacute;spech</trd>,
    <trd>uspie&tcaron;</trd>
  </trdgrp>
  <trd lng="tp">kama</trd>
</drv>

<drv mrk="sukces.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Feli&ccirc;a atingita rezultato:
      <ekz>
        liaj verkoj havis, akiris, ricevis, &gcirc;uis, atingis entuziasman
        <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">sukses</trd>
  <trd lng="be">&c_p;&c_o;&c_s;&c_mol;&c_p;&c_je;&c_h;</trd>
  <trd lng="bg">&c_u;&c_s;&c_p;&c_je;&c_h;</trd>
  <trd lng="bo">&#xF62;&#xF92;&#xFB1;&#xF63;&#xF0B;&#xF41;&#xF0B;&#xF50;&#xF7C;&#xF56;</trd>
  <trd lng="ca">&egrave;xit</trd>
  <trdgrp lng="cs">
    <trd>sukces</trd>,
    <trd>zdar</trd>,
    <trd>&uacute;sp&ecaron;ch</trd>
  </trdgrp>
  <trd lng="de">Erfolg</trd>
  <trd lng="en">success</trd>
  <trdgrp lng="es">
    <trd>&eacute;xito</trd>,
    <trd>suceso</trd>
  </trdgrp>
  <trd lng="fr">succ&egrave;s</trd>
  <trd lng="hu">siker</trd>
  <trd lng="nl">succes</trd>
  <trd lng="pt">sucesso</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_p;&c_je;&c_h;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_ch;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zdar</trd>,
    <trd>&uacute;spech</trd>
  </trdgrp>
  <trd lng="sq">suksesi</trd>
</drv>

<drv mrk="sukces.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Tia, ke &gcirc;i <tld/>is:
      <ekz>
        Kaj nun mi deziras al vi &ccirc;iuj gajan feston kaj <tld/>an
        laboron.
        <fnt><bib>Paroloj</bib>, <lok>Sepa Kongreso 1911 en
        Antwerpen</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_p;&c_ja;&c_h;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>exit&oacute;s</trd>,
    <trd>reeixit</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>poda&rcaron;en&yacute;</trd>,
    <trd>&uacute;sp&ecaron;&scaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">erfolgreich</trd>
  <trd lng="es">exitoso/a</trd>
  <trd lng="fr">r&eacute;ussi(e)</trd>
  <trd lng="sk">&uacute;spe&scaron;n&yacute;</trd>
</drv>


<drv mrk="sukces.mal0i">
  <kap>mal<tld/>i</kap>
  <snc>
    <dif>
      Ne <tld/>i
    </dif>
    <refgrp tip="sin">
      <ref cel="bankro.0i">bankroti</ref>,
      <ref cel="fiask.0i">fiaski</ref>,
      <ref cel="ruin">runini&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja; &c_m;&c_je;&c_c;&c_mol; <ind>&c_p;&c_o;&c_s;&c_mol;&c_p;&c_je;&c_h;&c_u;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_b;&c_je;&c_s;&c_p;&c_a;&c_s;&c_mol;&c_p;&c_ja;&c_h;&c_o;&c_v;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>fracassar</trd>,
    <trd>fallir</trd>,
    <trd>esguerrar-se</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>neusp&ecaron;t</trd>,
    <trd>selhat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>missgl&uuml;cken</trd>,
    <trd>Misserfolg haben</trd>
  </trdgrp>
  <trd lng="en">fail</trd>
  <trd lng="es">fracasar</trd>
  <trd lng="fr">rater</trd>
  <trdgrp lng="hu">
    <trd>nem siker&uuml;l</trd>,
    <trd>meghi&uacute;sul</trd>,
    <trd>megbukik</trd>,
    <trd>elbukik</trd>
  </trdgrp>
  <trd lng="nl">mislukken</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol; <ind>&c_n;&c_je;&c_u;&c_d;&c_a;&c_ch;&c_u;</ind></trd>,
    <trd>&c_n;&c_je; <ind>&c_s;&c_m;&c_o;&c_ch;&c_mol;</ind></trd>,
    <trd>&c_n;&c_je; <ind>&c_s;&c_u;&c_m;&c_je;&c_t;&c_mol;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ma&tcaron; ne&uacute;spech</trd>,
    <trd>prepadn&uacute;&tcaron;</trd>,
    <trd>zlyha&tcaron;</trd>
  </trdgrp>
</drv>
</art>
