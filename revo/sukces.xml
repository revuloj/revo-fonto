<drv mrk="sukces.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Tia, ke &gcirc;i <tld/>as:
      <ekz>
        ni eksciu, &ccirc;u estos <tld/>a nia vojo
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 18:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estos ankora&ubreve; malpli <tld/>a ol la fratoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu penado estis <tld/>a kaj rapida
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a argumentado kaj konvinkado
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun mi deziras al vi &ccirc;iuj gajan feston kaj <tld/>an
        laboron
        <fnt><bib>Paroloj</bib>, <lok>Sepa Kongreso 1911 en
        Antwerpen</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a venkanto de pordoj malfermitaj
        <fnt>
          <bib>PrV</bib>
          <lok>S</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxanc.bon0a">bon&scirc;anca</ref>,
      <ref cel="felicx.0a.sukcesanta">feli&ccirc;a<sncref/></ref>,
      <ref cel="prospe1.0a">prospera</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_p;&c_ja;&c_h;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="ca">
