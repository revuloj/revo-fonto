<drv mrk="sukces.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Tia, ke &gcirc;i <tld/>is:
      <ekz>
        Kaj nun mi deziras al vi &ccirc;iuj gajan feston kaj <tld/>an
        laboron.
        <fnt><bib>Paroloj</bib>, <lok>Sepa Kongreso 1911 en
        Antwerpen</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_p;&c_ja;&c_h;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>exit&oacute;s</trd>,
    <trd>reeixit</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>poda&rcaron;en&yacute;</trd>,
    <trd>&uacute;sp&ecaron;&scaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="de">erfolgreich</trd>
  <trd lng="es">exitoso/a</trd>
  <trd lng="fr">r&eacute;ussi(e)</trd>
  <trd lng="sk">&uacute;spe&scaron;n&yacute;</trd>
</drv>


<drv mrk="sukces.mal0i">
  <kap>mal<tld/>i</kap>
  <snc>
    <dif>
      Ne <tld/>i
    </dif>
    <refgrp tip="sin">
      <ref cel="bankro.0i">bankroti</ref>,
      <ref cel="fiask.0i">fiaski</ref>,
      <ref cel="ruin">runini&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja; &c_m;&c_je;&c_c;&c_mol; <ind>&c_p;&c_o;&c_s;&c_mol;&c_p;&c_je;&c_h;&c_u;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_b;&c_je;&c_s;&c_p;&c_a;&c_s;&c_mol;&c_p;&c_ja;&c_h;&c_o;&c_v;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>fracassar</trd>,
    <trd>fallir</trd>,
    <trd>esguerrar-se</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>neusp&ecaron;t</trd>,
    <trd>selhat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>missgl&uuml;cken</trd>,
    <trd>Misserfolg haben</trd>
  </trdgrp>
  <trd lng="en">fail</trd>
  <trd lng="es">fracasar</trd>
  <trd lng="fr">rater</trd>
  <trdgrp lng="hu">
    <trd>nem siker&uuml;l</trd>,
    <trd>meghi&uacute;sul</trd>,
    <trd>megbukik</trd>,
    <trd>elbukik</trd>
  </trdgrp>
  <trd lng="nl">mislukken</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol; <ind>&c_n;&c_je;&c_u;&c_d;&c_a;&c_ch;&c_u;</ind></trd>,
    <trd>&c_n;&c_je; <ind>&c_s;&c_m;&c_o;&c_ch;&c_mol;</ind></trd>,
    <trd>&c_n;&c_je; <ind>&c_s;&c_u;&c_m;&c_je;&c_t;&c_mol;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ma&tcaron; ne&uacute;spech</trd>,
    <trd>prepadn&uacute;&tcaron;</trd>,
    <trd>zlyha&tcaron;</trd>
  </trdgrp>
