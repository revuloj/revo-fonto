<drv mrk="sukces.0ema">
  <kap>
    <tld/>ema
  </kap>
  <snc>
    <dif>
      Celanta, strebanta <tld/>on:
      <ekz>
        <tld/>emaj kuracadoj
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia edzo estis <tld/>ema komercisto
        <fnt>
          <vrk><url ref="https://www.eventoj.hu/arkivo/eve-053.htm"
              >Historio, Zilbernika genealogio</url></vrk>
          <lok>Eventoj n-ro 0053, 1/majo - '94</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Betli</nom> estis homo <tld/>ema kaj opiniis, ke &ccirc;iuj aliaj devas same &scirc;ati la vivon,
        kiel li
        <fnt>
          <aut>Sever Gansovskij, trad. Nina Cvileneva</aut>
          <vrk><url ref="https://www.esperanto.mv.ru/Arkivoj/Fantastiko/Dies_irae.html"
              >Dies irae</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ambici.0a">ambicia</ref>,
      <ref cel="streb.0ema">strebema</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_m;&c_b;&c_ib;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_y; &c_d;&c_a; <ind>&c_p;&c_o;&c_s;&c_mol;&c_p;&c_je;&c_h;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ehrgeizig</trd>,
    <trd>erfolgshungrig</trd>,
    <trd>ambitioniert</trd>
  </trdgrp>
</drv>

<drv mrk="sukces.mal0i">
  <kap>mal<tld/>i</kap>
  <snc>
    <dif>
      Ne <tld/>i:
      <ekz>
        &ccirc;iuj liaj klopodoj mal<tld/>is
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi mal<tld/>is edzi&gcirc;on, kaj vi suferas pro tio
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia provo ridinde mal<tld/>is
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam mi mal<tld/>is en la anta&ubreve;lasta klaso, mi forlasis la instrufabrikon kaj
        iris al la maro
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Pa&ubreve;lo <nom>Petrovi&ccirc;</nom> mem rimarkis, ke lia sprita&jcirc;o mal<tld/>is kaj komencis paroli pri
        la terkulturaj aferoj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>V</lok>
        </fnt>;
      </ekz>
      <ekz>
        la afero mal<tld/>is
        <fnt>
          <bib>PrV</bib>
          <lok>L</lok>
        </fnt>.
      </ekz>
    </dif>
