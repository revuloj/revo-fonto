<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: urodel.xml,v 1.16 2022/03/28 11:08:54 revo Exp $">
<kap>
   <rad>urodel</rad>/oj
</kap>
<drv mrk="urodel.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_ordoj" cel="ord.0o.BIO">Ordo</ref>
      el la klaso <ref tip="super" cel="amfibi.0oj">amfibioj</ref>
      (<trd lng="la">Urodela</trd>), havantaj voston:
      <ekz>
        la &ccirc;ina giganta salamandro (<nom>Megalobatrachus davidianus</nom> 
        a&ubreve; <nom>Andrias davidianus</nom>)
        estas amfibio el la ordo de <tld/>oj kaj familio de kriptobran&hcirc;edoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49683">&Ccirc;ina giganta salamandro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la tilototritono (<nom>Tylototriton</nom> spp.) estas el la ordo de <tld/>oj kaj familio de
        salamandredoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49684">Tilototritono</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trd lng="be">&c_h;&c_v;&c_a;&c_s;&c_t;&c_a;&c_t;&c_y;&c_ja;<klr tip="amb"> (&c_a;&c_t;&c_r;&c_a;&c_d;
   &c_z;&c_je;&c_m;&c_n;&c_a;&c_v;&c_o;&c_d;&c_n;&c_y;&c_h;)</klr></trd>
   <trd lng="bg">&c_o;&c_p;&c_a;&c_sh;&c_a;&c_t;&c_i; &c_z;&c_je;&c_m;&c_n;&c_o;&c_v;&c_o;&c_d;&c_n;&c_i;</trd>
   <trd lng="ca">urodels</trd>
   <trd lng="de">Schwanzlurche</trd>
   <trd lng="en">salamanders</trd>
   <trd lng="es">urodelos</trd>
   <trd lng="fi">pyrst&ouml;sammakot</trd>
   <trd lng="fr">urod&egrave;les</trd>
   <trd lng="hu">farkos <ind>k&eacute;t&eacute;lt&udblac;ek</ind></trd>
   <trdgrp lng="it">
   <trd>urodeli</trd>, 
   <trd>caudati</trd>
   </trdgrp>
   <trd lng="nl">salamanders<klr tip="ind"> (fam.)</klr></trd>
   <trd lng="pl">p&lstroke;azy ogoniaste</trd>
   <trdgrp lng="pt">
    <trd>urodelos</trd>
   </trdgrp>
   <trd lng="ru">&c_h;&c_v;&c_o;&c_s;&c_t;&c_a;&c_t;&c_y;&c_je; &c_z;&c_je;&c_m;&c_n;&c_o;&c_v;&c_o;&c_d;&c_n;&c_y;&c_je;<klr tip="amb"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
   <trd lng="sv">stj&auml;rtgroddjur</trd>
  <trd lng="uk">&#x445;&#x432;&#x43E;&#x441;&#x442;&#x430;&#x442;&#x456; &#x437;&#x435;&#x43C;&#x43D;&#x43E;&#x432;&#x43E;&#x434;&#x43D;&#x456;</trd>
</drv>
</art>
