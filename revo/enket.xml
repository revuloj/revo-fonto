    <trd>&#27665;&#24847;&#27979;&#35797; <pr>m&iacute;ny&igrave;c&egrave;sh&igrave;</pr></trd>,
    <trd>&#21839;&#21367;&#35519;&#26597; <pr>w&egrave;nju&agrave;ndi&agrave;och&aacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="enket.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      <tld lit="E"/>e <ref tip="super" cel="esplor.0i">esplori</ref>:
      <ekz>
        la polico nun <tld/>as pri pliaj 140 eventuale suspektaj mortoj
        <fnt>
       <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2000/006872.php">Doktoro Morto</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        usonaj kaj koreaj sciencistoj <tld/>is pri la influo de
        aermalpura&jcirc;oj sur la pezon de koreaj beboj
        <fnt>
        <aut>P. S. Viana</aut>, <vrk>Tro malpezaj beboj</vrk>,
        <bib>Monato</bib>, <lok>2002/03, p.
        20</lok></fnt>;
      </ekz>
      <ekz>
        ja anta&ubreve;e lernejanoj seksumis: tiam, tamen, tio estis socie
        netolerebla kaj do
        neniu parolis, neniu prie <tld/>is
         <fnt>
          <aut>Guido van Damme</aut>
          <vrk><url ref="&Monato;/1995/A950102.php">Gejunula seksumado</url></vrk>
          <bib>Monato</bib>
          <lok>1995/01, p. 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        post vasta <tld/>ado inter religiuloj evidenti&gcirc;is ke ili ne
        esprimis
        malagrablajn sentojn rilate la virgulinon
        <fnt>
          <aut>div.</aut>
          <vrk><url ref="&Monato;/1993/A931002.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/10, p. 4</lok>
         </fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_mol;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>investigar</trd>,
    <trd>enquestar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dotazovat se <klr>(anketn&ecaron;)</klr></trd>,
    <trd>prov&aacute;d&ecaron;t anketu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erheben</trd>,
    <trd>ermitteln</trd>,
    <trd>untersuchen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>make an inquiry</trd>,
    <trd>investigate</trd>,
    <trd>survey</trd>
  </trdgrp>
  <trd lng="es">encuestar</trd>
  <trd lng="fr">enqu&ecirc;ter</trd>
  <trd lng="he">&lamed;&het;&qof;&vav;&resh;</trd>
  <trd lng="it">indagare</trd>
  <trdgrp lng="ja">
