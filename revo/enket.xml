  <trd lng="ru">&c_r;&c_a;&c_s;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">prev&aacute;dza&tcaron; anketu</trd>
