    <trd>inchiesta</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#35519;&#26619;  <pr>&#12385;&#12423;&#12358;&#12373;</pr></trd>,
    <trd>&#12450;&#12531;&#12465;&#12540;&#12488;  <pr>&#12354;&#12435;&#12369;&#12540;&#12392;</pr></trd>,
