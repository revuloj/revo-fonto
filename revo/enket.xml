<drv mrk="enket.opini0o">
  <kap>opini<tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc mrk="enket.opini0o.opinisondo">
    <dif>
      <ref tip="dif" cel="sond.opini0o">Opinisondo</ref>:
      <ekz>
        anta&ubreve; jaro plej granda partio en la opini<tld/>oj estis la
        Progresa Partio
        <fnt><aut>B. Hekland</aut>, <vrk>Triumfas
        tedulo</vrk>, <bib>Monato</bib>, <lok>jaro 2001a, numero 11a, p.
        6a</lok></fnt>;
      </ekz>
      <ekz>
        grava inform-esplora organiza&jcirc;o en Pekino faris opini<tld/>on,
        por informi&gcirc;i,
        kiel peza estos tiu &scirc;ar&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2006/009055.php">Pezaj enlandaj &scirc;ar&gcirc;oj por la
          registaro</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        la opini<tld/>o de majo 1990 <klr>[&#x2026;]</klr> en Sarajevo kontra&ubreve;
        la militdan&gcirc;ero
        montris, ke ties popolo ne deziris la disi&gcirc;on
        <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio: 
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_y;&c_t;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_k;&c_a;&c_j; &c_d;&c_u;&c_m;&c_k;&c_ib;)</klr></trd>
  <trd lng="de">Meinungsumfrage</trd>
  <trd lng="en">opinion survey</trd>
  <trdgrp lng="es">
    <trd>encuesta de opini&oacute;n</trd>,
    <trd>sondeo<klr> (encuesta de opini&oacute;n)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enqu&ecirc;te d'opinion</trd>,
    <trd>sondage<klr> (enqu&ecirc;te d'opinion)</klr></trd>
