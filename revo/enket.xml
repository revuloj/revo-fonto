  <trdgrp lng="uk">
    <trd>&c_d;&c_o;&c_s;&c_l;&c_ib;&c_d;&c_zh;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_s;&c_l;&c_ib;&c_d;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
