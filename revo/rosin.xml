<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rosin.xml,v 1.16 2025/04/05 10:02:34 revo Exp $">
<kap>
  <rad>rosin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rosin.0o">
  <kap><tld/>o</kap>
  <snc mrk="rosin.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="ber.sekvin0o.KUI">Sekvinbero</ref>:
      <ekz>
        &scirc;i &scirc;utis plenmane avelojn, migdalojn kaj <tld/>ojn por
        man&gcirc;eto
        <fnt>
          <aut>Mahbube Mirkadiri, Nazi Solat trad., Tomasz Chmielik
          trad.</aut>,
          <vrk>Donaco el voja&gcirc;o</vrk>,
          <bib>LOdE</bib>,
          <lok>jaro 2003a, numero 3a (101)</lok>
        </fnt>;
      </ekz>
      <ekz>
        tradicia e&ubreve;ropa fruktosalato konsistas el &scirc;elitaj dispecigitaj pomoj, piroj,
        oran&gcirc;oj kaj bananoj <klr>[&#8230;]</klr> anka&ubreve; sekaj fruktoj kiel <tld/>oj, aveloj,
        migdaloj ktp. estas foje aldonitaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=248072">Fruktosalato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>sekvinbero</ke>, <ke>seka vinbero</ke>.
  </rim>

  <trd lng="be">&c_r;&c_a;&c_z;&c_y;&c_n;&c_k;&c_a;</trd>
  <trd lng="bg">&c_s;&c_t;&c_a;&c_f;&c_i;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>hrozinka</trd>,
    <trd>hrozinky</trd>,
    <trd>rozinka</trd>,
    <trd>rozinky</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rosine</trd>,
    <trd>Korinthe</trd>
  </trdgrp>
  <trd lng="en">raisin</trd>
  <trdgrp lng="fr">
    <trd>raisin sec</trd>,
    <trd>raisin de Corinthe</trd>
  </trdgrp>
  <trd lng="hu">mazsola</trd>
  <trd lng="ja">&#12524;&#12540;&#12474;&#12531;</trd>
  <trd lng="pl">rodzynek</trd>
  <trdgrp lng="pt">
    <trd>uva-passa</trd>,
    <trd>passa <klr>(de uva)</klr></trd>
  </trdgrp>
  <trd lng="sk">hrozienko</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#1086;&c_d;&c_z;&c_i;&#1085;&#1082;&c_a;</trd>,
    <trd>&c_ib;&c_z;&#1102;&#1084;</trd>,
    <trd>&#1082;&c_i;&c_sh;&#1084;&c_i;&c_sh;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33889;&#33796;&#20094; <pr>p&#250;taog&#257;n</pr></trd>,
    <trd>&#33889;&#33796;&#24178; <pr>p&#250;taog&#257;n</pr></trd>,
    <trd>&#33889;&#33796;&#20094;&#20818; <pr>p&#250;taog&#257;nr</pr></trd>,
    <trd>&#33889;&#33796;&#24178;&#20799; <pr>p&#250;taog&#257;nr</pr></trd>,
    <trd>&#33889;&#33796;&#20094; <pr>p&#250;t&#225;og&#257;n</pr></trd>,
    <trd>&#33889;&#33796;&#24178; <pr>p&#250;t&#225;og&#257;n</pr></trd>
  </trdgrp>
</drv>
</art>
