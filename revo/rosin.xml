<art mrk="$Id$">
<kap>
  <rad>rosin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rosin.0o">
  <kap><tld/>o</kap>
  <snc mrk="rosin.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Sekigita vinbero:
      <ekz>
        &scirc;i &scirc;utis plenmane avelojn, migdalojn kaj <tld/>ojn por man&gcirc;eto
        <fnt>
        <aut>Mahbube Mirkadiri, Nazi Solat trad., Tomasz Chmielik
        trad.</aut>,
        <vrk>Donaco el voja&gcirc;o</vrk>,
        <bib>LOdE</bib>,
        <lok>jaro 2003a, numero 3a (101)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_y;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>raisin sec</trd>,
    <trd>raisin de Corinthe</trd>
  </trdgrp>
</drv>
</art>

