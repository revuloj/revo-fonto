<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>perlamot</rad>/o
</kap>
<drv mrk="perlam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Blanka materio kun &ccirc;ielarkaj rebriloj, teganta la internon de kelkaj
      specoj de markonkoj kaj uzata por fari butonojn, ventumilojn,
      tenilon de tran&ccirc;iloj kc.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Perlmutter</trd>,
    <trd>Permutt</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>paarlemoer</trd>,
    <trd>parelmoer</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_l;&c_a;&c_m;&c_u;&c_t;&c_r;</trd>
</drv>
</art>
