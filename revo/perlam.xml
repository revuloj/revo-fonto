<kap>
  <ofc>*</ofc>
  <rad>perlamot</rad>/o
</kap>
<drv mrk="perlam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Blanka kalka substanco kun &ccirc;ielarkaj rebriloj, teganta la internon de kelkaj
      specoj de markonkoj kaj uzata por fari butonojn, ventumilojn,
      tenilojn de tran&ccirc;iloj kaj ceterajn:
      <ekz>
        ili <klr>...</klr> malfermis la &scirc;rankon inkrustitan per <tld/>o
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 51a, p. 159a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_l;&c_ja;&c_m;&c_u;&c_t;&c_r;</trd>
  <trd lng="br">nakr</trd>
  <trd lng="cs">perle&tcaron;</trd>
  <trdgrp lng="de">
    <trd>Perlmutter</trd>,
    <trd>Perlmutt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mother of pearl</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>nacre</trd>
  </trdgrp>
  <trd lng="hu">gy&ouml;ngyh&aacute;z</trd>
  <trdgrp lng="ja">
    <trd>&#x771F;&#x73E0;&#x5C64;  <pr>&#x3057;&#x3093;&#x3058;&#x3085;&#x305D;&#x3046;</pr></trd>,
    <trd>&#x87BA;&#x923F;  <pr>&#x3089;&#x3067;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>paarlemoer</trd>,
    <trd>parelmoer</trd>
  </trdgrp>
  <trd lng="pl">masa per&lstroke;owa</trd>
  <trdgrp lng="pt">
    <trd>madrep&eacute;rola</trd>,
    <trd>n&aacute;car</trd>
  </trdgrp>
  <trd lng="ro">sidef</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_l;&c_a;&c_m;&c_u;&c_t;&c_r;</trd>
  <trd lng="sk">perle&tcaron;</trd>
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x43B;&#x430;&#x43C;&#x443;&#x442;&#x440;</trd>
  <trdgrp lng="zh">
    <trd>&#x73E7; <pr>y&#xE1;o</pr></trd>,
    <trd>&#x7464; <pr>y&#xE1;o</pr></trd>,
    <trd>&#x7476; <pr>y&#xE1;o</pr></trd>,
    <trd>&#x73E0;&#x6BCD;&#x8C9D; <pr>zh&#x16B;m&#x1D4;b&#xE8;i</pr></trd>,
    <trd>&#x73E0;&#x6BCD;&#x8D1D; <pr>zh&#x16B;m&#x1D4;b&#xE8;i</pr></trd>
  </trdgrp>
</drv>
