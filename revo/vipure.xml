<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vipure.xml,v 1.16 2022/03/28 11:08:55 revo Exp $">
<kap>
  <rad>vipered</rad>/oj <fnt><bib>PIV3</bib></fnt>
</kap>
<drv mrk="vipure.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="skvam.0uloj">skvamuloj</ref> (<trd lng="la">Viperidae</trd>):
      <ekz>
        <tld/>oj estas familio de venenaj serpentoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=157246">Viperedoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej gravaj familioj estas kolubredoj kaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8442">Serpentoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="vipur.0o">vipero</ref>, 
      <ref cel="krotal.0o">krotalo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_d;&c_z;&c_ju;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_o;&c_t;&c_r;&c_o;&c_v;&c_n;&c_i;&c_c;&c_i;</trd>
  <trd lng="ca">vip&egrave;rids</trd>
  <trd lng="de">Vipern<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="en">vipers<klr tip="ind"> (family)</klr></trd>
  <trd lng="fr">Vip&eacute;rid&eacute;s</trd>
  <trd lng="hu">viperaf&eacute;l&eacute;k</trd>
  <trd lng="nl">adders<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pt">viper&iacute;deos</trd>
  <trd lng="ru">&c_g;&c_a;&c_d;&c_ju;&c_k;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x433;&#x430;&#x434;&#x44E;&#x43A;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>
