  <art mrk="$Id: cxuvasx.xml,v 1.2 2023/08/31 10:10:28 afido Exp $">
    <kap>
      <rad>&ccirc;uva&scirc;</rad>/o
    </kap>

<drv mrk="cxuvasx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ano de tjurka etno el la regiono de meza Volgo:
      <ekz>
        <tld/>oj parolas la <tld/>an lingvon kaj estas
        ortodoksaj kristanoj kun kelkaj
        paganaj tradicioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=12355">&Ccirc;uva&scirc;oj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;tomata tradukilo el la turka al la <tld/>a, kiun nia
        esplorgrupo disvolvas
        <fnt>
          <bib>Monato</bib>
          <aut>H&egrave;ctor Al&ograve;s i Font</aut>
          <vrk><url ref="&Monato;/2013/011015.php">La turka:
          pli granda, pli diversa familio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Tschuwasche</trd>
    <trd lng="en">Chuvash</trd>
    <trd lng="fr">Tchouvache</trd>
    <trd lng="ru">&c_ch;&c_u;&c_v;&c_a;&c_sh;</trd>
  </snc>
  <trd lng="ja">&#x30C1;&#x30E5;&#x30D0;&#x30B7;&#x4EBA;  [<ind>&#x30C1;&#x30E5;&#x30D0;&#x30B7;&#x3058;&#x3093;</ind>]</trd>
</drv>

  </art>

