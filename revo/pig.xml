<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pig</rad>/o
</kap>
<drv mrk="pig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pig.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="korved.0oj">korvedoj</ref>
      (<trd lng="la">Pica</trd>), kun okulfrapa nigra blanka plumaro
      kaj longa vosto.
    </dif>
  </snc>                     
  <trd lng="be">&c_s;&c_a;&c_r;&c_o;&c_k;&c_a;</trd>
  <trd lng="br">pig</trd>
  <trd lng="ca">garsa</trd>
  <trd lng="cs">straka</trd>
  <trd lng="de">Elster</trd>
  <trd lng="en">magpie</trd>
  <trd lng="fr">pie</trd>
  <trd lng="hu">szarka</trd>
  <trd lng="nl">ekster</trd>
  <trd lng="pl">sroka</trd>
  <trd lng="pt">pega (&ecirc;)</trd>
  <trd lng="ru">&c_s;&c_o;&c_r;&c_o;&c_k;&c_a;</trd>
</drv>
</art>
