<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pig.xml,v 1.28 2024/02/17 06:43:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pig</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pig.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="korved.0oj">korvedoj</ref>
      (<trd lng="la">Pica</trd>), kun okulfrapa nigra blanka plumaro
      kaj longa vosto:
      <ekz>
        la  <tld/>oj kaj kornikoj kaj anka&ubreve; la monedoj timegi&gcirc;is de la krioj kaj bruo
        <fnt>
          <bib>Fab4</bib>
          <lok>Sonorila profundo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_o;&c_k;&c_a;</trd>
  <trd lng="bg">&c_s;&c_v;&c_r;&c_a;&c_k;&c_a;</trd>
  <trd lng="br">pig</trd>
  <trd lng="ca">garsa</trd>
  <trdgrp lng="cs">
    <trd>straka</trd>,
    <trd>straka obecn&aacute;</trd>
  </trdgrp>
  <trd lng="de">Elster</trd>
  <trd lng="en">magpie</trd>
  <trd lng="fr">pie</trd>
  <trd lng="hu">szarka</trd>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30B5;&#x30B5;&#x30AE;</trd>,
    <trd>&#x30AB;&#x30C1;&#x30AC;&#x30E9;&#x30B9;</trd>
  </trdgrp>
  <trd lng="nl">ekster</trd>
  <trd lng="pl">sroka</trd>
  <trd lng="pt">pega (&ecirc;)</trd>
  <trd lng="ru">&c_s;&c_o;&c_r;&c_o;&c_k;&c_a;</trd>
  <trd lng="sk">straka</trd>
  <trd lng="uk">&c_s;&#x43E;&c_r;&#x43E;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x9D72; <pr>qu&#xE8;</pr></trd>,
    <trd>&#x9E4A; <pr>qu&#xE8;</pr></trd>,
    <trd>&#x559C;&#x9D72; <pr>x&#x1D0;qu&#xE8;</pr></trd>,
    <trd>&#x559C;&#x9E4A; <pr>x&#x1D0;qu&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pig.0a">
  <kap><tld/>a</kap>
  <snc mrk="pig.0a.KOL">
    <uzo tip="klr">(precipe pri besta felo)</uzo>
    <dif>
      de har<ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>
      en kiu alternas blankaj kaj malhelaj, ordinare nigraj makuloj,
      simile al la plumaro de <tld/>o:
      <ekz>
        la granda piga kato de nia familio estas efektive stranga en
        karaktero
        <fnt>
          <aut>Lao She, trad. Pej Pia&ubreve;</aut>
          <vrk><url ref="&CRI;2841/2018/04/18/173s198597.htm">
          Nia familia kato</url></vrk>
          <lok>&Ccirc;ina Radio Internacia, 2018-04-18</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bej1.0a.KOL">beja</ref>,
      <ref cel="blond.0a.hararo">blonda</ref>,
      <ref cel="brun.0a">bruna</ref>,
      <ref cel="ruan.0a.KOL">ruana</ref>,
      <ref cel="ruf.0a.KOL">rufa</ref>
    </refgrp>
    <trd lng="fr">pie<klr> (couleur)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_y;</trd>,
    <trd>&c_p;&c_ja;&c_r;&c_e;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_k;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30B5;&#x30B5;&#x30AE;&#x306E;</trd>,
    <trd>&#x767D;&#x9ED2;&#x307E;&#x3060;&#x3089;&#x306E;  <pr>&#x3057;&#x308D;&#x304F;&#x308D;&#x307E;&#x3060;&#x3089;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43E;&c_r;&#x43E;&c_ch;&c_a;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_r;&#x43E;&#x43A;&c_a;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_r;&#x44F;&c_b;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_ib;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
