<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kajt.xml,v 1.29 2023/12/02 21:20:04 revo Exp $">
<kap>
   <rad>kajt</rad>/o
</kap>
<drv mrk="kajt.0o">
   <kap><tld/>o</kap>
   <snc mrk="kajt.0o.SPO">
     <uzo tip="fak">SPO</uzo>
     <dif>
       <ref tip="lst" cel="lud.0ilo" lst="voko:ludiloj">Ludilo</ref>,
       konsistanta el malpeza skeleto kovrita per papero 
a&ubreve; &scirc;tofo,
       kiun oni levas en aero tirante &gcirc;in kontra&ubreve; la vento:
       <ekz>
         la knabinoj a&ubreve;dis pri flugigo de <tld/>o
         <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 70a, p. 
493a</lok></fnt>.
       </ekz>
     </dif>
   <trdgrp lng="pt">
     <trd>papagaio<klr tip="ind"> (brinquedo)</klr></trd>,
     <trd>pipa<klr tip="ind"> (brinquedo)</klr></trd>,
     <trd>pandorga<klr tip="ind"> (brinquedo)</klr></trd>
   </trdgrp>
   </snc>
   <snc mrk="kajt.0o.TEK">
     <uzo tip="fak">TEK</uzo>
     <dif>
       Aparato, similrimede levi&gcirc;anta, uzata por sciencaj celoj.
     </dif>
   </snc>
   <refgrp tip="sin">
     <ref cel="drak.0o.kajto">drako</ref>,
     <ref cel="drak.flug0o">flugdrako</ref>,
     <ref cel="lud.flug0ilo">flugludilo</ref>
   </refgrp>
   <trdgrp lng="be">
     <trd>&c_p;&c_a;&c_v;&c_je;&c_t;&c_r;&c_a;&c_n;&c_y; 
&c_z;&c_mol;&c_m;&c_je;&c_j;</trd>,
     <trd>&c_l;&c_ja;&c_t;&c_u;&c_ch;&c_y; 
&c_z;&c_mol;&c_m;&c_je;&c_j;</trd>
   </trdgrp>
   <trdgrp lng="ca">
     <trd>estel</trd>,
     <trd>milotxa</trd>,
     <trd>catxerulo</trd>
   </trdgrp>
   <trd lng="de">Drachen <klr tip="ind">(Spielzeug)</klr></trd>
   <trd lng="en">kite</trd>
   <trdgrp lng="es">
     <trd>cometa</trd>,
     <trd>papalote</trd>
   </trdgrp>
   <trd lng="fi">leija</trd>
   <trd lng="fr">cerf-volant<klr tip="ind"> (jouet,
   instrument)</klr></trd>
   <trd lng="he">&ayin;&pe;&yod;&pe;&vav;&fnun;</trd>
   <trd lng="hu">pap&iacute;rs&aacute;rk&aacute;ny</trd>
   <trd lng="ja">&#x51E7;  <pr>&#x3044;&#x304B;&#x306E;&#x307C;&#x308A;</pr></trd>
  <trd lng="nl">vlieger</trd>
   <trd lng="pl">latawiec</trd>
   <trd lng="ro">zmeu</trd>
   <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j; 
&c_z;&c_m;&c_je;&c_j;</trd>
   <trd lng="sv">drake</trd>
   <trd lng="tr">u&ccedil;urtma</trd>
  <trd lng="sk">&scaron;arkan</trd>
  <trd lng="cs">drak <klr>(hra&ccaron;ka| letadlo)</klr></trd>
  <trd lng="uk">&#x43F;&#x430;&#x43F;&#x435;&#x440;&#x43E;&#x432;&#x438;&#x439;/&#x43F;&#x43E;&#x432;&#x456;&#x442;&#x440;&#x44F;&#x43D;&#x438;&#x439; &#x437;&#x43C;&#x456;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x98A8;&#x7B8F; <pr>f&#x113;ngzh&#x113;ng</pr></trd>,
    <trd>&#x7D19;&#x9DC2; <pr>zh&#x1D0;y&#xE0;o</pr></trd>,
    <trd>&#x9CF6; <pr>yu&#x101;n</pr></trd>,
    <trd>&#x7D19;&#x9CF6; <pr>zh&#x1D0;yu&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>
