<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>esting</rad>/i
</kap>
<drv mrk="esting.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;esigi bruladon:
      <ekz>
        <tld/>i fajron, brulegon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>l&ouml;schen</trd>,
      <trd>ausl&ouml;schen</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      &Ccirc;esigi lumadon:
      <ekz>
        kiam la muzikistoj finis, ili <tld/>is la lumon kaj ser&ccirc;is lokon
        por la dormo
        <fnt><bib>ElektFab</bib>, <lok>La bremenaj muzikistoj</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i lampon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxalt.mal0i">mal&scirc;alti</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>l&ouml;schen</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;esigi kaj kvietigi la ardon a&ubreve; eksciton:
      <ekz>
        <tld/>i soifon, deziron.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>beruhigen</trd>,
      <trd>mildern</trd>
    </trdgrp>
    <trd lng="ru">&c_u;&c_t;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;esigi, malaperigi:
      <ekz>
        pardono <tld/>as pekon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i &scirc;uldon;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sufok.0i">sufoki</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>tilgen</trd>,
      <trd>kompensieren</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_t;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">extinguish</trd>
  <trd lng="fr">&eacute;teindre</trd>
  <trdgrp lng="hu">
    <trd>elolt</trd>,
    <trd>kiolt</trd>,
    <trd>megsz&udblac;ntet<klr tip="ind"> (&aacute;tv.)</klr></trd>
  </trdgrp>
  <trd lng="pt">extinguir</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_g;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_a;&c_s;&c_i;&c_t;&c_i;</trd>
</drv>

<drv mrk="esting.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;esi bruli, ardi, lumi; iom post iom malaktivi, malaperi:
      <ekz>
        la lumilo <tld/>i&gcirc;os
        <fnt><bib>MT</bib>, <lok>Sentencoj 13, 9</lok></fnt>;
      </ekz>
      <ekz>
        la vespera &ccirc;ielru&gcirc;o <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        la roza brilo <tld/>i&gcirc;is sur la maro
        <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>;
      </ekz>
      <ekz>
        lia fervoro komencos <tld/>i&gcirc;i;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is ilia bonkoreco kaj iliaj ridetoj;
      </ekz>
      <ekz>
        lia revado <tld/>i&gcirc;is &ccirc;e la brilo de nova <klr>...</klr> pasio
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 7a</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iutage <tld/>i&gcirc;as multaj homaj vivoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        lia memkonfido <tld/>i&gcirc;is<fnt>B</fnt>;
      </ekz>
      <ekz>
        tiu familio <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        <tld/>i&gcirc;anta vo&ccirc;o, rigardo, okulo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vapor.el0igxi">elvapori&gcirc;i</ref>,
      <ref cel="for.0igxi">fori&gcirc;i</ref>,
      <ref cel="visx">forvi&scirc;i&gcirc;i</ref>,
      <ref cel="mort.0i">morti</ref>,
      <ref cel="neni">nenii&gcirc;i</ref>,
      <ref cel="pas.0i">pasi</ref>,
      <ref cel="velk.0i">velki</ref>
    </refgrp>
  </snc>
  <trd lng="fr">s'<ind>&eacute;teindre</ind></trd>
</drv>

<drv mrk="esting.brul0isto">
  <kap>brul<tld/>isto</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      Fajr<tld/>isto.
    </dif>
  </snc>
  <adm>
    Evidente temas pri arhxaikajxo. Oni nuntempe dirus nek unu,
    nek la alian, sed fajrobrigadisto (enkondukenda). <aut>MB</aut>
  </adm>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_r;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Feuerwehrmann</trd>
  <trd lng="en">fireman</trd>
  <trd lng="fr">pompier<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">t&udblac;zolt&oacute;</trd>
  <trd lng="nl">brandweerman</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_r;&c_n;&c_i;&c_k;</trd>
  <trd lng="uk">&c_p;&c_o;&c_zh;&c_je;&c_zh;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="esting.fajr0isto">
  <kap>fajr<tld/>isto</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      Ano de korporacio, speciale komisiita por estingi brulegojn;
      brulestingisto, fajrobrigadano.
    </dif>
  </snc>
  <adm>
    Vd adm sub esting. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_r;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Feuerwehrmann</trd>
  <trd lng="en">fireman</trd>
  <trd lng="fr">pompier<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">t&udblac;zolt&oacute;</trd>
  <trd lng="nl">brandweerman</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_r;&c_n;&c_i;&c_k;</trd>
  <trd lng="uk">&c_p;&c_o;&c_zh;&c_je;&c_zh;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="esting.fajr0ilo">
  <kap>fajr<tld/>ilo</kap>
  <snc>
    <dif>
      Portebla aparato, entenanta fajro<tld/>an substancon, por <tld/>i
      fajrojn.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_g;&c_n;&c_je;&c_t;&c_u;&c_sh;&c_y;&c_c;&c_je;&c_l;&c_mol;</trd>
  <trd lng="de">Feuerl&ouml;scher</trd>
  <trd lng="fr">extincteur</trd>
  <trd lng="hu">t&udblac;zolt&oacute;k&eacute;sz&uuml;l&eacute;k</trd>
  <trd lng="pt">extintor de inc&ecirc;ndio</trd>
  <trd lng="ru">&c_o;&c_g;&c_n;&c_je;&c_t;&c_u;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="uk">&c_v;&c_o;&c_g;&c_n;&c_je;&c_g;&c_a;&c_s;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
