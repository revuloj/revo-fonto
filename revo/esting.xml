<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>esting</rad>/i
</kap>
<drv mrk="esting.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      &Ccirc;esigi bruladon:
      <ekz>
        <tld/>i fajron, brulegon.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      &Ccirc;esigi lumadon:
      <ekz>
        <tld/>i lumon, kandelon, lampon;
      </ekz>
      <ekz>
        la lumilo <tld/>i&gcirc;os
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Sentencoj 13, 9</lok>)</fnt>;
      </ekz>
      <ekz>
        la vespera &ccirc;ielru&gcirc;o <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        la roza brilo <tld/>i&gcirc;is sur la maro
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
        La virineto de maro</vrk>)</fnt>.
      </ekz> 
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;esigi k kvietigi la ardon a&ubreve; eksciton:
      <ekz>
        <tld/>i soifon, deziron;
      </ekz>
      <ekz>
        lia fervoro komencos <ind><tld/>i&gcirc;i</ind>.
        <trd lng="fr">s'<ind>&eacute;teindre</ind></trd>
      </ekz>
    </dif>
    <trd lng="ru">&c_u;&c_t;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc num="4">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;esigi, malaperigi:
      <ekz>
        pardono <tld/>as pekon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i &scirc;uldon;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is ilia bonkoreco k iliaj ridetoj;
      </ekz>
      <ekz>
        lia revado <tld/>i&gcirc;is &ccirc;e la brilo de nova pasio
        <fnt>(<aut>Prus, trad. Kabe</aut>: <vrk>La Faraono, 2a vol.</vrk>,
        <lok>&ccirc;ap. 7</lok>)</fnt>;
      </ekz>
      <ekz>
        &ccirc;iutage <tld/>i&gcirc;as multaj homaj
        vivoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        lia memkonfido <tld/>i&gcirc;is<fnt>B</fnt>;
      </ekz>
      <ekz>
        tiu familio <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        <tld/>i&gcirc;anta vo&ccirc;o, rigardo, okulo
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sufok.0i">sufoki</ref>,
      <ref cel="pas.0i">pasi</ref>,
      <ref cel="velk.0i">velki</ref>,
      <ref cel="mort.0i">morti</ref>,
      <ref cel="for.0igxi">fori&gcirc;i</ref>,
      <ref cel="neni">nenii&gcirc;i</ref>,
      <ref cel="visx">forvi&scirc;i&gcirc;i</ref>,
      <ref cel="vapor.el0igxifor0igxi">elvapori&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="de">l&ouml;schen</trd>
  <trd lng="en">extinguish</trd>
  <trd lng="fr">&eacute;teindre</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_g;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="esting.brul0isto">
  <kap>brul<tld/>isto</kap>
  <snc>
    <dif>
      Fajr<tld/>isto.  
    </dif>
  </snc>
  <adm>
  Evidente temas pri arhxaikajxo. Oni nuntempe dirus nek unu,
  nek la alian, sed fajrobrigadisto (enkondukenda). <aut>MB</aut>
  </adm>
  <trd lng="de">Feuerwehrmann</trd>
  <trd lng="fr">pompier<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_r;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
