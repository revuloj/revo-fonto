<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: esting.xml,v 1.32 2022/05/27 19:24:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>esting</rad>/i
</kap>
<drv mrk="esting.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="cxes.0igi">&Ccirc;esigi</ref> bruladon:
      <ekz>
        <tld/>i brulegon
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2015/011481.php"
              >Estingi fajron per sonoj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> pa&scirc;is en la fajron, dis&jcirc;etis la karbojn kaj volis <tld/>i &gcirc;in
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sklavoj komencis rapide <tld/>i la tor&ccirc;ojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        Pa&ubreve;lo <tld/>is sian cigaredon
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vento <tld/>is la alumeton
        <fnt>
          <bib>LGA</bib>
          <lok>VI.</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>is la skoriojn per akvo, kiun mi prenis el la ligna kuvo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 30</lok>
        </fnt>;
      </ekz>
      <ekz>
        la incendion <tld/>is la fajrobrigado
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="https://www.monato.be/2014/011189.php"
              >Nek malamo nek amaro</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>u la fajron, kiu vin ne bruligas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>l&ouml;schen</trd>,
      <trd>ausl&ouml;schen</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      &Ccirc;esigi lumadon:
      <ekz>
        kiam la muzikistoj finis, ili <tld/>is la lumon kaj ser&ccirc;is 
        lokon por la dormo
        <fnt><bib>ElektFab</bib>, <lok>La bremenaj
        muzikistoj</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> estingis la lucernojn
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 29:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno ankora&ubreve; ne <tld/>is la a&ubreve;rorostelon
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxalt.mal0i">mal&scirc;alti</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>l&ouml;schen</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;esigi kaj <ref tip="super" cel="kviet.0igi">kvietigi</ref> la ardon a&ubreve; eksciton:
      <ekz>
        la soifon li <tld/>as per eksterlandaj vinoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiom da fojoj ekflamis en li kolero kontra&ubreve; &ccirc;i tiuj homoj, tiom da fojoj
        <tld/>is &gcirc;in la sento de l' honto
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        li paci&gcirc;is <klr>[&#8230;]</klr> kaj en la propra koro <tld/>is la malnovan malkonfidon
        al ili
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kokete logas sian edzon, por ke li <tld/>u &scirc;iajn dezirojn
        <fnt>
          <bib>KrB</bib>
          <lok>Por &ccirc;iu plezuro devas esti mezuro</lok>
        </fnt>;
      </ekz>
      <ekz>
        per volstre&ccirc;o &scirc;i provis <tld/>i la tremon de siaj genuoj
        <fnt>
          <bib>MkM</bib>
          <lok>22</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili volas <tld/>i al ili malagrablan debaton
        <fnt>
          <bib>Monato</bib>
          <aut>Aimo Rantanen</aut>
          <vrk><url ref="https://www.monato.be/2004/008379.html"
              >Senkura&gcirc;o malbonas kiel konsilanto</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne volus blovi sur vian entuziasmon por &gcirc;in <tld/>i
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dron.0igi">dronigi</ref>,
      <ref cel="fort.mal0igi">malfortigi</ref>,
      <ref cel="trankv.0igi">trankviligi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>beruhigen</trd>,
      <trd>mildern</trd>
    </trdgrp>
    <trd lng="pl">gasi&cacute;</trd>
    <trd lng="ro">a stinge</trd>
    <trd lng="ru">&c_u;&c_t;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;esigi, <ref tip="super" cel="aper.mal0igi">malaperigi</ref>:
      <ekz>
        la malliberejo kaj la malsano <tld/>is parte &scirc;ian belecon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i <klr>[&#8230;]</klr> prudenton per sensencaj galimatioj
        <fnt>
          <bib>VaK</bib>
          <lok>Dekkvara &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Unua Mondmilito <tld/>is preska&ubreve; &ccirc;iun agadon
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu &scirc;uldojn <tld/>as (kp <ref cel="amorti.0i">amortizi</ref>, 
        <ref cel="pag.re0i.redoni_0on">repagi</ref>), ri&ccirc;econ atingas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        fajron <tld/>as akvo, pekon pardono
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="for.0igi">forigi</ref>,
      <ref cel="neni.0igi">neniigi</ref>,
      <ref cel="sufok.0i">sufoki</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>tilgen</trd>,
      <trd>kompensieren</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_t;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">apagar</trd>
  <trdgrp lng="cs">
    <trd>doho&rcaron;et</trd>,
    <trd>hasit</trd>,
    <trd>uhasit</trd>,
    <trd>zadusit <klr>(cigaretu)</klr></trd>,
    <trd>zhasit</trd>,
    <trd>zhasnout</trd>
  </trdgrp>
  <trd lng="en">extinguish</trd>
  <trd lng="es">extinguir</trd>
  <trd lng="fr">&eacute;teindre</trd>
  <trdgrp lng="hu">
    <trd>elolt</trd>,
    <trd>kiolt</trd>,
    <trd>megsz&udblac;ntet<klr tip="ind"> (&aacute;tv.)</klr></trd>
  </trdgrp>
  <trd lng="oc">atudar</trd>
  <trd lng="pl">gasi&cacute;</trd>
  <trdgrp lng="pt">
    <trd>extinguir</trd>,
    <trd>apagar</trd>
  </trdgrp>
  <trd lng="ro">a stinge</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_g;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>uhasi&tcaron;</trd>,
    <trd>zhasi&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&c_a;&c_s;&c_i;&c_t;&c_i;</trd>
</drv>

<drv mrk="esting.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;esi bruli, ardi, lumi; iom post iom malaktivi, malaperi:
      <ekz>
        la lumilo <tld/>i&gcirc;os
        <fnt><bib>MT</bib>, <lok>Sentencoj 13, 9</lok></fnt>;
      </ekz>
      <ekz>
      &ccirc;u la kandelo estis <tld/>ita, a&ubreve; &gcirc;i <tld/>i&gcirc;is mem?<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la vespera &ccirc;ielru&gcirc;o <tld/>i&gcirc;as;
      </ekz>
      <ekz>
        la roza brilo <tld/>i&gcirc;is sur la maro
        <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>;
      </ekz>
      <ekz>
        lia fervoro komencos <tld/>i&gcirc;i;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is ilia bonkoreco kaj iliaj ridetoj;
      </ekz>
      <ekz>
        lia revado <tld/>i&gcirc;is &ccirc;e la brilo de nova 
<klr>...</klr> pasio
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 7a</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iutage <tld/>i&gcirc;as multaj homaj vivoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        lia memkonfido <tld/>i&gcirc;is<fnt>B</fnt>;
      </ekz>
      <ekz>
        tiu familio <tld/>i&gcirc;is;
      </ekz>
      <ekz>
        <tld/>i&gcirc;anta vo&ccirc;o, rigardo, okulo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vapor.el0igxi">elvapori&gcirc;i</ref>,
      <ref cel="for.0igxi">fori&gcirc;i</ref>,
      <ref cel="visx">forvi&scirc;i&gcirc;i</ref>,
      <ref cel="mort.0i">morti</ref>,
      <ref cel="neni">nenii&gcirc;i</ref>,
      <ref cel="pas.0i">pasi</ref>,
      <ref cel="velk.0i">velki</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_g;&c_a;&c_s;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_g;&c_a;&c_s;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_t;&c_u;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>&eacute;teindre</ind></trd>
  <trd lng="ca"><ind>apagar</ind>-se</trd>
  <trdgrp lng="de">
    <trd>verl&ouml;schen</trd>
  </trdgrp>
  <trd lng="he">&lamed;&kaf;&bet;&vav;&tav;</trd>
  <trd lng="oc">s'<ind>atudar</ind></trd>
  <trd lng="pl">gasn&aogonek;&cacute;</trd>
  <trd lng="es">extinguirse</trd>
  <trd lng="ro">a se stinge</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_s;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_a;&c_s;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_g;&c_a;&c_s;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_u;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_s;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_a;&c_s;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="esting.brul0isto">
  <kap>brul<tld/>isto</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <ref tip="dif" cel="brigad.fajro0isto">fajrobrigadisto</ref>
  </snc>
  <adm>
    Evidente temas pri arhxaikajxo. Oni nuntempe dirus nek unu,
    nek la alian, sed fajrobrigadisto (enkondukenda). <aut>MB</aut>
  </adm>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_r;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Feuerwehrmann</trd>
  <trd lng="en">fireman</trd>
  <trd lng="fr">pompier<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="he">&mem;&tet;&fpe; &kaf;&yod;&bet;&vav;&yod; &alef;&shin;</trd>
  <trd lng="hu">t&udblac;zolt&oacute;</trd>
  <trd lng="nl">brandweerman</trd>
  <trd lng="pl">stra&zdot;ak</trd>
  <trd lng="ro">pompier</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_r;&c_n;&c_i;&c_k;</trd>
  <trd lng="uk">&c_p;&c_o;&c_zh;&c_je;&c_zh;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">hasi&ccaron;</trd>
  <trd lng="cs">hasi&ccaron;</trd>
</drv>

<drv mrk="esting.fajr0isto">
  <kap>fajr<tld/>isto</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
   <ref tip="dif" cel="brigad.fajro0isto">fajrobrigadisto</ref>
  </snc>
  <adm>
    Vd adm sub esting. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_r;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Feuerwehrmann</trd>
  <trd lng="en">fireman</trd>
  <trd lng="fr">pompier<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="he">&kaf;&bet;&alef;&yod;</trd>
  <trd lng="hu">t&udblac;zolt&oacute;</trd>
  <trd lng="nl">brandweerman</trd>
  <trd lng="pl">stra&zdot;ak</trd>
  <trd lng="ro">pompier</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_r;&c_n;&c_i;&c_k;</trd>
  <trd lng="uk">&c_p;&c_o;&c_zh;&c_je;&c_zh;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">hasi&ccaron;</trd>
  <trd lng="cs">hasi&ccaron;</trd>
</drv>

<drv mrk="esting.fajr0ilo">
  <kap>fajr<tld/>ilo</kap>
  <snc>
    <dif>
      Portebla aparato, entenanta fajro<tld/>an substancon, por <tld/>i
      fajrojn.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_g;&c_n;&c_je;&c_t;&c_u;&c_sh;&c_y;&c_c;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>hasi&ccaron;sk&aacute; st&rcaron;&iacute;ka&ccaron;ka</trd>,
    <trd>has&iacute;c&iacute; p&rcaron;&iacute;stroj</trd>
  </trdgrp>
  <trd lng="de">Feuerl&ouml;scher</trd>
  <trd lng="es">extintor</trd>
  <trd lng="fr">extincteur</trd>
  <trd lng="he">&mem;&tet;&fpe; &kaf;&yod;&bet;&vav;&yod; &alef;&shin;</trd>
  <trd lng="hu">t&udblac;zolt&oacute;k&eacute;sz&uuml;l&eacute;k</trd>
  <trd lng="pl">ga&sacute;nica</trd>
  <trd lng="pt">extintor de inc&ecirc;ndio</trd>
  <trd lng="ro">extinctor</trd>
  <trd lng="ru">&c_o;&c_g;&c_n;&c_je;&c_t;&c_u;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">hasiaci pr&iacute;stroj</trd>
  <trd lng="uk">&c_v;&c_o;&c_g;&c_n;&c_je;&c_g;&c_a;&c_s;&c_n;&c_i;&c_k;</trd>
</drv>
