<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kilokalori</rad>/o
</kap>
<drv mrk="kiloka.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Malnova komunuza
      <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de varmo, egala al
      mil <ref tip="prt" cel="kalori.0o">kalorioj</ref>; simb. kcal.
    </dif>
    <ref tip="sin" cel="kalori.granda0o">granda kalorio</ref>
  </snc>
  <trd lng="es">kilocalor&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>kilocalorie</trd>,
    <trd>grande <ind>calorie</ind></trd>
  </trdgrp>
</drv>
</art>

