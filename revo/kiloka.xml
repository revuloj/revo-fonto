<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kiloka.xml,v 1.8 2019/02/09 13:10:23 revo Exp $">
<kap>
  <rad>kilokalori</rad>/o
</kap>

<drv mrk="kiloka.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <mlg>kcal</mlg>
  <snc>
    <dif>
      Malnova komunuza <ref tip="lst" lst="voko:mezurunuoj" cel="unu.mezur0o.SCI">mezurunuo</ref>
      de varmo, egala al mil <ref tip="prt" cel="kalori.0o">kalorioj</ref>:
      <ekz>
        100&#8239;g da blanka brasiko enhavas 25 <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=309699"
              >Blanka brasiko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        dum biciklado kun rapideco 30&#8239;km/h oni bruligas 15&#8239;kkal/km (<tld/>oj por
        kilometro) a&ubreve; 450&#8239;kkal/horo, dum irado kun rapideco 5&#8239;km/horo oni bruligas 60
        kkal/km a&ubreve; 300&#8239;kkal/horo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=420"
              >Biciklo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kalori.granda0o">granda kalorio</ref>
  </snc>
  <trd lng="de">Kilokalorie</trd>
  <trd lng="es">kilocalor&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>kilocalorie</trd>,
    <trd>grande <ind>calorie</ind></trd>
  </trdgrp>
  <trd lng="nl">kilocalorie</trd>
  <trd lng="pl">kilokaloria</trd>     
  <trd lng="ro">kilocalorie</trd>
  <trd lng="ru">&c_k;&c_i;&c_l;&c_o;&c_k;&c_a;&c_l;&c_o;&c_r;&c_i;&c_ja;</trd>
</drv>
</art>

