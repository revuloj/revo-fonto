<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trikot.xml,v 1.20 2024/01/25 18:30:29 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>trikot</rad>/o
</kap>
<drv mrk="trikot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Trikita <ref tip="super" cel="teks.0ajxo.TEKS">teksa&jcirc;o</ref>
      a&ubreve; vesto:
      <ekz>
        oni teksas tiujn fadenojn en teksa&jcirc;ojn, sed ili uzeblas anka&ubreve; por trikado (<tld/>o)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22227">Lano</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en la 12-a / 13-a jarcentoj preska&ubreve; samtemp<klr>[&#8230;]</klr>aj
        teknikaj nova&jcirc;oj: vitraj porfenestraj paneloj, pordoj
        rezistaj je a&ccirc;a vetero, sketiloj, sunokulvitroj, pantalonoj, <tld/>o, butonoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=39070">Kamentubo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
         en la <nom>Tour de France</nom> li gajnis kiel la sola sesfoje la verdan <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=338728">Erik Zabel</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_y;&c_k;&c_o;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_k;&c_a;&c_t;&c_a;&c_zh;</trd>
  </trdgrp>
  <trd lng="cs">trikot <klr>(l&aacute;tka)</klr></trd>
  <trdgrp lng="de">
    <trd>Trikot</trd>,
    <trd>Trikotstoff</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>knit fabric</trd>,
    <trd>knit</trd>, <trd>knitwear</trd>
  </trdgrp>
  <trd lng="fr">tricot</trd>
  <trd lng="hu">trik&oacute; <klr tip="amb">(anyag &eacute;s ruha)</klr></trd>
  <trdgrp lng="it">
    <trd>canottiera</trd>,
    <trd>maglia<klr> (tessuto)</klr></trd>,
    <trd>tricot</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7DE8;&#22320;  <pr>&#12354;&#x307F;&#12376;</pr></trd>,
    <trd>&#x30CB;&#x30C3;&#x30C8;</trd>
  </trdgrp>
  <trd lng="nl">tricot</trd>
  <trd lng="pl">trykot</trd>
  <trd lng="pt">tric&ocirc;</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_i;&c_k;&c_o;</trd>,
    <trd>&c_t;&c_r;&c_i;&c_k;&c_o;&c_t;&c_a;&c_zh;</trd>
  </trdgrp>
  <trd lng="sk">trikot <klr>(l&aacute;tka)</klr></trd>
  <trd lng="uk">&c_t;&c_r;&c_i;&#x43A;&#x43E;</trd>
</drv>
</art>
