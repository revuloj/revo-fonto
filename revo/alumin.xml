<art mrk="$Id: alumin.xml,v 1.21 2023/08/30 18:23:33 revo Exp $">
<kap>
  <rad>alumin</rad>/o
</kap>
<drv mrk="alumin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <frm>Al<sub>2</sub>O<sub>3</sub></frm>; <ref tip="prt" cel="alumin1.0o">aluminia</ref>
      <ref tip="super" cel="oksid.0o">oksido</ref>:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> interalie estas uzata por la produktado de aluminioksida <klr>[c]</klr>eramiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=137724">Alumino</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_l;&c_ib;&c_n;&c_a;&c_z;&c_jo;&c_m;</trd>,
    <trd>&c_a;&c_k;&c_s;&c_ib;&c_d; &c_a;&c_l;&c_ju;&c_m;&c_ib;&c_n;&c_ib;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hl&iacute;novka</trd>,
    <trd>kysli&ccaron;n&iacute;k hlinit&yacute;</trd>,
    <trd>oxid hlinit&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Tonerde</trd>,
    <trd>Aluminiumoxyd</trd>
  </trdgrp>
  <trd lng="en">alumina</trd>
  <trd lng="es">al&uacute;mina</trd>
  <trd lng="fa">&a_A_madda;&a_l;&a_w;&a_m;&f_y;&a_n;</trd>
  <trd lng="fr">alumine</trd>
  <trd lng="he">&#x5D0;&#x5B2;&#x5DC;&#x5D5;&#x5BC;&#x5DE;&#x5B4;&#x5D9;&#x5E0;&#x5B0;&#x5D9;&#x5D5;&#x5BC;&#x5DD;</trd>
  <trdgrp lng="it">
    <trd>allumina</trd>,
    <trd>ossido <klr>ceramico </klr>di <ind>alluminio</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30DF;&#x30CA;</trd>
  <trd lng="nl">toonaarde</trd>
  <trdgrp lng="pl">
    <trd>korund</trd>,
    <trd>tlenek glinu</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_l;&c_i;&c_n;&c_o;&c_z;&c_jo;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>alum&iacute;niumoxid</trd>,
    <trd>kysli&ccaron;n&iacute;k hlinit&yacute;</trd>,
    <trd>oxid hlinit&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&#x433;&#x43B;&#x438;&#x43D;&#x43E;&#x437;&#x435;&#x43C;</trd>
</drv>
</art>
