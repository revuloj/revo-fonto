<kap>
  <rad>lared</rad>/oj
</kap>
<drv mrk="lared.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="lar.0oformaj">laroformaj
      birdoj</ref> (<trd lng="la">Laridae</trd>):
      <ekz>
        la brunkapa <ref tip="prt" cel="mev.0o">mevo</ref>
        el la <klr>[&#x2026;]</klr> familio de <tld/>oj fekundi&gcirc;as en
        okcidenta <nom>&Scirc;in&gcirc;jang</nom>, <nom>&Ccirc;inghaj</nom> kaj Tibeto
        kaj travintras en <nom>Junnan</nom> kaj e&ccirc; pli sudaj lokoj en
        &Ccirc;inio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49898">Brunkapa mevo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="mev.0o">mevo</ref>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_j;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">l&agrave;rids</trd>
  <trd lng="de">M&ouml;wen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">Larid&eacute;s</trd>
  <trd lng="hu">sir&aacute;lyf&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30AB;&#x30E2;&#x30E1;&#x79D1;  <pr>&#x30AB;&#x30E2;&#x30E1;&#x304B;</pr></trd>
  <trd lng="nl">meeuwen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">mewowate</trd>
  <trd lng="pt">lar&iacute;deos</trd>
  <trd lng="ro">pesc&abreve;ru&#x219;ii</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_j;&c_k;&c_i;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_ch;&c_a;&c_j;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>
  </trdgrp>
</drv>
