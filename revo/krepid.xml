<art mrk="$Id: krepid.xml,v 1.18 2023/09/01 15:46:45 revo Exp $">
<kap>
  <rad>krepid</rad>/o
</kap>
<drv mrk="krepid.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Crepis</trd>):
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estas terofita a&ubreve; hemikriptofita, la
        floroj
        estas duseksaj, kaj komunaj polenigantoj inkludas abelojn a&ubreve;
        mu&scirc;ojn, 
        la supraj folioj estas sentigaj kun du pintaj oreletoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=409131">Crepis
          capillaris</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="leonto.0o">leontodo</ref>
    <bld lok="&WCU;/thumb/e/ef/Crepis_pontana_%28Berg-Pippau%29_IMG_7996.jpg/400px-Crepis_pontana_%28Berg-Pippau%29_IMG_7996.jpg" prm="CC0">monta <tld/>o (Crepis pontana)
        <fnt>
          <aut>Hermann Schachner, propra verko</aut>
          <lok><url ref="&WCW;/File:Crepis_pontana_(Berg-Pippau)_IMG_7996.jpg">&WCW;/File:Crepis_pontana_(Berg-Pippau)_IMG_7996.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_z;&c_u;&c_b;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Pippau</trd>
  <trd lng="en">hawksbeard</trd>
  <trdgrp lng="fr">
    <trd>Cr&eacute;pis</trd>,
    <trd>Cr&eacute;pides</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30BF;&#x30DE;&#x30BF;&#x30BF;&#x30F3;&#x30DD;&#x30DD;</trd>,
    <trd>&#x30AF;&#x30EC;&#x30D4;&#x30B9;</trd>
  </trdgrp>
  <trd lng="nl">streepzaad</trd>
  <trd lng="pl">p&eogonek;pawa</trd>
  <trd lng="pt">cr&eacute;pis</trd>
  <trd lng="ro">barba lupului</trd>
  <trd lng="ru">&c_s;&c_k;&c_je;&c_r;&c_d;&c_a;</trd>
  <trd lng="sv">fibbla</trd>
  <trd lng="uk">&#x441;&#x43A;&#x435;&#x440;&#x435;&#x434;&#x430;</trd>
</drv>
</art>
