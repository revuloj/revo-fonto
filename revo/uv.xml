<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>uv</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="uv.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="uv.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="ber.vin0o.BOT">Vinberoj</ref>:
      <ekz>
        sangokoloraj morusoj kaj <tld/>ograpoloj haveblas
        <fnt>
          <aut>W. Auld</aut>,
          <vrk><url
          ref="http://www.sunela.eu/maku/Esperanto/La%20infana%20raso">
          La Infana Raso</url></vrk>,
          <lok>1956</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;aselaso estas man&gcirc;ebla <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=533265">Vitspeco</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Weinbeere</trd>
    <trd lng="fr">raisin</trd>
    <trd lng="pl">winogrono</trd>
  </snc>
</drv>
</art>

