<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: besti.xml,v 1.8 2022/09/05 09:33:32 revo Exp $">
<kap>
  <rad>besti</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="besti.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="besti.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="barbar.0o.FIG">Barbaro<sncref/></ref>,
      <ref tip="dif" cel="best.0o.homo">besto<sncref/></ref>:
      <ekz>
        li furiozis: &ndash; &ccirc;i tiu <tld/>o! &ccirc;i tiu fia kadavrovulturo
        <fnt><bib>SatirRak</bib>, <lok>la barbaro</lok></fnt>!
      </ekz>
      <ekz>
        Bertolt Brecht: La <tld/>o 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=10004">Anna Tarant&iacute;kov&aacute;</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        tiutempe vagadis tra koridoroj de Universitato Johns Hopkins esta&jcirc;o nomita hopkinsa <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=12982">Roboto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_v;&c_je;&c_r;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_r;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_b;&c_e;&c_s;&c_t;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bestie</trd>,
    <trd>divok&eacute; zv&iacute;&rcaron;e</trd>
  </trdgrp>
  <trd lng="de">Bestie</trd>
  <trd lng="fr">brute<klr> (subst.)</klr></trd>
  <trd lng="he">&#x5D7;&#x5D9;&#x5D9;&#x5EA; &#x5E4;&#x5E8;&#x5D0;</trd>
  <trdgrp lng="sk">
    <trd>be&scaron;tia</trd>,
    <trd>div&aacute; zver</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&#x43A;&c_i;&c_j; &c_z;&c_v;&c_ib;&c_r;</trd>,
    <trd>&#x43B;&c_a;&c_j;&#x43B;.&#xA0;&c_h;&c_u;&c_d;&#x43E;&c_b;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_t;&c_ib;&#x44F;&#xA0;(&#x43F;&c_r;&#x43E; &#x43B;&#x44E;&c_d;&c_i;&#x43D;&c_u;)</trd>
  </trdgrp>
</drv>

<drv mrk="besti.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="besti.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="hom.mal0a.KOMUNE">Malhoma</ref>,
      <ref tip="sin" cel="brutal.0a">brutala</ref>:
      <ekz>
        <tld/>a kaj feroca Mongus okupis lian hejmolokon kaj 
        mortigis liajn gepatrojn
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;chinaabc/chapter15/chapter150402.htm">
          Jangar</url></vrk>,
          <lok>&Ccirc;ina ABC, [vidita en 2014]</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu malamo montri&gcirc;is en <tld/>a formo
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="http://www.esperanto.be/fel/mon/007170.html">Ni hontas kaj
          postulas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_mol;&c_v;&c_je;&c_r;&c_s;&c_k;&c_ib;</trd>,
      <trd>&c_z;&c_mol;&c_v;&c_ja;&c_r;&c_y;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_r;&c_u;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>bestialisch</trd>,
      <trd>brutal</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="uk">
    <trd>&c_z;&c_v;&c_ib;&c_r;&#x44F;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43A;&#x43E;&c_t;&c_i;&#x43D;&#x44F;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

