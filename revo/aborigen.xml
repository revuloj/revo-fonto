<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aborigen.xml,v 1.25 2025/03/24 19:26:35 revo Exp $">
<kap>
  <rad>aborigen</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="aborigen.0o">
  <kap><tld/>o</kap>
  <snc mrk="aborigen.0o.auxstralia">
    <dif>
      Ano de iu el la pralo&gcirc;aj popoloj de A&ubreve;stralio:
      <ekz>
        a&ubreve;stralianoj komisias elstaran <tld/>inon porti la 
        olimpian tor&ccirc;on
        <fnt><aut>Werner Schad</aut>: <vrk>Tor&ccirc;o de
        hipokriteco</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 3a,
        p. 4a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_y;&c_g;&c_je;&c_n;
<klr>
(&c_a;&c_w;&c_s;&c_t;&c_r;&c_a;&c_l;&c_ib;&c_j;&c_s;&c_k;&c_ib;)
</klr></trd>
    <trd lng="bg">&c_a;&c_b;&c_o;&c_r;&c_i;&c_g;&c_je;&c_n;</trd>
    <trd lng="ca">aborigen australi&agrave;</trd>
    <trdgrp lng="de">
      <trd>Ureinwohner Australiens</trd>,
      <trd>Aborigine</trd>
    </trdgrp>
    <trd lng="en">aborigene</trd>
    <trd lng="es">aborigen australiano</trd>
    <trdgrp lng="fr">
      <trd>aborig&egrave;ne<klr tip="ind"> (subst.,
d'Australie)</klr></trd>
    </trdgrp>
    <trd lng="hu"><klr>(ausztr&aacute;l)</klr> &odblac;slakos</trd>
    <trd lng="it">Aborigeno<klr> (australiano)</klr></trd>
    <trd lng="pl">aborygen</trd>
    <trdgrp lng="zh">
      <trd>&#28595;&#22823;&#21033;&#20122;&#21407;&#20303;&#27665; <pr>&#224;od&#224;l&#236;y&#224;yu&#225;nzh&#249;m&#237;n</pr></trd>,
      <trd>&#28595;&#22823;&#21033;&#20126;&#21407;&#20303;&#27665; <pr>&#224;od&#224;l&#236;y&#224;yu&#225;nzh&#249;m&#237;n</pr></trd>,
      <trd>&#28595;&#27954;&#21407;&#20303;&#27665; <pr>&#224;ozh&#333;uyu&#225;nzh&#249;m&#237;n</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="aborigen.0o.indigxeno">
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="indigx.0o">indi&gcirc;eno</ref>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_a;&c_r;&c_y;&c_g;&c_je;&c_n;</trd>,
      <trd>&c_t;&c_u;&c_b;&c_y;&c_l;&c_je;&c_c;</trd>
    </trdgrp>
    <trd lng="de">Ureinwohner</trd>
    <trd lng="en">native</trd>
    <trdgrp lng="es">
      <trd>aborigen<klr tip="ind"> (subst., aut&oacute;ctono)</klr></trd>,
      <trd>aut&oacute;ctono<klr tip="ind"> (subst.)</klr></trd>,
      <trd>ind&iacute;gena<klr tip="ind"> (subst.)</klr></trd>,
      <trd>nativo<klr tip="ind"> (subst.)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>aborig&egrave;ne<klr tip="ind"> (subst., autochtone)
      </klr></trd>
      <trd>autochtone<klr tip="ind"> (subst.)</klr></trd>,
      <trd>indig&egrave;ne<klr tip="ind"> (subst.)</klr></trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>aborigeno<klr tip="ind"> (sost.=autoctono)</klr></trd>
      <trd>autoctono<klr tip="ind"> (sost.)</klr></trd>,
      <trd>indigeno<klr tip="ind"> (sost.)</klr></trd>,
      <trd>nativo<klr tip="ind"> (sost.=autoctono)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>aborygen</trd>,
      <trd>tubylec</trd>,
      <trd>autochton</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>abor&iacute;gene<klr>(origin&aacute;rio da pr&oacute;pria regi&atilde;o)</klr></trd>,
      <trd>nativo</trd>,
      <trd>primitivo</trd>,
      <trd>ind&iacute;gena</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_u;&c_z;&c_je;&c_m;&c_je;&c_c;</trd>
    <trdgrp lng="zh">
      <trd>&#22303;&#30528;&#23621;&#27665; <pr>t&#468;zh&#249;j&#363;m&#237;n</pr></trd>,
      <trd>&#22303;&#33879;&#23621;&#27665; <pr>t&#468;zh&#249;j&#363;m&#237;n</pr></trd>,
      <trd>&#23665;&#22320;&#20154; <pr>sh&#257;nd&#236;r&#233;n</pr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>pralo&#285;anto</ke>.
  </rim>

  <adm>
    Mi dubas, cxu vere la dua senco estas arhxaika. Sxajnas al
    mi, ke la unua estas nura specialigo de la dua (por Auxstralio).
    <aut>MB</aut>
  </adm>
  <trdgrp lng="ca">
    <trd>aborigen</trd>,
    <trd>ind&iacute;gena</trd>
  </trdgrp>
  <trd lng="he">&qof;&#1463;&dalet;&mem;&vav;&#1465;&fnun;</trd>
  <trd lng="nl">inboorling</trd>
  <trdgrp lng="hu">
    <trd>bennsz&uuml;l&ouml;tt</trd>,
    <trd>&odblac;slakos</trd>
  </trdgrp>
  <trd lng="ru">
  &c_a;&c_b;&c_o;&c_r;&c_i;&c_g;&c_je;&c_n;</trd>
  <trd lng="sk">&ccaron;len m&yacute;tick&eacute;ho talianskeho pran&aacute;roda</trd>
  <trdgrp lng="cs">
    <trd>domorodec</trd>,
    <trd>p&uring;vodn&iacute; obyvatel</trd>,
    <trd>rostlina</trd>,
    <trd>zv&iacute;&rcaron;e</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_b;&c_o;&c_r;&c_i;&c_gu;&c_je;&c_n;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_ib;&c_n;&c_n;&c_i;&c_j; &c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_t;&c_u;&c_b;&c_ib;&c_l;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="aborigen.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj:
      <ekz>
        &gcirc;i same deklaris sin por repaci&gcirc;o kun homoj
        <tld/>aj
        <fnt><aut>Ralph Harry</aut>: <vrk>Nacio kontra&ubreve;
        rasismo</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 1a, p.
        8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_a;&c_b;&c_a;&c_r;&c_y;&c_g;&c_je;&c_n;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>aborigen <klr>(adj.)</klr></trd>,
    <trd>aut&ograve;cton</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>domorod&yacute;</trd>,
    <trd>p&uring;vodn&iacute; <klr>(obyvatel)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Aboriginie-...</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aborigen<klr tip="ind"> (adj.)</klr></trd>,
    <trd>aut&oacute;ctono<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aborig&egrave;ne<klr tip="ind"> (adj.)</klr></trd>
    <trd>autochtone<klr tip="ind"> (adj.)</klr></trd>,
    <trd>indig&egrave;ne<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="he">&qof;&#1463;&dalet;&mem;&vav;&#1465;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>&odblac;slakos</trd>,
    <trd>bennsz&uuml;l&ouml;tt</trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>aborigeno<klr tip="ind"> (agg.)</klr></trd>
      <trd>autoctono<klr tip="ind"> (agg.)</klr></trd>,
      <trd>indigeno<klr tip="ind"> (agg.)</klr></trd>,
      <trd>nativo<klr tip="ind"> (agg. =autoctono)</klr></trd>
    </trdgrp>
  <trdgrp lng="pl">
    <trd>aboryge&nacute;ski</trd>,
    <trd>tubylczy</trd>,
    <trd>autochtoniczny</trd>
  </trdgrp>
  <trdgrp lng="ru">
  <trd>
  &c_a;&c_b;&c_o;&c_r;&c_i;&c_g;&c_je;&c_n;&c_n;&c_y;&c_j;
  </trd>,
  <trd>
  &c_a;&c_b;&c_o;&c_r;&c_i;&c_g;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;
  </trd>,
  <trd>
  &c_t;&c_u;&c_z;&c_je;&c_m;&c_n;&c_y;&c_j;
  </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>domorod&yacute;</trd>,
    <trd>p&ocirc;vodn&yacute; <klr>(obyvate&lcaron;)</klr></trd>,
    <trd>p&ocirc;vodn&yacute; <klr>(obyvate&lcaron;)</klr></trd>
  </trdgrp>
</drv>
</art>
