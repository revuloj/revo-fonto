<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>modest</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="modest.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="modest.0a.KOMUNE">
    <dif>
      Mal<ref tip="ant" cel="impon.0a">impona</ref>.
    </dif>
    <subsnc mrk="modest.0a.konduto">
      <uzo tip="klr">(iu)</uzo>
      <dif>
        Ka&scirc;anta a&ubreve; ne alte taksanta siajn meritojn; montranta nenian
        pretendemon pri siaj meritoj:
        <ekz>
          <tld/>a homo;
        </ekz><ekz>
          <tld/>a tono.
        </ekz>
      </dif>
      <trd lng="en">unassuming</trd>
      <trd lng="hu">szer&eacute;ny</trd>        
      <ref tip="vid" cel="humil.0a">humila</ref>
    </subsnc>
    <subsnc mrk="modest.0a.valoro">
      <uzo tip="klr">(io)</uzo>
      <dif>
        Prezentanta nealtan meriton,
        <ref tip="vid" cel="luks">senluksa</ref>,
        <ref tip="vid" cel="pomp">senpompa</ref>,
        <ref tip="vid" cel="simpl.0a.senornama">simpla</ref>,
        <ref tip="vid" cel="bril.0a">senbrila</ref>:
        <ekz>
          <tld/>a domo<fnt>Z</fnt>,
        </ekz>
        <ekz>
          pre&gcirc;ejo, vivo;
        </ekz>
        <ekz>
          <tld/>a situacio, rango;
        </ekz>
        <ekz>
          <tld/>aj elspezoj;
        </ekz>
        <ekz>
          <tld/>a prezo, salajro, postulo, plano;
        </ekz>
        <ekz>
          rangeto rangon respektu kaj <tld/>an lokon
          elektu<fnt>Z</fnt>;
        </ekz>
        <ekz>
          <tld/>a estas nia kunveno<fnt>Z</fnt>.
        </ekz>
      </dif>
      <trd lng="en">moderate</trd>
      <trdgrp lng="hu">
        <trd>ig&eacute;nytelen</trd>,
        <trd>egyszer&udblac;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_ib;&c_p;&c_l;&c_y;</trd>
  <trd lng="de">bescheiden</trd>
  <trd lng="en">modest</trd>
  <trd lng="fr">modeste</trd>
  <trd lng="nl">bescheiden</trd>
  <trd lng="pt">modesto</trd>
  <trd lng="es">modesto, -a</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_r;&c_o;&c_m;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_t;&c_ja;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="modest.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="modest.mal0a.PSI">
    <dif>
      Fiere montranta siajn kvalitojn,
      <ref tip="vid" cel="glor.0ama.PSI">glorama</ref>,
      <ref tip="vid" cel="vant.0a.iu">vanta</ref>:
      <ekz>
        &scirc;i estis bela virino, sed fiera kaj mal<tld/>a
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>immodeste</trd>,
    <trd>orgueilleux</trd>
  </trdgrp>
</drv>
</art>
