    <trd>&#x43D;&c_je;&c_v;&c_i;&c_b;&c_a;&c_g;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8B19;&#x905C; <pr>qi&amacron;nx&#xF9;n</pr></trd>,
    <trd>&#x8C26;&#x900A; <pr>qi&amacron;nx&#xF9;n</pr></trd>,
