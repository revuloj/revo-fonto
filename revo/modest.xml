<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>modest</rad>/a
    </kap>

    <drv mrk="modest.0a">
      <kap><ofc>*</ofc><tld/>a</kap>
      <snc mrk="modest.0a.konduto">
        <dif>
          Ka&scirc;anta a&ubreve; ne alte taksanta siajn meritojn; montranta nenian
          pretendemon pri siaj meritoj:
          <ekz>
            <tld/>a homo;
          </ekz><ekz>
            <tld/>a tono.
          </ekz>
        </dif>
        <trd lng="hu">szer&eacute;ny</trd>        
        <ref tip="vid" cel="humil.0a">humila</ref>
      </snc>
      <snc mrk="modest.0a.valoro">
        <dif>
          Prezentanta nealtan, nesuperan meriton a&ubreve; gradon kaj neebliganta
          pretendemon:
          <ekz>
            <tld/>a domo<fnt>Z</fnt>,
          </ekz><ekz>
            pre&gcirc;ejo, vivo
            (<refgrp tip="sin">
              <ref cel="luks">senluksa</ref>,
              <ref cel="pomp">senpompa</ref>,
              <ref cel="simpl.0a.senornama">simpla</ref>
            </refgrp>);
          </ekz><ekz>
            <tld/>a situacio, rango
            (<refgrp tip="sin">
              <ref cel="alt.0a">malalta</ref>,
              <ref cel="bril.0a">senbrila</ref>
            </refgrp>);
          </ekz><ekz>
            <tld/>aj elspezoj;
          </ekz><ekz>
            <tld/>a prezo, salajro, postulo, plano
            (<ref tip="sin" cel="moder.0a">modera</ref>);
          </ekz><ekz>
            rangeto rangon respektu kaj <tld/>an lokon elektu<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>a estas nia kunveno<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="hu">
          <trd>ig&eacute;nytelen</trd>,
          <trd>egyszer&udblac;</trd>
        </trdgrp>
      </snc>
      <trd lng="be">&c_s;&c_mol;&c_c;&c_ib;&c_p;&c_l;&c_y;</trd>
      <trd lng="de">bescheiden</trd>
      <trd lng="nl">bescheiden</trd>
      <trd lng="es">modesto, -a</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_k;&c_r;&c_o;&c_m;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_t;&c_ja;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
    </drv>
  </art>
