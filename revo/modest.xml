<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: modest.xml,v 1.22 2020/11/05 23:11:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>modest</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="modest.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="modest.0a.KOMUNE">
    <dif>
      Mal<ref tip="ant" cel="impon.0a">impona</ref>.
    </dif>
    <subsnc mrk="modest.0a.konduto">
      <uzo tip="klr">(iu)</uzo>
      <dif>
        Ka&scirc;anta a&ubreve; ne alte taksanta siajn meritojn; montranta nenian
        pretendemon pri siaj meritoj:
        <ekz>
          <tld/>a homo;
        </ekz><ekz>
          <tld/>a tono.
        </ekz>
      </dif>
      <ref tip="vid" cel="humil.0a">humila</ref>
      <trd lng="en">unassuming</trd>
      <trd lng="hu">szer&eacute;ny</trd>        
    </subsnc>
    <subsnc mrk="modest.0a.valoro">
      <uzo tip="klr">(io)</uzo>
      <dif>
        Prezentanta nealtan meriton,
        <ref tip="vid" cel="luks">senluksa</ref>,
        <ref tip="vid" cel="pomp">senpompa</ref>,
        <ref tip="vid" cel="simpl.0a.senornama">simpla</ref>,
        <ref tip="vid" cel="bril.0a">senbrila</ref>:
        <ekz>
          <tld/>a kunveno, domo
          <fnt>
            <bib>Paroloj</bib>
            <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista 
              en Boulogne sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>a pre&gcirc;ejo, vivo;
        </ekz>
        <ekz>
          ili kontenti&gcirc;is je sia <tld/>a sorto
          <fnt>
            <bib>Fab3</bib>
            <lok>Ib kaj malgranda kristino</lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>a situacio, rango;
        </ekz>
        <ekz>
          <tld/>aj elspezoj;
        </ekz>
        <ekz>
          <tld/>a prezo, salajro, postulo, plano;
        </ekz>
        <ekz>
          rangeto rangon respektu kaj <tld/>an lokon
          elektu
          <fnt>
            <bib>PrV</bib>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="en">moderate</trd>
      <trdgrp lng="hu">
        <trd>ig&eacute;nytelen</trd>,
        <trd>egyszer&udblac;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_ib;&c_p;&c_l;&c_y;</trd>
  <trd lng="bg">&c_s;&c_k;&c_r;&c_o;&c_m;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>nepatrn&yacute;</trd>,
    <trd>skromn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bescheiden</trd>,
    <trd>zur&uuml;ckhaltend</trd>,
    <trd>unauff&auml;llig</trd>,
    <trd>schlicht</trd>
  </trdgrp>
  <trd lng="en">modest</trd>
  <trd lng="es">modesto, -a</trd>
  <trd lng="fr">modeste</trd>
  <trd lng="nl">bescheiden</trd>
  <trd lng="pl">skromny</trd>
  <trd lng="pt">modesto</trd>
  <trd lng="ro">modest</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_r;&c_o;&c_m;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_t;&c_ja;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">skromn&yacute;</trd>
</drv>

<drv mrk="modest.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="modest.mal0a.PSI">
    <dif>
      Fiere montranta siajn kvalitojn,
      <ref tip="vid" cel="glor.0a">glorama</ref>,
      <ref tip="vid" cel="vant.0a.iu">vanta</ref>:
      <ekz>
        &scirc;i estis bela virino, sed fiera kaj mal<tld/>a
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>;
      </ekz>
      <ekz>
        estis iom mal<tld/>esta de lia flanko, ke li permesis al si diri al la filino de la
        imperiestro: &ccirc;u vi min deziras
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>?
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>unbescheiden</trd>,
      <trd>angeberisch</trd>,
      <trd>aufschneiderisch</trd>,
      <trd>protzig</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_n;&c_je;&c_s;&c_k;&c_r;&c_o;&c_m;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>chamtiv&yacute;</trd>,
    <trd>neskromn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>immodeste</trd>,
    <trd>orgueilleux</trd>
  </trdgrp>
  <trd lng="nl">onbescheiden</trd>
  <trdgrp lng="pl">
    <trd>arogancki</trd>,
    <trd>butny</trd>,
    <trd>w&sacute;cibski</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>arogant</trd>,
    <trd>b&abreve;g&abreve;cios </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>neskromn&yacute;</trd>,
    <trd>n&aacute;ro&ccaron;n&yacute;</trd>
  </trdgrp>
</drv>
</art>
