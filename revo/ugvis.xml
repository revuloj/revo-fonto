<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ugvis</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="ugvis.0o">
  <kap><tld/>o</kap>
  <snc mrk="ugvis.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Orientazia malgranda birdo brune-verda, bele kantanta en printempo,
      specio el familio <ref tip="super" cel="silvie.0oj.ZOO">silviedoj</ref>      
      (<trd lng="la">Cettia diphone</trd>):
      <ekz>
        <tld/>o kantas / pli lerte &ndash; baldaux venos / via ekiro
        <fnt>
        <aut>Vastalto</aut>,
        <vrk><url ref="http://vastalto.com/hajko/hajko.cgi">Hajko en Esperanto</url></vrk>,
        <lok>2006-03-31</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bouscarle chanteuse</trd>,
    <trd>uguisu</trd>
  </trdgrp>
  <trd lng="en">manchurian bush-warbler</trd>
  <trd lng="pl">wierzb&oacute;wka chi&nacute;ska</trd>
  <trd lng="pt">rouxinol-das-cani&ccedil;as <klr>(PT)</klr></trd>
</drv>
</art>

