<kap>
   <rad>ugvis</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="ugvis.0o">
   <kap><tld/>o</kap>
   <snc mrk="ugvis.0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       Orientazia malgranda <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref>
       brune-verda, bele kantanta en printempo,
       <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">specio</ref>
       el familio <ref tip="super" cel="silvie.0oj.ZOO">silviedoj</ref>
       (<trd lng="la">Horornis diphone</trd>, sin. <trd lng="la">Cettia diphone</trd>):
      <ekz>
        ni &gcirc;uis anka&ubreve; belajn kantojn de malgranda birdo <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2007/009114.php">Vizito al Takaha&scirc;i</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
       <ekz>
         <tld/>o kantas / pli lerte &ndash; balda&ubreve; venos / via ekiro
         <fnt>
         <aut>Vastalto</aut>,
         <vrk><url ref="http://vastalto.com/hajko/hajko.cgi">Hajko en Esperanto</url></vrk>,
         <lok>2006-03-31</lok>
         </fnt>.
       </ekz>
     </dif>
    <bld
    lok="&WCU;/thumb/4/41/Cettia_diphone_cantans_on_branch.JPG/480px-Cettia_diphone_cantans_on_branch.JPG"
    prm="CC BY-SA 3.0"><tld/>o, Japanujo
        <fnt>
          <aut>Alpsdake, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Cettia_diphone_cantans_on_branch.JPG">&WCW;/File:Cettia_diphone_cantans_on_branch.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Japanbuschs&auml;nger</trd>
  <trd lng="en">manchurian bush-warbler</trd>
   <trd lng="es">cetia de Jap&oacute;n<klr tip="amb"> (peque&ntilde;o pajarillo
   cantor)</klr></trd>
  <trd lng="fi">id&auml;nsilkkikerttunen</trd>
  <trdgrp lng="fr">
     <trd>bouscarle chanteuse</trd>,
     <trd>uguisu</trd>
  </trdgrp>
  <trd lng="hu">jap&aacute;n berki <ind>posz&aacute;ta</ind></trd>
  <trdgrp lng="it">
     <trd>usignolo<klr> giapponese</klr></trd>,
     <trd>uguisu</trd>
   </trdgrp>
   <trd lng="ja">&#x9D2C; <pr>&#12358;&#12368;&#12356;&#12377;</pr></trd>
  <trd lng="pl">wierzb&oacute;wka chi&nacute;ska</trd>
   <trd lng="pt">rouxinol-das-cani&ccedil;as <klr>(PT)</klr></trd>
  <trd lng="uk">&c_b;&c_ib;&#x43B;&#x43E;&c_ch;&c_je;&c_r;&c_je;&c_v;&c_a; &#x43E;&c_ch;&c_je;&c_r;&c_je;&c_t;&#x44F;&#x43D;&#x43A;&c_a;
  &#x43A;&#x43E;&c_r;&#x43E;&c_t;&#x43A;&#x43E;&#x43A;&c_r;&c_i;&#x43B;&c_a;</trd>
</drv>
