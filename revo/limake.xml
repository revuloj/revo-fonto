<vortaro>
<art mrk="$Id: limake.xml,v 1.12 2024/01/03 08:30:39 revo Exp $">
<kap><rad>limaked</rad>/oj</kap>
<drv mrk="limake.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="gastro2.0oj">gastropodoj</ref>
      (<trd lng="la">Limacidae</trd>):
      <ekz>
        <ref tip="prt" cel="limak.0o">limako</ref> estas <klr>[&#8230;]</klr>
        genro el la familio <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=9316">Limako</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_m;&c_a;&c_c;&c_y;&c_d;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;&#8217;&c_ja; &c_s;&c_mol;&c_l;&c_ib;&c_m;&c_a;&c_k;&c_o;&c_w;)</klr></trd>
  <trd lng="en">slugs</trd>
  <trd lng="de">Schnegel<klr> [Familie]</klr></trd>
  <trd lng="fr">Limacid&eacute;s</trd>
  <trd lng="hu">meztelencsig&aacute;k<klr tip="ind"> (csal&aacute;d)</klr></trd>
  <trd lng="nl">Limacidae</trd>
  <trd lng="pl">pomrowiowate</trd>
  <trd lng="pt">limac&iacute;deos</trd>
  <trd lng="ro">limacidae</trd>
</drv>
</art>

