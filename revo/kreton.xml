<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kreton</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kreton.0o">
  <kap><tld/>o</kap>
  <snc mrk="kreton.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Fortika tolo, kies <ref tip="vid" cel="varp.0o">varpo</ref>
      estas el kanabo a&ubreve; el kotono, kaj
      <ref tip="vid" cel="veft.0o">vefto</ref>
      el lino a&ubreve; el lano, uzata interalie por kurtenoj:
      <ekz>
        la <tld/>a kurteno estis disfaldita kaj elbaris la taglumon
        <fnt><bib>KielAkvo</bib>, <lok>2:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_t;&c_o;&c_n;</trd>
  <trd lng="de">Cretonne</trd>
  <trd lng="en">cretonne</trd>
  <trd lng="fr">cretonne</trd>
  <trd lng="pl">kreton</trd>
  <trd lng="hu">kreton</trd>
  <trdgrp lng="pt">
    <trd>cretone</trd>,
    <trd>bramante</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_t;&c_o;&c_n;</trd>
</drv>
</art>
