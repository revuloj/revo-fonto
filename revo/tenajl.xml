<kap>
  <rad>tenajl</rad>/o
</kap>
<drv mrk="tenajl.0o">
  <kap><tld/>o</kap>
  <snc mrk="tenajl.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Speco de fera tenilo konsistanta el du krucigitaj pecoj, moveblaj
      kaj ligitaj per &scirc;ra&ubreve;bo a&ubreve; bolto kaj uzata por
      ion teni a&ubreve; eltiri:
      <ekz>
        Kasem kuspis la manikojn, malfermis <tld/>on
        <fnt>
          <aut>Mahbube Mirkadiri</aut>,
          <vrk>Donaco el voja&gcirc;o</vrk>,
          <bib>LOdE</bib>,
          <lok>2003:3 (101)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_c;&c_u;&c_g;&c_ib;</trd>, 
    <trd>&c_k;&c_l;&c_je;&c_sh;&c_ch;&c_y;</trd>, 
    <trd>&c_sh;&c_ch;&c_y;&c_p;&c_c;&c_y;</trd>, 
    <trd>&c_k;&c_u;&c_s;&c_a;&c_ch;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="ca">estenalles</trd>
  <trd lng="cs">kle&scaron;t&ecaron;</trd>
  <trd lng="de">Zange</trd>
  <trdgrp lng="en">
    <trd>pincers</trd>,
    <trd>pliers</trd>,
    <trd>tongs</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>alicates</trd>,
    <trd>tenaza</trd>
  </trdgrp>
  <trd lng="fr">tenaille</trd>
  <trd lng="hu">fog&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x30DA;&#x30F3;&#x30C1;</trd>,
    <trd>&#x3084;&#x3063;&#x3068;&#x3053;</trd>,
    <trd>&#x91D8;&#x629C;&#x304D;  <pr>&#x304F;&#x304E;&#x306C;&#x304D;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tang</trd>,
    <trd>nijptang</trd>,
    <trd>trektang</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>c&eogonek;gi</trd>,
    <trd>obc&eogonek;gi</trd>,
    <trd>kleszcze</trd>,
    <trd>szczypce</trd>,
    <trd>szczypy</trd>,
    <trd>kombinerki</trd>
  </trdgrp>
  <trd lng="pt">tenaz</trd>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_i;&c_p;&c_c;&c_y;</trd>,
    <trd>&c_k;&c_l;&c_je;&c_shch;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">ve&lcaron;k&eacute; klie&scaron;te</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43B;&#x456;&#x449;&#x456;</trd>,
    <trd>&#x449;&#x438;&#x43F;&#x446;&#x456;</trd>,
    <trd>&#x433;&#x43E;&#x441;&#x442;&#x440;&#x43E;&#x437;&#x443;&#x431;&#x446;&#x456;</trd>,
    <trd>&#x43A;&#x443;&#x441;&#x430;&#x447;&#x43A;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9257; <pr>qi&#xE1;n</pr></trd>,
    <trd>&#x94B3; <pr>qi&#xE1;n</pr></trd>,
    <trd>&#x9257;&#x5B50; <pr>qi&#xE1;nzi</pr></trd>,
    <trd>&#x94B3;&#x5B50; <pr>qi&#xE1;nzi</pr></trd>,
    <trd>&#x62D1; <pr>qi&#xE1;n</pr></trd>,
    <trd>&#x7B9D; <pr>qi&#xE1;n</pr></trd>,
    <trd>&#x5939;&#x94B3; <pr>ji&#x101;qi&#xE1;n</pr></trd>,
    <trd>&#x593E;&#x9257; <pr>ji&#x101;qi&#xE1;n</pr></trd>,
