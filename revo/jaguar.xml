<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jaguar.xml,v 1.23 2019/02/13 17:10:44 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>jaguar</rad>/o
</kap>
<drv mrk="jaguar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Sudamerika <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">specio</ref> 
      de <ref tip="lst" cel="panter.0o.genro" lst="voko:rabobestoj">pantero</ref>
      (<trd lng="la">Panthera onca</trd>), kun brune flava felo kaj nigraj makuloj:
      <ekz>
        &ccirc;efa elemento en la olmeka kulturo estas la <tld/>o, kiu povas esti konsiderata
        ties totemo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16187"
              >Olmekoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/f/f8/Jaguar_(Panthera_onca_palustris)_male_Rio_Negro_2.JPG/640px-Jaguar_(Panthera_onca_palustris)_male_Rio_Negro_2.JPG"
      prm="CC BY-SA 4.0"
      ><tld/>o (Panthera onca palustris), Pantanal, Brazilo
        <fnt>
          <aut>Charles J Sharp, propra verko</aut>
          <lok><url ref="&WCW;/File:Jaguar_(Panthera_onca_palustris)_male_Rio_Negro_2.JPG"
              >https://commons.wikimedia.org/wiki/File:Jaguar_(Panthera_onca_palustris)_male_Rio_Negro_2.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_ja;&c_g;&c_u;&c_a;&c_r;</trd>
  <trd lng="ca">jaguar</trd>
  <trd lng="de">Jaguar</trd>
  <trd lng="en">jaguar</trd>
  <trd lng="es">jaguar</trd>
  <trdgrp lng="fr">
    <trd>jaguar</trd>
  </trdgrp>
  <trd lng="hu">jagu&aacute;r</trd>
  <trd lng="id">jaguar</trd>
  <trd lng="it">giaguaro</trd>
  <trd lng="nl">jaguar</trd>
  <trd lng="pl">jaguar</trd>
  <trdgrp lng="pt">
   <trd>on&ccedil;a-pintada</trd>,
   <trd>on&ccedil;a</trd>,
   <trd>jaguar</trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_g;&c_u;&c_a;&c_r;</trd>
  <trd lng="sk">jagu&aacute;r</trd>
  <trd lng="sv">jaguar</trd>
</drv>
</art>
