<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>afrikat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="afrika.0o">
  <kap><tld/>o</kap>
  <snc mrk="afrika.0o.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      Kombino de
      <ref tip="vid" cel="plosiv.0o.FON">plosivo</ref>
      kaj
      <ref tip="vid" cel="frikat.0o.FON">frikativo</ref>,
      &ccirc;e kiu plosiva alteno
      estas sekvata de frikativa forteno (c, &ccirc;, &gcirc;).
    </dif>
  <refgrp tip="vid">
  <ref cel="nazal.0o.FON">nazalo</ref>,
  <ref cel="likvid1.0o.FON">likvido</ref>,
  <ref cel="frikat.0o.FON">frikativo</ref>,
  <ref cel="plosiv.0o.FON">plosivo</ref>
  </refgrp>  
  </snc>
  <trd lng="be">&c_a;&c_f;&c_r;&c_y;&c_k;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Affrikat</trd>,
    <trd>Affrikata</trd>,
    <trd>Affrikate</trd>
  </trdgrp>
  <trd lng="ca">africat</trd>
  <trd lng="en">affricative</trd>
  <trd lng="es">africada</trd>
  <trd lng="fa">&a_h;&a_m;&a_kh;&a_w;&a_A;&a_n; &a_m;&a_r;&f_k;&a_b;</trd>
  <trd lng="fr">affriqu&eacute;e<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>z&aacute;r-r&eacute;shang</trd>,
    <trd>affrik&aacute;ta</trd>
  </trdgrp>
  <trd lng="it">affricata</trd>
  <trd lng="pl">afrykata</trd>
  <trdgrp lng="pt">
    <trd>africado</trd>,
    <trd>explosivo-fricativo</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_f;&c_f;&c_r;&c_i;&c_k;&c_a;&c_t;&c_a;</trd>
</drv>
</art>
