<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>afrikat</rad>/o
</kap>
<drv mrk="afrika.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      Kombino de plosivo kaj frikativo, &ccirc;e kiu plosiva alteno
estas
      sekvata de frikativa forteno (c, &ccirc;, &gcirc;).
    </dif>
    <refgrp tip="vid">
      <ref cel="plosiv.0o">plosivo</ref>,
      <ref cel="frikat.0o">frikativo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_f;&c_r;&c_y;&c_k;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Affrikat</trd>,
    <trd>Affrikata</trd>,
    <trd>Affrikate</trd>
  </trdgrp>
  <trd lng="ca">africat</trd>
  <trd lng="en">affricative</trd>
  <trd lng="es">africada</trd>
  <trd lng="fa">&a_h;&a_m;&a_kh;&a_w;&a_A;&a_n; &a_m;&a_r;&f_k;&a_b;</trd>
  <trd lng="fr">affriqu&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>z&aacute;r-r&eacute;shang</trd>,
    <trd>affrik&aacute;ta</trd>
  </trdgrp>
  <trd lng="it">affricata</trd>
  <trd lng="pl">afrykata</trd>
  <trdgrp lng="pt">
    <trd>africado</trd>,
    <trd>explosivo-fricativo</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_f;&c_f;&c_r;&c_i;&c_k;&c_a;&c_t;&c_a;</trd>
</drv>
</art>
