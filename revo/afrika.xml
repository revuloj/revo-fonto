<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>afrikat</rad>/o
</kap>
<drv mrk="afrika.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      Kombino de plosivo kaj frikativo, &ccirc;e kiu plosiva alteno estas
      sekvata de frikativa forteno (c, &ccirc;, &gcirc;).
    </dif>
    <refgrp tip="vid">
      <ref cel="plosiv.0o">plosivo</ref>,
      <ref cel="frikat.0o">frikativo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Affrikat</trd>,
    <trd>Affrikata</trd>,
    <trd>Affrikate</trd>
  </trdgrp>
  <trd lng="es">africada</trd>
  <trd lng="fr">affriqu&eacute;e</trd>
  <trdgrp lng="pt">
    <trd>africado</trd>,
    <trd>explosivo-fricativo</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_f;&c_f;&c_r;&c_i;&c_k;&c_a;&c_t;&c_a;</trd>
</drv>
</art>
