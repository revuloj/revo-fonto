<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gurd</rad>/o
</kap>
<drv mrk="gurd.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malgranda portebla orgeneto kies klavaron a&ubreve;tomate funkciigas
      cilindro turnata per manturnilo. 
    </dif>
  </snc>
  <trd lng="ru">&c_sh;&c_a;&c_r;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>
</drv>
<drv mrk="gurd.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Ludi per <tld/>o:
      <ekz>la mizerulo <tld/>is melankolian arion.</ekz>
    </dif>
    <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_sh;&c_a;&c_r;&c_m;&c_a;&c_n;&c_k;&c_je;</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fari a&ubreve;tomate, ma&scirc;ine, rutine, kvaza&ubreve; turnante senpense
      la turnilon de <tld/>o:
      <ekz>
        <tld/>itaj (banalaj, malmulte signifaj, tede ripetitaj) frazoj.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_t;&c_v;&c_je;&c_r;&c_d;&c_i;&c_t;&c_mol; &c_o;&c_d;&c_n;&c_o; &c_i; &c_t;&c_o; &c_zh;&c_je;</trd>,
      <trd>&c_t;&c_a;&c_l;&c_d;&c_y;&c_ch;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
