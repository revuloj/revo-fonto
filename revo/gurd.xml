<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gurd</rad>/o
</kap>
<drv mrk="gurd.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Malgranda portebla orgeneto kies klavaron a&ubreve;tomate funkciigas
      cilindro turnata per manturnilo. 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_r;&c_y;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_t;&c_r;&c_y;&c_n;&c_k;&c_a;</trd> 
  </trdgrp>
  <trdgrp lng="de">
     <trd>Leierkasten</trd>,
     <trd>Drehorgel</trd></trdgrp>
  <trdgrp lng="es">
     <trd>organillo</trd>,
     <trd>cilindro</trd></trdgrp>
  <trdgrp lng="hu">
     <trd>verkli</trd>,
     <trd>kintorna</trd>,
     <trd>s&iacute;pl&aacute;da</trd>
  </trdgrp>
  <trd lng="fr">orgue de Barbarie</trd>
  <trd lng="nl">draaiorgel</trd>
  <trd lng="ru">&c_sh;&c_a;&c_r;&c_m;&c_a;&c_n;&c_k;&c_a;</trd>
</drv>

<drv mrk="gurd.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ludi per <tld/>o:
      <ekz>la mizerulo <tld/>is melankolian arion.</ekz>
    </dif>
    <trd lng="be">&c_g;&c_r;&c_a;&c_c;&c_mol; &c_n;&c_a; 
       <ind>&c_k;&c_a;&c_t;&c_r;&c_y;&c_n;&c_c;&c_y;</ind></trd>
    <trd lng="fr">jouer<klr tip="ind"> (sur un orgue de
Barbarie)</klr></trd>
    <trdgrp lng="hu">
       <trd>verkliz</trd>,
       <trd>verklin j&aacute;tszik</trd>
    </trdgrp>
    <trd lng="nl">een <ind>draaiorgel</ind> bespelen</trd>
    <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a;
&c_sh;&c_a;&c_r;&c_m;&c_a;&c_n;&c_k;&c_je;</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fari a&ubreve;tomate, ma&scirc;ine, rutine, kvaza&ubreve; turnante
senpense
      la turnilon de <tld/>o:
      <ekz>
        <tld/>itaj <klr>(banalaj, malmulte signifaj, tede ripetitaj)</klr> 
        frazoj.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_w;&c_t;&c_a;&c_r;&c_a;&c_c;&c_mol; 
      &c_a;&c_d;&c_n;&c_o; &c_j; &c_t;&c_o;&c_je; &c_zh;</trd>
    <trdgrp lng="fr">
      <trd>rab&acirc;cher</trd>,
      <trd>ressasser</trd></trdgrp> 
    <trdgrp lng="hu">
       <trd>verkliz<klr tip="ind"> (&aacute;tv...)</klr></trd>,
       <trd>elcs&eacute;pel<klr tip="ind"> (&aacute;tv.)</klr></trd>,
       <trd>hajtogat<klr tip="ind"> (&aacute;tv.)</klr></trd>
    </trdgrp>
    <trd lng="nl">tot treurens toe <ind>herhalen</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_v;&c_je;&c_r;&c_d;&c_i;&c_t;&c_mol; &c_o;&c_d;&c_n;&c_o;
&c_i; &c_t;&c_o; &c_zh;&c_je;</trd>,
      <trd>&c_t;&c_a;&c_l;&c_d;&c_y;&c_ch;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
