<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stamf.xml,v 1.13 2022/05/05 14:43:48 revo Exp $">
<kap>
   <rad>stamf</rad>/i <fnt><bib>PIV1</bib></fnt>,
   <var><kap><rad var="p">stampf</rad>/i <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="stamf.0i">
   <kap><tld/>i,
   <var><kap><tld var="p"/>i</kap></var>
   </kap>
   <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Senpacience piedfrapadi, danceti:
      <ekz>
         la nigraj kavaliroj de <nom>Voland</nom>, rajdantaj du ardajn,
         <tld/>antajn
         &ccirc;evalojn<fnt><bib>MkM</bib>, <lok>&ccirc;apitro 22a, p.
         222a</lok></fnt>;
      </ekz>
      <ekz>
        li vipis la &ccirc;evalon, ke &gcirc;i levis sin kaj <tld var="p"/>is
        <fnt>
          <aut>Selma Lagerl&ouml;f, trad. Stellan Engholm</aut>
          <vrk><url ref="https://epdf.tips/la-mono-de-sinjoro-arne.html">La Mono de Sinjoro Arne</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la fremdulo komencis <tld var="p"/>i tien-reen
        <fnt>
          <aut>John Collier, trad. Reto Rossetti</aut>
          <vrk><url ref="&DonH;Revuoj/np/np5703/mono.html">Sor&ccirc;ita mono</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tang.0i">tangi</ref>,
      <ref cel="tret.0i">treti</ref>
    </refgrp>
  </snc>
   <trd lng="be">&c_b;&c_ib;&c_c;&c_mol;
   <ind>&c_k;&c_a;&c_p;&c_y;&c_t;&c_a;&c_m;&c_ib;</ind></trd>
   <trd lng="de">stampfen</trd>
   <trdgrp lng="en">
     <trd>to stomp</trd>,
     <trd>stamp</trd>, <trd>pound feet</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>piaffer</trd>,
     <trd>taper du pied</trd>,
     <trd>tr&eacute;pigner</trd>
   </trdgrp>
  <trdgrp lng="hu">
    <trd>r&uacute;gkap&aacute;l</trd>,
    <trd>toporz&eacute;kol</trd>
  </trdgrp>
   <trdgrp lng="nl">
     <trd>stampvoeten</trd>,
     <trd>trappelen</trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>przest&eogonek;powa&cacute; z nogi na nog&eogonek;</trd>,
     <trd>tupa&cacute;</trd>,
     <trd>przytupywa&cacute;</trd>,
     <trd>grzeba&cacute; kopytem<klr tip="ind"> (np. o koniu)</klr></trd>
   </trdgrp>
   <trd lng="ru">&c_b;&c_i;&c_t;&c_mol; &c_k;&c_o;&c_p;&c_y;&c_t;&c_a;&c_m;&c_i;</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x440;&#x438;&#x43A;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x431;&#x438;&#x442;&#x438; &#x43A;&#x43E;&#x43F;&#x438;&#x442;&#x430;&#x43C;&#x438;</trd>,
    <trd>&#x442;&#x443;&#x43F;&#x430;&#x442;&#x438; &#x43D;&#x43E;&#x433;&#x43E;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="stamf.0ado">
   <kap><tld/>ado</kap>
   <snc>
     <dif>
       Senpacienca surloka piedfrapado:
       <ekz>
         liaj piedoj preska&ubreve; rompi&gcirc;is pro <tld/>ado
         <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 69a, p. 468a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="cs">dupot <klr>(zvl. kon&ecaron;)</klr></trd>
  <trd lng="de">Stampfen</trd>
  <trd lng="en">stomping</trd>
  <trdgrp lng="fr">
     <trd>battement de pieds</trd>,
     <trd>tr&eacute;pignement</trd>
   </trdgrp>
  <trdgrp lng="hu">
     <trd>r&uacute;gkap&aacute;l&aacute;s</trd>,
     <trd>toporz&eacute;kol&aacute;s</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>przytupywanie</trd>,
     <trd>grzebanie kopytem<klr tip="ind"> (np. o koniu)</klr></trd>
   </trdgrp>
  <trd lng="sk">dupot</trd>
</drv>
</art>

