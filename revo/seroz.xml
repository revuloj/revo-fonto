<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: seroz.xml,v 1.11 2023/10/21 06:31:44 revo Exp $">
<kap>
  <rad>seroz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="seroz.0o">
  <kap><tld/>o</kap>
  <snc mrk="seroz.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Membrano, kiu tegas korpan kava&jcirc;on:
      <ref tip="sub" cel="perika.0o">perikardo</ref>,
      <ref tip="sub" cel="perito.0o">peritoneo</ref>,
      <ref tip="sub" cel="pleuxr.0o.ANA">ple&ubreve;ro</ref>:
      <ekz>
        ple&ubreve;ro estas <tld/>o, kiu tegas la internon de la torako
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Ple%C5%ADro">
          ple&ubreve;ro</url>, 2013-03-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_r;&c_o;&c_z;&c_n;&c_a;&c_ja; &c_a;&c_b;&c_a;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>
  <trd lng="cs">sliznice</trd>
  <trd lng="de">Tunica serosa</trd>
  <trd lng="en">serous membrane</trd>
  <trd lng="et">kelme</trd>
  <trdgrp lng="fr">
    <trd>membrane s&eacute;reuse</trd>,
    <trd>s&eacute;reuse<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="it">membrana sierosa</trd>
  <trd lng="ja">&#28479;&#33180; <pr>&#12375;&#12423;&#12358;&#12414;&#12367;</pr></trd>
  <trd lng="ko">&#51109;&#50529;&#47561;</trd>
  <trd lng="pl">surowic&oacute;wka</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_o;&c_z;&c_n;&c_a;&c_ja; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a;</trd>
  <trd lng="sk">sliznica</trd>
  <trd lng="sv">ser&ouml;s hinna</trd>
  <trd lng="uk">&c_s;&c_je;&c_r;&#x43E;&c_z;&#x43D;&c_a; &#x43E;&c_b;&#x43E;&#x43B;&#x43E;&#x43D;&#x43A;&c_a;</trd>
</drv>

<drv mrk="seroz.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="seroz.0ajxo.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Sero-simila korpa flua&jcirc;o, kiun produktas <tld/>oj:
      <ekz>
        <tld/>a&jcirc;o akumuli&gcirc;is en la genuo pro inflamo
        &ccirc;e la menisko
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url ref="http://www.vastalto.com/blog/?p=758">
          Genuo doloras</url></vrk>,
          <lok>Akvosfero, 2007-08-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_r;&c_o;&c_z;&c_n;&c_a;&c_je; &c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="fr">s&eacute;rosit&eacute;</trd>
  <trd lng="pl">b&lstroke;ona  <ind>surowiczna</ind></trd>
</drv>
</art>

