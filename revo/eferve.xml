<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eferve.xml,v 1.27 2025/03/29 13:05:54 revo Exp $">
<kap>
  <rad>efervesk</rad>/i
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="eferve.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="bol.0i">Boli</ref> produktante eli&gcirc;an gason en ia likva&jcirc;o:
      <ekz>
        kiam acido atakas iujn substancojn &gcirc;i <tld/>as;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en Japanio, tuj post la fino de la milito <tld/>is demokratio
        <fnt>
          <aut>YAMASAKI Seik&ocirc;</aut>
          <vrk><url ref="&Tekstaro;?nomo=artikoloj-el-monato">Militanto kontra&ubreve; la
          milito</url></vrk>
          <bib>Monato</bib>
          <!-- la teksto estis preparita por Monato, sed &scirc;ajne neniam aperis -->
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxauxm.0i">&scirc;a&ubreve;mi</ref>,
      <ref cel="sxpruc.0i">&scirc;pruci</ref>
    </refgrp>
  </snc>

  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_s;&c_mol;&c_p;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bublat</trd>,
    <trd>klokotat</trd>,
    <trd>perlit se</trd>,
    <trd>p&ecaron;nit</trd>,
    <trd>zp&ecaron;nit</trd>,
    <trd>&scaron;lehat</trd>,
    <trd>&scaron;um&ecaron;t</trd>,
    <trd>&scaron;um&ecaron;t <klr>(v&iacute;no)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufbrausen</trd>,
    <trd>aufwallen</trd>,
    <trd>sprudeln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>effervesce</trd>,
    <trd>bubble</trd>,
    <trd>fizz</trd>
  </trdgrp>
  <trd lng="fr">bouillonner<klr tip="ind"> (par
  effervescence)</klr></trd>
  <trdgrp lng="hu">
    <trd>pezseg</trd>,
    <trd>habzik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#36215;&#27832;&#12377;&#12427;  <pr>&#12362;&#12371;&#12431;&#12377;&#12427;</pr></trd>,
    <trd>&#30330;&#27873;&#12377;&#12427;  <pr>&#12399;&#12387;&#12413;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">bruisen</trd>
  <trdgrp lng="pl">
    <trd>pieni&cacute; si&eogonek;</trd>,
    <trd>musowa&cacute;</trd>,
    <trd>buzowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">efervescer</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_s;&c_p;&c_je;&c_n;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bubla&tcaron;</trd>,
    <trd>perli&tcaron;</trd>,
    <trd>&scaron;umie&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&#1082;&c_i;&#1087;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_h;&#1083;&c_i;&c_v;&#1086; &#1087;&c_ib;&#1085;&c_i;&c_t;&c_i;&c_s;&#1103; (&#1087;&c_r;&c_i; &c_v;&c_i;&c_d;&c_ib;&#1083;&c_je;&#1085;&#1085;&c_ib;
    &c_g;&c_a;&c_z;&c_ib;&c_v;)</trd>
  </trdgrp>
  <trd lng="zh">&#26292;&#36339;&#22914;&#31867; <pr>b&#224;oti&#224;or&#250;l&#232;i</pr></trd>
</drv>

<drv mrk="eferve.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta <tld/>on:
      <ekz>
        <ind><tld/>a pulvoro</ind> estas bongusta kaj kun iom da salivo &gcirc;i bone &scirc;a&ubreve;mas
        <fnt>
          <aut>WACHA Bal&aacute;zs</aut>
          <vrk><url ref="&Monato;/2002/007405.html">Libroj &dash; Mirakla tambureto</url></vrk>
          <bib>Monato</bib>
          <lok>2003/03, p. 18</lok>
        </fnt>;
        <trd lng="de">Brausepulver</trd>
        <trd lng="nl">bruispoeder</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a <klr>(<ref cel="bol.0i.fervori">bola</ref>, <ref cel="fervor.0a">fervora</ref>)</klr>
        dialogo en parola lingvo flua
        <fnt>
        <aut>Christian Declerck</aut>
          <vrk><url ref="&Tekstaro;?nomo=artikoloj-el-monato">Pliaj novaj vortoj
          poiradas!</url></vrk>
          <bib>Monato</bib>
          <lok>1998/04, p. 22-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    figurasence, la&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>ekbolanta</ke>, <ke>vigla</ke>, <ke>gaje vigla</ke>.
  </rim>  
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_n;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_p;&c_je;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_y;&c_p;&c_u;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prchliv&yacute; (p&rcaron;en.)</trd>,
    <trd>&scaron;umiv&yacute; <klr>(n&aacute;poj)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufbrausend</trd>,
    <trd>aufwallend</trd>,
    <trd>sprudelnd</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>effervescent</trd>,
    <trd>fizzy</trd>
  </trdgrp>
  <trd lng="es">efervescente</trd>
  <trd lng="fr">effervescent</trd>
  <trd lng="he">&tav;&vav;&#1465;&samekh;&#1461;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>pezsg&odblac;</trd>,
    <trd>habz&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#27873;&#31435;&#12387;&#12390;&#12356;&#12427;  <pr>&#12354;&#12431;&#12384;&#12387;&#12390;&#12356;&#12427;</pr></trd>,
    <trd>&#30330;&#27873;&#24615;&#12398;  <pr>&#12399;&#12387;&#12413;&#12358;&#12379;&#12356;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="nl">bruisend</trd>
  <trdgrp lng="pl">
    <trd>pieni&aogonek;cy</trd>,
    <trd>musuj&aogonek;cy</trd>,
    <trd>buzuj&aogonek;cy</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_i;&c_p;&c_u;&c_ch;&c_i;&c_j;</trd>
  <trd lng="sk">&scaron;umiv&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_r;&c_h;&#1083;&c_i;&c_v;&#1086; &#1087;&c_ib;&#1085;&#1083;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&#1082;&c_i;&#1087;&c_a;&#1102;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="eferve.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago de tio, kio <tld/>as; rezulto de tiu ago:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        oni &jcirc;us eniris tiun unuan fazon de <tld/>o <klr>(entuziasma disvolvi&gcirc;o)</klr>
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">La gazetoj restas
          la &ccirc;efa portanto de nia kulturo &dash; Intervjuo kun d-ro Giorgio Silfer </url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_p;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_ib;&c_p;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; &c_b;&c_u;&c_r;&c_b;&c_a;&c_l;&c_a;&c_k; &c_g;&c_a;&c_z;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>efervescence</trd>,
    <trd>klokot</trd>,
    <trd>nap&ecaron;n&ecaron;n&iacute;</trd>,
    <trd>p&ecaron;n&ecaron;n&iacute;</trd>,
    <trd>roz&ccaron;ilen&iacute;</trd>,
    <trd>zp&ecaron;n&ecaron;n&iacute;</trd>,
    <trd>&scaron;leh&aacute;n&iacute;</trd>,
    <trd>&scaron;um&ecaron;n&iacute; <klr>(miner&aacute;lky)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Aufbrausen</trd>,
    <trd>Aufwallen</trd>,
    <trd>Aufwallung</trd>
  </trdgrp>
  <trd lng="en">effervescence</trd>
  <trd lng="fr">effervescence</trd>
  <trd lng="he">&tav;&vav;&#1465;&samekh;&#1461;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>pezsg&eacute;s</trd>,
    <trd>habz&aacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#30330;&#27873;  <pr>&#12399;&#12387;&#12413;&#12358;</pr></trd>
  <trdgrp lng="pl">
    <trd>spienianie</trd>,
    <trd>musowanie</trd>,
    <trd>buzowanie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_s;&c_k;&c_i;&c_p;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_v;&c_s;&c_p;&c_je;&c_n;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_sh;&c_i;&c_p;&c_je;&c_n;&c_i;&c_je;<klr tip="amb">
    (&c_p;&c_r;&c_i; &c_v;&c_y;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_i;
    &c_g;&c_a;&c_z;&c_o;&c_v;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>penenie</trd>,
    <trd>&scaron;umenie</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&#1082;&c_i;&#1087;&c_a;&#1085;&#1085;&#1103;</trd>
</drv>
</art>
