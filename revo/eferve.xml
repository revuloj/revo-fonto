<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eferve.xml,v 1.19 2022/03/16 15:54:27 revo Exp $">
<kap>
  <rad>efervesk</rad>/i
</kap>
<drv mrk="eferve.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="bol.0i">Boli</ref> produktante eli&gcirc;an gason en ia likva&jcirc;o:
      <ekz>
        kiam acido atakas iujn substancojn &gcirc;i <tld/>as;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en Japanio, tuj post la fino de la milito <tld/>is demokratio
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Japanio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxauxm.0i">&scirc;a&ubreve;mi</ref>,
      <ref cel="sxpruc.0i">&scirc;pruci</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_s;&c_mol;&c_p;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bublat</trd>,
    <trd>klokotat</trd>,
    <trd>perlit se</trd>,
    <trd>p&ecaron;nit</trd>,
    <trd>zp&ecaron;nit</trd>,
    <trd>&scaron;lehat</trd>,
    <trd>&scaron;um&ecaron;t</trd>,
    <trd>&scaron;um&ecaron;t <klr>(v&iacute;no)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufbrausen</trd>,
    <trd>aufwallen</trd>,
    <trd>sprudeln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>effervesce</trd>,
    <trd>bubble</trd>,
    <trd>fizz</trd>
  </trdgrp>
  <trd lng="fr">bouillonner<klr tip="ind"> (par
  effervescence)</klr></trd>
  <trdgrp lng="hu">
    <trd>pezseg</trd>,
    <trd>habzik</trd>
  </trdgrp>
  <trd lng="nl">bruisen</trd>
  <trdgrp lng="pl">
    <trd>pieni&cacute; si&eogonek;</trd>,
    <trd>musowa&cacute;</trd>,
    <trd>buzowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">efervescer</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_s;&c_p;&c_je;&c_n;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bubla&tcaron;</trd>,
    <trd>perli&tcaron;</trd>,
    <trd>&scaron;umie&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43A;&#x438;&#x43F;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x431;&#x443;&#x440;&#x445;&#x43B;&#x438;&#x432;&#x43E; &#x43F;&#x456;&#x43D;&#x438;&#x442;&#x438;&#x441;&#x44F; (&#x43F;&#x440;&#x438; &#x432;&#x438;&#x434;&#x456;&#x43B;&#x435;&#x43D;&#x43D;&#x456; &#x433;&#x430;&#x437;&#x456;&#x432;)</trd>
  </trdgrp>
</drv>

<drv mrk="eferve.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta <tld/>on:
      <ekz>
        <ind><tld/>a pulvoro</ind> estas bongusta kaj kun iom da salivo &gcirc;i bone &scirc;a&ubreve;mas
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Libroj &dash; Mirakla tambureto</url></vrk>
        </fnt>;
        <trd lng="de">Brausepulver</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a <klr>(<ref cel="bol.0i.fervori">bola</ref>, <ref cel="fervor.0a">fervora</ref>)</klr> 
        dialogo en parola lingvo flua
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato">Pliaj novaj vortoj poiradas!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>prchliv&yacute; (p&rcaron;en.)</trd>,
    <trd>&scaron;umiv&yacute; <klr>(n&aacute;poj)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufbrausend</trd>,
    <trd>aufwallend</trd>,
    <trd>sprudelnd</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>effervescent</trd>,
    <trd>fizzy</trd>
  </trdgrp>
  <trd lng="es">efervescente</trd>
  <trd lng="fr">effervescent</trd>
  <trd lng="he">&tav;&vav;&#x5B9;&samekh;&#x5B5;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>pezsg&odblac;</trd>,
    <trd>habz&oacute;</trd>
  </trdgrp>
  <trd lng="nl">bruisend</trd>
  <trdgrp lng="pl">
    <trd>pieni&aogonek;cy</trd>,
    <trd>musuj&aogonek;cy</trd>,
    <trd>buzuj&aogonek;cy</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_i;&c_p;&c_u;&c_ch;&c_i;&c_j;</trd>
  <trd lng="sk">&scaron;umiv&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x440;&#x445;&#x43B;&#x438;&#x432;&#x43E; &#x43F;&#x456;&#x43D;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x43A;&#x438;&#x43F;&#x430;&#x44E;&#x447;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="eferve.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago de tio, kio <tld/>as; rezulto de tiu ago:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        oni &jcirc;us eniris tiun unuan fazon de <tld/>o <klr>(entuziasma disvolvi&gcirc;o)</klr>
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">La gazetoj restas 
          la &ccirc;efa portanto de nia kulturo &dash; Intervjuo kun d-ro Giorgio Silfer </url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>efervescence</trd>,
    <trd>klokot</trd>,
    <trd>nap&ecaron;n&ecaron;n&iacute;</trd>,
    <trd>p&ecaron;n&ecaron;n&iacute;</trd>,
    <trd>roz&ccaron;ilen&iacute;</trd>,
    <trd>zp&ecaron;n&ecaron;n&iacute;</trd>,
    <trd>&scaron;leh&aacute;n&iacute;</trd>,
    <trd>&scaron;um&ecaron;n&iacute; <klr>(miner&aacute;lky)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Aufbrausen</trd>,
    <trd>Aufwallen</trd>,
    <trd>Aufwallung</trd>
  </trdgrp>
  <trd lng="en">effervescence</trd>
  <trd lng="fr">effervescence</trd>
  <trd lng="he">&tav;&vav;&#x5B9;&samekh;&#x5B5;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>pezsg&eacute;s</trd>,
    <trd>habz&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>spienianie</trd>,
    <trd>musowanie</trd>,
    <trd>buzowanie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_s;&c_k;&c_i;&c_p;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_v;&c_s;&c_p;&c_je;&c_n;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_sh;&c_i;&c_p;&c_je;&c_n;&c_i;&c_je;<klr tip="amb">
    (&c_p;&c_r;&c_i; &c_v;&c_y;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_i;
    &c_g;&c_a;&c_z;&c_o;&c_v;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>penenie</trd>,
    <trd>&scaron;umenie</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x43A;&#x438;&#x43F;&#x430;&#x43D;&#x43D;&#x44F;</trd>
</drv>
</art>
