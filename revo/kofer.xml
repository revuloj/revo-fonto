<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kofer.xml,v 1.12 2023/12/03 09:58:51 revo Exp $">
<kap>
  <rad>kofer</rad>/o <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="kofer.0o">
  <kap><tld/>o</kap>
  <snc mrk="kofer.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Dornarbeto kreskanta en Nordafriko kaj regionoj de tropika Azio,
      kun maldensa foliaro kaj tre malgrandaj florpanikloj,
      el kies folioj oni faras <ref tip="vid" cel="hena.0o.KOS">henaon</ref>;
      sola <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">specio</ref> de sia genro
      en familio
      <ref tip="super" cel="litrum1.0oj">litrumacoj</ref>
      (<trd lng="la">Lawsonia inermis</trd>):
      <ekz>
        viaj kreska&jcirc;oj estas paradizo de granatoj kun multevaloraj
        fruktoj, plena de <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Alt; 4:13</lok></fnt>;
      </ekz>
      <ekz>
        oni kultivas anka&ubreve; cerealojn, legomojn, <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=289056">Rivero Dra</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/f7/Lawsonia_inermis_Blanco_part.png/279px-Lawsonia_inermis_Blanco_part.png" prm="PD">ilustra&jcirc;o de <tld/>obran&ccirc;o (Lawsonia inermis)
        <fnt>
          <aut>Ayacop</aut>
          <lok><url ref="&WCW;/File:Lawsonia_inermis_Blanco_part.png">&WCW;/File:Lawsonia_inermis_Blanco_part.png</url></lok>
        </fnt>
    </bld>
  </snc>
  <rim mrk="kofer.0o.RIM_SIN">
    PIV2 proponas la nomon <em>lavsonio</em> kiel esperantigon de la
    scienca nomo.
  </rim>
  <trd lng="be">&c_l;&c_a;&c_w;&c_s;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="ca">alquena</trd>
  <trd lng="de">Hennahstrauch</trd>
  <trd lng="fa">&a_H;&a_n;&a_A;<klr tip="ind"> (&a_d;&a_r;&a_kh;&a_t;&f_ch;&a_h;)</klr></trd>
  <trd lng="fr">henn&eacute;<klr> (arbre)</klr></trd>
  <trd lng="pl">henna</trd>
  <trd lng="ro">henna</trd>
  <trd lng="sk">hena</trd>
  <trd lng="cs">henna</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_a;&c_v;&c_s;&#x43E;&#x43D;&c_ib;&#x44F;</trd>,
    <trd>&c_h;&#x43D;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x6307;&#x7532;&#x82B1; <pr>zh&#x1D0;jiahu&#x101;</pr></trd>
</drv>
</art>

