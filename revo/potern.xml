<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: potern.xml,v 1.13 2022/07/12 08:42:04 revo Exp $">
<kap>
  <rad>potern</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="potern.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Eta, duaranga <ref tip="super" cel="pord.0o">pordo</ref> tra la
      murego de fortika&jcirc;o:
      <ekz>
        &ccirc;i tie la malamikoj enpu&scirc;as sin! Gardu tiun <tld/>on
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 53a,
        p.
        694a</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_t;&c_e;&c_r;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_t;&c_a;&c_je;&c_m;&c_n;&c_y; &c_h;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="ca">poterna</trd>
  <trdgrp lng="cs">
    <trd>podzemn&iacute; chodba</trd>,
    <trd>poterna</trd>
  </trdgrp>
  <trd lng="en">postern</trd>
  <trd lng="es">poterna</trd>
  <trd lng="fr">poterne</trd>
  <trdgrp lng="it">
    <trd>postierla</trd>,
    <trd>pusterla</trd>,
    <trd>porta secondaria</trd>
  </trdgrp>
  <trd lng="nl">poterne</trd>
  <trdgrp lng="pl">
    <trd>poterna</trd>,
    <trd>potajnik</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>poterna</trd>,
    <trd>porta falsa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_t;&c_a;&c_j;&c_n;&c_o;&c_j; &c_h;&c_o;&c_d;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_a;&c_j;&c_n;&c_a;&c_ja;
    &c_d;&c_v;&c_je;&c_r;&c_mol;</trd>
    <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">podzemn&aacute; chodba</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_t;&c_je;&c_r;&#x43D;&c_a;</trd>,
    <trd>&#x43F;&#x43E;&c_t;&c_a;&c_j;&#x43D;&c_i;&c_j; &c_h;&c_ib;&c_d;</trd>
  </trdgrp>
</drv>
</art>
