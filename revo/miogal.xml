<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>miogal</rad>/o
</kap>
<drv mrk="miogal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="miogal.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="talped.0oj">talpedoj</ref>
      (<trd lng="la">Desmana</trd>), insektoman&gcirc;ulo kun
      rostro kaj na&gcirc;ha&ubreve;to inter la piedfingroj, vivanta
      en e&ubreve;ropaj kaj aziaj riveroj.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_h;&c_u;&c_l;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Bisamr&uuml;ssler</trd>,
    <trd>Desman</trd>
  </trdgrp>
  <trd lng="en">desman</trd>
  <trd lng="fr">desman</trd>
  <trdgrp lng="hu">
    <trd>p&eacute;zsmacick&aacute;ny</trd>,
    <trd>dezm&aacute;n</trd>
  </trdgrp>
  <trd lng="nl">muscusspitsmuis</trd>
  <trd lng="ru">&c_v;&c_y;&c_h;&c_u;&c_h;&c_o;&c_l;&c_mol;</trd>
</drv>
</art>
