<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: miogal.xml,v 1.22 2023/09/02 06:04:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>miogal</rad>/o
</kap>
<drv mrk="miogal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="miogal.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref cel="mangx.insekto0ulo" lst="voko:insektoman&gcirc;uloj">Insektoman&gcirc;ulo</ref>
      kun <ref tip="prt" cel="rostr.0o">rostro</ref> kaj 
      na&gcirc;ha&ubreve;to inter la piedfingroj,
      vivanta en e&ubreve;ropaj kaj aziaj riveroj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="talped.0oj">talpedoj</ref>
      (<trd lng="la">Desmana</trd>):
      <ekz>
        la serio El <klr>[<nac>Hombre y la Tierra</nac>&#x2026;]</klr> filmis kelkajn animalojn unuafoje,
        <klr>[ekz.]</klr> la Pirenean <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=359040">F&eacute;lix Rodr&iacute;guez de la Fuente</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/b/b0/Desman-drawing.jpg/640px-Desman-drawing.jpg" prm="PD">desegno de <tld/>oj (Myogale moschata, sin. Sorex moschatus kaj Desmana moschata)
        <fnt>
          <aut>Gustav M&uuml;tzel, Brehms Tierleben, Small Edition 1927</aut>
          <lok><url ref="&WCW;/File:Desman-drawing.jpg">&WCW;/File:Desman-drawing.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_h;&c_u;&c_l;&c_ja;</trd>
  <trd lng="ca">almesquera</trd>
  <trdgrp lng="de">
    <trd>Bisamr&uuml;ssler</trd>,
    <trd>Desman</trd>
  </trdgrp>
  <trd lng="en">desman</trd>
  <trd lng="es">desm&aacute;n</trd>
  <trd lng="fr">desman</trd>
  <trdgrp lng="hu">
    <trd>p&eacute;zsmacick&aacute;ny</trd>,
    <trd>dezm&aacute;n</trd>
  </trdgrp>
  <trd lng="ja">&#x30C7;&#x30B9;&#x30DE;&#x30F3;</trd>
  <trd lng="nl">muscusspitsmuis</trd>
  <trd lng="pl">desman ukrai&nacute;ski</trd>     
  <trd lng="ro">desman ucrainean</trd>
  <trd lng="ru">&c_v;&c_y;&c_h;&c_u;&c_h;&c_o;&c_l;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&#x445;&#x43E;&#x445;&#x443;&#x43B;&#x44F;</trd>,
    <trd>&#x432;&#x438;&#x445;&#x443;&#x445;&#x456;&#x43B;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
