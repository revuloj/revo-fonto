<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>miogal</rad>/o
</kap>
<drv mrk="miogal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="talp.0edoj">talpedoj</ref>
      (<trd lng="la">Desmana</trd>), insektoman&gcirc;ulo kun 
      rostro kaj na&gcirc;ha&ubreve;to inter la piedfingroj, vivanta
      en e&ubreve;ropaj kaj aziaj riveroj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bisamr&uuml;ssler</trd>,
    <trd>Desman</trd>
  </trdgrp>
  <trd lng="en">desmana</trd>
</drv>
</art>

