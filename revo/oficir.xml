<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: oficir.xml,v 1.27 2024/01/30 16:09:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>oficir</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="oficir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="oficir.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      &Gcirc;enerala nomo de &ccirc;iuj armeaj <ref tip="super" cel="estr.0o">estro</ref>j, ekde 
      <ref tip="sub" cel="leuxte.sub0o">suble&ubreve;tenanto</ref>, 
      &gcirc;is <ref tip="sub" cel="marsxa.0o.MIL">mar&scirc;alo</ref> a&ubreve;
      <ref tip="sub" cel="genera1.0o">generalo</ref>:
      <ekz>
        <tld/>o mi fine jam estas sed, kiel anta&ubreve;e, ri&ccirc;eco forestas
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patro opiniis lin supera <tld/>o kaj kondukis lin en la &ccirc;ambron
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ursa Felo</lok>
        </fnt>;
      </ekz>
      <ekz>
        eniris polica <tld/>o kaj petis apartan a&ubreve;diencon en tre grava afero
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel <ind><tld/>aspiranto</ind> li batalis en la rusa fronto
        <fnt>
          <bib>LGA</bib>
        </fnt>;
        <trd lng="de">Offiziersanw&auml;rter</trd>
      </ekz>
      <ekz>
        oficiro kovrita de amuletoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_f;&c_ib;&c_c;&c_e;&c_r;</trd>
    <trd lng="bg">&c_o;&c_f;&c_i;&c_c;&c_je;&c_r;</trd>
    
    <trd lng="en">officer</trd>
    <trd lng="fa">&a_A;&a_f;&a_s;&a_r;</trd>
    <trd lng="fr">officier<klr tip="ind"> (militaire)</klr></trd>
    <trd lng="he">&qof;&tsadi;&yod;&fnun;</trd>
    <trd lng="hu">katonatiszt</trd>
    <trd lng="it">ufficiale<klr> (mil.)</klr></trd>
    <trd lng="nl">officier</trd>
    <trd lng="pl">oficer</trd>
    <trd lng="pt">oficial</trd>
    <trd lng="ro">ofi&#x21B;er</trd>
    <trd lng="ru">&c_o;&c_f;&c_i;&c_c;&c_je;&c_r;</trd>
    <trd lng="sv">officer</trd>
  </snc>
  <snc mrk="oficir.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Rangulo sub la <ref cel="sxip.0estro">&scirc;ipestro</ref>, kun gvidantaj 
      navigaj a&ubreve; te&hcirc;nikaj taskoj:
      <ekz>
        la &scirc;ipestro a&ubreve; la unua <tld/>o akceptadis la po&scirc;ton
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la Dua <tld lit="O"/>o tenis notblokon kun en&scirc;ovita karbopapero
        <fnt>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;ipestro, la <tld/>oj kaj la in&gcirc;enieroj dufoje &ccirc;iutage ebrii&gcirc;is en la tuta
        tempo, kiam ni nun ankrumis
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="stevard.0o">stevardo</ref>
    </refgrp>
    <trd lng="de">Nautischer <ind>Offizier</ind></trd>
  </snc>
  <snc mrk="oficir.0o.en_ordeno">
    <dif>
      Tiu, kiu havas, en <ref cel="orden.0o.honora">ordeno</ref>, rangon
      mezan inter <ref cel="kavali.0o.en_ordeno">kavaliro</ref>
      kaj <ref cel="komand2.0o">komandoro</ref>:
      <ekz>
        <tld/>o de la franca ordeno <nom>Ordre National du M&eacute;rite</nom>
        <fnt>
          <vrk><url ref="&UEA;pdf/IKU/IKU2011.pdf">Sunsistemoj kaj planedoj &ccirc;irka&ubreve; foraj steloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">officier<klr tip="ind"> (grade
    honorifique)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>d&uring;stojn&iacute;k</trd>,
    <trd>ofic&iacute;r</trd>
  </trdgrp>
  <trd lng="de">Offizier</trd>
  <trdgrp lng="ja">
    <trd>&#x58EB;&#x5B98;  <pr>&#x3057;&#x304B;&#x3093;</pr></trd>,
    <trd>&#x5C06;&#x6821;  <pr>&#x3057;&#x3087;&#x3046;&#x3053;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="sk">d&ocirc;stojn&iacute;k</trd>
  <trd lng="uk">&#x43E;&#x444;&#x456;&#x446;&#x435;&#x440;</trd>
  <trdgrp lng="zh">
    <trd>&#x5C09; <pr>w&#xE8;i</pr></trd>,
    <trd>&#x519B;&#x5B98; <pr>j&#x16B;ngu&#x101;n</pr></trd>,
    <trd>&#x8ECD;&#x5B98; <pr>j&#x16B;ngu&#x101;n</pr></trd>,
    <trd>&#x519B;&#x957F; <pr>j&#x16B;nzh&#x1CE;ng</pr></trd>,
    <trd>&#x8ECD;&#x9577; <pr>j&#x16B;nzh&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="oficir.sub0o">
  <kap>sub<tld/>o</kap>
  <snc mrk="oficir.sub0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      &Gcirc;enerala nomo de &ccirc;iuj armeaj estroj, ekde 
      <ref tip="sub" cel="sergxe.0o">ser&gcirc;ento</ref> &gcirc;is ekskluzive
      <ref tip="sub" cel="leuxte.sub0o">suble&ubreve;tenanto</ref>:
      <ekz>
        la suboficiroj &ccirc;iam armitaj per kanoj kalkulis al li kelkdekon da bastonoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        post ioma interkonsili&gcirc;o li revenis kaj parolis
        kun la alkurintaj sub<tld/>oj gestante al la oriento
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      En kelkaj armeoj, kaporalo estas jam sub<tld/>a rango (ekz. en
      Svislando).
      <aut>Antonio Codazzi</aut>
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_n;&c_t;&c_e;&c_r;-&c_a;&c_f;&c_ib;&c_c;&c_e;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_p;&c_a;&c_r;&c_sh;&c_ch;&c_y;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Unteroffizier</trd>
  <trdgrp lng="en">
    <trd>non-commissioned <ind>officer</ind></trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_r;&a_j;&a_h;&zwnj;&a_d;&a_A;&a_r;</trd>
  <trd lng="fr">sous-officier</trd>
  <trdgrp lng="he">
    <trd>&mem;&shin;"&qof;</trd>,
    <trd>&nun;&gimel;&dalet;<klr tip="ind"> (&bet;&tsadi;&bet;&alef;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tiszthelyettes</trd>,
    <trd>altiszt</trd>
  </trdgrp>
  <trd lng="it">sottufficiale</trd>
  <trd lng="ja">&#x4E0B;&#x58EB;&#x5B98;  <pr>&#x304B;&#x3057;&#x304B;&#x3093;</pr></trd>
  <trd lng="nl">onderofficier</trd>
  <trd lng="pl">podoficer</trd>
  <trd lng="pt">suboficial</trd>
  <trd lng="ro">subofi&#x21B;er</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_n;&c_t;&c_je;&c_r;-&c_o;&c_f;&c_i;&c_c;&c_je;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_p;&c_o;&c_r;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">underofficer</trd>
  <trd lng="sk">podd&ocirc;stojn&iacute;k</trd>
  <trd lng="cs">podd&uring;stojn&iacute;k</trd>
  <trd lng="uk">&#x443;&#x43D;&#x442;&#x435;&#x440;-&#x43E;&#x444;&#x456;&#x446;&#x435;&#x440;</trd>
  <trdgrp lng="zh">
    <trd>&#x58EB;&#x5B98; <pr>sh&#xEC;gu&#x101;n</pr></trd>,
    <trd>&#x519B;&#x58EB; <pr>j&#x16B;nsh&#xEC;</pr></trd>,
    <trd>&#x8ECD;&#x58EB; <pr>j&#x16B;nsh&#xEC;</pr></trd>,
    <trd>&#x4E2D;&#x58EB; <pr>zh&#x14D;ngsh&#xEC;</pr></trd>,
    <trd>&#x4E0B;&#x7D1A;&#x8ECD;&#x5B98; <pr>xi&#xE0;j&#xED;j&#x16B;ngu&#x101;n</pr></trd>,
    <trd>&#x4E0B;&#x7EA7;&#x519B;&#x5B98; <pr>xi&#xE0;j&#xED;j&#x16B;ngu&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>
