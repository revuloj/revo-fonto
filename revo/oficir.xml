<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>oficir</rad>/o
</kap>
<drv mrk="oficir.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Gcirc;enerala nomo de &ccirc;iuj armeaj estroj, ekde vicle&ubreve;tenanto, &gcirc;is mar&scirc;alo.
    </dif>
  </snc>
  <trd lng="de">Offizier</trd>
  <trd lng="nl">officier</trd>
  <trd lng="ru">&c_o;&c_f;&c_i;&c_c;&c_je;&c_r;</trd>
  <trd lng="sv">officer</trd>
</drv>

<drv mrk="oficir.sub0o">
  <kap>sub<tld/>o</kap>
  <snc>
    <dif>
      &Gcirc;enerala nomo de &ccirc;iuj armeaj estroj, ekde kaporalo &gcirc;is ekskluzive
      vicle&ubreve;tenanto.
    </dif>
  </snc>
  <trd lng="de">Unteroffizier</trd>
  <trd lng="nl">onderofficier</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_n;&c_t;&c_je;&c_r;-&c_o;&c_f;&c_i;&c_c;&c_je;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_p;&c_o;&c_r;&c_shch;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">underofficer</trd>
</drv>
</art>
