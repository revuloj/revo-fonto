<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rauxp.xml,v 1.23 2024/04/13 20:15:52 revo Exp $">
<kap>
  <ofc>*</ofc><rad>ra&ubreve;p</rad>/o
</kap>

<drv mrk="rauxp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rauxp.0o.ZOO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="larv.0o.ZOO">Larvo</ref> de la
      <ref tip="vid" cel="papili.0o">papilioj</ref> kaj de kelkaj aliaj flugilaj
      insektoj:
      <ekz>
        kio restis de la <tld/>oj, tion man&gcirc;is la akridoj
        <fnt>
          <bib>MT</bib>
          <lok>Joel 1:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur levkojo rampis <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la blukribrila <tld/>o atakis centojn da arboj, precipe fraksenojn
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag/pg</aut>
          <vrk><url ref="&Monato;/2006/008925.php">Kribrilo mortiga</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pup.0o.ZOO">pupo <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_s;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_g;&c_malmol;&c_s;&c_je;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="ca">eruga</trd>
  <trdgrp lng="de">
    <trd>Raupe</trd>,
  </trdgrp>
  <trd lng="en">caterpillar</trd>
  <trd lng="fr">chenille<klr tip="amb"> (zool.)</klr></trd>
  <trd lng="hu">herny&oacute;</trd>
  <trd lng="nl">rups</trd>
  <trdgrp lng="pl">
    <trd>g&aogonek;sienica</trd>,
    <trd>liszka</trd>
  </trdgrp>
  <trd lng="pt">lagarta</trd>
  <trd lng="ru">&c_g;&c_u;&c_s;&c_je;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="uk">&c_g;&c_u;&c_s;&c_je;&#x43D;&c_i;&c_c;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x6BDB;&#x6BDB;&#x866B; <pr>m&#xE1;omaoch&#xF3;ng</pr></trd>,
    <trd>&#x6BDB;&#x6BDB;&#x87F2; <pr>m&#xE1;omaoch&#xF3;ng</pr></trd>,
    <trd>&#x880B; <pr>zh&#xFA;</pr></trd>,
    <trd>&#x86D3; <pr>c&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rauxp.0veturilo">
  <kap>
    <tld/>a&jcirc;o, <var><kap><tld/>veturilo</kap></var>
  </kap>
  <snc mrk="rauxp.0veturilo.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Veturilo, kiu movi&gcirc;as pere de &ccirc;enoj ekipitaj per metalaj tabuloj:
      <ekz>
        knarantaj <tld/>veturiloj
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Horv&aacute;th</aut>
          <vrk><url ref="&Monato;/2006/009060.php">Tra infanaj okuloj: memoroj pri la eventoj de
          '56</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;tal<tld/>a&jcirc;o abrupte bremsis, haltis kaj radiofone oni haltigis anka&ubreve; la
        tutan kolonon
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Horv&aacute;th</aut>
          <vrk><url ref="&Monato;/2006/009060.php">Tra infanaj okuloj: memoroj pri la eventoj de
          '56</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="buldoz.0o">buldozo</ref>,
       <ref cel="tank.0o">tanko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_s;&c_je;&c_n;&c_ib;&c_ch;&c_n;&c_y; &c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Raupe <klr>(Fahrzeug)</klr></trd>,
    <trd>Kettenfahrzeug</trd>
  </trdgrp>
  <trd lng="en"> tracked vehicle</trd>
  <trd lng="pl">pojazd  <ind>g&aogonek;sienicowy</ind></trd>
  <trdgrp lng="zh">
    <trd>&#x5C65;&#x5E26;&#x8F66;&#x8F86; <pr>l&#x1DA;d&#xE0;ich&#x113;li&#xE0;ng</pr></trd>,
    <trd>&#x5C65;&#x5E36;&#x8ECA;&#x8F1B; <pr>l&#x1DA;d&#xE0;ich&#x113;li&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rauxp.silk0o">
  <kap>silk<tld/>o</kap>
  <snc mrk="rauxp.silk0o.ZOO">
    <uzo tip="fak">TEKS</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <tld lit="R"/>o de <ref tip="vid" cel="bombik.0o">bombikso</ref>,
      bredata por la kruda silko:
      <ekz>
        knabinoj por prepari sian doton devis scipovi kulturi silk<tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2008/009567.php">Japanio tra homoj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi iris al la <ctl>Instituto de la Silk<tld/>oj</ctl>, kie mi
        preparas la temon de mia disertacio
        <fnt><aut>Lina Gabrielli</aut>, <vrk>Karnavalo. - Romo:
        International Center of Arts, 1973</vrk>, <lok>p.
        48a</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/cc/Bombyx_mori_001.JPG/320px-Bombyx_mori_001.JPG" prm="CC BY-SA     3.0">silk<tld/>oj (Bombyx mori)
        <fnt>
          <aut>Ma&lstroke;gorzata Mi&lstroke;aszewska, propra verko</aut>
          <lok><url ref="&WCW;/File:Bombyx_mori_001.JPG">&WCW;/File:Bombyx_mori_001.JPG</url></lok>
        </fnt>
    </bld>
    <trd lng="ca">cuc de seda</trd>
    <trd lng="en">silkworm</trd>
    <trd lng="fr">vers &agrave; soie</trd>
    <trd lng="nl">zijderups</trd>
    <trd lng="pl">jedwabnik</trd>
    <trd lng="pt">bicho-da-seda</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_sh;&c_a;&c_w;&c_k;&c_a;&c_v;&c_ib;&c_ch;&c_n;&c_y; &c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;</trd>,
  <trd>&c_ja;&c_d;&c_v;&c_a;&c_b;&c_n;&c_ib;&c_k;</trd>
</trdgrp>
<trd lng="de">Seidenraupe</trd>
  <trd lng="uk">&c_g;&c_u;&c_s;&c_je;&#x43D;&c_i;&c_c;&#x44F; &c_sh;&#x43E;&c_v;&#x43A;&#x43E;&#x43F;&c_r;&#x44F;&c_d;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x87D3; <pr>xi&#xE0;ng</pr></trd>,
    <trd>&#x883A; <pr>c&#xE1;n</pr></trd>,
    <trd>&#x6851;&#x8695; <pr>s&#x101;ngc&#xE1;n</pr></trd>,
    <trd>&#x6851;&#x8836; <pr>s&#x101;ngc&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>
