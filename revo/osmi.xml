<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: osmi.xml,v 1.27 2024/01/30 16:09:26 revo Exp $">
<kap><rad>osmi</rad>/o <fnt>Z</fnt></kap>
<drv mrk="osmi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <fnt><bib>ZR</bib></fnt>
  <mlg kod="IUPAC">Os</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="76">elemento</ref> kun
      atomnumero 76, atompezo 190,2;
      tre malmola transir<ref tip="super" cel="metal.0o">metalo</ref>, 
      trovata en <ref tip="vid" cel="platen.0o">platenaj</ref>
      mina&jcirc;oj:
      <ekz>
        pulvoro de <tld/>o estas <klr>[&#x2026;]</klr> pro oksigeno de aero 
        <klr>[&#x2026;]</klr>transformi&gcirc;as en vapori&gcirc;eman kaj odoreman tetroksidon
        <frm><k>Os</k><k>O</k><sub>4</sub></frm>, kiu estas toksa
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=25717">Osmio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_o;&c_s;&c_mol;&c_m;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_o;&c_s;&c_m;&c_i;&c_j;</trd>
  <trd lng="br">osmiom</trd>
  <trd lng="cs">osmium</trd>
  <trd lng="de">Osmium</trd>
  <trd lng="el">&omikron_ton;&sigma;&my;&jota;&omikron;</trd>
  <trd lng="en">osmium</trd>
  <trd lng="es">osmio</trd>
  <trd lng="fr">osmium</trd>
  <trd lng="hu">ozmium</trd>
  <trd lng="id">osmium</trd>
  <trd lng="it">osmio</trd>
  <trd lng="ja">&#x30AA;&#x30B9;&#x30DF;&#x30A6;&#x30E0;</trd>
  <trd lng="la">Osmium</trd>
  <trd lng="nl">osmium</trd>
  <trd lng="pl">osm</trd>
  <trd lng="pt">&oacute;smio</trd>
  <trd lng="ru">&c_o;&c_s;&c_m;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Os</trd>,
    <trd>osmium</trd>
  </trdgrp>
  <trd lng="sv">osmium</trd>
  <trd lng="uk">&#x43E;&#x441;&#x43C;&#x456;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x92E8; <pr>&#xE9;</pr></trd>,
    <trd>&#x9507; <pr>&#xE9;</pr></trd>
  </trdgrp>
</drv>
</art>
