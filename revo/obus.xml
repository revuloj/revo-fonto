<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>obus</rad>/o
</kap>
<drv mrk="obus.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">MIL</uzo>
   <snc>
     <dif>
       Cilindroforma kuglego kun ogiva pinto, plenigita de 
eksploda&jcirc;o kaj
       kiun oni elpafas per kanono:
       <ekz>
         <tld/>o eksplodas tie, kie &gcirc;i falas.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="grenad.0o">grenado</ref>..
     </refgrp>
   </snc>
   <trd lng="be">&c_s;&c_n;&c_a;&c_r;&c_a;&c_d;</trd>
   <trd lng="de">Granate</trd>
   <trd lng="en">artillery shell</trd>
   <trd lng="fr">obus</trd>
   <trd lng="el">&omikron;&beta;&jota_ton;&delta;&alfa;</trd>
   <trdgrp lng="hu">
     <trd>t&uuml;z&eacute;rs&eacute;gi l&ouml;ved&eacute;k</trd>,
     <trd>gr&aacute;n&aacute;t</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>obus</trd>,
     <trd>houwitsergranaat</trd>
   </trdgrp>   
   <trd lng="pt">obus</trd>
   <trd lng="ru">&c_s;&c_n;&c_a;&c_r;&c_ja;&c_d;</trd>
</drv>
</art>
