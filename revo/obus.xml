<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: obus.xml,v 1.19 2022/03/24 15:12:20 revo Exp $">
<kap>
   <rad>obus</rad>/o
</kap>
<drv mrk="obus.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">MIL</uzo>
   <snc>
     <dif>
       Cilindroforma kuglego kun ogiva pinto, plenigita de 
eksploda&jcirc;o kaj
       kiun oni elpafas per kanono:
       <ekz>
         <tld/>o eksplodas tie, kie &gcirc;i falas.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="grenad.0o">grenado</ref>.
     </refgrp>
   </snc>
  <trd lng="be">&c_s;&c_n;&c_a;&c_r;&c_a;&c_d;</trd>
  <trd lng="ca">ob&uacute;s</trd>
  <trdgrp lng="cs">
    <trd>d&#x11B;lost&#x159;eleck&#xFD; gran&#xE1;t</trd>,
    <trd>d&#x11B;lov&#xE1; koule</trd>,
    <trd>d&#x11B;lov&#xE1; st&#x159;ela</trd>
  </trdgrp>
  <trd lng="de">Granate</trd>
  <trd lng="el">&omikron;&beta;&jota_ton;&delta;&alfa;</trd>
  <trd lng="en">artillery shell</trd>
  <trd lng="fa">&a_kh;&a_m;&f_p;&a_A;&a_r;&a_h;</trd>
  <trd lng="fr">obus</trd>
  <trdgrp lng="hu">
     <trd>t&uuml;z&eacute;rs&eacute;gi l&ouml;ved&eacute;k</trd>,
     <trd>gr&aacute;n&aacute;t</trd>
   </trdgrp>
  <trdgrp lng="it">
     <trd>proiettile<klr> da obice</klr></trd>,
     <trd>candelotto<klr> (da obice)</klr></trd>
   </trdgrp>
  <trdgrp lng="nl">
     <trd>obus</trd>,
     <trd>houwitsergranaat</trd>
   </trdgrp>
  <trd lng="pl">pocisk <klr>(artyleryjski)</klr></trd>
  <trd lng="pt">obus</trd>
  <trd lng="ru">&c_s;&c_n;&c_a;&c_r;&c_ja;&c_d;</trd>
  <trd lng="sk">delostreleck&#xFD; gran&#xE1;t</trd>
  <trd lng="uk">&#x441;&#x43D;&#x430;&#x440;&#x44F;&#x434; (&#x430;&#x440;&#x442;&#x438;&#x43B;&#x435;&#x440;&#x456;&#x439;&#x441;&#x44C;&#x43A;&#x438;&#x439;)</trd>
</drv>
</art>
