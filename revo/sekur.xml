    <dif>
      Trovi <tld/>an, stabilan kondi&ccirc;on:
      <ekz>
        ju pli ni laboros por glatigi nian lingvon kaj
        perfekti&gcirc;i pri &gcirc;ia
        uzado, des pli &gcirc;i firmi&gcirc;os kaj <tld/>i&gcirc;os.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_v;&c_a;&c_c;&c_mol; &c_s;&c_t;&c_a;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_je; &c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_ib;&c_sh;&c_ch;&c_a; <klr>(&c_s;&c_a;&c_b;&c_je;)</klr></trd>
