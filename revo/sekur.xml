<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>sekur</rad>/a
</kap>
<drv mrk="sekur.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Estanta en sendan&gcirc;era situacio:
      <ekz>
        <tld/>a rifu&gcirc;ejo;
      </ekz>
      <ekz>
        estas diferenco inter afero <tld/>a kaj afero spekulacia;
      </ekz>
      <ekz>
        &gcirc;ui senton de <tld/>eco;
      </ekz>
      <ekz>
        tio sufi&ccirc;os por da&ubreve;re <tld/>igi la vivon de la infano;
      </ekz>
      <ekz>
        ju pli ni laboros por glatigi nian lingvon kaj perfekti&gcirc;i pri &gcirc;ia
        uzado, des pli &gcirc;i firmi&gcirc;os kaj <tld/>i&gcirc;os.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">sicher</trd>
  <trd lng="en">safe</trd>
  <trd lng="nl">veilig</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_o;&c_p;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">s&auml;ker</trd>
</drv>

<drv mrk="sekur.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari ke io estas sekura.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>sichern</trd>,
     <trd>absichern</trd>,
     <trd>sch&uuml;tzen</trd>
  </trdgrp>
</drv>

</art>
