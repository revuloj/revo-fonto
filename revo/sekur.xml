<drv mrk="sekur.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="sekur.0eco.KOMUNE">
    <dif>
      Stato de iu a&ubreve; io ekster &ccirc;ia dan&gcirc;ero:
      <ekz>
        mil pretorianoj <klr>[&#x2026;]</klr> la&ubreve;longe de la vojo, gardis la trankvilecon kaj
        <tld/>econ de lia enveturo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la notoj donas al la oratoro la necesan <tld/>econ
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        postuloj pri kiel eble plej granda ekonomia egaleco kaj socia <tld/>eco
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        plena psika <tld/>eco
        <fnt>
          <bib>ChV</bib>
          <lok>11</lok>
        </fnt>;
      </ekz>
      <ekz>
        personeco, al kiu baza <tld/>eco mankis
        <fnt>
          <bib>ChV</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas nemalhavebla <klr>[&#x2026;]</klr>, kio donas al mi senton de profesia
        <tld/>eco
        <fnt>
          <bib>CKv</bib>
          <lok>5</lok>
        </fnt>;
      </ekz>
      <ekz>
        Ministrejo pri &Scirc;tata <tld lit="S"/>eco
        <fnt>
          <bib>ChR</bib>
          <lok>Ne nur tamtamado</lok>
        </fnt>;
      </ekz>
      <ekz>
        E&ubreve;ropa Konferenco pri <tld lit="S"/>eco kaj Kunlaboro
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Organiza&jcirc;o pri <tld lit="S"/>eco kaj Kunlaboro en E&ubreve;ropo
        <fnt><aut>Bardhyl Selimi</aut>:
          <vrk><url ref="&Monato;/2001/007205.html">Voja&gcirc;impresoj, somero 2000</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/01, p. 26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_s;&c_mol;&c_p;&c_je;&c_k;&c_a;</trd>
  <trd lng="bo">&t_nya;&t_drengbu;&t_na;&t_tsheg;&t_ga;&t_tsheg;&t_ma;&t_drengbu;&t_da;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_shabchu;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>bezpe&ccaron;nost</trd>,
    <trd>zabezpe&ccaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s&eacute;curit&eacute;</trd>,
    <trd>suret&eacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5B89;&#x5168;  <pr>&#x3042;&#x3093;&#x305C;&#x3093;</pr></trd>,
    <trd>&#x30BB;&#x30AD;&#x30E5;&#x30EA;&#x30C6;&#x30A3;</trd>,
    <trd>&#x5B89;&#x5168;&#x4FDD;&#x969C;  <pr>&#x3042;&#x3093;&#x305C;&#x3093;&#x307B;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x4FDD;&#x5B89;  <pr>&#x307B;&#x3046;&#x3042;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bezpiecze&nacute;stwo</trd>,
    <trd>pewno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="sk">bezpe&ccaron;nos&tcaron;</trd>
  <trd lng="uk">&c_b;&c_je;&c_z;&c_p;&c_je;&c_k;&c_a;</trd>
