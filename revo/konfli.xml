<drv mrk="konfli.0o">
  <kap><tld/>o</kap>
  <snc mrk="konfli.0o.interhoma">
    <dif>
      Mal<ref tip="ant" cel="konsen.0o.akordo">konsento</ref> inter homoj, ka&ubreve;zita de
      kontra&ubreve;eco de iliaj deziroj a&ubreve; opinioj:
      <ekz>
        prediki <klr>[&#x2026;]</klr> meze de granda <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>I. Tesalonikanoj 2:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tiuj politikaj principoj estas]</klr> konstanta
        ka&ubreve;zo de neniam fini&gcirc;ontaj intergentaj <tld/>oj
        <fnt><bib>Homaranismo</bib><lok/></fnt>;
      </ekz>
      <ekz>
        mi tre timas <tld/>on inter la re&gcirc;o kaj la
        &ccirc;efpastroj
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 14</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>(ka&ubreve;zita)</klr> de interesoj, pasioj;
      </ekz>
      <ekz>
        <tld/>o kun la propra konscienco<fnt>B</fnt>
        <klr>(kp <ref cel="konfli.anim0o">anim<tld/>o</ref>)</klr>;
      </ekz>
      <ekz>
        la instruitaj personoj jam ofte toleras <klr>[&#x2026;]</klr> malsamajn kredojn de
        aliaj inteligentuloj, sed pri kutimoj la <tld/>o estas multe pli grava
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>armita <tld/>o</ind> inter la opozicio kaj la registaro
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/011072.php">Hejmen la blukaskuloj</url></vrk>
          <lok>2013</lok>
        </fnt>;
        <trd lng="de">bewaffnete Auseinandersetzung</trd>
        <trdgrp lng="fr">
          <trd><ind>conflit</ind> arm&eacute;</trd>
          <trd>guerre</trd>
        </trdgrp>
      </ekz>
      <ekz>
        oni akuzis <nom>Collins</nom> pri <ind>intereskonflikto</ind>: &scirc;i kunsidis kun &ccirc;efoj de
        kompanio, kiu
        donacis milojn da dolaroj
        <fnt>
          <bib>Monato</bib>
          <aut>D. E. Rogers</aut>
          <vrk><url ref="&Monato;/2014/011388.php">Momento de vero en naciaj
          elektoj</url></vrk>
          <lok>2014</lok>
        </fnt>;
        <trd lng="de">Interessenskonflikt</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>veni en <tld/>on</ind>
        <klr>(<ref cel="konfli.ek0i">ek<tld/>i</ref>)</klr>.
        <trdgrp lng="fr">
          <trd>se prendre de <ind>querelle</ind></trd>,
          <trd>entrer en <ind>conflit</ind></trd>
        </trdgrp>
        <trd lng="nl">in <ind>conflict</ind> geraken</trd>
        <trd lng="pl">wej&sacute;&cacute; w sp&oacute;r</trd>
        <trd lng="ro">intr&abreve; &icirc;n controvers&abreve;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="antago.0o">antagonismo</ref>,
      <ref cel="akord.mal0o">malakordo</ref>,
      <ref cel="batal.0o">batalo</ref>,
      <ref cel="kolizi.0o">kolizio</ref>,
      <ref cel="kverel.0o">kverelo</ref>,
      <ref cel="milit.0o">milito</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Streit</trd>,
      <trd>Widerstreit</trd>,
      <trd>Auseinandersetzung</trd>,
      <trd>Zwist</trd>
    </trdgrp><trd lng="th">&#xE04;&#xE27;&#xE32;&#xE21;&#xE02;&#xE31;&#xE14;&#xE41;&#xE22;&#xE49;&#xE07;</trd>
  </snc>
  <snc mrk="konfli.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne<ref tip="ant" cel="kongru.0eco">kongrueco</ref>,
      <ref tip="sin" cel="kontraux.0eco">kontra&ubreve;eco</ref>:
      <ekz>
        la filmo <nom>Guasha</nom> prenis tiun terapion kiel la &ccirc;efan
        intrigon <klr>[...]</klr> por montri la <tld/>on de la &ccirc;ina kaj
        usona kulturoj
        <fnt>
          <vrk>&Ccirc;ina filmo GUASHA kaj la &ccirc;ina tradicia
          skrap-terapio</vrk>
          <lok><url ref="http://es.chinabroadcast.cn/1/2003/12/11/1@1125.htm">&Ccirc;ina Radio
          Internacia</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <klr>[&#x2026;]</klr> elportis grandan <tld/>on de suferoj
        <klr>(kp <ref cel="konfli.anim0o">anim<tld/>o</ref>)</klr>
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 10:32</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Widerspruch</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_f;&c_l;&c_ib;&c_k;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>konflikt</trd>,
    <trd>p&rcaron;e</trd>,
    <trd>rozpor</trd>,
    <trd>roztr&zcaron;ka</trd>,
    <trd>spor</trd>,
    <trd>st&rcaron;etnut&iacute;</trd>,
    <trd>sv&aacute;r</trd>
  </trdgrp>
  <trd lng="de">Konflikt</trd>
  <trdgrp lng="en">
    <trd>conflict</trd>,
    <trd>antagonism</trd>,
    <trd>clash</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>conflit</trd>,
    <trd>antagonisme</trd>,
    <trd>contentieux<klr tip="ind"> (subst.)</klr></trd>,
    <trd>d&eacute;saccord</trd>,
    <trd>diff&eacute;rend</trd>,
    <trd>dispute</trd>,
    <trd>litige</trd>,
    <trd>lutte</trd>,
    <trd>opposition</trd>
    <trd>querelle</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8ECB;&#x8F62;  <pr>&#x3042;&#x3064;&#x308C;&#x304D;</pr></trd>,
    <trd>&#x845B;&#x85E4;  <pr>&#x3064;&#x3065;&#x3089;&#x3075;&#x3058;</pr></trd>,
    <trd>&#x4E89;&#x3044;  <pr>&#x3042;&#x3089;&#x305D;&#x3044;</pr></trd>,
    <trd>&#x885D;&#x7A81;  <pr>&#x3057;&#x3087;&#x3046;&#x3068;&#x3064;</pr></trd>,
    <trd>&#x4E89;&#x8B70;  <pr>&#x305D;&#x3046;&#x304E;</pr></trd>,
    <trd>&#x7D1B;&#x4E89;  <pr>&#x3075;&#x3093;&#x305D;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">conflict</trd>
  <trdgrp lng="pl">
    <trd>afera</trd>,
    <trd>burda</trd>,
    <trd>konflikt</trd>,
    <trd>sp&oacute;r</trd>,
    <trd>zatarg</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>conflito</trd>,
    <trd>antagonismo</trd>,
    <trd>oposi&ccedil;&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>afacere</trd>,
    <trd>ceart&abreve;</trd>,
    <trd>conflict</trd>,
    <trd>controvers&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_f;&c_l;&c_i;&c_k;&c_t;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>konflikt</trd>,
    <trd>nezhoda</trd>,
    <trd>rozpor</trd>,
    <trd>spor</trd>
  </trdgrp>
  <trd lng="tok">utala</trd>
  <trdgrp lng="zh">
    <trd>&#x4E89;&#x7AEF; <pr>zh&#x113;ngdu&#x101;n</pr></trd>,
    <trd>&#x51B2;&#x7A81; <pr>ch&#x14D;ngt&#x16B;</pr></trd>,
    <trd>&#x8F47; <pr>ji&#x16B;</pr></trd>,
    <trd>&#x77DB;&#x76FE; <pr>m&#xE1;od&#xF9;n</pr></trd>,
    <trd>&#x6597;&#x4E89; <pr>d&#xF2;uzh&#x113;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konfli.0a">
  <kap><tld/>a</kap>
  <snc mrk="konfli.0a.0orilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la transdono de Heroldo de Esperanto <klr>[...]</klr> okazis
        en <tld/>a etoso inter <nom>LF-koop</nom> kaj la Societo
        &leftquot;Heroldo de Esperanto&rightquot;
        <fnt>
        <vrk>Heroldo de Esperanto</vrk>
        <lok><url ref="&Viki;/Heroldo_de_Esperanto">Vikipedio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ilia devojigo por egoistaj celoj estigas konstantan <tld/>an
        klimaton
        <fnt>
        <vrk>Ekzistokialo kaj specifeco de la SAT-movado</vrk>
        <lok><url ref="http://satenhispanio.eresmas.com/SATeo.htm">SATEH</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        se <tld/>antaj partioj ne sukcesas interkonsenti, <tld/>a
        komisiono
        priju&gcirc;as kaj findecidas
        <fnt>
        <vrk>IFEF: Statuto kaj regularoj</vrk>
        <lok><url ref="http://ifef.free.fr/ifefstatut.htm">http://ifef.free.fr/ifefstatut.htm</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="nl">conflict-</trd>
  </snc>
  <snc mrk="konfli.0a.0anta">
    <dif>
      Estanta en <ref cel="konfli.0o.FIG"><tld/>o <sncref/></ref>,
      <ref cel="konfli.0i.FIG"><tld/>anta <sncref/></ref>:
      <ekz>
        <tld/>aj interesoj
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 10</lok></fnt>;
      </ekz>
      <ekz>
        lia kapo estis plena de maltrankvilaj kaj <tld/>aj pensoj
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 15</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la interhoman komunikadon]</klr> stiras distiraj,
        kontra&ubreve;aj kaj <tld/>aj fortoj
        <fnt>
        <aut>K. Schubert</aut>
        <vrk>Optimumigo de la internacia komunikado: interlingvistika
        rigardo
        al la nova industria lingvokonstruado</vrk>
        <lok><url ref="http://esperantic.org/librejo/dbstudoj/12_Schubert.htm">Studoj
pri interlingvistiko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;das <tld/>ajn onidirojn pri la Nazaretana sekto
        <fnt>
        <aut>A. Kvinke</aut>
        <vrk>La aristarka raporto de Albisturo Kvinke</vrk>
        <lok><url ref="&Monato;/2003/008041.html">Monato</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kontraux.0a.KOMUNE">kontra&ubreve;a</ref>,
      <ref cel="mal">mala</ref>,
      <ref cel="akord.mal0a">malakorda</ref>,
      <ref cel="kongru">nekongrua</ref>
    </refgrp>
    <trdgrp lng="nl">
      <trd>conflicterend</trd>,
      <trd>confligerend</trd>
    </trdgrp>
  </snc>
  <snc mrk="konfli.0a.0oveka">
    <dif>
      <klr>(p.p. opinio, ago k.s.)</klr>
      Vekanta <tld/>ojn:
      <ekz>
        tro da talentaj homoj turnis sin for de la temporaba
        kaj ofte <tld/>a organiza laboro je monda skalo
        <fnt>
        <aut>M. Fettes</aut>
        <vrk>Ni vidas la fruktojn de multjara neglektado</vrk>
        <lok><url ref="http://www.algonet.se/~kallekn/folio/fettes.html">Libera
Folio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      &Ccirc;i-sence kelkaj uzas <ctl><tld/>ema</ctl>, kiu tamen pli
      bone
      apliki&gcirc;as al personoj avidaj partopreni en <tld/>oj,
      eventuale ilin kreinte. Se ial <ctl><tld/>a</ctl> aspektas
      ambigua, pli bone estus uzi la tute klarajn
      <ctl><tld/>oveka</ctl>, <ctl><tld/>osprona</ctl>
      a&ubreve; <ctl><tld/>instiga</ctl>.
    </rim>
     <trdgrp lng="nl">
      <trd>conflicterend</trd>,
      <trd>confligerend</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>de <ind>conflit</ind></trd>,
    <trd>conflictuel</trd>,
    <trd>contraire<klr tip="ind"> (adj.)</klr></trd>,
    <trd>contradictoire</trd>,
    <trd>divergent</trd>,
    <trd>oppos&eacute;</trd>
  </trdgrp>
  <trd lng="pl">konfliktowy</trd>
  <trd lng="ro">conflictual</trd>
</drv>

<drv mrk="konfli.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti en <tld/>o:
    </dif>
    <subsnc ref="konfli.0o.interhoma" mrk="konfli.0i.interhome">
      <ekz>
        li jam <tld/>is kun partiestroj en la propra partio
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2010/010011.php">Malnova problemo pri maljunaj homoj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        persone mi ne <tld/>is kun iu, nek estas io ajn persona
        en la fono de mia demisio
        <fnt>
        <aut>N. Ra&scaron;i&cacute;</aut>
        <vrk>&leftquot;La komitato ne montris sufi&ccirc;e da respondeco&rightquot;</vrk>
        <lok><url ref="http://www.liberafolio.org/UEA/Demisioj/Document.2004-02-21.9558698698/">Libera
        Folio</url></lok>
        </fnt>.
      </ekz>
      <refgrp tip="vid">
        <ref cel="batal.0i">batali</ref>,
        <ref cel="kverel.0i">kvereli</ref>,
        <ref cel="milit.0i">militi</ref>
      </refgrp>
      <trdgrp lng="de">
        <trd>einen <ind>Konflikt</ind> austragen</trd>,
        <trd>streiten</trd>,
        <trd>im <ind>Widerstreit</ind> sein</trd>
      </trdgrp>
    </subsnc>
    <subsnc ref="konfli.0o.FIG" mrk="konfli.0i.FIG">
      <ekz>
        <tld/>i a&ubreve; mal<tld/>i kun la ekzistanta kaj portempe valida pun-juro estas jura
        afero, ne morala
        <fnt>
          <aut>Reinhard Haupenthal</aut>
          <vrk><url ref="&Monato;/2000/006848.php">Mallogika kaj senracia</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 4</lok>
          </fnt>;
      </ekz>
      <ekz>
        <klr>[la silaba analizo]</klr> povas iam kaj iam <tld/>i kun
        la limoj de envortaj radikoj
        <fnt>
        <aut>Pejno S.</aut>
        <vrk>Dividaj dilemoj</vrk>
        <lok><url ref="http://ourworld.compuserve.com/homepages/profcon/e_divid.htm">http://ourworld.compuserve.com/homepages/profcon/e_divid.htm</url></lok>
        </fnt>.
      </ekz>
    <trdgrp lng="de">
      <trd>im <ind>Konflikt</ind> stehen</trd>,
      <trd>sich <ind>widersprechen</ind></trd>
    </trdgrp></subsnc>
    <refgrp tip="vid">
      <ref cel="akord.mal0i">malakordi</ref>,
      <ref cel="kontraux.0i">kontra&ubreve;i</ref>,
      <ref cel="dir.kontraux0i">kontra&ubreve;diri</ref>,
      <ref cel="kolizi.0i">kolizii</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_f;&c_l;&c_ib;&c_k;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t ve sporu</trd>,
    <trd>odporovat</trd>,
    <trd>znesv&aacute;&rcaron;it se</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conflict</trd>,
    <trd>clash</trd>,
    <trd>be at <ind>odds</ind> with</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre en <ind>conflit</ind></trd>,
    <trd>&ecirc;tre en <ind>contradiction</ind></trd>,
    <trd>&ecirc;tre en <ind>d&eacute;saccord</ind></trd>,
    <trd>avoir un <ind>contentieux</ind></trd>,
    <trd>avoir un <ind>diff&eacute;rend</ind></trd>,
    <trd>avoir une <ind>dispute</ind></trd>,
    <trd>avoir un <ind>litige</ind></trd>,
    <trd>se <ind>quereller</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8ECB;&#x8F62;&#x3092;&#x751F;&#x3058;&#x3066;&#x3044;&#x308B;  <pr>&#x3042;&#x3064;&#x308C;&#x304D;&#x3092;&#x3057;&#x3087;&#x3046;&#x3058;&#x3066;&#x3044;&#x308B;</pr></trd>,
    <trd>&#x7D1B;&#x4E89;&#x304C;&#x3042;&#x308B;  <pr>&#x3075;&#x3093;&#x305D;&#x3046;&#x304C;&#x3042;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>conflicteren</trd>,
    <trd>in <ind>conflict</ind> zijn</trd>,
    <trd>confligeren</trd>
  </trdgrp>
  <trd lng="pl">k&lstroke;&oacute;ci&cacute; si&eogonek;</trd>
  <trd lng="ro">certa</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_f;&c_l;&c_i;&c_k;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_o;&c_j;&c_t;&c_i; &c_v;
&c_k;&c_o;&c_n;&c_f;&c_l;&c_i;&c_k;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>by&tcaron; v&nbsp;rozpore</trd>,
    <trd>konflikte</trd>
  </trdgrp>
  <trd lng="th">&#xE02;&#xE31;&#xE14;&#xE41;&#xE22;&#xE49;&#xE07;</trd>
  <trdgrp lng="zh">
    <trd>&#x62B5;&#x89E6; <pr>d&#x1D0;ch&#xF9;</pr></trd>,
    <trd>&#x62B5;&#x727E; <pr>d&#x1D0;w&#x1D4;</pr></trd>,
    <trd>&#x4E89;&#x7AEF; <pr>zh&#x113;ngdu&#x101;n</pr></trd>,
    <trd>&#x53CD;&#x76EE; <pr>f&#x1CE;nm&#xF9;</pr></trd>,
    <trd>&#x5435;&#x67B6; <pr>ch&#x1CE;oji&#xE0;</pr></trd>,
    <trd>&#x5E72;&#x67B6; <pr>g&#xE0;nji&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konfli.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="konfli.ek0i.komenci">
    <dif>
      Komenci <tld/>i:
      <ekz>
        tiu ideo povas ek<tld/>i kun la individua libereco
        <fnt>
          <vrk><url ref="https://anarkiismokajesperanto.wordpress.com/">Nek Dio Nek
          Mastro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        prefereblas fakte ek<tld/>i kun homoj neinteligentaj kaj senkonsideraj
        <klr>[&#x2026;]</klr> ol kun Dio mem
        <fnt>
          <vrk><url ref="http://www.ueci.it/eldonoj/verkoj/letero.htm">Letero al la
          Korintanoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
      veni en <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">dostat se do konfliktu</trd>
  <trdgrp lng="fr">
    <trd>se prendre de <ind>querelle</ind></trd>,
    <trd>entrer en <ind>conflit</ind></trd>
  </trdgrp>
