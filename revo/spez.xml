  <trd lng="de">verschwenderisch</trd>
  <trd lng="fr">d&eacute;pensier</trd>
  <trd lng="nl">verkwistend</trd>
  <trd lng="sk">rozhadzova&ccaron;n&yacute;</trd>
</drv>

