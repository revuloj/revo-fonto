          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxpar.mal0ema">mal&scirc;parema</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_a;&c_sh;&c_ch;&c_a;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_ib;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_n;&c_a;&c_t;&c_r;&c_a;&c_w;&c_n;&c_y;</trd>
