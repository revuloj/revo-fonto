<drv mrk="spez.en0o">
  <kap>en<tld/>o</kap>
  <snc mrk="spez.en0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Enkasigo de mono:
      <ekz>
        en<tld/>o el la vinpremejo
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 18:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        komercen<tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 23:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        redonu al &scirc;i <klr>[&#8230;]</klr> &ccirc;iujn en<tld/>ojn de la kampo
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 8:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        per la en<tld/>oj de sia mano &scirc;i plantas vinber&gcirc;ardenon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 31:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        per &ccirc;i tiu metio ni havas bonajn en<tld/>ojn
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 19:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        plekti fi&scirc;kaptajn retojn <klr>[&#8230;]</klr> donis bonan laboren<tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia praavo havis 100 mil talentojn da jara en<tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia edzo havis grandajn en<tld/>ojn kaj lasis al &scirc;i grandan havon
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivi de estontaj en<tld/>oj
        <klr>(per <ref cel="prunt.0o.pruntajxo">prunto</ref>)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        tro da en<tld/>o ne estas tro da pezo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ri&ccirc;igas ne en<tld/>o, sed prudenta el<tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_h;&c_o;&c_d;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;
    <klr tip="amb">(&c_g;&c_r;&c_o;&c_sh;&c_a;&c_j;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>ingr&eacute;s</trd>,
    <trd>guany</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>po&zcaron;itek</trd>,
    <trd>p&rcaron;&iacute;jem</trd>,
    <trd>p&rcaron;&iacute;jem pen&ecaron;z</trd>,
    <trd>tr&zcaron;ba</trd>,
    <trd>v&yacute;nos</trd>,
    <trd>v&yacute;t&ecaron;&zcaron;ek</trd>
  </trdgrp>
  <trd lng="de"><klr>(finanzielle)</klr> Einnahme</trd>
  <trd lng="en">income</trd>
  <trd lng="es">ingreso<klr>(dinero)</klr></trd>
  <trdgrp lng="fr">
    <trd>entr&eacute;e<klr> (d'argent)</klr></trd>,
    <trd>recette</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bev&eacute;tel</trd>,
    <trd>j&ouml;vedelem</trd>,
    <trd>kereset</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>dapat</baz>pendapatan</trd>,
    <trd><baz>hasil</baz>penghasilan</trd>,
    <trd><baz>masuk</baz>pemasukan</trd>,
    <trd><baz>oleh</baz>perolehan</trd>,
    <trd>rezeki</trd>,
    <trd><baz>terima</baz>penerimaan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21454;&#20837;  <pr>&#12375;&#12421;&#12358;&#12395;&#12421;&#12358;</pr></trd>,
    <trd>&#20837;&#37329;  <pr>&#12395;&#12421;&#12358;&#12365;&#12435;</pr></trd>
