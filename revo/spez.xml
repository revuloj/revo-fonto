<drv mrk="spez.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="spez.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Enkasigo a&ubreve; elkasigo de <ref cel="mon.0o">mono</ref>:
      <ekz>
        li sola decidis pri la bu&gcirc;eto, la <tld/>oj ktp
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter Nov-Jorko kaj Germanujo estas komercaj komuniki&gcirc;oj, kiuj prezentas la
        <tld/>on de centoj da milionoj markoj &ccirc;iujare
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la eksterkomerca <tld/>o de Ruslando atingis 27,2 miliardojn da usonaj dolaroj
        <fnt>
          <aut>div.</aut>
          <vrk><url ref="&Monato;/1993/A931002.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/10, p. 4</lok>
         </fnt>;
      </ekz>
      <ekz>
        multaj el tiuj, kiuj faris la decidojn, ne vere komprenis la
        internaciajn <tld/>ojn
        <fnt>
          <aut>K. Kniivil&auml;</aut>
          <vrk>Homoj de Putin</vrk>
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_o;&c_t; <klr
  tip="amb">(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_y;)</klr></trd>
  <trdgrp lng="ca">
    <trd>moviment</trd>,
    <trd>transacci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pen&ecaron;&zcaron;n&iacute; obrat</trd>,
    <trd>pen&ecaron;&zcaron;n&iacute; p&rcaron;evod</trd>
  </trdgrp>
  <trd lng="de">Umsatz</trd>
  <trd lng="en">turnover</trd>
  <trd lng="es">transacci&oacute;n <klr tip="amb">(de dinero o
  bienes)</klr></trd>
  <trd lng="fr">transaction financi&egrave;re</trd>
  <trd lng="hu">p&eacute;nzforgalom</trd>
  <trdgrp lng="id">
    <trd>jual beli</trd>,
    <trd>transaksi</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20986;&#32013;  <pr>&#12377;&#12356;&#12392;&#12358;</pr></trd>,
    <trd>&#21454;&#25903;  <pr>&#12375;&#12421;&#12358;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">omzet</trd>
  <trd lng="pl">obr&oacute;t pieni&eogonek;&zdot;ny</trd>
  <trd lng="pt">transa&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;
  <ind>&c_o;&c_b;&c_o;&c_r;&c_o;&c_t;</ind></trd>
  <trd lng="sk">pe&ncaron;a&zcaron;n&yacute; obrat</trd>
  <trd lng="uk">&c_g;&c_r;&c_o;&c_sh;&c_o;&c_v;&c_i;&c_j; &c_o;&c_b;&c_o;&c_r;&c_o;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#29151;&#26989;&#38989; <pr>y&iacute;ngy&egrave;&eacute;</pr></trd>,
    <trd>&#33829;&#19994;&#39069; <pr>y&iacute;ngy&egrave;&eacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="spez.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Enkasigi a&ubreve; elkasigi:
      <ekz>
        &gcirc;i estas grandvalora monero, per kiu oni povas majstre brokanti, se oni scias
        bone &gcirc;in <tld/>i
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mono jam estis el <tld/>ita por aliaj a&jcirc;oj, kiuj &scirc;ajnis same gravaj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 41</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">umsetzen</trd>
  <trdgrp lng="id">
    <trd><baz>jual beli</baz>berjual beli</trd>,
    <trd><baz>jual beli</baz>menjualbelikan</trd>,
    <trd><baz>jual beli</baz>memperjualbelikan</trd>,
    <trd><baz>transaksi</baz>bertransaksi</trd>,
    <trd><baz>transaksi</baz>mentransaksikan</trd>
  </trdgrp>
  <trd lng="ja">&#20986;&#32013;&#12377;&#12427;  <pr>&#12377;&#12356;&#12392;&#12358;&#12377;&#12427;</pr></trd>
  <trd lng="pl">przelewa&cacute;  <ind>sum&eogonek;</ind></trd>
  <trd lng="pt">transacionar</trd>
</drv>

