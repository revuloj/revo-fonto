<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>spez</rad>/o
</kap>
<uzo tip="fak">EKON</uzo>
<drv mrk="spez.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Enkasigo kaj elkasigo de mono.
    </dif>
  </snc>
  <trd lng="de">Umsatz</trd>
  <trd lng="en">turnover</trd>
  <trd lng="es">transacci&oacute;n</trd>
  <trd lng="nl">omzet</trd>
  <trd lng="ru">&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j; <ind>&c_o;&c_b;&c_o;&c_r;&c_o;&c_t;</ind></trd>
</drv>

<drv mrk="spez.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Enkasigi; ricevi monon por liverita varo a&ubreve; servo:
      <ekz>
        ri&ccirc;igas ne en<tld/>o, sed prudenta el<tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>einnehmen</trd>,
    <trd>umsetzen</trd>,
    <trd>verdienen</trd>
  </trdgrp>
  <trd lng="nl">verdienen</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_ch;&c_a;&c_t;&c_mol; <ind>&c_d;&c_o;&c_h;&c_o;&c_d;</ind></trd>
</drv>

<drv mrk="spez.en0o">
  <kap>en<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Enkasigita mono.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Einnahme</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_o;&c_h;&c_o;&c_d;</trd>
</drv>

<drv mrk="spez.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Pagi monon por ricevita varo a&ubreve; servo:
      <ekz>
        mi devis el<tld/>i mil markojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tio estas vana el<tld/>o de energio kaj tempo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>ausgeben</trd>,
    <trd>aufwenden</trd>
  </trdgrp>
  <trd lng="es">gastar</trd>
  <trd lng="nl">uitgeven</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_h;&c_o;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="spez.el0o">
  <kap>el<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Elspezita mono.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Ausgabe</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_h;&c_o;&c_d;</trd>
</drv>

</art>
