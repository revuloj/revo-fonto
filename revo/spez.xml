  <trd lng="gu">&#xA96;&#xAB0;&#xACD;&#xA9A;</trd>
  <trd lng="ha">kashe</trd>
  <trd lng="hi">&#2326;&#2352;&#x94D;&#x91A;</trd>
  <trd lng="hmn">siv</trd>
  <trd lng="hr">potro&scaron;iti</trd>
  <trd lng="ht">depanse</trd>
