<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>spez</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="spez.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="spez.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Enkasigo a&ubreve; elkasigo de mono.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_o;&c_t; <klr tip="amb">(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_y;)</klr></trd>
  <trdgrp lng="ca">
    <trd>moviment</trd>,
    <trd>transacci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>peněžní obrat</trd>,
    <trd>peněžní převod</trd>
  </trdgrp>
  <trd lng="de">Umsatz</trd>
  <trd lng="en">turnover</trd>
  <trd lng="es">transacci&oacute;n <klr tip="amb">(de dinero o
  bienes)</klr></trd>
  <trd lng="fr">transaction</trd>
  <trd lng="hu">p&eacute;nzforgalom</trd>
  <trd lng="nl">omzet</trd>
  <trd lng="ru">&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j; <ind>&c_o;&c_b;&c_o;&c_r;&c_o;&c_t;</ind></trd>
  <trd lng="sk">peňažný obrat</trd>
</drv>

<drv mrk="spez.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="spez.en0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Enkasigi; ricevi monon por liverita varo a&ubreve; servo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <ind>&c_d;&c_a;&c_h;&c_o;&c_d;</ind></trd>,
    <trd>&c_a;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol; <ind>&c_g;&c_r;&c_o;&c_sh;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>percebre</trd>,
    <trd>guanyar</trd>
  </trdgrp>
  <trd lng="cs">mít příjem</trd>
  <trdgrp lng="de">
    <trd>einnehmen</trd>,
    <trd>umsetzen</trd>,
    <trd>verdienen</trd>
  </trdgrp>
  <trd lng="en">receive <klr>(money)</klr></trd>
  <trd lng="es">recibir <klr>(dinero)</klr></trd>
  <trdgrp lng="fr">
    <trd>gagner<klr> (de l'argent)</klr></trd>,
    <trd>entrer<klr> (de l'argent)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>beszed <klr tip="ind">(p&eacute;nzt)</klr></trd>,
    <trd>bev&eacute;telez <klr tip="ind">(p&eacute;nzt)</klr></trd>,
    <trd>inkassz&aacute;l</trd>,
    <trd>keres <klr tip="ind">(p&eacute;nzt)</klr></trd>,
    <trd>felvesz <klr tip="ind">(p&eacute;nzt)</klr></trd>
  </trdgrp>
  <trd lng="nl">verdienen</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_ch;&c_a;&c_t;&c_mol; <ind>&c_d;&c_o;&c_h;&c_o;&c_d;</ind></trd>
  <trd lng="sk">prijať peniaze</trd>
</drv>

<drv mrk="spez.en0o">
  <kap>en<tld/>o</kap>
  <snc mrk="spez.en0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Enkasigo de mono:
      <ekz>
        ri&ccirc;igas ne en<tld/>o, sed prudenta el<tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_h;&c_o;&c_d;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr
    tip="amb">(&c_g;&c_r;&c_o;&c_sh;&c_a;&c_j;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>ingr&eacute;s</trd>,
    <trd>guany</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>požitek</trd>,
    <trd>příjem</trd>,
    <trd>příjem peněz</trd>,
    <trd>tržba</trd>,
    <trd>výnos</trd>,
    <trd>výtěžek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Einnahme</trd>
  </trdgrp>
  <trd lng="en">income</trd>
  <trd lng="es">ingreso<klr>(dinero)</klr></trd>
  <trdgrp lng="fr">
    <trd>entr&eacute;e<klr> (d'argent)</klr></trd>,
    <trd>recette</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bev&eacute;tel</trd>,
    <trd>j&ouml;vedelem</trd>,
    <trd>kereset</trd>
  </trdgrp>
  <trd lng="nl">inkomsten</trd>
  <trd lng="ru">&c_d;&c_o;&c_h;&c_o;&c_d;</trd>
  <trd lng="sk">príjem</trd>
</drv>

<drv mrk="spez.el0ema">
  <kap>el<tld/>ema</kap>
  <snc mrk="spez.el0ema.KOMUNE">
    <dif>
      Kiu facile el<tld/>as, a&ccirc;etas,
      <ref tip="vid" cel="sxpar.mal0i.KOMUNE">mal&scirc;paras</ref>:
      <ekz>
        la sinjorino estis el<tld/>ema, kaj &ccirc;iun duan a&ubreve; 
        trian tagon &scirc;i iris straten por fari a&ccirc;etojn
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url
          ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>marnotratný</trd>,
    <trd>rozhazovačný</trd>,
    <trd>utrácivý</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;pensier</trd>
  <trd lng="sk">rozhadzovačný</trd>
</drv>

<drv mrk="spez.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="spez.el0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Pagi monon por ricevita varo a&ubreve; servo:
      <ekz>
        mi devis el<tld/>i mil markojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_t;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_h;&c_o;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">gastar</trd>
  <trdgrp lng="cs">
    <trd>utratit</trd>,
    <trd>utrácet</trd>,
    <trd>vydávat</trd>,
    <trd>vynakládat</trd>,
    <trd>zaplatit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausgeben</trd>,
    <trd>aufwenden</trd>
  </trdgrp>
  <trd lng="en">spend</trd>
  <trd lng="es">gastar</trd>
  <trd lng="fr">d&eacute;penser<klr> (de l'argent)</klr></trd>
  <trdgrp lng="hu">
    <trd>kiad <klr tip="ind">(p&eacute;nzt)</klr></trd>,
    <trd>kifizet</trd>,
    <trd>r&aacute;ford&iacute;t</trd>,
    <trd>k&ouml;lt <klr tip="ind">(p&eacute;nzt)</klr></trd>
  </trdgrp>
  <trd lng="nl">uitgeven</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_h;&c_o;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>míňať</trd>,
    <trd>vydať peniaze</trd>
  </trdgrp>
</drv>

<drv mrk="spez.el0o">
  <kap>el<tld/>o</kap>
  <snc mrk="spez.el0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Elspezado de mono:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tio estas vana el<tld/>o de energio kaj tempo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_d;&c_a;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_h;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="ca">despesa</trd>
  <trdgrp lng="cs">
    <trd>vydání</trd>,
    <trd>výdaj</trd>,
    <trd>výdaje</trd>,
    <trd>výlohy</trd>,
    <trd>útrata</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ausgabe</trd>
  </trdgrp>
  <trd lng="en">expenditure</trd>
  <trd lng="es">gasto</trd>
  <trdgrp lng="fr">
    <trd>frais<klr> (d&eacute;pense)</klr></trd>,
    <trd>d&eacute;pense</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kiad&aacute;s <klr tip="ind">(p&eacute;nz&eacute;)</klr></trd>,
    <trd>r&aacute;ford&iacute;t&aacute;s</trd>,
    <trd>k&ouml;lts&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">uitgaven</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_h;&c_o;&c_d;</trd>
  <trd lng="sk">výdavok</trd>
</drv>

<drv mrk="spez.vojel0o">
  <kap>vojel<tld/>o</kap>
  <snc mrk="spez.vojel0o.EKON">
    <dif>
      El<tld/>o pro diversaj dumvoja&gcirc;aj pagoj:
      <ekz>
        por viaj vojel<tld/>oj tie &ccirc;i estas kvar rubloj
        <fnt><aut>I. G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>[1898?]</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">despeses de viatge</trd>
  <trd lng="en">travel costs</trd>
  <trd lng="es">gastos de viaje</trd>
  <trdgrp lng="fr">
    <trd>frais de d&eacute;placement</trd>,
    <trd>frais de route</trd>
  </trdgrp>
  <trd lng="nl">reisonkosten</trd>
</drv>
</art>
