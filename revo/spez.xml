<drv mrk="spez.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="spez.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Enkasigo a&ubreve; elkasigo de <ref cel="mon.0o">mono</ref>:
      <ekz>
        li sola decidis pri la bu&gcirc;eto, la <tld/>oj ktp
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter Nov-Jorko kaj Germanujo estas komercaj komuniki&gcirc;oj, kiuj prezentas la
        <tld/>on de centoj da milionoj markoj &ccirc;iujare
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la eksterkomerca <tld/>o de Ruslando atingis 27,2 miliardojn da usonaj dolaroj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9310-artikoletoj.txt"
              >Malkopiilo</url></vrk>
          <lok>93/10</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj el tiuj, kiuj faris la decidojn, ne vere komprenis la
        internaciajn <tld/>ojn
        <fnt>
          <aut>K. Kniivil&auml;</aut>
          <vrk>Homoj de Putin</vrk>
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_o;&c_t; <klr tip="amb">(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_y;)</klr></trd>
  <trdgrp lng="ca">
    <trd>moviment</trd>,
    <trd>transacci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pen&ecaron;&zcaron;n&iacute; obrat</trd>,
    <trd>pen&ecaron;&zcaron;n&iacute; p&rcaron;evod</trd>
  </trdgrp>
  <trd lng="de">Umsatz</trd>
  <trd lng="en">turnover</trd>
  <trd lng="es">transacci&oacute;n <klr tip="amb">(de dinero o
  bienes)</klr></trd>
  <trd lng="fr">transaction financi&egrave;re</trd>
  <trd lng="hu">p&eacute;nzforgalom</trd>
  <trd lng="nl">omzet</trd>
  <trd lng="ru">&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;
  <ind>&c_o;&c_b;&c_o;&c_r;&c_o;&c_t;</ind></trd>
  <trd lng="sk">pe&ncaron;a&zcaron;n&yacute; obrat</trd>
  <trd lng="uk">&c_g;&c_r;&c_o;&c_sh;&c_o;&c_v;&c_i;&c_j; &c_o;&c_b;&c_o;&c_r;&c_o;&c_t;</trd>
</drv>

<drv mrk="spez.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Enkasigi a&ubreve; elkasigi:
      <ekz>
        &gcirc;i estas grandvalora monero, per kiu oni povas majstre brokanti, se oni scias
        bone &gcirc;in <tld/>i
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mono jam estis el <tld/>ita por aliaj a&jcirc;oj, kiuj &scirc;ajnis same gravaj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 41</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">umsetzen</trd>
</drv>






<drv mrk="spez.en0i">
  <kap><ofc>*</ofc>en<tld/>i <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5205">en'spez'</url></lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="spez.en0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="kas.en0igi">Enkasigi</ref>; <ref tip="super" cel="ricev.0i.ajxon">ricevi</ref> monon por 
      liverita varo a&ubreve; servo:
      <ekz>
        pli facile estas multe el<tld/>i, ol malmulte en<tld/>i
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu laboren<tld/>as monon, tiu laboren<tld/>as por truhava saketo
        <fnt>
          <bib>MT</bib>
          <lok>&Hcirc;agaj 1:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro nesufi&ccirc;a salajro kaj premantaj &scirc;uldoj kaj li diradas, ke li devas en<tld/>i
        pli da mono
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="profit.0i.EKON">profiti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_d;&c_a;&c_h;&c_o;&c_d;</ind></trd>,
    <trd>&c_a;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_g;&c_r;&c_o;&c_sh;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>percebre</trd>,
    <trd>guanyar</trd>
  </trdgrp>
  <trd lng="cs">m&iacute;t p&rcaron;&iacute;jem</trd>
  <trdgrp lng="de">
    <trd>einnehmen</trd>,
    <trd>umsetzen</trd>,
    <trd>verdienen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>earn</trd>,
    <trd>receive <klr>(money)</klr></trd>
  </trdgrp>
  <trd lng="es">recibir <klr>(dinero)</klr></trd>
  <trdgrp lng="fr">
