  <trd lng="de">verschwenderisch</trd>
  <trd lng="fr">d&eacute;pensier</trd>
  <trd lng="ja">&#x91D1;&#36963;&#12356;&#x306E;&#33618;&#12356; 
  <pr>&#x304B;&#x306D;&#x305A;&#x304B;&#12356;&#x306E;&#12354;&#12425;&#12356;</pr></trd>
  <trd lng="nl">verkwistend</trd>
  <trd lng="pl">lubi&aogonek;cy wydawa&cacute;  <ind>pieni&aogonek;dze</ind></trd>
  <trd lng="sk">rozhadzova&ccaron;n&yacute;</trd>
</drv>

<drv mrk="spez.el0i">
  <kap><ofc>*</ofc>el<tld/>i <fnt><bib>F</bib>, <lok>Universala Vortaro,
    <url ref="&AdE;/fundamento/universala_vortaro.html#av-5204">el'spez'</url></lok></fnt>,
    <var><kap>for<tld/>i</kap></var>
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="spez.el0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="pag.0i.EKON">Pagi</ref> monon por ricevita varo a&ubreve; servo:
      <ekz>
        ili el<tld/>is, sed ne profitis
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 12:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro multaj kuracistoj <klr>[li]</klr> el<tld/>is sian tutan havon
        <fnt>
          <bib>NT</bib>
          <lok>Marko 5:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        kion pli vi el<tld/>os, mi tion repagos al vi, kiam mi revenos
        <fnt>
          <bib>NT</bib>
          <lok>Luko 10:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &ccirc;iutage nur el<tld/>adis monon kaj neniam ion en<tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili tre rapide el<tld/>is la tri mil piastrojn
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXX</lok>
        </fnt>;
      </ekz>
      <ekz>
        post kelka tempo li for<tld/>is &ccirc;iujn fortojn kaj rimedojn siajn
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">spandeer</trd>
  <trd lng="am">&#x121B;&#x12CD;&#4899;&#4725;</trd>
  <trd lng="ar">&a_A_hamza_sure;&a_n;&a_f;&a_q;</trd>
  <trd lng="az">s&#601;rf</trd>
