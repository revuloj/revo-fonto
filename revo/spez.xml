  <trd lng="en">spend</trd>
  <trd lng="es">gastar</trd>
  <trd lng="et">kulutada</trd>
  <trd lng="eu">pasatzeko</trd>
  <trd lng="fr">d&eacute;penser<klr> (de l'argent)</klr></trd>
  <trd lng="fy">trochbringe</trd>
  <trd lng="ga">chaitheamh</trd>
