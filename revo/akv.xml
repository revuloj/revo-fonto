<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>akv</rad>/o
</kap>
<drv mrk="akv.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       H<sub>2</sub>O. Flua&jcirc;o, el kiu konsistas la riveroj kaj maroj:
       <ekz>
         <tld/>o  silenta subfosas la bordon
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
       </ekz>
       <ekz>
         el malgrandaj <tld/>eroj fari&gcirc;as grandaj riveroj
         <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="cs">voda</trd>
   <trd lng="de">Wasser</trd>
   <trd lng="en">water</trd>
   <trd lng="la">aqua</trd>
   <trd lng="es">agua</trd>
   <trd lng="fr">eau</trd>
   <trd lng="it">acqua</trd>
   <trd lng="nl">water</trd>
   <trd lng="pt">&aacute;gua</trd>
   <trd lng="pl">woda</trd>
   <trd lng="tr">su</trd>
   <trd lng="ru">&c_v;&c_o;&c_d;&c_a;</trd>
</drv>
<drv mrk="akv.0a">
   <kap><tld/>a</kap>
   <snc num="1">
     <dif>
       El <tld/>o:
       <ekz>
         <tld/>a trinka&jcirc;o.
       </ekz>
     </dif>
     <trdgrp lng="fr">
       <trd>&agrave; base d'<ind>eau</ind></trd>,
       <trd>aqueux</trd>
     </trdgrp>
     <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j;</trd>
     <trd lng="nl">waterig</trd>
   </snc>
   <snc num="2">
     <dif>
       Vivanta en <tld/>o:
       <ekz>
         <tld/>aj bestoj.
       </ekz>
     </dif>
     <trd lng="fr">d'<ind>eau</ind></trd>
     <trd lng="nl">water-</trd>
     <trd lng="ru">&c_v;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
   </snc>
   <trd lng="pt">aqu&aacute;tico</trd>
</drv>
<drv mrk="akv.0umi">
   <kap>
     <tld/>oprovizi<fnt>Z</fnt>,
     <tld/>i<fnt>Z</fnt>,
     <tld/>umi<fnt>Z</fnt>
   </kap>
   <snc>
     <dif>
       Malsekigi surver&scirc;ante a&ubreve; trafluigante:
       <ekz>
         oni devas <tld/>umi florojn post la sunsubiro
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="ros.0o">roso</ref>.
     </refgrp>
   </snc>
   <trdgrp lng="de">
     <trd>gie&szlig;en</trd>,
     <trd>w&auml;ssern</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>irriguer</trd>,
     <trd>arroser</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>gieten</trd>,
     <trd>begieten</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>banhar</trd>,
     <trd>molhar</trd>,
     <trd>regar</trd>
   </trdgrp>
   <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="akv.0ujo">
   <kap><tld/>ujo</kap>
   <snc>
     <dif>
       Loko por ricevi kaj konservi <tld/>on.
     </dif>
   </snc>
   <trd lng="fr">citerne <klr>(pour recueillir l'eau)</klr></trd>
   <trd lng="nl">watervat</trd>
   <trdgrp lng="pt">
     <trd>cisterna</trd>,
     <trd>caixa d'&aacute;gua</trd>,
     <trd>reservat&oacute;rio</trd>
   </trdgrp>
   <trd lng="ru">&c_c;&c_i;&c_s;&c_t;&c_je;&c_r;&c_n;&c_a;</trd>
</drv>
<drv mrk="akv.0ofalofal0o">
   <kap><tld/>ofalo, fal<tld/>o<fnt>Z</fnt></kap>
   <snc>
     <dif>
       Da&ubreve;ra akvofluo, rapide falanta de alta&jcirc;o.
     </dif>
   </snc>
   <trd lng="de">Wasserfall</trd>
   <trdgrp lng="fr">
     <trd>cascade</trd>,
     <trd>chute d'eau</trd>
   </trdgrp>
   <trd lng="nl">waterval</trd>
   <trdgrp lng="pt">
     <trd>cachoeira</trd>,
     <trd>cascata</trd>,
     <trd>queda-d'&aacute;gua</trd>
   </trdgrp>
   <trd lng="ru">&c_v;&c_o;&c_d;&c_o;&c_p;&c_a;&c_d;</trd>
</drv>
<drv mrk="akv.0okondukilo">
   <kap><tld/>okondukilo</kap>
   <snc>
     <dif>
       Tubo, tra kiu oni irigas <tld/>on.
     </dif>
   </snc>
   <trd lng="de">Wasserleitung</trd>
   <trdgrp lng="fr">
     <trd>conduite</trd>,
     <trd>canalisation</trd>
   </trdgrp>
   <trd lng="nl">waterleiding</trd>
   <trd lng="pt">cano de &aacute;gua</trd>
   <trd lng="ru">&c_v;&c_o;&c_d;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;</trd>
</drv>
<drv mrk="akv.sub0igi">
   <kap>sub<tld/>igi</kap>
   <snc>
     <dif>
       Kovri per <tld/>o, meti sub la <tld/>on:
       <ekz>
         la Holandanoj, por sin
         defendi, sub<tld/>igis sian landon.
       </ekz>
     </dif>
   </snc>
   <trd lng="de">&uuml;berschwemmen</trd>
   <trdgrp lng="fr">
     <trd>immerger</trd>,
     <trd>plonger <klr>(tr.)</klr></trd>,
     <trd>inonder</trd>
   </trdgrp>
   <trd lng="nl">onder <ind>water</ind> zetten</trd>
   <trdgrp lng="pt">
     <trd>submergir</trd>,
     <trd>mergulhar</trd>,
     <trd>afogar</trd>
   </trdgrp>
   <trd lng="ru">&c_p;&c_o;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="akv.sub0igxi">
   <kap>sub<tld/>i&gcirc;i</kap>
   <snc>
     <dif>
       Profundi&gcirc;i sub <tld/>on:
       <ekz>
         la submar&scirc;ipo
         sub<tld/>i&gcirc;is, &ccirc;ar &gcirc;i ekvidis kroziston malamikan.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="fr">
     <trd>plonger <klr>(intr.)</klr></trd>,
     <trd>s'<ind>immerger</ind></trd>
   </trdgrp>
   <trd lng="nl">ondergaan<klr tip="ind"> (onder water)</klr></trd>
   <trdgrp lng="pt">
     <trd>submergir-se</trd>,
     <trd>mergulhar-se</trd>,
     <trd>afogar-se</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_z;&c_a;&c_t;&c_o;&c_n;&c_u;&c_t;&c_mol;</trd>,
     <trd>&c_u;&c_j;&c_t;&c_i; &c_p;&c_o;&c_d; &c_v;&c_o;&c_d;&c_u;</trd>
   </trdgrp>
</drv>
<drv mrk="akv.super0i">
   <kap>super<tld/>i<fnt>Z</fnt></kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Inundi:
       <ekz>
         la herbejoj estis super<tld/>itaj<fnt>B</fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="fr">inonder</trd>
   <trd lng="de">&uuml;berschwemmen</trd>
   <trd lng="nl">overstromen</trd>
   <trdgrp lng="pt">
     <trd>inundar</trd>,
     <trd>alagar</trd>
   </trdgrp>
   <trd lng="ru">&c_z;&c_a;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="akv.super0ego">
   <kap>super<tld/>ego</kap>
   <snc>
     <dif>
       Diluvo.
     </dif>
   </snc>
   <trd lng="de">&Uuml;berschwemmung</trd>
   <trd lng="fr">d&eacute;luge</trd>
   <trd lng="nl">overstroming</trd>
   <trd lng="pt">dil&uacute;vio</trd>
   <trd lng="ru">&c_p;&c_o;&c_t;&c_o;&c_p;</trd>
</drv>
<drv mrk="akv.tra0igi">
   <kap>tra<tld/>igi</kap>
   <snc>
     <dif>
       Alkonduki <tld/>on sur kulturatan terspacon; irigacii.
     </dif>
   </snc>
   <trd lng="de">bew&auml;ssern</trd>
   <trd lng="nl">bevloeien</trd>
   <trd lng="pt">irrigar</trd>
   <trd lng="ru">&c_o;&c_b;&c_v;&c_o;&c_d;&c_n;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
