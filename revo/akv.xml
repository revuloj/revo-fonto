<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>akv</rad>/o
</kap>
<drv mrk="akv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="akv.0o.KEM">
    <dif>
      Flua&jcirc;o, el kiu konsistas la riveroj kaj maroj; &hcirc;emia simbolo
      H<sub>2</sub>O:
      <ekz>
        prenu mem <tld/>on, se vi volas trinki
        <fnt><bib>F</bib>, <lok>&FE; 19, la feino.</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o silenta subfosas la bordon
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        el malgrandaj <tld/>eroj fari&gcirc;as grandaj riveroj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_a;</trd>
  <trd lng="br">dour</trd>
  <trd lng="cs">voda</trd>
  <trd lng="de">Wasser</trd>
  <trd lng="en">water</trd>
  <trd lng="la">aqua</trd>
  <trd lng="es">agua</trd>
  <trd lng="fr">eau</trd>
  <trd lng="it">acqua</trd>
  <trd lng="nl">water</trd>
  <trd lng="pt">&aacute;gua</trd>
  <trd lng="pl">woda</trd>
  <trd lng="tr">su</trd>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="sk">voda</trd>
  <trd lng="zh">&#27700;<klr> [shu&#464;]</klr></trd>
</drv>

<drv mrk="akv.0a">
  <kap><tld/>a</kap>
  <snc mrk="akv.0a.el_akvo">
    <dif>
      El <tld/>o:
      <ekz>
        ili tiel similis unu la alian, kiel du <tld/>aj gutoj
        <fnt><bib>ElektFab</bib>, <lok>du fratoj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="br">dourek</trd>
    <trd lng="en">watery</trd>
    <trdgrp lng="fr">
      <trd>&agrave; base d'<ind>eau</ind></trd>,
      <trd>aqueux</trd>
    </trdgrp>
    <trd lng="be">&c_v;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_y;</trd>
    <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j;</trd>
    <trd lng="nl">waterig</trd>
  </snc>
  <snc mrk="akv.0a.akvomedia">
    <dif>
      Vivanta en <tld/>o:
      <ekz>
        aligatoroj kaj aliaj <tld/>aj bestoj
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_o;&c_d;&c_n;&c_y;</trd>
    <trd lng="br">-<ind>dour</ind></trd>
    <trd lng="en">aquatic</trd>
    <trd lng="es">acu&aacute;tico</trd>
    <trd lng="fr">d'<ind>eau</ind></trd>
    <trd lng="nl">water-</trd>
    <trd lng="ru">&c_v;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="pt">aqu&aacute;tico</trd>
  <trd lng="sk">vodn&yacute;</trd>
</drv>

<drv mrk="akv.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <snc mrk="akv.0i.akvumi">
    <ref tip="dif" cel="akv.0umi.AGR"><tld/>umi</ref>
  </snc>
</drv>

<drv mrk="akv.0umi">
  <kap><tld/>umi</kap>
  <snc mrk="akv.0umi.KOMUNE">
    <dif>
      Malsekigi surver&scirc;ante a&ubreve; trafluigante:
      <ekz>
        vi estas kiel &gcirc;ardeno <tld/>umata
        <fnt><bib>MT</bib>, <lok>&Jes; 58:11</lok></fnt>;
      </ekz>
      <ekz>
        tirbestoj <tld/>umis sin en la rivero
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>&ccirc;e Lev Tolstoj en Jasnaja
        Poljana</lok></fnt>;
      </ekz>
      <ekz>
        oni devas <tld/>umi florojn post la sunsubiro.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_r;&c_a;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>doura<klr> [akvoprovizi]</klr></trd>,
    <trd>doura&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gie&szlig;en</trd>,
    <trd>w&auml;ssern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>flush</trd>,
    <trd>rinse</trd>,
    <trd>irrigate</trd>,
    <trd>water</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>arroser</trd>,
    <trd>irriguer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gieten</trd>,
    <trd>begieten</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>banhar</trd>,
    <trd>molhar</trd>,
    <trd>regar</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">polia&tcaron;</trd>
</drv>

<drv mrk="akv.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="akv.0ujo.KOMUNE">
    <dif>
      Loko por ricevi kaj konservi <tld/>on:
      <ekz>
        oni konstruis <tld/>ujon, kiu povis enteni &ccirc;irka&ubreve; tri miliardojn
        da kubaj metroj
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">basin</trd>
  <trdgrp lng="fr">
    <trd>citerne <klr>(d'eau)</klr></trd>,
    <trd>r&eacute;servoir<klr tip="amb"> (d'eau)</klr></trd>
  </trdgrp>
  <trd lng="nl">watervat</trd>
  <trdgrp lng="pt">
    <trd>cisterna</trd>,
    <trd>caixa d'&aacute;gua</trd>,
    <trd>reservat&oacute;rio</trd>
  </trdgrp>
  <trd lng="ru">&c_c;&c_i;&c_s;&c_t;&c_je;&c_r;&c_n;&c_a;</trd>
</drv>

<drv mrk="akv.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="akv.el0igxi.KOMUNE">
    <dif>
      Eliri, aperi el <tld/>o:
      <ekz>
        en la mezo de Nilo oni renkontadis insulojn, kiuj, &scirc;ajnis, hiera&ubreve;
        el<tld/>i&gcirc;is
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&eacute;merger</trd>
</drv>

<drv mrk="akv.fal0o">
  <kap>fal<tld/>o</kap>
  <snc mrk="akv.fal0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <ref tip="dif" cel="fal.akvo0o.GEOG">akvofalo</ref>
      <ekz>
        li rakontis <klr>...</klr> pri la fal<tld/>oj, kiuj sa&ubreve;mante
        falegas malsupren kun bruo simila al frapoj de tondro
        <fnt><bib>Fab2</bib>, <lok>monteto de elfoj</lok></fnt>.
      </ekz>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_a;&c_s;&c_p;&c_a;&c_d;</trd>
  <trd lng="br">lamm-dour</trd>
  <trdgrp lng="en">
    <trd>waterfall</trd>,
    <trd>cascade</trd>
  </trdgrp>
  <trd lng="de">Wasserfall</trd>
  <trd lng="es">cascada</trd>
  <trdgrp lng="fr">
    <trd>cascade</trd>,
    <trd>chute d'eau</trd>
  </trdgrp>
  <trd lng="nl">waterval</trd>
  <trdgrp lng="pt">
    <trd>cachoeira</trd>,
    <trd>cascata</trd>,
    <trd>queda-d'&aacute;gua</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_o;&c_p;&c_a;&c_d;</trd>
  <trd lng="sk">vodop&aacute;d</trd>
</drv>

<drv mrk="akv.sub0igi">
  <kap>sub<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akv.sub0igi.KOMUNE">
    <dif>
      Kovri per <tld/>o, meti sub la <tld/>on:
      <ekz>
        la Holandanoj, por sin defendi, sub<tld/>igis sian landon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_t;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_g;&c_r;&c_u;&c_z;&c_ib;&c_c;&c_mol;
    <klr>&c_u; &c_v;&c_a;&c_d;&c_u;</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>beuzi&ntilde;<klr> (tr.)</klr></trd>,
    <trd>souba&ntilde;<klr> (en dour)</klr></trd>
  </trdgrp>
  <trd lng="de">&uuml;berschwemmen</trd>
  <trd lng="en">submerge</trd>
  <trd lng="es">sumergir</trd>
  <trdgrp lng="fr">
    <trd>immerger</trd>,
    <trd>inonder</trd>,
    <trd>plonger <klr>(tr.)</klr></trd>,
    <trd>submerger</trd>
  </trdgrp>
  <trd lng="nl">onder <ind>water</ind> zetten</trd>
  <trdgrp lng="pt">
    <trd>submergir</trd>,
    <trd>mergulhar</trd>,
    <trd>afogar</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">potopi&tcaron;</trd>
</drv>

<drv mrk="akv.sub0igxi">
  <kap>sub<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="akv.sub0igxi.KOMUNE">
    <dif>
      Profundi&gcirc;i sub <tld/>on:
      <ekz>
        la submar&scirc;ipo sub<tld/>i&gcirc;is, &ccirc;ar &gcirc;i ekvidis kroziston malamikan.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_t;&c_a;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_g;&c_r;&c_u;&c_z;&c_ib;&c_c;&c_c;&c_a; <klr>&c_w; &c_v;&c_a;&c_d;&c_u;</klr></trd>
  </trdgrp>
  <trd lng="br">spluja&ntilde;<klr tip="ind"> (dindan an dour)</klr></trd>
  <trd lng="en">become submerged</trd>
  <trdgrp lng="fr">
    <trd>plonger <klr>(intr.)</klr></trd>,
    <trd>s'<ind>immerger</ind></trd>
  </trdgrp>
  <trd lng="nl">ondergaan<klr tip="ind"> (onder water)</klr></trd>
  <trdgrp lng="pt">
    <trd>submergir-se</trd>,
    <trd>mergulhar-se</trd>,
    <trd>afogar-se</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_t;&c_o;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_j;&c_t;&c_i; &c_p;&c_o;&c_d; &c_v;&c_o;&c_d;&c_u;</trd>
  </trdgrp>
  <trd lng="sk">potopi&tcaron; sa</trd>
</drv>

<drv mrk="akv.super0i">
  <kap>super<tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akv.super0i.KOMUNE">
    <dif>
      Inundi:
      <ekz>
        la herbejoj estis super<tld/>itaj<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_t;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">li&ntilde;va&ntilde;</trd>
  <trdgrp lng="en">
    <trd>flood</trd>,
    <trd>inundate</trd>
  </trdgrp>
  <trd lng="fr">inonder</trd>
  <trd lng="de">&uuml;berschwemmen</trd>
  <trd lng="nl">overstromen</trd>
  <trdgrp lng="pt">
    <trd>inundar</trd>,
    <trd>alagar</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="akv.super0ego">
  <kap>super<tld/>ego</kap>
  <snc mrk="akv.super0ego.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Diluvo:
      <ekz>
        la fino de la mondo povas esti ka&ubreve;zita de super<tld/>ego
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_t;&c_o;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>li&ntilde;vadenn</trd>,
    <trd>dour-beuz</trd>
  </trdgrp>
  <trd lng="de">&Uuml;berschwemmung</trd>
  <trdgrp lng="en">
    <trd>flood</trd>,
    <trd>deluge</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;luge</trd>
  <trd lng="nl">overstroming</trd>
  <trd lng="pt">dil&uacute;vio</trd>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_o;&c_p;</trd>
  <trd lng="sk">potopa</trd>
</drv>

<drv mrk="akv.tra0igi">
  <kap>tra<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akv.tra0igi.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Alkonduki <tld/>on sur kulturatan terspacon.
    </dif>
    <refgrp tip="sin">
      <ref cel="irigaci.0i.AGR">irigacii</ref>
    </refgrp>
  </snc>
  <trd lng="br">doura&ntilde;<klr tip="ind"> (un dachenn douar)</klr></trd>
  <trd lng="de">bew&auml;ssern</trd>
  <trd lng="en">irrigate</trd>
  <trd lng="es">irrigar</trd>
  <trd lng="fr">irriguer</trd>
  <trd lng="hu">&ouml;nt&ouml;z</trd>
  <trdgrp lng="nl">
    <trd>bevloeien</trd>,
    <trd>irrigeren</trd>
  </trdgrp>
  <trd lng="pt">irrigar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_v;&c_o;&c_d;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_r;&c_o;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zavla&zcaron;i&tcaron;</trd>
</drv>

<drv mrk="akv.trink0o">
  <kap>trink<tld/>o</kap>
  <snc mrk="akv.trink0o.KOMUNE">
    <dif>
      Akvo tia, ke &gcirc;i estas trinkebla sen dan&gcirc;ero por homa sano:
      <ekz>
        estis anka&ubreve; du malgrandaj cisternoj en unu el la kajutoj, unu por
        lavakvo kaj la alia por trink<tld/>o
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>tria libro, &ccirc;apitro
        47a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">eau potable</trd>
</drv>
</art>
