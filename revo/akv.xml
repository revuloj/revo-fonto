<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>akv</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="akv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="akv.0o.KEM">
    <dif>
      Flua&jcirc;o, el kiu konsistas la riveroj kaj maroj;
      &hcirc;emia simbolo H<sub>2</sub>O:
      <ekz>
        prenu mem <tld/>on, se vi volas trinki
        <fnt><bib>F</bib>, <lok>&FE; 19</lok></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>o silenta subfosas la bordon</ind>
        <trd lng="fr">il faut se m&eacute;fier de l'eau qui dort</trd>
        <klr>(iu silenta a&ubreve; senaga povas subite i&gcirc;i
        dan&gcirc;era)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_a;</trd>
  <trd lng="bg">&c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="br">dour</trd>
  <trd lng="ca">aigua</trd>
  <trd lng="cs">voda</trd>
  <trd lng="de">Wasser</trd>
  <trd lng="en">water</trd>
  <trd lng="es">agua</trd>
  <trd lng="fa">&a_A_madda;&a_b;</trd>
  <trd lng="fr">eau</trd>
  <trd lng="it">acqua</trd>
  <trd lng="ja">&#27700; [<ind>&#12377;&#12356;</ind>]</trd>
  <trd lng="ku">av</trd>
  <trd lng="la">aqua</trd>
  <trd lng="nl">water</trd>
  <trd lng="oc">aiga</trd>
  <trd lng="pt">&aacute;gua</trd>
  <trd lng="pl">woda</trd>
  <trd lng="tr">su</trd>
  <trd lng="tp">telo</trd>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="sk">voda</trd>
  <trd lng="sv">vatten</trd>
  <trd lng="zh">&#27700;<klr> [shu&#464;]</klr></trd>
</drv>

<drv mrk="akv.0a">
  <kap><tld/>a</kap>
  <snc mrk="akv.0a.el_akvo">
    <dif>
      El <tld/>o:
      <ekz>
        ili tiel similis unu la alian, kiel du <tld/>aj gutoj
        <fnt><bib>ElektFab</bib>, <lok>du fratoj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="br">dourek</trd>
    <trdgrp lng="ca">
      <trd>aqu&oacute;s</trd>,
      <trd>d'aigua</trd>
    </trdgrp>
    <trd lng="en">watery</trd>
    <trd lng="be">&c_v;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_y;</trd>
    <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j;</trd>
    <trd lng="nl">waterig</trd>
    <trd lng="sv">vatten-</trd>
  </snc>
  <snc mrk="akv.0a.akvomedia">
    <dif>
      Vivanta en <tld/>o:
      <ekz>
        aligatoroj kaj aliaj <tld/>aj bestoj
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_o;&c_d;&c_n;&c_y;</trd>
    <trd lng="br">-<ind>dour</ind></trd>
    <trd lng="ca">aqu&agrave;tic</trd>
    <trd lng="en">aquatic</trd>
    <trd lng="es">acu&aacute;tico</trd>
    <trd lng="nl">water-</trd>
    <trd lng="ru">&c_v;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>d'<ind>eau</ind></trd>,
    <trd>aqueux</trd>,
    <trd>aquatique</trd>
  </trdgrp>
  <trd lng="pl">wodny</trd>
  <trd lng="pt">aqu&aacute;tico</trd>
  <trd lng="sk">vodn&yacute;</trd>
</drv>

<drv mrk="akv.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <snc mrk="akv.0i.akvumi">
    <ref tip="dif" cel="akv.0umi.KOMUNE"><tld/>umi</ref>
  </snc>
  <trd lng="ca">regar</trd>
  <trdgrp lng="de">
    <trd>gie&szlig;en</trd>,
    <trd>w&auml;ssern</trd>
  </trdgrp>
  <trd lng="es">regar</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b; &a_d;&a_A;&a_d;&a_n;</trd>,
    <trd>&a_A_madda;&a_b;&f_y;&a_A;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="sv">vattna</trd>
</drv>

<drv mrk="akv.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="akv.0ero.KOMUNE">
    <dif>
      Tre malgranda kvanto da <tld/>o, ordinare en formo de <ref
      tip="vid" cel="gut.0o">guto</ref>:
      <ekz>
        <ind>
          el malgrandaj <tld/>eroj fari&gcirc;as grandaj riveroj
        </ind>
        <trd lng="fr">les petits ruisseaux font les grandes
        rivi&egrave;res</trd>
        <klr>(per multaj malgrandaj faroj esti&gcirc;as grandaj
        fara&jcirc;oj)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_o;&c_p;&c_l;&c_ja; &c_v;&c_a;&c_d;&c_y;</trd>
  <trd lng="ca">gota</trd>
  <trd lng="de">Wassertropfen</trd>
  <trd lng="fa">&a_q;&a_T;&a_r;&a_h;&a_hamza_sure; &a_A_madda;&a_b;</trd>
  <trd lng="fr">goutte d'eau</trd>
  <trd lng="nl">waterdruppel</trd>
  <trd lng="pl">kropla <ind>wody</ind></trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_l;&c_ja; &c_v;&c_o;&c_d;&c_y;</trd>
  <trd lng="sv">vattendroppe</trd>
</drv>

<drv mrk="akv.A0isto">
  <kap><tld lit="A"/>isto <fnt><bib>PIV2</bib></fnt></kap>
  <mlg>Aqr</mlg>
  <mlg>&#9810;</mlg>
  <snc mrk="akv.0isto.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Ekvatora <ref tip="lst" cel="stel.0figuro"
      lst="voko:zodiakaj_signoj"
      val="11">stelfiguro</ref>
      kaj signo de la <ref tip="malprt"
      cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Aquarius</trd>).
    </dif>
    <ref tip="sin" cel="versx.V0isto">Ver&scirc;isto</ref>
    <trdgrp lng="be">
      <trd>&c_V;&c_a;&c_d;&c_a;&c_l;&c_je;&c_j;</trd>,
      <trd>&c_V;&c_a;&c_d;&c_a;&c_l;&c_ib;&c_w;</trd>
    </trdgrp>
    <trd lng="ca">Aquari</trd>
    <trd lng="de">Wassermann<klr tip="amb"> (Sternbild)</klr></trd>
    <trd lng="fa">&a_d;&a_l;&a_w;<klr tip="amb"> (&a_S;&a_w;&a_r;&a_t; &a_f;&a_l;&f_k;&f_y;)</klr></trd>
    <trd lng="fr">Verseau<klr> (constellation)</klr></trd>
    <trd lng="he">&mem;&zayin;&lamed; &dalet;&lamed;&yod;</trd>
    <trd lng="hu">V&iacute;z&ouml;nt&odblac;</trd>
    <trd lng="ja">&#23453;&#29942;&#23470; [<ind>&#12411;&#12358;&#12408;&#12356;&#12365;&#12421;&#12358;</ind>]</trd>
    <trd lng="nl">Waterman<klr tip="ind"> (sterrebeeld)</klr></trd>
    <trd lng="pl">wodnik</trd>
    <trd lng="pt">Aqu&aacute;rio</trd>
    <trd lng="ru">&c_V;&c_o;&c_d;&c_o;&c_l;&c_je;&c_j;</trd>
    <trd lng="sk">Vodn&aacute;r</trd>
  </snc>
</drv>

<drv mrk="akv.0umi">
  <kap><tld/>umi</kap>
  <snc mrk="akv.0umi.KOMUNE">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">HOR</uzo>
    <dif>
      Malsekigi per surver&scirc;o a&ubreve; trafluigo de <tld/>o:
      <ekz>
        vi estas kiel &gcirc;ardeno <tld/>umata
        <fnt><bib>MT</bib>, <lok>&Jes; 58:11</lok></fnt>;
      </ekz>
      <ekz>
        tirbestoj <tld/>umis sin en la rivero
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>&ccirc;e Lev Tolstoj en
        Jasnaja Poljana</lok></fnt>;
      </ekz>
      <ekz>
        oni devas <tld/>umi florojn post la sunsubiro.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_r;&c_a;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>doura<klr> [akvoprovizi]</klr></trd>,
    <trd>doura&ntilde;</trd>
  </trdgrp>
  <trd lng="ca">regar</trd>
  <trdgrp lng="de">
    <trd>gie&szlig;en</trd>,
    <trd>w&auml;ssern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>flush</trd>,
    <trd>rinse</trd>,
    <trd>irrigate</trd>,
    <trd>water</trd>
  </trdgrp>
  <trd lng="es">regar</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b; &a_d;&a_A;&a_d;&a_n;</trd>,
    <trd>&a_A_madda;&a_b;&f_y;&a_A;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>arroser</trd>,
    <trd>irriguer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gieten</trd>,
    <trd>begieten</trd>,
    <trd>spoelen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nawadnia&cacute;</trd>,
    <trd>podlewa&cacute;</trd>,
    <trd>zrasza&cacute;</trd>,
    <trd>irygowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>banhar</trd>,
    <trd>molhar</trd>,
    <trd>regar</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">polia&tcaron;</trd>
  <trdgrp lng="sv">
    <trd>vattna</trd>,
    <trd>bevattna</trd>
  </trdgrp>
</drv>

<drv mrk="akv.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="akv.0ujo.KOMUNE">
    <dif>
      Loko por ricevi kaj konservi <tld/>on:
      <ekz>
        oni konstruis <tld/>ujon, kiu povis enteni &ccirc;irka&ubreve;
        tri miliardojn da kubaj metroj
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>cisterna</trd>,
      <trd>aljub</trd>
    </trdgrp>
    <trd lng="en">basin</trd>
    <trdgrp lng="fa">
      <trd>&a_A_madda;&a_b;&zwnj;&a_A;&a_n;&a_b;&a_A;&a_r;</trd>,
      <trd>&a_m;&a_kh;&a_z;&a_n; &a_A_madda;&a_b;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>citerne <klr>(d'eau)</klr></trd>,
      <trd>r&eacute;servoir<klr tip="amb"> (d'eau)</klr></trd>
    </trdgrp>
    <trd lng="nl">watervat</trd>
    <trdgrp lng="pl">
      <trd>zbiornik<klr> wodny</klr></trd>,
      <trd>basen<klr> z wod&aogonek;</klr></trd>,
      <trd>cysterna<klr> z wod&aogonek;</klr></trd>,
      <trd>rezerwuar</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>cisterna</trd>,
      <trd>caixa d'&aacute;gua</trd>,
      <trd>reservat&oacute;rio</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_c;&c_i;&c_s;&c_t;&c_je;&c_r;&c_n;&c_a;</trd>,
      <trd>&c_r;&c_je;&c_z;&c_je;&c_r;&c_v;&c_u;&c_a;&c_r;</trd>
    </trdgrp>
  </snc>
  <snc mrk="akv.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <tld lit="A"/>a objekto en terkavo, kun modera a&ubreve;
      nenia movo de la <tld/>o:
      <ekz>
        <em>bordo</em>: Tero &ccirc;e la rando de <tld/>ujo (rivero,
        lago a&ubreve; maro)
        <fnt>
          <aut>Laulum</aut>:
          <vrk><url
          ref="http://www.espero.com.cn/05e/05e-04l/03-5e-04l-4.htm"
                    >Vortaro de Esperanto por lernantoj (B)</url>
          </vrk>
        </fnt>;
      </ekz><ekz>
        <ind>natura <tld/>ujo</ind> <klr>(maro, lago, rivero)</klr>
        <trd lng="ru">&c_je;&c_s;&c_t;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j; &c_v;&c_o;&c_d;&c_o;&c_jo;&c_m;</trd>
      </ekz>
    </dif>
    <trd lng="ca">massa d'aigua</trd>
    <trdgrp lng="de">
      <trd>Wasserbecken</trd>,
      <trd>Gew&auml;sser</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>water body</trd>,
      <trd>reservoir</trd>
    </trdgrp>
    <trd lng="fa">&a_A_madda;&a_b;&f_g;&f_y;&a_r;</trd>
    <trdgrp lng="nl">
      <trd>bekken<klr tip="ind"> (waterbekken)</klr></trd>,
      <trd>waterbekken</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_o;&c_d;&c_o;&c_jo;&c_m;</trd>
  </snc>
</drv>

<drv mrk="akv.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="akv.el0igxi.KOMUNE">
    <dif>
      Eliri, aperi el <tld/>o:
      <ekz>
        en la mezo de Nilo oni renkontadis insulojn, kiuj, &scirc;ajnis,
        hiera&ubreve; el<tld/>i&gcirc;is
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">emergir</trd>
  <trd lng="fr">&eacute;merger</trd>
  <trd lng="nl">uit het <ind>water</ind> komen</trd>
  <trdgrp lng="pl">
    <trd>wynurza&cacute; si&eogonek;<klr> z wody</klr></trd>
    <trd>wy&lstroke;ania&cacute; si&eogonek;<klr> z wody</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_s;&c_p;&c_l;&c_y;&c_t;&c_mol;</trd>
</drv>

<drv mrk="akv.fal0o">
  <kap>fal<tld/>o</kap>
  <snc mrk="akv.fal0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <ref tip="dif" cel="fal.akvo0o.GEOG">akvofalo</ref>
    <ekz>
      li rakontis <klr>...</klr> pri la fal<tld/>oj, kiuj
      sa&ubreve;mante falegas malsupren kun bruo simila al frapoj de
      tondro <fnt><bib>Fab2</bib>, <lok>monteto de elfoj</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_a;&c_s;&c_p;&c_a;&c_d;</trd>
  <trd lng="bg">&c_v;&c_o;&c_d;&c_o;&c_p;&c_a;&c_d;</trd>
  <trd lng="br">lamm-dour</trd>
  <trdgrp lng="ca">
    <trd>cascada</trd>,
    <trd>saltant</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>waterfall</trd>,
    <trd>cascade</trd>
  </trdgrp>
  <trd lng="de">Wasserfall</trd>
  <trd lng="es">cascada</trd>
  <trd lng="fa">&a_A_madda;&a_b;&a_sh;&a_A;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>cascade</trd>,
    <trd>chute d'eau</trd>
  </trdgrp>
  <trd lng="nl">waterval</trd>
  <trdgrp lng="pl">
    <trd>wodospad</trd>,
    <trd>siklawa</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>cachoeira</trd>,
    <trd>cascata</trd>,
    <trd>queda-d'&aacute;gua</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_o;&c_p;&c_a;&c_d;</trd>
  <trd lng="sk">vodop&aacute;d</trd>
  <trd lng="sv">vattenfall</trd>
</drv>

<drv mrk="akv.klor0o">
  <kap>klor<tld/>o</kap>
  <snc mrk="akv.klor0o.KOMUNE">
    <dif>
      Solva&jcirc;o de natria hipoklorito en akvo,
      uzata kontra&ubreve; mikroboj kaj por senkolorigo de &scirc;tofoj:
      <ekz>
        mia avino <klr>...</klr> plenigas tinon per akvo varma kun 
        klor<tld/>o
        <fnt>
          <aut>A. LeQuint</aut>,
          <vrk>Infantraviva&jcirc;oj</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">lleixiu</trd>
  <trd lng="fa">&a_A_madda;&a_b; &f_zh;&a_A;&a_w;&a_l;</trd>
  <trd lng="fr">eau de javel</trd>
</drv>

<drv mrk="akv.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="akv.mar0o.KOMUNE">
    <dif>
      Sala akvo el maro, kontraste al
      <ref cel="sal.ne0a">nesala</ref> akvo pluva, rivera...:
      <ekz>
        pro pluvo mar<tld/>o estis neklara
        <fnt>
        <aut>Segilo</aut>,
        <vrk><url ref="http://segilo.cocolog-nifty.com/taglibro/">
        Taglibro</url></vrk>,
        <lok>2006-07-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja; <ind>&c_v;&c_a;&c_d;&c_a;</ind></trd>
  <trd lng="ca">aigua marina</trd>
  <trd lng="de">Meerwasser</trd>
  <trd lng="fa">&a_A_madda;&a_b; &a_d;&a_r;&f_y;&a_A;</trd>
  <trd lng="fr">eau de mer</trd>
  <trd lng="ja">&#28023;&#27700;
[<ind>&#12363;&#12356;&#12377;&#12356;</ind>]</trd>
  <trd lng="nl">zeewater</trd>
  <trd lng="pl">woda morska</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="sv">havsvatten</trd>
</drv>

<drv mrk="akv.mineral0o">
  <kap>mineral<tld/>o</kap>
  <snc mrk="akv.mineral0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Trink<tld/>o el aparta fonto,
      kun konstantaj kvantoj de diversaj mineraloj:
      <ekz>
        fra&ubreve;lino Emilio Krip <klr>...</klr> glutis la 
        lastajn gutojn el sia botelo da mineral<tld/>o  
        <fnt>
          <aut>M. Boulton</aut>,
          <vrk>Ebrivirgeco</vrk>,
          <lok>Nica Literatura Revuo, 1958, numero 3/3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_e;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; <ind>&c_v;&c_a;&c_d;&c_a;</ind></trd>
  <trd lng="ca">aigua mineral</trd>
  <trd lng="fr">eau min&eacute;rale</trd>
</drv>

<drv mrk="akv.roz0o">
  <kap>roz<tld/>o</kap>
  <snc mrk="akv.roz0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="A"/>o el distilado de rozpetaloj, la&ubreve; la speco
      a&ubreve; uzata kiel bonodora&jcirc;o
      a&ubreve; uzata kiel gustigilo de dol&ccirc;a&jcirc;oj:
      <ekz>
        kuiru sur forta fajro 30 minutojn gis la miksa&jcirc;o estos tre
        malfluida; Aldonu la roz-<tld/>on kaj citronsukon, kaj zorge
        miksu
        <fnt>
        <vrk><url
        ref="http://eo.wikibooks.org/wiki/Kuirlibro">Kuirlibro</url>,
        </vrk>,
        <lok>Konfita&jcirc;o el karotoj, 2005-02-25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_r;&c_o;&c_z;&c_o;&c_v;&c_a; &c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="ca">aigua de roses</trd>
  <trd lng="de">Rosenwasser</trd>
  <trd lng="fa">&f_g;&a_l;&a_A;&a_b;</trd>
  <trd lng="fr">eau de rose</trd>
  <trd lng="nl">rozenwater</trd>
  <trd lng="pl">woda r&oacute;&zdot;ana</trd>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_o;&c_v;&c_a;&c_ja; &c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="sv">rosenvatten</trd>
</drv>

<drv mrk="akv.sankt0ujo">
  <kap>sankt<tld/>ujo</kap>
  <snc mrk="akv.sankt0ujo.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      En tradicia kristanismo, baseneto,
      de kiu oni fingroprenas <tld/>on por krucosigni,
      memorige pri bapto:
      <ekz>
        mi la koloron de la sankt<tld/>ujoj atribuis al la sanktigita 
        akvo mem
        <fnt><aut>B. Vaha</aut>, <vrk>Adolesko</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>beneitera</trd>,
      <trd>pica d'aigua beneita</trd>
    </trdgrp>
    <trd lng="fr">b&eacute;nitier</trd>
  </snc>
</drv>

<drv mrk="akv.sod0o">
  <kap>sod<tld/>o</kap>
  <snc mrk="akv.sod0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="trink.0ajxo.KUI">Trinka&jcirc;o</ref> el
      <tld/>o entenanta karbondioksidajn veziketojn:
      <ekz>
        jen viro eksoifas kaj aliras trinka&jcirc;a&ubreve;tomaton por
        a&ccirc;eti skatolon da sod<tld/>o
        <fnt>
          <aut>R. Cash</aut>,
          <vrk><url ref="http://www.kafejo.com/humuro/01.htm">
          &Scirc;ercaro unua</url></vrk>,
          <lok>2006-01-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">soda</trd>
  <trd lng="de">Sodawasser</trd>
  <trdgrp lng="fr">
    <trd>eau gazeuse</trd>,
    <trd>soda</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>soda<klr tip="ind"> (water)</klr></trd>,
    <trd>spuitwater</trd>
  </trdgrp>
  <trd lng="pl">woda sodowa</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_d;&c_o;&c_v;&c_a;&c_ja; &c_v;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_a;&c_ja; &c_v;&c_o;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="kek"><trd>aaw</trd><trd>kaxlan ha&#700;</trd></trdgrp>
</drv>

<drv mrk="akv.sub0igi">
  <kap>sub<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akv.sub0igi.KOMUNE">
    <dif>
      Kovri per <tld/>o, meti sub la <tld/>on:
      <ekz>
        la Holandanoj, por sin defendi, sub<tld/>igis sian landon;
      </ekz>
      <ekz>
        la senvandaj palafitoj de Naba-koi situis inter forhakitaj
        bran&ccirc;oj en sub<tld/>igita mar&ccirc;a grundo
      <fnt><aut>W. Schad</aut>, <vrk>Orinoko-delto: Indianoj,
      mar&ccirc;oj, arbarego</vrk>, <bib>Monato</bib>, <lok>2000/11, p.
      20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_t;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_g;&c_r;&c_u;&c_z;&c_ib;&c_c;&c_mol;
    <klr>&c_u; &c_v;&c_a;&c_d;&c_u;</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>beuzi&ntilde;<klr> (tr.)</klr></trd>,
    <trd>souba&ntilde;<klr> (en dour)</klr></trd>
  </trdgrp>
  <trd lng="ca">submergir</trd>
  <trd lng="de">&uuml;berschwemmen</trd>
  <trd lng="en">submerge</trd>
  <trd lng="es">sumergir</trd>
  <trdgrp lng="fa">
    <trd>&a_z;&f_y;&a_r; <ind>&a_A_madda;&a_b;</ind> &a_b;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_gh;&a_r;&a_q; &a_d;&a_r; <ind>&a_A_madda;&a_b;</ind> &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>immerger</trd>,
    <trd>inonder</trd>,
    <trd>plonger <klr>(tr.)</klr></trd>,
    <trd>submerger</trd>
  </trdgrp>
  <trd lng="nl">onder <ind>water</ind> zetten</trd>
  <trd lng="pl">zanurza&cacute;<klr> w wodzie</klr></trd>
  <trdgrp lng="pt">
    <trd>submergir</trd>,
    <trd>mergulhar</trd>,
    <trd>afogar</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">potopi&tcaron;</trd>
</drv>

<drv mrk="akv.sub0igxi">
  <kap>sub<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="akv.sub0igxi.KOMUNE">
    <dif>
      Profundi&gcirc;i sub <tld/>on:
      <ekz>
        la submar&scirc;ipo sub<tld/>i&gcirc;is, &ccirc;ar &gcirc;i
        ekvidis kroziston malamikan.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_t;&c_a;&c_n;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_g;&c_r;&c_u;&c_z;&c_ib;&c_c;&c_c;&c_a;
      <klr>&c_w; &c_v;&c_a;&c_d;&c_u;</klr>
    </trd>
  </trdgrp>
  <trd lng="br">
    spluja&ntilde;<klr tip="ind"> (dindan an dour)</klr>
  </trd>
  <trd lng="ca">submergir-se</trd>
  <trd lng="en">become submerged</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r; <ind>&a_A_madda;&a_b;</ind> &a_f;&a_r;&a_w; &a_r;&a_f;&a_t;&a_n;</trd>,
    <trd>&a_gh;&a_w;&a_T;&a_h;&zwnj;&a_w;&a_r; &a_sh;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>plonger <klr>(intr.)</klr></trd>,
    <trd>s'<ind>immerger</ind></trd>
  </trdgrp>
  <trd lng="nl">ondergaan<klr tip="ind"> (onder water)</klr></trd>
  <trd lng="pl">zanurza&cacute;<klr> w wodzie</klr></trd>
  <trdgrp lng="pt">
    <trd>submergir-se</trd>,
    <trd>mergulhar-se</trd>,
    <trd>afogar-se</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_t;&c_o;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_j;&c_t;&c_i; &c_p;&c_o;&c_d; &c_v;&c_o;&c_d;&c_u;</trd>
  </trdgrp>
  <trd lng="sk">potopi&tcaron; sa</trd>
</drv>

<drv mrk="akv.sub0igxo">
  <kap>sub<tld/>i&gcirc;o</kap>
  <snc mrk="akv.sub0igxo.KOMUNE">
    <dif>
      Profundi&gcirc;o sub <tld/>on por sportaj a&ubreve; profesiaj celoj:
      <ekz>
        kompreneble, iam lia hobio estis sub<tld/>i&gcirc;o
        <fnt><aut>A. Tilmont</aut>, <vrk><url
        ref="http://www.esperanto.be/fel/mon/005932.html">Kosmopolita
        civitano kun okulo al la universo</url></vrk>,
        <bib>Monato</bib>,
        <lok>1998-04, p. 9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">immersionisme</trd>
  <trd lng="nl">duiken<klr tip="ind"> (zn.)</klr></trd>
 </drv>

<drv mrk="akv.sub0igxanto">
  <kap>sub<tld/>i&gcirc;anto</kap>
  <snc mrk="akv.sub0igxanto.KOMUNE">
    <dif>
      Persono, kiu sub<tld/>i&gcirc;as por sportaj a&ubreve; profesiaj celoj:
      <ekz>
        en la litova urbo Trakai estas klubo de sub<tld/>i&gcirc;antoj
        <ctl>Amfibija</ctl>
        <fnt><aut>last</aut>, <vrk>Subakvaj emocioj</vrk>,
        <bib>Monato</bib>,
        <lok>2002/11, p. 16</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">immersionista</trd>
  <trd lng="nl">duiker</trd>
 </drv>

<drv mrk="akv.super0i">
  <kap>super<tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akv.super0i.KOMUNE">
    <dif>
      Inundi:
      <ekz>
        la herbejoj estis super<tld/>itaj<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_t;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">li&ntilde;va&ntilde;</trd>
  <trdgrp lng="ca">
    <trd>inundar</trd>,
    <trd>negar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>flood</trd>,
    <trd>inundate</trd>
  </trdgrp>
  <trd lng="fa">&a_gh;&a_r;&a_q; &a_d;&a_r; &a_A_madda;&a_b; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">inonder</trd>
  <trd lng="de">&uuml;berschwemmen</trd>
  <trd lng="nl">overstromen</trd>
  <trdgrp lng="pl">
    <trd>zalewa&cacute;<klr> wod&aogonek;</klr></trd>,
    <trd>zatapia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>inundar</trd>,
    <trd>alagar</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="akv.super0ego">
  <kap>super<tld/>ego</kap>
  <snc mrk="akv.super0ego.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Diluvo:
      <ekz>
        la fino de la mondo povas esti ka&ubreve;zita de super<tld/>ego
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_a;&c_d;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_t;&c_o;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>li&ntilde;vadenn</trd>,
    <trd>dour-beuz</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>barrumbada</trd>,
    <trd>x&agrave;fec</trd>
  </trdgrp>
  <trd lng="de">&Uuml;berschwemmung</trd>
  <trdgrp lng="en">
    <trd>flood</trd>,
    <trd>deluge</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&f_y;&a_l;</trd>
  <trd lng="fr">d&eacute;luge</trd>
  <trd lng="nl">overstroming</trd>
  <trd lng="pt">dil&uacute;vio</trd>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_o;&c_p;</trd>
  <trd lng="sk">potopa</trd>
</drv>

<drv mrk="akv.tra0igi">
  <kap>tra<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akv.tra0igi.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Alkonduki <tld/>on sur kulturatan terspacon.
    </dif>
    <refgrp tip="sin">
      <ref cel="irigaci.0i.AGR">irigacii</ref>
    </refgrp>
  </snc>
  <trd lng="br">
    doura&ntilde;<klr tip="ind"> (un dachenn douar)</klr>
  </trd>
  <trd lng="ca">irrigar</trd>
  <trd lng="de">bew&auml;ssern</trd>
  <trd lng="en">irrigate</trd>
  <trd lng="es">irrigar</trd>
  <trd lng="fa">&a_A_madda;&a_b;&f_y;&a_A;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">irriguer</trd>
  <trd lng="hu">&ouml;nt&ouml;z</trd>
  <trdgrp lng="nl">
    <trd>bevloeien</trd>,
    <trd>irrigeren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>irygowa&cacute;</trd>,
    <trd>nawadnia&cacute;</trd>
  </trdgrp>
  <trd lng="pt">irrigar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_v;&c_o;&c_d;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_r;&c_o;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zavla&zcaron;i&tcaron;</trd>
</drv>

<drv mrk="akv.trink0o">
  <kap>trink<tld/>o</kap>
  <snc mrk="akv.trink0o.KOMUNE">
    <dif>
      Akvo tia, ke &gcirc;i estas trinkebla sen dan&gcirc;ero por homa
      sano:
      <ekz>
        estis anka&ubreve; du malgrandaj cisternoj en unu el la kajutoj,
        unu por lavakvo kaj la alia por trink<tld/>o
        <fnt><bib>Mortul&Scirc;ip</bib>,
        <lok>tria libro, &ccirc;apitro 47a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">aigua potable</trd>
  <trd lng="de">Trinkwasser</trd>
  <trd lng="fa">&a_A_madda;&a_b; &a_A_madda;&a_sh;&a_A;&a_m;&f_y;&a_d;&a_n;&f_y;</trd>
  <trd lng="ja">&#39154;&#12415;&#27700;
[<ind>&#12398;&#12415;&#12415;&#12378;</ind>]</trd>
  <trd lng="nl">drinkwater</trd>
  <trd lng="pl">woda pitna</trd>
  <trd lng="fr">eau potable</trd>
  <trd lng="ru">&c_p;&c_i;&c_t;&c_mol;&c_je;&c_v;&c_a;&c_ja;
&c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="sv">dricksvatten</trd>
</drv>

<drv mrk="akv.0oscienco">
  <kap><tld/>oscienco</kap>
  <snc mrk="akv.0oscienco.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri <tld/>o, pri &gcirc;ia konsisto kaj &gcirc;ia natura
      cirkulado tra  tero, aero, maroj:
      <ekz>
        pro grandaj klopodoj dum jarcentoj sur la tereno de
        <tld/>oscienco Nederlando akiris la respekton kiun &gcirc;i nun
        havas
        <fnt>
          <aut>-</aut>,
          <vrk>
<url ref="http://www.esperanto-nederland.nl/bonvenon/nederlando.php">
          Nederlando, malalta lando...</url></vrk>,
          <lok>2006-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">hidrologia</trd>
  <trd lng="fa">&a_A_madda;&a_b;&zwnj;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">hydrologie</trd>
  <trdgrp lng="nl">
    <trd>waterbouwkunde</trd>,
    <trd>hydrologie</trd>
  </trdgrp>
  <trd lng="pl">hydrologia</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="akv.0osciencisto">
  <kap><tld/>osciencisto</kap>
  <snc mrk="akv.0osciencisto.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Fakulo pri <tld/>oscienco.
    </dif>
  </snc>
  <trd lng="ca">hidr&ograve;leg</trd>
  <trd lng="fa">&a_A_madda;&a_b;&zwnj;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trdgrp lng="fr">
    <trd>hydrologiste</trd>,
    <trd>hydrologue</trd>
  </trdgrp>
  <trd lng="it">idrologo</trd>
  <trdgrp lng="nl">
    <trd>waterbouwkundige</trd>,
    <trd>hydroloog</trd>
  </trdgrp>
  <trd lng="pl">hydrolog</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
