<drv mrk="akv.super0i">
  <kap><ofc>*</ofc>super<tld/>i <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5213">super'akv'</url></lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akv.super0i.KOMUNE">
    <dif>
      Kovri per nenormala superfluo da akvo,
      <ref tip="sin" cel="inund.0i.MET">inundi</ref>:
      <ekz>
        la tuta vila&gcirc;o estis super<tld/>ita, kaj oni remis per
        kanoto inter la domoj
        <fnt>
          <aut>S. Bergman, trad. B. Gerdman</aut>
          <vrk>Tra sova&gcirc;a Kam&ccirc;atko</vrk>
          <lok>1932</lok>
        </fnt>;
      </ekz>
      <ekz>
        granda <tld/>o ne povas estingi la amon, kaj riveroj ne povas
        super<tld/>i &gcirc;in
        <fnt>
          <bib>MT</bib>
          <lok>&Alt; 8:7</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#3609;&#3657;&#3635;&#3607;&#3656;&#3623;&#3617;</trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_t;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">li&ntilde;va&ntilde;</trd>
  <trdgrp lng="ca">
    <trd>inundar</trd>,
    <trd>negar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vytopit</trd>,
    <trd>zaplavit</trd>,
    <trd>zatopit <klr>(povode&ncaron;)</klr></trd>
  </trdgrp>
  <trd lng="de">&uuml;berschwemmen</trd>
  <trdgrp lng="en">
    <trd>flood</trd>,
    <trd>inundate</trd>
  </trdgrp>
  <trd lng="fa">&a_gh;&a_r;&a_q; &a_d;&a_r; &a_A_madda;&a_b;
  &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">inonder</trd>
  <trd lng="id">mem<ind>banjir</ind>i</trd>
  <trd lng="nl">overstromen</trd>
  <trdgrp lng="pl">
    <trd>zalewa&cacute;<klr> wod&aogonek;</klr></trd>,
    <trd>zatapia&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>inundar</trd>,
    <trd>alagar</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_t;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>vytopi&tcaron;</trd>,
    <trd>zaplavi&tcaron;</trd>,
    <trd>zatopi&tcaron;</trd>
  </trdgrp>
</drv>

