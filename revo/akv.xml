<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: akv.xml,v 1.93 2020/12/29 08:10:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>akv</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="akv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="akv.0o.KEM">
    <dif>
      Flua&jcirc;o, el kiu konsistas la riveroj kaj maroj;
      &hcirc;emia simbolo H<sub>2</sub>O:
      <ekz>
        prenu mem <tld/>on, se vi volas trinki
        <fnt><bib>F</bib>, <lok>&FE; 19</lok></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>o silenta subfosas la bordon</ind>
        <trd lng="fr">il faut se m&eacute;fier de l'eau qui dort</trd>
        <klr>(iu silenta a&ubreve; senaga povas subite i&gcirc;i
        dan&gcirc;era)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#3609;&#3657;&#3635;</trd>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_a;</trd>
  <trd lng="bg">&c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="bo">&#3910;&#3956;&#3851;</trd>
  <trd lng="br">dour</trd>
  <trd lng="ca">aigua</trd>
  <trd lng="ms">air</trd>
  <trdgrp lng="cs">
    <trd>aqua</trd>,
    <trd>voda</trd>
  </trdgrp>
  <trd lng="de">Wasser</trd>
  <trd lng="en">water</trd>
  <trd lng="es">agua</trd>
  <trd lng="fa">&a_A_madda;&a_b;</trd>
  <trd lng="fr">eau</trd>
  <trd lng="hi">&#2346;&#2366;&#2344;&#2368; [<ind>p&#257;n&#299;</ind>]</trd>
  <trd lng="id">air</trd>
  <trd lng="it">acqua</trd>
  <trd lng="ja">&#27700; [<ind>&#12415;&#12378;</ind>]</trd>
  <trd lng="ku">av</trd>
  <trd lng="la">aqua</trd>
  <trd lng="nl">water</trd>
  <trd lng="oc">aiga</trd>
  <trd lng="pl">woda</trd>
  <trd lng="pt">&aacute;gua</trd>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_a;</trd>
  <trd lng="sk">voda</trd>
  <trd lng="sv">vatten</trd>
  <trd lng="tp">telo</trd>
  <trd lng="tr">su</trd>
  <trd lng="zh">&#27700;<klr> [shu&#464;]</klr></trd>
</drv>

<drv mrk="akv.0a">
  <kap><tld/>a</kap>
  <snc mrk="akv.0a.el_akvo">
    <dif>
      El <tld/>o:
      <ekz>
        ili tiel similis unu la alian, kiel du <tld/>aj gutoj
        <fnt><bib>ElektFab</bib>, <lok>Du Fratoj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="br">dourek</trd>
    <trdgrp lng="ca">
      <trd>aqu&oacute;s</trd>,
      <trd>d'aigua</trd>
    </trdgrp>
    <trd lng="en">watery</trd>
    <trd lng="be">&c_v;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_y;</trd>
    <trdgrp lng="fr">
      <trd>d'<ind>eau</ind></trd>,
      <trd>aqueux</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j;</trd>
    <trd lng="nl">waterig</trd>
    <trd lng="sv">vatten-</trd>
  </snc>
  <snc mrk="akv.0a.akvomedia">
    <dif>
      Vivanta en <tld/>o:
      <ekz>
        aligatoroj kaj aliaj <tld/>aj bestoj
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_o;&c_d;&c_n;&c_y;</trd>
    <trd lng="br">-<ind>dour</ind></trd>
    <trd lng="ca">aqu&agrave;tic</trd>
    <trd lng="en">aquatic</trd>
    <trd lng="es">acu&aacute;tico</trd>
    <trd lng="fr">aquatique</trd>
    <trd lng="nl">water-</trd>
    <trd lng="ru">&c_v;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="cs">vodn&iacute;</trd>
  
  <trd lng="pl">wodny</trd>
  <trd lng="pt">aqu&aacute;tico</trd>
  <trd lng="sk">vodn&yacute;</trd>
</drv>

<drv mrk="akv.0i">
  <kap><tld/>i</kap>
  <snc mrk="akv.0i.akvumi">
    <dif>
      <ref tip="dif" cel="akv.0umi.KOMUNE"><tld lit="A"/>umi</ref>:
      <ekz>
        plugadu plue la senfruktan grundon kaj <tld/>u &gcirc;in
        <fnt>
          <aut>Pavel Orsz&aacute;gh Hv&ecaron;zdoslav, trad. Milo&scirc;
          Luka&scirc;</aut>
          <vrk>&Ccirc;e&hcirc;a kaj slovaka antologio, Unua Parto</vrk>
          <lok>La fina parto el la drama eposo "Kain"</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i prenas <tld/>okarafon de la bufedo kaj <tld/>as la
        floron
        <fnt>
          <aut>August Strindberg, trad. Stellan Engholm</aut>
          <vrk>Pasko</vrk>
          <lok>unua akto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">regar</trd>
  <trd lng="cs">zavla&zcaron;ovat</trd>
  <trdgrp lng="de">
    <trd>gie&szlig;en</trd>,
    <trd>w&auml;ssern</trd>
  </trdgrp>
  <trd lng="es">regar</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b; &a_d;&a_A;&a_d;&a_n;</trd>,
    <trd>&a_A_madda;&a_b;&f_y;&a_A;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">arroser</trd>
  <trd lng="id">meng<ind>air</ind>i</trd>
  <trdgrp lng="sk">
    <trd>polia&tcaron;</trd>,
    <trd>zavla&zcaron;i&tcaron;</trd>,
    <trd>zavodni&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">vattna</trd>
</drv>

<drv mrk="akv.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="akv.0ejo.KOMUNE">
    <dif>
      Loko, kie kolekti&gcirc;as <tld/>o:
      <ekz>
        la <tld/>ejoj, kie &gcirc;i restadis, estis fosoj kun malprofunda
        klara 
        <tld/>o
        <fnt>
          <aut>A.-C. M. Meeske, K. J. Rybczy&nacute;ski</aut>,
          <vrk>Telemetriaj studoj pri la mar&ccirc;a testudo Emys
          orbicularis 
          en suda Litovio</vrk>,
          <lok><url
          ref="https://scienca-revuo.info/article/viewFile/1585/1564">
          Scienca Revuo, 2006:2</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#3649;&#3627;&#3621;&#3656;&#3591;&#3609;&#3657;&#3635;</trd>
    <trd lng="pt">corpo de &aacute;gua</trd>
  </snc>

  <trdgrp lng="fr">
    <trd>cours d'eau</trd>,
    <trd>point d'eau</trd>
  </trdgrp>
</drv>

<drv mrk="akv.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="akv.0ero.KOMUNE">
    <dif>
      Tre malgranda kvanto da <tld/>o, ordinare en formo de <ref
      tip="vid" cel="gut.0o">guto</ref>:
      <ekz>
        <ind>
          el malgrandaj <tld/>eroj fari&gcirc;as grandaj riveroj
        </ind>
        <trd lng="fr">les petits ruisseaux font les grandes
        rivi&egrave;res</trd>
        <klr>(per multaj malgrandaj faroj esti&gcirc;as grandaj
        fara&jcirc;oj)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_o;&c_p;&c_l;&c_ja;
  &c_v;&c_a;&c_d;&c_y;</trd>
  <trd lng="ca">gota</trd>
  <trd lng="cs">kapka vody</trd>
  <trd lng="de">Wassertropfen</trd>
  <trd lng="fa">&a_q;&a_T;&a_r;&a_h;&a_hamza_sure;
  &a_A_madda;&a_b;</trd>
  <trd lng="fr">goutte d'eau</trd>
  <trd lng="nl">waterdruppel</trd>
  <trd lng="pl">kropla <ind>wody</ind></trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_l;&c_ja; &c_v;&c_o;&c_d;&c_y;</trd>
  <trd lng="sk">kvapka vody</trd>
  <trd lng="sv">vattendroppe</trd>
</drv>

<drv mrk="akv.A0isto">
  <kap><tld lit="A"/>isto <fnt><bib>PIV2</bib></fnt></kap>
  <mlg>Aqr</mlg>
  <mlg>&#9810;&#65038;</mlg>
  <snc mrk="akv.0isto.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Ekvatora <ref tip="lst" cel="stel.0figuro"
      lst="voko:zodiakaj_signoj" val="11">stelfiguro</ref>
      kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Aquarius</trd>:
      <ekz>
        Aglo <klr>[...]</klr> situas dekstre de la <tld lit="A"/>isto,
        kiu fakte estas Ganimedo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=115083">Aglo
          (konstelacio)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>NGC 6985-1</nom> <klr>[...]</klr> estas galaksio en
        konstelacio <tld lit="A"/>isto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=120331">NGC
          6985-1</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="versx.V0isto">Ver&scirc;isto</ref>
    <trdgrp lng="be">
      <trd>&c_V;&c_a;&c_d;&c_a;&c_l;&c_je;&c_j;</trd>,
      <trd>&c_V;&c_a;&c_d;&c_a;&c_l;&c_ib;&c_w;</trd>
    </trdgrp>
    <trd lng="ca">Aquari</trd>
    <trd lng="de">Wassermann<klr tip="amb"> (Sternbild)</klr></trd>
    <trd lng="fa">&a_d;&a_l;&a_w;<klr tip="amb"> (&a_S;&a_w;&a_r;&a_t;
    &a_f;&a_l;&f_k;&f_y;)</klr></trd>
    <trd lng="fr">Verseau<klr tip="ind"> (constellation)</klr></trd>
    <trd lng="he">&mem;&zayin;&lamed; &dalet;&lamed;&yod;</trd>
    <trd lng="hu">V&iacute;z&ouml;nt&odblac;</trd>
    <trdgrp lng="id">
      <trd>Akuarius</trd>,
      <trd>Delu</trd>,
      <trd>Kumba</trd>
    </trdgrp>
    <trd lng="ja">&#23453;&#29942;&#23470;
    [<ind>&#12411;&#12358;&#12408;&#12356;&#12365;&#12421;&#12358;</ind>]</trd>
    <trd lng="nl">Waterman<klr tip="ind"> (sterrebeeld)</klr></trd>
    <trd lng="pl">wodnik</trd>
    <trd lng="pt">Aqu&aacute;rio<klr> (constela&ccedil;&atilde;o, signo zodiacal)</klr></trd>
    <trd lng="ru">&c_V;&c_o;&c_d;&c_o;&c_l;&c_je;&c_j;</trd>
    <trd lng="sk">Vodn&aacute;r</trd>
    <trd
    lng="th">&#3585;&#3621;&#3640;&#3656;&#3617;&#3604;&#3634;&#3623;&#3588;&#3609;&#3649;&#3610;&#3585;&#3627;&#3617;&#3657;&#3629;&#3609;&#3657;&#3635;</trd>
  </snc>
</drv>

<drv mrk="akv.0umi">
  <kap><tld/>umi</kap>
  <snc mrk="akv.0umi.KOMUNE">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">HOR</uzo>
    <dif>
      Malsekigi per surver&scirc;o a&ubreve; trafluigo de <tld/>o:
      <ekz>
        vi estas kiel &gcirc;ardeno <tld/>umata
        <fnt><bib>MT</bib>, <lok>&Jes; 58:11</lok></fnt>;
      </ekz>
      <ekz>
        tirbestoj <tld/>umis sin en la rivero
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>&ccirc;e Lev Tolstoj en
        Jasnaja Poljana</lok></fnt>;
      </ekz>
      <ekz>
        oni devas <tld/>umi florojn post la sunsubiro.
      </ekz>
    </dif>
    <trdgrp lng="th">
      <trd>&#3619;&#3604;&#3609;&#3657;&#3635;</trd>,
      <trd>&#3626;&#3634;&#3604;&#3609;&#3657;&#3635;</trd>,
      <trd>&#3614;&#3619;&#3617;&#3609;&#3657;&#3635;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_r;&c_a;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>doura<klr> [akvoprovizi]</klr></trd>,
    <trd>doura&ntilde;</trd>
  </trdgrp>
  <trd lng="ca">regar</trd>
  <trd lng="cs">zavla&zcaron;ovat</trd>
  <trdgrp lng="de">
    <trd>gie&szlig;en</trd>,
    <trd>w&auml;ssern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>flush</trd>,
    <trd>rinse</trd>,
    <trd>irrigate</trd>,
    <trd>water</trd>
  </trdgrp>
  <trd lng="es">regar</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b; &a_d;&a_A;&a_d;&a_n;</trd>,
    <trd>&a_A_madda;&a_b;&f_y;&a_A;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>arroser</trd>,
    <trd>irriguer</trd>
  </trdgrp>
  <trd lng="id">meng<ind>air</ind>i</trd>
  <trdgrp lng="nl">
    <trd>gieten</trd>,
    <trd>begieten</trd>,
    <trd>spoelen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nawadnia&cacute;</trd>,
    <trd>podlewa&cacute;</trd>,
    <trd>zrasza&cacute;</trd>,
    <trd>irygowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>banhar</trd>,
    <trd>molhar</trd>,
    <trd>regar</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>polia&tcaron;</trd>,
    <trd>zavla&zcaron;i&tcaron;</trd>,
    <trd>zavodni&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>vattna</trd>,
    <trd>bevattna</trd>
  </trdgrp>
</drv>

<drv mrk="akv.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="akv.0ujo.KOMUNE">
    <dif>
      Loko por ricevi kaj konservi <tld/>on:
      <ekz>
        oni konstruis <tld/>ujon, kiu povis enteni &ccirc;irka&ubreve;
        tri miliardojn da kubaj metroj
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>cisterna</trd>,
      <trd>aljub</trd>
    </trdgrp>
    <trd lng="en">basin</trd>
    <trdgrp lng="fa">
      <trd>&a_A_madda;&a_b;&zwnj;&a_A;&a_n;&a_b;&a_A;&a_r;</trd>,
      <trd>&a_m;&a_kh;&a_z;&a_n; &a_A_madda;&a_b;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>citerne <klr>(d'eau)</klr></trd>,
      <trd>r&eacute;servoir<klr tip="amb"> (d'eau)</klr></trd>
    </trdgrp>
    <trd lng="nl">watervat</trd>
    <trdgrp lng="pl">
      <trd>zbiornik<klr> wodny</klr></trd>,
      <trd>basen<klr> z wod&aogonek;</klr></trd>,
      <trd>cysterna<klr> z wod&aogonek;</klr></trd>,
      <trd>rezerwuar</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>cisterna</trd>,
      <trd>caixa d'&aacute;gua</trd>,
      <trd>reservat&oacute;rio</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_c;&c_i;&c_s;&c_t;&c_je;&c_r;&c_n;&c_a;</trd>,
      <trd>&c_r;&c_je;&c_z;&c_je;&c_r;&c_v;&c_u;&c_a;&c_r;</trd>
    </trdgrp>
    <trd
    lng="th">&#3629;&#3656;&#3634;&#3591;&#3648;&#3585;&#3655;&#3610;&#3609;&#3657;&#3635;</trd>
  </snc>
  <snc mrk="akv.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <tld lit="A"/>a objekto en terkavo, kun modera a&ubreve;
      nenia movo de la <tld/>o:
      <ekz>
        <em>bordo</em>: Tero &ccirc;e la rando de <tld/>ujo (rivero,
        lago a&ubreve; maro)
        <fnt>
          <aut>Laulum</aut>:
          <vrk><url
          ref="http://www.espero.com.cn/05e/05e-04l/03-5e-04l-4.htm">Vortaro
          de Esperanto por lernantoj (B)</url>
          </vrk>
        </fnt>;
      </ekz><ekz>
        <ind>natura <tld/>ujo</ind> <klr>(maro, lago, rivero)</klr>
        <trd lng="ru">&c_je;&c_s;&c_t;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;
        &c_v;&c_o;&c_d;&c_o;&c_jo;&c_m;</trd>
      </ekz>
    </dif>
    <trd lng="ca">massa d'aigua</trd>
    <trdgrp lng="de">
      <trd>Wasserbecken</trd>,
      <trd>Gew&auml;sser</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>water body</trd>,
      <trd>reservoir</trd>
    </trdgrp>
    <trd lng="fa">&a_A_madda;&a_b;&f_g;&f_y;&a_r;</trd>
    <trdgrp lng="nl">
      <trd>bekken<klr tip="ind"> (waterbekken)</klr></trd>,
      <trd>waterbekken</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_o;&c_d;&c_o;&c_jo;&c_m;</trd>
  </snc>
  <trd lng="cs">vodojem</trd>
  <trd lng="sk">vodojem</trd>
</drv>

<drv mrk="akv.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="akv.el0igxi.KOMUNE">
    <dif>
      Eliri, aperi el <tld/>o:
      <ekz>
        en la mezo de Nilo oni renkontadis insulojn, kiuj, &scirc;ajnis,
        hiera&ubreve; el<tld/>i&gcirc;is
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 8a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="th">
      <trd>&#3586;&#3638;&#3657;&#3609;&#3617;&#3634;&#3592;&#3634;&#3585;&#3609;&#3657;&#3635;</trd>,
      <trd>&#3650;&#3614;&#3621;&#3656;&#3617;&#3634;&#3592;&#3634;&#3585;&#3609;&#3657;&#3635;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">emergir</trd>
  <trd lng="fr">&eacute;merger</trd>
  <trd lng="nl">uit het <ind>water</ind> komen</trd>
  <trdgrp lng="pl">
    <trd>wynurza&cacute; si&eogonek;<klr> z wody</klr></trd>
    <trd>wy&lstroke;ania&cacute; si&eogonek;<klr> z wody</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_s;&c_p;&c_l;&c_y;&c_t;&c_mol;</trd>
</drv>

<drv mrk="akv.fal0o">
  <kap>fal<tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="akv.fal0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="dif" cel="fal.akvo0o.GEOG">Akvofalo</ref>:
      <ekz>
        li rakontis<klr>[&#8230;]</klr> pri la fal<tld/>oj, kiuj
        &scirc;a&ubreve;mante falegas malsupren kun bruo simila al frapoj
