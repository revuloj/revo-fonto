        <fnt><bib>Fab2</bib> <lok>monteto de elfoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_a;&c_s;&c_p;&c_a;&c_d;</trd>
  <trd lng="bg">&c_v;&c_o;&c_d;&c_o;&c_p;&c_a;&c_d;</trd>
