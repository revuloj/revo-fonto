  <trdgrp lng="en">
    <trd>waterfall</trd>,
    <trd>cascade</trd>
  </trdgrp>
  <trd lng="es">cascada</trd>
  <trd lng="fa">&a_A_madda;&a_b;&a_sh;&a_A;&a_r;</trd>
  <trdgrp lng="fr">
