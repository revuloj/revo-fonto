<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: restauxr.xml,v 1.6 2024/04/13 20:15:53 revo Exp $">
<kap>
  <rad>resta&ubreve;r</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="restauxr.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="restauxr.0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="star.re0igi.KOMUNE">Restarigi</ref>,
      <ref tip="vid" cel="ripar.0i.KOMUNE">ripari</ref>,
      rekonduki al origina, bona stato:
      <ekz>
        &scirc;i <klr>...</klr> &scirc;an&gcirc;is la meblojn, <tld/>is la &gcirc;ardenon, 
        plantis novajn florojn
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_s;&c_t;&c_a;&c_w;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">wiederherstellen</trd>
  <trdgrp lng="en">
    <trd>restore</trd>
    <trd>recover</trd>
  </trdgrp>
  <trd lng="fr">restaurer</trd>
  <trd lng="nl">herstellen</trd>
  <trdgrp lng="pl">
    <trd>restaurowa&cacute;</trd>,
    <trd>odnawia&cacute;</trd>,
    <trd>od&sacute;wie&zdot;a&cacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&#x43D;&#x43E;&c_v;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_je;&c_s;&c_t;&c_a;&c_v;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4FEE;&#x590D; <pr>xi&#x16B;f&#xF9;</pr></trd>,
    <trd>&#x4FEE;&#x5FA9; <pr>xi&#x16B;f&#xF9;</pr></trd>,
    <trd>&#x6062;&#x590D; <pr>hu&#x12B;f&#xF9;</pr></trd>,
    <trd>&#x6062;&#x5FA9; <pr>hu&#x12B;f&#xF9;</pr></trd>,
    <trd>&#x6574;&#x4FEE; <pr>zh&#x11B;ngxi&#x16B;</pr></trd>
  </trdgrp>
</drv>
</art>

