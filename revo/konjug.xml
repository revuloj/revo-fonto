<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konjug</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="konjug.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <ref tip="dif" cel="konjug1.0i">konjugacii.</ref>
  </snc>
  <trd lng="nl">vervoeging</trd>
</drv>

<drv mrk="konjug.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="bild.0igo.MAT">Bildigo</ref>, kiu &jcirc;etas
      objekton al &gcirc;ia <ref cel="konjug.0ito.MAT"><tld/>ito</ref>:
      <ekz>
        la <tld/>o de kompleksoj estas
        <ref tip="super" cel="involu1.0o">involucio</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Konjugation</trd>
  <trd lng="en">conjugation</trd>
  <trd lng="fr">conjugaison<klr tip="ind"> (math.)</klr></trd>
  <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="konjug.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="konjug.0ajxo.MAT">
    <ref tip="dif" cel="konjug.0ito.MAT"><tld/>ito.</ref>
  </snc>
</drv>

<drv mrk="konjug.0ita">
  <kap><tld/>ita</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="konjug.0ita.MAT">
    <subsnc mrk="konjug.0ita.komplekso">
      <fnt><bib>MatVort</bib><lok>2326</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="komple.0o.MAT">komplekso</ref>, rilate al
        alia)</klr>
        Havanta saman <ref cel="part.reela0o">reelan parton</ref>, sed
        <ref cel="sign.kontraux0a">kontra&ubreve;signan</ref>
        <ref cel="part.imaginara0o">imaginaran parton</ref>:
        <ekz>
          <frm>1+2.<k>i</k></frm> estas <tld/>ita 
          rilate al <frm>1-2.<k>i</k></frm>;
        </ekz>
        <ekz>
          <tld/>ita funkcio <klr>(kies valoroj estas <tld/>itaj rilate al tiuj
          de la originalo)</klr>;
        </ekz>
        <ekz>
          <tld/>ita matrico <klr>(matrico, kies elementoj estas <tld/>itaj
          rilate al tiuj de la originalo)</klr>;
        </ekz>
        <ekz>
          la sumo de du <tld/>itaj kompleksoj estas reela;
        </ekz>
        <ekz>
          la produto de du <tld/>itaj kompleksoj egalas al kvadrato de ilia
          komuna <ref cel="modul.0o.komplekso">modulo</ref>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="konjug.0ita.subgrupo">
      <dif>
        <klr>(p.p. <ref cel="grup.sub0o.MAT">subgrupo</ref>
        <frm><g>S</g></frm>, rilate al subgrupo <frm><g>T</g></frm>)</klr>
        Tia, ke ekzistas <ref cel="auxtom2.interna0o">interna
        a&ubreve;tomorfio</ref> de <frm><g>S</g></frm> al <frm><g>T</g></frm>.
      </dif>
    </subsnc>
    <rim>
      Ekzistas pluraj aliaj matematikaj sencoj por &ccirc;i tiu adjektivo.
    </rim>
    <trd lng="en">conjugate</trd>
    <trd lng="de">konjugiert</trd>
    <trd lng="fr">conjugu&eacute;</trd>
    <trd lng="pl">sprz&eogonek;&zdot;ony</trd>
    <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>

<drv mrk="konjug.0ito">
  <kap><tld/>ito</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="konjug.0ito.MAT">
    <dif>
      <tld lit="K"/>ita objekto.
    </dif>
    <subsnc mrk="konjug.0ito.komplekso">
      <fnt><bib>PIV1</bib></fnt>
      <dif>
        <klr>(de <ref cel="komple.0o.MAT">komplekso</ref>)</klr>
        &Gcirc;ia <ref cel="konjug.0ita.komplekso"><tld/>ita
        <sncref ref="konjug.0ita.komplekso"/></ref> nombro:
        <ekz>
          la <tld/>iton de <frm><k>z</k></frm> oni kutime signas per 
          <frm><k>z</k></frm> kun supra horizontala
          streko, nome <frm><k>z&#x305;</k></frm> 
          <klr>(legu: zo trabo)</klr>;
        </ekz>
        <ekz>
          &ccirc;iu komplekso egalas al la <tld/>ito de sia <tld/>ito;
        </ekz>
        <ekz>
          <tld/>ito de matrico,
        </ekz>
        <ekz>
          de funkcio.
        </ekz>
      </dif>
      <trd lng="en">conjugate complex <ind>number</ind></trd>
      <trd lng="de">konjugierte komplexe <ind>Zahl</ind></trd>
      <trd lng="fr">nombre complexe conjugu&eacute;</trd>
      <trd lng="pl">liczba zespolona sprz&eogonek;&zdot;ona</trd>
      <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_o;&c_je; &c_k;&c_o;&c_m;&c_p;&c_l;&c_je;&c_k;&c_s;&c_n;&c_o;&c_je; <ind>&c_ch;&c_i;&c_s;&c_l;&c_o;</ind></trd>
    </subsnc>
    <subsnc mrk="konjug.0ito.subgrupo">
      <dif>
        <klr>(de <ref cel="grup.sub0o.MAT">subgrupo</ref>)</klr>
        Subgrupo, <ref cel="konjug.0ita.subgrupo"><tld/>ita
        <sncref ref="konjug.0ita.komplekso"/></ref> rilate al &gcirc;i.
      </dif>
      <trd lng="de">konjugierte <ind>Untergruppe</ind></trd>
      <trd lng="en">conjugate <ind>subgroup</ind></trd>
      <trd lng="fr">sous-groupe conjug&eacute;</trd>
      <trd lng="ru">&c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_a;&c_ja; <ind>&c_p;&c_o;&c_d;&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;</ind></trd>
    </subsnc>
    <rim>
      Apud <ctl><tld/>ito</ctl> kaj <ctl><tld/>a&jcirc;o</ctl>, kiu
      anstata&ubreve;as la unuan en <fnt><bib>PIV2</bib></fnt>, iuj provis
      enkonduki <ctl><tld/>o</ctl> por la sama senco, surbaze de rezonado
      simila al tiu en <fnt><bib>MatStokTerm</bib><lok>p. 97</lok></fnt>. Prave,
      ke la verbo <ctl><tld/>i</ctl> ne havas fakan sencon, sed tiu, kiun oni
      povas etimologie rekonstrui estas de la tipo <ctl>meti en rilaton du iel
      simetriajn objektojn</ctl>. Eblas do diri, ke <frm><k>z</k></frm> kaj 
      <frm><k>z&#x305;</k></frm> estas <tld/>itaj nombroj, do <tld/>itoj 
      (certe ne: <tld/>a&jcirc;oj). Tion konsiderante, la malnova formo 
      <ctl><tld/>ito</ctl> estas logika kaj restas la&ubreve; ni preferinda.
    </rim>
  </snc>
</drv>

<drv mrk="konjug.mem0ita">
  <kap>mem<tld/>ita</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="konjug.mem0ita.subgrupo">
    <dif>
      <klr>(p.p. <ref cel="grup.sub0o.MAT">subgrupo</ref>)</klr>
      <ref cel="konjug.0ita.subgrupo"><tld lit="K"/>ita</ref> kun si mem.
    </dif>
    <ref tip="dif" cel="invari.0a.subgrupo">invarianta <sncref/>.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>invariant<klr tip="amb"> (-e Untergruppe)</klr></trd>,
    <trd>Normal-<klr tip="amb">(teiler)</klr></trd>,
    <trd>selbst-konjugiert<klr tip="amb"> (-e Untergruppe)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>invariant<klr tip="amb"> (subgroup)</klr></trd>,
    <trd>normal<klr tip="amb"> (subgroup)</klr></trd>,
    <trd>distinguished<klr tip="amb"> (subgroup)</klr></trd>,
    <trd>self-conjugate<klr tip="amb"> (subgroup)</klr></trd>,
    <trd>normal<klr tip="amb"> (divisor)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd><klr tip="ind">(sous-groupe) </klr>invariant</trd>,
    <trd><klr tip="ind">(sous-groupe) </klr>normal</trd>,
    <trd><klr tip="ind">(sous-groupe) </klr>distingu&eacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd><klr tip="amb">(podgrupa) </klr>niezmiennicza</trd>,
    <trd><klr tip="amb">(podgrupa) </klr>normalna</trd>,
    <trd><klr tip="amb">(dzielnik) </klr>normalny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_v;&c_a;&c_r;&c_i;&c_a;&c_n;&c_t;&c_n;&c_a;&c_ja;
    <klr tip="amb"> (&c_p;&c_o;&c_d;&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;)</klr></trd>,
    <trd>&c_n;&c_o;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;
    <klr tip="amb"> (&c_p;&c_o;&c_d;&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;)</klr></trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_s;&c_o;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_a;&c_ja;
    <klr tip="amb"> (&c_p;&c_o;&c_d;&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;)</klr></trd>,
    <trd>&c_n;&c_o;&c_r;&c_m;&c_a;&c_l;&c_n;&c_y;&c_j;<klr tip="amb"> (&c_d;&c_je;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;)</klr> </trd>
  </trdgrp>
</drv>
</art>
