<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>epigraf</rad>/o
</kap>
<drv mrk="epigra.0o">
<kap><tld/>o</kap>
<snc num="1">
<uzo tip="fak">ARKI</uzo>
<dif>
Mallonga surskribo, metita sur konstrua&jcirc;o por indiki &gcirc;ian destinon,
daton ktp. 
</dif>
  <trd lng="de">Inschrift auf Denkm&auml;lern</trd>
  <trd lng="hu">felirat<klr tip="amb"> (&eacute;p&uuml;leten)</klr></trd>
</snc>
<snc num="2">
<dif>
Devizo.
</dif>
<refgrp tip="vid">
<ref cel="senten.0o">sentenco</ref>,
<ref cel="mot.0o">moto</ref>.
</refgrp>
  <trdgrp lng="de">
     <trd>Inschrift</trd>,
     <trd>Motto</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>jelige</trd>,
     <trd>jelmondat</trd>
  </trdgrp>
<trd lng="ru">&c_e;&c_p;&c_i;&c_g;&c_r;&c_a;&c_f;</trd>
</snc>
  <trd lng="de">Epigraph</trd>
<trd lng="es">ep&iacute;grafe</trd>
<trd lng="fr">&eacute;pigraphe</trd>
<trd lng="hu">epigr&aacute;f</trd>
<trd lng="it">epigrafe</trd>
</drv>
</art>
