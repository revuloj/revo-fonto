<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bram1.xml,v 1.4 2023/08/31 08:48:53 revo Exp $">
<kap>
   <rad>bram</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="bram1.0o">
  <kap><tld/>o</kap>
  <snc mrk="bram1.0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <dif>
      Tria, plej supera parto de
      <ref tip="malprt" cel="mast.0o.MAR">masto</ref>:
      <ekz>
        <tld/>o estas la dua plilongigo de masto supre de la topmasto (la unua plilongigo
        supre de la topo).
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/67/Balclutha_fore_topgallant_mast.jpg/413px-Balclutha_fore_topgallant_mast.jpg" prm="CC BY-SA 3.0"><tld/>o
        <fnt>
          <aut>Stan Shebs, propra verko</aut>
          <lok><url ref="&WCW;/File:Balclutha_fore_topgallant_mast.jpg">&WCW;/File:Balclutha_fore_topgallant_mast.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Bram</trd>,
      <trd>Bramstenge</trd>
    </trdgrp>
  </snc>
  <rim>
    Fundamente <ref tip="hom" cel="bram.0o">bramo</ref>
    estas la nomo de fi&scirc;specio.
  </rim>
  <trd lng="be">&c_b;&c_r;&c_a;&c_m;</trd>
  <trd lng="en">topgallant</trd>
  <trd lng="fr">ton<klr> (mar.)</klr></trd>
  <trd lng="hu">&aacute;rbocsud&aacute;r</trd>
  <trd lng="ja">&#x30C8;&#x30B2;&#x30EB;&#x30F3;&#x30DE;&#x30B9;&#x30C8;</trd>
  <trd lng="nl">bramsteng</trd>
  <trd lng="pl">bram</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_m;</trd>
</drv>
</art>

