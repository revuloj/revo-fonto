<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>Eden</rad>/o
</kap>
<drv mrk="eden.0o">
  <kap><tld/>o, <var><kap><tld/></kap></var></kap>
  <snc mrk="eden.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Plezurplena &gcirc;ardeno kie, la&ubreve; la Biblio,
      Dio lokis Adamon kaj Evan:
      <ekz>
        la Eternulo plantis &gcirc;ardenon en <tld/> en la Oriento, kaj Li
        metis tien la homon, kiun Li kreis
        <fnt><bib>MT</bib>, <lok>&Gen; 2:8</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;ajnas al mi, ke mi estas en <tld/>o, kaj &gcirc;uas tiel viglan
        internan plezuron, kiel se mi estus la plej feli&ccirc;a el la
        mortemuloj
        <fnt><bib>Revadoj</bib>, <lok>Oka promenado</lok></fnt>;
      </ekz>
      <ekz>
        la Eternulo eligis lin el la <tld/>a &gcirc;ardeno
        <fnt><bib>MT</bib>, <lok>&Gen; 3:23</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <ind><tld/>a</ind>j arboj, kiuj estas en la &gcirc;ardeno
        de Dio, enviis &gcirc;in
        <fnt><bib>MT</bib>, <lok>&Jeh; 31:9</lok></fnt>.
        <trd lng="fr">&eacute;d&eacute;nique</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_E;&c_d;&c_e;&c_m;</trd>,
    <trd>&c_R;&c_a;&c_j;</trd>,
    <trd>&c_R;&c_a;&c_j;&c_s;&c_k;&c_ib; &c_s;&c_a;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Eden</trd>,
    <trd>Paradiesgarten</trd>
  </trdgrp>
  <trd lng="es">Ed&eacute;n</trd>
  <trdgrp lng="fr">
    <trd>&Eacute;den</trd>,
    <trd>jardin d'&Eacute;den</trd>,
    <trd>Paradis terrestre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Paradicsom</trd>,
    <trd>&Eacute;den kertje<klr tip="ind"> (Biblia)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Hof van <ind>Eden</ind></trd>,
    <trd>Paradijs</trd>
  </trdgrp>
  <trd lng="sv">Eden</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_j;</trd>,
    <trd>&c_e;&c_d;&c_je;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="eden.e0o">
  <kap><tld lit="e"/>o</kap>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plezurplena loko.
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_j;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_a;&c_d;&c_y;&c_z;</trd>
    </trdgrp>
    <trd lng="de">Paradies</trd>
    <trd lng="es">ede&eacute;n</trd>
    <trdgrp lng="fr">
      <trd>&eacute;den</trd>,
      <trd>paradis</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>f&ouml;ldi <ind>paradicsom</ind></trd>,
      <trd>&eacute;den(kert)</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>paradijs</trd>
    </trdgrp>
    <trd lng="sv">paradis</trd>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_j;</trd>,
    <trd>&c_e;&c_d;&c_je;&c_m;</trd>,
    <trd>&c_r;&c_a;&c_j;&c_s;&c_k;&c_i;&c_je; &c_k;&c_u;&c_shch;&c_i;</trd>
  </trdgrp>
</drv>
</art>
