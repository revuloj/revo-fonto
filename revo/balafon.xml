<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>balafon</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="balafon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.frap0o">Frapinstrumento</ref>
      el serio de ku&scirc;aj lignolamenoj frapataj per bastonetoj,
      kun subaj resonaj kalabasoj provizitaj per zumaj membranetoj,
      populara en centra kaj okcidenta Afriko.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="ksilof.0o">ksilofono</ref>
  </refgrp>
  <trd lng="de">Balafon</trd>
  <trd lng="en">balaphone</trd>
  <trdgrp lng="fr">
    <trd>balafon</trd>
  </trdgrp>
  <trd lng="hu">balafon</trd>
  <trd lng="nl">balafoon</trd>
</drv>
</art>

