<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>balafon</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="balafon.0o">
  <kap><tld/>o</kap>
  <snc mrk="balafon.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.frap0o">Frapinstrumento</ref>
      el serio de lignolamenoj frapataj per bastonetoj, kun kalabasoj kiel
      sonkestoj, populara en centra kaj okcidenta Afriko:
      <ekz>
        <tld/>o, pigmeokanto, birdalvokoj kaj akvobruo...
        <fnt>
        <aut>O. Briand</aut>,
        <vrk><url ref="http://kantoturdo.free.fr/">Kantoturdo</url></vrk>,
        <lok>[vidita je 2006-03-09]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="ksilof.0o">ksilofono</ref>
  </refgrp>
  <trd lng="de">Balafon</trd>
  <trd lng="en">balaphone</trd>
  <trdgrp lng="fr">
    <trd>balafon</trd>
  </trdgrp>
  <trd lng="hu">balafon</trd>
  <trd lng="nl">balafoon</trd>
</drv>
</art>
