<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>beton</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="beton.0o">
<kap><tld/>o</kap>
<uzo tip="fak">KON</uzo>
<snc>
<dif>
Cemento miksita kun &scirc;tonetoj k sablo.  
</dif>
</snc>
<trd lng="be">&c_b;&c_e;&c_t;&c_o;&c_n;</trd>
<trd lng="de">Beton</trd>
<trd lng="en">concrete</trd>
<trd lng="fr">b&eacute;ton</trd>
<trd lng="hu">beton</trd>
<trd lng="ru">&c_b;&c_je;&c_t;&c_o;&c_n;</trd>
<trd lng="sk">bet&oacute;n</trd>
</drv>
<drv mrk="beton.fer0osubferigita0oarmita0o">
<kap>fer<tld/>o, subferigita <tld/>o, armita <tld/>o</kap>
<uzo tip="fak">KON</uzo>
<snc>
<dif>
<tld lit="B"/>o fortikigita per internaj feraj stangoj, tre uzata por la 
nuntempaj konstruoj.
</dif>
<trd lng="be">&c_zh;&c_a;&c_l;&c_je;&c_z;&c_a;&c_b;&c_e;&c_t;&c_o;&c_n;</trd>
<trdgrp lng="de">
   <trd>Eisenbeton</trd>,
   <trd>Stahlbeton</trd>,
   <trd>armierter Beton</trd></trdgrp>
</snc>
<trd lng="en">reinforced <ind>concrete</ind></trd>
<trd lng="fr">b&eacute;ton arm&eacute;</trd>
<trd lng="hu">vasbeton</trd>
<trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;&c_b;&c_je;&c_t;&c_o;&c_n;</trd>
<trd lng="sk">&zcaron;elezobet&oacute;n</trd>
</drv>
</art>
