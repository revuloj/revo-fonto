<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: beton.xml,v 1.29 2023/10/29 19:11:01 revo Exp $">
<kap>
  <ofc>9</ofc> <rad>beton</rad>/o
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="beton.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="beton.0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
     Konstrumaterialo, miksa&jcirc;o el sablo, &scirc;tonetoj, 
     liga&jcirc;o pliofte <ref tip="prt" cel="cement.0o">cementa</ref>,
     kaj akvo:
     <ekz>
        speciala fosma&scirc;ino formetas ter-tavolon, poste &ccirc;ion firmigas per 30 cm dika sur&scirc;prucigita
        <tld/>o kaj &scirc;talreto
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2005/008408.php">Pli da metroo-metroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        stratoj pavimitaj per <tld/>o, <klr>[&#x2026;]</klr> enhavas kelke da interspacoj plenigitaj per
        gudro
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011332.php">Dilati&gcirc;o, kuntiri&gcirc;o 
         ... kaj eksperimento por infana amuzi&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        metropoli tra spaliroj da <tld/>oj kaj cementoj
        <fnt>
          <aut>Grigori Arosev</aut>
            <vrk><url ref="&Monato;/2000/007198.php">Poezio sed ne poemoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 28</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x4E09;&#x5408;&#x571F; <pr>s&#x101;nh&#xE9;t&#x1D4;</pr></trd>
</snc>
  <trd lng="be">&c_b;&c_e;&c_t;&c_o;&c_n;</trd>
  <trd lng="ca">formig&oacute;</trd>
  <trd lng="de">Beton</trd>
  <trd lng="en">concrete</trd>
  <trd lng="es">hormig&oacute;n</trd>
  <trd lng="fr">b&eacute;ton</trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B5;&#x5D8;&#x5D5;&#x5B9;&#x5DF;</trd>
  <trd lng="hu">beton</trd>
  <trd lng="ja">&#x30B3;&#x30F3;&#x30AF;&#x30EA;&#x30FC;&#x30C8;  <pr>&#x3053;&#x3093;&#x304F;&#x308A;&#x30FC;&#x3068;</pr></trd>
  <trd lng="pl">beton</trd>
  <trd lng="ru">&c_b;&c_je;&c_t;&c_o;&c_n;</trd>
  <trd lng="sk">bet&oacute;n</trd>
  <trd lng="uk">&c_b;&c_je;&c_t;&#x43E;&#x43D;</trd>
</drv>

<drv mrk="beton.sxtal0o">
  <kap>&scirc;tal<tld/>o, 
  <var><kap>armita <tld/>o</kap></var></kap>
  <snc mrk="beton.sxtal0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      <ref tip="super" cel="beton.0o.KON"><tld lit="B"/>o</ref>
      fortikigita per internaj feraj stangoj, tre uzata
      por la nuntempaj konstruoj:
      <ekz>
        eblas konstrui iom aparte el &scirc;tal<tld/>o la &scirc;tuparejojn
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010952.php">Lige al ligno</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        specoj de <tld/>o <klr>[&#x2026;]</klr>
        armita <tld/>o (kun armaturo, al kiu oni atribuis statikan funkcion), 
        &scirc;tal<tld/>o (armita <tld/>o, kiu ne estas anta&ubreve;stre&ccirc;ita <klr>[&#x2026;]</klr>)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8964">Specoj de betono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_zh;&c_a;&c_l;&c_je;&c_z;&c_a;&c_b;&c_e;&c_t;&c_o;&c_n;</trd>
    <trd lng="ca">formig&oacute; armat</trd>
    <trdgrp lng="de">
      <trd>Stahlbeton</trd>,
      <trd>armierter Beton</trd>,
    </trdgrp>
    <trd lng="en">reinforced <ind>concrete</ind></trd>
    <trd lng="es">hormig&oacute;n armado</trd>
    <trd lng="fr">b&eacute;ton arm&eacute;</trd>
    <trd lng="hu">vasbeton</trd>
    <trdgrp lng="pl">
      <trd>&zdot;elbet</trd>,
      <trd>&zdot;elbeton</trd>,
      <trd>&zdot;elazobeton</trd>,
      <trd>beton zbrojony</trd>
    </trdgrp>
    <trd lng="ru">&c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;&c_b;&c_je;&c_t;&c_o;&c_n;</trd>
    <trd lng="sk">&zcaron;elezobet&oacute;n</trd>
    <trdgrp lng="zh">
    <trd>&#x92FC;&#x7B4B;&#x6C34;&#x6CE5; <pr>g&#x101;ngj&#x12B;nshu&#x1D0;n&#xED;</pr></trd>,
    <trd>&#x92FC;&#x7B4B;&#x6DF7;&#x51DD;&#x571F; <pr>g&#x101;ngj&#x12B;nh&#xF9;nn&#xED;ngt&#x1D4;</pr></trd>,
    <trd>&#x92FC;&#x7B4B;&#x783C; <pr>g&#x101;ngj&#x12B;nt&#xF3;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>&zcaron;elezobeton</trd>,
    <trd>&zcaron;elezov&yacute; beton</trd>
  </trdgrp>
  <trd lng="he">&#x5D1;&#x5D8;&#x5D5;&#x5DF; &#x5DE;&#x5D6;&#x5D5;&#x5D9;&#x5DF;</trd>
  <trd lng="sk">&zcaron;elezobet&oacute;n</trd>
  <trd lng="uk">&c_z;&c_a;&#x43B;&c_ib;&c_z;&#x43E;&c_b;&c_je;&c_t;&#x43E;&#x43D;</trd>
</drv>

<drv mrk="beton.prem0o">
  <kap>prem<tld/>o</kap>
  <snc mrk="beton.prem0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      <ref tip="super" cel="beton.0o.KON"><tld lit="B"/>o</ref>
      fortege premita dum farado
      kaj dank' al tio pli rezista al ekstera premo a&ubreve; pezo:
      <ekz>
        <nom>Zaleski-Zamenhof</nom> ... fine anka&ubreve; doktori&gcirc;is per disertacio
	pri prem<tld/>o, kiu estis tiam teknologia nova&jcirc;o
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://www.liberafolio.org/2019/10/11/esperanto-plu-vivos-en-la-familio-de-zamenhof/">
          Esperanto plu vivos en la familio de Zamenhof</url></vrk>
          <lok>libera folio, 2019-10-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_p;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_je; &c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_a;&c_n;&c_y; <ind>&c_b;&c_e;&c_t;&c_o;&c_n;</ind></trd>
  <trd lng="fr">b&eacute;ton pr&eacute;contraint</trd>
</drv>
</art>
