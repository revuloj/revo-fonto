<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kortizol.xml,v 1.4 2018/09/04 10:10:17 revo Exp $">
<kap>
  <rad>kortizol</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="kortizol.0o">
  <kap><tld/>o</kap>
  <snc mrk="kortizol.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="hormon.0o.FIZL">Hormono</ref>
      produktata de la surrenaj glandoj, kiu regas la konstantecon
      de diversaj fiziologiaj substancoj:
      <ekz>
        <tld/>o malplifortigas la imunan sistemon
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="https://eo.wikipedia.org/wiki/Kortizolo">
          Kortizolo</url>, 2014-07-17</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_r;&c_t;&c_i;&c_z;&c_o;&c_l;</trd>
  <trd lng="en">cortisol</trd>
  <trd lng="es">cortisol</trd>
  <trd lng="fa">&f_k;&a_w;&a_r;&a_t;&f_y;&a_z;&a_w;&a_l;</trd>
  <trd lng="fr">cortisol</trd>
  <trd lng="it">cortisolo</trd>
  <trd lng="nl">cortisol</trd>
  <trd lng="pl">kortyzol</trd>     
  <trd lng="ro">cortizol</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_t;&c_i;&c_z;&c_o;&c_l;</trd>
  <trd lng="uk">&c_k;&c_o;&c_r;&c_t;&c_i;&c_z;&c_o;&c_l;</trd>
</drv>
</art>

