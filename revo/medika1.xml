<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>medikament</rad>/o
</kap>
<drv mrk="medika1.0o">
  <kap><tld/>o</kap>
  <snc mrk="medika1.0o.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Drogo uzata kiel <ref tip="super" cel="kurac.0ilo">kuracilo</ref>:
      <ekz>
        &scirc;i akceptis de liaj manoj kuleron da <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 23a, p. 148a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="farmac.0ajxo">farmacia&jcirc;o</ref>,
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_d;&c_y;&c_k;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_l;&c_je;&c_k;&c_ib;</trd>,
    <trd>&c_l;&c_ja;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>        
  <trd lng="br">louzo&ugrave;<klr tip="ind"> (apotikerezh)</klr></trd>
  <trdgrp lng="ca">
    <trd>medicament</trd>,
    <trd>medecina</trd>
  </trdgrp>
  <trd lng="cs">l&eacute;k</trd>
  <trdgrp lng="de">
    <trd>Medikament</trd>,
    <trd>Arznei</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>medicine</trd>,
    <trd>medication</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>medicamento</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&eacute;dicament</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&oacute;gyszer</trd>,
    <trd>medic&iacute;na</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>geneesmiddel</trd>,
    <trd>medicijn</trd>,
    <trd>medicament</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>medicamento</trd>,
    <trd>rem&eacute;dio</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_m;&c_je;&c_d;&c_i;&c_k;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
</art>
