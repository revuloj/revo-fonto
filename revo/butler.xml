<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: butler.xml,v 1.15 2025/05/17 10:47:05 revo Exp $">
<kap>
  <rad>butler</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>

<drv mrk="butler.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="butler.0o.KUI">
    <dif>
      <ref tip="dif" cel="tabl.0estro.KUI">Tablestro</ref>:
      <ekz>
        alia viro, funkcianta kiel <tld/>o, metis pladon da fruktoj
        anta&ubreve; la gubernatoro
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 47a, p.
        651a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kel.0isto">kelestro</ref>,
      <ref cel="versx.0isto">ver&scirc;isto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="cs">vrchn&iacute; stoln&iacute;k</trd>
  <trdgrp lng="de">
    <trd>Butler</trd>,
    <trd>Diener</trd>,
    <trd>Haushofmeister</trd>,
    <trd>Kellermeister</trd>
  </trdgrp>
  <trd lng="en">butler</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&f_y;&a_sh;&a_kh;&a_d;&a_m;&a_t;</trd>,
    <trd>&a_kh;&a_w;&a_A;&a_n;&a_s;&a_A;&a_l;&a_A;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ma&icirc;tre d'h&ocirc;tel</trd>,
    <trd>majordome<klr> (ma&icirc;tre d'h&ocirc;tel)</klr></trd>
  </trdgrp>
  <trd lng="it">maggiordomo</trd>
  <trd lng="nl">butler</trd>
  <trd lng="pl">s&lstroke;u&zdot;&aogonek;cy</trd>
  <trd lng="ro">servitor</trd>
  <trd lng="sk">vrchn&yacute; stoln&iacute;k</trd>
  <trd lng="zh">&#20653;&#20154;&#38936;&#29677; <pr>y&omacron;ngr&eacute;nl&#464;ngb&amacron;n</pr></trd>
</drv>
</art>

