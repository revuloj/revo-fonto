<vortaro>
<art mrk="$Id$">
<kap>
  <rad>butler</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="butler.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="serv.0isto">Servisto</ref> oficanta kiel
      <ref tip="super" cel="festen.0estro">festenestro</ref> en nobela a&ubreve;
      ri&ccirc;a domo:
      <ekz>
        alia viro, funkcianta kiel <tld/>o, metis pladon da fruktoj anta&ubreve;
        la gubernatoro
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 47a, p.
        651a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">butler</trd>
  <trdgrp lng="fr">
    <trd>maitre d'h&ocirc;tel</trd>,
    <trd>majordome<klr> (maitre d'hotel)</klr></trd>
  </trdgrp>
</drv>
</art>

