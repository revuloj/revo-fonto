<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>didaktik</rad>/o
</kap>
<drv mrk="didakt.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="pedago1.0o">Pedagogio</ref>:
      <ekz>
        <tld/>a poemo estas versa verko, en kiu oni klarigas teknikan temon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Didaktik</trd>,
     <trd>Unterrichtslehre</trd>
  </trdgrp>
  <trd lng="es">did&aacute;ctica</trd>
  <trdgrp lng="fr">
    <trd>didactique<klr> (subst.)</klr></trd>,
    <trd>p&eacute;dagogie</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_d;&c_a;&c_k;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="didakt.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="instru.pri0a">Priinstrua</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>didaktisch</trd>
  </trdgrp>
  <trd lng="es">did&aacute;ctico</trd>
</drv>
</art>
