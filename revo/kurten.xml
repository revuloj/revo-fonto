<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kurten.xml,v 1.33 2022/11/27 22:28:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kurten</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kurten.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kurten.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Peco da &scirc;tofo pendigita por ka&scirc;i a&ubreve; &scirc;irmi ion:
      <ekz>
        meti <tld/>on anta&ubreve; pentra&jcirc;on;
      </ekz>
      <ekz>
        <tld/>oj de fenestro, lito, alkovo;
      </ekz>
      <ekz>
        levi, fermtiri la <tld/>on.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_f;&c_ib;&c_r;&c_a;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_g;&c_a;&c_r;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>curtain</trd>,
      <trd>screen</trd>,
      <trd>veil</trd>
    </trdgrp>
    <trd lng="es">cortina</trd>
    <trd lng="pl">firanka</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_n;&c_a;&c_v;&c_je;&c_s;&c_k;&c_a;</trd>,
      <trd>&c_g;&c_a;&c_r;&c_d;&c_i;&c_n;&c_a;</trd>,
      <trd>&c_sh;&c_t;&c_o;&c_r;&c_a;</trd>,
      <trd>&c_p;&c_o;&c_l;&c_o;&c_g;</trd>
    </trdgrp>
    <trd lng="sv">gardin</trd>
  </snc>
  <snc mrk="kurten.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Granda <tld/>o, kiu ka&scirc;as scenejon de teatro, dum oni ne ludas;
      <ref tip="sin" cel="kurten.0ego.TEA"><tld/>ego</ref>:
      <ekz>
        alveni anta&ubreve; la levi&gcirc;o de la <tld/>o.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_s;&c_l;&c_o;&c_n;&c_a;</trd>
    <trd lng="en">curtain</trd>
    <trd lng="es">tel&oacute;n</trd>
    <trd lng="pl">kurtyna</trd>
    <trd lng="ru">&c_z;&c_a;&c_n;&c_a;&c_v;&c_je;&c_s;</trd>
    <trdgrp lng="sv">
      <trd>rid&aring;</trd>,
      <trd>teaterf&ouml;rh&auml;nge</trd>
    </trdgrp>
  </snc>
  <snc mrk="kurten.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;io, kio baras vidon a&ubreve; konon:
      <ekz>
        <tld/>o da arboj, da montetoj;
      </ekz>
      <ekz>
        la trupoj manovris &scirc;irme de <tld/>o da kavalerio;
      </ekz>
      <ekz>
        la urbo estis volvekovrita de <tld/>o da fabrikaj fumoj;
      </ekz>
      <ekz>
        levi la <tld/>on, kiu ka&scirc;as la mondenigmojn.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_v;&c_je;&c_s;&c_a;</trd>
    <trdgrp lng="en">
      <trd>curtain</trd>,
      <trd>screen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>kotara</trd>,
      <trd>zas&lstroke;ona</trd>
    </trdgrp> 
    <trd lng="ru">&c_z;&c_a;&c_v;&c_je;&c_s;&c_a;</trd>
  </snc>
  <ref tip="super" cel="sxirm.0ilo">&scirc;irmilo</ref>
  <trd lng="bo">&#xF41;&#xFB2;&#xF0B;&#xF61;&#xF7C;&#xF63;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>gouel<klr tip="ind"> (stign)</klr></trd>,
    <trd>rideoz</trd>,
    <trd>stign</trd>,
    <trd>tenneris</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>clona</trd>,
    <trd>gardina</trd>,
    <trd>z&aacute;v&ecaron;s</trd>,
    <trd>z&aacute;clona</trd>,
    <trd>opona</trd>
  </trdgrp>
  <trd lng="de">Vorhang</trd>
  <trd lng="fr">rideau</trd>
  <trd lng="hu">f&uuml;gg&ouml;ny</trd>
  <trd lng="id">gorden</trd>
  <trd lng="nl">gordijn</trd>
  <trd lng="pt">cortina <klr tip="amb">(pe&ccedil;a de pano)</klr></trd>
  <trdgrp lng="ro">
    <trd>perdele</trd>,
    <trd>perdea</trd>,
    <trd>decor</trd>,
    <trd>umbr&abreve;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>opona</trd>,
    <trd>z&aacute;clona</trd>,
    <trd>z&aacute;ves</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#x43D;&c_a;&c_v;&c_ib;&c_s;&#x43A;&c_a;</trd>,
    <trd>&c_f;&c_ib;&c_r;&c_a;&#x43D;&#x43A;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_ib;&c_s;&c_a;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_d;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_sh;&c_t;&#x43E;&c_r;&c_a;</trd>,
    <trd>&c_z;&c_a;&#x43F;&#x43E;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kurten.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="kurten.0ego.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="dif" cel="kurten.0o.TEA"><tld/>o<sncref/></ref>:
      <ekz>
        la amika babilado de la kunvenintoj subite silenti&gcirc;is,
        la teatra <tld/>ego levi&gcirc;is
        <fnt>
          <aut>H. Bolingbroke Mudie</aut>,
          <vrk>Kongres-impresoj</vrk>,
          <lok><url ref="&Gutenberg;files/38240/38240-h/vol2/e209.htm">
          The Esperantist, 1905-09 (23)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="pl">kurtyna</trd>     
  <trd lng="ro">cortin&abreve;</trd>
</drv>

<drv mrk="kurten.fera0o">
  <kap>Fera <tld lit="K"/>o</kap>
  <snc mrk="kurten.fera0o.HIS">
    <uzo tip="stl">FRAZ</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Malfacile transirebla limo en E&ubreve;ropo inter okcidentaj 
      kapitalismaj kaj orientaj komunismaj &scirc;tatoj en 
      la 20a jarcento, precipe post la 
      <ref tip="vid" cel="milit.duamond0o.HIS">dua mondmilito</ref>
      kaj &gcirc;is 1991:
      <ekz>
        &ccirc;e amba&ubreve; flankoj de la Fera <tld lit="K"/>o estis proksimume du 
        milionoj da soldatoj
        <fnt><aut>-</aut>, <vrk/>, <lok><url ref="http://www.eventeo.net/web/">eventeo</url>, 
        2009-10-12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">tel&oacute;n de acero</trd>
  <trd lng="fr">rideau de fer</trd>
  <trd lng="pl">&Zdot;elazna Kurtyna</trd>     
  <trd lng="ro">Cortina de fier</trd>  
</drv>

<drv mrk="kurten.flank0o">
  <kap><ofc>*</ofc>flank<tld/>o <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="&AdE;/fundamento/universala_vortaro.html#av-5076">flank'kurten'</url></lok></fnt></kap>
  <snc>
    <dif>
      &Ccirc;iu el la <tld/>oj, pendantaj amba&ubreve;flanke de scenejo k uzataj kiel
      neformalaj kulisoj.
    </dif>
  </snc>
  <trd lng="de">Gardine</trd>
  <trdgrp lng="pl">
    <trd>firanka <klr>(boczna)</klr></trd>,
    <trd>boczna <ind>zas&lstroke;ona</ind></trd>
  </trdgrp>
  <trd lng="ro">perdea <klr>(lateral)</klr></trd>
  <trd lng="uk">&c_z;&c_a;&#x43D;&c_a;&c_v;&c_ib;&c_s;&#x43A;&c_a;</trd>
</drv>

<drv mrk="kurten.lat0o">
  <kap>lat<tld/>o</kap>
  <snc mrk="kurten.lat0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Fenestra kovrilo farita el klineblaj latoj por regi
      lum-enlason.
    </dif>
    <refgrp tip="vid">
      <ref cel="jxaluzi.0o.ARKI">&jcirc;aluzio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_ju;&c_z;&c_ib;</trd>
  <trd lng="ca">persiana veneciana</trd>
  <trd lng="cs">&zcaron;aluzie</trd>
  <trdgrp lng="de">
    <trd>Rollo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rollo</trd>,
    <trd>Rollladen</trd>,
    <trd>Jalousie</trd>
  </trdgrp>
  <trd lng="en">Venetian <ind>blind</ind></trd>
  <trd lng="fr">store v&eacute;nitien</trd>
  <trdgrp lng="hu">
    <trd>red&odblac;ny</trd>,
    <trd>rol&oacute;</trd>,
    <trd>reluxa</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>veneziana<klr> (infisso)</klr></trd>
  </trdgrp>
  <trd lng="nl">zonneblind</trd>
  <trd lng="pt">veneziana</trd>
  <trd lng="pl">&zdot;aluzja</trd>     
  <trd lng="ro">jaluzea</trd>
  <trd lng="ru">&c_zh;&c_a;&c_l;&c_ju;&c_z;&c_i;</trd>
  <trd lng="sk">latkov&aacute; roleta</trd>
</drv>

<drv mrk="kurten.rul0o">
  <kap><ofc>*</ofc>rul<tld/>o <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="&AdE;/fundamento/universala_vortaro.html#av-5077">rul'kurten'</url></lok></fnt></kap>
  <snc mrk="kurten.rul0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <tld lit="K"/>o me&hcirc;anike kunrulebla kaj derulebla
      de supra stango:
      <ekz>
        &scirc;irmataj kontra&ubreve; la suno per rul<tld/>o
        <fnt>
          <aut>M. Proust, trad. P. Combot</aut>,
          <vrk>Pri la legado</vrk>,
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>opona nav&iacute;jen&aacute; nahoru</trd>,
    <trd>roleta</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rollvorhang</trd>,
    <trd>Markise</trd>,
    <trd>Rollo</trd>
  </trdgrp>
  <trd lng="fr">store</trd>
  <trd lng="pl">roleta</trd>     
  <trd lng="ro">orb</trd>
  <trd lng="sk">roleta</trd>
  <trd lng="uk">&c_sh;&c_t;&#x43E;&c_r;&c_a;</trd>
</drv>

</art>
