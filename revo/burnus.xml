<kap>
  <ofc>2</ofc>
  <rad>burnus</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="burnus.0o">
  <kap><tld/>o</kap>
  <snc mrk="burnus.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Longa araba <ref tip="super" cel="mantel.0o">mantelo</ref>
      kun kapu&ccirc;o:
      <ekz>
        eniras fiermienaj nomadoj kun kurba ponardo sub
        sia <tld/>o el dromedara lano
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>Rotterdam, 1956</lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_n;&c_u;&c_s;</trd>
  <trd lng="ca">barn&uacute;s</trd>
  <trd lng="cs">burnus</trd>
  <trd lng="de">Burnus</trd>
  <trd lng="en">burnous</trd>
  <trd lng="fr">burnous</trd>
  <trdgrp lng="he">
    <trd>&#x5E2;&#x5D1;&#x5D9;&#x5D4;</trd>,
    <trd>&#x5DE;&#x5E2;&#x5D9;&#x5DC; &#x5E2;&#x5E8;&#x5D1;&#x5D9;</trd>
  </trdgrp>
  <trd lng="hu">burnusz</trd>
  <trd lng="ja">&#x30D6;&#x30EB;&#x30CC;&#x30B9;</trd>
  <trd lng="pl">burnus</trd>
  <trd lng="ru">&c_b;&c_u;&c_r;&c_n;&c_u;&c_s;</trd>
  <trd lng="sk">burnus</trd>
  <trd lng="uk">&#x431;&#x443;&#x440;&#x43D;&#x443;&#x441;</trd>
</drv>
</art>
