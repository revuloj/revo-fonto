<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: benzol.xml,v 1.15 2019/06/30 07:47:08 revo Exp $">
<kap>
  <rad>benzol</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="benzol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="benzol.0o.benzeno">
    <dif>
      <ref tip="dif" cel="benzen.0o">Benzeno</ref>
      (<frm>C<sub>6</sub>H<sub>6</sub></frm>):
      <ekz>
        precipa ka&ubreve;zo de poluado la&ubreve; la studa&jcirc;o estas <tld/>o, kiu estigas kanceron
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2004/008474.html">Koincido</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Benzol</trd>
  </snc>
  <snc mrk="benzol.0o.miksajxo">
    <dif>
      Miksa&jcirc;o de <ref cel="benzen.0o">benzeno</ref>
      kaj <ref cel="toluen.0o">tolueno</ref>, iam uzata por lumigado,
      hejtado a&ubreve; benzino ekz. en Britujo.
    </dif>
    <trd lng="ca">benzol</trd>
    <trd lng="cs">benzol</trd>
    <trd lng="de">Benzol-Toluol-Gemisch</trd>
    <trd lng="en">heating <ind>oil</ind></trd>
    <trd lng="fr">benzol</trd>
    <trd lng="hu">benzol</trd>
    <trd lng="pl">benzol</trd>
    <trd lng="sk">benzol</trd>
  </snc>
</drv>
</art>
