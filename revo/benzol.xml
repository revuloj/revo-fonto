<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: benzol.xml,v 1.21 2023/10/29 19:11:01 revo Exp $">
<kap>
  <rad>benzol</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="benzol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="benzol.0o.benzeno">
    <dif>
      <ref tip="dif" cel="benzen.0o">Benzeno</ref>
      (<frm>C<sub>6</sub>H<sub>6</sub></frm>):
      <ekz>
        precipa ka&ubreve;zo de poluado la&ubreve; la studa&jcirc;o estas <tld/>o, kiu estigas kanceron
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2004/008474.html">Koincido</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        iam sinjorino diris al mi, ke <tld/>o odoras je lavitaj gantoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 6 (68)</lok>
        </fnt>!
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_e;&c_n;&c_z;&c_o;&c_l;</trd>
    <trd lng="de">Benzol</trd>
    <trd lng="zh">&#x82EF; <pr>b&#x11B;n</pr></trd>
</snc>
  <snc mrk="benzol.0o.miksajxo">
    <dif>
      Miksa&jcirc;o de <ref cel="benzen.0o">benzeno</ref>
      kaj <ref cel="toluen.0o">tolueno</ref>, iam uzata por lumigado,
      hejtado a&ubreve; benzino ekz-e en Britujo.
    </dif>
    <trd lng="be">&c_s;&c_u;&c_m;&c_je;&c_s;&c_mol; &c_b;&c_e;&c_n;&c_z;&c_o;&c_l;&c_u; &c_ib; &c_t;&c_a;&c_l;&c_u;&c_o;&c_l;&c_u;</trd>
    <trd lng="ca">benzol</trd>
    <trd lng="cs">benzol</trd>
    <trd lng="de">Benzol-Toluol-Gemisch</trd>
    <trd lng="en">heating <ind>oil</ind></trd>
    <trd lng="fr">benzol</trd>
    <trd lng="hu">benzol</trd>
    <trd lng="pl">benzol</trd>
    <trd lng="sk">benzol</trd>
  </snc>
  <trd lng="ja">&#x30D9;&#x30F3;&#x30BE;&#x30FC;&#x30EB;</trd>
  <trd lng="uk">&c_b;&c_je;&c_n;&c_z;&c_o;&c_l;</trd>
</drv>
</art>
