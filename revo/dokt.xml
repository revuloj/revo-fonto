<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>dokt</rad>/a <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="dokt.0a">
      <kap><tld/>a</kap>
      <uzo tip="stl">RAR</uzo>
      <snc mrk="dokt.0a.KOMUNE">
        <refgrp tip="dif">
          <ref cel="sci.multe0a">multescia</ref>,
          <ref cel="erudic.0a">erudicia</ref>
        </refgrp>
        <ekz>
          li, kiu prikantinte por ilia utilo /
          la princojn kaj la re&gcirc;ojn per tiel <tld/>a stilo, /
          mortis malri&ccirc;a
          <fnt>
            <aut>Ronsardo, trad. G. Waringhien</aut>,
            <vrk>en: Joakimo du Bellay</vrk>,
            <lok>Nica Literatura Revuo, 1955-09 &gcirc;is 10,
              n-ro 1/1, p. 32a&ndash;40a</lok>
          </fnt>;
        </ekz><ekz>
          tia <tld/>a babilado ne apartenas ja por &jcirc;urnalo
          <fnt>
            <bib>Salamandroj</bib>,
            <lok>libro 1a, &ccirc;apitro 11a</lok>
          </fnt>.
        </ekz>
      </snc>
  <trdgrp lng="cs">
    <trd>sečtělý</trd>,
    <trd>učený</trd>
  </trdgrp>
  <trd lng="fr">docte<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
        <trd>tanult</trd>,
        <trd>tud&oacute;s</trd>
      </trdgrp>
  <trdgrp lng="nl">
        <trd>kundig</trd>,
        <trd>erudiet</trd>,
        <trd>geleerd</trd>,
        <trd>bekwaam</trd>
      </trdgrp>
  <trd lng="pl">uczony</trd>
  <trdgrp lng="pt">
        <trd>douto</trd>,
        <trd>erudito</trd>,
        <trd>culto</trd>
      </trdgrp>
  <trd lng="ru">&c_u;&c_ch;&c_jo;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>učený</trd>,
    <trd>vzdelaný</trd>
  </trdgrp>
</drv>
  </art>

