<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dokt</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="dokt.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="dokt.0a.KOMUNE">
    <dif>
      <ref tip="super" cel="kler.0a.PED">Kler</ref>ega:
      <ekz>
        li, kiu prikantinte por ilia utilo /
        la princojn kaj la re&gcirc;ojn per tiel <tld/>a stilo, /
        mortis malri&ccirc;a
        <fnt>
        <aut>Ronsardo, trad. G. Waringhien</aut>,
        <vrk><url ref="">en: Joakimo du Bellay</url></vrk>,
        <lok>Nica Literatura Revuo, 1955-09 &gcirc;is 10, numero 1/1, p. 
        32-40a</lok>
        </fnt>;
      </ekz>
      <ekz>
        tia <tld/>a babilado ne apartenas ja por &jcirc;urnalo
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 1a, &ccirc;apitro 11a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">docte<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>tanult</trd>,
    <trd>tud&oacute;s</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>douto</trd>,
    <trd>erudito</trd>,
    <trd>culto</trd>
  </trdgrp>
</drv>
</art>

