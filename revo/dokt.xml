<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dokt.xml,v 1.15 2023/11/04 18:50:51 revo Exp $">
<kap>
  <rad>dokt</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="dokt.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="dokt.0a.KOMUNE">
    <dif>
      <refgrp tip="dif">
        <ref cel="sci.multe0a">Multescia</ref>,
        <ref cel="erudic.0a">erudicia</ref></refgrp>:
      <ekz>
        li, kiu prikantinte por ilia utilo /
        la princojn kaj la re&gcirc;ojn per tiel <tld/>a stilo, /
        mortis malri&ccirc;a
        <fnt>
          <aut>Ronsardo, trad. G. Waringhien</aut>,
          <vrk>en: Joakimo du Bellay</vrk>,
          <lok>Nica Literatura Revuo, 1955-09 &gcirc;is 10,
            n-ro 1/1, p. 32a&ndash;40a</lok>
        </fnt>;
      </ekz><ekz>
        tia <tld/>a babilado ne apartenas ja por &jcirc;urnalo
        <fnt>
          <bib>Salamandroj</bib>,
          <lok>libro 1a, &ccirc;apitro 11a</lok>
        </fnt>;
      </ekz>
      <ekz>
        dank' al kunlaboro de la plej <tld/>aj cerboj en la nova federacio, grandaj
        avancoj estis faritaj en la kampoj de kemio kaj tekniko
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008265.html">Parakosmo</url></vrk>
          <bib>Monato</bib>
          <lok>2004/05, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        krom la <tld/>aj respondoj, &ccirc;i tie interesas la praktikaj
        <fnt>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2006/008871.php">&Scirc;tato, eklezio kaj ideologia laikeco</url></vrk>
          <bib>Monato</bib>
          <lok>2006/06, p. 19</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kler.0a">klera</ref>
    <refgrp tip="vid">
       <ref cel="instru.0iteco">instruiteco</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>gelehrt</trd>,
    <trd>kundig</trd>
  </trdgrp>
  <trd lng="en">erudite</trd>
  <trdgrp lng="cs">
    <trd>se&ccaron;t&ecaron;l&yacute;</trd>,
    <trd>u&ccaron;en&yacute;</trd>
  </trdgrp>
  <trd lng="fr">docte<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>tanult</trd>,
    <trd>tud&oacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kundig</trd>,
    <trd>erudiet</trd>,
    <trd>geleerd</trd>,
    <trd>bekwaam</trd>
  </trdgrp>
  <trd lng="pl">uczony</trd>
  <trdgrp lng="pt">
    <trd>douto</trd>,
    <trd>erudito</trd>,
    <trd>culto</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_ch;&c_jo;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>u&ccaron;en&yacute;</trd>,
    <trd>vzdelan&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_u;&c_ch;&c_je;&#x43D;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x7814;&#x7A76;&#x6027;&#x8D28; <pr>y&#xE1;nji&#x16B;x&#xEC;ngzh&#xEC;</pr></trd>,
    <trd>&#x9700;&#x8981;&#x52A8;&#x8111; <pr>x&#x16B;y&#xE0;od&#xF2;ngn&#x1CE;o</pr></trd>
  </trdgrp>
</drv>
