<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lampir1.xml,v 1.15 2024/01/15 16:14:08 revo Exp $">
<kap>
  <rad>lampired</rad>/oj
</kap>
<drv mrk="lampir1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo 
      <ref tip="malprt" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Lampyridae</trd>):
      <ekz>
        <tld/>oj <klr>[&#x2026;]</klr> anta&ubreve; &ccirc;io estas konataj pro la lumdonado (bioluminesko)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=590179">Lampiredoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">lamp&iacute;rids</trd> 
  <trd lng="de">Leuchtk&auml;fer</trd>
  <trdgrp lng="en">
    <trd>fireflies</trd>,
    <trd>lightning bugs</trd>
  </trdgrp>
  <trd lng="fr">lampyres</trd>
  <trd lng="hu">szentj&aacute;nosbog&aacute;r-f&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30DB;&#x30BF;&#x30EB;&#x79D1;  <pr>&#x30DB;&#x30BF;&#x30EB;&#x304B;</pr></trd>
  <trd lng="nl">glimwormen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">&sacute;wietlikowate</trd>
  <trd lng="pt">lampir&iacute;deos</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_t;&c_l;&c_ja;&c_k;&c_i;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x8424;&#x79D1; <pr>y&#xED;ngk&#x113;</pr></trd>,
    <trd>&#x87A2;&#x79D1; <pr>y&#xED;ngk&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>
