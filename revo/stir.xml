<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>stir</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="stir.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stir.0i.veturilon">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="direkt.0i.stiri">Direkti</ref>
      veturilon, &scirc;ipon, avion ktp:
      <ekz>
        li kulpis pri dan&gcirc;era <tld/>ado.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>lenken</trd>,
    <trd>steuern</trd>
  </trdgrp>
  <trd lng="en">steer</trd>
  <trdgrp lng="es">
    <trd>conducir</trd>,
    <trd>manejar</trd>,
    <trd>pilotar</trd>,
    <trd>navegar <klr>(pilotar una nave)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>conduire<klr> (piloter)</klr></trd>,
    <trd>piloter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>korm&aacute;nyoz</trd>,
    <trd>ir&aacute;ny&iacute;t</trd>,
    <trd>vezet <klr tip="ind">(j&aacute;rm&udblac;vet)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>condurre<klr> (guidare)</klr></trd>,
    <trd>pilotare</trd>.
    <trd>guidare</trd>
  </trdgrp>
  <trd lng="nl">besturen</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_s;&c_t;&c_i; <klr>(&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_mol; &c_i; &c_t;.&c_p;.)</klr></trd>,
    <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol; <klr>(&c_m;&c_a;&c_sh;&c_i;&c_n;&c_o;&c_j;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="stir.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="stir.0ejo..MAR">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">MAR</uzo>
    <dif>
      Ejo el kiu oni <tld/>as, &ccirc;eeste (ekzemple sur &scirc;ipo) a&ubreve; defore
      (ekzemple pri senpilota avio a&ubreve; raketo).
    </dif>
  </snc>
  <trd lng="fr">cabine de pilotage</trd>
  <trdgrp lng="hu">
    <trd>vezet&odblac;f&uuml;lke</trd>,
    <trd>vezet&odblac;&aacute;ll&aacute;s</trd>
  </trdgrp>
  <trd lng="it">cabina<klr> (di pilotaggio)</klr></trd>
  <trdgrp lng="nl">
    <trd>stuurhut</trd>,
    <trd>cockpit</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_b;&c_k;&c_a; <klr>(&c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_ja;)</klr></trd>,
    <trd>&c_c;&c_je;&c_n;&c_t;&c_r; &c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_ja; <klr>(&c_p;&c_o;&c_l;&c_jo;&c_t;&c_o;&c_m; &c_i;
    &c_t;.&c_p;..)</klr></trd>
  </trdgrp>
  <trd lng="sk">miestnosť kormidelníka</trd>
</drv>

<drv mrk="stir.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="stir.0ilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Aran&gcirc;a&jcirc;o por <tld/>ado, ekzemple <tld/>rado de a&ubreve;tomobilo,
      <tld/>stango de avio...
      <ekz>
        metu mian manon sur la <tld/>ilon,
        mi estas tro malforta
        <fnt>
          <aut>trad. senlime</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Diktator.htm">
          La Granda Diktatoro</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>volante</trd>,
    <trd>manillar</trd>,
    <trd>mando <klr>(de direcci&oacute;n de un veh&iacute;culo)</klr></trd>,
    <trd>tim&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dispositif de guidage</trd>,
    <trd>volant<klr> subst.</klr></trd>
  </trdgrp>
  <trd lng="hu">korm&aacute;ny</trd>
  <trdgrp lng="nl">
    <trd>stuur</trd>,
    <trd>stuurstang</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_t;&c_r;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o; &c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_r;&c_u;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kormidlo</trd>,
    <trd>riadidlá</trd>,
    <trd>volant</trd>
  </trdgrp>
</drv>

<drv mrk="stir.0globo">
  <kap><tld/>globo</kap>
  <snc mrk="stir.0globo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="en.0igilo">Enigilo</ref> uzata en
      <ref cel="dialog.0o.KOMP">dialogo</ref> kun komputilo por indiki
      datumon sur la ekrano, rulebla globo iom malmergi&gcirc;anta el
      la ebena surfaco de aparato (ekzemple de la klavaro). Rulante
      la globon, la uzanto movas la <ref cel="kursor.0o">kursoron</ref>.
      Alivorte, renversita <ref cel="mus.0o.KOMP">muso</ref>
      muntita kun pli granda komputila organo.
    </dif>
  </snc>
  <trd lng="de">Rollkugel</trd>
  <trdgrp lng="en">
    <trd>track ball</trd>, <trd>rolling ball</trd>
  </trdgrp>
  <trd lng="es">rat&oacute;n de bola <klr>(para aparatos
  inform&aacute;ticos)</klr></trd>
  <trd lng="fr">boule de commande</trd>
  <trd lng="hu">pozicion&aacute;l&oacute; g&ouml;mb</trd>  
  <trd lng="it">trackball</trd>
  <trd lng="ru">&c_sh;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j; &c_m;&c_a;&c_n;&c_i;&c_p;&c_u;&c_l;&c_ja;&c_t;&c_o;&c_r;</trd>
</drv>

<drv mrk="stir.0lernejo">
  <kap><tld/>lernejo</kap>
  <snc>
    <dif>
      Lernejo pri a&ubreve;tomobil<tld/>ado.
    </dif>
  </snc>
  <trd lng="es">autoescuela</trd>
  <trd lng="fr">auto-&eacute;cole</trd>
  <trd lng="hu">aut&oacute;siskola</trd>
  <trdgrp lng="it">
    <trd>autoscuola</trd>,
    <trd><ind>scuola</ind> guida</trd>
  </trdgrp>
  <trd lng="nl">autorijschool</trd>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_sh;&c_k;&c_o;&c_l;&c_a;</trd>
  <trd lng="sk">autoškola</trd>
</drv>

<drv mrk="stir.0signo">
  <kap><tld/>signo</kap>
  <snc mrk="stir.0signo.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="sign.0o.KOMP">Signo</ref> uzata por <tld/>i
      <ref cel="en.0igo">enigon</ref>, <ref cel="el.0igo">eligon</ref>
      a&ubreve; datumtransmeton (ka&ubreve;zanta ekigon, re&gcirc;im&scirc;an&gcirc;on,
      haltigon); ekzemple la <tld/>signo CR (a&ubreve; &Ccirc;R, dekume 13)
      ka&ubreve;zanta la <ctl>&ccirc;aret-revenon</ctl>.
    </dif>
  </snc>
  <trd lng="de">Steuerzeichen</trd>
  <trd lng="en">control character</trd>
  <trd lng="fr">caract&egrave;re de commande</trd>
  <trd lng="hu">vez&eacute;rl&odblac;karakter</trd>
  <trd lng="it">carattere di controllo</trd>
  <trd lng="nl">stuurteken</trd>
  <trd lng="pl">znak steruj&aogonek;cy</trd>
  <trd lng="ru">&c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_ju;&c_shch;&c_a;&c_ja; &c_l;&c_i;&c_t;&c_je;&c_r;&c_a;</trd>
  <trd lng="sk">ovládacia klávesa</trd>
</drv>

<drv mrk="stir.0stango">
  <kap><tld/>stango</kap>
  <snc mrk="stir.0stango.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Stangofoma <tld/>ilo oblikvigebla, precipe tiu uzata por
      <tld/>i avion.
    </dif>
    <trd lng="es">palanca de mando</trd>
    <trd lng="fr">manche &agrave; balai<klr> (avion)</klr></trd>
    <trd lng="hu">botkorm&aacute;ny</trd>    
    <trdgrp lng="it">
      <trd>cloche</trd>,
      <trd><ind>barra</ind> di comando</trd>
    </trdgrp>
    <trd lng="nl">stuurknuppel</trd>
    <trd lng="ru">&c_r;&c_y;&c_ch;&c_a;&c_g; &c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_ja;</trd>
  </snc>
  <snc mrk="stir.0stango.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="en.0igilo">Enigilo</ref> uzata en
      <ref cel="dialog.0o.KOMP">dialogo</ref> kun komputilo por indiki
      koordinatojn (precipe en komputilaj ludoj) forme simila al
      avia <tld/>stango.
    </dif>
    <trd lng="es">joystick</trd>
    <trd lng="fr">joystick</trd>
    <trdgrp lng="hu">
      <trd>botkorm&aacute;ny</trd>,
      <trd>joystick</trd>
    </trdgrp>
    <trd lng="it">joystick</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_o;&c_r;&c_d;&c_i;&c_n;&c_a;&c_t;&c_n;&c_a;&c_ja; &c_r;&c_u;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_r;&c_y;&c_ch;&c_a;&c_zh;&c_n;&c_y;&c_j; &c_u;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_d;&c_zh;&c_o;&c_j;&c_s;&c_t;&c_i;&c_k;</trd>
    </trdgrp>
  </snc>
  <trd lng="en">joystick</trd>
  <trdgrp lng="sk">
    <trd>knipel lietadla</trd>,
    <trd>riadiaca páka</trd>
  </trdgrp>
</drv>

<drv mrk="stir.mem0a">
  <kap>mem<tld/>a</kap>
  <snc mrk="stir.mem0a.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Kiu mem sin <tld/>as:
      <ekz>
        mem<tld/>a raketo.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">autoguiado</trd>
  <trd lng="fr">autoguid&eacute;</trd>
  <trdgrp lng="hu">
    <trd>&ouml;nir&aacute;ny&iacute;t&oacute;</trd>,
    <trd>&ouml;nvez&eacute;relt</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>autoguidato</trd>,
    <trd>intelligente<klr> (autoguidato)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s; <ind>&c_a;&c_v;&c_t;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_m;
  &c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;&c_m;</ind></trd>
</drv>

<drv mrk="stir.defora0ado">
  <kap>defora <tld/>ado</kap>
  <snc mrk="stir.defora0ado.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="S"/>ado de ekstere kaj de malproksime:
      <ekz>
        defore <tld/>ata raketo, avio, procezo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="telereg.0i.TEK">teleregi</ref>
    </refgrp>
  </snc>
  <trd lng="en">remote control</trd>
  <trd lng="es">teledirigido</trd>
  <trd lng="fr">commande &agrave; distance</trd>
  <trd lng="hu">t&aacute;vir&aacute;ny&iacute;t&aacute;s</trd> 
  <trd lng="it">teleguidato</trd> 
  <trd lng="nl">afstandsbediening</trd>
  <trd lng="ru">&c_d;&c_i;&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_o;&c_je; &c_u;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
