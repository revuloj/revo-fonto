<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>human</rad>/a
</kap>
<drv mrk="human.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Posedanta la kompatemon, helpemon, homamon, kiuj estas la plej
      noblaj kvalitoj de la homo:
      <ekz>
        oni ne povas esti justa, se oni ne estas <tld/>a;
      </ekz>
      <ekz>
        <ind>ne<tld/>a</ind> kruela&jcirc;o.
        <trd lng="fr">inhumain<klr tip="ind"> (insensible)</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>human</trd>,
    <trd>menschlich</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>humain<klr tip="ind"> (compatissant)</klr></trd>,
    <trd>bon<klr tip="ind"> (compatissant)</klr></trd>,
    <trd>compatissant</trd>,
    <trd>sensible<klr tip="ind"> (compatissant)</klr></trd></trdgrp> 
  <trdgrp lng="nl">
    <trd>humaan</trd>,
    <trd>menslievend</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_g;&c_u;&c_m;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="human.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco de <tld/>a homo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Humanit&auml;t</trd>,
    <trd>Menschlichkeit</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>humanit&eacute;<klr tip="ind"> (compassion)</klr></trd>,
    <trd>compassion</trd></trdgrp> 
  <trdgrp lng="nl">
    <trd>humaniteit</trd>,
    <trd>menslievendheid</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_u;&c_m;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="human.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi iun <tld/>a:
      <ekz>
        <tld/>igi militistojn, sova&gcirc;ulojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>humanisieren</trd>,
    <trd>menschlicher machen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>humaniser</trd>,
    <trd>adoucir<klr tip="ind"> (rendre plus humain)</klr></trd></trdgrp> 
  <trdgrp lng="nl">
    <trd>veredelen</trd>,
    <trd>menselijk maken</trd>
  </trdgrp>
</drv>

<drv mrk="human.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Doktrino, ser&ccirc;anta en la helenlatina civilizo la plej bonan
      rimedon por <tld/>igi kaj klerigi la homaron.
    </dif>
    <refgrp tip="vid">
      <ref cel="klasik.0a">klasika</ref>,
      <ref cel="helen.0ismo">helenismo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Humanismus</trd>,
    <trd>Menschlichkeit</trd>
  </trdgrp>
  <trd lng="fr">humanisme</trd>
  <trd lng="nl">humanisme</trd>
  <trd lng="ru">&c_g;&c_u;&c_m;&c_a;&c_n;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="human.0isto">
  <kap><tld/>isto<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Fakulo studanta la helenlatinan civilizon kaj la antikvajn
      klasikajn literaturojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Humanist</trd>
  </trdgrp>
  <trd lng="fr">humaniste</trd>
  <trd lng="nl">humanist</trd>
</drv>
</art>
