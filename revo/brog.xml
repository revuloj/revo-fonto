<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>brog</rad>/i
</kap>
<drv mrk="brog.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Trempi ion en bolantan akvon a&ubreve; surver&scirc;i per bolanta akvo. 
    </dif>
    <trdgrp lng="de"><trd>br&uuml;hen</trd>, <trd>blanchieren</trd></trdgrp>
    <trd lng="fr">blanchir</trd>
  </snc>
  <snc num="2">
    <dif>
      Brulvundi iun per bolanta flua&jcirc;o:
      <ekz>
        propra supo <tld/>as, fremda allogas
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        <tld/>ito e&ccirc; sur malvarman akvon blovas<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kornik.0o">korniko</ref>,
      <ref cel="spert.0o">sperto</ref>.
    </refgrp>
    <trd lng="de">verbr&uuml;hen</trd>
  </snc>
  <trd lng="en">scald</trd>
</drv>
<drv mrk="brog.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Akvo en kiu oni kuiris viandon 
    </dif>
    <refgrp tip="sin">
      <ref cel="buljon.0o">buljono</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Br&uuml;he</trd>
</drv>
</art>
