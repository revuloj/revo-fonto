<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>ciklon</rad>/o
</kap>

<drv mrk="ciklon.0o">
  <kap><tld/>o</kap>
  <snc mrk="ciklon.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Kirloventego, turni&gcirc;a kaj anta&ubreve;enira <ref tip="vid"
      cel="uragan.0o.MET">uragano</ref>:
      <ekz>
        kolerega <tld/>o
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tromb.0o">trombo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_k;&c_l;&c_jo;&c_n;</trd>
  <trd lng="br">kelc'hwidenn</trd>
  <trd lng="de">Zyklon</trd>
  <trd lng="el">&kappa;&ypsilon;&kappa;&lambda;&omega_ton;&ny;&alfa;&sigma_fina;</trd>
  <trd lng="es">cicl&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>cyclone</trd>,
    <trd>typhon</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ciklon</trd>,
    <trd>forg&oacute;sz&eacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>cycloon</trd>,
    <trd>wervelstorm</trd>
  </trdgrp>
  <trd lng="pt">ciclone</trd>
  <trd lng="ru">&c_c;&c_i;&c_k;&c_l;&c_o;&c_n;</trd>
  <trd lng="sk">cykl&oacute;n</trd>
</drv>
</art>
