<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: debito.xml,v 1.14 2022/03/15 15:50:54 revo Exp $">
  <kap>
    <rad>debitor</rad>/o
  </kap>
  <drv mrk="debito.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="sxuld.0anto">&Scirc;uldanto</ref>:
      <ekz>
        en la plej multaj landoj ekzistas le&gcirc;oj pri bankroto, kiuj <klr>[celas]</klr>
        malhelpi, ke <tld/>o profitu el &scirc;ajnigita nesolventeco <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34896">Bankroto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; la Le&gcirc;o de Nesolventaj <tld lit="D"/>oj, <nom>Dickens</nom> aran&gcirc;is pagon al siaj
        kreditoroj, kaj li kaj lia familio forlasis <nom>Marshalsea</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=768">Charles Dickens</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_e;&c_b;&c_ib;&c_t;&c_o;&c_r;</trd>
    <trdgrp lng="de">
       <trd>Schuldner</trd>,
       <trd>Debitor</trd></trdgrp>
    <trdgrp lng="en">
       <trd>debtor</trd>,
       <trd>account receivable (US)</trd></trdgrp>
    <trd lng="fr">d&eacute;biteur</trd>
    <trd lng="hu">ad&oacute;s</trd>
    <trdgrp lng="nl">
       <trd>schuldenaar</trd>,
       <trd>debiteur</trd>
    </trdgrp>
    <trd lng="pl">d&lstroke;u&zdot;nik</trd>
    <trd lng="pt">devedor</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_l;&c_zh;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_d;&c_je;&c_b;&c_i;&c_t;&c_o;&c_r;</trd>
    </trdgrp>
    <trd lng="sk">dl&zcaron;n&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x43E;&#x440;&#x436;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x434;&#x435;&#x431;&#x456;&#x442;&#x43E;&#x440;</trd>
  </trdgrp>
</drv>
</art>
