<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: debito.xml,v 1.18 2023/11/04 13:16:26 revo Exp $">
  <kap>
    <rad>debitor</rad>/o
    <fnt><bib>PV</bib></fnt>
  </kap>
  <drv mrk="debito.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="sxuld.0anto">&Scirc;uldanto</ref>:
      <ekz>
        interretaj a&ubreve;kcioj estas pli travideblaj ol la
        tradiciaj <klr>[&#x2026;]</klr>, kio
        povas bone protekti la interesojn de kreditoroj kaj debitoroj
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="https://www.monato.be/2014/011286.php">Ju&gcirc;ejoj enretigas pliajn profitojn</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la plej multaj landoj ekzistas le&gcirc;oj pri bankroto,
        kiuj <klr>[celas]</klr>
        malhelpi, ke <tld/>o profitu el &scirc;ajnigita
        nesolventeco <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34896">Bankroto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; la Le&gcirc;o de Nesolventaj <tld lit="D"/>oj,
        <nom>Dickens</nom> aran&gcirc;is pagon al siaj
        kreditoroj, kaj li kaj lia familio forlasis <nom>Marshalsea</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=768">Charles Dickens</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="kredit1.0o">kreditoro</ref>
    <ref tip="vid" cel="debet.0o">debeto</ref>
    <trd lng="be">&c_d;&c_e;&c_b;&c_ib;&c_t;&c_o;&c_r;</trd>
    <trdgrp lng="de">
       <trd>Schuldner</trd>,
       <trd>Debitor</trd></trdgrp>
    <trdgrp lng="en">
       <trd>debtor</trd>,
       <trd>account receivable (US)</trd></trdgrp>
    <trd lng="fr">d&eacute;biteur</trd>
    <trd lng="hu">ad&oacute;s</trd>
    <trd lng="ja">&#x50B5;&#x52D9;&#x8005;  <pr>&#x3055;&#x3044;&#x3080;&#x3057;&#x3083;</pr></trd>
  <trdgrp lng="nl">
       <trd>schuldenaar</trd>,
       <trd>debiteur</trd>
    </trdgrp>
    <trd lng="pl">d&lstroke;u&zdot;nik</trd>
    <trd lng="pt">devedor</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_l;&c_zh;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_d;&c_je;&c_b;&c_i;&c_t;&c_o;&c_r;</trd>
    </trdgrp>
    <trd lng="sk">dl&zcaron;n&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_o;&c_r;&c_zh;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_d;&c_je;&c_b;&c_ib;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="zh">&#x6B20;&#x6236; <pr>qi&#xE0;nh&#xF9;</pr></trd>
</drv>
</art>
