<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kadrat</rad>/o
</kap>
<drv mrk="kadrat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      Malgranda vergo, havante kvadratan sekcon,
      iom pli malalta ol presliteroj kaj uzata por apartigi
      kompostitajn vortojn, por intermeti presliterojn ktp. 
    </dif>
  </snc>
  <trd lng="nl">kwadraat<klr tip="ind"> (drukkunst)</klr>
  </trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_r;&c_z;&c_a;&c_n;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t; <klr>(&c_p;&c_o;&c_l;&c_i;&c_g;&c_r;.)</klr></trd>
  </trdgrp>
</drv>
<drv mrk="kadrat.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Malgranda kaj mallar&gcirc;a <tld/>o.  
    </dif>
  </snc>
<trd lng="nl">vierkant<klr tip="ind"> (drukkunst)</klr>
</trd>
</drv>
</art>

