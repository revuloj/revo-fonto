<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kadrat.xml,v 1.22 2023/08/31 19:24:08 revo Exp $">
<kap>
   <rad>kadrat</rad>/o
   <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kadrat.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">TIP</uzo>
   <snc mrk="kadrat.0o.konveksa">
     <dif>
       Tipografia <ref tip="lst" lst="voko:mezurunuoj" cel="unu.mezur0o">mezurunuo</ref>
       respondanta al kvadrata interspaca litertipo difinanta la&ubreve;alte la minimuman linidistancon kaj horizontale
       la subdividojn (&gcirc;is 24-ona) de diversaj interspacoj:
      <ekz>
        la spaco inter vortoj ordinare estas kvaron<tld/>o;
      </ekz>
      <ekz>
        en HTML oni povas esprimi <tld/>an spacon per &amp;emsp;, duon<tld/>an spacon per
        &amp;ensp;, kvinon<tld/>an spacon per &amp;thinsp;, kvaron<tld/>an spacon per
        &amp;emsp14;, trion<tld/>an spacon per &amp;emsp13;, <tld/>strekon per
        &amp;mdash; kaj duon<tld/>strekon per &amp;ndash;.
      </ekz>
    </dif>
    <ref tip="vid" cel="kompos.0i">komposti</ref>
  </snc>
  <trd lng="ca">quadrat<klr> (tipogr.)</klr></trd>
  <trd lng="cs">kadr&aacute;t</trd>
  <trd lng="de">Geviert</trd>
  <trdgrp lng="en">
    <trd>quad</trd>,
    <trd>quadrat</trd>
  </trdgrp>
  <trd lng="fi">suluke</trd>
  <trd lng="fr">cadrat</trd>
  <trdgrp lng="hu">
     <trd>kvadr&aacute;t <klr tip="ind">(nyomd&aacute;szat)</klr></trd>,
     <trd>t&eacute;rkit&ouml;lt&odblac; n&eacute;gyzet</trd>
   </trdgrp>
  <trd lng="ja">&#x30AF;&#x30EF;&#x30BF;</trd>
  <trdgrp lng="nl">
     <trd>holwit</trd>,
     <trd>kwadraat<klr tip="ind"> (boekdruk)</klr></trd>
   </trdgrp>
  <trd lng="pt">quadrado <klr tip="amb">(tip.)</klr></trd>
  <trd lng="pl">justunek</trd>
  <trd lng="ro">justificare</trd>
  <trdgrp lng="ru">
     <trd>&c_b;&c_a;&c_b;&c_a;&c_sh;&c_k;&c_a;</trd>,
     <trd>&c_m;&c_a;&c_r;&c_z;&c_a;&c_n;</trd>,
     <trd>&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;<klr> (&c_p;&c_o;&c_l;&c_i;&c_g;&c_r;.)</klr></trd>
   </trdgrp>
  <trd lng="sk">medzern&iacute;k <klr>(typ)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x440;&#x437;&#x430;&#x43D;</trd>,
    <trd>&#x43A;&#x432;&#x430;&#x434;&#x440;&#x430;&#x442;</trd>
  </trdgrp>
</drv>

