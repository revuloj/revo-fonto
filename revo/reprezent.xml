<drv mrk="reprezent.0anto">
  <kap>
    <tld/>anto
  </kap>
  <snc>
    <dif>
      Tiu, kiu <tld/>as:
      <ekz>
        la Danidoj sendis kvin homojn el sia tribo, el siaj <tld/>antoj, virojn
        batalkapablajn
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 18:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        Germanujo kaj Francujo alsendis <tld/>antojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi, sankta viro, anstata&ubreve;os min kiel <tld/>anto de la ministro de la milito
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_p;&c_r;&c_e;&c_z;&c_e;&c_n;&c_t;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vertreter</trd>,
    <trd>Repr&auml;sentant</trd>,
  </trdgrp>
  <trdgrp lng="en">
    <trd>agent</trd>,
    <trd>envoy</trd>,
    <trd>representative</trd>
  </trdgrp>
  <trd lng="ja">&#x4EE3;&#x8868;&#x8005;  <pr>&#x3060;&#x3044;&#x3072;&#x3087;&#x3046;&#x3057;&#x3083;</pr></trd>
  <trdgrp lng="pl">
    <trd>reprezentant</trd>,
    <trd>przedstawiciel</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_r;&c_je;&c_d;&c_s;&c_t;&c_a;&c_v;&c_n;&c_i;&c_k;</trd>
