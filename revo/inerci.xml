<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>inerci</rad>/o
</kap>
<drv mrk="inerci.0o">
  <kap><tld/>o</kap>
  <snc mrk="inerci.0o.psika">
    <dif>
      Mensa <ref cel="inert.0eco.psika">inerteco</ref>:
      <ekz>
        <klr>[tiu]</klr> forto de <tld/>o <klr>[...]</klr> terure malhelpis
        &ccirc;iun nian pa&scirc;on
        <fnt><bib>Paroloj</bib>
        <lok>Malferma parolado de la 3a UK en Cambridge (1907-08-12)</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>inertie<klr tip="ind"> (apathie)</klr></trd>,
      <trd>apathie</trd>,
      <trd>inaction</trd>,
      <trd>l&eacute;thargie</trd>
    </trdgrp>
  </snc>
  <snc mrk="inerci.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <ref tip="dif" cel="inert.0eco.FIZ">inerteco.</ref>
    <trd lng="fr">inertie<klr tip="ind"> (phys.)</klr></trd>
    <trd lng="ru">&c_i;&c_n;&c_je;&c_r;&c_c;&c_i;&c_ja;</trd>
  </snc>
</drv>

<drv mrk="inerci.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="inert.0eca">inerteca</ref>
    <ekz>
      <tld/>a forto,
      maso <fnt><bib>PIV2</bib></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>d'<ind>inertie</ind></trd>,
    <trd>inertiel</trd>
  </trdgrp>
</drv>

<drv mrk="inerci.0uma">
  <kap><tld/>uma</kap>
  <snc>
    <ref tip="dif" cel="inert.0eca">inerteca</ref>
    <ekz>
      <tld/>uma referenco-sistemo <fnt><bib>PIV2</bib></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>d'<ind>inertie</ind></trd>,
    <trd>inertiel</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
