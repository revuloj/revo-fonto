<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: inerci.xml,v 1.14 2023/11/19 10:22:12 revo Exp $">
<kap>
  <rad>inerci</rad>/o
</kap>
<drv mrk="inerci.0o">
  <kap><tld/>o</kap>
  <snc mrk="inerci.0o.psika">
    <dif>
      Mensa <ref cel="inert.0eco.psika">inerteco</ref>:
      <ekz>
        <klr>[tiu]</klr> forto de <tld/>o <klr>[&#x2026;]</klr> terure malhelpis
        &ccirc;iun nian pa&scirc;on
        <fnt><bib>Paroloj</bib>
        <lok>Malferma parolado de la 3a UK en Cambridge
        (1907-08-12)</lok></fnt>.
      </ekz>
      <ekz>
        la natura spirita <tld/>o de &ccirc;iu el ni
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>inertie<klr tip="ind"> (apathie)</klr></trd>,
      <trd>apathie</trd>,
      <trd>inaction</trd>,
      <trd>l&eacute;thargie</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tehetetlens&eacute;g</trd>,
      <trd>tunyas&aacute;g</trd>
    </trdgrp>
  </snc>
  <snc mrk="inerci.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <ref tip="dif" cel="inert.0eco.FIZ">Inerteco</ref>:
      <ekz>
        la tuta marsa medio estas malamika al la homaro <klr>[&#x2026;]</klr>,
        la tiea termika <tld/>o estas multe pli malalta
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/009989.php">Voja&gcirc;o unudirekta</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">inertie<klr tip="ind"> (phys.)</klr></trd>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_e;&c_r;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>inercie</trd>,
    <trd>nepohyblivost</trd>,
    <trd>nete&ccaron;nost</trd>,
    <trd>ne&ccaron;innost</trd>
  </trdgrp>
  <trd lng="en">inertia</trd>
  <trd lng="es">inercia</trd>
  <trdgrp lng="hu">
    <trd>tehetetlens&eacute;g</trd>,
    <trd>inercia</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6163;&#x6027;  <pr>&#x304B;&#x3093;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x60F0;&#x6027;  <pr>&#x3060;&#x305B;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="pt">in&eacute;rcia</trd>
  <trd lng="ru">&c_i;&c_n;&c_je;&c_r;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>nepohyblivos&tcaron;</trd>,
    <trd>ne&ccaron;innos&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x456;&#x43D;&#x435;&#x440;&#x446;&#x456;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x60F0;&#x6027; <pr>du&#xF2;x&#xEC;ng</pr></trd>,
    <trd>&#x60EF;&#x91CF; <pr>gu&#xE0;nli&#xE0;ng</pr></trd>,
    <trd>&#x60EF;&#x6027; <pr>gu&#xE0;nx&#xEC;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="inerci.0a">
  <kap><tld/>a</kap>
  <snc mrk="inerci.0a.inerteca">
    <dif>
      <ref tip="dif" cel="inert.0eca">Inerteca</ref>:
      <ekz>
        <tld/>aj homoj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>uloj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">FIZ</uzo>
        <tld/>a forto,
        maso <fnt><bib>PIV2</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_e;&c_r;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>iner&ccaron;n&iacute;</trd>,
    <trd>nepru&zcaron;n&yacute;</trd>,
    <trd>setrva&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="en">inertial</trd>
  <trd lng="es">inercial</trd>
  <trdgrp lng="fr">
    <trd>d'<ind>inertie</ind></trd>,
    <trd>inertiel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tehetetlens&eacute;gi</trd>,
    <trd>inerci&aacute;lis</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6163;&#x6027;&#x306E;  <pr>&#x304B;&#x3093;&#x305B;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x60F0;&#x6027;&#x306E;  <pr>&#x3060;&#x305B;&#x3044;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="pt">inercial</trd>
  <trd lng="ru">&c_i;&c_n;&c_je;&c_r;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>iner&ccaron;n&yacute;</trd>,
    <trd>zotrva&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x456;&#x43D;&#x435;&#x440;&#x446;&#x456;&#x439;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x456;&#x43D;&#x435;&#x440;&#x442;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
  <trd lng="zh">&#x60F0;&#x6027; <pr>du&#xF2;x&#xEC;ng</pr></trd>
</drv>

<drv mrk="inerci.0uma">
  <kap><tld/>uma</kap>
  <snc mrk="inerci.0uma.inerteca">
    <dif>
      <ref tip="dif" cel="inert.0eca">Inerteca</ref>:
      <ekz>
        <ind><tld/>uma referenco-sistemo</ind> <fnt><bib>PIV2</bib></fnt>.
        <trd lng="ru">&c_i;&c_n;&c_je;&c_r;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a; &c_o;&c_t;&c_s;&c_ch;&c_jo;&c_t;&c_a;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_e;&c_r;&c_c;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>inerci&aacute;ln&iacute;</trd>,
    <trd>setrva&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="en">inertial</trd>
  <trd lng="es">inercial</trd>
  <trdgrp lng="fr">
    <trd>d'<ind>inertie</ind></trd>,
    <trd>inertiel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tehetetlens&eacute;gi</trd>,
    <trd>inerci&aacute;lis</trd>
  </trdgrp>
  <trd lng="pt">inercial</trd>
  <trd lng="ru">&c_i;&c_n;&c_je;&c_r;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>inerci&aacute;lny</trd>,
    <trd>zotrva&ccaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&#x456;&#x43D;&#x435;&#x440;&#x446;&#x456;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>
</drv>
</art>
