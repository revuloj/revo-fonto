<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tor</rad>/o
</kap>
<drv mrk="tor.0o">
  <kap><tld/>o</kap>
  <snc mrk="tor.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>P</bib></fnt>
    <dif>
      <ref tip="super" cel="surfac.0o.MAT">Surfaco</ref>, naskita de
      <ref cel="cirkl.0o.linio">cirklo</ref> rotacianta &ccirc;irka&ubreve;
      rekto, kiu situas en la ebeno de la cirklo, sed &gcirc;in ne sekcas;
      <ref tip="super" cel="solid.0o.geometrio">solido</ref>, kiun limas
      tia surfaco:
      <ekz>
        <tld/>o estas <ref cel="homeom.0a">homeomorfia</ref> al la
        <ref cel="produt.kartezia0o">kartezia produto</ref> de du cirkloj;
      </ekz>
      <ekz>
        pli &gcirc;enerale, n-dimensian <tld/>on oni nomas la kartezian
        produton de n cirkloj.
      </ekz>
    </dif>
    <rim>
      Bricard 
      <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt> &ccirc;i-sence uzas
      formon <ctl>toruso</ctl>.
    </rim>
    <trdgrp lng="de">
      <trd>Torus</trd>,
      <trd>Ringfl&auml;che</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>torus</trd>,
      <trd>toroid</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_o;&c_r;</trd>
  </snc>
  <snc mrk="tor.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Ronda elstara&jcirc;o &ccirc;irka&ubreve; kolono.
    </dif>
    <trd lng="en">torus</trd>
    <trd lng="nl">voetring<klr tip="ind"> (v.e.zuil)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_r;&c_u;&c_s;</trd>,
      <trd>&c_p;&c_o;&c_l;&c_u;&c_k;&c_r;&c_u;&c_g;&c_l;&c_y;&c_j; &c_f;&c_r;&c_i;&c_z;</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">tore</trd>
</drv>
</art>
