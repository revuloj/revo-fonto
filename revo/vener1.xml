<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: vener1.xml,v 1.9 2022/03/28 11:08:54 revo Exp $">
    <kap>
      <rad>Vener</rad>/o<fnt>Z</fnt>
    </kap>

    <drv mrk="vener1.0o">
      <kap><tld/>o</kap>
      <snc mrk="vener1.0o.mita">
        <uzo tip="fak">MIT</uzo>
        <dif>
          Roma <ref tip="super" cel="di.0o.politeisma">diino</ref> de
          amo, printempo kaj &gcirc;ardenoj, responda al la greka
          <ref tip="vid" cel="afrodit.0o">Afrodito</ref>:
          <ekz>
            montri&gcirc;is la bildo de <tld/>o, de la surtera <tld/>o,
            tiel vivoplena kaj flama, kiel Ticiano tion vidis
            <fnt><bib>Fab1</bib>, <lok>Kupra porko</lok></fnt>;
          </ekz><ekz>
            &scirc;ajnas al mi, ke vi deziris Kupidonon kun <tld/>o?
            <fnt><bib>Marta</bib></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_V;&c_e;&c_n;&c_e;&c_r;&c_a;</trd>
      <trd lng="bg">&c_V;&c_je;&c_n;&c_je;&c_r;&c_a;</trd>
      <trd lng="bo">&#xF54;&#xF0B;&#xF66;&#xF44;&#xF66;&#xF0B;</trd>
  <trd lng="ca">Venus</trd>
      <trd lng="cs">Venu&scaron;e</trd>
      <trd lng="de">Venus</trd>
      <trd lng="el">&Alfa;&phi;&rho;&omikron;&delta;&jota_ton;&tau;&eta;</trd>
      <trd lng="en">Venus</trd>
      <trd lng="es">Venus</trd>
      <trd lng="fr">V&eacute;nus</trd>
      <trd lng="hu">Venus</trd>
      <trd lng="it">Venere</trd>
      <trd lng="la">Venus</trd>
      <trd lng="nl">Venus</trd>
      <trd lng="no">Venus</trd>
      <trdgrp lng="pl">
         <trd>Wenera</trd>, <trd>Wenus</trd>
      </trdgrp>
      <trd lng="pt">V&ecirc;nus</trd>
      <trd lng="ru">&c_V;&c_je;&c_n;&c_je;&c_r;&c_a;</trd>
      <trd lng="sk">Venu&scaron;a</trd>
  <trd lng="uk">&#x412;&#x435;&#x43D;&#x435;&#x440;&#x430;</trd>
</drv>

    <drv mrk="vener1.0a">
      <kap><tld lit="v"/>a</kap>
      <snc mrk="vener1.0a.mita">
        <uzo tip="fak">MIT</uzo>
        <dif>
          Rilata al <tld/>o.
        </dif>
        <subsnc mrk="vener1.0a.MED">
          <uzo tip="fak">MED</uzo>
          <fnt><bib>VdE</bib></fnt>
          <fnt><bib>NPIV</bib></fnt>
          <dif>
            Komunikeblaj per seksumado:
            <ekz>
              <tld lit="v"/>a ulcero<fnt><bib>VdE</bib></fnt>;
              la <tld lit="v"/>a koliero
              <klr>(&ccirc;irka&ubreve;kola sifilisa erupcio)</klr>
              <fnt><bib>NPIV</bib></fnt>;
            </ekz><ekz>
              la&ubreve; mia opinio, bona kuracisto por
              <tld lit="v"/>aj malsanoj estus &ccirc;i tie multe pli utila
              <fnt>
               <aut>V. Ero&scirc;enko</aut>:
               <vrk>El vivo de &ccirc;uk&ccirc;oj</vrk>,
               <lok><url ref="http://www.esperanto.mv.ru/Cxukcxoj/cxukcx1.html">Unua skizo: Filip Onkudimov</url></lok>.
              </fnt>.
            </ekz>
          </dif>
          <trd lng="en">venereal</trd>
          <trd lng="fr">v&eacute;n&eacute;rien</trd>
          <trd lng="ru">&c_v;&c_je;&c_n;&c_je;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
        </subsnc>
      </snc>
    </drv>

  </art>

