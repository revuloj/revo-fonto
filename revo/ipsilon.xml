<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>9</ofc><rad>ipsilon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ipsilon.0o">
  <kap><tld/>o</kap>
  <snc mrk="ipsilon.0o.SNC">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Neesperanta <ref tip="super" cel="liter.0o.propra">litero</ref>
      (y, Y) uzata i.a. kiel:
    </dif>
  <trdgrp lng="fr">
  <trd>y</trd>,
  <trd>Y<klr> (i grec)</klr></trd>
  </trdgrp>
  <subsnc>
  <uzo tip="fak">KEM</uzo>
  <dif>
  (maj.) simbolo de itrio: <ekz>Y</ekz>;
  </dif>
  <trd lng="fr">yttrium</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (maj.) simbolo de jota: <ekz>Y</ekz>;
  </dif>
  <trd lng="fr">yotta</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">MAT</uzo>
  <dif>
  (min.) simbolo algebra por la dua nekonata kvanto: <ekz>y</ekz>;
  </dif>
  <trd lng="fr">y<klr> (axe des ordonn&eacute;es)</klr></trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (min.) simbolo de jokto: <ekz>y</ekz>;
  </dif>
  <trd lng="fr">yocto</trd>
  </subsnc>
  </snc>  
</drv>
</art>

