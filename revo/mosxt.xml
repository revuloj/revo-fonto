<kap>
  <ofc>*</ofc>
  <rad>mo&scirc;t</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="mosxt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Universala <ref tip="super" cel="titol.0o.RANG">titolo</ref> de &gcirc;entileco kaj respekto,
      uzebla por &ccirc;ia rango:
      <ekz>
        el la tri leteroj unu estis adresita:
        al Lia <ind>Episkopa <tld lit="M"/>o</ind>, Sinjoro N.;
        la dua: al Lia Grafa <tld lit="M"/>o, Sinjoro P.;
        la tria: al Lia <tld lit="M"/>o, Sinjoro D.
        <fnt><bib>F</bib>, <lok>&FE;42.</lok></fnt>;
        <trd lng="ru">&c_p;&c_r;&c_je;&c_o;&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
      </ekz><ekz>
        en la momento de la malfermo de nia dua
        kongreso mi esprimos en la nomo de vi &ccirc;iuj mian koran
        dankon al la brava svisa lando por la gastameco
        <klr>[&#x2026;]</klr> kaj al lia <tld lit="M"/>o la Prezidanto de
        la Svisa Konfederacio, kiu afable akceptis anta&ubreve; du
        monatoj nian delegitaron
        <fnt>
          <aut>&Z;</aut>: <vrk>Parolado en la Dua Kongreso
            (1906, &Gcirc;enevo)</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia re&gcirc;ina <tld/>o estas nekonsolebla kaj
        ploregas
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;ap. 22a</lok></fnt>;
      </ekz>
      <ekz>
        lia barona <tld/>o
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia sinjorega <tld/>o havas tre belan kastelon
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia ministra <tld/>o ne akceptus &gcirc;in
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i havas mian plej<tld/>an favoron
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="ekscel.0o">ekscelenco</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol; <klr tip="amb">(&c_z;&c_v;&c_a;&c_r;&c_o;&c_t;, &c_t;&c_y;&c_t;&c_u;&c_l;)</klr></trd>,
    <trd>&c_m;&c_o;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ib;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol; <klr tip="amb">(&c_z;&c_v;&c_a;&c_r;&c_o;&c_t;)</klr></trd>,
    <trd>&c_v;&c_a;&c_s;&c_p;&c_a;&c_n;</trd>,
    <trd>&c_ja;&c_g;&c_a;&c_m;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>excelence</trd>,
    <trd>jasnost</trd>,
    <trd>p&aacute;n (v&scaron;eob. titul)</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>Meurdez<klr tip="ind"> (titl a zouja&ntilde;s)</klr></trd>,
    <trd>Uhelded<klr tip="ind"> (titl a zouja&ntilde;s)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hoheit <klr>(Titel)</klr></trd>,
    <trd>Durchlaucht</trd>,
    <trd>Hochw&uuml;rden</trd>,
    <trd>Gn&auml;diger</trd>,
    <trd>Majest&auml;t</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Majesty</trd>,
    <trd>Highness</trd>,
    <trd>Excellency</trd>,
    <trd>Eminence</trd>,
    <trd>Reverend</trd>,
    <trd>Honor</trd>
  </trdgrp>
  <trd lng="es">titulo<klr tip="amb"> (honor&iacute;fico)</klr></trd>
  <trdgrp lng="fr">
    <trd>Majest&eacute;</trd>,
    <trd>Excellence</trd>
    <trd>Altesse</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x95A3;&#x4E0B;  <pr>&#x304B;&#x3063;&#x304B;</pr></trd>,
    <trd>&#x6BBF;&#x4E0B;  <pr>&#x3067;&#x3093;&#x304B;</pr></trd>,
    <trd>&#x965B;&#x4E0B;  <pr>&#x3078;&#x3044;&#x304B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mo&sacute;&cacute;</trd>,
    <trd>wysoko&sacute;&cacute;</trd>,
    <trd>mi&lstroke;o&sacute;&cacute;</trd>,
    <trd>mi&lstroke;o&sacute;ciwo&sacute;&cacute;</trd>,
    <trd>ja&sacute;niepan</trd>,
    <trd>ja&sacute;niepani</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>Majestade <klr tip="amb">(t&iacute;tulo)</klr></trd>,
    <trd>Emin&ecirc;ncia <klr tip="amb">(t&iacute;tulo)</klr></trd>,
    <trd>Alteza <klr tip="amb">(t&iacute;tulo)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;
    </trd>,
    <trd>&c_v;&c_y;&c_s;&c_o;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_i;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_v;&c_o;&c_s;&c_h;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;<klr> &c_i; &c_t;.&c_d;.
    (&c_o;&c_b;&c_shch;&c_je;&c_je; &c_t;&c_i;&c_t;&c_u;&c_l;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>y&uuml;kseklik</trd>,
    <trd>&uuml;st&uuml;nl&uuml;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_t;&c_i;&c_t;&c_u;&c_l; &c_z;&c_v;&c_je;&c_r;&c_t;&c_a;&c_n;&c_n;&c_ja; &c_d;&c_o;
    &c_v;&c_i;&c_s;&c_o;&c_k;&c_o;&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_je;&c_n;&c_i;&c_h; &c_o;&c_s;&c_ib;&c_b;:
    &c_v;&c_je;&c_l;&c_i;&c_ch;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_ib;&c_jeu;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_v;&c_o;&c_s;&c_h;&c_o;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_ja;&c_s;&c_n;&c_o;&c_v;&c_je;&c_l;&c_mol;&c_m;&c_o;&c_zh;&c_n;&c_ib;&c_s;&c_t;&c_mol; &c_ib; &c_t;.&c_p;.</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E3B;&#x516C; <pr>zh&#x1D4;g&omacron;ng</pr></trd>,
    <trd>&#x6BBF;&#x4E0B; <pr>di&agrave;nxi&agrave;</pr></trd>,
    <trd>&#x965B;&#x4E0B; <pr>b&igrave;xi&agrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mosxt.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata, propra al nobeloj kaj altranguloj:
      <ekz>
        havi lecionon pri tio, kiel oni kantas anta&ubreve;
        tre <tld/>aj sinjoroj
        <fnt>
          <aut>Eyvind Johnson, tr. Ferenc Szil&aacute;gyi</aut>:
          <vrk>Etoliano</vrk>.
          <lok>Vespera ru&gcirc;o anoncas ventegon. Inko
          Tyres&ouml;, 2001</lok>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_o;&c_zh;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_ja;&c_l;&c_mol;&c_m;&c_o;&c_zh;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>majest&auml;tisch</trd>,
    <trd>hoheitlich</trd>,
    <trd>durchlauchtig</trd>
  </trdgrp>
  <trd lng="fr">majestueux</trd>
  <trd lng="pl">mo&zdot;ny</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_mol;&c_m;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5A01;&#x98A8;&#x51DC;&#x51DC; <pr>w&emacron;if&emacron;ngl&#x1D0;nl&#x1D0;n</pr></trd>,
    <trd>&#x5A01;&#x98CE;&#x51DB;&#x51DB; <pr>w&emacron;if&emacron;ngl&#x1D0;nl&#x1D0;n</pr></trd>,
    <trd>&#x5A01;&#x98A8; <pr>w&emacron;if&emacron;ng</pr></trd>,
    <trd>&#x5A01;&#x98CE; <pr>w&emacron;if&emacron;ng</pr></trd>,
    <trd>&#x96C4;&#x58EE; <pr>xi&oacute;ngzhu&agrave;ng</pr></trd>,
    <trd>&#x96C4;&#x58EF; <pr>xi&oacute;ngzhu&agrave;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mosxt.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Altrangulo, gravulo, estro, eminentulo (ofte iom
      ironie a&ubreve; malaprobe):
      <ekz>
        ofenditaj <tld/>uloj ne pardonos
        <fnt>
          <bib>Kon11</bib>
          <lok>&leftquot;Dipatrino, forpelu Putin-on&rightquot;</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u post la komuna mono, e&ubreve;ropaj <tld/>uloj nun
        devos pensi pri komuna lingvo
        <fnt>
          <aut>Dafydd ap Fergus</aut>:
          <vrk>Komuna mono, komuna lingvo</vrk>. <bib>LOdE</bib>
        </fnt>?
      </ekz>
      <ekz>
        esperantistaj <tld/>uloj ne multe influas e&ubreve;ropajn politikistojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;2</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_y;&c_t;&c_u;&c_l;&c_a;&c_v;&c_a;&c_n;&c_a;&c_ja; &c_a;&c_s;&c_o;&c_b;&c_a;</trd>,
    <trd>&c_v;&c_ja;&c_l;&c_mol;&c_m;&c_o;&c_zh;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hoheit <klr>(Person)</klr></trd>,
    <trd>W&uuml;rdentr&auml;ger</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8CB4;&#x4EBA; <pr>&#x304D;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x8981;&#x4EBA; <pr>&#x3088;&#x3046;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x9AD8;&#x5B98; <pr>&#x3053;&#x3046;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mo&zdot;now&lstroke;adca</trd>,
    <trd>wielmo&zdot;a</trd>,
    <trd>szycha</trd>,
    <trd>ja&sacute;niepan</trd>,
    <trd>ja&sacute;niepani</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_i;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_mol;&c_m;&c_o;&c_zh;&c_a;</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_i;&c_t;&c_u;&c_l;&c_o;&c_v;&c_a;&c_n;&c_a; &c_o;&c_s;&c_o;&c_b;&c_a;</trd>
</drv>

<drv mrk="mosxt.grafa0o">
  <kap>grafa <tld/>o</kap>
  <snc>
    <dif>
      Honortitolo de grafo:
      <ekz>
        faru do jam tian favoron, via grafa <tld/>o
        <fnt>
          <aut>N.V. Gogol, tr. &Z;</aut>: <vrk>La revizoro</vrk>
          <lok>Akto 4a, Sceno 10a</lok>.
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="de">gr&auml;fliche <ind>Hoheit</ind></trd>
  <trd lng="pl">panie<klr> hrabio</klr></trd>
  <trd lng="ru">&c_s;&c_i;&c_ja;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="mosxt.imperiestra0o">
  <kap>imperiestra <tld/>o</kap>
  <snc>
    <dif>
      Honortitolo de imperiestro:
      <ekz>
        sed kiam Pa&ubreve;lo apelaciis, ke li estu rezervata
        por la decido de lia Imperiestra <tld lit="M"/>o,
        mi ordonis gardi lin, &gcirc;is mi povos lin sendi
        al Cezaro
        <fnt><bib>NT</bib><lok>&Ago; 25:21.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kaiserliche <ind>Hoheit</ind></trd>
  <trdgrp lng="pl">
    <trd><klr>cesarska </klr>mo&sacute;&cacute;</trd>,
    <trd>wysoko&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_je;&c_r;&c_a;&c_t;&c_o;&c_r;&c_s;&c_k;&c_o;&c_je;
  <ind>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</ind></trd>
</drv>

<drv mrk="mosxt.kardinala0o">
  <kap>kardinala <tld/>o</kap>
  <snc>
  <uzo tip="fak">KAT</uzo>
    <dif>
      Honortitolo de kardinalo:
      <ekz>
        li fervore servis Lian Kardinalan <tld lit="M"/>on
        en lia malamo al la re&gcirc;ino patrino
        <fnt>
          <aut>A. Dumas, tr. Kabe</aut>:
          <vrk>Tri musketeroj (Vol. 1a, &ccirc;ap. 5a)</vrk>.
          <lok>Internacia Krestomatio</lok>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_ib;&c_n;&c_e;&c_n;&c_c;&c_y;&c_ja;
  </trd>
  <trd lng="de">Eminenz</trd>
  <trd lng="es">eminencia</trd>
  <trd lng="fr">&Eacute;minence</trd>
  <trd lng="hu">eminencia</trd>
  <trdgrp lng="pl">
    <trd>emeninencja</trd>,
    <trd>ekscelencja</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_s;&c_o;&c_k;&c_o;&c_p;&c_r;&c_je;&c_o;&c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="tr">kardinal &uuml;nvan&inodot;</trd>
</drv>
<drv mrk="mosxt.pastra0o">
  <kap>pastra <tld/>o</kap>
  <snc>
    <dif>
      Honortitolo de pastro:
      <ekz>
        pardonu, via pastra <tld/>o, mi lasis lin ekstere
         <fnt>
           <aut>P. Lengyel</aut>: <vrk>La Filo</vrk>.
           <lok>Fundamenta Krestomatio. Parto 3a: Anekdotoj</lok>.
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Hochw&uuml;rden</trd>,
    <trd>Ehrw&uuml;rden</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_p;&c_o;&c_d;&c_o;&c_b;&c_i;&c_je;</trd>
</drv>

<drv mrk="mosxt.regxa0o">
  <kap>re&gcirc;a <tld/>o</kap>
  <snc>
    <dif>
      Honortitolo de re&gcirc;o:
      <ekz>
        mi diris al la re&gcirc;o: via re&gcirc;a <tld/>o, pardonu
        min
        <fnt><bib>F</bib>, <lok>&FE;42.</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="de">K&ouml;nigliche <ind>Hoheit</ind></trd>
  <trd lng="fr">Sire</trd>
  <trd lng="ja">&#x56FD;&#x738B;&#x965B;&#x4E0B;  <pr>&#x3053;&#x304F;&#x304A;&#x3046;&#x3078;&#x3044;&#x304B;</pr></trd>
  <trd lng="pl"><klr>kr&oacute;lewska </klr>mo&sacute;&cacute;</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_o;&c_l;&c_je;&c_v;&c_s;&c_k;&c_o;&c_je;
  <ind>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</ind>
  </trd>
</drv>

</art>

