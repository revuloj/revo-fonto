<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: splend.xml,v 1.5 2023/10/18 21:21:46 afido Exp $">
    <kap>
      <rad>splend</rad>/a
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="splend.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="impon.0a">Impona</ref> per brila lukso; pompe
      <ref cel="impres.0a">impresanta</ref>:
      <ekz>
        policisto <klr>[&#8230;]</klr> vagis digne anta&ubreve; <tld/>a teatro
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470; 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        belo <tld/>e brilis en viaj okuloj ridantaj kaj elfu&gcirc;emaj, kaj vi, &gcirc;ojplena kaj
        enpensema, vin levis sur la sojlon de juneco
        <fnt>
          <aut>Giacomo Leopardi</aut>
          <vrk><url
          ref="https://www.akademio-literatura.org/literaturo/baldur-ragnarsson/giacomo-leopardi/al-silvia">Al
          Silvia</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="barok.0a">baroka</ref>,
      <ref cel="bril.0a">brila</ref>,
      <ref cel="grandi.0a">grandioza</ref>,
      <ref cel="luks.0a">luksa</ref>,
      <ref cel="majest.0a">majesta</ref>,
      <ref cel="pomp.0a">pompa</ref>,
      <ref cel="puc.0a">puca</ref>,
      <ref cel="ricx.0a.valora">ri&ccirc;a<sncref/></ref>,
      <ref cel="sxik.0a">&scirc;ika</ref>,
      <ref cel="snob.0a">snoba</ref>
    </refgrp>
    </snc>
    <trdgrp lng="be">
      <trd>&c_c;&c_u;&c_d;&c_o;&c_w;&c_n;&c_y;</trd>,
      <trd>&c_ch;&c_a;&c_r;&c_o;&c_w;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>prachtvoll</trd>,
      <trd>prunkvoll</trd>,
      <trd>pr&auml;chtig</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>splendid</trd>,
      <trd>magnificent</trd>
    </trdgrp>
    <trd lng="fr">splendide</trd>
    <trd lng="pl">wspania&lstroke;y</trd>
</drv>
  </art>

