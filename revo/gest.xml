<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gest.xml,v 1.24 2023/08/31 14:29:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gest</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="gest.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gest.0o.MOV">
    <dif>
      &Ccirc;iu montra, esprima movo de la korpo, precipe de kapo, brako,
      mano:
      <ekz>
        jesa <tld/>o, nea <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de kolero, surprizo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mimik.0o">mimiko</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_zh;&c_e;&c_s;&c_t;</trd>
  <trd lng="bg">&c_zh;&c_je;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>gesto</trd>,
    <trd>posun&#x11B;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geste</trd>,
    <trd>Geb&auml;rde</trd>,
    <trd>Handzeichen</trd>
  </trdgrp>
  <trd lng="en">gesture</trd>
  <trd lng="es">gesto</trd>
  <trd lng="fr">geste<klr tip="ind"> (mouvement du corps)</klr></trd>
  <trd lng="hu">gesztus</trd>
  <trdgrp lng="ja">
    <trd>&#x8EAB;&#x3076;&#x308A;  [<ind>&#x307F;&#x3076;&#x308A;</ind>]</trd>,
    <trd>&#x624B;&#x3076;&#x308A;  [<ind>&#x3066;&#x3076;&#x308A;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">gebaar</trd>
  <trdgrp lng="pl">
    <trd>gest</trd>,
    <trd>mig</trd>,
    <trd>znak</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gesto<klr tip="amb"> (movimento de corpo)</klr></trd>, 
   <trd>aceno</trd> 
  </trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_s;&c_t;</trd>
  <trd lng="sk">posunok</trd>
  <trd lng="uk">&c_zh;&c_je;&c_s;&c_t;</trd>
</drv>

<drv mrk="gest.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="gest.0i.MOV">
    <dif>
      Fari <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_zh;&c_e;&c_s;&c_t;</ind></trd>
  <trd lng="bo">&#xF63;&#xF90;&#xF74;&#xF42;&#xF0B;&#xF56;&#xF62;&#xFA1;&#xF0B;&#xF56;&#xFB1;&#xF7A;&#xF51;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>kynout</trd>,
    <trd>zdravit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gesten machen</trd>,
    <trd>Handzeichen geben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gesture</trd>,
    <trd>gesticulate</trd>
  </trdgrp>
  <trd lng="es">hacer <ind>gestos</ind></trd>
  <trd lng="fr">faire un <ind>geste</ind></trd>
  <trd lng="hu">gesztust tesz</trd>
  <trd lng="ja">&#x8EAB;&#x3076;&#x308A;&#x3092;&#x3059;&#x308B;  [<ind>&#x307F;&#x3076;&#x308A;&#x3092;&#x3059;&#x308B;</ind>]</trd>
  <trd lng="nl">een <ind>gebaar</ind> maken</trd>
  <trdgrp lng="pl">
    <trd>robi&cacute;  <ind>gesty</ind></trd>,
    <trd>czyni&cacute;  <ind>gesty</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>fazer um <ind>gesto</ind></trd>, 
   <trd>fazer um <ind>aceno</ind></trd> 
  </trdgrp>
  <trd lng="ru">&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_zh;&c_je;&c_s;&c_t;</ind></trd>
  <trdgrp lng="sk">
    <trd>k&#xFD;vnu&tcaron;</trd>,
    <trd>zdravi&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_zh;&c_je;&c_s;&c_t;&c_i;&#x43A;&c_u;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="gest.0adi">
  <kap><tld/>adi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="gest.0adi.MOV">
    <dif>
      Fari multajn rimarkeblajn <tld/>ojn.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_e;&c_s;&c_t;&c_y;&c_k;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">gestikulovat</trd>
  <trdgrp lng="de">
    <trd>gestikulieren</trd>,
    <trd>mit den H&auml;nden <ind>herumfuchteln</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>make gestures</trd>,
    <trd>gesticulate</trd>
  </trdgrp>
  <trd lng="es">gesticular</trd>
  <trd lng="fr">gesticuler</trd>
  <trd lng="hu">gesztikul&aacute;l</trd>
  <trd lng="ja">&#x3057;&#x304D;&#x308A;&#x306B;&#x8EAB;&#x3076;&#x308A;&#x3092;&#x3059;&#x308B;  [<ind>&#x3057;&#x304D;&#x308A;&#x306B;&#x307F;&#x3076;&#x308A;&#x3092;&#x3059;&#x308B;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>gebaren maken</trd>,
    <trd>gesticuleren</trd>
  </trdgrp>
  <trd lng="pl">gestykulowa&cacute;</trd>
  <trdgrp lng="pt">
   <trd>gesticular</trd>, 
   <trd>acenar</trd> 
  </trdgrp>
  <trd lng="ru">&c_zh;&c_je;&c_s;&c_t;&c_i;&c_k;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">gestikulova&tcaron;</trd>
</drv>

<drv mrk="gest.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="gest.0ema.MOV">
    <dif>
      Inklina <tld/>adi parolante.
    </dif>
  </snc>
  <trd lng="cs">gestikuluj&#xED;c&#xED; p&rcaron;i &rcaron;e&#x10D;i</trd>
  <trd lng="de">gestenreich</trd>
  <trdgrp lng="en">
    <trd>demonstrative</trd>,
    <trd>gesticulating</trd>
  </trdgrp>
  <trd lng="es">gesticulador</trd>
  <trd lng="fr">gesticulateur</trd>
  <trd lng="hu">gesztikul&aacute;l&oacute;s</trd>
  <trd lng="ja">&#x8EAB;&#x3076;&#x308A;&#x306E;&#x591A;&#x3044;  [<ind>&#x307F;&#x3076;&#x308A;&#x306E;&#x304A;&#x304A;&#x3044;</ind>]</trd>
  <trd lng="pl">lubi&aogonek;cy  <ind>gestykulowa&cacute;</ind>  przy m&oacute;wieniu</trd>
  <trd lng="sk">gestikuluj&#xFA;ci pri re&#x10D;i</trd>
</drv>

<drv mrk="gest.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gest.for0i.KOMUNE">
    <dif>
      Forpeli per <tld/>o:
      <ekz>
        Urga for<tld/>is la provoketon
        <fnt><aut>E. de Zilah</aut>, <vrk>La Princo &ccirc;e la Hunoj</vrk>, 
        <lok>2011</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_g;&c_a;&c_n;&c_ja;&c_c;&c_mol; <klr>(&c_zh;&c_e;&c_s;&c_t;&c_a;&c_m;)</klr></trd>
  <trd lng="fr">&eacute;carter d'un geste</trd>
  <trdgrp lng="pl">
    <trd>odjecha&cacute;  <klr>(gest)</klr></trd>,
    <trd>odej&sacute;&cacute;   <klr>(gest)</klr></trd>
  </trdgrp>
</drv>
</art>
