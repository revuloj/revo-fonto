  <trd lng="uk">&#x41C;&#x43E;&#x43D;&c_r;&#x43E;&c_v;&c_ib;&#x44F; (&c_s;&c_t;&#x43E;&#x43B;&c_i;&c_c;&#x44F;)</trd>
  <trdgrp lng="zh">
    <trd>&#x8499;&#x7F57;&#x7EF4;&#x4E9A; <pr>M&#xE9;nglu&#xF3;w&#xE9;iy&#xE0;</pr></trd>,
    <trd>&#x8499;&#x7F85;&#x7DAD;&#x4E9E; <pr>M&#xE9;nglu&#xF3;w&#xE9;iy&#xE0;</pr></trd>
