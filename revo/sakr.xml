<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sakr.xml,v 1.21 2019/04/14 09:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sakr</rad>/o
</kap>
<drv mrk="sakr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Triangula <ref tip="super" cel="ost.0o">osto</ref>
      kuniganta la vertebran spinon kun la <ref cel="pelv.0o.ANA">pelvo</ref>:
      <ekz>
        la amikoj neniam vidis tian beston <klr>[&#8230;]</klr> la anta&ubreve;a duono de la korpo
        forte altis super la <tld/>o, la dorso estis oblikva
        <fnt>
          <aut>Ivan Efremov, trad. Jurij Finkel</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/ekumeno_08.html"
              >La Granda Arko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
        <ref tip="hom" cel="sakr1.0o">Sakro</ref> estas anka&ubreve; 
        nedeca esprimo de kolero.
    </rim>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_zh; <klr tip="amb">(&c_k;&c_o;&c_s;&c_t;&c_k;&c_a;)</klr></trd>
  <trd lng="ca">sacre</trd>
  <trd lng="cs">k&rcaron;&iacute;&zcaron;ov&aacute; kost</trd>
  <trd lng="de">Kreuzbein</trd>
  <trd lng="en">sacrum</trd>
  <trd lng="es">sacro</trd>
  <trd lng="fa">&a_A;&a_s;&a_t;&a_kh;&a_w;&a_A;&a_n; &a_kh;&a_A;&a_j;&f_y;</trd>
  <trd lng="fr">sacrum</trd>
  <trd lng="hu">keresztcsont</trd>
  <trdgrp lng="it">
   <trd>osso <ind>sacro</ind></trd>
  </trdgrp>
  <trd lng="nl">heiligbeen</trd>
  <trd lng="pl">ko&sacute;&cacute; krzy&zdot;owa</trd>
  <trdgrp lng="pt">
   <trd>osso <ind>sacro</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_t;&c_je;&c_c;</trd>
  <trd lng="sk">kr&iacute;&zcaron;ov&aacute; kos&tcaron;</trd>
</drv>
</art>
