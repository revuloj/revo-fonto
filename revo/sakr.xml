<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sakr.xml,v 1.25 2023/10/21 06:31:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sakr</rad>/o
</kap>
<drv mrk="sakr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Triangula <ref tip="super" cel="ost.0o">osto</ref>
      kuniganta la vertebran spinon kun la <ref cel="pelv.0o.ANA">pelvo</ref>:
      <ekz>
        li alportu <klr>[&#x2026;]</klr> la tutan voston, detran&ccirc;itan &ccirc;e la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 3:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi donas min mem kiel sklavon <klr>[&#x2026;]</klr> se mi ne 
        rompos la <tld/>on al tiu bubalo, kiu &scirc;in gardas
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>on kvaza&ubreve; tiklas aroj da formikoj
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la amikoj neniam vidis tian beston <klr>[&#x2026;]</klr> la 
        anta&ubreve;a duono de la korpo
        forte altis super la <tld/>o, la dorso estis oblikva
        <fnt>
          <aut>Ivan Efremov, trad. Jurij Finkel</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/ekumeno_08.html">La Granda Arko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      <ref tip="hom" cel="sakr1.0o">Sakro</ref> estas anka&ubreve; 
      nedeca esprimo de kolero.
    </rim>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_zh; <klr tip="amb">(&c_k;&c_o;&c_s;&c_t;&c_k;&c_a;)</klr></trd>
  <trd lng="ca">sacre</trd>
  <trd lng="cs">k&rcaron;&iacute;&zcaron;ov&aacute; kost</trd>
  <trd lng="de">Kreuzbein</trd>
  <trd lng="en">sacrum</trd>
  <trd lng="es">sacro</trd>
  <trd lng="fa">&a_A;&a_s;&a_t;&a_kh;&a_w;&a_A;&a_n; &a_kh;&a_A;&a_j;&f_y;</trd>
  <trd lng="fr">sacrum</trd>
  <trd lng="hu">keresztcsont</trd>
  <trdgrp lng="it">
   <trd>osso <ind>sacro</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x4ED9;&#x9AA8;  <pr>&#x305B;&#x3093;&#x3053;&#x3064;</pr></trd>
  <trd lng="nl">heiligbeen</trd>
  <trd lng="pl">ko&sacute;&cacute; krzy&zdot;owa</trd>
  <trdgrp lng="pt">
   <trd>osso <ind>sacro</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_t;&c_je;&c_c;</trd>
  <trd lng="sk">kr&iacute;&zcaron;ov&aacute; kos&tcaron;</trd>
  <trd lng="uk">&#x43A;&#x440;&#x438;&#x436;&#x43E;&#x432;&#x430; &#x43A;&#x456;&#x441;&#x442;&#x43A;&#x430;</trd>
</drv>
</art>
