<art mrk="$Id: himeno.xml,v 1.20 2023/11/15 17:08:00 revo Exp $">
<kap>
  <rad>himenopter</rad>/oj
</kap>
<drv mrk="himeno.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_ordoj" cel="ord.0o.BIO">Ordo</ref>
      el la klaso <ref tip="malprt" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Hymenoptera</trd>),
      karakterizata &ccirc;efe per du paroj da membranecaj flugiloj kaj kies
      supra paro estas pli longa:
      <ekz>
        la himenopteroj <klr>[&#x2026;]</klr> inkluzivas
        <ref tip="prt" cel="abel.0o">abelojn</ref>,
        <ref tip="prt" cel="formik.0o">formikojn</ref>
        kaj <ref tip="prt" cel="vesp.0o">vespojn</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18030">Himenopteroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_p;&c_o;&c_n;&c_ch;&c_a;&c_t;&c_a;&c_k;&c_r;&c_y;&c_l;&c_y;&c_ja;</trd>
  <trd lng="ca">himen&ograve;pters</trd>
  <trd lng="cs">blanok&rcaron;&iacute;dl&iacute;</trd>
  <trd lng="de">Hautfl&uuml;gler</trd>
  <trd lng="el">&ypsilon;&my;&epsilon;&ny;&omikron_ton;&pi;&tau;&epsilon;&rho;&alfa;</trd>
  <trd lng="fr">hym&eacute;nopt&egrave;res</trd>
  <trd lng="hu">h&aacute;rty&aacute;ssz&aacute;rny&uacute;ak</trd>
  <trd lng="it">imenotteri</trd>
  <trd lng="ja">&#x819C;&#x7FC5;&#x985E;  <pr>&#x307E;&#x304F;&#x3057;&#x308B;&#x3044;</pr></trd>
  <trd lng="nl">vliesvleugeligen</trd>
  <trd lng="pl">b&lstroke;onkoskrzyd&lstroke;e</trd>
  <trd lng="pt">himen&oacute;pteros</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_p;&c_o;&c_n;&c_ch;&c_a;&c_t;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_je;</trd>
  <trd lng="sk">blanokr&iacute;dly</trd>
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x435;&#x442;&#x438;&#x43D;&#x447;&#x430;&#x441;&#x442;&#x43E;&#x43A;&#x440;&#x438;&#x43B;&#x456;</trd>
  <trd lng="zh">&#x819C;&#x7FC5;&#x76EE; <pr>m&#xF3;ch&#xEC;m&#xF9;</pr></trd>
</drv>
</art>
