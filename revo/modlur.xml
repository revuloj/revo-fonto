<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>modlur</rad>/o
</kap>
<drv mrk="modlur.0o">
  <kap><tld/>o</kap>
  <snc mrk="modlur.0o.ARKI">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Ornamo garnituranta iujn partojn de konstruo, a&ubreve; meblo kaj
      konsistanta el paralelaj rubandaj elstara&jcirc;oj, &ccirc;u plataj &ccirc;u rondaj
      (konveksaj a&ubreve; kavaj), a&ubreve; el kombino de tiuj diversaj formoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="kornic.0o">kornico</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_p;&c_n;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_l;&c_ja;&c_p;&c_n;&c_y; &c_d;&c_e;&c_k;&c_o;&c_r;</trd>,
    <trd>&c_l;&c_ja;&c_p;&c_n;&c_y; &c_r;&c_e;&c_l;&c_mol;&c_je;&c_f;</trd>
  </trdgrp>
  <trd lng="en">mo(u)lding</trd>
  <trd lng="fr">moulure</trd>
  <trdgrp lng="hu">
    <trd>tag</trd>,
    <trd>tagozat</trd>
  </trdgrp>
  <trd lng="nl">lofwerk</trd>
  <trd lng="pt">moldura</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_p;&c_n;&c_o;&c_j; &c_o;&c_r;&c_n;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_l;&c_je;&c_p;&c_n;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
</art>
