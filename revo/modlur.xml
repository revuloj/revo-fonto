<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>modlur</rad>/o
</kap>
<drv mrk="modlur.0o">
   <kap><tld/>o</kap>
   <snc>
     <uzo tip="fak">BELA</uzo>
     <uzo tip="fak">ARKI</uzo>
     <dif>
       Ornamo garnituranta iujn partojn de konstruo, a&ubreve; meblo kaj
       konsistanta el paralelaj rubandaj elstara&jcirc;oj, &ccirc;u 
plataj &ccirc;u rondaj
       (konveksaj a&ubreve; kavaj), a&ubreve; el kombino de tiuj 
diversaj formoj.
     </dif>
     <refgrp tip="sin">
       <ref cel="listel.0o">listelo</ref>,
       <ref cel="cimati.0o">cimatio</ref>,
       <ref cel="kornic.0o">kornico</ref>,
       <ref cel="tor">toruso</ref>,
       <ref cel="astrag.0o">astragalo</ref>.
     </refgrp>
   </snc>
   <trd lng="en">mo(u)lding</trd>
   <trdgrp lng="hu">
     <trd>p&aacute;lcatag</trd>,
     <trd>lemeztag</trd>,
     <trd>l&eacute;ctag</trd>
   </trdgrp>
   <trd lng="nl">lofwerk</trd>
   <trd lng="fr">moulure</trd>
   <trdgrp lng="ru">
     <trd>&c_l;&c_je;&c_p;&c_n;&c_o;&c_j; 
&c_o;&c_r;&c_n;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
     <trd>&c_l;&c_je;&c_p;&c_n;&c_i;&c_n;&c_a;</trd>
   </trdgrp>
</drv>
</art>
