<art mrk="$Id: skajp.xml,v 1.5 2023/09/03 18:39:54 revo Exp $">
<kap>
    <rad>Skajp</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="skajp.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="fak">RET</uzo>    
    <uzo tip="fak">TEL</uzo>    
    <dif>
      <ref tip="dif" cel="mesagx.tuj0ilo.KOMP">Tujmesa&gcirc;ilo</ref> kaj
      <ref tip="dif" cel="ret.Inter0o.KOMP">interreta</ref>
      telefono-reto, kiu funkcias per samtavola komunikado:
      <ekz>
      <tld/>o estas sufi&ccirc;e vaste uzata de esperantistoj tra la
      mondo<fnt><bib>Viki</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_j;&c_p;</trd>
  <trd lng="en">Skype</trd>
  <trd lng="fr">Skype</trd>
  <trd lng="ja">&#x30B9;&#x30AB;&#x30A4;&#x30D7;</trd>
  <trd lng="pl">Skype</trd>
</drv>
</art>

