    <trd>&c_t;&c_v;&c_o;&c_r;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_v;&c_o;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_i;&c_s;&c_a;&c_t;&c_i; (&c_l;&c_ib;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_n;&c_i;&c_j; &c_t;&c_v;&c_ib;&c_r; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#32232; <pr>bi&amacron;n</pr></trd>,
    <trd>&#32534; <pr>bi&amacron;n</pr></trd>,
    <trd>&#35676;&#23531; <pr>p&#468;xi&ecaron;</pr></trd>,
    <trd>&#35889;&#20889; <pr>p&#468;xi&ecaron;</pr></trd>,
    <trd>&#32232;&#23531; <pr>bi&amacron;nxi&ecaron;</pr></trd>,
