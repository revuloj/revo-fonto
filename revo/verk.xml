    <trd>&#26360;&#12367;  <pr>&#12363;&#12367;</pr></trd>,
    <trd>&#21046;&#20316;&#12377;&#12427;  <pr>&#12379;&#12356;&#12373;&#12367;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
