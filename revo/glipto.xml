<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gliptodont</rad>/o
</kap>
<drv mrk="glipto.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PAL</uzo>
  <snc>
    <dif>
      Genro de grandegaj fosiliaj sendentaj mambestoj, vivinta en la
      kvaternara epoko de Ameriko.  
    </dif>
  </snc>
  <trd lng="de">Glyptodon</trd>
  <trd lng="fr">glyptodon</trd>
  <trdgrp lng="nl">
    <trd>reuzengordeldier</trd>,
    <trd>glyptodont</trd>,
    <trd>glyptodon</trd>
  </trdgrp>
 <trd lng="ru">&c_g;&c_l;&c_i;&c_p;&c_t;&c_o;&c_d;&c_o;&c_n;&c_t;</trd>
</drv>
</art>
