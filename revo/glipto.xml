<art mrk="$Id: glipto.xml,v 1.18 2025/08/23 22:29:30 revo Exp $">
<kap>
  <rad>gliptodont</rad>/o
</kap>
<drv mrk="glipto.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PAL</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      de grandegaj fosiliaj sendentaj <ref tip="malprt" cel="best.mam0oj">mambestoj</ref>
      (<trd lng="la">Glyptodon</trd>), vivinta en la kvaternara epoko de Ameriko:
      <ekz>
        la giganta pratatuo a&ubreve; <tld/>o estis praa, giganta tatuo a&ubreve; dazipo, kiu vivis
        origine en Sudameriko kaj dum la glaciepoko disvasti&gcirc;is anka&ubreve; en Nordameriko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=59578">Giganta pratatuo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/68/Glyptodon_clavipes_01.jpg/441px-Glyptodon_clavipes_01.jpg" prm="CC
    BY-SA 3.0">skeleto kaj karapaco de <tld/>o (Glyptodon clavipes), en la Muzeo pri Naturscienco de
        Berlino
        <fnt>
          <aut>H. Zell, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Glyptodon_clavipes_01.jpg">&WCW;/File:Glyptodon_clavipes_01.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ib;&c_p;&c_t;&c_a;&c_d;&c_o;&c_n;&c_t;</trd>
  <trd lng="ca">gliptodont</trd>
  <trd lng="de">Glyptodon</trd>
  <trd lng="fr">glyptodon</trd>
  <trd lng="hu">Glyptodon</trd>
  <trdgrp lng="ja">
    <trd>&#x30B0;&#x30EA;&#x30D7;&#x30C8;&#x30C9;&#x30F3;</trd>,
    <trd>&#x5F6B;&#x6B6F;&#29539;  <pr>&#x307B;&#x306F;&#12369;&#12384;&#12418;&#x306E;</pr></trd>,
    <trd>&#x30AA;&#x30AA;&#x30A2;&#x30EB;&#x30DE;&#x30B8;&#x30ED;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>reuzengordeldier</trd>,
    <trd>glyptodont</trd>,
    <trd>glyptodon</trd>
  </trdgrp>
  <trd lng="pl">glyptodon</trd>
  <trd lng="pt">gliptodonte</trd>
  <trd lng="ru">&c_g;&c_l;&c_i;&c_p;&c_t;&c_o;&c_d;&c_o;&c_n;&c_t;</trd>
  <trd lng="uk">&c_g;&#x43B;&c_ib;&#x43F;&c_t;&#x43E;&c_d;&#x43E;&#x43D;&c_t;</trd>
  <trd lng="zh">&#x5F6B;&#x9F52;&#29560; <pr>di&amacron;och&#x1D0;sh&#xF2;u</pr></trd>
</drv>
</art>
