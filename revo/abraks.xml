<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: abraks.xml,v 1.3 2024/11/26 18:38:08 afido Exp $">
    <kap>
      <rad>Abraks</rad>/o <fnt><aut>R. Eichholtz k. a.</aut> <vrk>Pekoteko</vrk>
      <lok>1994</lok></fnt>
    </kap>

<drv mrk="abraks.0o">
  <kap><tld/>o</kap>
  <snc mrk="abraks.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="super" cel="di.0ajxo">Dia&jcirc;o</ref> en persa mitologio kaj gnostikismo:
      <ekz>
        la nova amiko <klr>[&#8230;]</klr> klerigas lin <klr>[pri]</klr> la
        dia&jcirc;o <tld/>o, kiu estas ne nur bona, sed anka&ubreve; malbona
        <fnt>
          <vrk><url ref="https://revuoesperanto.org/demian"
              >Akvarele nebula beleco, recenzo pri Demian</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi kun mia sa&gcirc;a amiko <tld/>o <klr>(korvo nomita la&ubreve; la dia&jcirc;o)</klr> surprizas
        la grandajn sor&ccirc;istinojn
        <fnt>
          <vrk><url ref="https://familioj.miraheze.org/wiki/La_malgranda_sor%C4%89istino"
              >La malgranda sor&ccirc;istino, recenzo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Abraxas</trd>
    <trd lng="en">Abraxas</trd>
    <trd lng="fr">Abraxas</trd>
    <trd lng="ru">&c_A;&c_b;&c_r;&c_a;&c_k;&c_s;&c_a;&c_s;</trd>
  </snc>
  <trd lng="be">&c_A;&c_b;&c_r;&c_a;&c_k;&c_s;&c_a;&c_s;</trd>
</drv>
</art>

