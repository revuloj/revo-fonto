  </trdgrp>
  <trd lng="en">showcase</trd>
  <trd lng="fr">affichage</trd>
  <trdgrp lng="ja">
    <trd>&#38515;&#21015;&#x53F0;  <pr>&#12385;&#12435;&#x308C;&#12388;&#12384;&#12356;</pr></trd>,
    <trd>&#x5C55;&#x793A;&#x53F0;  <pr>&#12390;&#12435;&#12376;&#12384;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="pl">st&oacute;&lstroke; demonstracyjny</trd>
</drv>

<drv mrk="tabl.skrib0o">
  <kap><ofc>*</ofc>skrib<tld/>o,
     <var><kap><ofc>*</ofc>skribo<tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <tld lit="T"/>o uzata por skribi, ordinare kun tirkestoj, ilaroj kaj
      similaj:
      <ekz>
        en mia skribo<tld/>o sin trovas kvar tirkestoj
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        skrib<tld/>o a&ubreve; skribo<tld/>o (= <tld/>o, sur kiu oni skribas)
        <fnt><bib>F</bib>, <lok>&FE; 27</lok></fnt>.
      </ekz>
      <ekz>
        &ccirc;u vi vidas tiun grandan nigran makulon sur la skrib<tld/>o de la
        patro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sin sidigis apud la skribo<tld/>o kaj iom pensinte, &scirc;i
        skribis la sekvantajn liniojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kandelingo ricevis lokon sur verda skrib<tld/>o
        <fnt>
          <bib>Fab2</bib>skribis
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> sidis apude apud skribo<tld/>o unu kontra&ubreve; la
        dua
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_s;&c_mol;&c_m;&c_o;&c_v;&c_y; <ind>&c_s;&c_t;&c_o;&c_l;</ind></trd>
