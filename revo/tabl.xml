<drv mrk="tabl.0estro">
  <kap><tld/>estro</kap>
  <snc mrk="tabl.0estro.KUI">
    <dif>
      <ref tip="super" cel="serv.0isto.KOMUNE">Servisto</ref>
      &ccirc;efoficanta super la man&gcirc;oj en ri&ccirc;a domo
      a&ubreve;
      en multehomaj festenoj;
      <ref tip="sin" cel="butler.0o.KUI">butlero</ref>:
      <ekz>
        <tld/>estro min gaste invitis al festenoj
        <fnt>
          <aut>A. Mickiewicz, trad. A. Grabowski</aut>,
          <vrk><url ref="http://malnova.esperanto.pl/page.php?tid=3013">
          Sinjoro Tadeo</url></vrk>,
          <lok>1918</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Butler</trd>,
      <trd>Diener</trd>,
      <trd>Haushofmeister</trd>
    </trdgrp>
    <trd lng="en">butler</trd>
    <trdgrp lng="fa">
      <trd>&f_p;&f_y;&a_sh;&a_kh;&a_d;&a_m;&a_t;</trd>,
      <trd>&a_kh;&a_w;&a_A;&a_n;&a_s;&a_A;&a_l;&a_A;&a_r;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ma&icirc;tre d'h&ocirc;tel</trd>,
      <trd>majordome<klr> (ma&icirc;tre d'h&ocirc;tel)</klr></trd>
    </trdgrp>
    <trd lng="id"><ind>kepala</ind> pelayan</trd>
    <trd lng="it">maggiordomo</trd>
    <trd lng="nl">butler</trd>
    <trd lng="pl">stolnik</trd>
  </snc>
</drv>

<drv mrk="tabl.0eto">
  <kap><ofc>*</ofc><tld/>eto</kap>
  <snc mrk="tabl.0eto.MEBL">
    <dif>
      Mallar&gcirc;a a&ubreve; malalta
      <ref tip="super" cel="tabl.0o.MEBL">tablo</ref>:
      <ekz>
        mi a&ccirc;etis por la infanoj <tld/>eton kaj kelke da
        se&gcirc;etoj
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
      </ekz>
      <ekz>
        avo kapsignis en la direkto al la spegulo,
        sur kies <tld/>eto staris la kalendaro
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>etoj &scirc;ar&gcirc;itaj per bagatela&jcirc;oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_o;&c_l;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>stolek</trd>,
    <trd>stole&ccaron;ek</trd>
  </trdgrp>
  <trd lng="de">Tischchen</trd>
  <trdgrp lng="es">
    <trd>mesita</trd>,
    <trd>mesilla</trd>
  </trdgrp>
  <trd lng="fr">tablette</trd>
  <trdgrp lng="hu">
    <trd>asztalka</trd>,
    <trd>kisasztal</trd>
  </trdgrp>
  <trd lng="ja">&#x5C0F;&#21331;  <pr>&#12375;&#12423;&#12358;&#12377;&#12368;&#x308B;</pr></trd>
  <trd lng="pl">stolik</trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_l;&c_i;&c_k;</trd>
  <trd lng="sk">stol&ccaron;ek</trd>
  <trd lng="uk">&c_s;&c_t;&c_o;&c_l;&c_i;&c_k;</trd>
</drv>

<drv mrk="tabl.al0igxi">
  <kap>al<tld/>i&gcirc;i,
  <var><kap>&ccirc;e<tld/>i&gcirc;i</kap></var></kap>
  <snc mrk="tabl.al0igxi.KOMUNE">
    <dif>
      Alsidi&gcirc;i &ccirc;e <tld/>o:
      <ekz>
        <klr>[ili]</klr> al<tld/>i&gcirc;is en kafejoj por sin sensoifigi
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni tuj &ccirc;e<tld/>i&gcirc;u kaj vi rakontos al ni dum la man&gcirc;o, &ccirc;u konsentite
        <fnt>
          <bib>ChV</bib>
        </fnt>?
      </ekz>
      <ekz>
        al<tld/>i&gcirc;inte, li tintigis la sonorilon kaj, ne atendante &ccirc;eson de la bruo,
        komencis paroli
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        grupo da oficiroj &ccirc;e<tld/>i&gcirc;intaj por
        vesperman&gcirc;o
        <fnt><bib>Munchhausen</bib>, <lok>&ccirc;apitro 10a</lok></fnt>;
      </ekz><ekz>
        kiam la man&gcirc;a&jcirc;o estis preta, ili al<tld/>i&gcirc;is
        <fnt>
          <aut>trad. R. Bartal</aut>,
          <vrk><url ref="http://egalite.hu/literat/nepodevirino.pdf">
          Nepo de virino</url></vrk>,
          <lok>[2009]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_d;&c_a;&c_c;&c_mol; &c_z;&c_a; <ind>&c_s;&c_t;&c_o;&c_l;</ind></trd>
  <trdgrp lng="de">
    <trd>sich zu <ind>Tisch</ind> begeben</trd>,
    <trd>sich um den Tisch <ind>setzen</ind></trd>
  </trdgrp>
  <trd lng="es">sentarse a la <ind>mesa</ind></trd>
  <trdgrp lng="fr">
    <trd>s'<ind>attabler</ind></trd>,
    <trd>se mettre &agrave; <ind>table</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C6;&#x30FC;&#x30D6;&#x30EB;&#x306B;&#12388;&#x304F;
    <pr>&#12390;&#x30FC;&#12406;&#x308B;&#x306B;&#12388;&#x304F;</pr></trd>,
    <trd>&#x98DF;&#21331;&#x306B;&#12388;&#x304F;  <pr>&#12375;&#12423;&#x304F;&#x305F;&#x304F;&#x306B;&#12388;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>siada&cacute; przy <ind>stole</ind></trd>,
    <trd>siada&cacute; do <ind>sto&lstroke;u</ind></trd>
  </trdgrp>
</drv>

<drv mrk="tabl.sur0igi">
  <kap>sur<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tabl.sur0igi.KOMUNE">
    <dif>
      Meti sur <tld/>on, tiel disponigi:
      <ekz>
        oni sur<tld/>igis por li aparte, kaj por ili aparte
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 43:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni sur<tld/>igu plej delikatajn man&gcirc;a&jcirc;ojn <klr>[kaj]</klr> la plej &ccirc;armaj
        knabinoj dancu &ccirc;irka&ubreve; &scirc;i
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        servoknabinoj venis ver&scirc;i vinon kaj sur<tld/>igi pladojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 85a, volumo 3a, p.
        79a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_mol; &c_n;&c_a; <ind>&c_s;&c_t;&c_o;&c_l;</ind></trd>,
    <trd>&c_s;&c_e;&c_r;&c_v;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">d&aacute;t na st&uring;l</trd>
  <trdgrp lng="de">
    <trd>servieren</trd>,
    <trd>auftischen</trd>
  </trdgrp>
  <trd lng="en">serve <klr>(food)</klr></trd>
  <trdgrp lng="es">
    <trd>poner sobre la mesa</trd>,
    <trd>servir<klr> (poner sobre la mesa)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mettre sur la table</trd>,
    <trd>servir<klr> (mettre sur la table)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
