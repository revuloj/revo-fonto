<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>aed</rad>/o <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="aed1.0o">
  <kap><tld/>o</kap>

  <snc mrk="aed1.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      (<trd lng="la">Aedes</trd>) de <ref tip="super" cel="moskit.0o">moskitoj</ref>,
      kies plej konata speco <nom>A. aegytpi</nom> 
      transportas la flavan <ref tip="vid" cel="febr.0o.MED">febron</ref>.
    </dif>
    <bld
      lok="&WCU;/thumb/d/d0/Aedes_aegypti.jpg/320px-Aedes_aegypti.jpg"
      prm="GFDL 1.2"
      >Egipta <tld/>o (Aedes aegypti), Dar es Salaam, Tanzanio
        <fnt>
          <aut>Muhammad Mahdi Karim, propra verko</aut>
          <lok><url ref="&WCW;/File:Aedes_aegypti.jpg"
              >https://commons.wikimedia.org/wiki/File:Aedes_aegypti.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="es">mosquito aedes aegypti<klr tip="amb"> (fiebre
    amarilla)</klr></trd>
    <trdgrp lng="it">
      <trd>aedes<klr> (zanzara)</klr></trd>,
      <trd><ind>zanzara</ind> tigre</trd>,
      <trd><ind>zanzara</ind> tigre egiziana</trd>
    </trdgrp>
    <trd lng="ca">aedes</trd>
   </snc>

</drv>

  </art>

