<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>pedagogi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="pedago1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      Scienco pri instruado kaj edukado, arto instrui kaj eduki precipe
      infanojn:
      <ekz>
        lernejo, kiu edukas la&ubreve; Steiner-<tld/>o, malfermis siajn pordojn
        por senpaperuloj<fnt><aut>Guido Van Damme</aut>: <vrk>Sep en unu
        bato</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 3a, p.
        6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="didakt.0o">didaktiko</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_d;&c_a;&c_g;&c_o;&c_g;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_e;&c_d;&c_a;&c_g;&c_o;&c_g;&c_ib;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>didactique<klr> (subst.)</klr></trd>,
    <trd>p&eacute;dagogie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pedag&oacute;gia</trd>,
    <trd>nevel&eacute;stan</trd>,
    <trd>nevel&eacute;studom&aacute;ny</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>opvoedkunde</trd>,
    <trd>pedagogie</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>P&auml;dagogig</trd>,
    <trd>Didaktik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="pedago1.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="instru.pri0a">priinstrua</ref>
    <ekz>
      tro malofte oni kritike konsideras ties enhavon, aspekton, valoron &ndash;
      sciencan, <tld/>an, lingvan<fnt><aut>Wim M. A. De Smet</aut>:
      <vrk>Ekzotaj arboj en Esperantujo (Lilli Giloteaux, Arboj en
      A&ubreve;stralio kaj suda E&ubreve;ropo)</vrk>, <bib>Monato</bib><lok>jaro 1997a,
      numero 10a, p. 19a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_y;&c_d;&c_a;&c_k;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>p&auml;dagogisch</trd>,
    <trd>didaktisch</trd>
  </trdgrp>
  <trd lng="es">did&aacute;ctico</trd>
  <trdgrp lng="fr">
    <trd>didactique</trd>,
    <trd>p&eacute;dagogique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pedag&oacute;giai</trd>,
    <trd>nevel&eacute;stani</trd>,
    <trd>nevel&eacute;studom&aacute;nyi</trd>
  </trdgrp>
  <trd lng="nl">didactisch</trd>
</drv>

<drv mrk="pedago1.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      Sciencisto studanta la <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_d;&c_a;&c_g;&c_o;&c_g; <klr>(&c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_v;&c_je;&c_c;)</klr></trd>
  <trdgrp lng="fr">
    <trd>p&eacute;dagogue</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nevel&eacute;studom&aacute;nyi szakember</trd>,
    <trd>pedag&oacute;gus <klr tip="amb">(tud&oacute;s)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>opvoedkundige</trd>,
    <trd>pedagoog</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>P&auml;dagoge</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;
  <klr>(&c_u;&c_ch;&c_jo;&c_n;&c_y;&c_j;)</klr></trd>
</drv>
</art>
