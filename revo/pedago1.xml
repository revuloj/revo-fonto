<drv mrk="pedago1.0o">
  <kap><tld/>o</kap>
  <snc mrk="pedago1.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Scienco pri instruado kaj edukado,
      arto instrui kaj eduki precipe infanojn;
      <ref tip="sin" cel="didakt.0o.PED">didaktiko</ref>:
      <ekz>
        lernejo, kiu edukas la&ubreve; <nom>Steiner</nom>-<tld/>o, malfermis siajn
        pordojn por senpaperuloj
        <fnt><aut>G. Van Damme</aut>: <vrk>Sep en unu
        bato</vrk>, <bib>Monato</bib>, <lok>1999/03, p.
        6</lok></fnt>;
      </ekz>
      <ekz>
        moderna <tld/>o postulas, ke anka&ubreve; malsani&gcirc;intaj infanoj <klr>[&#x2026;]</klr> rajtas
        kaj devas kunstudi en la samaj lernejoj kie aliuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009356.php">Sistemo ne imuna</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        fakaj prelegoj pri la muzikrilataj merkato, <tld/>o, sociologio, politiko,
        kulturo, ktp
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2010/010062.php">Merkato dampita</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_d;&c_a;&c_g;&c_o;&c_g;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_e;&c_d;&c_a;&c_g;&c_o;&c_g;&c_ib;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">pedagogia</trd>
  <trdgrp lng="cs">
    <trd>pedagogie</trd>,
    <trd>pedagogika</trd>,
    <trd>vychovatelstv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>P&auml;dagogig</trd>,
    <trd>Didaktik</trd>
  </trdgrp>
  <trd lng="en">pedagogy</trd>
  <trdgrp lng="fr">
    <trd>didactique<klr> (subst.)</klr></trd>,
    <trd>p&eacute;dagogie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pedag&oacute;gia</trd>,
    <trd>nevel&eacute;stan</trd>,
    <trd>nevel&eacute;studom&aacute;ny</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6559;&#x80B2;&#x5B66;  <pr>&#x304D;&#x3087;&#x3046;&#x3044;&#x304F;&#x304C;&#x304F;</pr></trd>,
    <trd>&#x5150;&#x7AE5;&#x6559;&#x6388;&#x6CD5;  <pr>&#x3058;&#x3069;&#x3046;&#x304D;&#x3087;&#x3046;&#x3058;&#x3085;&#x307B;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>opvoedkunde</trd>,
    <trd>pedagogie</trd>
  </trdgrp>
  <trd lng="pl">pedagogia</trd>
  <trd lng="pt">pedagogia</trd>
  <trd lng="ro">pedagogie</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;&c_i;&c_ja;</trd>
  </trdgrp>
