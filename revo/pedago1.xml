<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>pedagogi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="pedago1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Scienco pri instruado kaj edukado, a&ubreve; arto instrui kaj eduki la
      infanojn.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>opvoedkunde</trd>,
    <trd>pedagogie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="pedago1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Sciencisto studanta la <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>opvoedkundige</trd>,
    <trd>pedagoog</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g; <klr>(&c_u;&c_ch;&c_jo;&c_n;&c_y;&c_j;)</klr></trd>
</drv>
</art>
