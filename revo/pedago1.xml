<drv mrk="pedago1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pedago1.0isto.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Sciencisto studanta la <tld/>on:
      <ekz>
        la barono estis <tld/>isto kaj intencis plibonigi la edukadon de gejunuloj per
        sporto
        <fnt>
          <vrk><url ref="https://esperantaretradio.blogspot.com/2016/08/olimpikaj-ludoj-la-metaforo.html#!">Olimpikaj
          Ludoj, la metaforo</url></vrk>
          <lok>Esperanta Retradio</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jan Amos Komensk&yacute;</nom> &dash; la tuta kultura mondo festis la 400-an naski&gcirc;tagon de la
        granda <tld/>isto
        <fnt>
          <aut>Th. Kili&aacute;n</aut>
          <vrk><url ref="http://www.literatura.bucek.name/brno/kilian-theodor_85-naskigtago-masaryk.pdf">85-a
          naski&gcirc;tago de T.G. Masaryk</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_d;&c_a;&c_g;&c_o;&c_g; <klr>(&c_n;&c_a;&c_v;&c_u;&c_k;&c_o;&c_v;&c_je;&c_c;)</klr></trd>
  <trd lng="ca">pedagog</trd>
  <trd lng="cs">pedagog</trd>
  <trdgrp lng="de">
    <trd>P&auml;dagoge</trd>,
    <trd>Erziehungswissenschaftler</trd>
  </trdgrp>
  <trd lng="en">pedagogue</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;dagogue</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nevel&eacute;studom&aacute;nyi szakember</trd>,
    <trd>pedag&oacute;gus <klr tip="amb">(tud&oacute;s)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x6559;&#x80B2;&#x5B66;&#x8005;  <pr>&#x304D;&#x3087;&#x3046;&#x3044;&#x304F;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  <trdgrp lng="nl">
    <trd>opvoedkundige</trd>,
    <trd>pedagoog</trd>
  </trdgrp>
  <trd lng="pl">pedagog</trd>
  <trd lng="ro">pedagog</trd>
  <trd lng="ru">&c_p;&c_je;&c_d;&c_a;&c_g;&c_o;&c_g;
  <klr>(&c_u;&c_ch;&c_jo;&c_n;&c_y;&c_j;)</klr></trd>
  <trd lng="sk">pedag&oacute;g</trd>
  <trdgrp lng="zh">
    <trd>&#x6559;&#x4E66;&#x5320; <pr>ji&#x101;osh&#x16B;ji&#xE0;ng</pr></trd>,
    <trd>&#x6559;&#x66F8;&#x5320; <pr>ji&#x101;osh&#x16B;ji&#xE0;ng</pr></trd>,
