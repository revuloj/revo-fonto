<drv mrk="pedago1.0a">
  <kap><tld/>a</kap>
  <snc mrk="pedago1.0a.PED">
    <dif>
      <ref tip="dif" cel="instru.pri0a">Priinstrua</ref>:
      <ekz>
        tro malofte oni kritike konsideras ties enhavon, aspekton, valoron
        &ndash; sciencan, <tld/>an, lingvan
        <fnt>
        <aut>W. M. A. de Smet</aut>,
        <vrk>Ekzotaj arboj en Esperantujo</vrk>,
        <bib>Monato</bib>,
        <lok>1997/10, p. 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>a revuo, eldonita de la flandra ministrejo pri edukado, aperis raporto
        pri la seksa konduto de lernejanoj
         <fnt>
          <aut>Guido van Damme</aut>
          <vrk><url ref="&Monato;/1995/A950102.php">Gejunula seksumado</url></vrk>
          <bib>Monato</bib>
          <lok>1995/01, p. 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la leningrada <tld/>a instituto
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Monato;/2003/007812.html">Estas dan&gcirc;ere esti
          poeto</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro pretendas havi <tld/>an sistemon, per kiu li povus el &ccirc;iu sana infano
        eduki geniulon pri iu ajn fako, kaj la edukadon de siaj infanoj li nomas
        <tld/>a eksperimento, per kiu li pruvis la efikecon de sia sistemo
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="&Monato;/2007/009012.php">Pri edukado de geniuloj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel instruistojn oni dungis unue <tld/>e neinstruitajn islamanojn a&ubreve; teologojn
        el eksterlando
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2012/010837.php">100 jarojn le&gcirc;o pri
          islamo</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_v;&c_u;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_y;&c_d;&c_a;&c_k;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">pedag&ograve;gic</trd>
