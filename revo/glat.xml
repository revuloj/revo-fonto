<drv mrk="glat.mal0ajxo">
  <kap>
    mal<tld/>a&jcirc;o
  </kap>
  <snc mrk="glat.mal0ajxo.FIZ">
    <dif>
      Mal<tld/>a loko de objekto:
      <ekz>
        palpetante per la piedo &ccirc;iujn mal<tld/>a&jcirc;ojn
        <fnt>
          <aut>H. Vallienne</aut>
          <vrk>Kastelo de Prelongo</vrk>
          <lok>1907</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="glat.mal0ajxo.FIG">
    <dif>
      Misa&jcirc;o, <ref tip="vid" cel="gxen.0i">&gcirc;eno</ref>:
      <ekz>
        stilaj mal<tld/>a&jcirc;oj
        <fnt>
          <aut>I. Lapenna</aut>
          <vrk>Esperanto en Perspektivo</vrk>
          <lok>1974</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
     <ref cel="tuber.0o">tubero</ref>
  </refgrp>
  <trdgrp lng="ca">
    <trd>entrebanc, trava</trd>,
    <trd>aspresa</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rugosit&eacute;<klr tip="ind"> (in&eacute;galit&eacute; de
    surface)</klr></trd>,
    <trd>accroc<klr tip="ind"> (incident)</klr></trd>,
    <trd>anicroche</trd>,
    <trd>incident</trd>,
    <trd>obstacle</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3067;&#x3053;&#x307C;&#x3053;</trd>,
