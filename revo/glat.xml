    <trd>seret</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3056;&#x3089;&#x3056;&#x3089;&#x3057;&#x305F;</trd>,
    <trd>&#x3054;&#x3064;&#x3054;&#x3064;&#x3057;&#x305F;</trd>,
    <trd>&#x3067;&#x3053;&#x307C;&#x3053;&#x306E;</trd>
  </trdgrp>
  <trd lng="nl">ruw<klr tip="ind"> (niet glad)</klr></trd>
  <trdgrp lng="pt">
   <trd>&aacute;spero</trd>,
   <trd>rugoso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_je;&c_r;&c_sh;&c_a;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_sh;&c_je;&c_r;&c_o;&c_h;&c_o;&c_v;&c_a;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>drap&lcaron;av&yacute;</trd>,
    <trd>hrbo&lcaron;at&yacute;</trd>,
    <trd>nerovn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
