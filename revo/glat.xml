<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glat.xml,v 1.36 2025/11/08 18:37:29 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>glat</rad>/a
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="glat.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="glat.0a.FIZ">
    <dif>
      Mal<ref tip="ant" cel="rasp.0a">raspa</ref>, sen elstara&jcirc;oj sur la supra&jcirc;o; sur kiu la fingro
      facile glitas:
      <ekz>
        <tld/>a vitro, &scirc;tofo, ha&ubreve;to;
      </ekz>
      <ekz>
        la akvo fari&gcirc;is <tld/>a kiel spegulo
        <fnt><bib>Fab2</bib>, <lok>Najbaraj familioj</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a kvaza&ubreve; manplato
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e kombitaj haroj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        vangoj <tld/>e razitaj
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Esav</nom> estas ja homo harkovrita kaj mi <klr>(<nom>Jakob</nom>)</klr>
        estas homo <tld/>a
        <fnt><bib>MT</bib>, <lok>&Gen; 27:11</lok></fnt>;
      </ekz>
      <ekz>
        finita kaj <tld/>igita
        <klr>(tute preta)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>suau</trd>,
      <trd>llis</trd>
    </trdgrp>
    <trd lng="es">liso, -a</trd>
  <trdgrp lng="id">
    <trd>licin</trd>,
    <trd>klimis</trd>,
    <trd>halus</trd>,
    <trd>lancap</trd>,
    <trd>lecok</trd>,
    <trd>lelas</trd>,
    <trd>lincir</trd>
  </trdgrp>
  </snc>
  <snc mrk="glat.0a.senembarasa">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senobstrukca, senembarasa:
      <ekz>
        la vojo, kompreneble, ne estas <tld/>a, nek rekta, sed plena de obstakloj kaj
        zigzagoj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter la <tld/>a&jcirc;oj de la valo estas via parto
        <fnt><bib>MT</bib> <lok>&Jes; 57:6</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>lliure</trd>,
      <trd>rodat</trd>
    </trdgrp>
    <trd lng="es">sin obst&aacute;culos</trd>
  </snc>
  <snc mrk="glat.0a.facila">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Facila, progresanta sen malhelpoj a&ubreve; baroj:
      <ekz>
        <tld/>a laboro, stilo, parolo;
      </ekz>
      <ekz>
        mi esperas, ke &ccirc;io <tld/>e pasos
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[komence]</klr> mi <tld/>e travenis, sed post la limo <klr>[&#x2026;]</klr> ili
        kaptas min
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 39</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nova delegitaro efektive funkcias pli <tld/>e, &ccirc;ar plendoj pri neplenumitaj
        servopetoj preska&ubreve; tute malaperis
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 2 (52)</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>e venkis en la konkurso;
      </ekz>
      <ekz>
        la afero ne iras <tld/>e
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        por <tld/>e mensogi oni spriton bezonas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vivi sate kaj <tld/>e
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vivo <ind><tld/>e</ind> ne fluas, &ccirc;iam batas kaj skuas;
        <trd lng="fr">bien<klr tip="ind"> (sans
        anicroche)</klr></trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="buter.0o">butero</ref>,
      <ref tip="vid" cel="glit.0eglate">glite-glate</ref>,
      <ref cel="rel.0o">relo</ref>.
    </refgrp>
    <trdgrp lng="ca">
      <trd>flu&iuml;d</trd>,
      <trd>natural</trd>
    </trdgrp>
    <trd lng="es">f&aacute;cil</trd>
  <trdgrp lng="id">
    <trd>lancar</trd>,
    <trd>mulus</trd>
  </trdgrp>
  </snc>
  <snc mrk="glat.0a.flata">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Karesa, dol&ccirc;a, lerte <ref tip="vid" cel="flat.0i">flata</ref>:
      <ekz>
        virino, kies paroloj estas <tld/>aj
        <fnt><bib>MT</bib> <lok>&Sen; 2:16</lok></fnt>;
      </ekz>
      <ekz>
        per sia <tld/>a bu&scirc;o &scirc;i lin entiris
        <fnt><bib>MT</bib> <lok>&Sen; 7:21</lok></fnt>;
      </ekz>
      <ekz>
        la bu&scirc;o de mal&ccirc;astulino elver&scirc;as mielon kaj
        &scirc;ia gor&gcirc;o estas pli <tld/>a ol oleo
        <fnt><bib>MT</bib> <lok>&Sen; 5:3</lok></fnt>;
      </ekz>
      <ekz>
        por gardi vin kontra&ubreve; <tld/>a lango de fremdulino
        <fnt><bib>MT</bib> <lok>&Sen; 6:24</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>e komplimenti iun.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>dol&ccedil;</trd>,
      <trd>tendre</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>gentil</trd>,
      <trd>suave<klr> (figurativo)</klr></trd>,
      <trd>dulce<klr> (figurativo)</klr></trd>
    </trdgrp>
    <trd lng="en">slick</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_ib;</trd>,
    <trd>&c_r;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_ja;&t_ma;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trd lng="cs">hladk&yacute;</trd>
  <trd lng="de">glatt</trd>
  <trd lng="en">smooth</trd>
  <trdgrp lng="fr">
    <trd>&eacute;gal<klr tip="ind"> (sans asp&eacute;rit&eacute;)</klr></trd>,
    <trd>lisse</trd>,
    <trd>plat<klr tip="ind"> (lisse)</klr></trd>,
    <trd>sans <ind>asp&eacute;rit&eacute;</ind></trd>,
    <trd>coulant<klr tip="ind"> (sans &agrave;-coup)</klr></trd>,
    <trd>sans <ind>&agrave;-coup</ind></trd>,
    <trd>glabre</trd>,
    <trd>flatteur</trd>,
    <trd>sans <ind>anicroche</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&het;&lamed;&qof;</trd>,
    <trd>&het;&lamed;&qof;&lamed;&qof;</trd>,
    <trd>&mem;&lamed;&vav;&tet;&shin;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x306A;&#x3081;&#x3089;&#x304B;&#x306A;</trd>,
    <trd>&#x3059;&#x3079;&#x3059;&#x3079;&#x3057;&#x305F;</trd>,
    <trd>&#x5E73;&#x3089;&#x306A;  <pr>&#x305F;&#x3044;&#x3089;&#x306A;</pr></trd>,
    <trd>&#x4EBA;&#x5F53;&#x305F;&#x308A;&#x306E;&#x3088;&#x3044;  <pr>&#x3072;&#x3068;&#x3042;&#x305F;&#x308A;&#x306E;&#x3088;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">glad</trd>
  <trdgrp lng="pt">
   <trd>liso<klr tip="ind"> (adj.)</klr></trd>,
   <trd>plano<klr tip="ind"> (adj.)</klr></trd>,
   <trd>polido<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hladk&yacute;</trd>,
    <trd>ploch&yacute;</trd>,
    <trd>rovn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_ib;&c_v;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E00;&#x5F84; <pr>y&imacron;j&igrave;ng</pr></trd>,
    <trd>&#x5149;&#x6ED1; <pr>gu&amacron;nghu&aacute;</pr></trd>,
    <trd>&#x516C;&#x7136; <pr>g&omacron;ngr&aacute;n</pr></trd>,
    <trd>&#x76F4;&#x622A;&#x4E86;&#x5F53; <pr>zh&iacute;ji&eacute;li&#x1CE;od&agrave;ng</pr></trd>,
    <trd>&#x9806;&#x5229; <pr>sh&ugrave;nl&igrave;</pr></trd>,
    <trd>&#x5E73;&#x5766; <pr>p&iacute;ngt&#x1CE;n</pr></trd>,
    <trd>&#x6ED1; <pr>hu&aacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="glat.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="glat.0igi.KOMUNE">
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi sian barbon;
      </ekz>
      <ekz>
        karese <tld/>igi <klr>(ordigi)</klr> al knabo la harojn;
      </ekz>
      <ekz>
        <klr>[la vento]</klr> plugas, ree <tld/>igas, tordas, alpelas
        novajn akvajn montojn
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 11</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>igi <klr>(trabati)</klr> la vojon al iu;
      </ekz>
      <ekz>
        <tld/>igi <klr>(reguligi)</klr> sian stilon;
      </ekz>
      <ekz>
        <tld/>igi la malfacila&jcirc;ojn;
      </ekz>
      <ekz>
        lia malafabla viza&gcirc;o iom <tld/>i&gcirc;is<fnt>Z</fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="glad.0i">gladi</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_w;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>allisar</trd>,
    <trd>aplanar</trd>,
    <trd>igualar</trd>,
    <trd>deslliurar</trd>,
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hladit</trd>,
    <trd>uhladit</trd>,
    <trd>uhlazovat</trd>,
    <trd>urovnat</trd>,
    <trd>vyhladit</trd>,
    <trd>zahladit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gl&auml;tten</trd>,
    <trd><ind>glatt</ind> streichen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>smooth</trd>,
    <trd>smoothen</trd>
  </trdgrp>
  <trdgrp lng="es">
   <trd>alisar</trd>,
   <trd>suavizar<klr> (figurativo)</klr></trd>,
   <trd>pulir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lisser</trd>,
    <trd>aplanir<klr tip="ind"> (rendre lisse, supprim&eacute; les
    difficult&eacute;s)</klr></trd>,
    <trd>peaufiner</trd>,
    <trd>polir<klr tip="ind"> (peaufiner)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&he;&het;&lamed;&yod;&qof;</ind></trd>,
    <trd>&lamed;<ind>&lamed;&tet;&shin;</ind></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>licin</baz>melicinkan</trd>,
    <trd><baz>lancar</baz>melancarkan</trd>,
    <trd><baz>mulus</baz>memuluskan</trd>
  </trdgrp>
  <trd lng="ja">&#x306A;&#x3081;&#x3089;&#x304B;&#x306B;&#x3059;&#x308B;</trd>
  <trdgrp lng="nl">
    <trd>glad maken</trd>,
    <trd>glad <ind>strijken</ind></trd>,
    <trd>polijsten</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>alisar</trd>,
   <trd>polir</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_o;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vyhladi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_g;&c_l;&c_a;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_ib;&c_v;&c_n;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x62BF; <pr>m&#x1D0;n</pr></trd>,
    <trd>&#x62C9;&#x76F4; <pr>l&amacron;zh&iacute;</pr></trd>,
    <trd>&#x78E8;&#x5149; <pr>m&ograve;gu&amacron;ng</pr></trd>,
    <trd>&#x5F04;&#x5E73; <pr>n&ograve;ngp&iacute;ng</pr></trd>
  </trdgrp>
</drv>

    <drv mrk="glat.0umi">
      <kap><tld/>umi <fnt><bib>PIV1</bib></fnt></kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="glat.0umi.KOMUNE">
        <dif>
          Konduki (plej ofte ripete kaj samdirekte)  la
          manplaton a&ubreve; la fingrojn sur io karesata:
          <ekz>
            infano <tld/>umas la hundon;
          </ekz>
          <ekz>
            li manon || Unu al sia nigra silka zono metis, ||
            Per dua sian grizan barbon <tld/>umetis
            <fnt>
              <aut>A. Mickiewicz, trad. A. Grabowski</aut>
              <vrk>Sinjoro Tadeo</vrk> <lok>Libro 4a</lok>.
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_g;&c_l;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
        <trd>&c_l;&c_a;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="ca">
        <trd>acariciar</trd>,
        <trd>acaronar</trd>
      </trdgrp>
      <trdgrp lng="cs">
        <trd>hladit</trd>,
        <trd>laskat</trd>
      </trdgrp>
      <trd lng="en">stroke</trd>
      <trd lng="fr">flatter<klr> (de la main)</klr></trd>
      <trdgrp lng="id">
        <trd>belai</trd>,
        <trd>elus</trd>,
        <trd>usap</trd>,
        <trd><baz>belai</baz>membelai</trd>,
        <trd><baz>elus</baz>mengelus</trd>,
        <trd><baz>usap</baz>mengusap</trd>
      </trdgrp>
      <trdgrp lng="ja">
    <trd>&#x306A;&#x3067;&#x308B;</trd>,
    <trd>&#x306A;&#x3067;&#x3064;&#x3051;&#x308B;</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
      <trd lng="uk">&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_i;</trd>
</drv>

<drv mrk="glat.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Raspa, maldol&ccirc;a por la tu&scirc;o; obstrukca; malfacila:
      <ekz>
        mal<tld/>a lano, vojo, verko, vivo;
      </ekz>
      <ekz>
        mal<tld/>aj versoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_r;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_g;&c_l;&c_a;&c_d;&c_k;&c_ib;</trd>,
    <trd>&c_sh;&c_u;&c_r;&c_p;&c_a;&c_t;&c_y;</trd>
