<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sark</rad>/i
</kap>
<drv mrk="sark.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Flegi kulturatan teron forigante la herba&ccirc;ojn:
      <ekz>
        <tld/>i bedon;
      </ekz>
    </dif>
  </snc>
  <trd lng="de">j&auml;ten</trd>
  <trd lng="en">weed</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;sherber</trd>,
    <trd>sarcler</trd>
  </trdgrp>
  <trd lng="hu">gyom</trd>
  <trdgrp lng="nl">
    <trd>wieden</trd>,
    <trd>schoffelen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_p;&c_a;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sark.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Diversforma speciala ilo por <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>weeder</trd>,
    <trd>weeding tool</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>raclette<klr> (sarcloir)</klr></trd>,
    <trd>sarcloir</trd>
  </trdgrp>
  <trd lng="hu">gyoml&aacute;l&oacute;kapa</trd>
  <trd lng="nl">schoffel</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_t;&c_y;&c_g;&c_a;</trd>,
    <trd>&c_t;&c_ja;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sark.0indajxo">
  <kap><tld/>inda&jcirc;o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      For&jcirc;eta&jcirc;o.
    </dif>
  </snc>
  <trd lng="fr">mauvaise herbe</trd>
  <trd lng="hu">gyom</trd>
  <trd lng="nl">uitschot</trd>
  <trd lng="ru">&c_s;&c_o;&c_r;&c_n;&c_ja;&c_k;</trd>
</drv>

<drv mrk="sark.el0i">
  <kap><tld/>o</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sark.el0i.AGR">
    <dif>
      Forigi herba&ccirc;ojn el kulturata tero.
    </dif>
    <trd lng="fr">&eacute;radiquer</trd>
  </snc>
  <snc mrk="sark.el0i.FIG">
  <uzo tip="stl">FIG</uzo>
    <dif>
      Forigi mankojn, erarojn, makulojn el io:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        el<tld/>i nekorekta&jcirc;ojn el teksto.
      </ekz>
    </dif>
    <trd lng="fr">nettoyer <klr> (les impuret&eacute;s)</klr></trd>
  </snc>
</drv>
</art>
