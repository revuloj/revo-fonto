<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sark</rad>/i
</kap>
<drv mrk="sark.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Flegi kulturitan teron forigante la herba&ccirc;ojn:
      <ekz>
        <tld/>i bedon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        el<tld/>i nekorekta&jcirc;ojn el teksto.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">j&auml;ten</trd>
  <trd lng="en">weed</trd>
  <trdgrp lng="nl">
    <trd>wieden</trd>,
    <trd>schoffelen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_p;&c_a;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sark.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Diversforma speciala ilo por <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>weeder</trd>,
    <trd>weeding tool</trd>
  </trdgrp>
  <trd lng="nl">schoffel</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_t;&c_y;&c_g;&c_a;</trd>,
    <trd>&c_t;&c_ja;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_a;&c_p;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sark.0indajxo">
  <kap><tld/>inda&jcirc;o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      For&jcirc;eta&jcirc;o.
    </dif>
  </snc>
  <trd lng="nl">uitschot</trd>
  <trd lng="ru">&c_s;&c_o;&c_r;&c_n;&c_ja;&c_k;</trd>
</drv>
</art>
