<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>sulfur</rad>/o
</kap>
<drv mrk="sulfur.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <mlg>S</mlg>
   <uzo tip="fak">KEM</uzo>
   <snc>
     <dif>
       &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> 
kun atomnumero 16, atompezo 32,064, simbolo S. Flava
       solida senodora, facile fandebla kaj brulema <ref tip="super" 
cel="metalo.0o">metaloido</ref>, kiun oni
       trovas en la vulkanaj elsputa&jcirc;oj a&ubreve; ekstraktas el 
metalaj
       kombina&jcirc;oj.
     </dif>
   </snc>
   <trd lng="be">&c_s;&c_je;&c_r;&c_a;</trd>      
   <trdgrp lng="br">
     <trd>soufr</trd>,
     <trd>sulfur</trd>
   </trdgrp>
   <trd lng="de">Schwefel</trd>
   <trd lng="en">sulfur</trd>
   <trd lng="es">azufre</trd>
   <trd lng="fr">soufre</trd>
   <trd lng="hu">k&eacute;n</trd>
   <trd lng="it">zolfo</trd>
   <trd lng="nl">zwavel</trd>
   <trd lng="pt">enxofre</trd>
   <trd lng="ru">&c_s;&c_je;&c_r;&c_a;</trd>
</drv>
</art>
