<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sulfur</rad>/o
</kap>
<drv mrk="sulfur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia elemento kun atomnumero 16, atompezo 32,064, simbolo S.
Flava
      solida senodora, facile fandebla kaj brulema metaloido, kiun oni
      trovas en la vulkanaj elsputa&jcirc;oj a&ubreve; ekstraktas el metalaj
      kombina&jcirc;oj.
    </dif>
  </snc>
  <trd lng="de">Schwefel</trd>
  <trd lng="hu">k&eacute;n</trd>
  <trd lng="nl">zwavel</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_a;</trd>
</drv>
</art>
