<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>1</ofc>
   <rad>mitr</rad>/o
</kap>
<drv mrk="mitr.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">KAT</uzo>
   <snc>
     <dif>
       Altforma dupinta kapvesto de episkopoj kaj superaj prelatoj por
       la solenaj religiaj ceremonioj.
     </dif>
     <refgrp tip="sin">
       <ref cel="cidar.0o">cidaro</ref>,
       <ref cel="tiar.0o">tiaro</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_m;&c_ib;&c_t;&c_r;&c_a;</trd>
   <trd lng="de">Mitra</trd>
   <trdgrp lng="hu">
     <trd>p&uuml;sp&ouml;ks&uuml;veg</trd>,
     <trd>mitra</trd></trdgrp>
   <trd lng="nl">mijter</trd>
   <trd lng="fr">mitre</trd>
   <trdgrp lng="pl">
     <trd>mitra</trd>,
     <trd>infu&lstroke;a</trd>
   </trdgrp>
   <trd lng="pt">mitra</trd>
   <trd lng="ru">&c_m;&c_i;&c_t;&c_r;&c_a;</trd>
</drv>
</art>
