<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mitr.xml,v 1.22 2023/10/19 16:45:37 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>mitr</rad>/o
</kap>
<drv mrk="mitr.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">KAT</uzo>
   <snc>
     <dif>
       Altforma dupinta <ref tip="super" cel="vest.kap0o">kapvesto</ref>
       de episkopoj kaj superaj prelatoj por la solenaj religiaj ceremonioj:
      <ekz>
        alligu sur ili <tld/>ojn, por ke al ili apartenu la pastreco la&ubreve; eterna le&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 29:9</lok>
        </fnt>.
      </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="cidar.0o">cidaro</ref>,
       <ref cel="tiar.0o">tiaro</ref>.
     </refgrp>
   </snc>
  <trd lng="be">&c_m;&c_ib;&c_t;&c_r;&c_a;</trd>
  <trd lng="ca">mitra</trd>
  <trdgrp lng="cs">
    <trd>infule</trd>,
    <trd>mitra</trd>
  </trdgrp>
  <trd lng="de">Mitra</trd>
  <trd lng="en">mitre</trd>
  <trd lng="fa">&f_k;&a_l;&a_A;&a_h; &a_A;&a_s;&a_q;&a_f;&f_y;</trd>
  <trd lng="fr">mitre</trd>
  <trdgrp lng="hu">
     <trd>p&uuml;sp&ouml;ks&uuml;veg</trd>,
     <trd>mitra</trd></trdgrp>
  <trd lng="it">mitra<klr> (copricapo)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30DF;&#x30C8;&#x30E9;</trd>,
    <trd>&#x53F8;&#x6559;&#x51A0;  <pr>&#x3057;&#x304D;&#x3087;&#x3046;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">mijter</trd>
  <trdgrp lng="pl">
     <trd>mitra</trd>,
     <trd>infu&lstroke;a</trd>
   </trdgrp>
  <trd lng="pt">mitra</trd>
  <trd lng="ru">&c_m;&c_i;&c_t;&c_r;&c_a;</trd>
  <trd lng="sk">mitra</trd>
  <trd lng="uk">&#x43C;&#x438;&#x442;&#x440;&#x430;</trd>
</drv>
</art>
