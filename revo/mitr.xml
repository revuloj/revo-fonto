<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>mitr</rad>/o
</kap>
<drv mrk="mitr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Altforma dupinta kapvesto de episkopoj kaj superaj prelatoj por la
      solenaj religiaj ceremonioj.
    </dif>
    <refgrp tip="sin">
      <ref cel="cidar.0o">cidaro</ref>,
      <ref cel="tiar.0o">tiaro</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">mijter</trd>
  <trd lng="ru">&c_m;&c_i;&c_t;&c_r;&c_a;</trd>
</drv>
</art>
