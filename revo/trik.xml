<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>trik</rad>/i
</kap>
<drv mrk="trik.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Fari ma&scirc;teksa&jcirc;on plektante fadenojn per vergetoj a&ubreve; per speciala
      ma&scirc;ino.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_z;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_a;&c_p;&c_r;., &c_s;&c_mol;&c_p;&c_ib;&c_c;&c_a;&c_m;&c_ib;)</klr></trd>
  <trd lng="de">stricken</trd>
  <trd lng="en">knit</trd>
  <trd lng="fr">tricoter</trd>
  <trd lng="hu">k&ouml;t <klr tip="ind">(t&udblac;vel)</klr></trd>  
  <trd lng="nl">breien</trd>
  <trd lng="ru">&c_v;&c_ja;&c_z;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_s;&c_p;&c_i;&c_c;&c_a;&c_h; &c_i; &c_t;.&c_p;.)</klr></trd>
</drv>

<drv mrk="trik.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Rekta vergeto uzata por <tld/>i,
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_p;&c_ib;&c_c;&c_a; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_v;&c_ja;&c_z;&c_a;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>,
    <trd>&c_p;&c_r;&c_u;&c_t;&c_o;&c_k; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_v;&c_ja;&c_z;&c_a;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  </trdgrp>
  <trd lng="de">Stricknadel</trd>
  <trd lng="fr">aiguille &agrave; tricoter</trd>
  <trd lng="hu">k&ouml;t&odblac;t&udblac;</trd>  
  <trd lng="nl">breinaald</trd>
  <trd lng="ru">&c_v;&c_ja;&c_z;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_s;&c_p;&c_i;&c_c;&c_a;</trd>
</drv>

<drv mrk="trik.krocx0i">
  <kap>kro&ccirc;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <tld lit="T"/>i per vergeto kro&ccirc;ilforme entran&ccirc;ita &ccirc;e la ekstrema&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_z;&c_a;&c_c;&c_mol; &c_k;&c_r;&c_u;&c_ch;&c_k;&c_o;&c_m;</trd>
  <trd lng="de">h&auml;keln</trd>
  <trd lng="fr">faire du <ind>crochet</ind></trd>
  <trd lng="hu">horgol</trd>  
  <trd lng="nl">haken</trd>
  <trd lng="ru">&c_v;&c_ja;&c_z;&c_a;&c_t;&c_mol; &c_k;&c_r;&c_ju;&c_ch;&c_k;&c_o;&c_m;</trd>
</drv>
</art>
