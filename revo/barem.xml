<art mrk="$Id: barem.xml,v 1.20 2023/10/14 08:39:48 revo Exp $">
<kap>
  <rad>barem</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="barem.0o">
<kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="tabel.0o.TIP">Tabelo</ref> de anta&ubreve;pretaj kalkuloj:
      <ekz>
        <tld/>o de primaj faktoroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=87318">Divizoro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la helena astronomo <nom>Hipparchus</nom> (-190...-120) kreis <tld/>on de &hcirc;ordaj funkcioj
        donantaj la longecon de la &hcirc;ordo por &ccirc;iu angulo, kaj ekzistas referencoj al
        lia uzado de polusaj koordinatoj por priskribi stelajn poziciojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=164494">Polusa koordinata sistemo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_b;&c_l;&c_ib;&c_c;&c_a; &c_g;&c_a;&c_t;&c_o;&c_v;&c_y;&c_h; &c_v;&c_y;&c_l;&c_ib;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_ja;&c_w;</trd>
  <trd lng="ca">barem</trd>
  <trdgrp lng="cs">
    <trd>barem</trd>,
    <trd>barrem</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>numerische <ind>Tafel</ind></trd>,     
     <trd>numerische Tabelle</trd>,
     <trd>Wertetafel</trd>,
     <trd>Wertetabelle</trd>
  </trdgrp>
  <trd lng="en">(arithmetic) <ind>table</ind></trd>
  <trd lng="es">baremo</trd>
  <trd lng="fa">&a_b;&a_A;&a_r;&a_kasra;&a_m;</trd>
  <trd lng="fr">bar&egrave;me</trd>
  <trd lng="he">&tet;&bet;&lamed;&tav; &het;&yod;&shin;&vav;&bet;&yod;&fmem;</trd>
  <trd lng="hu">(sz&aacute;m&iacute;t&aacute;si) <ind>t&aacute;bl&aacute;zat</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x89A7;&#x8868;  <pr>&#x3044;&#x3061;&#x3089;&#x3093;&#x3072;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x65E9;&#x898B;&#x8868;  <pr>&#x306F;&#x3084;&#x307F;&#x3072;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_b;&c_l;&c_i;&c_c;&c_a; 
&c_g;&c_o;&c_t;&c_o;&c_v;&c_y;&c_h; 
&c_v;&c_y;&c_ch;&c_i;&c_s;&c_l;&c_je;&c_n;&c_i;&c_j;</trd>
  <trd lng="sk">v&yacute;po&ccaron;tov&aacute; tabu&lcaron;ka</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_n;&c_i;&c_g;&c_a;</trd>,
    <trd>&c_z;&c_b;&c_ib;&c_r;&c_n;&c_i;&c_k; &c_t;&c_a;&c_b;&c_l;&c_i;&c_c;&c_mol; &c_g;&c_o;&c_t;&c_o;&c_v;&c_i;&c_h; &c_o;&c_b;&c_ch;&c_i;&c_s;&c_l;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
