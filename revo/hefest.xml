    <kap>
      <rad>Hefest</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="hefest.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIT</uzo>
  <snc>
    <dif>
      <klr>(en la helena mitologio)</klr> <ref tip="super" cel="di.0o">Dio</ref> de fajro,
      vulkanoj kaj for&gcirc;ado:
      <ekz>
        &Ccirc;iu el ili foriris dormi en sian lo&gcirc;ejon, &parsl; Kiun al &ccirc;iu lamapieda artisto
        Hefesto &parsl; Ekkonstruis per sia multescianta prudento
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        neestingeble eksciti&gcirc;is la ridado de l&#8217;&#8239;beataj dioj kiam ili vidis <tld/>on
        rapide movi&gcirc;antan tra l&#8217;&ccirc;ambrego
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Tri</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Hephaistos</trd>
    <trd lng="en">Hephaestus</trd>
    <trd lng="fr">H&eacute;pha&iuml;stos</trd>
    <trd lng="grc">&Eta_densa_acute;&phi;&alfa;&jota;&sigma;&tau;&omikron;&sigma_fina;</trd>
    <trd lng="ru">&c_G;&c_je;&c_f;&c_je;&c_s;&c_t;</trd>
  </snc>
</drv>

  </art>

