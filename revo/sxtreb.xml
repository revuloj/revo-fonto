<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: sxtreb.xml,v 1.1 2024/03/21 19:19:42 afido Exp $">
    <kap>
      <rad>&scirc;treb</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="sxtreb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KON</uzo>
  <snc mrk="sxtreb.0o.KON">
    <dif>
      Klinita flanka konstruelemento, forkondukanta fortojn de &ccirc;efelemento;
      <ref tip="sin" cel="apog.kontraux0ilo">kontra&ubreve;apogilo</ref>:
      <ekz>
        la argankonstruistoj kro&ccirc;i&gcirc;is per genuo al <tld/>o, dume ili <klr>[&#8230;]</klr>
        &scirc;ovis masivajn trabojn
        <fnt>
          <aut>B. Traven, trad. Dorothea kaj Hans-Georg Kaiser</aut>
          <vrk><url ref="https://books.google.de/books?id=KtiaEAAAQBAJ&amp;lpg=PA53&amp;ots=lrdE8aA0fK&amp;pg=PA53"
              > La trezoro de la Sierra Madre</url></vrk>
          <lok>p. 53</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Strebe</trd>
    <trdgrp lng="en">
      <trd>stiffener</trd>,
      <trd>stay</trd>
    </trdgrp>
    <trd lng="fr">contrefort</trd>
  </snc>
</drv>

  </art>

