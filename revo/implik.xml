<vortaro>
<art mrk="$Id: implik.xml,v 1.25 2022/04/30 08:42:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>implik</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="implik.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="implik.0i.movojn">
    <dif>
      <ref tip="super" cel="facil.mal0igi">Malfaciligi</ref> a&ubreve; 
      <ref tip="super" cel="help.mal0i.iun">malhelpi</ref> 
      la movojn de iu a&ubreve; io, senorde
      &ccirc;irka&ubreve;volvante fadenojn a&ubreve;
      &ccirc;irka&ubreve;plektante malliberigilojn:
      <ekz>
        la vento <tld/>is lian barbon kun la &scirc;nuro
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i havis cindron en la <tld/>itaj haroj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        buloj da inter<tld/>itaj fadenoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        La krimulino tiam sur lin &jcirc;etis // Teksa&jcirc;on faldori&ccirc;an kaj <tld/>an
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve;volvis lin la fatalaj &scirc;nuroj, en kiuj &ccirc;e &ccirc;iu movo li pli <tld/>is la
        manojn kaj la piedojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brid.0i">bridi</ref>,
      <ref cel="tord">&ccirc;irka&ubreve;tordi</ref>,
      <ref cel="volv.cxirkaux0i">&ccirc;irka&ubreve;volvi</ref>,
      <ref cel="gxen.0i">&gcirc;eni</ref>,
      <ref cel="lig.0i">ligi</ref>,
      <ref cel="obstru.0i">obstrukci</ref>,
      <ref cel="vind.0i">vindi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>verfangen</trd>,
      <trd>verfilzen</trd>,
      <trd>verstricken</trd>,
      <trd>verheddern</trd>
    </trdgrp>
    <trd lng="es">enredar</trd> 
    <trdgrp lng="hu">
      <trd>belegabaly&iacute;t</trd>,
      <trd>belekever</trd>,
      <trd>&ouml;sszekusz&aacute;l</trd>
    </trdgrp>
    <trdgrp lng="pt">
     <trd>enredar</trd>, 
     <trd>embara&ccedil;ar</trd> 
    </trdgrp>
  </snc>
  <snc mrk="implik.0i.aferojn">
    <dif>
      <ref tip="super" cel="facil.mal0igi">Malfaciligi</ref>, 
      <ref tip="super" cel="help.mal0i.ion">malhelpi</ref> 
      la agadon de iu a&ubreve; la efikon a&ubreve; solvon de io per komplika&jcirc;oj, 
      senorda&jcirc;oj kaj konfuza&jcirc;oj:
      <ekz>
        per sia pekado malbona homo sin <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 29:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Fariseoj <klr>[&#x2026;]</klr> konsili&gcirc;is, kiel ili povos <tld/>i lin per
        interparolado
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 22:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi intencis <tld/>i <klr>[la novelon]</klr> en interplekton da intrigoj, en
        kronon da epizodoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[vi]</klr> <tld/>as lin en bati&gcirc;ojn kaj en fripona&jcirc;ojn, &gcirc;is li
        <klr>[&#x2026;]</klr> pri sia bona nomo fari&gcirc;as tute bankrota
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne dubis, ke f-ino <nom>Martin</nom>, kiu videble malamas min <klr>[&#x2026;]</klr> min <tld/>os
        <fnt>
          <bib>CKv</bib>
          <lok>9</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni min a&ccirc;etas kaj <tld/>as en plej dubindan aferon kiu al mi kare kostos
        <fnt>
          <bib>MkM</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la asertoj <klr>[&#x2026;]</klr> <tld/>us proceson
        <fnt>
          <bib>Monato</bib>
          <aut>IRL</aut>
          <vrk><url ref="&Steloj;monato2/007096.html">Ekskapo eskapas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is sin en kontra&ubreve;diroj
        <fnt>
          <bib>Kiso</bib>
          <lok>La Punkto De Arkimedo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bar.0i">bari</ref>,
      <ref cel="brid.0i">bridi</ref>,
      <ref cel="embara.0i">embarasi</ref>,
      <ref cel="gxen.0i">&gcirc;eni</ref>,
      <ref cel="kompli.0i">kompliki</ref>,
      <ref cel="konfuz.0i">konfuzi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>verwickeln</trd>,
      <trd>verwirren</trd>,
      <trd>durcheinander bringen</trd>,
      <trd>verkomplizieren</trd>
    </trdgrp>
    <trd lng="es">complicar</trd> 
    <trdgrp lng="hu">
      <trd>&ouml;sszekusz&aacute;l</trd>,
      <trd>&ouml;sszezavar</trd>,
      <trd>t&uacute;lkomplik&aacute;l</trd>
    </trdgrp>
    <trdgrp lng="pt">
     <trd>implicar</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_l;&c_y;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_k;&c_l;&c_a;&c_d;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>zapl&eacute;st</trd>,
    <trd>znesnadnit</trd>,
    <trd>zt&iacute;&zcaron;it</trd>
  </trdgrp>
  <trd lng="en">entangle</trd>
  <trdgrp lng="fr">
    <trd>embrouiller</trd>,
    <trd>emm&ecirc;ler</trd>,
    <trd>enchev&ecirc;trer</trd>
    <trd>entraver</trd>
  </trdgrp>
  <trd lng="nl">verwarren</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_u;&c_t;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>pomota&tcaron;</trd>,
    <trd>zaplies&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x43F;&#x43B;&#x443;&#x442;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x443;&#x441;&#x43A;&#x43B;&#x430;&#x434;&#x43D;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x443;&#x442;&#x440;&#x443;&#x434;&#x43D;&#x44F;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x443;&#x442;&#x430;&#x442;&#x438; (&#x43A;&#x43E;&#x43D;&#x44F;)</trd>,
    <trd>&#x441;&#x43F;&#x440;&#x438;&#x447;&#x438;&#x43D;&#x44F;&#x442;&#x438; &#x431;&#x435;&#x437;&#x43B;&#x430;&#x434;</trd>
  </trdgrp>
</drv>

<drv mrk="implik.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <dif>
    Embarasi&gcirc;i pro kaptiloj, pro konfuzo, pro malhelpa&jcirc;oj.
  </dif>
  <snc ref="implik.0i.movojn">
    <ekz>
      vir&scirc;afo <tld/>i&gcirc;is per la kornoj en la
      arbeta&jcirc;oj <fnt><bib>MT</bib>, <lok>&Gen; 22:13</lok></fnt>;
    </ekz>
    <ekz>
      birdoj <tld/>i&gcirc;intaj en kaptilon
      <fnt><bib>MT</bib>, <lok>&Pre; 9:12</lok></fnt>;
    </ekz>
    <ekz>
      &gcirc;ia petolo faligis teren la volva&jcirc;on kaj la kato mem <tld/>i&gcirc;is en la fadenoj
      <fnt>
        <bib>IK</bib>
        <lok>&Ccirc;aptiro V</lok>
      </fnt>;
    </ekz>
    <ekz>
      lianoj, &ccirc;irka&ubreve;preninte unu la alian, <tld/>i&gcirc;is en la propraj tigoj
      <fnt>
        <bib>Metrop</bib>
      </fnt>.
    </ekz>
  </snc>
  <snc ref="implik.0i.aferojn">
    <ekz>
      malvirtuloj <tld/>i&gcirc;as en <klr>(ne povas eliri el)</klr> la
      malfeli&ccirc;o
      <fnt><bib>MT</bib>, <lok>&Sen; 24:16</lok></fnt>;
    </ekz>
    <ekz>
      staru fortike, kaj ne re<tld/>i&gcirc;u en jugon de sklaveco
      <fnt>
        <bib>NT</bib>
        <lok>Galatoj 5:1</lok>
      </fnt>;
    </ekz>
    <ekz>
      sed tie &ccirc;i li <tld/>i&gcirc;is, konfuzi&gcirc;is kaj estis
      devigata iom silenti
      <fnt>
        <bib>PatrojFiloj</bib>
        <lok>XXVI</lok>
      </fnt>;
    </ekz>
    <ekz>
      <nom>Ramzes</nom> volis konjekti la direkton, sed tuj liaj pensoj
      <tld/>i&gcirc;is
      <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>sich <ind>verheddern</ind></trd>,
    <trd>sich <ind>verwickeln</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>embrouiller</ind></trd>,
    <trd>s'<ind>emm&ecirc;ler</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x43F;&#x43B;&#x443;&#x442;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x432;&#x43F;&#x43B;&#x443;&#x442;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="implik.0o">
  <kap><tld/>o</kap>
  <dif>
    Ago <tld/>i a&ubreve; ties rezulto.
  </dif>
  <snc ref="implik.0i.movojn">
    <ekz>
      volva&jcirc;o de lanfadeno <klr>[&#x2026;]</klr> plena de nodoj kaj <tld/>oj, kaj en la mezo
      la katido
      <fnt>
        <bib>LOdE</bib>
        <lok>2001, &#x2116;7</lok>
      </fnt>.
    </ekz>
  </snc>
  <snc ref="implik.0i.aferojn">
    <ekz>
      &ccirc;u li mem amas &scirc;in? <klr>[&#x2026;]</klr>, kia <tld/>o! malbela afero!
      <fnt>
        <bib>PatrojFiloj</bib>
        <lok>XXIV</lok>
      </fnt>;
    </ekz>
    <ekz>
      suba&ccirc;eto de altranga prokurora amiko <klr>[&#x2026;]</klr> kaj <tld/>o de la
      bu&gcirc;eto-ministro
      <fnt>
        <bib>Monato</bib>
        <aut>Yves Nicolas</aut>
        <vrk><url ref="&Monato;/2010/010225.php">Komuna agado, komunaj valoroj</url></vrk>
        <lok>2010</lok>
      </fnt>;
    </ekz>
    <ekz>
      sentigi al li la <tld/>ojn de tia situacio
      <fnt>
        <bib>LOdE</bib>
        <lok>1999, No 10 (60)</lok>
      </fnt>.
    </ekz>
  </snc>
  <refgrp tip="vid">
    <ref cel="intrig.0o">intrigo</ref>,
    <ref cel="konfuz.0o.malordo">konfuzo<sncref/></ref>,
    <ref cel="nod.0o.implikajxo">nodo<sncref/></ref>
  </refgrp>
  <trd lng="be">&c_z;&c_a;&c_b;&c_l;&c_y;&c_t;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="es">
    <trd>complicaci&oacute;n</trd>,
    <trd>enredo</trd>
  </trdgrp> 
  <trdgrp lng="fr">
    <trd>complication</trd>,
    <trd>embrouillement</trd>,
    <trd>enchev&ecirc;trement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kuszas&aacute;g</trd>,
    <trd>&ouml;sszevisszas&aacute;g</trd>,
    <trd>t&uacute;lbonyol&iacute;t&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">ingewikkeldheid</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_u;&c_t;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">z&aacute;pletka</trd>
  <trdgrp lng="cs">
    <trd>zapleten&iacute;</trd>,
    <trd>z&aacute;pletka</trd>
  </trdgrp>
<trdgrp lng="de">
  <trd>Verwicklung</trd>,
  <trd>Implikation</trd>
</trdgrp>
  <trd lng="uk">&#x437;&#x430;&#x43F;&#x43B;&#x443;&#x442;&#x430;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>
</drv>

<drv mrk="implik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="implik.0ajxo.KOMUNE">
    <dif>
      Io <tld/>ita:
      <ekz>
        <tld/>a&jcirc;o <klr>[&#x2026;]</klr> preska&ubreve; senelira
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        rektaj kaj harmoniaj linioj, liberaj de &ccirc;iaj <tld/>a&jcirc;oj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXIX</lok>
        </fnt>.
      </ekz>
    </dif>
<ref tip="vid" cel="labiri.0o">labirinto</ref>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Verwirrung</trd>,
    <trd>Gewirr</trd>,
    <trd>Verwicklung</trd>,
    <trd>Fitz</trd>,
    <trd>Durcheinander</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>complicaci&oacute;n</trd>,
    <trd>enredo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>complication</trd>,
    <trd>embrouillamini</trd>,
    <trd>enchev&ecirc;trement</trd>,
    <trd>tracasserie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>z&udblac;r</trd>,
    <trd>bonyodalom</trd>,
    <trd>gubanc</trd>,
    <trd>kuszas&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">verwikkeling</trd>
  <trd lng="ru">&c_p;&c_u;&c_t;&c_a;&c_n;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x43F;&#x43B;&#x443;&#x442;&#x430;&#x43D;&#x430; &#x441;&#x43F;&#x440;&#x430;&#x432;&#x430;</trd>,
    <trd>&#x43F;&#x43B;&#x443;&#x442;&#x430;&#x43D;&#x438;&#x43D;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="implik.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="implik.mal0i.KOMUNE">
    <dif>
      Ordigi, klarigi konfuza&jcirc;ojn:
      <ekz>
        rompopecoj tinte &scirc;uti&gcirc;is malsupren, <klr>[&#x2026;]</klr> &leftquot;mi devos esti ankora&ubreve; pli
        atenta&rightquot; &scirc;i pensis, &leftquot;&rightquot;tie pendas tiom da ajna&jcirc;oj, ke oni apena&ubreve; povos sin
        mal<tld/>i&rightquot;
        <fnt>
          <bib>MkM</bib>
          <lok>21</lok>
        </fnt>;
      </ekz>
      <ekz>
        furiozante kontra&ubreve; si mem, Levio sin mal<tld/>is el la amaso kaj kuris reen al
        la urbo
        <fnt>
          <bib>MkM</bib>
          <lok>16</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kapablus elpensi sistemojn tiel makiavelajn ke neniu ilin iam ajn mal<tld/>os
        <fnt>
          <bib>ChB</bib>
          <lok>26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_l;&c_y;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_l;&c_u;&c_m;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_l;&c_u;&c_m;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">rozmotat</trd>
  <trdgrp lng="de">
    <trd>entwirren</trd>,
    <trd>entfitzen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desenredar</trd>,
    <trd>simplificar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;brouiller</trd>,
    <trd>d&eacute;m&ecirc;ler<klr tip="ind"> (fig., une
affaire)</klr></trd>,
    <trd>d&eacute;senchev&ecirc;trer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kibogoz</trd>,
    <trd>tiszt&aacute;z<klr tip="ind"> (bonyodalmat)</klr></trd>
  </trdgrp>
  <trd lng="nl">ontwarren</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_malmol;&c_ja;&c_s;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozmota&tcaron;</trd>,
    <trd>rozplies&tcaron;</trd>,
    <trd>rozvin&uacute;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x437;&#x43F;&#x43B;&#x443;&#x442;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x43F;&#x443;&#x442;&#x443;&#x432;&#x430;&#x442;&#x438; (&#x43A;&#x43E;&#x43D;&#x44F;)</trd>
  </trdgrp>
</drv>
</art>
