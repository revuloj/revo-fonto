<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>hungar</rad>/o</kap>
<drv mrk="hungar.0o">
   <kap><tld/>o</kap>
   <snc mrk="hungar.0o.sxtatano">
     <dif>
       <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
       <ref tip="malprt" cel="hungar.H0ujo"><tld lit="H"/>ujo</ref>.
     </dif>
     <trd lng="hu">magyar<klr tip="amb">
     (&aacute;llampolg&aacute;r)</klr></trd>
     <trdgrp lng="pl">
       <trd>W&eogonek;gier</trd>,
       <trd>Madziar<klr> (pot., &zdot;art.)</klr></trd>
     </trdgrp>
     <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_i;&c_n;
     <ind>&c_V;&c_je;&c_n;&c_g;&c_r;&c_i;&c_i;</ind></trd>
   </snc>
   <snc mrk="hungar.0o.naciano">
     <dif>
       Homo kun <tld/>a nacieco:
       <ekz>
         sufi&ccirc;as mencii la situacion de <tld/>oj en Rumanio,
         poloj en &Ccirc;e&hcirc;io, ukrainoj en Pollando ktp.
         <fnt>
         <aut>Sylvia BRYLSKA</aut>:
         <vrk>&Ccirc;u reiri al naciaj radikoj?</vrk>,
         <bib>Monato</bib>, <lok>n-ro 08/1993</lok>
         </fnt>
       </ekz>
     </dif>
     <trdgrp lng="ca">
       <trd>hongar&egrave;s</trd>,
       <trd>magiar</trd>
     </trdgrp>
     <trd lng="de">Ungar</trd>
     <trd lng="fr">Hongrois</trd>
     <trd lng="es">h&uacute;ngaro</trd>
     <trd lng="hu">magyar<klr tip="amb">
     (nemzetis&eacute;g&udblac;)</klr></trd>
     <trd lng="nl">Hongaar</trd>
     <trdgrp lng="pl">
       <trd>W&eogonek;gier</trd>,
       <trd>Madziar<klr> (pot., &zdot;art.)</klr></trd>
     </trdgrp>
     <trd lng="pt">h&uacute;ngaro<klr tip="ind"> (subst.)</klr></trd>
     <trd lng="ru">&c_v;&c_je;&c_n;&c_g;&c_r;</trd>
     <trd lng="sk">Ma&dcaron;ar</trd>
   </snc>                     
   <trd lng="be">&c_v;&c_u;&c_g;&c_o;&c_r;&c_a;&c_c;</trd>
   <trd lng="br">Hungariad</trd>
   <trd lng="en">Hungarian</trd>
   <trd lng="fa">&a_m;&a_j;&a_A;&a_r;</trd>
</drv>

<drv mrk="hungar.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Rilantanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
       <ekz>
         <ind><tld/>a lingvo</ind>, a&ubreve;toro, konstitucio;
         <trd lng="br">hungareg</trd>
       </ekz>
       <ekz>
         la historio rakontis pri <tld lit="H"/>a kavaliro,
         kiu estis kaptita de Turka guberniestro,
         kiu lasis aljungi lin kune kun la bovoj al la plugilo,
         peladi per vipado kaj terure mokadi &gcirc;is li tute
         senforti&gcirc;is
         <fnt><bib>Fab3</bib>, <lok>Historio de unu patrino</lok></fnt>;
       </ekz>
       <ekz>
         oni devis kanti plejparte <tld/>e, kio ne baris
         eksterlandanojn:
         japana virina koruso parkere kantis <tld/>ajn melodiojn
         <fnt>
         <aut>Juh&aacute;sz K&aacute;roly</aut>:
         <vrk>Korusa konkurso</vrk>,
         <bib>Monato</bib>, <lok>n-ro 12/2000, p. 14</lok>
         </fnt>.
       </ekz>
     </dif>
   </snc>                        
   <trd lng="be">&c_v;&c_u;&c_g;&c_o;&c_r;&c_s;&c_k;&c_ib;</trd>
   <trd lng="br">hungariat</trd>
   <trdgrp lng="ca">
     <trd>hongar&egrave;s</trd>,
     <trd>magiar</trd>
   </trdgrp>
   <trd lng="de">ungarisch</trd>
   <trd lng="en">Hungarian</trd>
   <trd lng="es">h&uacute;ngaro</trd>
   <trd lng="hu">magyar</trd>
   <trdgrp lng="fa">
     <trd>&a_m;&a_j;&a_A;&a_r;&a_s;&a_t;&a_A;&a_n;&f_y;</trd>,
     <trd>&a_m;&a_j;&a_A;&a_r;&f_y;</trd>,
     <trd>&a_m;&a_j;&a_A;&a_r;</trd>
   </trdgrp>
   <trd lng="fr">hongrois</trd>
   <trd lng="nl">Hongaars</trd>
   <trd lng="pl">w&eogonek;gierski</trd>
   <trd lng="pt">h&uacute;ngaro<klr tip="ind"> (adj.)</klr></trd>
   <trd
   lng="ru">&c_v;&c_je;&c_n;&c_g;&c_je;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
   <trd lng="sk">ma&dcaron;arsk&yacute;</trd>
</drv>

<drv mrk="hungar.H0ujo">
   <kap><tld lit="H"/>ujo</kap>
   <uzo tip="fak">GEOG</uzo>
   <fnt><bib>ZR</bib></fnt>
   <snc>
     <dif>
       <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta
       en meza <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
       kaj limanta oriente al Ukrainujo kaj Rumanujo, okcidente al
       A&ubreve;strujo, norde
       al Slovakujo kaj sude al Slovenujo, Kroatujo kaj Jugoslavio.
     </dif>
   </snc>
   <ref tip="sin" cel="hungari.H0o">Hungario</ref>
   <trd
   lng="be">&c_V;&c_u;&c_g;&c_o;&c_r;&c_sh;&c_ch;&c_y;&c_n;&c_a;</trd>  
                    
   <trd lng="br">Hungaria</trd>
   <trd lng="ca">Hongria</trd>
   <trd lng="de">Ungarn</trd>
   <trd lng="en">Hungary</trd>
   <trd lng="es">Hungr&iacute;a</trd>  
   <trd lng="fa">&a_m;&a_j;&a_A;&a_r;&a_s;&a_t;&a_A;&a_n;</trd>
   <trd lng="fr">Hongrie</trd>   
   <trd lng="hu">Magyarorsz&aacute;g</trd>
   <trd lng="nl">Hongarije</trd>
   <trd lng="pl">W&eogonek;gry</trd>
   <trd lng="pt">Hungria</trd>
   <trd lng="ru">&c_V;&c_je;&c_n;&c_g;&c_r;&c_i;&c_ja;</trd>
   <trd lng="sk">Ma&dcaron;arsko</trd>
</drv>
</art>
