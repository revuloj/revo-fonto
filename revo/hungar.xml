<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>8</ofc><rad>hungar</rad>/o</kap>

<drv mrk="hungar.0o">
   <kap><tld/>o <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>,
  <var><kap><tld/>ino</kap></var></kap>
   <snc mrk="hungar.0o.sxtatano">
     <dif>
       <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref>, &scirc;tatanino de
       <ref tip="malprt" cel="hungar.H0ujo"><tld lit="H"/>ujo</ref>.
     </dif>
     <trd lng="hu">magyar<klr tip="amb">
     (&aacute;llampolg&aacute;r)</klr></trd>
     <trdgrp lng="pl">
       <trd>W&eogonek;gier</trd>,
       <trd>Madziar<klr> (pot., &zdot;art.)</klr></trd>
     </trdgrp>
     <trd lng="ru">&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_i;&c_n;
     <ind>&c_V;&c_je;&c_n;&c_g;&c_r;&c_i;&c_i;</ind></trd>
   </snc>
   <snc mrk="hungar.0o.naciano">
     <dif>
       Homo kun <tld/>a <ref tip="lst" cel="naci.0eco"
       lst="voko:popolanoj">nacieco</ref>:
       <ekz>
         sufi&ccirc;as mencii la situacion de <tld/>oj en Rumanio,
         poloj en &Ccirc;e&hcirc;io, ukrainoj en Pollando ktp.
         <fnt>
         <aut>Sylvia BRYLSKA</aut>:
         <vrk>&Ccirc;u reiri al naciaj radikoj?</vrk>,
         <bib>Monato</bib>, <lok>n-ro 08/1993</lok>
         </fnt>
       </ekz>
     </dif>
     <trdgrp lng="ca">
       <trd>hongar&egrave;s</trd>,
       <trd>magiar</trd>
     </trdgrp>
     <trd lng="de">Ungar</trd>
     <trd lng="es">h&uacute;ngaro</trd>
     <trd lng="hu">magyar<klr tip="amb">
     (nemzetis&eacute;g&udblac;)</klr></trd>
     <trd lng="it">ungherese<klr> (sost.)</klr></trd>
     <trd lng="nl">Hongaar</trd>
     <trdgrp lng="pl">
       <trd>W&eogonek;gier</trd>,
       <trd>Madziar<klr> (pot., &zdot;art.)</klr></trd>
     </trdgrp>
     <trd lng="pt">h&uacute;ngaro<klr tip="ind"> (subst.)</klr></trd>
     <trd lng="ru">&c_v;&c_je;&c_n;&c_g;&c_r;</trd>
     <trd lng="sk">Ma&dcaron;ar</trd>
   </snc>
  <trd lng="be">&c_v;&c_u;&c_g;&c_o;&c_r;&c_a;&c_c;</trd>
  <trd lng="bg">&c_u;&c_n;&c_g;&c_a;&c_r;&c_je;&c_c;</trd>
  <trd lng="br">Hungariad</trd>
  <trd lng="cs">Ma&dcaron;ar</trd>
  <trd lng="en">Hungarian</trd>
  <trd lng="fa">&a_m;&a_j;&a_A;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>Hongrois</trd>,
    <trd>Hongroise<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="sk">Ma&dcaron;ar</trd>
</drv>

<drv mrk="hungar.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
       <ekz>
         <ind><tld/>a lingvo</ind>, a&ubreve;toro, konstitucio;
         <trd lng="br">hungareg</trd>
       </ekz>
       <ekz>
         la historio rakontis pri <tld lit="H"/>a kavaliro,
         kiu estis kaptita de Turka guberniestro,
         kiu lasis aljungi lin kune kun la bovoj al la plugilo,
         peladi per vipado kaj terure mokadi &gcirc;is li tute
         senforti&gcirc;is
         <fnt><bib>Fab3</bib>, <lok>Historio de unu patrino</lok></fnt>;
       </ekz>
       <ekz>
         oni devis kanti plejparte <tld/>e, kio ne baris
         eksterlandanojn:
         japana virina koruso parkere kantis <tld/>ajn melodiojn
         <fnt>
         <aut>Juh&aacute;sz K&aacute;roly</aut>:
         <vrk>Korusa konkurso</vrk>,
         <bib>Monato</bib>, <lok>n-ro 12/2000, p. 14</lok>
         </fnt>.
       </ekz>
     </dif>
    <trd lng="en">Hungarian<klr> (adj.)</klr></trd>
    <trdgrp lng="fr">
     <trd>hongrois</trd>,
     <trd>hongroise<klr> (adj.)</klr></trd>
    </trdgrp>
   </snc>
   <snc>
     <dif>
     "la hungara" = la nacia lingvo de Hungarujo
     </dif>
    <trd lng="en">Hungarian<klr> (language)</klr></trd>
    <trd lng="fr">hongrois<klr> (langue)</klr></trd>
   </snc>
  <trd lng="be">&c_v;&c_u;&c_g;&c_o;&c_r;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_u;&c_n;&c_g;&c_a;&c_r;&c_s;&c_k;&c_i;</trd>
  <trd lng="br">hungariat</trd>
  <trdgrp lng="ca">
     <trd>hongar&egrave;s</trd>,
     <trd>magiar</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>ma&dcaron;arsk&yacute;</trd>,
    <trd>uhersk&yacute;</trd>
  </trdgrp>
  <trd lng="de">ungarisch</trd>
  <trd lng="en">Hungarian</trd>
  <trd lng="es">h&uacute;ngaro</trd>
  <trdgrp lng="fa">
     <trd>&a_m;&a_j;&a_A;&a_r;&a_s;&a_t;&a_A;&a_n;&f_y;</trd>,
     <trd>&a_m;&a_j;&a_A;&a_r;&f_y;</trd>,
     <trd>&a_m;&a_j;&a_A;&a_r;</trd>
   </trdgrp>
  <trd lng="hu">magyar</trd>
  <trdgrp lng="it">
    <trd>ungherese<klr> (agg.)</klr></trd>, 
    <trd>magiaro<klr> (agg.)</klr></trd>
   </trdgrp>
  <trd lng="nl">Hongaars</trd>
  <trd lng="pl">w&eogonek;gierski</trd>
  <trd lng="pt">h&uacute;ngaro<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_v;&c_je;&c_n;&c_g;&c_je;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>ma&dcaron;arsk&yacute;</trd>,
    <trd>ma&dcaron;arsk&yacute;</trd>,
    <trd>uhorsk&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="hungar.H0ujo">
   <kap><tld lit="H"/>ujo
   (<var><kap><tld lit="H"/>/i/o</kap></var>)
   <fnt><bib>ZR</bib></fnt>
   <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
    ref="http://eo.wikipedia.org/wiki/Hungario">Hungario</url></lok></fnt>
   </kap>
   <snc>
   <uzo tip="fak">GEOG</uzo>
   <uzo tip="fak">POL</uzo>
     <dif>
       <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref>
       situanta
       en meza <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
       kaj limanta oriente al Ukrainujo kaj Rumanujo, okcidente al
       A&ubreve;strujo, norde
       al Slovakujo kaj sude al Slovenujo, Kroatujo kaj Jugoslavio.
     </dif>
    <ref tip="sin" cel="hungari.H0o">Hungario</ref>
   </snc>
   <rim mrk="hungar.H0io.RIM">
  "Gentobazaj landnomoj. Oni ordinare formas la nomon de la lando per la
  <em>Fundamenta sufikso UJ</em> a&ubreve; per la <em>neoficiala sufikso
  I</em>. Pri la karaktero de tiu I sufikso (uzata post popolvortoj
  anstata&ubreve; UJ) ekzistas diversaj opinioj. Iuj rigardas &gcirc;in kiel
  <em>efektivan sufikson</em> (kvankam neoficialan), dum aliaj preferas
  rigardi &gcirc;in kiel <em>eron de aparta landnoma radiko</em> paralela kun
  la responda popolnoma radiko. 
  Tiuj diversaj analizoj &ccirc;iuokaze ne faras praktikan
  diferencon en la uzado de tiaj landnomoj kiel Francio, Rusio, Japanio
  kaj &Ccirc;inio. <em>En tiu &ccirc;i dokumento la Akademio ne alprenas starpunkton
  pri tiu teoria demando</em>" <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>.
  </rim>
   <trd lng="be">&c_V;&c_u;&c_g;&c_o;&c_r;&c_sh;&c_ch;&c_y;&c_n;&c_a;</trd>  
   <trd lng="bg">&c_U;&c_n;&c_g;&c_a;&c_r;&c_i;&c_ja;</trd>
   <trd lng="br">Hungaria</trd>
   <trd lng="ca">Hongria</trd>
   <trd lng="de">Ungarn</trd>
   <trd lng="en">Hungary</trd>
   <trd lng="es">Hungr&iacute;a</trd>  
   <trd lng="fa">&a_m;&a_j;&a_A;&a_r;&a_s;&a_t;&a_A;&a_n;</trd>
   <trd lng="fr">Hongrie</trd>   
   <trd lng="hu">Magyarorsz&aacute;g</trd>
   <trd lng="id">Hongaria</trd>
   <trd lng="it">Ungheria</trd>
   <trd lng="nl">Hongarije</trd>
   <trd lng="pl">W&eogonek;gry</trd>
   <trd lng="pt">Hungria</trd>
   <trd lng="ru">&c_V;&c_je;&c_n;&c_g;&c_r;&c_i;&c_ja;</trd>
   <trd lng="sk">Ma&dcaron;arsko</trd>
</drv>
</art>
