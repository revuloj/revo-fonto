<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc> <rad>monoton</rad>/a <fnt>Z</fnt>
</kap>
<drv mrk="monoto.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="ton.unu0a">unutona</ref>
    <ekz>
      tio renversis la trankvilan kaj <tld/>an vivon de
      Kuniberto<fnt><aut>Irina Gon&ccirc;arova</aut>: <vrk>Vivi plu: kial?
      (Gudrun Pausewang: La avo en la &ccirc;areto)</vrk>,
      <bib>Monato</bib><lok>jaro 2002a, numero 6a, p. 20a</lok></fnt>.
    </ekz>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_n;&c_a;&c_t;&c_o;&c_n;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_d;&c_n;&c_a;&c_t;&c_o;&c_n;&c_n;&c_y;</trd>
    </trdgrp>
  <trdgrp lng="fr">
    <trd>monotone</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>egyhang&uacute;</trd>,
    <trd>monoton</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>eentonig</trd>,
    <trd>monotoon</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_n;&c_o;&c_t;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_d;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_d;&c_n;&c_o;&c_z;&c_v;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  </snc>
  <snc mrk="monoto.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="bild.0igo.MAT">bildigo</ref> <frm><k>f</k></frm>
      inter du <ref cel="ar.orda0o">ordaj aroj</ref>)</klr>
      <ref cel="kresk.0anta.bildigo">Kreskanta</ref> a&ubreve;
      <ref cel="kresk.mal0anta.bildigo">malkreskanta</ref>:
      <ekz>
        la funkcio <frm><k>x</k><sup>2</sup></frm> ne estas <tld/>a, sed
        <frm><k>x</k><sup>3</sup></frm> ja estas;
      </ekz>
      <ekz>
        <ind>popece <tld/>a</ind> reela funkcio <klr>(<tld/>a sur &ccirc;iu 
        elemento de dispartigo de &gcirc;ia fonto-aro en intervalojn)</klr>;
        <trd lng="de">st&uuml;ckweise <ind>monoton</ind><klr tip="ind"> (-e Funktion)</klr></trd>
        <trd lng="en">piecewise <ind>monotonic</ind><klr tip="ind"> (function)</klr></trd>
        <trd lng="fr"><klr tip="ind">(fonction) </klr>monotone par morceaux</trd>
        <trd lng="hu">szakaszonk&eacute;nt <ind>monoton</ind><klr tip="ind"> (f&uuml;ggv&eacute;ny)</klr></trd>        
        <trd lng="pl"><klr tip="amb">(funkcja) </klr>przedzia&lstroke;ami <ind>monotoniczna</ind></trd>
        <trd lng="ru">&c_k;&c_u;&c_s;&c_o;&c_ch;&c_n;&c_o;-&c_m;&c_o;&c_n;&c_o;&c_t;&c_o;&c_n;&c_n;&c_a;&c_ja;<klr tip="amb"> (&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;)</klr></trd>
      </ekz>
      <ekz>
        <ind>strikte <tld/>a</ind> bildigo <klr>(strikte kreskanta a&ubreve; strikte
        malkreskanta)</klr>.
        <trd lng="de">streng <ind>monoton</ind><klr tip="ind"> (-e Abbildung)</klr></trd>
        <trd lng="en">strictly <ind>monotonic</ind><klr tip="ind"> (mapping)</klr></trd>
        <trd lng="fr"><klr tip="ind">(application) </klr>strictement monotone</trd>
        <trd lng="hu">szigor&uacute;an <ind>monoton</ind><klr tip="ind"> (lek&eacute;pez&eacute;s)</klr></trd>        
        <trd lng="pl"><klr tip="amb">(funkcja) </klr>silnie <ind>monotoniczna</ind></trd>
        <trd lng="ru">&c_s;&c_t;&c_r;&c_o;&c_g;&c_o;-&c_m;&c_o;&c_n;&c_o;&c_t;&c_o;&c_n;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_n;&c_a;&c_t;&c_o;&c_n;&c_n;&c_y;</trd>
    <trd lng="de">monoton<klr tip="ind"> (-e Abbildung)</klr></trd>
    <trdgrp lng="en">
      <trd>monotonic<klr tip="ind"> (mapping)</klr></trd>,
      <trd>monoton<klr tip="ind"> (mapping)</klr></trd>
    </trdgrp>
    <trd lng="fr"><klr tip="ind">(application) </klr>monotone</trd>
    <trd lng="hu">monoton<klr tip="amb"> (lek&eacute;pez&eacute;s)</klr></trd>    
    <trd lng="pl"><klr tip="amb">(funkcja) </klr>monotoniczna</trd>
    <trd lng="ru">&c_m;&c_o;&c_n;&c_o;&c_t;&c_o;&c_n;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </snc>
</drv>
</art>
