    <trd>pika</trd>,
    <trd>dzsida</trd>
  </trdgrp>
  <trd lng="it">picca<klr> (arma)</klr></trd>
  <trd lng="ja">&#x3084;&#x308A;</trd>
  <trd lng="nl">piek</trd>
  <trdgrp lng="pl">
    <trd>pika</trd>,
