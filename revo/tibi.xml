<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tibi.xml,v 1.30 2024/09/24 10:04:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tibi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tibi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tibi.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Longa dika <ref tip="super" cel="ost.0o.ANA">osto</ref> inter
      genuo kaj maleolo:
      <ekz>
        mi portis <klr>[&#8230;]</klr> partojn da felo &ccirc;irka&ubreve; miaj <tld/>oj, sed ne &scirc;uojn
        <fnt>
          <bib>Rob</bib>
        </fnt>;
      </ekz>
      <ekz>
        li faris ankora&ubreve; unu pa&scirc;on kaj <klr>[&#8230;]</klr> premis la risorton <klr>[&#8230;]</klr>,
        ektremis kaj kaptis sian <tld/>on per la mano, sango fluetis sur lia blanka
        pantalono
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi levi&gcirc;as pene kun vundbatitaj <tld/>oj kaj sangantaj genuoj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili &ccirc;asis multajn antilopojn, el ties <tld/>oj ili faris tran&ccirc;ilojn
        <fnt>
          <bib>SatirRak</bib>
        </fnt>;
      </ekz>
      <ekz>
        la akrido <klr>[&#8230;]</klr> ludis per la flugilo sur sia <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Lasta son&gcirc;o de maljuna kverko</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_g;&c_a;&c_l;&c_jo;&c_n;&c_k;&c_a;&c_v;&c_a;&c_ja; <ind>&c_k;&c_o;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>
  <trd lng="ca">t&iacute;bia</trd>
  <trdgrp lng="cs">
    <trd>holenn&iacute; kost</trd>,
    <trd>hole&ncaron;</trd>,
    <trd>tibie</trd>
  </trdgrp>
  <trd lng="de">Schienbein</trd>
  <trd lng="en">tibia</trd>
  <trd lng="es">tibia</trd>
  <trd lng="fa"><klr>(&a_A;&a_s;&a_t;&a_kh;&a_w;&a_A;&a_n;) </klr>&a_d;&a_r;&a_sh;&a_t;&#x200C;&a_n;&f_y;</trd>
  <trd lng="fr">tibia<klr> (os)</klr></trd>
  <trd lng="hu">s&iacute;pcsont</trd>
  <trd lng="it">tibia</trd>
  <trd lng="ja">&#x811B;&#x9AA8;  <pr>&#12369;&#12356;&#12371;&#12388;</pr></trd>
  <trd lng="la">tibia</trd>
  <trd lng="nl">scheenbeen</trd>
  <trd lng="pl">ko&sacute;&cacute; goleniowa</trd>
  <trdgrp lng="pt">
    <trd>t&iacute;bia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_a;&c_ja; &c_b;&c_je;&c_r;&c_c;&c_o;&c_v;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>holenn&aacute; kos&tcaron;</trd>,
    <trd>hole&ncaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&#x43B;&c_i;&#x43A;&#x43E;&c_g;&#x43E;&#x43C;&c_ib;&#x43B;&#x43A;&#x43E;&c_v;&c_a; &#x43A;&c_ib;&c_s;&c_t;&#x43A;&c_a;</trd>,
    <trd>&c_g;&#x43E;&#x43C;&c_ib;&#x43B;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x80EB;&#x9AA8; <pr>j&#xEC;ngg&#x1D4;</pr></trd>,
    <trd>&#x811B;&#x9AA8; <pr>j&#xEC;ngg&#x1D4;</pr></trd>,
    <trd>&#x9AAD; <pr>g&#xE0;n</pr></trd>,
    <trd>&#x80EB;&#x90E8; <pr>j&#xEC;ngb&#xF9;</pr></trd>,
    <trd>&#x811B;&#x90E8; <pr>j&#xEC;ngb&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tibi.0ingo">
  <kap>
    <tld/>ingo
  </kap>
  <snc>
    <uzo tip="fak">TEKS</uzo><uzo tip="fak">MIL</uzo>
    <dif>
      Parto de vesto, kiu protektas la kruron inter la genuo kaj maleolo:
      <ekz>
        kiel profunda estas la strata defluilo? <klr>[&#8230;]</klr> &gcirc;is la mezo de la
        <tld/>ingoj <klr>(de la botoj)</klr>
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj <klr>[portis]</klr> botojn kun <tld/>ingoj, &ccirc;iuj estis armitaj per glavoj, arkoj kaj
        lancoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        bronzaj <tld/>ingoj protektis liajn krurojn kaj li portis &jcirc;etlancon sur&scirc;ultre
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=247965">Goljato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_l;&c_ja;&c_v;&c_a; <klr tip="amb">(&c_b;&c_o;&c_t;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>Schaft <klr>(Stiefel)</klr></trd>,
    <trd>Beinschiene</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bootleg</trd>,
    <trd>greave</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cn&eacute;mide</trd>,
    <trd>jambe de botte </trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x80F4;  <pr>&#12393;&#12358;</pr></trd>,
    <trd>&#12377;&#x306D;&#x5F53;&#12390;  <pr>&#12377;&#x306D;&#12354;&#12390;</pr></trd>
  </trdgrp>
  <trd lng="pl">cholewa</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_n;&c_o;&c_zh;&c_i;</trd>,
    <trd>&c_n;&c_o;&c_g;&c_a; &c_b;&c_o;&c_t;&c_i;&c_n;&c_k;&c_a; </trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x811B;&#30002;</trd>,
    <trd>&#38772;&#x817F;</trd>
  </trdgrp>
</drv>

<drv mrk="tibi.0karno">
  <kap><tld/>karno</kap>
  <snc mrk="tibi.0karno.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Muskola elstara&jcirc;o malanta&ubreve; la <tld/>o:
      <ekz>
        ulceroj sur la genuoj kaj sur la tibikarnoj
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 28:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        li observis sian korpon de la <tld/>karnmuskoloj supren &gcirc;is la bicepso
        <fnt>
          <bib>VRA</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="sur1.0o.ANA">suro</ref>
  </snc>
  <trd lng="be">&c_l;&c_y;&c_t;&c_k;&c_a;</trd>
  <trd lng="ca">panxell</trd>
  <trd lng="cs">l&yacute;tko</trd>
  <trdgrp lng="de">
    <trd>Wade</trd>,
    <trd>Sura</trd>
  </trdgrp>
  <trd lng="en">calf</trd>
  <trd lng="es">pantorrilla</trd>
  <trd lng="fr">mollet</trd>
  <trdgrp lng="hu">
    <trd>l&aacute;bikra</trd>,
    <trd>v&aacute;dli</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>gastrocnemio</trd>,
    <trd>gemelli<klr tip="amb"> (muscolo)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#12405;&#x304F;&#12425;&#x306F;&#x304E;</trd>
  <trd lng="nl">kuit<klr tip="ind"> (v.been)</klr></trd>
  <trd lng="pl">&lstroke;ydka</trd>
  <trd lng="ru">&c_i;&c_k;&c_r;&c_a; <klr tip="amb">(&c_n;&c_o;&c_g;&c_i;)</klr></trd>
  <trd lng="sk">l&yacute;tko <klr>(m&auml;so)</klr></trd>
  <trd lng="uk">&#x43B;&c_i;&c_t;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#33107; <pr>f&#xE9;i</pr></trd>,
    <trd>&#x817F;&#x809A;&#x5B50; <pr>tu&#x1D0;d&#xF9;zi</pr></trd>
  </trdgrp>
</drv>
</art>
