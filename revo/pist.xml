<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pist</rad>/i
</kap>
<drv mrk="pist.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Dispecetigi a&ubreve; pulvorigi per batado, froto kaj premo:
      <ekz>
        <tld/>i pipron, salon, gria&jcirc;on;
      </ekz>
      <ekz>
        se vi <tld/>os malsa&gcirc;ulon en <tld/>ujo, lia
        malsa&gcirc;eco de
        li ne aparti&gcirc;os<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dis<tld/>i <klr>(subpremi)</klr> la pastraron<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frakas.0i">frakasi</ref>,
      <ref cel="bat">disbati</ref>,
      <ref cel="muel.0i">mueli</ref>,
      <ref cel="macx.0i">ma&ccirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_w;&c_ch;&c_y;</trd>
  <trd lng="de">zersto&szlig;en</trd>
  <trd lng="en">pound</trd>
  <trd lng="hu">apr&oacute;ra <ind>t&ouml;r</ind></trd>
  <trdgrp lng="nl">
    <trd>stampen<klr tip="ind"> (fijnstampen)</klr></trd>,
    <trd>fijnstampen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pisar (no <klr tip="amb">gral</klr>)</trd>,
    <trd>triturar</trd>,
    <trd>socar</trd>    
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_l;&c_o;&c_ch;&c_mol;</trd>
</drv>

<drv mrk="pist.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="pist.0ilo.ilo">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Instrumento por <tld/>i, ordinare konsistanta el
      tenilo kun globforma malsupro.
    </dif>
    <trd lng="de">St&ouml;&szlig;el</trd>
    <trd lng="hu">mozs&aacute;rt&ouml;r&odblac;</trd>
  </snc>
  <snc mrk="pist.0ilo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="pistil.0o">Pistilo</ref>.
    </dif>
    <trd lng="hu">term&odblac;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_w;&c_k;&c_a;&c_ch;</trd>,
    <trd>&c_m;&c_ja;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="nl">stamper</trd>
  <trd lng="ru">&c_p;&c_je;&c_s;&c_t;&c_i;&c_k;</trd>
</drv>

<drv mrk="pist.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Speciala vazo, en kiu oni <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_u;&c_p;&c_a;</trd>
  <trd lng="de">M&ouml;rser</trd>
  <trd lng="hu">mozs&aacute;r</trd>
  <trdgrp lng="nl">
    <trd>vijzel</trd>,
    <trd>mortier</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_u;&c_p;&c_a;</trd>
</drv>
</art>
