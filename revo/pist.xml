<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>pist</rad>/i
</kap>
<drv mrk="pist.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Pecetigi a&ubreve; pulvorigi per ripetitaj batoj:
      <ekz>
        <tld/>i pipron, salon, gria&jcirc;on;
      </ekz>
      <ekz>
        se vi <tld/>os malsa&gcirc;ulon en <tld/>ujo, lia malsa&gcirc;eco de
        li ne aparti&gcirc;os<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo> dis<tld/>i <klr>(subpremi)</klr> la pastraron<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="frakas.0i">frakasi</ref>,
      <ref cel="bat">disbati</ref>,
      <ref cel="muel.0i">mueli</ref>,
      <ref cel="macx.0i">ma&ccirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Zersto&szlig;en</trd>
  <trd lng="en">pound</trd>
</drv>
<drv mrk="pist.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="pist.0ilo.ilo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Instrumento por <tld/>i, ordinare konsistanta el
      tenilo kun globforma malsupro.
    </dif>
    <trd lng="de">St&ouml;&szlig;el</trd>
  </snc>
</drv>
<drv mrk="pist.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <dif>
      Speciala vazo, en kiu oni <tld/>as.
    </dif>
  </snc>
  <trd lng="de">M&ouml;rser</trd>
</drv>
</art>
