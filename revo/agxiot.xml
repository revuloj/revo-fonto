<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: agxiot.xml,v 1.22 2024/10/29 23:28:20 revo Exp $">
<kap>
  <rad>a&gcirc;iot</rad>/i
  <fnt>Z</fnt> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="agxiot.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="spekul.0i">Spekuli</ref>
      pri la prezo&scirc;an&gcirc;o de la borsaj valora&jcirc;oj
      a&ubreve; de komerca&jcirc;oj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_l;&c_ja;&c_c;&c_mol; &c_n;&c_a; <ind>&c_b;&c_ib;&c_r;&c_zh;&c_y;</ind></trd>,
    <trd>&c_s;&c_p;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_b;&c_ib;&c_r;&c_zh;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>spekulieren</trd>,
    <trd>in <ind>Differenzgesch&auml;ften</ind> <ind>spekulieren</ind></trd>,
    <trd>Agiotage betreiben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stockjobbing</trd>,
    <trd>speculate in stocks</trd>
  </trdgrp>
  <trd lng="es">especular<klr tip="amb"> (en bolsa)</klr></trd>
  <trd lng="fr">agioter</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5DE;&#x5E8; &#x5E2;&#x5DC; &#x5E2;&#x5E8;&#x5DB;&#x5D9;
  &#x5E1;&#x5D7;&#x5D5;&#x5E8;&#x5D5;&#x5EA; &#x5D0;&#x5D5;
  &#x5DE;&#x5D8;&#x5D1;&#x5E2;&#x5D5;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>&uuml;z&eacute;rkedik</trd>,
    <trd>spekul&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="it">
     <trd>aggiotare</trd>, <trd>speculare<klr> (econ.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25237;&#x6A5F;&#x58F2;&#x8CB7;&#12377;&#x308B;  <pr>&#12392;&#12358;&#x304D;&#12400;&#12356;&#12400;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#21033;&#38808;&#12434;&#x304B;&#x305B;&#12368;  <pr>&#x308A;&#12374;&#12420;&#12434;&#x304B;&#x305B;&#12368;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>spekulowa&cacute;</trd>,
    <trd>uprawia&cacute;  <ind>a&zdot;iota&zdot;</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>agiotar</trd>,
    <trd>praticar agiotagem</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_b;&c_i;&c_r;&c_zh;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_i;&#x43D;&c_i;&c_t;&c_i; &c_a;&c_zh;&c_ib;&#x43E;&c_t;&c_a;&c_zh;</trd>,
    <trd>&c_s;&#x43F;&c_je;&#x43A;&c_u;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i; &#x43D;&c_a; &c_z;&#x43C;&c_ib;&#x43D;&c_a;&c_h; &#x43A;&c_u;&c_r;&c_s;&c_ib;&c_v;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_t;&c_i; &#x43D;&c_a; &c_b;&c_ib;&c_r;&c_zh;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="agxiot.0ado">
  <kap><tld/>ado</kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Ago <tld/>i:
      <ekz>
        la troa abundo da cirkulanta mono (la <klr>[militkompensoj el Francujo]</klr> estis multoblo
        de
        la hejma monaro en Germanujo) logis al intensa <tld/>ado, nur inter 1871 kaj
        1873 esti&gcirc;is en Germanujo &ccirc;. 700 akciaj kompanioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=467886">Periodo de
          spekulanto-kompanioj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_l;&c_mol;&c_n;&c_ja; &c_n;&c_a; <ind>&c_b;&c_ib;&c_r;&c_zh;&c_y;</ind></trd>
  <trdgrp lng="de">
    <trd>Agiotage</trd>,
    <trd>B&ouml;rsenspekulation</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>jobbing</trd>,
    <trd>stockjobbing</trd>
  </trdgrp>
  <trd lng="es">especulaci&oacute;n<klr tip="amb"> (burs&aacute;til)</klr></trd>
  <trd lng="fr">agiotage</trd>
  <trd lng="it">aggiotaggio</trd>
  <trdgrp lng="pl">
    <trd>a&zdot;iota&zdot;</trd>,
    <trd>spekulowanie na gie&lstroke;dzie</trd>
  </trdgrp>
</drv>

<drv mrk="agxiot.0isto">
  <kap><tld/>isto</kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Borsa <ref tip="super" cel="spekul.0anto">spekulanto</ref>:
      <ekz>
        <nom>Joseph Meyer</nom> <klr>[&#8230;]</klr> laboris en Londono, komence kiel helpulo en la
        eksportado, poste kiel memstara <tld/>isto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=502615">Joseph Meyer</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Spekulant</trd>,
    <trd>B&ouml;rsenspekulant</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stock <ind>jobber</ind></trd>,
    <trd>stockjobber</trd>
  </trdgrp>
  <trd lng="es">especulador<klr tip="amb"> (burs&aacute;til)</klr></trd>
  <trd lng="fr">agioteur</trd>
  <trd lng="it">speculatore<klr> (di borsa)</klr></trd>
  <trdgrp lng="pl">
    <trd>a&zdot;ioter</trd>,
    <trd>spekulant gie&lstroke;dowy</trd>
  </trdgrp>
  </drv>
</art>
