<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>a&gcirc;iot</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="agxiot.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Spekuli pri la prezo&scirc;an&gcirc;o de la borsaj valora&jcirc;oj
      a&ubreve; de komerca&jcirc;oj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_l;&c_ja;&c_c;&c_mol; &c_n;&c_a; <ind>&c_b;&c_ib;&c_r;&c_zh;&c_y;</ind></trd>,
    <trd>&c_s;&c_p;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_b;&c_ib;&c_r;&c_zh;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>spekulieren</trd>,
    <trd>in <ind>Differenzgesch&auml;ften</ind> <ind>spekulieren</ind></trd>,
    <trd>Agiotage betreiben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stockjobbing</trd>,
    <trd>speculate in stocks</trd>
  </trdgrp>
  <trd lng="fr">agioter</trd>
  <trdgrp lng="hu">
    <trd>&uuml;z&eacute;rkedik</trd>,
    <trd>spekul&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>agiotar</trd>,
    <trd>praticar agiotagem</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_b;&c_i;&c_r;&c_zh;&c_je;</trd>
</drv>
<drv mrk="agxiot.0ado">
  <kap><tld/>ado</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Spekulado pri prezo&scirc;an&gcirc;oj je akcioj, komerca&jcirc;oj ktp.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_l;&c_mol;&c_n;&c_ja; &c_n;&c_a; <ind>&c_b;&c_ib;&c_r;&c_zh;&c_y;</ind></trd>
  <trdgrp lng="de">
    <trd>Agiotage</trd>,
    <trd>B&ouml;rsenspekulation</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>jobbing</trd>,
    <trd>stockjobbing</trd>
  </trdgrp>
</drv>

<drv mrk="agxiot.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Spekulanto pri prezo&scirc;an&gcirc;oj je akcioj, komerca&jcirc;oj ktp.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Spekulant</trd>,
    <trd>B&ouml;rsenspekulant</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stock <ind>jobber</ind></trd>,
    <trd>stockjobber</trd>
  </trdgrp>
</drv>

</art>
