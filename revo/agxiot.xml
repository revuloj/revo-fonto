<vortaro>
<art mrk="$Id: agxiot.xml,v 1.21 2023/10/12 16:57:43 revo Exp $">
<kap>
  <rad>a&gcirc;iot</rad>/i
  <fnt>Z</fnt> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="agxiot.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="spekul.0i">Spekuli</ref>
      pri la prezo&scirc;an&gcirc;o de la borsaj valora&jcirc;oj
      a&ubreve; de komerca&jcirc;oj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_l;&c_ja;&c_c;&c_mol; &c_n;&c_a; <ind>&c_b;&c_ib;&c_r;&c_zh;&c_y;</ind></trd>,
    <trd>&c_s;&c_p;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_b;&c_ib;&c_r;&c_zh;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>spekulieren</trd>,
    <trd>in <ind>Differenzgesch&auml;ften</ind> <ind>spekulieren</ind></trd>,
    <trd>Agiotage betreiben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stockjobbing</trd>,
    <trd>speculate in stocks</trd>
  </trdgrp>
  <trd lng="es">especular<klr tip="amb"> (en bolsa)</klr></trd>
  <trd lng="fr">agioter</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5DE;&#x5E8; &#x5E2;&#x5DC; &#x5E2;&#x5E8;&#x5DB;&#x5D9; &#x5E1;&#x5D7;&#x5D5;&#x5E8;&#x5D5;&#x5EA; &#x5D0;&#x5D5; &#x5DE;&#x5D8;&#x5D1;&#x5E2;&#x5D5;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>&uuml;z&eacute;rkedik</trd>,
    <trd>spekul&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="it">
     <trd>aggiotare</trd>, <trd>speculare<klr> (econ.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6295;&#x6A5F;&#x58F2;&#x8CB7;&#x3059;&#x308B;  <pr>&#x3068;&#x3046;&#x304D;&#x3070;&#x3044;&#x3070;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5229;&#x9798;&#x3092;&#x304B;&#x305B;&#x3050;  <pr>&#x308A;&#x3056;&#x3084;&#x3092;&#x304B;&#x305B;&#x3050;</pr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>agiotar</trd>,
    <trd>praticar agiotagem</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_g;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_b;&c_i;&c_r;&c_zh;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&#x447;&#x438;&#x43D;&#x438;&#x442;&#x438; &#x430;&#x436;&#x456;&#x43E;&#x442;&#x430;&#x436;</trd>,
    <trd>&#x441;&#x43F;&#x435;&#x43A;&#x443;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438; &#x43D;&#x430; &#x437;&#x43C;&#x456;&#x43D;&#x430;&#x445; &#x43A;&#x443;&#x440;&#x441;&#x456;&#x432;</trd>,
    <trd>&#x433;&#x440;&#x430;&#x442;&#x438; &#x43D;&#x430; &#x431;&#x456;&#x440;&#x436;&#x456;</trd>
  </trdgrp>
</drv>

<drv mrk="agxiot.0ado">
  <kap><tld/>ado</kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Ago <tld/>i:
      <ekz>
        la troa abundo da cirkulanta mono (la <klr>[militkompensoj el Francujo]</klr> estis multoblo de
        la hejma monaro en Germanujo) logis al intensa <tld/>ado, nur inter 1871 kaj
        1873 esti&gcirc;is en Germanujo &ccirc;. 700 akciaj kompanioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=467886">Periodo de spekulanto-kompanioj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_l;&c_mol;&c_n;&c_ja; &c_n;&c_a; <ind>&c_b;&c_ib;&c_r;&c_zh;&c_y;</ind></trd>
  <trdgrp lng="de">
    <trd>Agiotage</trd>,
    <trd>B&ouml;rsenspekulation</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>jobbing</trd>,
    <trd>stockjobbing</trd>
  </trdgrp>
  <trd lng="fr">agiotage</trd>
  <trd lng="it">aggiotaggio</trd>
  <trd lng="es">especulaci&oacute;n<klr tip="amb"> (burs&aacute;til)</klr></trd>
</drv>

<drv mrk="agxiot.0isto">
  <kap><tld/>isto</kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Borsa <ref tip="super" cel="spekul.0anto">spekulanto</ref>:
      <ekz>
        <nom>Joseph Meyer</nom> <klr>[&#x2026;]</klr> laboris en Londono, komence kiel helpulo en la
        eksportado, poste kiel memstara <tld/>isto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=502615">Joseph Meyer</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Spekulant</trd>,
    <trd>B&ouml;rsenspekulant</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stock <ind>jobber</ind></trd>,
    <trd>stockjobber</trd>
  </trdgrp>
  <trd lng="es">especulador<klr tip="amb"> (burs&aacute;til)</klr></trd>
  <trd lng="fr">agioteur</trd>
  <trd lng="it">speculatore<klr> (di borsa)</klr></trd>
</drv>

</art>
