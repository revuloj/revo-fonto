<drv mrk="kolubr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kolubr1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Coluber</trd>), nevenena serpento.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_zh;</trd>
  <trd lng="ca">colobra</trd>
  <trd lng="de">Zornnatter</trd>
  <trd lng="es">culebra</trd>
  <trd lng="fr">couleuvre</trd>
  <trd lng="nl">toornslang</trd>
  <trd lng="ru">&c_u;&c_zh;</trd>
</drv>

</art>

