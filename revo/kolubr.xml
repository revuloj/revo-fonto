<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kolubr</rad>/o
</kap>
<drv mrk="kolubr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kolubr.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Coluber</trd>), nevenena serpento.
    </dif>
  </snc>
  <trd lng="de">Zornnatter</trd>
</drv>
<drv mrk="kolubr.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Colubridae</trd>), al kiu apartenas la plej
      multaj serpentoj, normale nedan&gcirc;eraj al la homo.
    </dif>
  </snc>
  <trd lng="de">Nattern</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
