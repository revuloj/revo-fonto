  <trd lng="uk">&c_r;&c_o;&c_z;&c_s;&c_u;&c_d;</trd>
</drv>

<drv mrk="trov.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="T"/>i, distingi ion novan a&ubreve; nekonitan:
      <ekz>
        el<tld/>i novan planedon, novan metodon;
      </ekz>
      <ekz>
        &scirc;ajnis tute neeble, ke la hindo inter la enpremoj de la
        piedoj de 200 homoj povus el<tld/>i la bezonatan postesignon
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>;
      </ekz>
      <ekz>
        la lastaj el<tld/>oj de la scienco;
      </ekz>
      <ekz>
        el<tld/>i la jam el<tld/>itan Amerikon
        <klr>(ion jam de &ccirc;iuj konatan)</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pens.el0i">elpensi</ref>,
      <ref cel="invent.0i">inventi</ref>,
      <ref cel="kovr.mal0i">malkovri</ref>
