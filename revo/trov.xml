<drv mrk="trov.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="T"/>i ion perditan, ne plu havatan:
      <ekz>
        re<tld/>i perditan braceleton;
      </ekz>
      <ekz>
        iu malsanulo re<tld/>is la sanon
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 15a</lok></fnt>;
      </ekz>
      <ekz>
        mi tiel kuris, por esprimi al vi mian respekton, ke mi ne povas
        re<tld/>i la spiron
        <fnt><bib>Revizoro</bib>, <lok>akto 3a, sceno 2a</lok></fnt>;
      </ekz>
      <ekz>
        la perditan tempon oni ne povas re<tld/>i.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_n;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;
  <klr>(&c_r;&c_a;&c_n;&c_je;&c_j;
  &c_z;&c_g;&c_u;&c_b;&c_l;&c_je;&c_n;&c_a;&c_je;)</klr></trd>
  <trdgrp lng="cs">
    <trd>shledat</trd>,
    <trd>znovu nal&eacute;zt</trd>
  </trdgrp>
  <trd lng="de">wieder finden</trd>
  <trdgrp lng="es">
    <trd>reencontrar</trd>,
    <trd>recuperar</trd>
  </trdgrp>
  <trd lng="fr">retrouver</trd>
  <trd lng="hu">megtal&aacute;l <klr tip="ind">(elveszettet)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x518D;&#x767A;&#x898B;&#12377;&#x308B;  <pr>&#12373;&#12356;&#x306F;&#12387;&#12369;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#x53D6;&#x308A;&#x623B;&#12377;  <pr>&#12392;&#x308A;&#12418;&#12393;&#12377;</pr></trd>
  </trdgrp>
  <trd lng="nl">terugvinden</trd>
  <trdgrp lng="pl">
    <trd>odnajdywa&cacute;</trd>,
    <trd>odszukiwa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_j;&c_t;&c_i;
  &c_v;&c_n;&c_o;&c_v;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>znova n&aacute;js&tcaron;</trd>,
    <trd>z&iacute;ska&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
