    <trd>&c_v;&c_ib;&c_d;&c_sh;&c_u;&c_k;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="trov.sin0i">
  <kap><ofc>*</ofc>sin <tld/>i</kap>
  <snc mrk="trov.sin0i.ie">
    <dif>
      <ref tip="dif" cel="trov.0igxi.ie"><tld lit="T"/>i&gcirc;i<sncref/></ref>:
      <ekz>
        trans la rivero sin <tld/>is aliaj hirundoj
        <fnt><bib>F</bib>, <lok>&FE; 26</lok></fnt>;
      </ekz>
      <ekz>
        la kaldrono, en kiu sin <tld/>as bolanta akvo
        <fnt><bib>F</bib>, <lok>&FE; 25</lok></fnt>;
      </ekz>
      <ekz>
        la fenestro <klr>[&#8230;]</klr> sin <tld/>is apud la pordo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        en nia lando sin ne <tld/>as montoj
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">sich befinden</trd>
    <trdgrp lng="hu">
      <trd>tal&aacute;lhat&oacute;</trd>,
      <trd>el&odblac;fordul</trd>,
      <trd>l&eacute;tezik</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>znajdowa&cacute; si&eogonek;</trd>,
      <trd>odnajdywa&cacute; si&eogonek;</trd>,
      <trd>spotyka&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <snc mrk="trov.sin0i.ia">
    <dif>
      <ref tip="dif" cel="trov.0igxi.ia"><tld lit="T"/>i&gcirc;i<sncref/></ref>:
      <ekz>
        sin <tld/>i tute sola.
      </ekz>
    </dif>
    <trd lng="hu">v&aacute;lik <klr tip="amb">(vmilyenn&eacute;)</klr></trd>
    <trd lng="pl">znajdowa&cacute; si&eogonek;</trd>
  </snc>
  <trd lng="be">&c_z;&c_n;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">sich <ind>finden</ind></trd>
  <trd lng="fr">se <ind>trouver</ind></trd>
  <trd lng="nl">zich <ind>bevinden</ind></trd>
  <trd lng="ru">&c_n;&c_a;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
