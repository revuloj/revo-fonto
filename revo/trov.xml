</drv>

<drv mrk="trov.0itajxo">
  <kap><tld/>ita&jcirc;o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="trov.0ajxo"><tld lit="T"/>a&jcirc;o</ref>:
      <ekz>
        pririgardante la <tld/>ita&jcirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mem transdonu al la estraro la grandvaloran <tld/>ita&jcirc;on
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        boteleto da spica brando estis jam metita en la paka&jcirc;eton,
        kiam subite aperis la knaboj kun pli granda, pli ampleksa
        botelo, sia
        <tld/>ita&jcirc;o
        <fnt><bib>Fab3</bib>, <lok>kolo de botelo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_n;&c_a;&c_h;&c_o;&c_d;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Fund</trd>,
    <trd>Fundsache</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;couverte<klr tip="amb"> (objet)</klr></trd>,
    <trd>trouvaille<klr tip="amb"> (objet)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lelet</trd>,
    <trd>tal&aacute;lt t&aacute;rgy</trd>
  </trdgrp>
  <trd lng="id"><ind>temu</ind>an</trd>
  <trd lng="nl">vondst</trd>
  <trdgrp lng="pl">
    <trd>rzecz <ind>znaleziona</ind></trd>,
    <trd>odkryty obiekt</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_h;&c_o;&c_d;&c_k;&c_a;</trd>
</drv>

<drv mrk="trov.bon0o">
