<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: influenc.xml,v 1.12 2023/11/19 10:22:13 revo Exp $">
<kap>
  <rad>influenc</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="influenc.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="influenc.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="grip.0o.MED">Gripo</ref>:
      <ekz>
        li havas <klr>[&#x2026;]</klr> simplan <tld/>on
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/migranta/nature.html">
          Nur nature!</url></vrk>,
          <lok>en: Migranta plumo, 1929</lok>
        </fnt>;
      </ekz>
      <ekz>
        se la <tld/>o estas severa, e&ccirc; pli ol 75&#x202F;% da populacio povas
        elmorti
        <fnt>
          <vrk><url ref="http://www.literatura.bucek.name/brno/fischer-influencoj_170119.pdf">Influencoj</url></vrk>
          <lok>literatura.bucek.name</lok>
        </fnt>;
      </ekz>
      <ekz>
        gripo (<tld/>o) estas al &ccirc;iu konata malsano
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2003/008099.html">Birda
          gripo, &ccirc;u dan&gcirc;era por la homo?</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pips.0o">pipso</ref>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_y;&c_p;</trd>
  <trdgrp lng="de">
    <trd>Grippe</trd>,
    <trd>Influenza</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>influenza</trd>,
    <trd>flu</trd>
  </trdgrp>
  <trd lng="fr">influenza</trd>
<!-- tiu tradukoj &scirc;ajne rilatas al fizika fenomeno:
  <trdgrp lng="cs">
    <trd>elektrostatick&aacute; indukce</trd>,
    <trd>influence</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>elektrostatick&aacute; indukcia</trd>,
    <trd>influencia</trd>
  </trdgrp>
-->
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_i;&#x43F;</trd>,
    <trd>&c_ib;&#x43D;&c_f;&#x43B;&#x44E;&c_je;&#x43D;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6D41;&#x884C;&#x6027;&#x611F;&#x5192; <pr>li&#xFA;x&#xED;ngx&#xEC;ngg&#x1CE;nm&#xE0;o</pr></trd>,
    <trd>&#x6D41;&#x611F; <pr>li&#xFA;g&#x1CE;n</pr></trd>,
    <trd>&#x6D41;&#x6027;&#x611F;&#x5192; <pr>li&#xFA;x&#xEC;ngg&#x1CE;nm&#xE0;o</pr></trd>
  </trdgrp>
</drv>
</art>

