<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: mart1.xml,v 1.2 2025/02/10 06:37:23 afido Exp $">
    <kap>
      <rad>Mart</rad>/o
    </kap>

<drv mrk="mart1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc mrk="mart1.0o.nomo">
    <dif>
      Aramedevena virina <ref tip="lst" lst="voko:personaj_nomoj"
      cel="nom.persona0o.GEN">nomo</ref>:
      <ekz>
        <tld/>o rigardis lin kaj la fratinon, ne komprenante
        <fnt>
          <bib>IKr</bib>
          <lok>En Kvieta Vespero</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> similetas al mia <tld/>o, amatino
        <fnt>
          <bib>SrT</bib>
          <lok>Libro X.</lok>
        </fnt>!
      </ekz>
    </dif>
    <trd lng="de">Martha</trd>
    <trd lng="en">Martha</trd>
  </snc>
  <ref tip="hom" cel="mart.0o">Marto (monato)</ref>
<trd lng="be">&c_M;&c_a;&c_r;&c_t;&c_a; <klr>(&c_ib;&c_m;&c_ja;)</klr></trd>
</drv>

  </art>

