<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: volb.xml,v 1.26 2023/12/17 21:21:10 revo Exp $">
<kap>
  <rad>volb</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="volb.0o">
  <kap><tld/>o</kap>
  <snc mrk="volb.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="ark.0ajxo">Arka&jcirc;o</ref> el vicoj de &scirc;tonoj,
      kiuj konsistigas murojn kaj supre kuni&gcirc;as en kurba
      <ref tip="super" cel="plafon.0o.ARKI">plafono</ref>:
      <ekz>
        ponto kun rondaj arkaj <tld/>oj
        <fnt><bib>Kiso</bib>, <lok>la brulanta urbo</lok></fnt>;
      </ekz>
      <ekz>
        apena&ubreve; <klr>[mi povis]</klr> savi min sub pordegan <tld/>on, kiam ekfuriozis la
        &scirc;tormo
        <fnt>
          <bib>Kiso</bib>
        </fnt>;
      </ekz>
      <ekz>
        du grandaj salonoj sur kies <tld/>o estis pentritaj siringviolaj &ccirc;evaloj
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="volb.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io <tld/>forma:
      <ekz>
        sub la stelornamita velur&scirc;tofo de la &ccirc;iela <tld/>o ek&scirc;vebis vualrubando de prae
        orienta melodio
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ale.0o.TRA">aleo<sncref/></ref>,
      <ref cel="lauxb.0o">la&ubreve;bo</ref>,
      <ref cel="firmam.0o.BIB">firmamento<sncref/></ref>
    </refgrp>
    <trd lng="de">W&ouml;lbung</trd>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_ja;&c_p;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bg">&c_s;&c_v;&c_o;&c_d;</trd>
  <trd lng="ca">volta</trd>
  <trd lng="cs">klenba</trd>
  <trd lng="de">Gew&ouml;lbe</trd>
  <trdgrp lng="el">
    <trd>&theta;&omikron_ton;&lambda;&omikron;&sigma_fina;</trd>,
    <trd>&tau;&rho;&omikron;&ypsilon_ton;&lambda;&lambda;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>vault</trd>,
    <trd>arched roof</trd>
  </trdgrp>
  <trd lng="es">b&oacute;veda</trd>
  <trdgrp lng="fr">
    <trd>voute</trd>
  </trdgrp>
  <trd lng="hu">boltozat</trd>
  <trdgrp lng="ja">
    <trd>&#12508;&#12540;&#12523;&#12488;</trd>,
    <trd>&#12450;&#12540;&#12481;&#24418;&#22825;&#20117;  <pr>&#12450;&#12540;&#12481;&#12363;&#12383;&#12390;&#12435;&#12376;&#12423;&#12358;</pr></trd>,
    <trd>&#20024;&#22825;&#20117;  <pr>&#12414;&#12427;&#12390;&#12435;&#12376;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">gewelf</trd>
  <trd lng="pt">ab&oacute;bada</trd>
  <trd lng="ru">&c_s;&c_v;&c_o;&c_d; <klr tip="amb">(&c_a;&c_r;&c_h;&c_i;&c_t;.)</klr></trd>
  <trd lng="sk">klenba</trd>
  <trd lng="uk">&c_s;&c_k;&c_l;&c_je;&c_p;&c_ib;&c_n;&c_n;&c_ja;</trd>
</drv>

<drv mrk="volb.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="volb.0a.provizita">
    <dif>
      Provizita, konstruita per <tld/>oj:
      <ekz>
        mi &ccirc;irka&ubreve;rigardis en la multkolora, antikveca <tld/>a salono
        <fnt>
          <bib>SatirRak</bib>
        </fnt>;
      </ekz>
      <ekz>
        renovigita <tld/>a&jcirc;kelo de la mona&hcirc;ejo
        <fnt>
        <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/007113.php">A&ubreve;strio &dash; Ser&ccirc;i la
          perditan paradizon</url></vrk>
          <bib>Monato</bib>
          <lok>2000/10, p. 20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="volb.0a.oforma">
    <dif>
      <tld lit="V"/>oforma:
      <ekz>
        <tld/>a &ccirc;apelo
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi venis finfine al granda alte <tld/>i&gcirc;anta pordego, kiu aspektis tre antikva
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="kurb.0a.FORM">kurba<sncref/></ref>,
       <ref cel="rond.0a.FORM">ronda<sncref/></ref>
    </refgrp>
    <refgrp tip="sub">
      <ref cel="konkav.0a">konkava</ref>,
      <ref cel="konvek.0a">konveksa</ref>
    </refgrp>
    <trd lng="de">gew&ouml;lbt</trd>
  </snc>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_l;&c_ja;&c_p;&c_je;&c_n;&c_ib;&c_s;&c_t;&c_y;</trd>,
      <trd>&c_s;&c_k;&c_l;&c_ja;&c_p;&c_je;&c_n;&c_n;&c_y;</trd>,
      <trd>&c_d;&c_u;&c_g;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;</trd>
    </trdgrp>
  <trd lng="ja">&#12450;&#12540;&#12481;&#24418;&#22825;&#20117;&#12398;  <pr>&#12450;&#12540;&#12481;&#12363;&#12383;&#12390;&#12435;&#12376;&#12423;&#12358;&#12398;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_l;&c_je;&c_p;&c_ib;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_u;&c_g;&c_o;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="volb.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      Super<ref tip="sin" cel="ark.0i">arki</ref> kiel <tld/>o:
      <ekz>
        alte super la tero kun &gcirc;ia mola krepuskolumo <tld/>is sin vasta, nubostriita
        spaco
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        per vere kosmaj proporcioj: giganta katedralo <tld/>anta la <klr>[&#8230;]</klr>
        printempan vivon
        <fnt>
          <bib>SatirRak</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_ib;&c_s;&c_a;&c_c;&c_mol; <klr>(&c_s;&c_k;&c_l;&c_ja;&c_p;&c_je;&c_n;&c_mol;&c_n;&c_je;&c_m;)</klr></trd>
  <trd lng="de">w&ouml;lben</trd>
  <trd lng="ja">&#12450;&#12540;&#12481;&#24418;&#22825;&#20117;&#12434;&#20184;&#12369;&#12427;  <pr>&#12450;&#12540;&#12481;&#12363;&#12383;&#12390;&#12435;&#12376;&#12423;&#12358;&#12434;&#12388;&#12369;&#12427;</pr></trd>
  <trd lng="uk">&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_s;&c_k;&c_l;&c_je;&c_p;&c_ib;&c_n;&c_n;&c_ja;</trd>
</drv>

</art>
