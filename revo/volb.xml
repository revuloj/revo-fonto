<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: volb.xml,v 1.18 2021/04/09 13:06:19 revo Exp $">
<kap>
  <rad>volb</rad>/o
</kap>
<drv mrk="volb.0o">
  <kap><tld/>o</kap>
  <snc mrk="volb.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="ark.0ajxo">Arka&jcirc;o</ref> el vicoj de &scirc;tonoj, 
      kiuj konsistigas murojn kaj supre kuni&gcirc;as en kurba 
      <ref tip="super" cel="plafon.0o.ARKI">plafono</ref>:
      <ekz>
        ponto kun rondaj arkaj <tld/>oj
        <fnt><bib>Kiso</bib>, <lok>la brulanta urbo</lok></fnt>;
      </ekz>
      <ekz>
        apena&ubreve; <klr>[mi povis]</klr> savi min sub pordegan <tld/>on, kiam ekfuriozis la
        &scirc;tormo
        <fnt>
          <bib>Kiso</bib>
        </fnt>;
      </ekz>
      <ekz>
        du grandaj salonoj sur kies <tld/>o estis pentritaj siringviolaj &ccirc;evaloj
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="volb.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io <tld/>forma:
      <ekz>
        sub la stelornamita velur&scirc;tofo de la &ccirc;iela <tld/>o ek&scirc;vebis vualrubando de prae
        orienta melodio
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ale.0o.TRA">aleo<sncref/></ref>,
      <ref cel="lauxb.0o">la&ubreve;bo</ref>,
      <ref cel="firmam.0o.BIB">firmamento<sncref/></ref>
    </refgrp>
    <trd lng="de">W&ouml;lbung</trd>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_ja;&c_p;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bg">&c_s;&c_v;&c_o;&c_d;</trd>
  <trd lng="ca">volta</trd>
  <trd lng="cs">klenba</trd>
  <trd lng="de">Gew&ouml;lbe</trd>
  <trdgrp lng="el">
    <trd>&theta;&omikron_ton;&lambda;&omikron;&sigma_fina;</trd>,
    <trd>&tau;&rho;&omikron;&ypsilon_ton;&lambda;&lambda;&omikron;&sigma_fina;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>vault</trd>,
    <trd>arched roof</trd>
  </trdgrp>
  <trd lng="es">b&oacute;veda</trd>
  <trdgrp lng="fr">
    <trd>voute</trd>
  </trdgrp>
  <trd lng="hu">boltozat</trd>
  <trd lng="nl">gewelf</trd>
  <trd lng="pt">ab&oacute;bada</trd>
  <trd lng="ru">&c_s;&c_v;&c_o;&c_d; <klr tip="amb">(&c_a;&c_r;&c_h;&c_i;&c_t;.)</klr></trd>
  <trd lng="sk">klenba</trd>
</drv>

<drv mrk="volb.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="volb.0a.provizita">
    <dif>
      Provizita, konstruita per <tld/>oj:
      <ekz>
        mi &ccirc;irka&ubreve;rigardis en la multkolora, antikveca <tld/>a salono
        <fnt>
          <bib>SatirRak</bib>
        </fnt>;
      </ekz>
      <ekz>
        renovigita <tld/>a&jcirc;kelo de la mona&hcirc;ejo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato"
              >A&ubreve;strio &dash; Ser&ccirc;i la perditan paradizon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="volb.0a.oforma">
    <dif>
      <tld lit="V"/>oforma:
      <ekz>
        <tld/>a &ccirc;apelo
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi venis finfine al granda alte <tld/>i&gcirc;anta pordego, kiu aspektis tre antikva
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="kurb.0a.FORM">kurba<sncref/></ref>,
       <ref cel="rond.0a.FORM">ronda<sncref/></ref>
    </refgrp>
    <trd lng="de">gew&ouml;lbt</trd>
  </snc>
</drv>


<drv mrk="volb.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      Super<ref tip="sin" cel="ark.0i">arki</ref> kiel <tld/>o:
      <ekz>
        alte super la tero kun &gcirc;ia mola krepuskolumo <tld/>is sin vasta, nubostriita
        spaco
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        per vere kosmaj proporcioj: giganta katedralo <tld/>anta la <klr>[&#8230;]</klr>
        printempan vivon
        <fnt>
          <bib>SatirRak</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">w&ouml;lben</trd>
</drv>


</art>
