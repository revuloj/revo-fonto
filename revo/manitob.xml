<art mrk="$Id: manitob.xml,v 1.10 2024/01/25 20:10:34 revo Exp $">
<kap>
  <rad>Manitob</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="manitob.M0o">
  <kap><tld/>o</kap>
  <snc mrk="manitob.M0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Provinco de
      <ref tip="malprt" cel="kanad.K0o.GEOG">Kanado</ref>
      en &gcirc;ia sud-centra parto:
      <ekz>
        tiuj grupoj da kristanoj kiuj nomi&gcirc;as <esc>huteritanoj</esc>
        (kredas al <esc>huteritismo</esc>) <klr>(pli &gcirc;uste huteranoj, huterismo la&ubreve; <nom>Jakob
        Hutter</nom>)</klr> &ccirc;efe lo&gcirc;as en la provincoj
        Alberto kaj <tld/>o
        <fnt>
          <aut>Be&ccirc;jo</aut>,
          <vrk><url ref="http://becxjo.blogspot.com/2009/07/rifuzo-de-esti-fotita.html">
          Rifuzo de esti fotita!</url></vrk>,
          <lok>La mondo el mia vidpunkto, 2009-07-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_n;&c_ib;&c_t;&c_o;&c_b;&c_a;</trd>
  <trd lng="bg">&c_M;&c_a;&c_n;&c_i;&c_t;&c_o;&c_b;&c_a;</trd>
  <trd lng="ca">Manitoba</trd>
  <trd lng="de">Manitoba</trd>
  <trd lng="en">Manitoba</trd>
  <trd lng="fr">Manitoba</trd>
  <trd lng="ja">&#x30DE;&#x30CB;&#x30C8;&#x30D0;&#x5DDE;  <pr>&#x30DE;&#x30CB;&#x30C8;&#x30D0;&#x3057;&#x3085;&#x3046;</pr></trd>
  <trd lng="pl">Manitoba</trd>     
  <trd lng="ro">Manitoba</trd>
  <trd lng="uk">&#x41C;&c_a;&#x43D;&c_ib;&c_t;&#x43E;&c_b;&c_a; (&#x43A;&c_a;&#x43D;&c_a;&c_d;&c_s;&#x44C;&#x43A;&c_a;
  &#x43F;&c_r;&#x43E;&c_v;&c_ib;&#x43D;&c_c;&c_ib;&#x44F;)</trd>
  <trdgrp lng="zh">
    <trd>&#x7DEC;&#x5C3C;&#x8A17;&#x5DF4; <pr>mi&#x1CE;nn&#xED;tu&#x14D;b&#x101;</pr></trd>,
    <trd>&#x7F05;&#x5C3C;&#x6258;&#x5DF4; <pr>mi&#x1CE;nn&#xED;tu&#x14D;b&#x101;</pr></trd>,
    <trd>&#x99AC;&#x5C3C;&#x6258;&#x5DF4; <pr>M&#x1CE;n&#xED;tu&#x14D;b&#x101;</pr></trd>,
    <trd>&#x9A6C;&#x5C3C;&#x6258;&#x5DF4; <pr>M&#x1CE;n&#xED;tu&#x14D;b&#x101;</pr></trd>,
    <trd>&#x66FC;&#x5C3C;&#x6258;&#x5DF4; <pr>M&#xE0;nn&#xED;tu&#x14D;b&#x101;</pr></trd>
  </trdgrp>
</drv>
</art>

