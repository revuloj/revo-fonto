<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gad.xml,v 1.29 2023/11/11 10:00:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gad</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gad.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gad.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
       Iu el familio <ref tip="malprt" cel="gaded.0oj">gadedoj</ref> de
       maraj <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>j
       kun &gcirc;is tri dorsaj kaj du anusaj na&gcirc;iloj, kies pluraj specioj
       estas man&gcirc;ataj:
        <refgrp tip="sub">
         <ref cel="moru.0o">moruo</ref>,
         <ref cel="eglefi.0o">eglefino</ref>,
         <ref cel="lojt.0o">lojto</ref>,
         <ref cel="merlan.0o">merlango</ref>;
       </refgrp>
       <ekz>
        Kajetano alportis kaserolon da rizo kun <tld/>o
        <fnt>
          <aut>V. Catal&agrave;, trad. J. Ventura i Freixas</aut>,
          <vrk>Soleco</vrk>,
          <lok>1967</lok>
        </fnt>.
       </ekz>
      
    </dif>
    <trd lng="de">Dorsch</trd>
    <trd lng="zh">&#x9C48;&#x9B5A; <pr>xu&#x11B;y&#xFA;</pr></trd>
</snc>
  <snc mrk="gad.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="gaded.0oj"><tld/>edoj</ref>
      (<trd lng="la">Gadus</trd>).
    </dif>
    <trd lng="de">Kabeljau</trd>
    <trdgrp lng="fr">
      <trd>cabillaud</trd>,
       <trd>morue</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_l;&c_u;&c_sh;&c_ch;&c_o;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>bacall&agrave;</trd>,
    <trd>abadejo</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mn&#xED;k mo&rcaron;sk&#xFD; a j.</trd>,
    <trd>su&scaron;en&#xE1; nesolen&#xE1; treska</trd>
  </trdgrp>
  <trd lng="en">cod</trd>
  <trd lng="hu">t&odblac;kehal</trd>
  <trdgrp lng="ja">
    <trd>&#x30BF;&#x30E9;</trd>,
    <trd>&#x5E72;&#x3057;&#x9C48;  <pr>&#x307B;&#x3057;&#x3060;&#x3089;</pr></trd>
  </trdgrp>
  <trd lng="nl">dors</trd>
  <trd lng="pt">gado<klr tip="amb"> (peixe)</klr></trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_s;&c_k;&c_a; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="sk">treska</trd>
  <trd lng="sv">torsk</trd>
  <trd lng="uk">&c_s;&c_u;&c_sh;&c_je;&#x43D;&c_a; &c_t;&c_r;&c_ib;&c_s;&#x43A;&c_a;</trd>
</drv>

<drv mrk="gad.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="gad.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> de maraj
      <ref tip="malprt" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Gadiformes</trd>) kun nur supraj ripoj.
    </dif>
  </snc>
  <trd lng="ca">gadiformes</trd>
  <trd lng="cs">tresky <klr>(rod)</klr></trd>
  <trd lng="de">Dorschartige</trd>
  <trd lng="fr">gadiformes</trd>
  <trd lng="hu">t&odblac;kehal-alak&uacute;ak</trd>
  <trd lng="nl">dorsachtigen</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_s;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">tresky <klr>(rod)</klr></trd>
  <trd lng="uk">&c_t;&c_r;&c_ib;&c_s;&#x43A;&#x43E;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_ib;</trd>
  <trd lng="zh">&#x9C48;&#x5F62;&#x76EE; <pr>xu&#x11B;x&#xED;ngm&#xF9;</pr></trd>
</drv>
</art>
