<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gad</rad>/o
</kap>
<drv mrk="gad.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="gaded.0oj"><tld/>edoj</ref>
      (<trd lng="la">Gadus</trd>), marfi&scirc;o.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>bacall&agrave;</trd>,
    <trd>abadejo</trd>
  </trdgrp>
  <trd lng="de">Dorsch</trd>
  <trdgrp lng="fr">
    <trd>cabillaud</trd>,
    <trd>morue</trd>
  </trdgrp>
  <trd lng="hu">t&odblac;kehal</trd>
  <trd lng="nl">dors</trd>
  <trd lng="pt">gado<klr tip="amb"> (peixe)</klr></trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_s;&c_k;&c_a; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="sv">torsk</trd>
</drv>

<drv mrk="gad.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso
      <ref tip="super" cel="fisx.osto0oj">ostofi&scirc;oj</ref>
      (<trd lng="la">Gadiformes</trd>).
    </dif>
  </snc>
  <trd lng="ca">gadiformes</trd>
  <trd lng="de">Dorschartige</trd>
  <trd lng="fr">gadiformes</trd>
  <trd lng="hu">t&odblac;kehal-alak&uacute;ak</trd>
  <trd lng="nl">dorsachtigen</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_s;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
