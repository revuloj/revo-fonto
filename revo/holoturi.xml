<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: holoturi.xml,v 1.7 2023/08/31 15:46:18 revo Exp $">
<kap>
  <rad>holoturi</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="holoturi.0o">
  <kap><tld/>o</kap>
  <snc mrk="holoturi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="kukum.mar0o.ZOO">Markukumo</ref>:
      <ekz>
        longformaj fi&scirc;oj tage dormas en la korpo de <tld/>oj kaj nokte 
        elna&gcirc;as
        <fnt>
          <aut>-</aut>,
          <vrk>El &ccirc;inaj gazetoj</vrk>,
          <lok><url ref="http://esperanto.china.org.cn/EL/EL/ElPopolaCxinio/98-7.html">
          El popola &Ccirc;inio, 1998-07</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o- kaj bird-amantoj povas trovi la buceron sur tiu strando
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=442575">Pangkor Island</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">cogombre de mar</trd>
  <trd lng="de">Holothurie</trd>
  <trdgrp lng="fr">
    <trd>concombre de mer</trd>,
    <trd>holothurie</trd>
  </trdgrp>
  <trd lng="hu">tengeri ugorka</trd>
  <trd lng="it">oloturia</trd>
  <trd lng="ja">&#x30CA;&#x30DE;&#x30B3;</trd>
  <trdgrp lng="nl">
    <trd>zeekomkommer</trd>,
    <trd>holothurie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pepino-do-mar</trd>,
    <trd>holot&uacute;ria</trd>
  </trdgrp>
</drv>
</art>

