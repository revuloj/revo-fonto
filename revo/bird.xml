<drv mrk="bird.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="bird.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bestoj kovritaj de <ref tip="prt" cel="plum.0o">plumoj</ref>, kun
      du piedoj, <ref tip="prt" cel="flug.0ilo">flugiloj</ref>,
      naskantaj
      <ref cel="ov.0o">ovojn</ref>, el <ref tip="lst" cel="klas.0o.BIO" lst="voko:zoologiaj_klasoj">klaso</ref> de <ref tip="malprt" cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Aves</trd>), solaj plu vivantaj
      <ref tip="malprt" cel="dinosa.0oj">dinosa&ubreve;roj</ref>:
      <ekz>
        <tld lit="B"/>oj estas klaso de vertebruloj kun &ccirc;irka&ubreve; 9 &gcirc;is 10 mil vivantaj
        specioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16060">Birdoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_t;&c_u;&c_sh;&c_k;&c_ib; 
<klr>(&c_k;&c_l;&c_ja;&c_s;&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;&c_w;)</klr></trd>
  <trd lng="de">V&ouml;gel</trd>
  <trd lng="en">birds</trd>
  <trd lng="es">aves</trd>
  <trd lng="fr">oiseaux</trd>
  <trd lng="hu">madarak<klr tip="amb"> (oszt&aacute;lya)</klr></trd>
  <trd lng="no">fugler</trd>
  <trd lng="pl">ptaki</trd>
  <trd lng="ru">&c_p;&c_t;&c_i;&c_c;&c_y;<klr tip="amb"> 
(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  <trd lng="tr">ku&scedil;lar</trd>
</drv>

<drv mrk="bird.korto0o">
  <kap>korto<tld/>o, <var><kap>kort<tld/>o</kap></var></kap>
  <snc mrk="bird.korto0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref cel="bird.0o" tip="super"><tld lit="B"/>o</ref> bredata en 
      <ref tip="vid" cel="kort.birdo0o.ARKI">
      <tld/>okorto</ref> pro siaj viando a&ubreve;
      <ref cel="ov.0o">ovoj</ref>:
      <ekz>
        <ref tip="sub" cel="anas.0o">anaso</ref>,
        <ref tip="sub" cel="kok.0o">koko</ref> kaj 
        <ref tip="sub" cel="meleag.0o">meleagro</ref> estas
        korto<tld/>oj;
      </ekz>
      <ekz>
        sklavinoj <klr>[&#x2026;]</klr> komencis disporti kukojn, rostitajn kortbirdojn kaj
        &ccirc;asa&jcirc;on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la viruson kunportas la migrantaj birdoj, kaj ili infektas bredejajn
        korto<tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Tailhades</aut>
          <vrk><url ref="https://www.monato.be/2006/008985.php">Gurdado de onidiroj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_o;&c_j;&c_s;&c_k;&c_a;&c_ja; 
<ind>&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;</ind></trd>
  <trd lng="de">Gefl&uuml;gel</trd>
  <trdgrp lng="en">
    <trd>farm bird</trd>,
    <trd>domestic fowl</trd>
  </trdgrp>
  <trd lng="es">ave de corral</trd>
  <trdgrp lng="fr">
    <trd>oiseau de basse-cour</trd>,
    <trd>volaille</trd>
  </trdgrp>
  <trd lng="hu">baromfi</trd>
  <trd lng="pl">dr&oacute;b</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_ja;&c_ja; 
&c_p;&c_t;&c_i;&c_c;&c_a;</trd>
</drv>

<drv mrk="bird.musx0o">
  <kap>mu&scirc;<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="bird.musx0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="kolibr.0o.ZOO">Kolibro</ref>:
      <ekz>
        <nom>Nel</nom> estis ravita precipe pro la vido de paradizaj
        mu&scirc;<tld/>etoj
        <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj 
        praarbaro</url></vrk>, <lok>1978</lok></fnt>;
      </ekz>
      <ekz>
        la mu&scirc;<tld/>o sidis sur la flugiloj de la aglo, kaj tiamaniere flugis pii alten
        ol &ccirc;iuj
        <fnt>
          <aut>Giovanni Balconi</aut>
          <vrk><url ref="http://www.ueci.it/grupoj/milano/Homilioj/MM010421.doc">2a Paska Diman&ccirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ib;&c_b;&c_r;&c_y;</trd>
  <trd lng="bg">&c_k;&c_o;&c_l;&c_i;&c_b;&c_r;&c_i;</trd>
  <trd lng="ca">colibr&iacute;</trd>
  <trd lng="de">Kolibri</trd>
  <trd lng="en">humming bird</trd>
  <trd lng="es">colibr&iacute;</trd>
  <trdgrp lng="fr">
    <trd>colibri</trd>,
    <trd>oiseau-mouche</trd>
  </trdgrp>
  <trd lng="hu">kolibri</trd>
  <trd lng="nl">kolibrie</trd>
  <trd lng="pl">koliber</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_i;&c_b;&c_r;&c_i;</trd>
  <trd lng="tr">sinekku&scedil;u</trd>
</drv>

<drv mrk="bird.nagxo0o">
  <kap>na&gcirc;o<tld/>o, <var><kap>na&gcirc;<tld/>o</kap></var></kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="bird.nagxo0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref cel="bird.0o" tip="super"><tld lit="B"/>o</ref> havanta
      na&gcirc;okapablon:
      <ekz>
        <ref tip="sub" cel="anas.0o">anaso</ref>, 
        <ref tip="sub" cel="cign.0o">cigno</ref>, 
        <ref tip="sub" cel="mev.0o">mevo</ref> estas na&gcirc;o<tld/>oj;
      </ekz>
      <ekz>
        mara na&gcirc;<tld/>o kun blanka plumaro
        <fnt>
          <aut>S. Grenkamp</aut>
          <vrk><url ref="https://arkivo.esperanto-france.org/divers/grenkamp/ove_m.htm">Oficiala Vortaro de Esperanto</url></vrk>
          <lok>mevo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_d;&c_a;&c_p;&c_l;&c_a;&c_w;&c_n;&c_a;&c_ja;  
&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="br">evn palvezek</trd>
  <trdgrp lng="de">
    <trd>Schwimmvogel</trd>,
    <trd>Wasservogel</trd>
  </trdgrp>
  <trd lng="en">waterfowl</trd>
  <trd lng="es">ave acu&aacute;tica</trd>
  <trd lng="fr">oiseau d'eau</trd>
  <trd lng="hu">&uacute;sz&oacute;mad&aacute;r</trd>
  <trd lng="nl">zwemvogel</trd>
  <trd lng="ru">&c_v;&c_o;&c_d;&c_o;&c_p;&c_l;&c_a;&c_v;&c_a;&c_ju;&c_shch;&c_a;&c_ja;
&c_p;&c_t;&c_i;&c_c;&c_a;</trd>
  <trd lng="tr">su ku&scedil;u</trd>
</drv>

<drv mrk="bird.paradiz0o">
  <kap>paradiz<tld/>o</kap>
  <snc mrk="bird.paradiz0o.SNC">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Nov-Gvinea <ref tip="lst" cel="bird.0o" lst="voko:birdoj"><tld/>o</ref> kun plenkolora plumaro, <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="paradizeed.0oj">paradizeedoj</ref>
      (<trd lng="la">Paradisaea</trd>):
      <ekz>kantantaj paradiz<tld/>oj enflugis
        <fnt>
          <aut>Franjo Modrijan, trad. John Lundgren</aut>
          <vrk>Fabeloj</vrk>
          <lok>&ccirc;apitro 6a</lok>
        </fnt>;
      </ekz>
      <ekz>niaj svedaj fra&ubreve;linoj <klr>[...]</klr> estas nur paseroj
      kompare kun tiuj &ccirc;i paradiz<tld/>oj kaj kolibroj
        <fnt>
          <aut>Oscar Levertin, trad. John Lundgren</aut>
          <vrk>Uzino 2</vrk>
          <lok>La fremdlanda voja&gcirc;o de Mathias Waldius</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="paradize.0o">paradizeo</ref>
  </snc>
  <snc mrk="bird.paradiz0o.fenikso">
    <dif>
      <ref tip="dif" cel="feniks.0o.MIT">Fenikso</ref>:
      <ekz>
        ho vi paradiza birdo, &ccirc;iucentjare renovigata, en flamoj naskata kaj en flamoj
        mortanta
        <fnt>
          <bib>Fab3</bib>
          <lok>Birdo Fenikso</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr"><trd>paradisier</trd><trd>oiseau de
  paradis</trd></trdgrp>
</drv>

<drv mrk="bird.rabo0oj">
  <kap>rabo<tld/>oj, <var><kap>rab<tld/>oj</kap></var></kap>
  <snc mrk="bird.rabo0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="malprt" cel="bird.0oj"><tld lit="B"/>oj</ref> kun akra
      beko kaj ungegoj, tage &ccirc;asantaj el la ordoj 
      <trd lng="la">Accipitriformes</trd> kaj <trd lng="la">
      Falconiformes</trd>, foje anka&ubreve; la nokte &ccirc;asantaj el
      la ordo <trd lng="la">Strigiformes</trd>:
      <ekz>
        la rabo<tld/>oj mallevi&gcirc;is sur la kadavrojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 15:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        rabo<tld/>o, kiun &ccirc;irka&ubreve;e atakis aliaj rabo<tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 12:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rabo<tld/>oj de la dezerto persekutis la kolombojn kaj monedojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        rabo<tld/>oj elektas la altan konstrua&jcirc;on kiel gvatejon kaj man&gcirc;ejon
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007052.html">Verda nature, science, socie</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        turfalko estas la plej ofta rabo<tld/>o en A&ubreve;strio
        <fnt>
          <bib>Monato</bib>
          <aut>Walter, Renate, Irene, Stefan Klag</aut>
          <vrk><url ref="https://www.monato.be/2007/009167.php">Birdo de la jaro</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_k;&c_a;&c_l;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;
    <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr>
  </trd>
  <trdgrp lng="bg">
    <trd>&c_h;&c_i;&c_shch;&c_n;&c_a; &c_p;&c_t;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_b;&c_l;&c_i;&c_v;&c_a; 
&c_p;&c_t;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
