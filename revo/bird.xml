<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bird</rad>/o
</kap>
<drv mrk="bird.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el la klaso <ref tip="malprt" cel="bird.0oj"><tld/>oj</ref>:
      <ekz>&gcirc;i estas ankora&ubreve; <tld/>o sur la tegmento
        <klr>(neefektivigita revo.
        <ref tip="vid" cel="cert.0a">certa
2</ref>)</klr><fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <trd lng="en">bird</trd>
    <trd lng="es">p&aacute;jaro</trd>
    <trd lng="fr">oiseau</trd>
    <trd lng="hu">mad&aacute;r</trd>
    <trd lng="tr">ku&scedil;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ridinde afekta homo:
      <ekz>bela birdo
        <klr>(dando)</klr><fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <trd lng="fr">gandin</trd>
    <trd lng="hu">piperk&odblac;c</trd>
  </snc>
  <trd lng="de">Vogel</trd>
  <trd lng="nl">vogel</trd>
  <trd lng="pl">ptak</trd>
  <trd lng="ru">&c_p;&c_t;&c_i;&c_c;&c_a;</trd>
</drv>
<drv mrk="bird.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de vertebruloj (<trd lng="la">Aves</trd>),
      kovritaj de plumoj, kun du piedoj, flugiloj, naskantaj ovojn.
    </dif>
  </snc>
  <trd lng="de">V&ouml;gel</trd>
  <trd lng="en">birds</trd>
  <trd lng="es">aves</trd>
  <trd lng="fr">oiseaux</trd>
  <trd lng="hu">madarak<klr tip="amb"> (oszt&aacute;lya)</klr></trd>
</drv>
<drv mrk="bird.rabo0oj">
  <kap>rabo<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj"><tld/>oj</ref>
      (<trd lng="la">Falconiformes</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Taggreife</trd>,
    <trd>Greifv&ouml;gel</trd>
  </trdgrp>
  <trd lng="en">birds of prey</trd>
  <trd lng="fr">oiseaux de proie</trd>
  <trdgrp lng="hu">
    <trd>s&oacute;lyomalak&uacute;ak</trd>,
    <trd>ragad&oacute;z&oacute;madarak</trd>
  </trdgrp>
</drv>
<drv mrk="bird.vad0oj">
  <kap>vad<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj"><tld/>oj</ref>
      (<trd lng="la">Ciconiiformes</trd>), kies anoj havas plej ofte longajn
      krurojn kaj bekojn.
    </dif>
  </snc>
  <trd lng="de">Schreitv&ouml;gel</trd>
  <trd lng="en">wading <ind>birds</ind></trd>
  <trd lng="fr">&eacute;chassiers</trd>
  <trdgrp lng="hu">
    <trd>g&oacute;lyaalak&uacute;ak</trd>,
    <trd>g&aacute;zl&oacute;madarak</trd>
  </trdgrp>
</drv>
</art>
