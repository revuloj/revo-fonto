<?xml version="1.0"?>

<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bird</rad>/o
</kap>
<drv mrk="bird.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bird.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el la klaso <ref tip="malprt" cel="bird.0oj"><tld/>oj</ref>:
      <ekz>
        &gcirc;i estas ankora&ubreve; <tld/>o sur la tegmento
        <klr>(neefektivigita revo.
        <ref tip="vid" cel="cert.0a">certa
        2</ref>)</klr><fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>evn</trd>,
      <trd>labous</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>ocell</trd>,
      <trd>pardal</trd>
    </trdgrp>
    <trd lng="en">bird</trd>
    <trd lng="es">p&aacute;jaro</trd>
    <trd lng="fr">oiseau</trd>
    <trd lng="it">uccello</trd>
    <trdgrp lng="he">
      <trd>&tsadi;&yod;&pe;&vav;&resh;</trd>,
      <trd>&ayin;&vav;&fpe;</trd>
    </trdgrp>
    <trd lng="hu">mad&aacute;r</trd>
    <trd lng="tr">ku&scedil;</trd>
  </snc>
  <snc mrk="bird.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ridinde afekta homo:
      <ekz>
        bela birdo
        <klr>(dando)</klr><fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">gandin</trd>
    <trd lng="hu">piperk&odblac;c</trd>
  </snc>
  <trd lng="be">&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="de">Vogel</trd>
  <trd lng="nl">vogel</trd>
  <trd lng="pl">ptak</trd>
  <trd lng="ru">&c_p;&c_t;&c_i;&c_c;&c_a;</trd>
</drv>

<drv mrk="bird.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de vertebruloj (<trd lng="la">Aves</trd>),
      kovritaj de plumoj, kun du piedoj, flugiloj, naskantaj ovojn.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_t;&c_u;&c_sh;&c_k;&c_ib; <klr>(&c_k;&c_l;&c_ja;&c_s; &c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;&c_w;)</klr></trd>
  <trd lng="de">V&ouml;gel</trd>
  <trd lng="en">birds</trd>
  <trd lng="es">aves</trd>
  <trd lng="fr">oiseaux</trd>
  <trd lng="hu">madarak<klr tip="amb"> (oszt&aacute;lya)</klr></trd>
  <trd lng="ru">&c_p;&c_t;&c_i;&c_c;&c_y;<klr tip="amb"> (&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
</drv>

<drv mrk="bird.rabo0oj">
  <kap>rabo<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj"><tld/>oj</ref>
      (<trd lng="la">Falconiformes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_k;&c_a;&c_l;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trdgrp lng="br">
    <trd>evned-preizh</trd>,
    <trd>laboused-preizh</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Taggreife</trd>,
    <trd>Greifv&ouml;gel</trd>,
    <trd>Raubv&ouml;gel</trd>
  </trdgrp>
  <trd lng="en">birds of prey</trd>
  <trd lng="fr">oiseaux de proie</trd>
  <trd lng="he">&ayin;&vav;&fpe; &dalet;&vav;&resh;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>s&oacute;lyomalak&uacute;ak</trd>,
    <trd>ragad&oacute;z&oacute;madarak</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_i;&c_shch;&c_n;&c_y;&c_je; &c_p;&c_t;&c_i;&c_c;&c_y;<klr tip="ind"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_d;&c_n;&c_je;&c_v;&c_n;&c_y;&c_je; &c_h;&c_i;&c_shch;&c_n;&c_y;&c_je; &c_p;&c_t;&c_i;&c_c;&c_y;<klr tip="amb"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="bird.vad0oj">
  <kap>vad<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj"><tld/>oj</ref>
      (<trd lng="la">Ciconiiformes</trd>), kies anoj havas plej ofte
      longajn
      krurojn kaj bekojn.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_s;&c_l;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="br">hirc'hareged</trd>
  <trd lng="de">Schreitv&ouml;gel</trd>
  <trd lng="en">wading <ind>birds</ind></trd>
  <trd lng="fr">&eacute;chassiers</trd>
  <trdgrp lng="hu">
    <trd>g&oacute;lyaalak&uacute;ak</trd>,
    <trd>g&aacute;zl&oacute;madarak</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_i;&c_s;&c_t;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;<klr tip="ind"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_g;&c_o;&c_l;&c_je;&c_n;&c_a;&c_s;&c_t;&c_y;&c_je;<klr tip="amb"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
</drv>
</art>
