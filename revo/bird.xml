    <trd><ind>unggas</ind> air</trd>
  </trdgrp>
  <trd lng="ja">&#27700;&#40165;  <pr>&#12415;&#12378;&#12392;&#12426;</pr></trd>
  <trd lng="nl">zwemvogel</trd>
  <trd lng="pl">ptak wodny</trd>
