<drv mrk="bird.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="bird.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bestoj kovritaj de <ref tip="prt" cel="plum.0o">plumoj</ref>, kun
      du piedoj, <ref tip="prt" cel="flug.0ilo">flugiloj</ref>,
      naskantaj
      <ref cel="ov.0o">ovojn</ref>, el <ref tip="lst" cel="klas.0o.BIO"
      lst="voko:zoologiaj_klasoj">klaso</ref> de <ref tip="malprt"
      cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Aves</trd>), solaj plu vivantaj
      <ref tip="malprt" cel="dinosa.0oj">dinosa&ubreve;roj</ref>:
      <ekz>
        <tld lit="B"/>oj estas klaso de vertebruloj kun &ccirc;irka&ubreve; 9 &gcirc;is 10 mil vivantaj
        specioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16060">Birdoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#40165;&#32177; <pr>ni&#462;og&amacron;ng</pr></trd>
  </snc>
  <trd lng="be">&c_p;&c_t;&c_u;&c_sh;&c_k;&c_ib; <klr>(&c_k;&c_l;&c_ja;&c_s;
  &c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;&c_w;)</klr></trd>
  <trd lng="de">V&ouml;gel</trd>
  <trd lng="en">birds</trd>
  <trd lng="es">aves</trd>
  <trd lng="fr">oiseaux</trd>
  <trd lng="hu">madarak<klr tip="amb"> (oszt&aacute;lya)</klr></trd>
  <trd lng="id">burung</trd>
  <trd lng="ja">&#40165;&#39006;  <pr>&#12385;&#12423;&#12358;&#12427;&#12356;</pr></trd>
  <trd lng="no">fugler</trd>
  <trd lng="pl">ptaki</trd>
  <trd lng="ru">&c_p;&c_t;&c_i;&c_c;&c_y;<klr tip="amb"> (&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  <trd lng="tr">ku&scedil;lar</trd>
  <trd lng="uk">&c_p;&c_t;&c_a;&c_h;&c_i;</trd>
</drv>

<drv mrk="bird.0aro">
  <kap>
    <tld/>aro
  </kap>
  <snc mrk="bird.0aro.ZOO">
    <dif>
      Aro da <tld/>oj kunaj en iu loko:
      <ekz>
        tutaj <tld/>aroj forflugis de &ccirc;iu parto de la arbaro kun la&ubreve;taj krioj
        <fnt>
          <bib>Rob</bib>
          <lok>Parto 2a</lok>
        </fnt>;
      </ekz>
      <ekz>
        konfuzita <tld/>aro ekflugis al-alte
        <fnt>
          <bib>SrT</bib>
          <lok>Libro 5a</lok>
        </fnt>;
      </ekz>
      <ekz>
        farmodoma korto plena da kort<tld/>aro man&gcirc;etadanta en sterko
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekunua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_a;&c_d;&c_a; <klr
    tip="amb">(&c_p;&c_t;&c_u;&c_sh;&c_a;&c_k;)</klr></trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_j; <klr tip="amb">(&c_ch;&c_a;&c_r;&c_a;&c_d;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="de">Vogelschar</trd>
  <trd lng="en"><ind>flock</ind> of birds</trd>
  <trd lng="pl">ptactwo</trd>
</drv>

<drv mrk="bird.0ido">
  <kap><ofc>*</ofc> <tld/>ido</kap>
  <snc mrk="bird.0ido.ZOO">
    <dif>
      <ref tip="super" cel="id.0o.BIO">Ido<sncref/></ref> de <tld/>o:
      <ekz>
        <tld/>ido &mdash; nematura <tld/>o
        <fnt><bib>F</bib> <lok>&FE; 36</lok></fnt>;
      </ekz>
      <ekz>
        se vi trovos neston de <tld/>o <klr>&#8230;</klr> kaj la patrino sidas
        sur la <tld/>idoj a&ubreve; sur la ovoj, ne prenu la patrinon kune kun
        la idoj
        <fnt><bib>MT</bib> <lok>&Rea; 22:6</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_t;&c_u;&c_sh;&c_a;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_t;&c_u;&c_sh;&c_a;&c_n;&c_jo;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>nestling</trd>,
    <trd>fledgeling</trd>,
    <trd>chick</trd>
  </trdgrp>
  <trd lng="id"><ind>anak</ind> burung</trd>
  <trd lng="ja">&#12402;&#12394;&#40165;  <pr>&#12402;&#12394;&#12393;&#12426;</pr></trd>
  <trdgrp lng="pl">
    <trd>piskl&eogonek;</trd>,
    <trd>ptasz&aogonek;tko</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_t;&c_je;&c_n;&c_je;&c_c;</trd>
</drv>

<drv mrk="bird.korto0o">
  <kap>korto<tld/>o, <var><kap>kort<tld/>o</kap></var></kap>
  <snc mrk="bird.korto0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref cel="bird.0o" tip="super"><tld lit="B"/>o</ref> bredata en
      <ref tip="vid" cel="kort.birdo0o.ARKI">
      <tld/>okorto</ref> pro siaj viando a&ubreve;
      <ref cel="ov.0o">ovoj</ref>:
      <ekz>
        <ref tip="sub" cel="anas.0o">anaso</ref>,
        <ref tip="sub" cel="kok.0o">koko</ref> kaj
        <ref tip="sub" cel="meleag.0o">meleagro</ref> estas
        korto<tld/>oj;
      </ekz>
      <ekz>
        sklavinoj <klr>[&#8230;]</klr> komencis disporti kukojn, rostitajn kort<tld/>ojn kaj
        &ccirc;asa&jcirc;on
        <fnt><bib>Far1</bib> <lok>&Ccirc;apitro 24a</lok></fnt>;
      </ekz>
      <ekz>
        la viruson kunportas la migrantaj <tld/>oj, kaj ili infektas bredejajn
        korto<tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Tailhades</aut>
          <vrk><url ref="&Monato;/2006/008985.php">Gurdado de onidiroj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#23478;&#31165; <pr>ji&amacron;q&iacute;n</pr></trd>,
    <trd>&#23478;&#31165;&#30340;&#32905; <pr>ji&amacron;q&iacute;nder&ograve;u</pr></trd>,
    <trd>&#37326;&#31165; <pr>y&ecaron;q&iacute;n</pr></trd>,
    <trd>&#31165; <pr>q&iacute;n</pr></trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_o;&c_j;&c_s;&c_k;&c_a;&c_ja;
  <ind>&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;</ind></trd>
  <trd lng="ca">ocell de corral</trd>
  <trd lng="cv">&c_k;&c_i;&c_l;&c_t;&c_i; <ind>&c_k;&c_a;&c_j;&#1233;&c_k;-&c_k;&#1239;&c_sh;&#1239;&c_k;</ind></trd>
  <trd lng="de">Gefl&uuml;gel</trd>
  <trdgrp lng="en">
    <trd>farm bird</trd>,
    <trd>domestic fowl</trd>
  </trdgrp>
  <trd lng="es">ave de corral</trd>
  <trdgrp lng="fr">
    <trd>oiseau de basse-cour</trd>,
    <trd>volaille</trd>
  </trdgrp>
  <trd lng="hu">baromfi</trd>
  <trd lng="id">unggas</trd>
  <trd lng="ja">&#23478;&#31165;  <pr>&#12363;&#12365;&#12435;</pr></trd>
  <trd lng="pl">dr&oacute;b</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_ja;&c_ja; &c_p;&c_t;&c_i;&c_c;&c_a;</trd>
</drv>

<drv mrk="bird.lauxbo0o">
  <kap>la&ubreve;bo<tld/>o,
  <var><kap>satena la&ubreve;bo<tld/>o <fnt><aut>R. Harry, B. Fox</aut> <vrk><url
  ref="https://www.esperanto.org.nz/anzed/anzed.html">Australian and New Zealand Esperanto
  Dictionary</url></vrk> <lok>2020</lok></fnt></kap></var></kap>
  <snc mrk="bird.lauxbo0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mezgranda <tld/>o vivanta en malsekaj arbaroj de nordorienta kaj sudorienta A&ubreve;stralio,
      konata pro tio ke la viro por la pari&gcirc;o konstruas bran&ccirc;an la&ubreve;bon ornamitan per precipe bluaj
      objektoj;
      genro el familio ptilonorin&hcirc;edoj (<trd lng="la">Ptilonorhynchus</trd>);
      <ref tip="sin" cel="ptilonorink.0o.ZOO">ptilonorinko</ref>:
      <ekz>
        de la arboj la sonoril<tld/>o <klr>[&#8230;]</klr> kaj la la&ubreve;bo<tld/>o plenigis la aeron per sonoj
        <fnt>
          <aut>E. Turner, trad. R. McGrath</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0144/0001.php">Sep Junaj
          A&ubreve;stralianoj</url></vrk>
          <lok>1994</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Satinlaubenvogel</trd>,
    <trd>Seidenlaubenvogel</trd>
  </trdgrp>
  <trd lng="en">satin bower bird</trd>
  <trd lng="fr">jardinier satin&eacute;</trd>
  <trd lng="it">uccello giardiniere satinato</trd>
</drv>

<drv mrk="bird.musx0o">
  <kap>mu&scirc;<tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="bird.musx0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="kolibr.0o.ZOO">Kolibro</ref>:
      <ekz>
        <nom>Nel</nom> estis ravita precipe pro la vido de paradizaj
        mu&scirc;<tld/>etoj
        <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut> <vrk><url
        ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj
        praarbaro</url></vrk> <lok>1978</lok></fnt>;
      </ekz>
      <ekz>
        la mu&scirc;<tld/>o sidis sur la flugiloj de la aglo, kaj tiamaniere flugis pii alten
        ol &ccirc;iuj
        <fnt>
          <aut>Giovanni Balconi</aut>
          <vrk><url ref="http://www.ueci.it/grupoj/milano/Homilioj/MM010421.doc">2a Paska
          Diman&ccirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#34562;&#38592; <pr>f&emacron;ngqu&egrave;</pr></trd>
  </snc>
