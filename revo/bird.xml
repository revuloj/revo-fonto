<drv mrk="bird.paradiz0o">
  <kap>paradiz<tld/>o</kap>
  <snc mrk="bird.paradiz0o.SNC">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Nov-Gvinea <ref tip="lst" cel="bird.0o" lst="voko:birdoj"><tld/>o</ref> kun plenkolora
      plumaro, <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="paradizeed.0oj">paradizeedoj</ref>
      (<trd lng="la">Paradisaea</trd>):
      <ekz>kantantaj paradiz<tld/>oj enflugis
        <fnt>
          <aut>Franjo Modrijan, trad. John Lundgren</aut>
          <vrk>Fabeloj</vrk>
          <lok>&ccirc;apitro 6a</lok>
        </fnt>;
      </ekz>
      <ekz>niaj svedaj fra&ubreve;linoj <klr>[...]</klr> estas nur paseroj
      kompare kun tiuj &ccirc;i paradiz<tld/>oj kaj kolibroj
        <fnt>
          <aut>Oscar Levertin, trad. John Lundgren</aut>
          <vrk>Uzino 2</vrk>
          <lok>La fremdlanda voja&gcirc;o de Mathias Waldius</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="paradize.0o">paradizeo</ref>
  </snc>
  <snc mrk="bird.paradiz0o.fenikso">
    <dif>
      <ref tip="dif" cel="feniks.0o.MIT">Fenikso</ref>:
      <ekz>
        ho vi paradiza <tld/>o, &ccirc;iucentjare renovigata, en flamoj naskata kaj en flamoj
        mortanta
        <fnt>
          <bib>Fab3</bib>
          <lok>Birdo Fenikso</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_j;&c_s;&c_k;&c_a;&c_ja;
  <ind>&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;</ind></trd>
  <trdgrp lng="fr">
     <trd>paradisier</trd>
     <trd>oiseau de paradis</trd>
  </trdgrp>
  <trd lng="id">cenderawasih</trd>
  <trdgrp lng="ja">
    <trd>&#12501;&#12454;&#12481;&#12519;&#12454;</trd>,
    <trd>&#12468;&#12463;&#12521;&#12463;&#12481;&#12519;&#12454;</trd>
  </trdgrp>
  <trd lng="pl">rajski <ind/>ptak</trd>
  <trd lng="uk">&c_r;&c_a;&c_j;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_p;&c_t;&c_a;&c_h;</trd>
</drv>

<drv mrk="bird.rabo0oj">
  <kap>rabo<tld/>oj, <var><kap>rab<tld/>oj</kap></var></kap>
  <snc mrk="bird.rabo0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="malprt" cel="bird.0oj"><tld lit="B"/>oj</ref> kun akra
      beko kaj ungegoj, tage &ccirc;asantaj el la ordoj
      <trd lng="la">Accipitriformes</trd> kaj <trd lng="la">
      Falconiformes</trd>, foje anka&ubreve; la nokte &ccirc;asantaj el
      la ordo <trd lng="la">Strigiformes</trd>:
      <ekz>
        la rabo<tld/>oj mallevi&gcirc;is sur la kadavrojn
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 15:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        rabo<tld/>o, kiun &ccirc;irka&ubreve;e atakis aliaj rabo<tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>&Jer; 12:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rabo<tld/>oj de la dezerto persekutis la kolombojn kaj monedojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro 1a</lok>
        </fnt>;
      </ekz>
      <ekz>
        rabo<tld/>oj elektas la altan konstrua&jcirc;on kiel gvatejon kaj man&gcirc;ejon
        <fnt>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2000/007052.php">Verda
          nature, science, socie</url></vrk>
          <bib>Monato</bib>
          <lok>2000/08, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        turfalko estas la plej ofta rabo<tld/>o en A&ubreve;strio
        <fnt>
          <bib>Monato</bib>
          <aut>Walter, Renate, Irene, Stefan Klag</aut>
          <vrk><url ref="&Monato;/2007/009167.php">Birdo de la jaro</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#29467;&#31165; <pr>m&ecaron;ngq&iacute;n</pr></trd>,
    <trd>&#38588;&#24418;&#30446; <pr>zh&#468;nx&iacute;ngm&ugrave;</pr></trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_k;&c_a;&c_l;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;
    <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr>
  </trd>
  <trdgrp lng="bg">
    <trd>&c_h;&c_i;&c_shch;&c_n;&c_a; &c_p;&c_t;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_b;&c_l;&c_i;&c_v;&c_a; &c_p;&c_t;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>evned-preizh</trd>,
    <trd>laboused-preizh</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Taggreife</trd>,
    <trd>Greifv&ouml;gel</trd>,
    <trd>Raubv&ouml;gel</trd>
  </trdgrp>
  <trd lng="en">birds of prey</trd>
  <trdgrp lng="es">
    <trd>rapaces</trd>,
    <trd>aves de presa</trd>,
    <trd>rapaz <klr tip="amb">(ave)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>oiseaux de proie</trd>,
    <trd>rapaces<klr> (oiseaux)</klr></trd>
  </trdgrp>
  <trd lng="he">&ayin;&vav;&fpe; &dalet;&vav;&resh;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>s&oacute;lyomalak&uacute;ak</trd>,
    <trd>ragad&oacute;z&oacute;madarak</trd>
  </trdgrp>
  <trd lng="id"><ind>burung</ind> pemangsa</trd>
  <trd lng="pl"><ind>ptaki</ind> drapie&zdot;ne</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_i;&c_shch;&c_n;&c_y;&c_je; &c_p;&c_t;&c_i;&c_c;&c_y;<klr tip="ind">
    (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_d;&c_n;&c_je;&c_v;&c_n;&c_y;&c_je; &c_h;&c_i;&c_shch;&c_n;&c_y;&c_je;
    &c_p;&c_t;&c_i;&c_c;&c_y;<klr tip="amb"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_h;&c_i;&c_zh;&c_ib; &c_p;&c_t;&c_a;&c_h;&c_i;</trd>,
    <trd>&c_s;&c_o;&c_k;&c_o;&c_l;&c_o;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="bird.vad0oj">
  <kap>vad<tld/>oj, <var><kap>vado<tld/>oj</kap></var></kap>
  <snc mrk="bird.vad0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <tld lit="B"/>oj kun plej ofte
      longaj kruroj kaj bekoj, kiuj ofte
      vadas en akvo, <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">ordo</ref>
      el la klaso <ref tip="malprt" cel="bird.0oj"><tld/>oj</ref> (<trd
      lng="la">Ciconiiformes</trd>):
      <ekz>
        la longaj piedfingroj de vad<tld/>oj estas formitaj
        por piediri sur mar&ccirc;oj kaj flosantaj plantoj
        <fnt><bib>OriginoDeSpecioj</bib> <lok>&ccirc;apitro
        6a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#40475;&#24418;&#30446; <pr>gu&agrave;nx&iacute;ngm&ugrave;</pr></trd>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_s;&c_l;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;<klr tip="amb">
  (&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="br">hirc'hareged</trd>
  <trdgrp lng="cs">
    <trd>brodiv&iacute;</trd>,
    <trd>brodiv&yacute; pt&aacute;k</trd>
  </trdgrp>
  <trd lng="de">Schreitv&ouml;gel</trd>
  <trd lng="en">wading <ind>birds</ind></trd>
  <trd lng="es">aves lim&iacute;colas</trd>
  <trd lng="fr">&eacute;chassiers</trd>
  <trdgrp lng="hu">
    <trd>g&oacute;lyaalak&uacute;ak</trd>,
    <trd>g&aacute;zl&oacute;madarak</trd>
  </trdgrp>
  <trd lng="ja">&#28169;&#31165;&#39006;  <pr>&#12375;&#12423;&#12358;&#12365;&#12435;&#12427;&#12356;</pr></trd>
  <trd lng="pl"><ind>ptaki</ind> w&eogonek;drowne</trd>
  <trd lng="ro">p&abreve;s&abreve;ri limicole</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_i;&c_s;&c_t;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; <klr tip="ind">
    (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_g;&c_o;&c_l;&c_je;&c_n;&c_a;&c_s;&c_t;&c_y;&c_je; <klr
    tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
  <trd lng="sk">brodiv&eacute; vt&aacute;ky</trd>
  <trd lng="tr">uzun bacakl&inodot; ku&scedil;lar</trd>
  <trd lng="uk">&c_g;&c_o;&c_l;&c_ib;&c_n;&c_a;&c_s;&c_t;&c_ib;</trd>
</drv>
