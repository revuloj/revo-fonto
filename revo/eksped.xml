<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eksped.xml,v 1.30 2025/08/28 08:43:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>eksped</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="eksped.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="eksped.0i.POSX">
    <uzo tip="fak">POSX</uzo>
    <dif>
      <ref tip="super" cel="send.el0i.KOMUNE">Elsendi</ref> al difinita adreso, loko:
      <ekz>
        <tld/>i leteron, komerca&jcirc;on, armeon;
      </ekz>
      <ekz>
        ni <tld/>as al vi <klr>[&#8230;]</klr> keston enhavantan 60 botelojn da vinoj
        diversaj, konforme je la mendo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa generalo <tld/>is spionojn al la proksimi&gcirc;antaj Libianoj, por ekscii
        ilian nombron
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u oni ne povas por &ccirc;iu okazo &ccirc;iam havi &ccirc;e si pulvoreton, kiu vin tute silente
        <tld/>os trans la A&hcirc;eronon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>?
      </ekz>
      <ekz>
        neniun mi volus tro frue <tld/>i en la &ccirc;ielon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_s;&c_y;&c_l;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_p;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_z;&c_p;&c_r;&c_a;&c_shch;&c_a;&c_m;</trd>
  <trdgrp lng="ca">
    <trd>expedir</trd>,
    <trd>trametre</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>expedovat</trd>,
    <trd>odeslat</trd>,
    <trd>vypravit</trd>,
    <trd>zas&iacute;lat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>absenden</trd>,
    <trd>abschicken</trd>,
    <trd>aufgeben</trd>,
    <trd>wegschicken</trd>,
    <trd>expedieren<klr> [ark]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>expedite</trd>,
    <trd>dispatch</trd>
  </trdgrp>
  <trd lng="fr">exp&eacute;dier<klr tip="ind"> (envoyer)</klr></trd>
  <trdgrp lng="hu">
    <trd>k&uuml;ld<klr tip="ind"> (felad)</klr></trd>,
    <trd>felad<klr tip="ind"> (k&uuml;ld)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x767A;&#36865;&#12377;&#x308B;  <pr>&#x306F;&#12387;&#x305D;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#36865;&#x308B;  <pr>&#x304A;&#x304F;&#x308B;</pr></trd>,
    <trd>&#x6D3E;&#36963;&#12377;&#x308B;  <pr>&#x306F;&#12369;&#12435;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verzenden</trd>,
    <trd>versturen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekspediowa&cacute;</trd>,
    <trd>wysy&lstroke;a&cacute;<klr> (np. list)</klr></trd>,
    <trd>nadawa&cacute;<klr> (np. list)</klr></trd>,
    <trd>odprawia&cacute;</trd>,
    <trd>wyprawia&cacute;<klr> si&eogonek; (w drog&eogonek;)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>expedir</trd>,
    <trd>aviar</trd>,
    <trd>enviar</trd>,
    <trd>despachar</trd>
  </trdgrp>
  <trd lng="ro">trimite prin po&#537;t&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_s;&c_y;&c_l;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odosla&tcaron;</trd>,
    <trd>vypravi&tcaron;</trd>,
    <trd>zasla&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_i;&#x43B;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&#x43F;&c_r;&c_a;&c_v;&#x43B;&#x44F;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5BC4; <pr>j&#xEC;</pr></trd>,
    <trd>&#x8D4D; <pr>j&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="eksped.0o">
  <kap><tld/>o</kap>
  <snc mrk="eksped.0o.ado">
    <dif>
      <tld lit="E"/>ado:
      <ekz>
        koncerne vian proponon pri drapoj, mi beda&ubreve;ras, <klr>[&#8230;]</klr> atendante vian
        raporton pri la <tld/>o <klr>[&#8230;]</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kvin tagojn post la <tld/>o la unuaj japanaj abonantoj sciigis pri la (aera)
        alveno de la maja numero
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Peeraerts</aut>
          <vrk><url ref="&Monato;/2003/008156.html">Statistiko</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_s;&c_y;&c_l;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_a;&c_d;&c_p;&c_r;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_d;&c_z;&c_je;&c_ja;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>expedici&oacute;</trd>,
      <trd>tramesa</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Absendung</trd>,
      <trd>Aufgabe</trd>,
      <trd>Expedition<klr> [ark]</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&uuml;ld&eacute;s</trd>,
      <trd>felad&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>verzenden<klr tip="ind"> (zn.)</klr></trd>,
      <trd>verzending</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>spedycja</trd>,
      <trd>nadanie</trd>,
      <trd>wysy&lstroke;ka</trd>
    </trdgrp>
    <trd lng="zh">&#x5BC4;&#36865; <pr>j&#xEC;s&#xF2;ng</pr></trd>
</snc>
  <snc mrk="eksped.0o.itajxo">
    <dif>
      <tld lit="E"/>ita&jcirc;o.
    </dif>
    <trd lng="be">&c_a;&c_d;&c_p;&c_r;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_r;&c_e;&c_ch;)</klr></trd>
    <trdgrp lng="ca">
      <trd>cosa expedida</trd>,
      <trd>cosa tramesa</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Sendung</trd>
    </trdgrp>
    <trd lng="hu">k&uuml;ldem&eacute;ny</trd>
    <trd lng="nl">zending</trd>
  </snc>
  <trd lng="bg">&c_i;&c_z;&c_p;&c_r;&c_a;&c_shch;&c_a;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>odesl&aacute;n&iacute;</trd>,
    <trd>vypraven&iacute;</trd>
  </trdgrp>
  <trd lng="en">dispatch</trd>
  <trdgrp lng="fr">
    <trd>envoi</trd>,
    <trd>exp&eacute;dition<klr tip="ind"> (envoi)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x767A;&#36865;  <pr>&#x306F;&#12387;&#x305D;&#12358;</pr></trd>,
    <trd>&#x6D3E;&#36963;  <pr>&#x306F;&#12369;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">przesy&lstroke;ka</trd>
  <trdgrp lng="sk">
    <trd>expedovanie</trd>,
    <trd>odoslanie</trd>,
    <trd>vypravenie</trd>,
    <trd>zaslanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&#x43F;&c_r;&c_a;&c_v;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_v;&c_ib;&c_d;&#x43F;&c_r;&c_a;&c_v;&#x43A;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_i;&#x43B;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="eksped.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc mrk="eksped.0ejo.loko">
    <dif>
      Loko destinita por <tld/>ado:
      <ekz>
        la <tld/>ejo <klr>[&#8230;]</klr> ku&scirc;is tre oportune sur la bazaro post malfermita
        pordo <klr>[&#8230;]</klr>, tiel ke la kolera farmanto, kiu postulis proceson, kun
        la plej granda facileco povis ensalti
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        gvidanto de la <tld/>ejo de SEU
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>ejo, oni plenigas kontenerojn per ru&gcirc;aj kestoj, numerita plumbsigelo
        fermas la &leftquot;skatolegon&rightquot;: ek al Afriko
        <fnt>
          <bib>MD17</bib>
          <vrk>&Ccirc;inaj falsitaj produktoj por Afriko</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_n;&c_k;&c_t; &c_a;&c_d;&c_p;&c_r;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Versandb&uuml;ro</trd>,
    <trd>Expeditionsb&uuml;ro</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ekspedycja</trd>,
    <trd>wysy&lstroke;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&#x43A;&c_s;&#x43F;&c_je;&c_d;&c_i;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_d;&c_ib;&#x43B;&c_je;&#x43D;&#x43D;&#x44F; &c_d;&#x43B;&#x44F; &c_v;&c_ib;&c_d;&#x43F;&c_r;&c_a;&c_v;&#x43A;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#22577;&#x520A;&#x767C;&#36865;&#x623F; <pr>b&#xE0;ok&amacron;nf&amacron;s&#xF2;ngf&#xE1;ng</pr></trd>
</drv>

<drv mrk="eksped.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="eksped.0isto.EKON">
    <dif>
      Tiu, kiu profesie <tld/>as:
      <ekz>
        se iu kliento mendas ion &ccirc;e mi,
        mi donas la mendilon al iu <tld/>isto
        <fnt>
         <aut>Yao Yongzhong, red. Xie Ruifeng</aut>,
         <vrk><url ref="http://esperanto.china.org.cn/2015-07/02/content_35967223.htm">
         Fari&gcirc;i verkisto kaj starigi retbutikon...</url></vrk>,
         <lok>El Popola &Ccirc;inio, 2015-07-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_e;&c_d;&c_y;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="fr">
    <trd>entreprise d'exp&eacute;dition</trd>,
    <trd>entreprise de routage</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x767A;&#36865;&#x4FC2;  <pr>&#x306F;&#12387;&#x305D;&#12358;&#x304B;&#x304B;&#x308A;</pr></trd>,
    <trd>&#x904B;&#36865;&#x696D;&#32773;  <pr>&#12358;&#12435;&#x305D;&#12358;&#x304E;&#12423;&#12358;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nadawca</trd>,
    <trd>ekspedytor</trd>
  </trdgrp>
  <trd lng="uk">&c_je;&#x43A;&c_s;&#x43F;&c_je;&c_d;&c_i;&c_t;&#x43E;&c_r;</trd>
</drv>
</art>
