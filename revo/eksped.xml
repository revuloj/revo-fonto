<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>eksped</rad>/i
</kap>
<drv mrk="eksped.0i">
<kap><ofc>*</ofc><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Sendi al difinita adreso, loko:
<ekz><tld/>i leteron, komerca&jcirc;on, armeon.</ekz>
</dif>
</snc>
  <trdgrp lng="de">
     <trd>absenden</trd>,
     <trd>abschicken</trd>,
     <trd>aufgeben</trd>,
     <trd>wegschicken</trd>,
     <trd>expedieren<klr> [ark]</klr></trd>
  </trdgrp>
<trd lng="en">expedite</trd>
<trd lng="fr">exp&eacute;dier<klr tip="ind"> (envoyer)</klr></trd>
<trdgrp lng="ru">
  <trd>&c_o;&c_t;&c_s;&c_y;&c_l;&c_a;&c_t;&c_mol;</trd>,
  <trd>&c_o;&c_t;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
</trdgrp>
</drv>
<drv mrk="eksped.0o">
<kap><tld/>o</kap>
<snc num="1">
<dif>
<tld lit="E"/>ado. 
</dif>
  <trdgrp lng="de">
     <trd>Absendung</trd>,
     <trd>Aufgabe</trd>,
     <trd>Expedition<klr> [ark]</klr></trd>
  </trdgrp>
</snc>
<snc num="2">
<dif>
<tld lit="E"/>ita&jcirc;o.  
</dif>
  <trdgrp lng="de">
     <trd>Sendung</trd>
  </trdgrp>
</snc>
<trdgrp lng="fr">
  <trd>envoi</trd>,
  <trd>exp&eacute;dition<klr tip="ind"> (envoi)</klr></trd></trdgrp> 
</drv>
</art>
