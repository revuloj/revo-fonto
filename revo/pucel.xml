<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pucel</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="pucel.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Virga junulino:
      <ekz>
        li malice trompis kompatindan knabinon, kiu ne plu estas <tld/>o
        pro lia kulpo
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 52a, p.
        688a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pucelle</trd>
  <trdgrp lng="hu">
    <trd>sz&udblac;z</trd>,
    <trd>sz&udblac;zl&aacute;ny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pucela</trd>,  
    <trd>donzela</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>

