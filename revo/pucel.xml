<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pucel.xml,v 1.10 2025/05/03 06:35:54 revo Exp $">
<kap>
  <rad>pucel</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="pucel.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="pucel.0o.virgulino">
    <dif>
      <ref tip="super" cel="virg.0ulino">Virgulino</ref> (juna):
      <ekz>
        li malice trompis kompatindan knabinon, kiu ne plu estas <tld/>o
        pro lia kulpo
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 52a,
        p.
        688a</lok></fnt>;
      </ekz>
      <ekz>
        Johana de Arko, kromnomata la &leftquot;<tld lit="P"/>o de Orleano&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7654"
              >Johana de Arko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frauxl.0ino.GEN">fra&ubreve;lino</ref>,
      <ref cel="midinet.0o">midineto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_je;&c_v;&c_a;&c_ja; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_ib;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_c;&c_e;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">panna</trd>
  <trdgrp lng="de">
    <trd>Jungfrau</trd>,
    <trd>Jungfer</trd>,
    <trd>Maid</trd>
  </trdgrp>
  <trd lng="en">maiden</trd>
  <trd lng="es">doncella</trd>
  <trd lng="fr">pucelle</trd>
  <trdgrp lng="hu">
    <trd>sz&udblac;z</trd>,
    <trd>sz&udblac;zl&aacute;ny</trd>
  </trdgrp>
  <trd lng="pl">m&lstroke;oda dziewica</trd>
  <trdgrp lng="pt">
    <trd>pucela</trd>,
    <trd>donzela</trd>
  </trdgrp>
  <trd lng="ro">t&acirc;n&abreve;r&abreve; fecioar&abreve;</trd>
  <trd
  lng="ru">&c_d;&c_je;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">panna</trd>
</drv>
</art>

