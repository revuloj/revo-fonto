<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&hcirc;an</rad>/o
</kap>
<drv mrk="hxan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc num="1">
    <dif>
      Titolo de princo a&ubreve; reganto en Centra Azio.
    </dif>
  </snc>
  <snc num="2">
    <dif>
      <klr>(Mezepoko)</klr> Suvereno de la tjurkaj, tataraj kaj mongolaj
      triboj, kaj imperiestro de &Ccirc;inujo:
      <ekz>
        <ind>&Gcirc;en&gcirc;is <tld/>o</ind>,
        <trd lng="de">Dschingis Khan</trd>
        <trd lng="fr">Gengis-khan</trd>
        <trd lng="ru">&c_Ch;&c_i;&c_n;&c_g;&c_i;&c_z;-&c_h;&c_a;&c_n;</trd>
      </ekz>
      <ekz>
        <ind>Batu <tld/>o</ind>.
        <trd lng="ru">&c_h;&c_a;&c_n; &c_B;&c_a;&c_t;&c_y;&c_j;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_n;</trd>
  <trd lng="de">Khan</trd>
  <trd lng="en">khan</trd>
  <trd lng="fr">khan</trd>
  <trd lng="nl">kan<klr tip="ind"> (vorst)</klr></trd>
  <trd lng="ru">&c_h;&c_a;&c_n;</trd>
</drv>
</art>

