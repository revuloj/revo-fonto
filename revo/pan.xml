<drv mrk="pan.maiz0o">
  <kap>maiz<tld/>o</kap>
  <snc mrk="pan.maiz0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="P"/>o el maizo:
      <ekz>
        tradicia man&gcirc;a&jcirc;o dum Lunaso estas kuketoj homformaj, oni kutime bakas ilin el
        maiz<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=294749">Lunaso</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_k;&c_u;&c_r;&c_u;&c_z;&c_n;&c_y; &c_h;&c_l;&c_je;&c_b;</trd>
