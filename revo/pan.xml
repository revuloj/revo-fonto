<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pan.xml,v 1.44 2020/01/15 20:10:20 revo Exp $">
<kap>
<ofc>*</ofc>
  <rad>pan</rad>/o
</kap>
<drv mrk="pan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pan.0o.mangxo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="mangx.0ajxo" lst="voko:man&gcirc;a&jcirc;oj">Man&gcirc;a&jcirc;o</ref>
       farita el knedita fermentinta kaj bakita faruno:
      <ekz>
        seka peco da <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 17:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i malfre&scirc;a <tld/>o estas malmola, kiel &scirc;tono
        <fnt>
          <bib>F</bib>
          <lok>&FE; 33</lok>
        </fnt>;
      </ekz>
      <ekz>
        man&gcirc;i <tld/>on sen butero
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> provizis sin per buter<tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kun froma&gcirc;o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        el la pasto <klr>[&#8230;]</klr> ili bakis macajn plat<tld/>ojn, &ccirc;ar &gcirc;i ankora&ubreve; ne
        fermentis
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 12:39</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>on ne esperis, subite kuko aperis
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o bu&scirc;on ne ser&ccirc;as
        <klr>(oni devas peni por ricevi la dezirata&jcirc;on)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kie <tld/>o estas, tie musoj ne mankas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ima <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi havas grandan tritikan <tld/>on
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>;
      </ekz>
      <ekz>
        blank<tld/>eto
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 16:31</lok>
        </fnt> <klr>(bulko)</klr>;
      </ekz>
      <ekz>
        &scirc;i man&gcirc;as nigran <tld/>on
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        hordea <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        li postulis bieron kaj soldatan <tld/>on kun ajlo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        vendistino de suker<tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        eh, mi man&gcirc;is <tld/>on ne el unu forno, mi vidis multajn
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kuko estas &ccirc;ia speco de franda <tld/>o
        <fnt>
          <bib>LR</bib>
          <lok>2. Vortuzo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="pan.0o.rimedo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="pan.viv0o.FIG">Vivpano</ref>:
      <ekz>
        espero <tld/>on ne donas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        Dio donis infanon, Dio donos por &gcirc;i <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne karesu per mano, sed karesu per <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu, kiu man&gcirc;as mian <tld/>on, levis kontra&ubreve; min la piedon
        <fnt>
          <bib>NT</bib>
          <lok>Johano 13:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi forlasis mian naskurbon kaj nun perlaboras mian <tld/>on en alia lando
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006811.html">Reago: Nekredeblaj malveroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        vi estas alkatenita, sed al <tld/>dona arbo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>dona okupo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_l;&c_je;&c_b;</trd>
  <trd lng="br">bara</trd>
  <trd lng="ca">pa</trd>
  <trdgrp lng="cs">
    <trd>chl&eacute;b</trd>,
    <trd>chleba</trd>
  </trdgrp>
  <trd lng="de">Brot</trd>
  <trd lng="en">bread</trd>
  <trd lng="es">pan</trd>
  <trd lng="fr">pain</trd>
  <trd lng="hu">keny&eacute;r</trd>
  <trd lng="id">roti</trd>
  <trd lng="ja">&#12497;&#12531; [<ind>&#12401;&#12435;</ind>]</trd>
  <trd lng="ku">nan</trd>
  <trd lng="nl">brood</trd>
  <trd lng="pl">chleb</trd>
  <trd lng="pt">p&atilde;o</trd>
  <trd lng="ru">&c_h;&c_l;&c_je;&c_b;</trd>
  <trd lng="sl">kruh</trd>
  <trd lng="sk">kruh</trd>
  <trd lng="sk">chlieb</trd>
  <trd lng="sv">br&ouml;d</trd>
  <trd lng="zh">&#38754;&#21253; [<ind>mi&agrave;nb&#257;o</ind>]</trd>
</drv>

<drv mrk="pan.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="pan.0ero.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Tre malgranda rompopeco da <tld/>o:
      <ekz>
        ili devis <klr>[&#8230;]</klr> nutri sin per la <tld/>eroj, kiujn oni &jcirc;etadis al ili
        <fnt>
          <bib>Fab4</bib>
          <lok>Malbona princo (Legendo)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_o;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_y;&c_h;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">engruna</trd>
  <trdgrp lng="cs">
    <trd>drobek chleba</trd>,
    <trd>sk&yacute;va chleba</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Br&ouml;sel</trd>,
    <trd>Brotkr&uuml;mel</trd>
  </trdgrp>
  <trd lng="es">miga <klr>(de pan)</klr></trd>
  <trd lng="fr">miette de pain</trd>
  <trd lng="hu">keny&eacute;rmorzsa</trd>
  <trd lng="id"><ind>remah</ind>-remah (roti)</trd>
  <trd lng="ja">&#12497;&#12531;&#23633;
[<ind>&#12401;&#12435;&#12367;&#12378;</ind>]</trd>
  <trd lng="nl">kruimel</trd>
  <trd lng="ru">&c_h;&c_l;&c_je;&c_b;&c_n;&c_a;&c_ja;
  &c_k;&c_r;&c_o;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">omrvinka</trd>
  <trd lng="sv">br&ouml;dsmula</trd>
</drv>

<drv mrk="pan.0erumi">
  <kap><tld/>erumi, <var><kap><tld/>umi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pan.0erumi.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Kovri per <tld/>eroj anta&ubreve; fritado:
      <ekz>
        <tld/>erumi fi&scirc;on.
      </ekz>
    </dif>
    <ref tip="vid" cel="graten.0i">grateni</ref>
  </snc>
  <trd lng="ca">arrebossar</trd>
  <trd lng="de">panieren</trd>
  <trdgrp lng="es">
    <trd>empanizar</trd>,
    <trd>empanar</trd>
  </trdgrp>
  <trd lng="fr">paner</trd>
  <trdgrp lng="hu">
    <trd>pan&iacute;roz</trd>,
    <trd>bepan&iacute;roz</trd>
  </trdgrp>
  <trd lng="nl">paneren</trd>
  <trd lng="ru">&c_p;&c_a;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">oba&lcaron;ova&tcaron; do chlebovej str&uacute;hanky</trd>
  <trd lng="cs">obalovat v chlebov&eacute; strouhance</trd>
</drv>

<drv mrk="pan.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pan.0isto.KOMUNE">
    <dif>
      Homo, kiu faras kaj vendas <tld/>on:
      <ekz>
        por ili la <tld/>isto kuiras kukojn ja misterajn
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        malfacilis al lokaj <tld/>istoj kaj kukistoj akiri buteron el eksterlandaj
        merkatoj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="https://www.monato.be/2008/009534.php">Ja-pan-io kaj butero</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_l;&c_je;&c_b;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_a;&c_ch;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>flequer</trd>,
    <trd>forner</trd>
  </trdgrp>
  <trd lng="cs">peka&rcaron; a prodava&ccaron; chleba</trd>
  <trd lng="de">Brotb&auml;cker</trd>
  <trd lng="es">panadero</trd>
  <trd lng="fr">boulanger</trd>
  <trd lng="hu">p&eacute;k</trd>
  <trd lng="nl">bakker</trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">pek&aacute;r a predava&ccaron; chleba</trd>
</drv>

<drv mrk="pan.maiz0o">
  <kap>maiz<tld/>o</kap>
  <snc mrk="pan.maiz0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="P"/>o el maizo:
      <ekz>
        tradicia man&gcirc;a&jcirc;o dum Lunaso estas kuketoj homformaj, oni kutime bakas ilin el
        maiz<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=294749"
              >Lunaso</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kuku&rcaron;i&ccaron;n&yacute; chl&eacute;b</trd>
  <trd lng="de">Maisbrot</trd>
  <trd lng="es">pan de ma&iacute;z</trd>
  <trd lng="fr">pain de ma&iuml;s</trd>
  <trd lng="sk">kukuri&ccaron;n&yacute; chlieb</trd>
</drv>

<drv mrk="pan.ringo0o">
  <kap>ringo<tld/>o</kap>
  <snc mrk="pan.ringo0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="P"/>o muldita la&ubreve; ringa formo:
      <ekz>
        <nom>Simeoni</nom> pretigis la vesperman&gcirc;on, alportis sur la 
        tablon sep ringo<tld/>ojn
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Bagel</trd>
  <trdgrp lng="es">
    <trd>rosco</trd>,
    <trd>rosca</trd>
  </trdgrp>
  <trd lng="fr">couronne<klr> (pain)</klr></trd>
</drv>

<drv mrk="pan.sur0ajxo">
  <kap>sur<tld/>a&jcirc;o</kap>
  <snc mrk="pan.sur0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Io, kion oni povas meti sur <tld/>on por kunman&gcirc;i, ekz.
      froma&gcirc;o a&ubreve; kolbastran&ccirc;a&jcirc;o:
      <ekz>
        <klr>[el brindzo]</klr> oni <klr>[faras]</klr> sur<tld/>a&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="https://www.monato.be/2013/011027.php"
              >Oro blanka</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        fre&scirc;aj fruktoj (banano, frago, pomo, beroj, ...) anka&ubreve; tre ta&ubreve;gas kiel
        sur<tld/>a&jcirc;o, &ccirc;u en pecoj, &ccirc;u kiel ka&ccirc;o
        <fnt>
          <vrk><url ref="https://issuu.com/lodchjo/docs/plantbazitaj_surpanajhoj"
              >Plantbazitaj surpana&jcirc;oj by Lode Van de Velde - Issuu</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Aufstrich</trd>,
    <trd>Belag <klr>(Brot)</klr></trd>
  </trdgrp>
  <trd lng="hu">keny&eacute;rreval&oacute;</trd>
  <trd lng="nl">beleg</trd>
</drv>

<drv mrk="pan.sxmir0o">
  <kap>&scirc;mir<tld/>o</kap>
  <snc mrk="pan.sxmir0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="P"/>peco &scirc;mirita per butero, konfita&jcirc;o kc.
    </dif>
    <ref tip="vid" cel="sandvicx.0o">sandvi&ccirc;o</ref>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_t;&c_e;&c_r;&c_b;&c_r;&c_o;&c_d;</trd>
  <trdgrp lng="de">
    <trd>Butterbrot</trd>,
    <trd>Stulle</trd>
  </trdgrp>
  <trd lng="fr">tartine</trd>
  <trd lng="hu">megkent <ind>keny&eacute;r</ind></trd>
  <trd lng="nl">boterham</trd>
  <trd lng="ru">&c_b;&c_u;&c_t;&c_je;&c_r;&c_b;&c_r;&c_o;&c_d;</trd>
</drv>

<drv mrk="pan.viv0o">
  <kap>viv<tld/>o, <var><kap>vivo<tld/>o</kap></var></kap>
  <snc mrk="pan.viv0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Vivrimedo, <ref tip="vid" cel="viv.por0ajxo.KOMUNE">porviva&jcirc;o</ref>:
      <ekz>
        la vivo<tld/>o, mia kara, estas por ili la grava demando
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        por akiri sian viv<tld/>on, la avo liveras lakton al diversaj klientoj
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.be/2005/008477.php"
              >Malnovstila rakonto porinfana</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_l;&c_je;&c_b; &c_n;&c_a;&c_d;&c_z;&c_jo;&c_n;&c_n;&c_y;</trd>,
   <trd>&c_h;&c_l;&c_je;&c_b; &c_sh;&c_t;&c_o;&c_d;&c_z;&c_jo;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">Broterwerb</trd>
  <trdgrp lng="fr">
    <trd>pain quotidien</trd>,
    <trd>subsistance</trd>
  </trdgrp>
  <trd lng="he">&mem;&het;&yod;&yod;&he;</trd>
  <trdgrp lng="hu">
    <trd>betev&odblac; falat</trd>,
    <trd>mindennapi <ind>keny&eacute;r</ind></trd>
  </trdgrp>
  <trd lng="nl">levensonderhoud</trd>
  <trd lng="os">&c_a;&c_h;&c_s;&c_d;&c_zh;&c_i;&c_a;&c_g; &c_d;&c_z;&c_u;&c_l;</trd>
  <trdgrp lng="pt">
    <trd>p&atilde;o de cada dia</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_l;&c_je;&c_b; &c_n;&c_a;&c_s;&c_u;&c_shch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">levebr&ouml;d</trd>
</drv>

<drv mrk="pan.voj0o">
  <kap>voj<tld/>o</kap>
  <snc mrk="pan.voj0o.KOMUNE">
    <dif>
      <tld lit="P"/>o a&ubreve; alia baza man&gcirc;a&jcirc;o facile konservebla,
      kiun oni kunprenas por vojado:
      <ekz>
        tiu voj<tld/>o de la elfoj havis potencon, kiu pliforti&gcirc;is, 
        kiam voja&gcirc;antoj dependis sole de &gcirc;i sen miksi&gcirc;o
        <fnt>
          <aut>J. R. R. Tolkien, trad. W. Auld</aut>,
          <vrk><url ref="http://esperanto-ondo.ru/Tolkien/Tolkien6.htm">
          La reveno de la Re&gcirc;o</url></vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="provia.0o">provianto</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Wegzehrung</trd>,
    <trd>Proviant</trd>
  </trdgrp>
  <trd lng="fr">pain de voyage</trd>
</drv>
</art>
