<drv mrk="pan.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pan.0isto.KOMUNE">
    <dif>
      Homo, kiu faras kaj vendas <tld/>on:
      <ekz>
        por ili la <tld/>isto kuiras kukojn ja misterajn
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        malfacilis al lokaj <tld/>istoj kaj kukistoj akiri buteron el eksterlandaj
        merkatoj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2008/009534.php">Ja-pan-io kaj butero</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_l;&c_je;&c_b;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_a;&c_ch;&c_n;&c_ib;&c_k;</trd>
