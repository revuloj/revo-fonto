<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
  <rad>pan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pan.0o.mangxo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="mangx.0ajxo" lst="voko:man&gcirc;a&jcirc;oj">Man&gcirc;a&jcirc;o</ref>
       farita el knedita fermentinta kaj bakita faruno:
      <ekz>
        seka peco da <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o bu&scirc;on ne ser&ccirc;as
        <klr>(oni devas peni por ricevi la dezirata&jcirc;on)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kie <tld/>o estas, musoj ne mankas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        blank<tld/>eto<fnt>Z</fnt> <klr>(bulko)</klr>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="pan.0o.rimedo">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="pan.viv0o.FIG">vivpano</ref>
      <ekz>
        espero <tld/>on ne donas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>o al li mankas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kies <tld/>on mi man&gcirc;as, ties kanton mi kantas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne karesu per mano, karesu per <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        esti alkatenita al <tld/>dona arbo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>dona okupo.
      </ekz>
  </snc>
  <trd lng="be">&c_h;&c_l;&c_je;&c_b;</trd>
  <trd lng="br">bara</trd>
  <trd lng="ca">pa</trd>
  <trdgrp lng="cs">
    <trd>chl&eacute;b</trd>,
    <trd>chleba</trd>
  </trdgrp>
  <trd lng="de">Brot</trd>
  <trd lng="en">bread</trd>
  <trd lng="es">pan</trd>
  <trd lng="fr">pain</trd>
  <trd lng="hu">keny&eacute;r</trd>
  <trd lng="id">roti</trd>
  <trd lng="ja">&#12497;&#12531; [<ind>&#12401;&#12435;</ind>]</trd>
  <trd lng="ku">nan</trd>
  <trd lng="nl">brood</trd>
  <trd lng="pl">chleb</trd>
  <trd lng="pt">p&atilde;o</trd>
  <trd lng="ru">&c_h;&c_l;&c_je;&c_b;</trd>
  <trd lng="sl">kruh</trd>
  <trd lng="sk">kruh</trd>
  <trd lng="sk">chlieb</trd>
  <trd lng="sv">br&ouml;d</trd>
  <trd lng="zh">&#38754;&#21253; <ind>[mi&agrave;nb&#257;o]</ind></trd>
</drv>

<drv mrk="pan.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="pan.0ero.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Tre malgranda rompopeco da <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_o;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_y;&c_h;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">engruna</trd>
  <trdgrp lng="cs">
    <trd>drobek chleba</trd>,
    <trd>sk&yacute;va chleba</trd>
  </trdgrp>
  <trd lng="es">miga <klr>(de pan)</klr></trd>
  <trd lng="fr">miette de pain</trd>
  <trd lng="hu">keny&eacute;rmorzsa</trd>
  <trd lng="id"><ind>remah</ind>-remah (roti)</trd>
  <trd lng="ja">&#12497;&#12531;&#23633;
[<ind>&#12401;&#12435;&#12367;&#12378;</ind>]</trd>
  <trd lng="nl">kruimel</trd>
  <trd lng="ru">&c_h;&c_l;&c_je;&c_b;&c_n;&c_a;&c_ja;
  &c_k;&c_r;&c_o;&c_sh;&c_k;&c_a;</trd>
  <trd lng="sk">omrvinka</trd>
  <trd lng="sv">br&ouml;dsmula</trd>
</drv>

<drv mrk="pan.0erumi">
  <kap><tld/>erumi, <var><kap><tld/>umi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pan.0erumi.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Kovri per <tld/>eroj anta&ubreve; fritado:
      <ekz>
        <tld/>erumi fi&scirc;on..
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="graten.0i">grateni</ref>
  <trd lng="ca">arrebossar</trd>
  <trdgrp lng="es">
    <trd>empanizar</trd>,
    <trd>empanar</trd>
  </trdgrp>
  <trd lng="fr">paner</trd>
  <trdgrp lng="hu">
    <trd>pan&iacute;roz</trd>,
    <trd>bepan&iacute;roz</trd>
  </trdgrp>
  <trd lng="nl">paneren</trd>
  <trd lng="ru">&c_p;&c_a;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">oba&lcaron;ova&tcaron; do chlebovej str&uacute;hanky</trd>
  <trd lng="cs">obalovat v chlebov&eacute; strouhance</trd>
</drv>

<drv mrk="pan.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pan.0isto.KOMUNE">
    <dif>
      Homo, kiu faras kaj vendas <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_l;&c_je;&c_b;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_a;&c_ch;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>flequer</trd>,
    <trd>forner</trd>
  </trdgrp>
  <trd lng="cs">peka&rcaron; a prodava&ccaron; chleba</trd>
  <trd lng="es">panadero</trd>
  <trd lng="fr">boulanger</trd>
  <trd lng="hu">p&eacute;k</trd>
  <trd lng="nl">bakker</trd>
  <trd lng="ru">&c_b;&c_u;&c_l;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">pek&aacute;r a predava&ccaron; chleba</trd>
</drv>

<drv mrk="pan.maiz0o">
  <kap>maiz<tld/>o</kap>
  <snc mrk="pan.maiz0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="P"/>o el maizo.
    </dif>
  </snc>
  <trd lng="cs">kuku&rcaron;i&ccaron;n&yacute; chl&eacute;b</trd>
  <trd lng="es">pan de ma&iacute;z</trd>
  <trd lng="fr">pain de ma&iuml;s</trd>
  <trd lng="sk">kukuri&ccaron;n&yacute; chlieb</trd>
</drv>

<drv mrk="pan.ringo0o">
  <kap>ringo<tld/>o</kap>
  <snc mrk="pan.ringo0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="P"/>o muldita la&ubreve; ringa formo:
      <ekz>
        Simeoni pretigis la vesperman&gcirc;on, alportis sur la 
        tablon sep ringo<tld/>ojn
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>rosco</trd>,
    <trd>rosca</trd>
  </trdgrp>
  <trd lng="fr">couronne<klr> (pain)</klr></trd>
</drv>

<drv mrk="pan.sur0ajxo">
  <kap>sur<tld/>a&jcirc;o</kap>
  <snc mrk="pan.sur0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Io, kion oni povas meti sur <tld/>on por kunman&gcirc;i, ekz.
      froma&gcirc;o a&ubreve; kolbastran&ccirc;a&jcirc;o.
    </dif>
  </snc>
  <trd lng="hu">keny&eacute;rreval&oacute;</trd>
  <trd lng="nl">beleg</trd>
</drv>

<drv mrk="pan.sxmir0o">
  <kap>&scirc;mir<tld/>o</kap>
  <snc mrk="pan.sxmir0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="P"/>peco &scirc;mirita per butero, konfita&jcirc;o kc.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_t;&c_e;&c_r;&c_b;&c_r;&c_o;&c_d;</trd>
  <trd lng="de">Butterbrot</trd>
  <trd lng="fr">tartine</trd>
  <trd lng="hu">megkent <ind>keny&eacute;r</ind></trd>
  <trd lng="nl">boterham</trd>
  <trd lng="ru">&c_b;&c_u;&c_t;&c_je;&c_r;&c_b;&c_r;&c_o;&c_d;</trd>
</drv>

<drv mrk="pan.viv0o">
  <kap>viv<tld/>o, <var><kap>vivo<tld/>o</kap></var></kap>
  <snc mrk="pan.viv0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Vivrimedo, <ref tip="vid"
      cel="viv.por0ajxo.KOMUNE">porviva&jcirc;o</ref>:
      <ekz>
        gajni viv<tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_l;&c_je;&c_b; &c_n;&c_a;&c_d;&c_z;&c_jo;&c_n;&c_n;&c_y;</trd>,
   <trd>&c_h;&c_l;&c_je;&c_b; &c_sh;&c_t;&c_o;&c_d;&c_z;&c_jo;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pain quotidien</trd>,
    <trd>subsistance</trd>
  </trdgrp>
  <trd lng="he">&mem;&het;&yod;&yod;&he;</trd>
  <trdgrp lng="hu">
    <trd>betev&odblac; falat</trd>,
    <trd>mindennapi <ind>keny&eacute;r</ind></trd>
  </trdgrp>
  <trd lng="nl">levensonderhoud</trd>
  <trd lng="os">&c_a;&c_h;&c_s;&c_d;&c_zh;&c_i;&c_a;&c_g; &c_d;&c_z;&c_u;&c_l;</trd>
  <trdgrp lng="pt">
    <trd>p&atilde;o de cada dia</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_l;&c_je;&c_b; &c_n;&c_a;&c_s;&c_u;&c_shch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">levebr&ouml;d</trd>
</drv>

<drv mrk="pan.voj0o">
  <kap>voj<tld/>o</kap>
  <snc mrk="pan.voj0o.KOMUNE">
    <dif>
      <tld lit="P"/>o a&ubreve; alia baza man&gcirc;a&jcirc;o facile konservebla,
      kiun oni kunprenas por vojado:
      <ekz>
        tiu voj<tld/>o de la elfoj havis potencon, kiu pliforti&gcirc;is, 
        kiam voja&gcirc;antoj dependis sole de &gcirc;i sen miksi&gcirc;o
        <fnt>
          <aut>J. R. R. Tolkien, trad. W. Auld</aut>,
          <vrk><url ref="http://esperanto-ondo.ru/Tolkien/Tolkien6.htm">
          La reveno de la Re&gcirc;o</url></vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pain de voyage</trd>
</drv>
</art>
