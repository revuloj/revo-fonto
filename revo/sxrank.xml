<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>&scirc;rank</rad>/o
    </kap>

    <drv mrk="sxrank.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
	<dif>
	  Meblo en formo de alta kesto, fermebla per unu a&ubreve; pluraj
	  pordetoj, provizita je bretoj a&ubreve; hokoj por konservi vestojn,
	  tola&jcirc;ojn, man&gcirc;a&jcirc;ojn, teleraron a&ubreve; alispecajn objektojn.
	</dif>
      </snc>
      <trd lng="be">&c_sh;&c_a;&c_f;&c_a;</trd>
      <trd lng="ca">armari</trd>
      <trd lng="cs">sk&rcaron;&iacute;&ncaron;</trd>
      <trd lng="de">Schrank</trd>
      <trd lng="el">&epsilon;&rho;&my;&alfa_ton;&rho;&jota;</trd>
      <trd lng="en">cupboard</trd>
      <trd lng="es">armario</trd>
      <trd lng="he">&alef;&resh;&vav;&fnun;</trd>
      <trd lng="hu">szekr&eacute;ny</trd>
      <trd lng="nl">kast</trd>
      <trd lng="pl">szafa</trd>
      <trd lng="pt">arm&aacute;rio</trd>
      <trd lng="ru">&c_sh;&c_k;&c_a;&c_f;</trd>
      <trd lng="tr">dolap</trd>
    </drv>

    <drv mrk="sxrank.libro0o">
      <kap>libro<tld/>o</kap>
      <snc>
	<dif>
	  Brethava <ref tip="super" cel="sxrank.0o"><tld/>o</ref>,
	  plej ofte kun vitraj pordetoj, destinita por libroj:
	  <ekz>
	    la granda libro<tld/>o, en kiu staris multe da sciencaj
	    kaj interesaj libroj
	    <fnt><bib>Fab4</bib>, <lok>Abocolibro</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="ru">&c_k;&c_n;&c_i;&c_zh;&c_n;&c_y;&c_j; &c_sh;&c_k;&c_a;&c_f;</trd>
    </drv>

  </art>
