<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>karmin</rad>/o
</kap>
<drv mrk="karmin.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Rozru&gcirc;a kolorilo, ekstraktata el la ko&ccirc;enilo: (f)
      <ekz>
        <tld/>aj lipoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Karminrot</trd>
  <trdgrp lng="en">
    <trd>carmine</trd>,
    <trd>crimson</trd>
  </trdgrp>
  <trd lng="nl">karmijn</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_m;&c_i;&c_n;</trd>
</drv>
</art>
