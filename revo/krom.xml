</subart>

<subart>
  <dif>
    Prefikso samsignifa:
  </dif>
  
  <drv mrk="krom.0folio">
    <kap><tld/>folio</kap>
    <snc>
      <dif>
        Folio aldonita al libro ktp.:
        <ekz>
          <tld/>folio de &leftquot;Juna Esperantisto&rightquot;
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=27131">Austin
            Richardson</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="hu">p&oacute;tlap</trd>
      <trd lng="nl">inlegvel</trd>
    </snc>
    <trd lng="cs">p&rcaron;&iacute;loha</trd>
    <trdgrp lng="es">
      <trd>hoja suplementaria</trd>,
      <trd>anexo</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>p&oacute;t-</trd>,
      <trd>k&uuml;l&ouml;n-</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>kartka dodatkowa</trd>,
      <trd>za&lstroke;&aogonek;cznik</trd>
    </trdgrp>   
    <trdgrp lng="ro">
      <trd>card suplimentar</trd>,    
      <trd>anex&abreve;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>dodatkov&yacute; list</trd>,
      <trd>pr&iacute;loha</trd>
    </trdgrp>
  </drv>
  
  <drv mrk="krom.0persono">
    <kap><tld/>persono</kap>
    <snc>
      <dif>
        Persono apud la precipa rilate iun aferon:
        <ekz>
          posedanto anka&ubreve; povas prokuracii al <tld/>persono, kiun li
          tiam mandatigas por la konto
          <fnt>
            <bib>Viki</bib>
            <lok><url
            ref="https://eo.wikipedia.org/?curid=100488">Spezokonto</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="hu">k&iacute;v&uuml;l&aacute;ll&oacute;</trd>
      <trd lng="nl">buitenstaander</trd>
      <trd lng="pl">osoba postronna</trd>
      <trd lng="ro">str&abreve;in</trd>
      <trd lng="ru">&c_p;&c_o;&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_n;&c_i;&c_j;</trd>
    </snc>
    <trdgrp lng="cs">
      <trd>nez&uacute;&ccaron;astn&ecaron;n&aacute; osoba</trd>,
      <trd>vedlej&scaron;&iacute; osoba</trd>,
      <trd>vedlej&scaron;&iacute; &uacute;&ccaron;astn&iacute;k</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>tercera persona</trd>,
      <trd><klr>(un) </klr>tercero</trd>
    </trdgrp>
    <trd lng="sk">nepriamy &uacute;&ccaron;astn&iacute;k</trd>
  </drv>
  
  <drv mrk="krom.0segxo">
    <kap><tld/>se&gcirc;o</kap>
    <snc>
      <dif>
        Se&gcirc;o aldonita al ia regula, kutima nombro, en teatro, 
        a&ubreve;lo kc.
      </dif>
    </snc>
    <trd lng="es">asiento a&ntilde;adido</trd>
    <trd lng="hu">p&oacute;tsz&eacute;k</trd>
    <trd lng="nl">bijzetstoel</trd>
    <trd lng="pl">dostawka</trd>
    <trd lng="ro">pat</trd>
    <trd lng="ru">&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_s;&c_t;&c_u;&c_l;<klr> (&c_n;&c_a;&c_p;&c_r;.
        &c_v; &c_t;&c_je;&c_a;&c_t;&c_r;&c_je;)</klr></trd>
  </drv>
  
  <drv mrk="krom.0edzino">
    <kap>
      <tld/>edzino
    </kap>
    <snc>
      <dif>
        Plia edzino apud la regula unua:
        <ekz>
          mian <tld/>edzinon ili turmentis tiel, ke &scirc;i mortis
          <fnt>
            <bib>MT</bib>
            <lok>Ju&gcirc;istoj 20:5</lok>
          </fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="de">Nebenfrau</trd>
    <trdgrp lng="es">
      <trd>amante</trd>,
      <trd>querida</trd>,
      <trd>entretenida</trd>
    </trdgrp>
    <trd lng="pl">&zdot;ona  <klr>(poligamia)</klr></trd>     
    <trd lng="ro">so&#539;ia  <klr>(poligamia)</klr></trd>
  </drv>
  
  
  <drv mrk="krom.0virino">
    <kap><tld/>virino</kap>
    <snc>
      <dif>
        Nele&gcirc;a kunvivantino, kiun havas viro, krom sia le&gcirc;a 
        edzino; <ref tip="sin" cel="konkub.0o">konkubino</ref>:
        <ekz>
          lia edzino fari&gcirc;is <tld/>virino de iu bankiero
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          li havis dek ok edzinojn kaj sesdek <tld/>virinojn, kaj li
          naskigis dudek ok filojn kaj sesdek filinojn
          <fnt>
            <bib>MT</bib>
            <lok>II. Kkroniko 11:21</lok>
          </fnt>.
        </ekz>
      </dif>         
    </snc>
    <trdgrp lng="cs">
      <trd>dru&zcaron;ka</trd>,
      <trd>konkub&iacute;na</trd>,
      <trd>milenka</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>concubina</trd>,
      <trd>amante</trd>
    </trdgrp>
    <trd lng="hu">&aacute;gyas</trd>
    <trd lng="nl">bijvrouw</trd>
    <trdgrp lng="pl">
      <trd>konkubina</trd>,
      <trd>kochanka</trd>,
      <trd>utrzymanka</trd>,
      <trd>na&lstroke;o&zdot;nica</trd>
    </trdgrp>
    <trdgrp lng="ro">
      <trd>concubin&abreve;</trd>,
      <trd>iubit&abreve;</trd>,
      <trd>metres&abreve;</trd>,
      <trd>iubit</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_l;&c_ju;&c_b;&c_o;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>,
      <trd>&c_d;&c_r;&c_u;&c_g;&c_a;&c_ja; &c_zh;&c_je;&c_n;&c_shch;&c_i;&c_n;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>dru&zcaron;ka</trd>,
      <trd>konkub&iacute;na</trd>,
      <trd>milenka</trd>
    </trdgrp>
  </drv>
