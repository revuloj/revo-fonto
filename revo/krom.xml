<drv mrk="krom.0ke">
  <kap><tld/> ke,
  <var><kap><tld/> se</kap></var>,
  <var><kap><tld/> tio ke</kap></var></kap>
  <gra><vspec>konjunkcio</vspec></gra>
  <snc mrk="krom.0ke.ARG">
    <dif>
      Esceptante, ne konsiderante tion ke..., tamen aldone...:
      <ekz>
        mi nenion pli postulus, <tld/> ke oni montru al mi
        sindonecon kaj estimon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mortis pro koro&ccirc;eso; tio signifas nenion, <tld/> tio, ke li vivis kaj nun ne
        vivas plu
        <fnt>
          <aut>J. Valano</aut>
          <vrk>&Ccirc;u vi kuiras &ccirc;ine?</vrk>
          <lok>1976</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> malofte malfermis sian bu&scirc;on <tld/> se oni rekte alparolis &scirc;in
        <fnt>
          <aut>A. L&ouml;wenstein</aut>
          <vrk>La &scirc;tona urbo</vrk>
          <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>excepto que</trd>,
    <trd>salvo que</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&agrave; ceci pr&egrave;s que</trd>,
    <trd>sauf que</trd>
  </trdgrp>
  <trd lng="nl">behalve dat</trd>
  <trdgrp lng="pl">
    <trd>dodatkowo</trd>,
    <trd>pr&oacute;cz tego</trd>,
    <trd>opr&oacute;cz tego &zdot;e</trd>,
    <trd>poza tym</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pe l&acirc;ng&abreve;</trd>,
    <trd>cu excep&#x21B;ia faptului c&abreve;</trd>,
    <trd>&icirc;n plus</trd>
  </trdgrp>    
</drv>
