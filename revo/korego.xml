<art mrk="$Id: korego.xml,v 1.20 2023/12/03 09:58:58 revo Exp $">
<kap>
<rad>koregon</rad>/o
</kap>
<drv mrk="korego.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro<sncref/></ref> 
      el familio <ref tip="malprt" cel="korego1.0oj">koregonedoj</ref>
      (<trd lng="la">Coregonus</trd>)
      de man&gcirc;eblaj salmoformaj <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;oj</ref>, vivantaj
      en profundaj lagoj sub 50&nbsp;m, sed anka&ubreve; en Norda kaj Balta Maroj:
      <ekz>
        la kvartaloj havas nomojn de fi&scirc;oj: <tld/>o, salmo, ezoko
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2014/011095.php">Tammisaari, urbeto &ccirc;e Balta Maro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        aliaj fi&scirc;oj en Bajkalo estas timalo, <tld/>o, sturgo, lojto, salmotruto, ezoko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24216">Bajkalo</url></lok>
        </fnt>.
      </ekz>     
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_l;&c_ja;&c_v;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Renke</trd>,
    <trd>Mar&auml;ne</trd>,
    <trd>Felchen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>whitefish</trd>,
    <trd>cisco</trd>
  </trdgrp>
  <trd lng="fr">cor&eacute;gone</trd>
  <trd lng="hu">mar&eacute;na</trd>
  <trd lng="ja">&#x30B3;&#x30AF;&#x30C1;&#x30DE;&#x30B9;</trd>
  <trd lng="nl">houting</trd>
  <trdgrp lng="pl">
    <trd>sieja</trd>,
    <trd>sielawa</trd>
  </trdgrp>   
  <trd lng="pt">esp&eacute;cie de <ind>salm&atilde;o</ind></trd>
  <trd lng="ro">coregonul</trd>
  <trd lng="ru">&c_s;&c_i;&c_g;</trd>
  <trd lng="zh">&#x767D;&#x9B5A; <pr>b&#xE1;iy&#xFA;</pr></trd>
</drv>

</art>
