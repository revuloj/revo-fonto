<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: etn.xml,v 1.23 2023/11/07 20:34:12 revo Exp $">
<kap>
  <ofc>9</ofc><rad>etn</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="etn.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Historie esti&gcirc;inta homgrupo, kies vivmanieron karakterizas
      komunaj trajtoj kulturaj, civilizaj a&ubreve; religiaj, kaj
      havanta
      konscion pri tiu komuneco:
      <ekz>
        la celo de la Internacia Lingvo Esperanto ekde la plej fruaj
        tempoj
        estas liveri al la mondo egalecan formon de komunikado, kiu
        konservas
        la <tld/>ajn lingvojn por ties utiligo interne de la diversaj
        regionoj,
        nacioj kaj <tld/>oj
        <fnt>
        <vrk>Rezolucio de la 84a Universala Kongreso de Esperanto</vrk>,
        <bib>LOdE</bib>, <lok>1999, n-ro 58-59.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="popol.0o.etno">popolo</ref>;
      <ref cel="gent.0o">gento</ref>,
      <ref cel="klan.0o">klano</ref>,
      <ref cel="naci.0o">nacio</ref>,
      <ref cel="trib.0o">tribo</ref>
    </refgrp>
    <rim>
      Komuna lingvo, &scirc;tato a&ubreve; alia socipolitika strukturo
      estas
      ofta, sed ne nepra trajto de <tld/>o. Kontraste al <em>gento</em>,
      la nocio
      <tld/>o ne insistas pri komunaj prauloj, fikciaj a&ubreve; realaj.
    </rim>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_n;&c_a;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Ethnie</trd>,
    <trd>ethnische Gruppe</trd>,
    <trd>Volk</trd>,
    <trd>Stamm</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ethnos</trd>,
    <trd>ethnic group</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ethnie</trd>,
    <trd>peuple</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>etnosz</trd>,
    <trd>etnikum</trd>,
    <trd>n&eacute;pcsoport</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6C11;&#x65CF;  <pr>&#x307F;&#x3093;&#x305E;&#x304F;</pr></trd>,
    <trd>&#x30A8;&#x30B9;&#x30CB;&#x30C3;&#x30AF;&#x30B0;&#x30EB;&#x30FC;&#x30D7;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>grupa <ind>etniczna</ind></trd>,
    <trd>lud</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>etnia</trd>,
    <trd>gente</trd>,
    <trd>povo</trd>,
    <trd>na&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_t;&c_n;&c_o;&c_s;</trd>
  <trd lng="sv">etnisk grupp</trd>
  <trdgrp lng="uk">
    <trd>&c_je;&c_t;&#x43D;&#x43E;&c_s;</trd>,
    <trd>&#x43F;&#x43B;&c_je;&#x43C;&#x2019;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x65CF;&#x7FA4; <pr>z&#xFA;q&#xFA;n</pr></trd>,
    <trd>&#x90E8;&#x843D; <pr>b&#xF9;lu&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="etn.sub0o">
  <kap>sub<tld/>o</kap>
  <snc>
  <uzo tip="fak">SCI</uzo>
    <dif>
      Etno rigardata kiel parto de alia, pli granda etno.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_b;&c_e;&c_t;&c_n;&c_a;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Subethnie</trd>,
    <trd>ethnische Untergruppe</trd>,
    <trd>Teilvolk</trd>,
    <trd>Stamm</trd>
  </trdgrp>
  <trd lng="en">ethnic subgroup</trd>
  <trd lng="fr">sous-ethnie</trd>
  <trd lng="hu">szubetnikum</trd>
  <trd lng="ru">&c_s;&c_u;&c_b;&c_e;&c_t;&c_n;&c_o;&c_s;</trd>
</drv>

<drv mrk="etn.super0o">
  <kap>super<tld/>o</kap>
  <snc>
  <uzo tip="fak">SCI</uzo>
    <dif>
      Etno entenanta aliajn, malpli grandajn etnojn.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_p;&c_e;&c_r;&c_e;&c_t;&c_n;&c_a;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Superethnie</trd>,
    <trd>ethnische Obergruppe</trd>,
    <trd>Stammesverband</trd>
  </trdgrp>
  <trd lng="en">ethnic supergroup</trd>
  <trd lng="hu">szuperetnikum</trd>
  <trd lng="ru">&c_v;&c_k;&c_l;&c_ju;&c_ch;&c_a;&c_ju;&c_shch;&c_i;&c_j; <ind>&c_e;&c_t;&c_n;&c_o;&c_s;</ind></trd>
</drv>

<drv mrk="etn.0ogenezo">
  <kap><tld/>ogenezo</kap>
  <snc>
  <uzo tip="fak">SCI</uzo>
    <dif>
      Esti&gcirc;o, genezo de <tld/>o, unu el la studobjektoj de
      <ref tip="malprt" cel="etnolo1.0o">etnologio</ref>:
      <ekz>
        <tld/>ogenezo de la slavoj, de la rusoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_n;&c_a;&c_g;&c_je;&c_n;&c_e;&c_z;</trd>
  <trd lng="cs">etnogeneze</trd>
  <trdgrp lng="de">
    <trd>Ethnogenese</trd>,
    <trd>Volksgeschichte</trd>,
    <trd>Stammesentwicklung</trd>
  </trdgrp>
  <trd lng="en">ethnogenesis</trd>
  <trd lng="fr">ethnogen&egrave;se</trd>
  <trd lng="hu">etnogenezis</trd>
  <trd lng="pl">etnogeneza</trd>
  <trd lng="ru">&c_e;&c_t;&c_n;&c_o;&c_g;&c_je;&c_n;&c_je;&c_z;</trd>
  <trd lng="sk">etnogen&eacute;za</trd>
  <trdgrp lng="uk">
    <trd>&c_je;&c_t;&#x43D;&#x43E;&c_g;&c_je;&#x43D;&c_je;&c_z;</trd>,
    <trd>&#x43F;&#x43E;&c_h;&#x43E;&c_d;&c_zh;&c_je;&#x43D;&#x43D;&#x44F; &#x43D;&c_a;&c_r;&#x43E;&c_d;&c_ib;&c_v;</trd>
  </trdgrp>
  <trd lng="zh">&#x79CD;&#x7CFB;&#x53D1;&#x751F; <pr>zh&#x1D2;ngx&#xEC;f&#x101;sh&#x113;ng</pr></trd>
</drv>

<drv mrk="etn.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al etno.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">ethnisch</trd>
  <trd lng="en">ethnic</trd>
  <trd lng="fr">ethnique</trd>
  <trd lng="hu">etnikai</trd>
  <trd lng="ja">&#x6C11;&#x65CF;&#x306E;  <pr>&#x307F;&#x3093;&#x305E;&#x304F;&#x306E;</pr></trd>
  <trd lng="pl">etniczny</trd>
  <trd lng="uk">&c_je;&c_t;&#x43D;&c_ib;&c_ch;&#x43D;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x4EBA;&#x79CD; <pr>r&#xE9;nzh&#x1D2;ng</pr></trd>,
    <trd>&#x6C11;&#x65CF; <pr>m&#xED;nz&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>
