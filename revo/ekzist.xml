<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>ekzist</rad>/i
</kap>
<drv mrk="ekzist.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Efektive k nune esti:
      <ekz>
        mi pensas, do mi <tld/>as;
      </ekz>
      <ekz>
        homoj ne &ccirc;iam <tld/>is;
      </ekz>
      <ekz>
        socio ne povus <tld/>i sen le&gcirc;oj;
      </ekz>
      <ekz>
        tiu monumento ne <tld/>as plu;
      </ekz>
      <ekz>ne <tld/>as honoro sen laboro<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        nia societo ne vivas, &gcirc;i nur <tld/>as.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="veget.0i">vegeti</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">existovat</trd>
  <trdgrp lng="de">
    <trd>existieren</trd>,
    <trd>geben <klr tip="ind">(es gibt)</klr></trd>,
    <trd>sein</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>exister</trd>,
    <trd>avoir<klr tip="amb"> (il y a)</klr></trd></trdgrp> 
  <trd lng="en">exist</trd>
  <trd lng="ru">&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="ekzist.0ado">
  <kap><tld/>ado</kap>
  <snc num="1">
    <dif>
      Stato de tio, kio <tld/>as:
      <ekz>
        ni ricevis la <tld/>adon de niaj
        gepatroj;
      </ekz>
      <ekz>
        nei la <tld/>adon de komploto.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Vivo de l' homo:
      <ekz>
        esti sen rimedoj por <tld/>ado;
      </ekz>
      <ekz>
        malglora <tld/>ado;
      </ekz>
      <ekz>
        akiri <tld/>adon por si k por la familio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        treni sian senesperan <tld/>adon<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Dasein</trd>,
    <trd>Existenz</trd>
  </trdgrp>
  <trd lng="fr">existence</trd>
  <trd lng="ru">&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>
<drv mrk="ekzist.0adismo">
  <kap><tld/>adismo</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Doktrino, la&ubreve; kiu la homo, kies ekzisto estas
      komence preska&ubreve; nur metafizika, kreas sin mem per la elekto
      inter la diversaj eblaj agoj k per la plenumado de tiuj agoj:
      <ekz>por tiu skolo, ekzisto anta&ubreve;as esencon</ekz>.
    </dif>
  </snc>
  <trd lng="fr">existentialisme</trd>
  <trd lng="ru">&c_e;&c_k;&c_z;&c_i;&c_s;&c_t;&c_je;&c_n;&c_c;&c_i;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>
<drv mrk="ekzist.0ajxo">
  <kap><tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <snc num="1">
    <dif>
      Esta&jcirc;o:
      <ekz>
        &ccirc;iu viva <tld/>a&jcirc;o
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Genezo 9, 16</lok>)</fnt>.
      </ekz> 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Estulo, vivulo.  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Wesen</trd>,
    <trd>Gesch&ouml;pf</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cr&eacute;ature</trd>,
    <trd>&ecirc;tre<klr tip="ind"> (cr&eacute;ature)</klr></trd></trdgrp> 
  <trd lng="ru">&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>
</art>
