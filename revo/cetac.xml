<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cetac.xml,v 1.28 2025/03/26 16:35:49 revo Exp $">
<kap>
<rad>cetac</rad>/o
</kap>
<drv mrk="cetac.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Iu el <ref tip="malprt" cel="cetac.0oj"><tld/>oj</ref>:
      <ekz>
        la vorto <tld/>o devenis el la helena mitologia rolulo Keto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16373">Cetacoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        belugo estas <tld/>o, kiu lo&gcirc;as en la Arkto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=42305">Belugo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_n;&c_ib;&c_k; &c_a;&c_t;&c_r;&c_a;&c_d;&c_a; <ind>&c_k;&c_ib;&c_t;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_h;</ind></trd>
  <trd lng="en">cetacean</trd>
  <trd lng="es">cet&aacute;ceo</trd>
  <trd lng="fr">c&eacute;tac&eacute;</trd>
  <trd lng="pl">bia&lstroke;ucha</trd>
</drv>
<drv mrk="cetac.0oj">
  <kap><tld/>oj</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ordo el la klaso <ref tip="malprt" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Cetacea</trd>), vivantaj en la maro.
    </dif>
    <refgrp tip="prt">
      <ref cel="balen.0o.KOMUNE">baleno</ref>,
      <ref cel="delfen.0o.KOMUNE">delfeno</ref>,
      <ref cel="narval.0o">narvalo</ref>
    </refgrp>
  <trd lng="be">&c_k;&c_ib;&c_t;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;
  <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">cetacis</trd>
  <trd lng="de">Wale</trd>
  <trd lng="el">&kappa;&eta;&tau;&omikron;&epsilon;&jota;&delta;&eta_ton;</trd>
  <trd lng="es">cet&aacute;ceos</trd>
  <trd lng="en">cetaceans</trd>
  <trd lng="fr">c&eacute;tac&eacute;s</trd>
  <trd lng="hu">cetek <klr tip="amb"> (rend)</klr></trd>
  <trd lng="it">cetacei</trd>
  <trdgrp lng="nl">
    <trd>walwisachtigen</trd>,
    <trd>cetacee&euml;n</trd>
  </trdgrp>
  <trd lng="pl">walenie</trd>
  <trd lng="pt">cet&aacute;ceos</trd>
  <trd lng="ru">&c_k;&c_i;&c_t;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
  </snc>
  <trd lng="cs">kytovci</trd>
  <trd lng="ja">&#12463;&#12472;&#12521;&#39006;  <pr>&#12463;&#12472;&#12521;&#12427;&#12356;</pr></trd>
  <trd lng="sk">morsk&eacute; cicavce</trd>
  <trd lng="uk">&c_k;&c_i;&c_t;&c_o;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_ib;</trd>
  <trd lng="zh">&#39912;&#30446; <pr>j&imacron;ngm&ugrave;</pr></trd>
</drv>
</art>

