<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sekci.xml,v 1.23 2021/05/29 16:15:02 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>sekci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sekci.0o">
  <kap><tld/>o</kap>
  <snc mrk="sekci.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Aparta parto de institucio, societo, ordinare kun propra tasko
      a&ubreve; loko:
      <ekz>
        ili decidis proponi al &ccirc;iuj amikoj de interpopola justeco 
        unui&gcirc;i en apartan grupon, kiu inter la amikoj de internacia 
        lingvo prezentus apartan <tld/>on
        <fnt>
          <bib>Homaranismo</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i necesigas la kunlaboron de &ccirc;iuj <tld/>oj en la kompanio en 
        unu grandega projekto
        <fnt>
          <aut>M. Vanden Bempt</aut>,
          <vrk>Al nova komuna epoko</vrk>,
          <bib>Monato</bib>,
          <lok>1997:3, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fak.0o">fako</ref>
    </refgrp>
    <trd lng="es">departamento</trd>
    <trdgrp lng="hu">
      <trd>oszt&aacute;ly</trd>,
      <trd>r&eacute;szleg</trd>,
      <trd>tagozat</trd>
    </trdgrp>
  </snc>
  <snc mrk="sekci.0o.ajxa">
    <dif>
      <ref tip="super" cel="part.0o.KOMUNE">Parto</ref>
      de sinetenda tuta&jcirc;o, ekzemple de strato, verko,
      kiu prezentas aliajn ecojn, alian funkcion,
      ol la ceteraj:
      <ekz>
        la&ubreve;longe de kanal<tld/>o &ccirc;. sesoble pli longa ol la profundo
        de la akvo
        <fnt>
        <aut>OH</aut>
        <vrk>&Ccirc;u akvo fluas supren ie ajn en la mondo?</vrk>,
        <bib>Monato</bib>
        <lok>2001:3, p. 22a</lok>
        </fnt>;
      </ekz>
      <ekz>
        tion mi klarigos en la sekva <tld/>o <klr>[de la teksto]</klr>
        <fnt><aut>Klivo</aut>, <vrk>Kio
        estas Javo?</vrk>, <bib>Monato</bib>
        <lok>1998:5, p. 13a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="es">secci&oacute;n<klr> (parte)</klr></trd>
    <trdgrp lng="hu">
      <trd>szakasz</trd>,
      <trd>r&eacute;sz</trd>
    </trdgrp>
  </snc>
  <trd lng="af">artikel</trd>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_k;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_s;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_a;&c_d;&c_d;&c_z;&c_je;&c_l;</trd>,
    <trd>&c_a;&c_d;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_l;</trd>
  </trdgrp>
  <trd lng="bo">&#xF51;&#xF74;&#xF58;&#xF0B;&#xF56;&#xF74;</trd>
  <trdgrp lng="cs">
    <trd>odbor</trd>,
    <trd>odd&ecaron;len&iacute;</trd>,
    <trd>sekce</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Abteilung</trd>,
    <trd>Sektion</trd>
  </trdgrp>
  <trd lng="en">section</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_kh;&a_sh;</trd>,
    <trd>&a_q;&a_s;&a_m;&a_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>section<klr> (partie)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
      <trd>settore<klr> (parte, gruppo)</klr></trd>,
      <trd>sezione<klr> (parte, gruppo)</klr></trd>
    </trdgrp>
  <trd lng="ne">&#x916;&#x923;&#x94D;&#x921;</trd>
  <trd lng="nl">afdeling</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_o;&c_t;&c_d;&c_je;&c_l;</trd>,
    <trd>&c_o;&c_t;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odbor</trd>,
    <trd>oddelenie</trd>
  </trdgrp>
  <trd lng="sv">sektion</trd>
</drv>

<drv mrk="sekci.0estro">
  <kap><tld/>estro</kap>
  <snc mrk="sekci.0estro.POL">
    <dif>
      &Ccirc;efulo de <tld/>o:
      <ekz>
        <tld/>estro en asekura entrepreno
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>oborov&yacute; &scaron;&eacute;f</trd>,
    <trd>sek&ccaron;n&iacute; &scaron;&eacute;f</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>jefe de <ind>secci&oacute;n</ind></trd>,
    <trd>jefe de negociado</trd>
  </trdgrp>
  <trd lng="fa">&a_r;&a_y_hamza;&f_y;&a_s; &a_b;&a_kh;&a_sh;</trd>
  <trdgrp lng="fr">
    <trd>chef de groupe</trd>,
    <trd>chef de section</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>caposezione</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odborov&yacute; &scaron;&eacute;f</trd>,
    <trd>sek&ccaron;n&yacute; &scaron;&eacute;f</trd>
  </trdgrp>
</drv>

<drv mrk="sekci.sub0o">
  <kap>sub<tld/>o</kap>
  <snc mrk="sekci.sub0o.POL">
    <dif>
      Subparto de
      <ref tip="super" cel="sekci.0o.POL"><tld/>o<sncref/></ref>.
    </dif>
  </snc>
  <snc mrk="sekci.sub0o.ajxa">
    <dif>
      Subparto de
      <ref tip="super" cel="sekci.0o.ajxa"><tld/>o<sncref/></ref>:
      <ekz>
        mi revenos al tio pli specife en la sub<tld/>oj 4.2.2 kaj 4.2.11
        <fnt>
          <aut>W. Jansen</aut>
          <vrk>Radikoj kaj vortoj en esperanto</vrk>
          <lok>Esperantologio / Esperanto Studies, 2013:6, p.
          9a-43a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">subsecci&oacute;n</trd>
  <trd lng="fr">sous-section</trd>
</drv>
</art>
