      &Ccirc;efulo de <tld/>o:
      <ekz>
        <tld/>estro en asekura entrepreno
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k; &c_a;&c_d;&c_d;&c_z;&c_je;&c_l;&c_a;</trd>
