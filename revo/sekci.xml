<drv mrk="sekci.sub0o">
  <kap>sub<tld/>o</kap>
  <snc mrk="sekci.sub0o.POL">
    <dif>
      Subparto de
      <ref tip="super" cel="sekci.0o.POL"><tld/>o<sncref/></ref>.
    </dif>
  </snc>
  <snc mrk="sekci.sub0o.ajxa">
    <dif>
      Subparto de
      <ref tip="super" cel="sekci.0o.ajxa"><tld/>o<sncref/></ref>:
      <ekz>
        mi revenos al tio pli specife en la sub<tld/>oj 4.2.2 kaj 4.2.11
        <fnt>
          <aut>W. Jansen</aut>
          <vrk>Radikoj kaj vortoj en esperanto</vrk>
          <lok>Esperantologio / Esperanto Studies, 2013:6, p.
          9a-43a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_je;&c_l;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_s;&c_e;&c_k;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_s;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="es">subsecci&oacute;n</trd>
  <trd lng="fr">sous-section</trd>
  <trdgrp lng="pl">
