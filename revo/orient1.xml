<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: orient1.xml,v 1.21 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>orientalism</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="orient1.0o">
  <kap><tld/>o</kap>
  <snc mrk="orient1.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">BELE</uzo>
    <dif>
      <ref tip="super" cel="ekzot.0ismo.BELA">Ekzotisma</ref>
      uzo de aziaj temoj a&ubreve; procedoj en belarto de alia
      mondoparto:
      <ekz>
        tiun kastelon kontruigis Antono de Abadio de Arasto
        <klr>[&#x2026;]</klr> en stilo nov-gotika kun iom da
        <tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.esperanto-sat.info/article1292.html">
          Tuttaga Ekskurso: e&ubreve;ska lando</url></vrk>,
          <lok>SAT-Amikaro, [2009?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_r;&c_y;&c_je;&c_n;&c_t;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
  </snc>
  <snc mrk="orient1.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Rilata al la politiko, kulturo ktp. de Azio:
      <ekz>
        <klr>[la]</klr> profesoro eldiris sian ina&ubreve;guran lekcion pri Studado pri Korano
        kaj Hadiso en Okcidento inter <tld/>a kaj okcidentalisma sintenoj
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2011/010194.php">Islama universitato rekonita</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="orient1.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="dif" cel="orientalistik.0o.SCI">Orientalistiko</ref>:
      <ekz>
        Instituto de <tld lit="O"/>o
        <fnt>
          <aut>trad. S. &Sacute;migielski</aut>,
          <vrk>Islamo Koliziis kun Batalanta Laikismo</vrk>,
          <lok><url ref="http://www.ikue.org/cz/arkivo/a-62-2-06.htm">
          Dio Benu, 2006:2 (62)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_s;&c_h;&c_o;&c_d;&c_a;&c_z;&c_n;&c_a;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_a;&c_r;&c_y;&c_je;&c_n;&c_t;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>oriental&iacute;stica</trd>,
      <trd>orientalisme</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Orientalistik</trd>,
      <trd>Orientwissenschaft</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>orientalisztika</trd>,
      <trd>keletkutat&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">ori&euml;ntalisme</trd>
    <trd lng="pt">orientalismo</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_o;&c_s;&c_t;&c_o;&c_k;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_a;&c_l;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x4E1C;&#x65B9;&#x5F0F; <pr>d&#x14D;ngf&#x101;ngsh&#xEC;</pr></trd>,
    <trd>&#x6771;&#x65B9;&#x5F0F; <pr>d&#x14D;ngf&#x101;ngsh&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>orientalistika</trd>,
    <trd>orientalizmus</trd>
  </trdgrp>
  <trd lng="fr">orientalisme</trd>
  <trdgrp lng="ja">
    <trd>&#x6771;&#x6D0B;&#x8DA3;&#x5473;  <pr>&#x3068;&#x3046;&#x3088;&#x3046;&#x3057;&#x3085;&#x307F;</pr></trd>,
    <trd>&#x6771;&#x6D0B;&#x5B66;  <pr>&#x3068;&#x3046;&#x3088;&#x3046;&#x304C;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="pl">orientalistyka</trd>
  <trd lng="ro">studii  <ind>orientale</ind></trd>
  <trd lng="sk">orientalizmus</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_h;&#x43E;&c_d;&#x43E;&c_z;&#x43D;&c_a;&c_v;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd>&#x43E;&c_r;&c_ib;&c_jeu;&#x43D;&c_t;&c_a;&#x43B;&c_ib;&c_s;&c_t;&c_i;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>
</art>
