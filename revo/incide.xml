<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>incident</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="incide.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Flanka kaj akcesora negrava fakto, ordinare iom malhelpa, kiu
      okazas dum la irado de pli ampleksa afero; interokaza&jcirc;o:
      <ekz>oni ne devas trogravigi la <tld/>ojn de la konferenco.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_c;&c_y;&c_d;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_z;&c_d;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zwischenfall</trd>,
    <trd>Vorfall</trd>
  </trdgrp>
  <trd lng="es">incidente</trd>
  <trd lng="fr">incident</trd>
  <trd lng="hu">incidens</trd>
  <trd lng="nl">incident</trd>
  <trd lng="pt">incidente</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_c;&c_i;&c_d;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_i;&c_s;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
