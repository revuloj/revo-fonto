<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: laz.xml,v 1.14 2025/04/02 05:08:26 revo Exp $">
<kap>
  <rad>laz</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="laz.0o">
  <kap><tld/>o</kap>
  <snc mrk="laz.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="sxnur.kapto0o">Kapto&scirc;nuro</ref>, kiun oni &jcirc;etas
      &ccirc;irka&ubreve; kapta&jcirc;on kaj tire striktigas:
      <ekz>
        la mastro postkuras &ccirc;evalinon tenante enmane longan
        <tld/>on
        <fnt>
          <aut>Marzena</aut>,
          <vrk><url ref="http://www.naszawyprawa.com/relacje_es.asp">
          Voja&gcirc;raporto</url></vrk>,
          <lok>Mongolio, 2003-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Lasso</trd>
    <trd lng="zh">&#22871;&#32034; <pr>t&#224;osu&#466;</pr></trd>
</snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>kapto&#349;nuro</ke>, <ke>ma&#349;o&#349;nuro</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_s;&c_o;</trd>,
    <trd>&c_a;&c_r;&c_k;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="cs">laso</trd>
  <trd lng="es">lazo <klr>vaquero</klr></trd>
  <trd lng="fr">lasso</trd>
  <trdgrp lng="it">
    <trd>laccio</trd>,
    <trd>lazo</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25237;&#12370;&#12394;&#12431;  <pr>&#12394;&#12370;&#12394;&#12431;</pr></trd>,
    <trd>&#36650;&#12394;&#12431;  <pr>&#12426;&#12435;&#12394;&#12431;</pr></trd>
  </trdgrp>
  <trd lng="nl">lasso</trd>
  <trdgrp lng="pl">
    <trd>lasso</trd>,
    <trd>arkan</trd>
  </trdgrp>
  <trd lng="sk">laso</trd>
  <trdgrp lng="uk">
    <trd>&#1083;&#1072;&#1089;&#1086;</trd>,
    <trd>&#1072;&#1088;&#1082;&#1072;&#1085;</trd>
  </trdgrp>
</drv>

<drv mrk="laz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="laz.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="kapt.0i.preni">Kapti</ref> per <tld/>o:
      <ekz>
        la koleraj araneoj &jcirc;etis grandan nombron da retofadenoj
        celante ilin, esperante <tld/>i ilin a&ubreve; impliki ilin per la fadenoj
        <fnt>
          <aut>L. Frank Baum, trad. D. Broadribb</aut>,
          <vrk><url ref="http://esperantujo.org/eLibrejo/pdf/X13-7">
          Glinda de Oz</url></vrk>,
          <lok>1996</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">mit dem Lasso <ind>einfangen</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_v;&c_ib;&c_c;&c_mol; &c_n;&c_a; <ind>&c_l;&c_a;&c_s;&c_o;</ind></trd>,
    <trd>&c_l;&c_a;&c_v;&c_ib;&c_c;&c_mol; &c_n;&c_a; <ind>&c_a;&c_r;&c_k;&c_a;&c_n;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chytat do lasa</trd>,
    <trd>chytat lasem</trd>
  </trdgrp>
  <trd lng="fr">capturer au <ind>lasso</ind></trd>
  <trd lng="ja">&#25237;&#12370;&#12394;&#12431;&#12391;&#25429;&#12425;&#12360;&#12427;  <pr>&#12394;&#12370;&#12394;&#12431;&#12391;&#12392;&#12425;&#12360;&#12427;</pr></trd>
  <trd lng="nl">met een <ind>lasso</ind> vangen</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;apa&cacute; na <ind>lasso</ind></trd>,
    <trd>&lstroke;apa&cacute; na <ind>arkan</ind></trd>
  </trdgrp>
  <trd lng="sk">chyta&tcaron; lasom</trd>
  <trdgrp lng="uk">
    <trd>&#1072;&#1088;&#1082;&#1072;&#1085;&#1080;&#1090;&#1080;</trd>,
    <trd>&#1083;&#1086;&#1074;&#1080;&#1090;&#1080; &#1079;&#1072; &#1076;&#1086;&#1087;&#1086;&#1084;&#1086;&#1075;&#1086;&#1102; &#1072;&#1088;&#1082;&#1072;&#1085;&#1072;/&#1083;&#1072;&#1089;&#1086;</trd>
  </trdgrp>
</drv>
</art>

