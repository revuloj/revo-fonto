<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: abdik.xml,v 1.51 2024/01/06 21:22:12 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>abdik</rad>/i
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="abdik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="abdik.0i.regxo">
    <dif>
      <refgrp tip="super">
        <ref cel="eks.0igxi">Eksi&gcirc;i</ref>,
        <ref cel="rezign.0i">rezigni</ref>
      </refgrp>
      pri plej supera potenco a&ubreve; rango:
      <ekz>
        devigita <tld/>i de la trono, <nom>Ulugbek</nom> estis sendita al ha&gcirc;o kaj samtempe oni
        organizis ju&gcirc;adon, dum kiu li estis mortkondamnita
        <fnt>
          <bib>Monato</bib>
          <aut>Vitalij kaj Anatolij Ionesov</aut>
          <vrk><url ref="&Monato;/2010/009996.php"
              >Mirzo Ulugbek: granda re&gcirc;o de astronomio</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        La regnestraj zorgoj min jam tro lacigas, &parsl;
        Do kun kor' facila ilin mi <tld/>as
        <fnt>
          <aut>S. Pet&odblac;fi, trad. K. Kalocsay</aut>,
          <vrk><url ref="http://www.egalite.hu/kalocsay/petofi/johano%20la%20brava.htm">
          Johano la brava</url></vrk>,
          <lok>1984</lok>
        </fnt>;
      </ekz>
      <ekz>
        du jarojn poste <tld/>is dua germana prezidanto
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2012/010642.php"
              >Perditaj prezidantoj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_r;&c_a;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_a;&c_d;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_ja;</trd>
    </trdgrp>
    <trd lng="br"><ind>dilezel</ind> e garg a roue</trd>
    <trd lng="ca">abdicar</trd>
    <trdgrp lng="da">
      <trd>abdicere</trd>,
      <trd>frasige sig tronen</trd>
    </trdgrp>
    <trd lng="de">abdanken</trd>
    <trd lng="en">abdicate</trd>
    <trd lng="es">abdicar</trd>
    <trd lng="fr">abdiquer<klr tip="ind"> (abandonner le
    pouvoir)</klr></trd>
    <trd lng="hu">lemond a tr&oacute;nr&oacute;l</trd>
    <trd lng="hr">abdicirati</trd>
    <trd lng="it">abdicare</trd>
    <trdgrp lng="nl">
      <trd>abdiceren</trd>,
      <trd>afstand doen van de troon</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>abdykowa&cacute;</trd>
      <trd>zrzec si&eogonek;<klr> tronu</klr></trd>
    </trdgrp>
    <trd lng="pt">abdicar</trd>
    <trd lng="ru">&c_o;&c_t;&c_r;&c_je;&c_ch;&c_mol;&c_s;&c_ja;</trd>
    <trd lng="sk">vzda&tcaron; sa <klr>(vl&aacute;dy)</klr></trd>
    <trdgrp lng="sl">
      <trd>odpovedati se/odre&ccaron;i se
      <klr>(polo&zcaron;aju/oblasti/pravici)</klr></trd>
      <trd>odstopiti</trd>
      <trd>abdicirati</trd>
    </trdgrp>
    <trd lng="sv">abdikera</trd>
    <trdgrp lng="zh">
      <trd>&#19979;&#33274; <pr>xi&agrave;t&aacute;i</pr></trd>,
      <trd>&#31146; <pr>sh&agrave;n</pr></trd>,
      <trd>&#31109;&#35753; <pr>sh&agrave;nr&agrave;ng</pr></trd>,
      <trd>&#31146;&#35731; <pr>sh&agrave;nr&agrave;ng</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="abdik.0i.demisii">
    <uzo tip="stl">ARK</uzo>
   <dif>
      <ref tip="dif" cel="demisi.0i">Demisii</ref>:
      <ekz>
        mi decidis post mia ebla reveno al mia patrio tuj <tld/>i mian oficiran rangon
        <fnt>
          <bib>VaK</bib>
          <lok>Deksesa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la barata ministro pri defendo <nom>Krishna Menon</nom> <klr>[&#8230;]</klr> <tld/>is
        <fnt>
          <bib>Monato</bib>
          <aut>Miland Joshi</aut>
          <vrk><url ref="&Monato;/2008/009541.php"
              >&Ccirc;u bojkoto? (13)</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_u;</ind></trd>,
      <trd>&c_s;&c_y;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_u;</ind></trd>
    </trdgrp>
    <trd lng="br">rei&ntilde; e zilez</trd>
    <trdgrp lng="ca">
      <trd>dimitir</trd>,
      <trd>renunciar</trd>
    </trdgrp>
    <trd lng="da">tr&aelig;de tilbage</trd>
    <trdgrp lng="de">
      <trd>zur&uuml;cktreten</trd>,
      <trd>sein <ind>Amt</ind> <ind>niederlegen</ind></trd>,
      <trd>demissionieren</trd>
    </trdgrp>
    <trd lng="en">resign</trd>
    <trd lng="es">dimitir</trd>
    <trdgrp lng="fr">
      <trd>d&eacute;missionner</trd>,
      <trd>se <ind>d&eacute;mettre</ind></trd>
    </trdgrp>
    <trd lng="hu">lemond <klr>(hivatalr&oacute;l)</klr></trd>
    <trd lng="it">dare le dimissioni</trd>
    <trdgrp lng="nl">
      <trd>abdiceren</trd>,
      <trd>ambt neerleggen</trd>
    </trdgrp>
   <trdgrp lng="pl">
     <trd>dymisjonowa&cacute;</trd>
     <trd><klr>da&cacute; </klr>dymisj&eogonek;</trd>
     <trd><klr>otrzyma&cacute; </klr>dymisj&eogonek;</trd>
     <trd>zwolni&cacute;<klr> (z urz&eogonek;du, ze stanowiska)</klr></trd>
     <trd><klr>udzieli&cacute; </klr>dymisji</trd>
     <trd><klr>poda&cacute; si&eogonek; do </klr>dymisji</trd>
   </trdgrp>
    <trdgrp lng="pt">
      <trd>demitir-se</trd>,
      <trd>pedir demiss&atilde;o</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>odst&uacute;pi&tcaron;</trd>,
      <trd>poda&tcaron; <ind>demisiu</ind></trd>
    </trdgrp>
    <trd lng="sv">avs&auml;ga sig</trd>
    <trdgrp lng="zh">
      <trd>&#19979;&#21488; <pr>xi&agrave;t&aacute;i</pr></trd>,
      <trd>&#19979;&#37326; <pr>xi&agrave;y&ecaron;</pr></trd>
    </trdgrp>
  </snc>
  <rim mrk="abdik.0o.RIM">
    <ref tip="super" cel="eks.0igxo">Eksi&gcirc;o</ref> estas la plej
    &gcirc;enerala inter la sinonimoj, la plej ofte uzata kaj kutime la
    preferinda.
    <ref cel="demisi.0i">Demisio</ref> implicas ekziston de supera
    instanco, al kiu la eksi&gcirc;anto prezentas sian rezignon, kaj
    eventuale oni povas akcepti a&ubreve; malakcepti la demision.
    <tld lit="A"/>o estas ago de plej supera mo&scirc;tulo (monarko,
    religia estro), super kiu ne estas akceptanta instanco.
    <aut>Sergio Pokrovskij</aut>
  </rim>
  <trdgrp lng="cs">
    <trd>abdikovat</trd>,
    <trd>odstoupit</trd>,
    <trd>podat demisi</trd>,
    <trd>rezignovat</trd>,
    <trd>vzd&aacute;t se</trd>,
    <trd>z&rcaron;&iacute;ci se <klr>(&uacute;&rcaron;adu, funkce)</klr></trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_k;&a_n;&a_A;&a_r;&a_h;&zwnj;&f_g;&f_y;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_A;&a_s;&a_t;&a_ayn;&a_f;&a_A; &a_d;&a_A;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="he">&lamed;&vav;&vav;&tav;&resh; &ayin;&lamed;
  &he;&mem;&lamed;&vav;&kaf;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#36864;&#20301;&#12377;&#12427; <pr>&#12383;&#12356;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#36864;&#20219;&#12377;&#12427; <pr>&#12383;&#12356;&#12395;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#35698;&#20301;&#12377;&#12427; <pr>&#12376;&#12423;&#12358;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#36766;&#32887;&#12377;&#12427; <pr>&#12376;&#12375;&#12423;&#12367;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>abdikova&tcaron;</trd>,
    <trd>vzda&tcaron; sa</trd>,
    <trd>zriec&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_ib;&c_k;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_r;&c_ib;&c_k;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_m;&c_o;&c_v;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="abdik.0o">
  <kap><tld/>o</kap>
  <snc mrk="abdik.0o.regxo">
    <dif>
      Ago <tld/>i:
      <ekz>
        <tld/>o de Napoleono
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 6 (68)</lok>
        </fnt>;
      </ekz>
      <ekz>
        en da&ubreve;re nestabila Ukrainio la opozicio postulis <tld/>on de la prezidanto
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009751.php"
              >Nova mizero</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_d;&c_r;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trdgrp lng="br">
      <trd>dilez</trd>,
      <trd>emzilez</trd>
    </trdgrp>
    <trd lng="ca">abdicaci&oacute;</trd>
    <trdgrp lng="da">
      <trd>abdikation</trd>,
      <trd>tronfrasigelse</trd>
    </trdgrp>
    <trd lng="de">Abdankung</trd>
    <trd lng="en">abdication</trd>
    <trd lng="es">abdicaci&oacute;n</trd>
    <trd lng="fr">abdication<klr tip="ind"> (abandon du
    pouvoir)</klr></trd>
    <trd lng="hr">abdikacija</trd>
    <trdgrp lng="pl">
     <trd>abdykacja</trd>
     <trd>abdykowanie</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_r;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
    <trdgrp lng="zh">
      <trd>&#19979;&#33274; <pr>xi&agrave;t&aacute;i</pr></trd>,
      <trd>&#36874;&#20301; <pr>x&ugrave;nw&egrave;i</pr></trd>,
      <trd>&#36956;&#20301; <pr>x&ugrave;nw&egrave;i</pr></trd>,
      <trd>&#24223;&#23265; <pr>f&egrave;id&iacute;</pr></trd>,
      <trd>&#24290;&#23265; <pr>f&egrave;id&iacute;</pr></trd>,
      <trd>&#35731;&#20301; <pr>r&agrave;ngw&egrave;i</pr></trd>,
      <trd>&#35753;&#20301; <pr>r&agrave;ngw&egrave;i</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="abdik.0o.demisio">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="demisi.0o">Demisio</ref>:
      <ekz>
        opozicia partio postulas <tld/>on de la pormedia ministro
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2006/009020.php"
              >Bavario funebras pro Beppo Bruno</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_a;</trd>
    <trdgrp lng="br">
      <trd>dilez</trd>,
      <trd>emzilez</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>dimissi&oacute;</trd>,
      <trd>ren&uacute;ncia</trd>
    </trdgrp>
    <trd lng="da">tilbagetr&aelig;delse</trd>
    <trdgrp lng="de">
      <trd>Demission</trd>,
      <trd>Amtsniederlegung</trd>
    </trdgrp>
    <trd lng="es">dimisi&oacute;n</trd>
    <trdgrp lng="fr">
      <trd>d&eacute;mission</trd>
    </trdgrp>
    <trd lng="pl">dymisja</trd>
    <trdgrp lng="zh">
      <trd>&#21368;&#20219; <pr>xi&egrave;r&egrave;n</pr></trd>,
      <trd>&#19979;&#33274; <pr>xi&agrave;t&aacute;i</pr></trd>,
      <trd>&#19979;&#37326; <pr>xi&agrave;y&ecaron;</pr></trd>,
      <trd>&#35299;&#20915;&#36890;&#30693; <pr>ji&ecaron;ju&eacute;t&omacron;ngzh&imacron;</pr></trd>,
      <trd>&#35299;&#27770;&#36890;&#30693; <pr>ji&ecaron;ju&eacute;t&omacron;ngzh&imacron;</pr></trd>,
      <trd>&#36874;&#20301; <pr>x&ugrave;nw&egrave;i</pr></trd>,
      <trd>&#36956;&#20301; <pr>x&ugrave;nw&egrave;i</pr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>abdikace</trd>,
    <trd>demise</trd>,
    <trd>odstoupen&iacute;</trd>
  </trdgrp>
  <trd lng="de">R&uuml;cktritt</trd>
  <trdgrp lng="fa">
    <trd>&f_k;&a_n;&a_A;&a_r;&a_h;&zwnj;&f_g;&f_y;&a_r;&f_y;</trd>,
    <trd>&a_A;&a_s;&a_t;&a_ayn;&a_f;&a_A;</trd>,
    <trd>&a_t;&a_f;&a_w;&f_y;&a_D;</trd>
  </trdgrp>
  <trd lng="he">&he;&#1460;&tav;&pe;&#1468;&#1463;&tet;&#1456;&resh;&vav;&#1468;&tav;</trd>
  <trdgrp lng="ja">
    <trd>&#36864;&#20301; <pr>&#12383;&#12356;&#12356;</pr></trd>,
    <trd>&#36864;&#20219; <pr>&#12383;&#12356;&#12395;&#12435;</pr></trd>,
    <trd>&#36766;&#20219; <pr>&#12376;&#12395;&#12435;</pr></trd>,
    <trd>&#36766;&#32887; <pr>&#12376;&#12375;&#12423;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>abdik&aacute;cia</trd>,
    <trd>odst&uacute;penie</trd>,
    <trd>vzdanie sa</trd>,
    <trd>zrieknutie sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_b;&c_d;&c_i;&c_k;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_z;&c_r;&c_je;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_m;&c_o;&c_v;&c_l;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
<adm>
  cxu por la duaj sencoj indas doni tradukojn?
  <aut>MB</aut>
</adm>
</art>
