<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: abdik.xml,v 1.50 2023/10/27 12:51:04 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>abdik</rad>/i
</kap>
<drv mrk="abdik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="abdik.0i.regxo">
    <dif>
      <refgrp tip="super">
        <ref cel="eks.0igxi">Eksi&gcirc;i</ref>,
        <ref cel="rezign.0i">rezigni</ref>
      </refgrp>
      pri plej supera potenco a&ubreve; rango:
      <ekz>
        La regnestraj zorgoj min jam tro lacigas, /
        Do kun kor' facila ilin mi <tld/>as
        <fnt>
          <aut>S. Pet&odblac;fi, trad. K. Kalocsay</aut>,
          <vrk><url ref="http://www.egalite.hu/kalocsay/petofi/johano%20la%20brava.htm">
          Johano la brava</url></vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_r;&c_a;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_a;&c_d;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_ja;</trd>
    </trdgrp>
    <trd lng="br"><ind>dilezel</ind> e garg a roue</trd>
    <trd lng="ca">abdicar</trd>
    <trdgrp lng="da">
      <trd>abdicere</trd>,
      <trd>frasige sig tronen</trd>
    </trdgrp>
    <trd lng="de">abdanken</trd>
    <trd lng="en">abdicate</trd>
    <trd lng="es">abdicar</trd>
    <trd lng="fr">abdiquer<klr tip="ind"> (abandonner le
    pouvoir)</klr></trd>
    <trd lng="hu">lemond a tr&oacute;nr&oacute;l</trd>
    <trd lng="hr">abdicirati</trd>
    <trd lng="it">abdicare</trd>
    <trdgrp lng="nl">
      <trd>abdiceren</trd>,
      <trd>afstand doen van de troon</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>abdykowa&cacute;</trd>
      <trd>zrzec si&eogonek;<klr> tronu</klr></trd>
    </trdgrp>
    <trd lng="pt">abdicar</trd>
    <trd lng="ru">&c_o;&c_t;&c_r;&c_je;&c_ch;&c_mol;&c_s;&c_ja;</trd>
    <trd lng="sk">vzda&tcaron; sa <klr>(vl&aacute;dy)</klr></trd>
    <trdgrp lng="sl">
      <trd>odpovedati se/odre&ccaron;i se
      <klr>(polo&zcaron;aju/oblasti/pravici)</klr></trd>
      <trd>odstopiti</trd>
      <trd>abdicirati</trd>
    </trdgrp>
    <trd lng="sv">abdikera</trd>
    <trdgrp lng="zh">
    <trd>&#x4E0B;&#x81FA; <pr>xi&#xE0;t&#xE1;i</pr></trd>,
    <trd>&#x79AA; <pr>sh&#xE0;n</pr></trd>,
    <trd>&#x7985;&#x8BA9; <pr>sh&#xE0;nr&#xE0;ng</pr></trd>,
    <trd>&#x79AA;&#x8B93; <pr>sh&#xE0;nr&#xE0;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="abdik.0i.demisii">
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="demisi.0i">demisii</ref>
    <refgrp tip="vid">
      <ref cel="eks.0igxi">eksi&gcirc;i</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_u;</ind></trd>,
      <trd>&c_s;&c_y;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_u;</ind></trd>
    </trdgrp>
    <trd lng="br">rei&ntilde; e zilez</trd>
    <trdgrp lng="ca">
      <trd>dimitir</trd>,
      <trd>renunciar</trd>
    </trdgrp>
    <trd lng="da">tr&aelig;de tilbage</trd>
    <trdgrp lng="de">
      <trd>zur&uuml;cktreten</trd>,
      <trd>sein <ind>Amt</ind> <ind>niederlegen</ind></trd>,
      <trd>demissionieren</trd>
    </trdgrp>
    <trd lng="en">resign</trd>
    <trd lng="es">dimitir</trd>
    <trdgrp lng="fr">
      <trd>d&eacute;missionner</trd>,
      <trd>se <ind>d&eacute;mettre</ind></trd>
    </trdgrp>
    <trd lng="hu">lemond <klr>(hivatalr&oacute;l)</klr></trd>
    <trd lng="it">dare le dimissioni</trd>
    <trdgrp lng="nl">
      <trd>abdiceren</trd>,
      <trd>ambt neerleggen</trd>
    </trdgrp>
    <trdgrp lng="pl">
   <trd>dymisjonowa&cacute;</trd>
   <trd><klr>da&cacute; </klr>dymisj&eogonek;</trd>
   <trd><klr>otrzyma&cacute; </klr>dymisj&eogonek;</trd>
   <trd>zwolni&cacute;<klr> (z urz&eogonek;du, ze stanowiska)</klr></trd>
   <trd><klr>udzieli&cacute; </klr>dymisji</trd>
   <trd><klr>poda&cacute; si&eogonek; do </klr>dymisji</trd>
 </trdgrp>
    <trdgrp lng="pt">
      <trd>demitir-se</trd>,
      <trd>pedir demiss&atilde;o</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>odst&uacute;pi&tcaron;</trd>,
      <trd>poda&tcaron; <ind>demisiu</ind></trd>
    </trdgrp>
    <trd lng="sv">avs&auml;ga sig</trd>
    <trdgrp lng="zh">
    <trd>&#x4E0B;&#x53F0; <pr>xi&#xE0;t&#xE1;i</pr></trd>,
    <trd>&#x4E0B;&#x91CE; <pr>xi&#xE0;y&#x11B;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>abdikovat</trd>,
    <trd>odstoupit</trd>,
    <trd>podat demisi</trd>,
    <trd>rezignovat</trd>,
    <trd>vzd&aacute;t se</trd>,
    <trd>z&rcaron;&iacute;ci se <klr>(&uacute;&rcaron;adu, funkce)</klr></trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_k;&a_n;&a_A;&a_r;&a_h;&#x200C;&f_g;&f_y;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_A;&a_s;&a_t;&a_ayn;&a_f;&a_A; &a_d;&a_A;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="he">&lamed;&vav;&vav;&tav;&resh; &ayin;&lamed;
  &he;&mem;&lamed;&vav;&kaf;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#x9000;&#x4F4D;&#x3059;&#x308B; <pr>&#x305F;&#x3044;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x9000;&#x4EFB;&#x3059;&#x308B; <pr>&#x305F;&#x3044;&#x306B;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8B72;&#x4F4D;&#x3059;&#x308B; <pr>&#x3058;&#x3087;&#x3046;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8F9E;&#x8077;&#x3059;&#x308B; <pr>&#x3058;&#x3057;&#x3087;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>abdikova&tcaron;</trd>,
    <trd>vzda&tcaron; sa</trd>,
    <trd>zriec&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_ib;&c_k;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_r;&c_ib;&c_k;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_m;&c_o;&c_v;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="abdik.0o">
  <kap><tld/>o</kap>
  <snc mrk="abdik.0o.regxo">
    <dif>
      Ago <tld/>i.
    </dif>
    <trd lng="be">&c_a;&c_d;&c_r;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trdgrp lng="br">
      <trd>dilez</trd>,
      <trd>emzilez</trd>
    </trdgrp>
    <trd lng="ca">abdicaci&oacute;</trd>
    <trdgrp lng="da">
      <trd>abdikation</trd>,
      <trd>tronfrasigelse</trd>
    </trdgrp>
    <trd lng="de">Abdankung</trd>
    <trd lng="en">abdication</trd>
    <trd lng="es">abdicaci&oacute;n</trd>
    <trd lng="fr">abdication<klr tip="ind"> (abandon du
    pouvoir)</klr></trd>
    <trd lng="hr">abdikacija</trd>
    <trdgrp lng="pl">
     <trd>abdykacja</trd>
     <trd>abdykowanie</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_r;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
    <trdgrp lng="zh">
    <trd>&#x4E0B;&#x81FA; <pr>xi&#xE0;t&#xE1;i</pr></trd>,
    <trd>&#x900A;&#x4F4D; <pr>x&#xF9;nw&#xE8;i</pr></trd>,
    <trd>&#x905C;&#x4F4D; <pr>x&#xF9;nw&#xE8;i</pr></trd>,
    <trd>&#x5E9F;&#x5AE1; <pr>f&#xE8;id&#xED;</pr></trd>,
    <trd>&#x5EE2;&#x5AE1; <pr>f&#xE8;id&#xED;</pr></trd>,
    <trd>&#x8B93;&#x4F4D; <pr>r&#xE0;ngw&#xE8;i</pr></trd>,
    <trd>&#x8BA9;&#x4F4D; <pr>r&#xE0;ngw&#xE8;i</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="abdik.0o.demisio">
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="demisi.0o">demisio</ref>
    <trd lng="be">&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_a;</trd>
    <trdgrp lng="br">
      <trd>dilez</trd>,
      <trd>emzilez</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>dimissi&oacute;</trd>,
      <trd>ren&uacute;ncia</trd>
    </trdgrp>
    <trd lng="da">tilbagetr&aelig;delse</trd>
    <trdgrp lng="de">
      <trd>K&uuml;ndigung</trd>,
      <trd>Demission</trd>
    </trdgrp>
    <trd lng="es">dimisi&oacute;n</trd>
    <trdgrp lng="fr">
      <trd>d&eacute;mission</trd>
    </trdgrp>
    <trd lng="pl">dymisja</trd>
    <trdgrp lng="zh">
    <trd>&#x5378;&#x4EFB; <pr>xi&#xE8;r&#xE8;n</pr></trd>,
    <trd>&#x4E0B;&#x81FA; <pr>xi&#xE0;t&#xE1;i</pr></trd>,
    <trd>&#x4E0B;&#x91CE; <pr>xi&#xE0;y&#x11B;</pr></trd>,
    <trd>&#x89E3;&#x51B3;&#x901A;&#x77E5; <pr>ji&#x11B;ju&#xE9;t&#x14D;ngzh&#x12B;</pr></trd>,
    <trd>&#x89E3;&#x6C7A;&#x901A;&#x77E5; <pr>ji&#x11B;ju&#xE9;t&#x14D;ngzh&#x12B;</pr></trd>,
    <trd>&#x900A;&#x4F4D; <pr>x&#xF9;nw&#xE8;i</pr></trd>,
    <trd>&#x905C;&#x4F4D; <pr>x&#xF9;nw&#xE8;i</pr></trd>
  </trdgrp>
</snc>
  <rim mrk="abdik.0o.RIM">
    <ref tip="super" cel="eks.0igxo">Eksi&gcirc;o</ref> estas la plej
    &gcirc;enerala inter la sinonimoj, la plej ofte uzata kaj kutime la
    preferinda.
    <ref cel="demisi.0o">Demisio</ref> implicas ekziston de supera
    instanco, al kiu la eksi&gcirc;anto prezentas sian rezignon, kaj
    eventuale oni povas akcepti a&ubreve; malakcepti la demision.
    <tld lit="A"/>o estas ago de plej supera mo&scirc;tulo (monarko,
    religia estro), super kiu ne estas akceptanta instanco.
    <aut>Sergio Pokrovskij</aut>
  </rim>
  <trdgrp lng="cs">
    <trd>abdikace</trd>,
    <trd>demise</trd>,
    <trd>odstoupen&iacute;</trd>
  </trdgrp>
  <trd lng="de">R&uuml;cktritt</trd>
  <trdgrp lng="fa">
    <trd>&f_k;&a_n;&a_A;&a_r;&a_h;&#x200C;&f_g;&f_y;&a_r;&f_y;</trd>,
    <trd>&a_A;&a_s;&a_t;&a_ayn;&a_f;&a_A;</trd>,
    <trd>&a_t;&a_f;&a_w;&f_y;&a_D;</trd>
  </trdgrp>
  <trd lng="he">&he;&#x5B4;&tav;&pe;&#x5BC;&#x5B7;&tet;&#x5B0;&resh;&vav;&#x5BC;&tav;</trd>
  <trdgrp lng="ja">
    <trd>&#x9000;&#x4F4D; <pr>&#x305F;&#x3044;&#x3044;</pr></trd>,
    <trd>&#x9000;&#x4EFB; <pr>&#x305F;&#x3044;&#x306B;&#x3093;</pr></trd>,
    <trd>&#x8F9E;&#x4EFB; <pr>&#x3058;&#x306B;&#x3093;</pr></trd>,
    <trd>&#x8F9E;&#x8077; <pr>&#x3058;&#x3057;&#x3087;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>abdik&aacute;cia</trd>,
    <trd>odst&uacute;penie</trd>,
    <trd>vzdanie sa</trd>,
    <trd>zrieknutie sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_b;&c_d;&c_i;&c_k;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_z;&c_r;&c_je;&c_ch;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_m;&c_o;&c_v;&c_l;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
<adm>
  Ne klaras, cxu vere la duaj sencoj ekzistas. Supozeble
  ne indas doni tradukojn por ili.
  <aut>MB</aut>
</adm>
</art>
