<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kafein.xml,v 1.30 2023/12/02 21:20:03 revo Exp $">
<kap>
   <rad>kafein</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kafein.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">KEM</uzo>
   <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="alkalo.0o">Alkaloido</ref> ekstraktita el
      kafo, uzata en la medicino por fortigi kaj stimuli la koron
      a&ubreve; nervojn:
      <ekz>
        <tld/>o, el kafo, estas ekzemplo de organika bazo kiu ne partoprenas en RNA
        <fnt>
          <aut>Ste&ccirc;jo Norvell</aut>
          <vrk><url ref="&Monato;/2000/007157.html">Riboj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 16-17</lok>
          </fnt>;
      </ekz>
      <ekz>
        konsumo de <tld/>o kaj alkohola&jcirc;oj malhelpas noktan ripozon, precipe &ccirc;e
        maljunuloj
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2012/010683.php">Dormu bone, vivu bone</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la strategio de multaj plantoj por garantii la lojalecon kaj fidelecon de iliaj
        polenantoj estas miksi sian nektaron kun substancoj, kiel nikotino a&ubreve;
        <tld/>o, kiuj provokas dependon
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011217.php">Nekovrilaj malkovroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Alia nomo de tiu &hcirc;emia&jcirc;o estas <ref tip="sin" cel="tein.0o">teino</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_f;&c_e;&c_ib;&c_n;</trd>
  <trd lng="ca">cafe&iuml;na</trd>
  <trdgrp lng="cs">
    <trd>kofein</trd>,
    <trd>tein</trd>
  </trdgrp>
  <trd lng="de">Koffein</trd>
  <trd lng="en">caffeine</trd>
  <trd lng="es">cafe&iacute;na</trd>
  <trd lng="fi">kofeiini</trd>
  <trd lng="fr">caf&eacute;ine</trd>
  <trd lng="hu">koffein</trd>
  <trd lng="ja">&#x30AB;&#x30D5;&#x30A7;&#x30A4;&#x30F3;</trd>
  <trd lng="nl">cafe&iuml;ne</trd>
  <trd lng="pl">kofeina</trd>
  <trd lng="pt">cafe&iacute;na</trd>
  <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_i;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>kofe&iacute;n</trd>,
    <trd>te&iacute;n</trd>
  </trdgrp>
  <trd lng="sv">koffein</trd>
  <trd lng="tr">kafein</trd>
  <trd lng="uk">&#x43A;&#x43E;&c_f;&c_je;&c_ji;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x5496;&#x5561;&#x56E0; <pr>k&#x101;f&#x113;iy&#x12B;n</pr></trd>,
    <trd>&#x5496;&#x5561;&#x78B1; <pr>k&#x101;f&#x113;iji&#x1CE;n</pr></trd>
  </trdgrp>
</drv>
</art>
