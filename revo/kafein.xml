<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>kafein</rad>/o
</kap>
<drv mrk="kafein.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">KEM</uzo>
   <uzo tip="fak">MED</uzo>
   <snc>
     <dif>
       <ref tip="super" cel="alkalo.0o">Alkaloido</ref> ekstraktita el
       kafo, uzata en la medicino por fortigi kaj stimuli la koron
       a&ubreve; nervojn.
       Alia nomo estas <ref tip="sin" cel="tein.0o">teino</ref>.
     </dif>
   </snc>
   <trd lng="be">&c_k;&c_a;&c_f;&c_e;&c_ib;&c_n;</trd>
   <trd lng="de">Koffein</trd>
   <trd lng="en">caffeine</trd>
   <trd lng="es">cafe&iacute;na</trd>
   <trd lng="fi">kofeiini</trd>
   <trd lng="fr">caf&eacute;ine</trd>
   <trd lng="hu">koffein</trd>
   <trd lng="nl">cafe&iuml;ne</trd>
   <trd lng="pl">kofeina</trd>
   <trd lng="pt">cafe&iacute;na</trd>
   <trd lng="ru">&c_k;&c_o;&c_f;&c_je;&c_i;&c_n;</trd>
   <trd lng="sv">koffein</trd>
</drv>
</art>
