<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>borag</rad>/o
</kap>
<drv mrk="borag.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="borag.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Borago</trd>), kun bluaj floroj, uzata por spici
      salatojn kaj legomojn.
    </dif>
  </snc>
  <bld lok="../bld/borag2.jpg">
    floro de ordinara <tld/>o
  </bld>
  <bld lok="../bld/borag.jpg">
    <ind>ordinara <tld/>o</ind>
    <trd lng="la">Borago officinalis</trd>
  </bld>
  <trd lng="de">Boretsch</trd>
  <trd lng="en">borage</trd>
  <trd lng="fr">bourrache</trd>
  <trd lng="ru">&c_b;&c_u;&c_r;&c_a;&c_ch;&c_n;&c_i;&c_k;</trd>
</drv>

</art>
