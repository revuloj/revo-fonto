<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>borag</rad>/o
</kap>
<drv mrk="borag.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="borag.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Borago</trd>), kun bluaj floroj, uzata por spici
      salatojn kaj legomojn.
    </dif>
  </snc>
  <bld lok="../bld/borag.jpg">
    ordinara <tld/>o (<trd lng="la">Borago officinalis</trd>)
  </bld>
  <trd lng="de">Boretsch</trd>
</drv>
<drv mrk="borag.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Boraginaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Boretschgew&auml;chse</trd>
</drv>
</art>
