<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>komision</rad>/o
</kap>
<drv mrk="komisi1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homgrupo, kiun institucio <ref cel="komisi.0i">komisias</ref>
      por esplori kaj priraporti difinitan demandon a&ubreve; 
      plenumi difinitan taskon:
      <ekz>
        <uzo tip="fak">POL</uzo>
        la <tld/>o pri virinaj aferoj de Unui&gcirc;intaj Nacioj
        <fnt><aut>Persa
        Homarano</aut>: <vrk>La&ubreve;le&gcirc;a perforto</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 5a, p. 9a</lok></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>estro</ind>
        <fnt><bib>SatirRak</bib><lok>Honorigo</lok></fnt>;
      </ekz>
      <ekz>
        ver&scirc;ajne ili &ccirc;iuj devos blokope aperi anta&ubreve;
        <ind>esplor<tld/>o</ind>
        <fnt><bib>UrdHadda</bib><lok>p. 26</lok></fnt>;
        <trd lng="fr">commission d'enqu&ecirc;te</trd>
      </ekz>
      <ekz>
        <ind>kontrol<tld/>o</ind>
        <fnt><bib>Metrop</bib><lok>&ccirc;ap. 8</lok></fnt>;
        <trdgrp lng="fr">
          <trd>commission de contr&ocirc;le</trd>,
          <trd>commission de suivi</trd>
        </trdgrp>
      </ekz>
      <ekz>
        la III-a Internacio starigis <ind>stud<tld/>o</ind>n, kies tasko
        estas raporti anta&ubreve; la proksima internacia Kongreso
        <fnt><bib>Lanti</bib><lok>Nia pozicio</lok></fnt>;
        <trd lng="fr">commission d'&eacute;tudes</trd>
      </ekz>
      <ekz>
        misinformi la <ind>rekrut<tld/>o</ind>n estas komunum-ofenda ago
        <fnt><bib>SatirRak</bib><lok>Honorigo</lok></fnt>.
        <trdgrp lng="fr">
          <trd>commission de recrutement</trd>,
          <trd>commission d'embauche</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="sin">
      Ar&hcirc;aikaj ekvivalentoj:
      <ref cel="komisi.0o.komisiono">komisio <sncref/></ref>,
      <ref cel="komisi.0itaro">komisiitaro</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="komita.0o">komitato</ref>
    </refgrp>
  </snc>
  <adm>
    PIV havas "enket~o" (kiel ekz-on) anstataux nia "esplor~o".
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_k;&c_a;&c_m;&c_ib;&c_s;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Ausschuss</trd>,
    <trd>Kommission</trd>
  </trdgrp>
  <trd lng="en">commission</trd>
  <trdgrp lng="fr">
    <trd>commission<klr tip="ind"> (groupe de personnes commises)</klr></trd>,
    <trd>groupe de <ind>travail</ind></trd>
  </trdgrp>
  <trd lng="nl">commissie</trd>
  <trd lng="pt">comiss&atilde;o</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_i;&c_ja;</trd>
</drv>

<drv mrk="komisi1.0ano">
  <kap><tld/>ano</kap>
  <snc>
    <dif>
      Partoprenanto en laborado de <tld/>o:
      <ekz>
        <tld/>anoj disponas pri interpretistoj
        <fnt><aut>Dafydd ap Fergus</aut>: <vrk>Patenta
        (mal)sukceso</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 11a,
        p. 11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_l;&c_je;&c_n; <ind>&c_k;&c_a;&c_m;&c_ib;&c_s;&c_ib;&c_ib;</ind></trd>
  <trdgrp lng="de">
    <trd>Ausschussmitglied</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>membre de la <ind>commission</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_l;&c_je;&c_n; <ind>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_i;&c_i;</ind></trd>
</drv>

<drv mrk="komisi1.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <ref tip="dif" cel="komisi1.0ano"><tld/>ano</ref>
    <ekz>
      la <tld/>uloj restis &scirc;ajne senemociaj
      <fnt><bib>SatirRak</bib><lok>Honorigo</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_ch;&c_l;&c_je;&c_n; <ind>&c_k;&c_a;&c_m;&c_ib;&c_s;&c_ib;&c_ib;</ind></trd>
  <trdgrp lng="de">
    <trd>Ausschussmitglied</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>membre de la <ind>commission</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_l;&c_je;&c_n; <ind>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_i;&c_i;</ind></trd>
</drv>
</art>
