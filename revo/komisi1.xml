<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: komisi1.xml,v 1.25 2024/03/23 12:38:49 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>komision</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="komisi1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homgrupo, kiun institucio <ref cel="komisi.0i">komisias</ref>
      por esplori kaj priraporti difinitan demandon a&ubreve;
      plenumi difinitan taskon:
      <ekz>
        <uzo tip="fak">POL</uzo>
        la <tld/>o pri virinaj aferoj de Unui&gcirc;intaj Nacioj
        <fnt><aut>Persa
        Homarano</aut>: <vrk>La&ubreve;le&gcirc;a perforto</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 5a, p. 9a</lok></fnt>;
      </ekz>
      <ekz>
        <ind><tld/>estro</ind>
        <fnt><bib>SatirRak</bib><lok>Honorigo</lok></fnt>;
      </ekz>
      <ekz>
        ver&scirc;ajne ili &ccirc;iuj devos blokope aperi anta&ubreve;
        <ind>esplor<tld/>o</ind>
        <fnt>
          <bib>UrdHadda</bib>
          <lok>p. 26</lok>
        </fnt>;
        <trd lng="de">Untersuchungsausschuss</trd>
        <trd lng="fr">commission d'enqu&ecirc;te</trd>
        <trd lng="nl">onderzoekscommissie</trd>
      </ekz>
      <ekz>
        la enket<tld/>o klare verdiktis
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2013/011041.php">Kulpas
          nur li</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kontrol<tld/>o</ind>
        <fnt><bib>Metrop</bib><lok>&ccirc;ap. 8</lok></fnt>;
        <trdgrp lng="fr">
          <trd>commission de contr&ocirc;le</trd>,
          <trd>commission de suivi</trd>
        </trdgrp>
        <trd lng="nl">controlecommissie</trd>
      </ekz>
      <ekz>
        la III-a Internacio starigis <ind>stud<tld/>o</ind>n, kies tasko
        estas raporti anta&ubreve; la proksima internacia Kongreso
        <fnt><bib>Lanti</bib><lok>Nia pozicio</lok></fnt>;
        <trd lng="fr">commission d'&eacute;tudes</trd>
        <trd lng="nl">studiecommissie</trd>
      </ekz>
      <ekz>
        misinformi la <ind>rekrut<tld/>o</ind>n estas komunum-ofenda ago
        <fnt><bib>SatirRak</bib><lok>Honorigo</lok></fnt>.
        <trdgrp lng="fr">
          <trd>commission de recrutement</trd>,
          <trd>commission d'embauche</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="komisi.0o.komisiono">komisio<sncref/></ref>,
      <ref cel="komisi.0itaro">komisiitaro</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="komita.0o">komitato</ref>
    </refgrp>
  </snc>
  <adm>
    PIV havas "enket~o" (kiel ekz-on) anstataux nia "esplor~o".
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_k;&c_a;&c_m;&c_ib;&c_s;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Ausschuss</trd>,
    <trd>Kommission</trd>
  </trdgrp>
  <trd lng="en">commission</trd>
  <trd lng="fa">&f_k;&a_m;&f_y;&a_s;&f_y;&a_w;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>commission<klr tip="ind"> (groupe de personnes
    commises)</klr></trd>,
    <trd>groupe de <ind>travail</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x59D4;&#x54E1;&#x4F1A;  <pr>&#12356;&#12356;&#12435;&#x304B;&#12356;</pr></trd>
  <trd lng="nl">commissie</trd>
  <trd lng="pl">komisja</trd>
  <trd lng="pt">comiss&atilde;o</trd>
  <trd lng="ro">comitet</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_i;&c_ja;</trd>
  <trd lng="sk">komisia</trd>
  <trdgrp lng="cs">
    <trd>komise</trd>,
    <trd>sbor pov&ecaron;&rcaron;en&yacute;ch</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x43E;&#x43C;&c_ib;&c_s;&c_ib;&#x44F;</trd>
  <trd lng="zh">&#x59D4;&#21592;&#x4F1A; <pr>w&#x11B;iyu&#xE1;nhu&#xEC;</pr></trd>
</drv>

<drv mrk="komisi1.0ano">
  <kap><tld/>ano</kap>
  <snc>
    <dif>
      Partoprenanto en laborado de <tld/>o:
      <ekz>
        <tld/>anoj disponas pri interpretistoj
        <fnt><aut>Dafydd ap Fergus</aut>: <vrk>Patenta
        (mal)sukceso</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero
        11a, p. 11a</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;eestis kontra&ubreve;terorisman konferencon en Vieno la EU-<tld/>ano
        pri internaj aferoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2015/011476.php">Kune
          kontra&ubreve; ekstremistoj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nova <tld/>ano pri energio tamen ne meritas kompaton
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2010/010010.php">Energie
          lernante</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="komisi.0ito">komisiito</ref>
  </snc>
  <trd lng="be">&c_ch;&c_l;&c_je;&c_n; <ind>&c_k;&c_a;&c_m;&c_ib;&c_s;&c_ib;&c_ib;</ind></trd>
  <trd lng="cs">&ccaron;len komise</trd>
  <trdgrp lng="de">
    <trd>Ausschussmitglied</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>membre de la <ind>commission</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x59D4;&#x54E1;&#x4F1A;&#x59D4;&#x54E1;  <pr>&#12356;&#12356;&#12435;&#x304B;&#12356;&#12356;&#12356;&#12435;</pr></trd>
  <trd lng="nl">lid van een <ind>commissie</ind></trd>
  <trd lng="pl">cz&lstroke;onek <ind>komisji</ind></trd>
  <trd lng="ru">&c_ch;&c_l;&c_je;&c_n; <ind>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_i;&c_i;</ind></trd>
  <trd lng="sk">&ccaron;len komisie</trd>
  <trd lng="zh">&#x59D4;&#21592; <pr>w&#x11B;iyu&#xE1;n</pr></trd>
</drv>

<drv mrk="komisi1.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="komisi1.0ano"><tld lit="K"/>ano</ref>:
      <ekz>
        la <tld/>uloj restis &scirc;ajne senemociaj
        <fnt><bib>SatirRak</bib><lok>Honorigo</lok></fnt>;
      </ekz>
      <ekz>
        E&ubreve;ropa Komisionulo pri la Interna Merkato kaj Servoj de la
        E&ubreve;ropa Unio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=452300">ESCP
          Europe</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_l;&c_je;&c_n; <ind>&c_k;&c_a;&c_m;&c_ib;&c_s;&c_ib;&c_ib;</ind></trd>
  <trdgrp lng="de">
    <trd>Ausschussmitglied</trd>
  </trdgrp>
  <trdgrp lng="fr">
  <trd>membre de la <ind>commission</ind></trd>
  </trdgrp>
  <trd lng="nl">lid van een <ind>commissie</ind></trd>
  <trd lng="pl">delegat</trd>
  <trd lng="ru">&c_ch;&c_l;&c_je;&c_n; <ind>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_i;&c_i;</ind></trd>
  <trd lng="zh">&#x59D4;&#21592; <pr>w&#x11B;iyu&#xE1;n</pr></trd>
</drv>
</art>
