<drv mrk="kapabl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ecoj, kondi&ccirc;oj necesaj por fari ion:
      <ekz>
        &ccirc;u ne estus al vi agrable havi tian saman <tld/>on?
        <fnt><bib>F</bib>, <lok>&FE; 17</lok></fnt>
      </ekz>
      <ekz>
        mi memoros vin, &gcirc;is mia kapo perdos la <tld/>on de
        &ccirc;ia memorado
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        fabeloj <klr>[&#x2026;]</klr> alkonformigitaj <klr>[&#x2026;]</klr> al la
        kompren<tld/>o de la <klr>[&#x2026;]</klr>idoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        li tute perdas parol<tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>G. O. &Ograve;sh&oacute;-Davies</aut>
          <vrk><url ref="&Monato;/2012/010385.php">La
          perfekta edzino</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kajo povas servi al &scirc;ipo kun la <ind>kargo<tld/>o</ind> de
        400 mil toneloj.
        <trd lng="de">Tragkraft</trd>
        <trd lng="nl">laadvermogen</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_d;&c_a;&c_t;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>capacitat</trd>,
    <trd>aptitud</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>schopnost</trd>,
    <trd>zdatnost</trd>,
    <trd>zp&uring;sobilost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>F&auml;higkeit</trd>,
    <trd>Fertigkeit</trd>,
    <trd>Kompetenz</trd>
  </trdgrp>
  <trd lng="es">capacidad</trd>
  <trdgrp lng="fr">
    <trd>aptitude</trd>,
    <trd>capacit&eacute;<klr tip="ind"> (aptitude)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;pess&eacute;g</trd>,
    <trd>tud&aacute;s</trd>
  </trdgrp>
  <trd lng="ne">&#x938;&#x940;&#x92A;</trd>
  <trdgrp lng="nl">
    <trd>bekwaamheid</trd>,
    <trd>capaciteit<klr tip="ind"> (bekwaamheid)</klr></trd>,
    <trd>vermogen<klr tip="ind"> (bekwaamheid)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dar <klr> (zdolno&sacute;&cacute;)</klr> </trd> 
    <trd>umiej&eogonek;tno&sacute;&cacute;</trd>,
    <trd>zdolno&sacute;&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>&icirc;ndem&acirc;nare</trd>,
    <trd>capabilitate</trd>
  </trdgrp>  
  <trd lng="ru">&c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">schopnos&tcaron;</trd>
  <trd lng="tok">ken</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x434;&#x430;&#x442;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x441;&#x43F;&#x440;&#x43E;&#x43C;&#x43E;&#x436;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x437;&#x434;&#x456;&#x431;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x437;&#x434;&#x456;&#x431;&#x43D;&#x43E;&#x441;&#x442;&#x456;</trd>
