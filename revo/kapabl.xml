    <trd>zdatnost</trd>,
    <trd>zp&uring;sobilost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>F&auml;higkeit</trd>,
    <trd>Fertigkeit</trd>,
    <trd>Kompetenz</trd>,
    <trd>T&uuml;chtigkeit</trd>
  </trdgrp>
  <trd lng="es">capacidad</trd>
