<drv mrk="kapabl.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="kapabl.0o"><tld lit="K"/>o</ref>:
      <ekz>
        <tld/>eco por desegnado
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la okuloj atestis pri lia sa&gcirc;eco kaj lia <tld/>eco
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj
          filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        al &ccirc;iu la&ubreve; lia <tld/>eco
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 25:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        batal<tld/>eco  
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 26:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>eco fordoni sin en la manojn de virino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>eco
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
