<drv mrk="kapabl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a je io:
      <ekz>
        &scirc;i ne <tld/>is labori
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        neniu <tld/>is malsova&gcirc;igi lin
        <fnt>
          <bib>NT</bib>
          <lok>Marko 5:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiom da viaj samtempuloj <tld/>as vivi senle&gcirc;e
        <fnt><bib>Lanti</bib>, <lok>publika letero al Romen
        Rolan</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_z;&c_d;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_z;&c_d;&c_a;&c_t;&c_n;&c_y;&c_m;</ind></trd>,
    <trd>&c_m;&c_a;&c_g;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>poder</trd>,
    <trd><ind>ser</ind> capa&ccedil; <ind>de</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t s to</trd>,
    <trd>b&yacute;t schopen</trd>,
    <trd>dok&aacute;zat</trd>,
    <trd>dov&eacute;st</trd>,
    <trd>um&ecaron;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>k&ouml;nnen</trd>,
    <trd>f&auml;hig sein (zu)</trd>,
    <trd>bereit sein</trd>
  </trdgrp>
  <trd lng="es">ser capaz</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>capable</ind> de</trd>,
    <trd>&ecirc;tre <ind>apte</ind> &agrave;</trd>
  </trdgrp>
  <trd lng="hu">k&eacute;pes <klr tip="ind">(vmire)</klr></trd>
  <trdgrp lng="nl">
    <trd>kunnen</trd>,
    <trd>in <ind>staat</ind> zijn</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>m&oacute;c</trd>,
    <trd>potrafi&cacute;</trd>,
    <trd>umie&cacute;</trd>,
    <trd>zdo&lstroke;a&cacute;</trd>,
    <trd>by&cacute; w stanie</trd>,
    <trd>by&cacute; zdolnym do</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>putea</trd>,
    <trd>fi capabil sau apt</trd>,
    <trd>s&abreve; fie &icirc;n m&abreve;sur&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_y;&c_t;&c_mol; &c_s;&c_p;&c_o;&c_s;&c_o;&c_b;&c_n;&c_y;&c_m; <klr>(&c_n;&c_a; &c_ch;&c_t;&c_o;-&c_l;., &c_k;
  &c_ch;&c_je;&c_m;&c_u;-&c_l;.)</klr></trd>
  <trd lng="sk">by&tcaron; schopn&yacute;</trd>
  <trd lng="tok">ken</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x442;&#x438; &#x437;&#x434;&#x430;&#x442;&#x43D;&#x438;&#x43C;/&#x441;&#x43F;&#x440;&#x43E;&#x43C;&#x43E;&#x436;&#x43D;&#x438;&#x43C;/&#x433;&#x43E;&#x434;&#x43D;&#x438;&#x43C; (&#x434;&#x43E; &#x447;&#x43E;&#x433;&#x43E;-&#x43D;.</trd>,
    <trd>&#x43D;&#x430; &#x449;&#x43E;-&#x43D;.)</trd>,
    <trd>&#x432;&#x438;&#x44F;&#x432;&#x43B;&#x44F;&#x442;&#x438; &#x437;&#x434;&#x456;&#x431;&#x43D;&#x43E;&#x441;&#x442;&#x456;</trd>
  </trdgrp>
