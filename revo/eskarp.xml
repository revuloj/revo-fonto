<art mrk="$Id: eskarp.xml,v 1.15 2023/10/14 15:49:05 revo Exp $">
<kap>
  <rad>eskarp</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="eskarp.0o">
  <kap><tld/>o</kap>
  <snc mrk="eskarp.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Interna tera a&ubreve; masona&jcirc;a deklivo de fosa&jcirc;o
      a&ubreve; fortika&jcirc;o:
      <ekz>
        li sukcese glitkrablis super la <tld/>o
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_k;&c_a;&c_r;&c_p;</trd>
  <trd lng="cs">eskarpa</trd>
  <trd lng="de">innere Grabenb&ouml;schung</trd>
  <trd lng="fr">escarpe</trd>
  <trd lng="ja">&#x5185;&#x5CB8;  <pr>&#x306A;&#x3044;&#x304D;&#x3057;</pr></trd>
  <trdgrp lng="pl">
    <trd>skarpa</trd>,
    <trd>&sacute;ciana wykopu</trd>
  </trdgrp>
  <trd lng="pt">escarpa</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_i;&c_j; &c_o;&c_t;&c_k;&c_o;&c_s;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_e;&c_s;&c_k;&c_a;&c_r;&c_p;</trd>  
  </trdgrp>
  <trd lng="sk">eskarpa</trd>
  <trdgrp lng="uk">
    <trd>&#x435;&#x441;&#x43A;&#x430;&#x440;&#x43F;</trd>,
    <trd>&#x432;&#x43D;&#x443;&#x442;&#x440;&#x456;&#x448;&#x43D;&#x44F; &#x441;&#x43F;&#x430;&#x434;&#x438;&#x43D;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="eskarp.kontraux0o">
  <kap>kontra&ubreve;<tld/>o</kap>
  <snc mrk="eskarp.kontraux0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      La ekstera deklivo de fosa&jcirc;o a&ubreve; fortika&jcirc;o.  
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_t;&c_a;&c_r;&c_e;&c_s;&c_k;&c_a;&c_r;&c_p;</trd>
  <trd lng="de">&auml;u&szlig;ere Grabenb&ouml;schung</trd>
  <trd lng="fr">contrescarpe</trd>
  <trd lng="pt">contra-escarpa</trd>
</drv>
</art>
