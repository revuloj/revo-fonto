<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: eskarp.xml,v 1.16 2025/01/14 06:33:16 revo Exp $">
<kap>
  <rad>eskarp</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="eskarp.0o">
  <kap><tld/>o</kap>
  <snc mrk="eskarp.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Interna tera a&ubreve; masona&jcirc;a deklivo de fosa&jcirc;o
      a&ubreve; fortika&jcirc;o:
      <ekz>
        li sukcese glitkrablis super la <tld/>o
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj krutaj alta&jcirc;oj, simile al <tld/>oj <klr>[&#8230;]</klr> faras profundan kanjonon
        en la koro de la financa kvartalo de San-Francisko
        <fnt>
          <bib>Monato</bib>
          <aut>Daniel L. Mason</aut>
          <vrk><url ref="&Monato;/2011/010276.php"
              >Matena kafo</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_s;&c_k;&c_a;&c_r;&c_p;</trd>
  <trd lng="cs">eskarpa</trd>
  <trdgrp lng="de">
    <trd>innere Grabenb&ouml;schung</trd>,
    <trd>Eskarpe</trd>,
    <trd>B&ouml;schung <klr>(Festungsgraben)</klr></trd>
  </trdgrp>
  <trd lng="fr">escarpe</trd>
  <trd lng="ja">&#20869;&#23736;  <pr>&#12394;&#12356;&#12365;&#12375;</pr></trd>
  <trdgrp lng="pl">
    <trd>skarpa</trd>,
    <trd>&sacute;ciana wykopu</trd>
  </trdgrp>
  <trd lng="pt">escarpa</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_n;&c_u;&c_t;&c_r;&c_je;&c_n;&c_n;&c_i;&c_j; &c_o;&c_t;&c_k;&c_o;&c_s;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_e;&c_s;&c_k;&c_a;&c_r;&c_p;</trd>
  </trdgrp>
  <trd lng="sk">eskarpa</trd>
  <trdgrp lng="uk">
    <trd>&c_je;&c_s;&c_k;&c_a;&c_r;&c_p;</trd>,
    <trd>&c_v;&c_n;&c_u;&c_t;&c_r;&c_ib;&c_sh;&c_n;&c_ja; &c_s;&c_p;&c_a;&c_d;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="eskarp.kontraux0o">
  <kap>kontra&ubreve;<tld/>o</kap>
  <snc mrk="eskarp.kontraux0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      La ekstera deklivo de fosa&jcirc;o a&ubreve; fortika&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_t;&c_a;&c_r;&c_e;&c_s;&c_k;&c_a;&c_r;&c_p;</trd>
  <trd lng="de">&auml;u&szlig;ere Grabenb&ouml;schung</trd>
  <trd lng="fr">contrescarpe</trd>
  <trd lng="pt">contra-escarpa</trd>
</drv>
</art>
