<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>alo</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="alo.0o">
  <kap><tld/>o</kap>
  <snc mrk="alo.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Suka vegeta&jcirc;o kun baza ringo de dikaj folioj,
      kun plejmultaj specioj en Afriko,
      genro el familio <ref tip="super" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Aloe</trd>):
      <ekz>
        diversaj <tld/>oj, kun raketformaj bran&ccirc;oj &scirc;ar&gcirc;ataj de
        flavru&gcirc;e 
        striataj floroj <klr>...</klr> superis la nigrajn kapojn de la 
        &scirc;tonegoj
        <fnt>
          <aut>H. Bernardin de Saint-Pierre, trad. H. Hodler</aut>,
          <vrk><url ref="http://www.luin.se/inko/180-1.pdf">
          Pa&ubreve;lo kaj Virginio</url></vrk>,
          <lok>1905</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_mol;&c_v;&c_a;&c_s;</trd>,
    <trd>&c_a;&c_l;&c_mol;&c_ja;&c_s;</trd>
  </trdgrp>
  <trd lng="bg">&c_a;&c_l;&c_o;&c_je;</trd>
  <trd lng="de">Aloe</trd>
  <trd lng="en">aloe</trd>
  <trd lng="fr">alo&egrave;s</trd>
  <trd lng="hu">alo&eacute;</trd>
  <trd lng="nl">alo&euml;</trd>
  <trdgrp lng="pt">
    <trd>babosa</trd>,
    <trd>alo&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_o;&c_e;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_l;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">aloe</trd>
  <trd lng="sv">aloe</trd>
</drv>

<drv mrk="alo.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="alo.0ajxo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Laksiga rezino, ricevata el folioj de iuj specioj de <tld/>o.
    </dif>
  </snc>
</drv>
</art>
