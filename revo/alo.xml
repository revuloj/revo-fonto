<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>4</ofc>
      <rad>alo</rad>/o
      <fnt>Z</fnt>
    </kap>
    <drv mrk="alo.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">BOT</uzo>
      <snc>
        <dif>
          Genro el familio <ref tip="super" cel="liliac.0oj">liliacoj</ref>
          (<trd lng="la">Aloe</trd>), la suko el la folioj de kelkaj tropikaj
          specioj estas laksiga.
        </dif>
      </snc>
      <trd lng="de">Aloe</trd>
      <trd lng="en">aloe</trd>
      <trd lng="fr">alo&egrave;s</trd>
      <trd lng="nl">alo&euml;</trd>
      <trdgrp lng="pt">
        <trd>babosa</trd>,
        <trd>alo&eacute;s</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_a;&c_l;&c_o;&c_e;</trd>,
        <trd>&c_s;&c_t;&c_o;&c_l;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>
      </trdgrp>
      <trd lng="sv">aloe</trd>
    </drv>

    <drv mrk="alo.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
        <dif>
          Laksiga rezino, ricevata el folioj de iuj specioj de <tld/>o.
        </dif>
      </snc>
    </drv>

  </art>
