<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fuk.xml,v 1.14 2019/06/30 07:41:50 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>fuk</rad>/o
</kap>
<drv mrk="fuk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      de maraj brun<ref tip="super" cel="alg.0oj">algoj</ref>
      (<trd lng="la">Fucus</trd>):
      <ekz>
        la seki&gcirc;intaj skeletoj de fi&scirc;oj, la velkseki&gcirc;intaj akvaj kreska&jcirc;oj, la <tld/>o,
        blanke lumanta, longa kaj mallar&gcirc;a kiel tolaj rubandoj, &ccirc;io estis por la
        okuloj kaj pensoj ludo kaj plezuro
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la agroj estas fekundigataj per <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>William Simcock</aut>
          <vrk><url ref="https://www.monato.be/2003/007873.html"
              >Impresoj pri Portugalio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        strioj a&ubreve;gurantaj malbonon pendis kiel sangaj mar<tld/>oj malsupren sur la
        horizonton
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>En la Lando de la Noktuloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/d/d3/Fucus_vesiculosus_at_Lyngen_fjord%2C_Troms%2C_Norway%2C_2015_September.jpg/640px-Fucus_vesiculosus_at_Lyngen_fjord%2C_Troms%2C_Norway%2C_2015_September.jpg"
      prm="CC BY-SA 4.0"
      ><tld/>oj (Fucus vesiculosus) &ccirc;e fjordo Lyngen, Troms, Norvegujo
        <fnt>
          <aut>Ximonic (Simo R&auml;s&auml;nen), propra verko</aut>
          <lok><url ref="&WCW;/File:Fucus_vesiculosus_at_Lyngen_fjord,_Troms,_Norway,_2015_September.jpg"
              >https://commons.wikimedia.org/wiki/File:Fucus_vesiculosus_at_Lyngen_fjord,_Troms,_Norway,_2015_September.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="cs">chaluha</trd>
  <trdgrp lng="de">
    <trd>Blasentang</trd>,
    <trd>S&auml;getang</trd> k.a.
  </trdgrp>
  <trd lng="fr">fucus</trd>
  <trdgrp lng="hu">
    <trd>fukusz</trd>,
    <trd>tengeri moszat</trd>,
    <trd>tengeri f&udblac;</trd>
  </trdgrp>
  <trd lng="pt">fuco</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_u;&c_k;&c_u;&c_s;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_a;&c_ja;
&c_v;&c_o;&c_d;&c_o;&c_r;&c_o;&c_s;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">chaluha</trd>
</drv>
</art>
