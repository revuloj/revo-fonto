<art mrk="$Id$">
<kap>
  <rad>nomes</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="nomes.0o">
  <kap><tld/>o</kap>
  <snc mrk="nomes.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="sin" cel="provin.0o.POL">Provinco</ref> en antikva Egiptujo:
      <ekz>
        la princo <klr>...</klr> foriris en la &ccirc;efurbon de la <tld/>o Sohem
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">nome<klr tip="amb"> (&eacute;gypte ancienne)</klr></trd>
  <trd lng="hu">nomosz</trd>
  <trd lng="ru">&c_n;&c_o;&c_m;</trd>
</drv>
</art>

