<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ustilag.xml,v 1.3 2023/02/27 14:30:38 afido Exp $">
<kap>
  <rad>ustilag</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="ustilag.0o">
  <kap><tld/>o</kap>
  <snc mrk="ustilag.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:fungoj" cel="fung.0o.BIO">Fungo</ref>, 
      kiu parazitas vegeta&jcirc;ojn, precipe poacojn,
      nigrigante kaj pulvorigante la atakatan &ccirc;elaron;
      iafoje anka&ubreve; nomata fulgofungo;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref>
      <trd lng="la">Ustilago</trd>:
      <ekz>
        nigra <tld/>o mortigis &ccirc;iom de la greno
        <fnt>
          <aut>J. Ruskin, trad. I. Kellermann</aut>
          <vrk>La Re&gcirc;o de la Ora Rivero</vrk>
          <lok>1926</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ergot.0fungo">ergotfungo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Brandpilz</trd>,
    <trd>Getreidebrand</trd>
  </trdgrp>
  <trd lng="fr">charbon<klr> (maladie v&eacute;g&eacute;tale)</klr></trd>
</drv>
</art>

