<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>hangar</rad>/o
</kap>
<drv mrk="hangar.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">AVI</uzo>
  <snc>
    <dif>
      Granda konstrua&jcirc;o, ta&ubreve;ga por &scirc;irmi kaj enteni direkteblajn balonojn,
      aviadilojn ks.
    </dif>
    <ref tip="super" cel="remiz.0o">remizo.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Hangar</trd>,
    <trd>Flughalle</trd>
  </trdgrp>
  <trd lng="el">&ypsilon;&pi;&omikron_ton;&sigma;&tau;&epsilon;&gamma;&omikron;</trd>
  <trd lng="en">hangar</trd>
  <trd lng="fr">hangar</trd>
  <trdgrp lng="nl">
    <trd>hangar</trd>,
    <trd>loods<klr tip="ind"> (hangar)</klr>
    </trd>
  </trdgrp>
  <trd lng="pt">hangar</trd>
  <trd lng="ru">&c_a;&c_n;&c_g;&c_a;&c_r;</trd>
  <trd lng="sv">hangar</trd>
</drv>
</art>
