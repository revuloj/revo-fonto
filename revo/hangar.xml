<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>hangar</rad>/o
</kap>
<drv mrk="hangar.0o">
  <kap><tld/>o</kap>
  <snc mrk="hangar.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Granda <ref tip="super" cel="konstr.0ajxo.domo">konstrua&jcirc;o</ref>,
      ta&ubreve;ga por &scirc;irmi kaj enteni balonojn, aviadilojn:
      <ekz>
        ili eliris kaj haltis apud muro de la <tld/>o
        <fnt>
        <aut>Lubovj Lukina, E&ubreve;geno Lukin, trad. Jurij Finkel</aut>,
        <vrk>La misiistoj</vrk>,
        <lok><url ref="http://www.esperanto.mv.ru/Lukin/mis2.html">parto
        2a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="remiz.0o">remizo</ref>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_g;&c_a;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Hangar</trd>,
    <trd>Flughalle</trd>
  </trdgrp>
  <trd lng="el">&ypsilon;&pi;&omikron_ton;&sigma;&tau;&epsilon;&gamma;&omikron;</trd>
  <trd lng="en">hangar</trd>
  <trd lng="es">hangar</trd>
  <trd lng="fr">hangar</trd>
  <trd lng="hu">hang&aacute;r</trd>
  <trdgrp lng="nl">
    <trd>hangar</trd>,
    <trd>loods<klr tip="ind"> (hangar)</klr>
    </trd>
  </trdgrp>
  <trd lng="pt">hangar</trd>
  <trd lng="ru">&c_a;&c_n;&c_g;&c_a;&c_r;</trd>
  <trd lng="sv">hangar</trd>
</drv>
</art>
