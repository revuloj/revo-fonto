<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>preska&ubreve;</rad></kap>
<drv mrk="preska.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Adv. signifanta &leftquot;ne tute, sed proksimume&rightquot;:
      <ekz>
        li estas <tld/> mia frato: nepo de onklo de kuzo de
        konato<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/> &ccirc;iuj;
      </ekz>
      <ekz>
        li estis <tld/> dis&scirc;irita de la popolo;
      </ekz>
      <ekz>
        ili estas <tld/> sama&gcirc;aj;
      </ekz>
      <ekz>
        estis <tld/> nokto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rand.0o">rando</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>fast</trd>,
     <trd>beinahe</trd>,
     <trd>nahezu</trd>
  </trdgrp>
  <trd lng="en">almost</trd>
  <trd lng="es">casi</trd>
  <trd lng="fr">presque</trd>
  <trd lng="he">&kaf;&mem;&ayin;&tet;</trd>
  <trd lng="nl">bijna</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_ch;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_ch;&c_t;&c_i; &c_ch;&c_t;&c_o;</trd>,
    <trd>&c_ch;&c_u;&c_t;&c_mol; &c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">n&auml;stan</trd>
  <trd lng="vo">ti</trd>
</drv>
</art>
