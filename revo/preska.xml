<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: preska.xml,v 1.28 2021/05/28 12:15:17 revo Exp $">
<kap><ofc>*</ofc> <rad>preska&ubreve;</rad></kap>
<drv mrk="preska.0">
   <kap><ofc>*</ofc><tld/></kap>
   <gra><vspec>adv.</vspec></gra>
   <snc mrk="preska.0.proksime">
     <dif>
      Ne <ref cel="tut.0e">tute</ref>, sed 
      <ref tip="sin" cel="proksi.0ume">proksimume</ref>:
      <ekz>
        li <tld/> ne povis iri
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis malsana kaj <tld/> mortanta
        <fnt>
          <bib>NT</bib>
          <lok>Luko 7:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        efektive li malsanis, <tld/> &gcirc;is morto
        <fnt>
          <bib>NT</bib>
          <lok>Filipianoj 2:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> dum la tuta nokto mi ne povis fermi la okulojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Re&gcirc;idino sur pizo</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/> la tuta urbo kolekti&gcirc;is, por a&ubreve;di la vorton de Dio
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 13:44</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kompatindajn &ccirc;evalojn oni <tld/> ne povis plu rekoni <klr>[&ccirc;ar]</klr> amasege
        sidis sur ili la mu&scirc;oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia laboro estis ja <tld/> finita
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i da&ubreve;rigis <tld/> plorante
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la tegmento <tld/> tu&scirc;is la teron
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis travoja&gcirc;inta <tld/> la tutan mondon kaj povosciis rakonti pri &ccirc;iuj
        eblaj aferoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
       <ekz>
         <klr>(ironie)</klr> li estas <tld/> mia frato: nepo de onklo de kuzo de
         konato
        <fnt>
          <bib>PrV</bib>
        </fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="ankora.0">ankora&ubreve;</ref> (ne),
       <ref cel="procen.cent0e">centprocente</ref>,
       (ne) <ref cel="jam.0">jam</ref>,
       <ref cel="rand.0o">rando</ref>
     </refgrp>
     <ref tip="ant" cel="apenau.0.preskau_ne">apena&ubreve;<sncref/></ref>
   </snc>
   <trd lng="be">&c_a;&c_m;&c_a;&c_l;&c_mol;</trd>
   <trd lng="bg">&c_p;&c_o;&c_ch;&c_t;&c_i;</trd>
   <trd lng="bo">&#xF55;&#xF63;&#xF0B;&#xF46;&#xF7A;&#xF62;&#xF0B;</trd>
  <trdgrp lng="ca">
     <trd>quasi</trd>,
     <trd>gaireb&eacute;</trd>
   </trdgrp>
   <trdgrp lng="de">
      <trd>fast</trd>,
      <trd>beinahe</trd>,
      <trd>nahezu</trd>
   </trdgrp>
   <trdgrp lng="en">
    <trd>almost</trd>,
    <trd>nearly</trd>,
    <trd>not <ind>yet</ind></trd>
  </trdgrp>
   <trd lng="es">casi</trd>
   <trdgrp lng="fi">
     <trd>melkein</trd>,
     <trd>l&auml;hes</trd>,
     <trd>miltei</trd>
   </trdgrp>
   <trd lng="fr">presque</trd>
   <trd lng="he">&kaf;&mem;&ayin;&tet;</trd>
   <trdgrp lng="hu">
     <trd>majdnem</trd>,
     <trd>szinte</trd>
   </trdgrp>
   <trd lng="id">hampir</trd>
   <trd lng="nl">bijna</trd>
   <trd lng="ms">hampir</trd>
   <trd lng="oc">quasi, gaireben</trd>
   <trdgrp lng="pl">
    <trd>bez ma&lstroke;a</trd>,
    <trd>niemal</trd>,
    <trd>prawie</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>quase</trd>,
     <trd>perto de</trd>,
     <trd>por <ind>pouco</ind></trd>
   </trdgrp>
   <trd lng="ro">de <ind>aproape</ind></trd>
   <trdgrp lng="ru">
     <trd>&c_p;&c_o;&c_ch;&c_t;&c_i;</trd>,
     <trd>&c_p;&c_o;&c_ch;&c_t;&c_i; &c_ch;&c_t;&c_o;</trd>,
     <trd>&c_ch;&c_u;&c_t;&c_mol; &c_n;&c_je;</trd>
   </trdgrp>
   <trd lng="sv">n&auml;stan</trd>
   <trd lng="vo">ti</trd>
   <trd lng="zh">&#x51E0;&#x4E4E; [<ind>j&imacron;h&umacron;</ind>]</trd>
</drv>
</art>
