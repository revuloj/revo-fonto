<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>flameng</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="flameng.0o">
  <kap><tld/>o</kap>
  <snc mrk="flameng.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="feniko.0o.ZOO">fenikoptero</ref>
      <ekz>
        vi malkovros la kastelojn de la katara lando, sed anka&ubreve; 
        <klr>...</klr> la rozajn <tld/>ojn, kiuj vivas en la lagunoj 
        de nia marbordo
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://mescladis.free.fr/katara-lando.htm">
          Aude - la lando de la kataroj</url></vrk>,
          <lok>[vidita en 2007-03]</lok>
        </fnt>;
      </ekz>
      <ekz>
        plenkreskaj <tld/>oj havas rozkolorajn a&ubreve; ru&gcirc;ajn plumojn
        <fnt><bib>Viki</bib>, <lok><url
        ref="http://eo.wikipedia.org/wiki/Flamengo">artikolo
        &leftquot;flamengo&rightquot;</url>, 
        2008-01-12</lok></fnt>.
      </ekz>
  </snc>
  <trd lng="bg">&c_f;&c_l;&c_a;&c_m;&c_i;&c_n;&c_g;&c_o;</trd>
  <trd lng="de">Flamingo</trd>
  <trd lng="fa">&a_f;&a_l;&a_A;&a_m;&f_y;&a_n;&f_g;&a_w;</trd>
  <trd lng="fr">flamant</trd>
</drv>
</art>

