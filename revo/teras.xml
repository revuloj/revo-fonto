<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>teras</rad>/o
</kap>
<drv mrk="teras.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Alta ebena teramaso subtenita per masona&jcirc;o kaj aran&gcirc;ita en &gcirc;ardeno
      por &gcirc;ui belvidon a&ubreve; faciligi promenadon:
      <ekz>
        anta&ubreve; la kastelo etendi&gcirc;as vasta <tld/>o
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="altan.0o">altano</ref>,
      <ref cel="belved.0o">belvedero</ref>,
      <ref cel="balkon.0o">balkono</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Ebena tegmento kun balustrado.
    </dif>
  </snc>
  <trd lng="de">Terrasse</trd>
  <trd lng="nl">terras</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_r;&c_a;&c_s;&c_a;</trd>
</drv>

<drv mrk="teras.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Formanta <tld/>ojn:
      <ekz>
        la urbo sin levas <tld/>e
        <klr>(&scirc;tupare)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="amfite.0o">amfiteatro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">terrassenf&ouml;rmig</trd>
  <trd lng="nl">terrasvormig</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
