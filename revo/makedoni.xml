<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Litovi</rad>/o
</kap>

<drv mrk="makedoni.M0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="makedoni.M0o.RESPUBLIKO">
    <dif>
      <ekz>
        <tld lit="M"/>o estas sendependa respubliko anka&ubreve; konata kiel 
        Iama Jugoslavia Respubliko Makedonio.
        <fnt><bib>Viki</bib>, <lok><url        
        ref="http://eo.wikipedia.org/wiki/Respubliko_de_Makedonio">Respubliko Makedonio</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="makedoni.M0o.GREKA_REGIONO">
    <dif>
      <ekz>
        <tld lit="M"/>o estas kultura landoparto de Grekio.
        <fnt><bib>Viki</bib>, <lok><url        
        ref="http://eo.wikipedia.org/wiki/Makedonio_%28Grekio%29">Makedonio (Grekio)</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_M;&c_a;&c_k;&c_je;&c_d;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="de">Mazedonien</trd>
  <trd lng="en">Macedonia</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>