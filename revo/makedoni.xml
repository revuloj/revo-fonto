<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Makedoni</rad>/o
</kap>

<drv mrk="makedoni.M0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
  <snc mrk="makedoni.M0o.respubliko">
    <dif>
      <ref tip="dif" cel="makedon.M0ujo.GEOG">Makedonujo<sncref/></ref>:
      <ekz>
        <tld lit="M"/>o estas sendependa respubliko anka&ubreve; konata
        kiel Iama Jugoslavia Respubliko Makedonio
        <fnt><bib>Viki</bib>, <lok><url        
        ref="http://eo.wikipedia.org/wiki/Respubliko_de_Makedonio">Respubliko
        Makedonio</url></lok></fnt>;
      </ekz>
      <ekz>
        pro malobservoj de la le&gcirc;o, &ccirc;efe en regionoj lo&gcirc;ataj de albanoj,
        la supera tribunalo de <tld/>o decidis ripetigi la balotojn en 230 balotsekcioj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006818.html">Balkanio: Nova &scirc;anco por &ccirc;iuj etnoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la altrangaj vizitontoj devis flugi el la &ccirc;efurbo de <tld/>o &dash; <nom>Skopje</nom> &dash;
        al <nom>Pri&scirc;tina</nom> per helikoptero, sed pro la risko de pripafo la flugo estis prokrastita je kvin horoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007007.html">Jugoslavio: Atenco ne preteratentita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pro malkonsentoj inter <tld/>o kaj Grekio, rilate la nomon de la unua, <tld/>o ne akcepti&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="http://www.monato.net/2008/009481.php">Sukcesas du el tri</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>Maced&ograve;nia</trd>,
      <trd>Rep&uacute;blica de Maced&ograve;nia</trd>
    </trdgrp>
    <trd lng="de">Mazedonien</trd>
    <trd lng="fr">Ancienne R&eacute;publique Yougoslave de Mac&eacute;doine</trd>
    <trd lng="pl">Macedonia</trd>
  </snc>
  <snc mrk="makedoni.M0o.greka_regiono">
    <dif>
      Regiono en la nordo de <ref tip="malprt" cel="grek.G0ujo">Grekujo</ref>:
      <ekz>
        <tld lit="M"/>o estas kultura landoparto de Grekio
        <fnt><bib>Viki</bib>, <lok><url        
        ref="http://eo.wikipedia.org/wiki/Makedonio_%28Grekio%29">Makedonio
        (Grekio)</url></lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="makedon.M0ujo.historie">Makedonujo<sncref/></ref>
    <trd lng="ca">Maced&ograve;nia</trd>
    <trd lng="de">Makedonien</trd>
    <trd lng="fr">Mac&eacute;doine</trd>
    <trd lng="pl">Macedonia</trd>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_k;&c_je;&c_d;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_M;&c_a;&c_k;&c_je;&c_d;&c_o;&c_n;&c_i;&c_ja;</trd>
  <trd lng="cs">Makedonie</trd>
  <trd lng="en">Macedonia</trd>
  <trd lng="fa">&a_m;&a_q;&a_d;&a_w;&a_n;&f_y;&a_h;</trd>
  <trd lng="fr">Mac&eacute;doine</trd>
  <trd lng="id">Makedonia</trd>
  <trd lng="nl">Macedoni&euml;</trd>
  <trd lng="sk">Maced&oacute;nsko</trd>
</drv>
</art>

