<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fuel.xml,v 1.21 2026/01/29 22:43:21 revo Exp $">
<kap>
  <rad>fuel</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="fuel.0o">
  <kap><tld/>o</kap>
  <snc mrk="fuel.0o.ENE">
    <dif>
      <ref tip="dif" cel="brul.0igajxo.KOMUNE">Bruliga&jcirc;o</ref>:
      <ekz>
        centoj da tunoj da dizela <tld/>o
        <fnt>
          <aut>L. S&ouml;z&uuml;er</aut>,
          <vrk>balenoj surmarbordi&gcirc;is</vrk>,
          <lok><url ref="&SCE;">soc.culture.esperanto</url>,
          1998-11-11</lok>
        </fnt>;
      </ekz>
      <ekz>
        la prezo de petrolo kaj sekve de <tld/>oj alti&gcirc;as
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2006/008809.php">Litovio volas resti nukle-energia
          &scirc;tato</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        mult<tld/>a motoro-generatoro
        <fnt>
          <bib>Monato</bib>
          <aut>Tam&aacute;s Slez&aacute;k</aut>
          <vrk><url ref="&Monato;/2008/009631.php">Pli ol a&ubreve;to: perspektivo kaj
          sinteno</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povas uzi la lignon anka&ubreve; por gajni energion kaj produkti likvajn <tld/>ojn
        a&ubreve; aliajn kemia&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fu&szlig;</aut>
          <vrk><url ref="&Monato;/2011/010273.php">Verdigu la dezertojn!</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="karbur.0ajxo">karbura&jcirc;o</ref>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>brula&jcirc;o</ke> (por motoroj),
    <ke>brulmaterialo</ke>, <ke>energifonto</ke>.
  </rim>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;</trd>
  <trd lng="bg">&c_g;&c_o;&c_r;&c_i;&c_v;&c_o;</trd>
  <trd lng="ca">combustible</trd>
  <trdgrp lng="cs">
    <trd>palivo</trd>,
    <trd>pohonn&aacute; hmota</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Treibstoff</trd>,
    <trd>Kraftstoff</trd>
  </trdgrp>
  <trd lng="en">fuel</trd>
  <trd lng="es">combustible</trd>
  <trdgrp lng="fr">
    <trd>combustible</trd>,
    <trd>fioul</trd>,
    <trd>fuel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hajt&oacute;anyag</trd>,
    <trd>&uuml;zemanyag</trd>,
    <trd>t&uuml;zel&odblac;anyag</trd>
  </trdgrp>
  <trd lng="nl">brandstof</trd>
  <trdgrp lng="pl">
    <trd>opa&lstroke;</trd>,
    <trd>paliwo</trd>
  </trdgrp>
  <trd lng="pt">combust&iacute;vel</trd>
  <trdgrp lng="sk">
    <trd>palivo</trd>,
    <trd>pohonn&aacute; l&aacute;tka</trd>
  </trdgrp>
  <trd lng="sv">br&auml;nsle</trd>
  <trd lng="uk">&c_p;&c_a;&c_l;&c_i;&c_v;&c_o;</trd>
  <trdgrp lng="zh">
    <trd>&#x52A8;&#x529B;&#x71C3;&#x6599; <pr>d&ograve;ngl&igrave;r&aacute;nli&agrave;o</pr></trd>,
    <trd>&#x71C3;&#x6599; <pr>r&aacute;nli&agrave;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fuel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fuel.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="proviz.0i.havigi">Provizi</ref> per
      bruliga&jcirc;o, konsuma&jcirc;o, energio:
      <ekz>
        a&ubreve;tobuso <tld/>ita per vegeta&jcirc;a oleo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=802235"
              >Alternativa fuelo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li diris, ke la eksterlanda politiko de la okcidentaj landoj
        &leftquot;<tld/>is la islaman radikalismon&rightquot;
        <klr>(kp <ref cel="favor.0i">favori</ref>, <ref cel="dorlot.0i.indulgi">dorloti<sncref/></ref>)</klr>
        <fnt>
          <aut>R. Peres-Franko</aut>,
          <vrk>Deklaracio de londona urbestro...</vrk>,
          <lok><url ref="http://raporto.info">Raporto.info</url>, 2005-07-20</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="benzin.0umi">benzinumi</ref>,
      <ref cel="sxarg.0i.aparaton">&scirc;argi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; <klr>(&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;&c_m;)</klr></trd>
  <trdgrp lng="ca">
    <trd>inflamar</trd>,
    <trd>atiar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>tanken</trd>,
    <trd>betanken</trd>,
    <trd>befeuern</trd>,
    <trd>sch&uuml;ren <klr>(Konflikt)</klr></trd>
  </trdgrp>
  <trd lng="en">fuel</trd>
  <trd lng="es">hechar combustible</trd>
  <trdgrp lng="fr">
    <trd>alimenter<klr> (en &eacute;nergie etc.)</klr></trd>,
    <trd>approvisionner<klr> (en &eacute;nergie etc.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>feltankol</trd>,
    <trd>&uuml;zemanyaggal ell&aacute;t</trd>
  </trdgrp>
  <trd lng="nl">van <ind>brandstof</ind> voorzien</trd>
  <trdgrp lng="pl">
    <trd>tankowa&cacute;</trd>,
    <trd>zaopatrzy&cacute; w  <ind>paliwo</ind></trd>
  </trdgrp>
  <trd lng="pt">abastecer <klr tip="amb">(com combust&iacute;vel)</klr></trd>
</drv>
</art>

