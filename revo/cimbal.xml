<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cimbal.xml,v 1.27 2022/04/26 16:27:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>cimbal</rad>/o
</kap>

<drv mrk="cimbal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="instru2.frap0o" lst="voko:frapinstrumentoj">Frapinstrumento</ref>,
      konsistanta el du metalaj diskoj, kiujn oni frapas unu
      kontra&ubreve; alia:
      <ekz>
        eliris la virinoj el &ccirc;iuj urboj de Izrael, kantante kaj dancante, renkonte al la
        re&gcirc;o Saul, kun tamburinoj, &gcirc;ojkrioj, kaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 18:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        ludado sur kupraj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 15:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        gloru Lin per la&ubreve;taj <tld/>oj, gloru Lin per tintantaj <tld/>oj.
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 150:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        el malproksimo eksonas tintiloj, <tld/>oj, fajfiloj kaj kordoj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/2e/Cymbal_Case_with_Set_of_Cymbals_MET_DP337459.jpg/640px-Cymbal_Case_with_Set_of_Cymbals_MET_DP337459.jpg" prm="CC0">tibeta <tld/>o kun skatolo
        <fnt>
          <aut>Frederick M. Lehman Bequest</aut>
          <lok><url ref="&WCW;/File:Cymbal_Case_with_Set_of_Cymbals_MET_DP337459.jpg">&WCW;/File:Cymbal_Case_with_Set_of_Cymbals_MET_DP337459.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_m;&c_b;&c_a;&c_l;&c_y;</trd>
  <trd lng="bo">&#xF66;&#xFA6;&#xF74;&#xF42;&#xF0B;&#xF46;&#xF63;&#xF0B;</trd>
  <trd lng="br">simbalenn</trd>
  <trd lng="cs">&ccaron;inely</trd>
  <trdgrp lng="de">
    <trd>Zimbel</trd>,
    <trd>Becken <klr>(Zimbel)</klr></trd>
  </trdgrp>
  <trd lng="el">&kappa;&ypsilon_ton;&my;&beta;&alfa;&lambda;&omikron;</trd>
  <trd lng="en">cymbal</trd>
  <trd lng="es">c&iacute;mbalo</trd>
  <trd lng="fr">cymbale</trd>
  <trd lng="he">&#x5DE;&#x5E6;&#x5D9;&#x5DC;&#x5EA;&#x5D9;&#x5D9;&#x5DD;</trd>
  <trd lng="hu">cint&aacute;ny&eacute;r</trd>
  <trd lng="id">simbal</trd>
  <trdgrp lng="it">
  <trd>piatto<klr> (mus.)</klr></trd>, 
  <trd>cimbalo</trd>
  </trdgrp>
  <trd lng="pl">talerze</trd>
  <trd lng="pt">c&iacute;mbalo</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_m;&c_v;&c_a;&c_l;</trd>,
    <trd>&c_c;&c_i;&c_m;&c_b;&c_a;&c_l;&c_y;</trd>,
    <trd>&c_t;&c_a;&c_r;&c_je;&c_l;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cimbal</trd>,
    <trd>&ccaron;inely</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x456;&#x43C;&#x432;&#x430;&#x43B;</trd>
</drv>
</art>
