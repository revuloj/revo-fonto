<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>cimbal</rad>/o
</kap>
<drv mrk="cimbal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.frap0o">Frapinstrumento</ref>, konsistanta el du metalaj diskoj, kiujn oni frapas unu kontra&ubreve; alia.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_m;&c_b;&c_a;&c_l;&c_y;</trd>
  <trd lng="br">simbalenn</trd>
  <trd lng="el">&kappa;&ypsilon_ton;&my;&beta;&alfa;&lambda;&omikron;</trd>
  <trd lng="es">c&iacute;mbalo</trd>
  <trd lng="fr">cymbale</trd>
  <trd lng="hu">cint&aacute;ny&eacute;r</trd>
  <trd lng="pt">c&iacute;mbalo</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_m;&c_v;&c_a;&c_l;</trd>,
    <trd>&c_c;&c_i;&c_m;&c_b;&c_a;&c_l;&c_y;</trd>,
    <trd>&c_t;&c_a;&c_r;&c_je;&c_l;&c_k;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">cimbal</trd>
</drv>
</art>
