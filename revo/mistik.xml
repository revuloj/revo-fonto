<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>mistik</rad>/o
</kap>
<drv mrk="mistik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">REL</uzo>
  <snc mrk="mistik.0o.sento">
    <dif>
      <ref tip="malprt" cel="religi.0o">Religia</ref> sinteno, kiu sin
      apogas al imago, sentoj, internaj spertoj, ekstazo, prefere ol al
      intelekto.
    </dif>
  </snc>
  <snc mrk="mistik.0o.supersticxo">
    <uzo tip="klr">(ironie)</uzo>
    <ref tip="dif" cel="supers.0o">supersti&ccirc;o</ref>
    <ekz>
      <tld/>o fari&gcirc;as ofte mistifiko.
    </ekz>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="de">Mystik</trd>
  <trd lng="fr">mystique<klr> (attitude)</klr></trd>
  <trd lng="hu">misztika</trd>
  <trd lng="nl">mystiek</trd>
  <trd lng="ru">&c_m;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="mistik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o, al precipe sentema religia sinteno.
    </dif>
  </snc>
  <trd lng="fr">mystique<klr> (adj.)</klr></trd>
  <trd lng="hu">misztikus</trd>
  <trd lng="ru">&c_m;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="mistik.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      Religia a&ubreve; filozofia doktrino alte taksanta <tld/>an sperton,
      male ol ekzemple <ref tip="vid" cel="raci.0ismo">raciismo</ref>.
    </dif>
  </snc>
  <trd lng="fr">mysticisme</trd>
  <trd lng="hu">miszticizmus</trd>
  <trd lng="ru">&c_m;&c_i;&c_s;&c_t;&c_i;&c_c;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="mistik.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo, kiu travivis <tld/>ajn spertojn kaj ilin interpretas religie.
    </dif>
  </snc>
  <trd lng="fr">mystique<klr> (pers.)</klr></trd>
  <trd lng="hu">misztikus</trd>
  <trd lng="ru">&c_m;&c_i;&c_s;&c_t;&c_i;&c_k;</trd>
</drv>
</art>
