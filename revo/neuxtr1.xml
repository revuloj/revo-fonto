<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>ne&ubreve;tral</rad>/a
</kap>
<drv mrk="neuxtr1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ne partoprenanta en diskuto, disputo, batalo, milito:
      <ekz>
        <tld/>a lando, lingvo;
      </ekz>
      <ekz>
        respekti la rajtojn de <tld/>uloj;
      </ekz>
      <ekz>
        la <tld/>igo de la intergentaj rilatoj estas la tuta celo de niaj
        laboroj<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="parti.sen0a">senpartia</ref>,
      <ref cel="parti">superpartia</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_w;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="ca">neutral</trd>
  <trd lng="cs">neutr&aacute;ln&iacute;</trd>
  <trd 
lng="el">&omikron;&ypsilon;&delta;&epsilon_ton;&tau;&epsilon;&rho;&omikron;&sigma_fina;</trd>
  <trdgrp lng="de">
     <trd>neutral</trd>,
     <trd>unparteilich</trd>,
     <trd>unparteiisch</trd>
  </trdgrp>
  <trd lng="en">neutral</trd>
  <trd lng="es">neutral</trd>
  <trd lng="fr">neutre</trd>
  <trdgrp lng="hu">
    <trd>semleges</trd>,
    <trd>neutr&aacute;lis</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onpartijdig</trd>,
    <trd>neutraal</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>neutro</trd>,
    <trd>neutral</trd>
  </trdgrp>
  <trd 
lng="ru">&c_n;&c_je;&c_j;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
