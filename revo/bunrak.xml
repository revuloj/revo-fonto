<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bunrak</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="bunrak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Japana tradicia <ref tip="super" cel="teatr.pup0o">pupteatro</ref>
      populara ekde fino de la 16a jarcento,
      ludata kun &ccirc;anto kaj <ref tip="vid"
cel="sxamisen.0o">&scirc;amisena</ref> muziko,
      en kiu pupojn po movas tri nigre vestitaj istoj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bunraku</trd>
  </trdgrp>
</drv>
</art>

