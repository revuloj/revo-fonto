<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bunrak.xml,v 1.10 2023/10/14 08:39:48 revo Exp $">
<kap>
  <rad>bunrak</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="bunrak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Japana tradicia <ref tip="super" cel="teatr.pup0o">pupteatro</ref>
      populara ekde fino de la 16a jarcento, ludata kun &ccirc;anto kaj
      <ref tip="vid" cel="sxamisen.0o">&scirc;amisena</ref>
      muziko, en kiu pupojn movas po tri nigre vestitaj homoj:
      <ekz>
        <tld/>o estas registrita kiel la nemateria kultura hereda&jcirc;o de UNESKO en 2009
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=506458"
              >Bunrako</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_n;&c_r;&c_a;&c_k;&c_u;</trd>
  <trd lng="de">Bunraku</trd>
  <trd lng="en">bunraku</trd>
  <trd lng="es">Bunraju</trd>
  <trdgrp lng="fr">
    <trd>bunraku</trd>
  </trdgrp>
  <trd lng="hu">bunraku</trd>
  <trd lng="ja">&#25991;&#27005; <pr>&#12406;&#12435;&#12425;&#12367;</pr></trd>
  <trd lng="ru">&c_b;&c_u;&c_n;&c_r;&c_a;&c_k;&c_u;</trd>
</drv>
</art>

