<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>sektor</rad>/o
</kap>
<drv mrk="sektor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <subsnc mrk="sektor.0o.de_ebeno">
      <fnt><bib>PIV2</bib></fnt>
      <dif>
        <klr>(<tld/>o de <ref cel="eben.0o.MAT">ebeno</ref>
        a&ubreve; angula <tld/>o)</klr>
        <ref tip="dif" cel="angul.0o.figuro">Angulo</ref>.
      </dif>
      <trd lng="de">Winkelraum</trd>
      <trd lng="en">angular <ind>domain</ind></trd>
      <trd lng="fr">secteur angulaire</trd>
      <trd lng="ru">&c_u;&c_g;&c_l;&c_o;&c_v;&c_o;&c_j; <ind>&c_s;&c_je;&c_k;&c_t;&c_o;&c_r;</ind></trd>
    </subsnc>
    <subsnc mrk="sektor.0o.de_cirklo">
      <fnt><bib>MatTerm</bib><lok>p. 28</lok></fnt>
      <dif>
        <klr>(<tld/>o de <ref cel="cirkl.0o.surfaco">cirklo</ref>
        a&ubreve; cirkla <tld/>o)</klr>
        Intersekco de la cirklo kun
        <ref cel="angul.centra0o">centra angulo</ref> de &gcirc;i.
      </dif>
      <trd lng="fr">secteur circulaire</trd>
    </subsnc>
    <subsnc mrk="sektor.0o.de_sfero">
      <fnt><bib>PIV1</bib></fnt>
      <dif>
        <klr>(<tld/>o de <ref cel="sfer.0o.solido">sfero</ref>
        a&ubreve; sfera <tld/>o)</klr>
        <ref tip="super" cel="solid.0o.geometrio">Solido</ref>, naskita
        de <tld/>o de cirklo rotacianta &ccirc;irka&ubreve; diametro
        de la koncerna cirklo.
      </dif>
      <trd lng="de">Kugelsektor</trd>
      <trd lng="en">spherical <ind>sector</ind></trd>
      <trd lng="fr">secteur sph&eacute;rique</trd>
      <trd lng="ru">&c_sh;&c_a;&c_r;&c_o;&c_v;&c_o;&c_j; <ind>&c_s;&c_je;&c_k;&c_t;&c_o;&c_r;</ind></trd>
    </subsnc>
  </snc>
  <trd lng="de">Sektor</trd>
  <trd lng="fr">secteur</trd>
  <trd lng="nl">sector</trd>
  <trd lng="ru">&c_s;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
</drv>
</art>
