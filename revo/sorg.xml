<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>sorg</rad>/o
</kap>
<drv mrk="sorg.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro de man&gcirc;eblaj <ref tip="super" cel="poac.0oj">poacoj</ref>,
      origina el Afriko kaj Hindujo:
      <ekz>
        la <tld/>o povas atingi alton de kvar &gcirc;is kvin metroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">sorghum</trd>
  <trd lng="nl">gierst</trd>
  <trd lng="ru">&c_s;&c_o;&c_r;&c_g;&c_o;</trd>
</drv>
</art>
