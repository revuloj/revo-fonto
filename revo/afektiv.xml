<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: afektiv.xml,v 1.7 2019/09/14 14:10:15 revo Exp $">
<kap>
  <rad>afektiv</rad>/a <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="afektiv.0a">
  <kap><tld/>a</kap>
  <snc mrk="afektiv.0a.afekcia">
    <dif>
      <ref tip="dif" cel="afekci.0a.anima">Afekcia<sncref/></ref>:
      <ekz>
        tia sinteno, nur <tld/>a kaj neniel racia, beda&ubreve;rinde renkonti&gcirc;as
        sufi&ccirc;e ofte
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 13a, p. 40a</lok></fnt>;
      </ekz>
      <ekz>
        ili argumentas ke la termino &leftquot;samseksemo&rightquot; malhumanigas la gejojn, malpliigas
        iliajn pasiojn, iliajn <tld/>ajn rilatojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1503">Gejo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>affektiv</trd>,
      <trd>gef&uuml;hlsbezogen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>afektivn&iacute;</trd>,
    <trd>citov&yacute;</trd>
  </trdgrp>
  <trd lng="es">afectivo</trd>
  <trdgrp lng="fr">
    <trd>affectif</trd>,
    <trd>&eacute;motionnel</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>affettivo</trd>,
    <trd>emotivo</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>afekt&iacute;vny</trd>,
    <trd>citov&yacute;</trd>
  </trdgrp>
</drv>
</art>
