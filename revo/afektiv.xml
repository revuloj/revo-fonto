<art mrk="$Id$">
<kap>
  <rad>afektiv</rad>/a <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="afektiv.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="afekci.0a">afekcia</ref>
    <ekz>
      tia sinteno, nur <tld/>a kaj neniel racia, beda&ubreve;rinde renkonti&gcirc;as
      sufi&ccirc;e ofte
      <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 13a, p. 40a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="es">afectivo</trd>
  <trdgrp lng="fr">
    <trd>affectif</trd>,
    <trd>&eacute;motionnel</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>affettivo</trd>,
    <trd>emotivo</trd>
  </trdgrp>
</drv>
</art>
