<vortaro>
  <art mrk="$Id: nuaksxot.xml,v 1.7 2024/01/29 15:59:31 afido Exp $">
    <kap>
      <rad>Nuak&scirc;ot</rad>/o
    </kap>

<drv mrk="nuaksxot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="mauxrit.M0o">Ma&ubreve;ritanio</ref>:
      <ekz>
        <tld/>o estas dua la plej granda sahara urbo (post <ref cel="kair.K0o">Kairo</ref>)
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Nouakchott">Nuak&scirc;oto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Sud-Afriko, Sieraleono, Namibio, Lesoto kaj Burundo subskribis la interkonsenton
        pri liberkomerca regiono en <tld/>o
        <fnt>
          <vrk><url ref="&CRI;2521/2018/07/05/179s199816.htm">Fokusoj de la Semajno (Jul. 06)</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        dek-na&ubreve;-jara fi&scirc;isto mortpafita en januaro 2018 <klr>[&#x2026;]</klr> revigligis la
        stre&ccirc;itecon inter <ref cel="dakar.0o">Dakaro</ref> kaj <tld/>o
        <fnt>
          <aut>Zo&eacute; Lamazou</aut>
          <vrk><url ref="&Mondediplo;article2629.html">&leftquot;Guantanamo&rightquot; en Mauritanio</url></vrk>
          <lok>Le Monde diplomatique' en Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_N;&c_u;&c_a;&c_k;&c_sh;&c_o;&c_t;</trd>
    <trd lng="de">Nouakchott</trd>
    <trd lng="en">Nouakchott</trd>
    <trd lng="fr">Nouakchott</trd>
    <trd lng="pl">Nawakszut</trd>
    <trd lng="ro">Nouakchott</trd>
  </snc>
  <trd lng="ja">&#x30CC;&#x30A2;&#x30AF;&#x30B7;&#x30E7;&#x30C3;&#x30C8;</trd>
  <trd lng="uk">&#x41D;&#x443;&#x430;&#x43A;&#x448;&#x43E;&#x442; (&#x441;&#x442;&#x43E;&#x43B;&#x438;&#x446;&#x44F;)</trd>
  <trdgrp lng="zh">
    <trd>&#x8AFE;&#x514B;&#x5C11; <pr>Nu&#xF2;k&#xE8;sh&#x1CE;o</pr></trd>,
    <trd>&#x8BFA;&#x514B;&#x5C11; <pr>Nu&#xF2;k&#xE8;sh&#x1CE;o</pr></trd>,
    <trd>&#x52AA;&#x74E6;&#x514B;&#x8096;&#x7279; <pr>N&#x1D4;w&#x1CE;k&#xE8;xi&#x101;ot&#xE8;</pr></trd>
  </trdgrp>
</drv>

