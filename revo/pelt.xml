<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pelt</rad>/o
</kap>
<drv mrk="pelt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Speciale preparita felo, ĝenerale belhara, uzata kiel vesto aŭ
      garnituro:
      <ekz>
        vespermantelo kun randoj el zibela <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">футра</trd>
  <trdgrp lng="br">
    <trd>feur</trd>,
    <trd>foulinenn</trd>
  </trdgrp>
  <trd lng="de">Pelz</trd>
  <trdgrp lng="en">
    <trd>pelt</trd>,
    <trd>fur</trd>
  </trdgrp>
  <trd lng="fr">fourrure</trd>
  <trd lng="hu">szőrme</trd>
  <trdgrp lng="nl">
    <trd>pels</trd>,
    <trd>bont</trd>
  </trdgrp>
  <trd lng="pt">peliça</trd>
  <trd lng="ru">выделанный <ind>мех</ind></trd>
  <trd lng="tr">kürk</trd>
</drv>

<drv mrk="pelt.0ajxo">
  <kap><tld/>aĵo</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <tld lit="P"/>a vesto.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>шуба</trd>,
    <trd>футра</trd>
  </trdgrp>
  <trd lng="br">mantell <ind>feur</ind></trd>
  <trd lng="fr">fur <klr tip="ind"> (clothing)</klr></trd>
  <trd lng="fr">fourrure <klr tip="ind"> (vêtement)</klr></trd>
  <trdgrp lng="hu">
    <trd>bunda</trd>,
    <trd>szőrmeruha</trd>,
    <trd>szőrmeáru</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bontmantel</trd>,
    <trd>pelswerk</trd>
  </trdgrp>
  <trd lng="ru">шуба</trd>
</drv>
</art>
