<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pelt.xml,v 1.17 2021/05/29 10:57:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pelt</rad>/o
</kap>
<drv mrk="pelt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Speciale preparita felo, &gcirc;enerale belhara, uzata kiel vesto a&ubreve;
      garnituro:
      <ekz>
        vespermantelo kun randoj el zibela <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_t;&c_r;&c_a;</trd>
  <trd lng="bo">&#xF66;&#xFA4;&#xF74;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>feur</trd>,
    <trd>foulinenn</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ko&#x17E;e&#x161;inov&#xFD; kab&#xE1;t</trd>,
    <trd>ko&#x17E;ich</trd>
  </trdgrp>
  <trd lng="de">Pelz</trd>
  <trdgrp lng="en">
    <trd>pelt</trd>,
    <trd>fur</trd>
  </trdgrp>
  <trd lng="fr">fourrure</trd>
  <trd lng="hu">sz&odblac;rme</trd>
  <trdgrp lng="nl">
    <trd>pels</trd>,
    <trd>bont</trd>
  </trdgrp>
  <trd lng="pt">peli&ccedil;a</trd>
  <trd lng="ru">&c_v;&c_y;&c_d;&c_je;&c_l;&c_a;&c_n;&c_n;&c_y;&c_j; <ind>&c_m;&c_je;&c_h;</ind></trd>
  <trd lng="sk">ko&#x17E;u&#x161;ina</trd>
  <trd lng="tr">k&uuml;rk</trd>
</drv>

<drv mrk="pelt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <tld lit="P"/>a vesto.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_u;&c_b;&c_a;</trd>,
    <trd>&c_f;&c_u;&c_t;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="br">mantell <ind>feur</ind></trd>
  <trd lng="fr">fur <klr tip="ind"> (clothing)</klr></trd>
  <trd lng="fr">fourrure <klr tip="ind"> (v&ecirc;tement)</klr></trd>
  <trdgrp lng="hu">
    <trd>bunda</trd>,
    <trd>sz&odblac;rmeruha</trd>,
    <trd>sz&odblac;rme&aacute;ru</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bontmantel</trd>,
    <trd>pelswerk</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_u;&c_b;&c_a;</trd>
</drv>
</art>
