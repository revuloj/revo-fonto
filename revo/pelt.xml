<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pelt.xml,v 1.21 2023/09/03 12:45:06 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pelt</rad>/o
</kap>
<drv mrk="pelt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Speciale preparita felo, &gcirc;enerale belhara, uzata kiel vesto a&ubreve;
      garnituro:
      <ekz>
        vespermantelo kun randoj el zibela <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_t;&c_r;&c_a;</trd>
  <trd lng="bo">&#xF66;&#xFA4;&#xF74;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>feur</trd>,
    <trd>foulinenn</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ko&#x17E;e&scaron;inov&#xFD; kab&#xE1;t</trd>,
    <trd>ko&#x17E;ich</trd>
  </trdgrp>
  <trd lng="de">Pelz</trd>
  <trdgrp lng="en">
    <trd>pelt</trd>,
    <trd>fur</trd>
  </trdgrp>
  <trd lng="fr">fourrure</trd>
  <trd lng="hu">sz&odblac;rme</trd>
  <trdgrp lng="ja">
    <trd>&#x6BDB;&#x76AE;  [<ind>&#x3051;&#x304C;&#x308F;</ind>]</trd>,
    <trd>&#x6BDB;&#x76AE;&#x306E;&#x670D;  [<ind>&#x3051;&#x304C;&#x308F;&#x306E;&#x3075;&#x304F;</ind>]</trd>,
    <trd>&#x30D5;&#x30A1;&#x30FC;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pels</trd>,
    <trd>bont</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>futro</trd>,
    <trd>ko&zdot;uch</trd>,
    <trd>szuba</trd>
  </trdgrp>
  <trd lng="pt">peli&ccedil;a</trd>
  <trdgrp lng="ro">
    <trd>blan&abreve;</trd>,
    <trd>hain&abreve; de blan&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_d;&c_je;&c_l;&c_a;&c_n;&c_n;&c_y;&c_j; <ind>&c_m;&c_je;&c_h;</ind></trd>
  <trd lng="sk">ko&#x17E;u&scaron;ina</trd>
  <trd lng="tr">k&uuml;rk</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_u;&c_t;&c_r;&#x43E; (&c_v;&c_i;&c_r;&#x43E;&c_b;&#x43B;&c_je;&#x43D;&c_je;)</trd>,
    <trd>&c_sh;&c_u;&c_b;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="pelt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <tld lit="P"/>a vesto.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_u;&c_b;&c_a;</trd>,
    <trd>&c_f;&c_u;&c_t;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="br">mantell <ind>feur</ind></trd>
  <trd lng="en">fur <klr tip="ind"> (clothing)</klr></trd>
  <trd lng="fr">fourrure <klr tip="ind"> (v&ecirc;tement)</klr></trd>
  <trdgrp lng="hu">
    <trd>bunda</trd>,
    <trd>sz&odblac;rmeruha</trd>,
    <trd>sz&odblac;rme&aacute;ru</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6BDB;&#x76AE;&#x88FD;&#x54C1;  [<ind>&#x3051;&#x304C;&#x308F;&#x305B;&#x3044;&#x3072;&#x3093;</ind>]</trd>,
    <trd>&#x6BDB;&#x76AE;&#x306E;&#x8863;&#x670D;  [<ind>&#x3051;&#x304C;&#x308F;&#x306E;&#x3044;&#x3075;&#x304F;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bontmantel</trd>,
    <trd>pelswerk</trd>
  </trdgrp>
  <trd lng="pl">ubranie  <ind>futrzane</ind></trd>
  <trd lng="ro">&icirc;mbr&abreve;c&abreve;minte de blan&abreve;</trd>
  <trd lng="ru">&c_sh;&c_u;&c_b;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_u;&c_b;&c_a;</trd>,
    <trd>&c_h;&c_u;&c_t;&c_r;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
