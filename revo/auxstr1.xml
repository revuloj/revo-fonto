<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>A&ubreve;strali</rad>/o
</kap>
<drv mrk="auxstr1.A0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>ZR</bib></fnt>
  <snc mrk="auxstr1.A0o.kontinento">
    <dif>
      La plej malgranda inter la
      <ref tip="super" cel="kontin.0o.terkuno">kontinentoj</ref>,
      situanta inter Pacifiko kaj Hinda Oceano.
    </dif>
    <ref tip="malprt" cel="oceani.O0o">Oceanio</ref>
  </snc>
  <snc mrk="auxstr1.A0o.sxtato">
    <dif>
      La sola <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>
      de la <ref tip="malprt" cel="auxstr1.A0o.kontinento"><tld
      lit="a"/>a</ref>
      kontinento:
      <ekz>
        &scirc;i helpis anka&ubreve; al elmigrintaj &ccirc;inoj ekz.
        en Usono, Kanado, <tld/>o, Singapuro kaj Hongkongo, kiuj
        deziris edzinigi junulinojn el la &ccirc;ina &ccirc;eflando
        <fnt><bib>Monato</bib>, <lok>n-ro 09/1993, Noticoj &dash;
        Eminenta svatistino</lok></fnt>;
      </ekz>
      <ekz>
        la pasio pri stereogramoj rapide disvasti&gcirc;is al aliaj landoj,
        la &leftquot;fundamento&rightquot; entenas tri librojn kun la titolo
        Magic Eye (Magia Okulo), da&ubreve;re inter la plej
        venditaj libroj en Usono, A&ubreve;stralio, Brazilio, Britio,
        Germanio
        <fnt><bib>Monato</bib>, <lok>n-ro 01/1995, Stereogramoj &dash;
        La modo de fiksokuloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_w;&c_s;&c_t;&c_r;&c_a;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="de">Australien</trd>
  <trd lng="es">Australia</trd>
  <trd lng="fr">Australie</trd>
  <trd lng="hu">Ausztr&aacute;lia</trd>
  <trd lng="nl">Australi&euml;</trd>
  <trd lng="ru">&c_A;&c_v;&c_s;&c_t;&c_r;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">Austr&aacute;lia</trd>
  <trd lng="vo">Talop</trd>
  <trd lng="pt">Austr&aacute;lia</trd>
</drv>
</art>
