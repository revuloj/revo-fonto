<kap>
  <ofc>*</ofc>
  <rad>tempi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tempi.0o">
  <kap><tld/>o</kap>
  <snc mrk="tempi.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Flanka parto de la <ref tip="malprt" cel="kap.0o.ANA">kapo</ref>
      inter okulangulo kaj supro de orelo:
      <ekz>
        Dume mi sentis, kiel mia sango batas &ccirc;e la tempioj
      <fnt>
        <aut>Albert Camus, trad. Michel Duc Goninaz</aut>,
        <vrk>La Fremdulo</vrk>,
        <lok>S.A.T., 1993, p. 25</lok>
      </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_r;&c_o;&c_n;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_o;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">templa</trd>
  <trdgrp lng="cs">
    <trd>skr&#xE1;&#x148;</trd>,
    <trd>sp&#xE1;nek</trd>
  </trdgrp>
  <trd lng="de">Schl&auml;fe</trd>
  <trd lng="en">temple</trd>
  <trd lng="es">sien</trd>
  <trd lng="fr">tempe</trd>
  <trd lng="hu">hal&aacute;nt&eacute;k</trd>
  <trd lng="id">pelipis</trd>
  <trd lng="it">tempia</trd>
  <trdgrp lng="ja">
    <trd>&#x5074;&#x982D;  <pr>&#x304C;&#x308F;&#x3042;&#x305F;&#x307E;</pr></trd>,
    <trd>&#x3053;&#x3081;&#x304B;&#x307F;</trd>
  </trdgrp>
  <trd lng="nl">slaap<klr tip="ind"> (v.hoofd)</klr></trd>
  <trd lng="pl">skro&nacute;</trd>
  <trd lng="pt">t&ecirc;mpora</trd>
  <trd lng="ru">&c_v;&c_i;&c_s;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>sluchy</trd>,
    <trd>sp&#xE1;nky</trd>
  </trdgrp>
  <trd lng="sv">tinning</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43A;&#x440;&#x43E;&#x43D;&#x44F;</trd>,
    <trd>&#x432;&#x438;&#x441;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
