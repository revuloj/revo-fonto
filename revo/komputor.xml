<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: komputor.xml,v 1.13 2023/12/03 09:58:54 revo Exp $">
<kap>
  <rad>komputor</rad>/o <fnt><bib>SPIV</bib></fnt>,
  <var><kap><rad var="e">komputer</rad>/o <fnt><bib>SPIV</bib></fnt></kap></var>
</kap>
<drv mrk="komputor.0o">
  <kap><tld/>o, <var><kap><tld var="e"/>o</kap></var></kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="komputor.0o.KOMP">
  <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="dif" cel="komput.0ilo">Komputilo</ref>:
      <ekz>
        inter la pli special-celaj oficejaj kaj personaj
        <tld/>oj povas ekzisti grandaj diferencoj
        <fnt>
          <aut>R. Eichholz</aut>,
          <vrk><url ref="http://www.familienforschung-pabst.de/EspBiographien/PP-pdf/11paroletoj.pdf">
          Dekunu Paroladetoj pri Pekoteko</url></vrk>,
          <lok>1993</lok>
        </fnt>;
      </ekz>    
      <ekz>
        instruisto de la fakultato pri <tld/>o
        <fnt>
          <vrk><url ref="http://esperanto.china.org.cn/2014-10/15/content_33773024.htm">Esperanto kiel deviga programero en Zaozhuang-a Profesia ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        verki esperante per la <tld/>o
        <fnt>
          <vrk><url ref="http://ttt.esperanto.org/us/USEJ/ktp/ktpindex.html">KTP! index of articles &#x2010; ttt.esperanto.org</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        produktado de facile vendeblaj varoj, kiaj pecoj de <klr>[&#x2026;]</klr>
        televida aran&gcirc;a&jcirc;o, <tld var="e"/>o
        <fnt><aut>&Ccirc;ina Radio Internacia</aut>, <vrk><url ref="http://es.chinabroadcast.cn/1/2005/03/04/1@21729.htm">Xi'an
        hodia&ubreve; (2)</url></vrk>, <lok>2005-03-04</lok></fnt>;
      </ekz>
      <ekz>
        didaktika lernado pri la <tld var="e"/>o kaj &gcirc;iaj partoj
        <fnt>
          <vrk><url ref="http://luisguillermo.com/9a_KKE/gxenerala.htm">&Gcirc;enerala Raporto pri la 9a Kolombia Kongreso de Esperanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tria tutmondi&gcirc;o okazas nun, ekde &ccirc;irka&ubreve; 1980, kun la individua <tld var="e"/>o kaj
        Interreto
        <fnt>
          <vrk><url ref="https://lernu.net/en/forumo/temo/23813">&Ccirc;u Esperanto estas la internacia lingvo de la revolucio ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&#xF42;&#xFB3;&#xF7C;&#xF42;&#xF0B;&#xF40;&#xFB3;&#xF51;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>komputer</trd>,
    <trd>po&ccaron;&iacute;ta&ccaron;</trd>
  </trdgrp>
  <trd lng="fr">ordinateur</trd>
  <trd lng="nl">computer<klr tip="ind"> (arch.)</klr></trd>
  <trd lng="pl">komputer</trd>
  <trd lng="pt">computador</trd>
  <trd lng="ro">calculator</trd>
  <trdgrp lng="sk">
    <trd>komputer</trd>,
    <trd>po&ccaron;&iacute;ta&ccaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x43E;&#x43C;&#x43F;&#x2019;&#x44E;&#x442;&#x435;&#x440;</trd>
  <trdgrp lng="zh">
    <trd>&#x5FAE;&#x673A; <pr>w&#x113;ij&#x12B;</pr></trd>,
    <trd>&#x8A08;&#x7B97;&#x6A5F; <pr>j&#xEC;su&#xE0;nj&#x12B;</pr></trd>,
    <trd>&#x7535;&#x5B50;&#x8BA1;&#x7B97;&#x673A; <pr>di&#xE0;nz&#x1D0;j&#xEC;su&#xE0;nj&#x12B;</pr></trd>,
    <trd>&#x7535;&#x8111; <pr>di&#xE0;nn&#x1CE;o</pr></trd>
  </trdgrp>
</drv>
</art>

