<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>prodrom</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="prodrom.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="prodrom.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="vid" cel="sign.antaux0o">Anta&ubreve;signo</ref>,
      anta&ubreve;<ref tip="vid" cel="simpto.0o.MED">simptomo</ref>
      de okazonta malsani&gcirc;o:
      <ekz>
        premson&gcirc;oj <klr>...</klr> min iom perpleksigas.  &Ccirc;u
        <tld/>oj de frenezi&gcirc;o
        <fnt><aut>E. de Zilah</aut>, <vrk>Vivi sur barko</vrk>, 
        <lok>1994</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>signe avant-coureur</trd>,
    <trd>prodrome</trd>
  </trdgrp>
</drv>
</art>

