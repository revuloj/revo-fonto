<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prodrom.xml,v 1.7 2022/06/02 22:30:25 revo Exp $">
<kap>
  <rad>prodrom</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="prodrom.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="prodrom.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="vid" cel="sign.antaux0o">Anta&ubreve;signo</ref>,
      anta&ubreve;<ref tip="vid" cel="simpto.0o.MED">simptomo</ref>
      de okazonta malsani&gcirc;o:
      <ekz>
        premson&gcirc;oj <klr>...</klr> min iom perpleksigas.  &Ccirc;u
        <tld/>oj de frenezi&gcirc;o
        <fnt><aut>E. de Zilah</aut>, <vrk>Vivi sur barko</vrk>, 
        <lok>1994</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_o;&c_d;&c_r;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_u;&c_v;&c_a;&c_shch; &c_s;&c_i;&c_m;&c_p;&c_t;&c_o;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">pr&ograve;drom</trd>
  <trd lng="cs">prodromus</trd>
  <trd lng="en">prodrome</trd>
  <trdgrp lng="fr">
    <trd>signe avant-coureur</trd>,
    <trd>prodrome</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>prodromale</trd>,
    <trd>voorloper</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>prodrom</trd>,
    <trd>objawy  <ind>prodromalne</ind></trd>
  </trdgrp>
  <trd lng="ro">prodrom</trd>
  <trd lng="sk">prodromus</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_d;&c_r;&#x43E;&#x43C; (&#x43F;&c_r;&#x43E;&c_v;&c_ib;&c_s;&#x43D;&c_i;&#x43A;
    &c_h;&c_v;&#x43E;&c_r;&#x43E;&c_b;&c_i;)</trd>,
    <trd>&#x43B;&c_je;&c_g;&#x43A;&c_je; &#x43D;&c_je;&c_d;&#x43E;&#x43C;&c_a;&c_g;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

