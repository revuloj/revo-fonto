<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prodrom.xml,v 1.8 2024/02/15 21:20:02 revo Exp $">
<kap>
  <rad>prodrom</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="prodrom.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="prodrom.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="vid" cel="sign.antaux0o">Anta&ubreve;signo</ref>,
      anta&ubreve;<ref tip="vid" cel="simpto.0o.MED">simptomo</ref>
      de okazonta malsani&gcirc;o:
      <ekz>
        premson&gcirc;oj <klr>[&#8230;]</klr> min iom perpleksigas, &ccirc;u
        <tld/>oj de frenezi&gcirc;o
        <fnt><aut>E. de Zilah</aut>, <vrk>Vivi sur barko</vrk>,
        <lok>1994</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_o;&c_d;&c_r;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_sh;&c_je;&c_s;&c_t;&c_v;&c_u;&c_v;&c_a;&c_shch; &c_s;&c_i;&c_m;&c_p;&c_t;&c_o;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">pr&ograve;drom</trd>
  <trd lng="cs">prodromus</trd>
  <trdgrp lng="de">
    <trd>Prodrom</trd>,
    <trd>Vorstadium <klr>(Medizin)</klr></trd>
  </trdgrp>
  <trd lng="en">prodrome</trd>
  <trdgrp lng="fr">
    <trd>signe avant-coureur</trd>,
    <trd>prodrome</trd>
  </trdgrp>
  <trd lng="grc">&pi;&rho;&omikron_ton;&delta;&rho;&omikron;&my;&omikron;&sigma_fina;</trd>
  <trd lng="lat">prodromus</trd>
  <trdgrp lng="nl">
    <trd>prodromale</trd>,
    <trd>voorloper</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>prodrom</trd>,
    <trd>objawy  <ind>prodromalne</ind></trd>
  </trdgrp>
  <trd lng="ro">prodrom</trd>
  <trd lng="sk">prodromus</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_d;&c_r;&c_o;&c_m; (&c_p;&c_r;&c_o;&c_v;&c_ib;&c_s;&c_n;&c_i;&c_k;
    &c_h;&c_v;&c_o;&c_r;&c_o;&c_b;&c_i;)</trd>,
    <trd>&c_l;&c_je;&c_g;&c_k;&c_je; &c_n;&c_je;&c_d;&c_o;&c_m;&c_a;&c_g;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
</art>

