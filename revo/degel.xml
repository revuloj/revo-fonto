<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: degel.xml,v 1.25 2022/03/15 15:50:54 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>degel</rad>/i <fnt><bib>PV</bib></fnt> 
</kap>

<drv mrk="degel.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="degel.0i.malglaciigxi">
    <dif>
      Malglacii&gcirc;i, malfrosti&gcirc;i, fluidi&gcirc;i de varmo,
      fandi&gcirc;i:
      <ekz>
        la suno <tld/>igas la stratojn;
      </ekz>
      <ekz>
        <tld/>a vetero.
      </ekz>
    </dif>
    <trd lng="ca">desgla&ccedil;ar</trd>
    <trdgrp lng="de">
      <trd>tauen</trd>,
      <trd>auftauen</trd>,
      <trd>schmelzen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;geler</trd>,
      <trd>fondre<klr tip="ind"> (se
      liqu&eacute;fier)</klr></trd></trdgrp> 
    <trdgrp lng="it">
      <trd>scongelarsi</trd>,
      <trd>sciogliersi<klr tip="ind"> (liquefarsi)</klr></trd>
      <trd>liquefarsi<klr tip="ind"> (sciogliersi)</klr></trd>
    </trdgrp> 
    <trdgrp lng="pl">
      <trd>taja&cacute;</trd>,
      <trd>topnie&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>degelar</trd>,
      <trd>derreter-se</trd>
    </trdgrp>
  </snc>
  <snc mrk="degel.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iom post iom malsolidi&gcirc;i, malgrandi&gcirc;i, malaperi:
      <ekz>
        mi rimarkis, ke la gramatiko pli kaj pli <tld/>as
        en miaj manoj
        <fnt>
          <aut>L. L. Zamenhof</aut>,
          <vrk><url ref="">
          [Letero al N. A. Borovko]</url></vrk>,
          <lok>Lingvo Internacia, 1896:6-7</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj &scirc;para&jcirc;oj degeladis tiom, ke li ne povis doni al Manjo la
        etan sumon
        <fnt><aut>J. Francis</aut>,
        <vrk>La Granda Kaldrono</vrk>, 
        <lok>1978</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flu">forflui</ref>,
      <ref cel="solv">solvi&gcirc;i</ref>.
    </refgrp>
    <trdgrp lng="ca">
      <trd>esvanir-se</trd>,
      <trd>dissipar-se</trd>
    </trdgrp>
    <trd lng="de">sich <ind>aufl&ouml;sen</ind></trd>
    <trdgrp lng="fr">
      <trd>dispara&icirc;tre</trd>,
      <trd>fondre<klr tip="ind"> (comme neige au
      soleil)</klr></trd></trdgrp> 
    <trd lng="it">svanire</trd>
    <trd lng="pl">topnie&cacute;</trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&#xF56;&#xF5E;&#xF74;&#xF62;&#xF0B;</trd>
  <trd lng="ca">fondre</trd>
  <trdgrp lng="cs">
    <trd>jihnout</trd>,
    <trd>rozmrzat</trd>,
    <trd>rozt&aacute;t</trd>,
    <trd>t&aacute;t</trd>
  </trdgrp>
  <trd lng="en">thaw</trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5E4;&#x5E9;&#x5D9;&#x5E8;</trd>
  <trdgrp lng="hu">
    <trd>olvad</trd>,
    <trd>elolvad<klr tip="ind"> (&aacute;tv.
    is)</klr></trd></trdgrp>
  <trd lng="pt">derreter-se</trd>
  <trd lng="ru">&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>roztopi&tcaron;</trd>,
    <trd>topi&tcaron; sa <klr>(sneh)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x430;&#x43D;&#x443;&#x442;&#x438;</trd>,
    <trd>&#x442;&#x43E;&#x43F;&#x438;&#x442;&#x438;&#x441;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="degel.0o">
  <kap><tld/>o</kap>
  <snc mrk="degel.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Tempo de atmosfera varmi&gcirc;o, dum kiu glacio, ne&gcirc;o <tld/>as:
      <ekz>
        la <tld/>o de la ne&gcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_l;&c_ib;&c_g;&c_a;</trd>
  <trd lng="fr">d&eacute;gel</trd>
  <trd lng="he">&#x5D4;&#x5E4;&#x5E9;&#x5E8;&#x5D4;</trd>
</drv>
</art>
