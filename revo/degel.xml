<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: degel.xml,v 1.31 2026/02/03 02:55:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>degel</rad>/i <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="degel.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="degel.0i.malglaciigxi">
    <dif>
      Malglacii&gcirc;i, malfrosti&gcirc;i, fluidi&gcirc;i de varmo,
      fandi&gcirc;i:
      <ekz>
        en la suno<klr>[&#x2026;]</klr> la glacio <tld/>os
        <fnt>
          <bib>Mizero</bib>
          <lok>VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        komencis <tld/>i la printempo venis
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en mia brusto <tld/>as iu peza bulo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la vetero &scirc;an&gcirc;i&gcirc;is, fari&gcirc;is vetero <tld/>a
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">desgla&ccedil;ar</trd>
    <trdgrp lng="de">
      <trd>tauen</trd>,
      <trd>auftauen</trd>,
      <trd>schmelzen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;geler</trd>,
      <trd>fondre<klr tip="ind"> (se
      liqu&eacute;fier)</klr></trd></trdgrp>
    <trdgrp lng="it">
      <trd>scongelarsi</trd>,
      <trd>sciogliersi<klr tip="ind"> (liquefarsi)</klr></trd>
      <trd>liquefarsi<klr tip="ind"> (sciogliersi)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>dooien</trd>,
      <trd>ontdooien</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>taja&cacute;</trd>,
      <trd>topnie&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>degelar</trd>,
      <trd>derreter-se</trd>
    </trdgrp>
  </snc>
  <snc mrk="degel.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iom post iom malsolidi&gcirc;i, malgrandi&gcirc;i, malaperi:
      <ekz>
        mi rimarkis, ke la gramatiko pli kaj pli <tld/>as
        en miaj manoj
        <fnt>
          <aut>L. L. Zamenhof</aut>,
          <vrk><url ref="">
          [Letero al N. A. Borovko]</url></vrk>,
          <lok>Lingvo Internacia, 1896:6-7</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj &scirc;para&jcirc;oj degeladis tiom, ke li ne povis doni al Manjo la
        etan sumon
        <fnt><aut>J. Francis</aut>,
        <vrk>La Granda Kaldrono</vrk>,
        <lok>1978</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flu.for0i">forflui</ref>,
      <ref cel="solv.0igxi">solvi&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>esvanir-se</trd>,
      <trd>dissipar-se</trd>
    </trdgrp>
    <trd lng="de">sich <ind>aufl&ouml;sen</ind></trd>
    <trdgrp lng="fr">
      <trd>dispara&icirc;tre</trd>,
      <trd>fondre<klr tip="ind"> (comme neige au
      soleil)</klr></trd></trdgrp>
    <trd lng="it">svanire</trd>
    <trd lng="nl">ontdooien</trd>
    <trd lng="pl">topnie&cacute;</trd>
    <trdgrp lng="zh">
    <trd>&#x74E6;&#x89E3; <pr>w&#x1CE;ji&ecaron;</pr></trd>,
    <trd>&#x89E3;&#x4F53; <pr>ji&ecaron;t&#x1D0;</pr></trd>,
    <trd>&#x5206;&#x89E3; <pr>f&emacron;nji&ecaron;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&t_ba;&t_zha;&t_shabchu;&t_ra;&t_tsheg;</trd>
  <trd lng="ca">fondre</trd>
  <trdgrp lng="cs">
    <trd>jihnout</trd>,
    <trd>rozmrzat</trd>,
    <trd>rozt&aacute;t</trd>,
    <trd>t&aacute;t</trd>
  </trdgrp>
  <trd lng="en">thaw</trd>
  <trd lng="he">&lamed;&he;&pe;&shin;&yod;&resh;</trd>
  <trdgrp lng="hu">
    <trd>olvad</trd>,
    <trd>elolvad<klr tip="ind"> (&aacute;tv.
    is)</klr></trd></trdgrp>
  <trd lng="ja">&#x3068;&#x3051;&#x308B;</trd>
  <trd lng="pt">derreter-se</trd>
  <trd lng="ru">&c_t;&c_a;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>roztopi&tcaron;</trd>,
    <trd>topi&tcaron; sa <klr>(sneh)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_a;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_o;&c_p;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="degel.0o">
  <kap><tld/>o</kap>
  <snc mrk="degel.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Tempo de atmosfera varmi&gcirc;o, dum kiu glacio, ne&gcirc;o <tld/>as:
      <ekz>
        la <tld/>o de la ne&gcirc;oj;
      </ekz>
      <ekz>
        morga&ubreve; la vetero &scirc;an&gcirc;i&gcirc;os: ni havos <tld/>on
        <fnt>
          <bib>Mizero</bib>
          <lok>II</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_l;&c_ib;&c_g;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Schmelze</trd>,
    <trd>Tauen</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;gel</trd>
  <trd lng="he">&he;&pe;&shin;&resh;&he;</trd>
  <trdgrp lng="ja">
    <trd>&#x96EA;&#x89E3;&#x3051;  <pr>&#x3086;&#x304D;&#x3069;&#x3051;</pr></trd>,
    <trd>&#x6C37;&#x89E3;  <pr>&#x3072;&#x3087;&#x3046;&#x304B;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">dooi</trd>
  <trd lng="zh">&#x96EA;&#x878D;&#x5316; <pr>xu&ecaron;r&oacute;nghu&agrave;</pr></trd>
</drv>

<drv mrk="degel.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Ka&ubreve;zi ke io <tld/>as:
      <ekz>
        por havi akvon en la mateno ni devis <tld/>igi ne&gcirc;on
        <fnt>
          <bib>Kon11</bib>
          <lok>Voja&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        brakumante la glaciblokon, por <tld/>igi &gcirc;in per la varmeco de sia korpo
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Minejo de l' Bildoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">schmelzen <klr>(lassen)</klr></trd>
  <trd lng="en">melt <klr>(something)</klr></trd>
</drv>

</art>
