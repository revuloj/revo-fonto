<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolbas.xml,v 1.35 2024/08/19 09:27:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kolbas</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="kolbas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kolbas.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Hakita spicita <ref cel="viand.0o">viando</ref>
      en&scirc;ovita en inteston a&ubreve; surogaton (a&ubreve; konservita en ladskatolo):
      <ekz>
        vino <klr>[&#8230;]</klr> estis metita en la korbon de man&gcirc;a&jcirc;oj al la tie jam
        trovi&gcirc;antaj &scirc;inko, <tld/>o kaj froma&gcirc;o
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        peco da kaprina <tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        tri &scirc;tonaj &scirc;tupoj kondukis de la korto rekte al la kuirejo,
        <klr>[&#8230;]</klr> de kies kalke pentritaj traboj pendis <tld/>oj
        <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>;
      </ekz>
      <ekz>
        terpoma salato kun peceto da <ind>hepata <tld/>o</ind>
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 4</lok>
        </fnt>;
        <trd lng="de">Leberwurst</trd>
        <trd lng="en">liver sausage</trd>
        <trd lng="fr">p&acirc;t&eacute; de foie</trd>
      </ekz>
      <ekz>
        la <tld/>o el la ladskatolo tuj el&ccirc;erpi&gcirc;as
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj konsistas centelcente el &ccirc;evala&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010998.php"
              >Kolbaso-fatraso</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/e/e4/Wurst_(Kochwurst).jpg/286px-Wurst_(Kochwurst).jpg"
      prm="CC BY-SA 3.0"
      >pecoj de sango<tld/>o kaj hepato<tld/>o
        <fnt>
          <aut>Rainer Zenz</aut>
          <lok><url ref="&WCW;/File:Wurst_(Kochwurst).jpg"
              >https://commons.wikimedia.org/wiki/File:Wurst_(Kochwurst).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_w;&c_b;&c_a;&c_s;&c_a;</trd>,
    <trd>&c_k;&c_ib;&c_l;&c_b;&c_a;&c_s;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_a;&c_l;&c_a;&c_m;</trd>
  <trd lng="br">silzigenn</trd>
  <trdgrp lng="cs">
    <trd>klob&aacute;sa</trd>,
    <trd>p&aacute;rek</trd>,
    <trd>sal&aacute;m</trd>,
    <trd>uzenka</trd>
  </trdgrp>
  <trd lng="de">Wurst</trd>
  <trdgrp lng="en">
    <trd>sausage</trd>,
    <trd>salami</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>embutido</trd>,
    <trd>salchicha <klr>(kolbaseto)</klr></trd>
  </trdgrp>
  <trd lng="fr">saucisse<klr tip="ind"> (mets)</klr></trd>
  <trd lng="id">sosis</trd>
  <trdgrp lng="ja">
    <trd>&#12477;&#12540;&#12475;&#12540;&#12472;</trd>,
    <trd>&#23566;&#28779;&#22178; <pr>&#12415;&#12385;&#12403;&#12402;&#12398;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">worst</trd>
  <trd lng="pl">kie&lstroke;basa</trd>
  <trd lng="ro">c&acirc;rnat</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_b;&c_a;&c_s;&c_a;</trd>
  <trd lng="sk">klob&aacute;sa</trd>
  <trd lng="uk">&c_k;&c_o;&c_v;&c_b;&c_a;&c_s;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#39321;&#32928; <pr>xi&amacron;ngch&aacute;ng</pr></trd>,
    <trd>&#32928;&#20180; <pr>ch&aacute;ngz&#464;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kolbas.trip0o">
  <kap>trip<tld/>o</kap>
  <snc mrk="kolbas.trip0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="K"/>o plenigita el tripoj.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_n;&c_t;&c_r;&c_a;&c_b;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="br">anduilhenn</trd>
  <trdgrp lng="cs">
    <trd>jaternice</trd>,
    <trd>jitrnice</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Chitterlings</trd>,
    <trd>Chitlin</trd>
  </trdgrp>
  <trd lng="en">chitterling</trd>
  <trd lng="es">charcuter&iacute;a</trd>
  <trd lng="fr">andouille<klr tip="ind"> (mets)</klr></trd>
  <trd lng="ja">&#12450;&#12531;&#12489;&#12453;&#12452;&#12518;</trd>
  <trd lng="nl">rolpens</trd>
  <trdgrp lng="pl">
    <trd>pasztetowa</trd>,
    <trd>w&aogonek;trobianka</trd>
  </trdgrp>
  <trd lng="ro">leb&abreve;rvurst</trd>
  <trd lng="ru">&c_l;&c_i;&c_v;&c_je;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_l;&c_b;&c_a;&c_s;&c_a;</trd>
  <trd lng="sk">jaternica</trd>
  <trd lng="uk">&c_l;&c_ib;&c_v;&c_je;&c_r;&c_n;&c_a; &c_k;&c_o;&c_v;&c_b;&c_a;&c_s;&c_a;</trd>
</drv>

<drv mrk="kolbas.sang0o">
  <kap>sang<tld/>o, <var><kap>sango<tld/>o</kap></var></kap>
  <snc mrk="kolbas.sang0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="K"/>o el porkaj viando, lardo kaj sango kun spicoj:
      <ekz>
        mi tiel bone provizis ilin per la sango<tld/>o, kiun mi kunportis de nederlanda
        &scirc;ipo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi havas hejme sango<tld/>on kaj vinon, &ccirc;u vi volas man&gcirc;eti kun mi
        <fnt>
          <aut>Albert Camus, trad. Michel Duc Goninaz</aut>,
          <vrk>La Fremdulo</vrk>,
          <lok>S.A.T., 1993, p. 38</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_v;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_malmol;&c_r;&c_v;&c_a;&c_v;&c_i;&c_c;&c_a;</trd>
  <trd lng="br">gwadegenn</trd>
  <trd lng="de">Blutwurst</trd>
  <trdgrp lng="en">
    <trd>black pudding</trd>,
    <trd>blood sausage</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>moronga</trd>,
    <trd>morcilla</trd>
  </trdgrp>
  <trd lng="fr">boudin<klr tip="ind"> (mets)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12502;&#12540;&#12480;&#12531;</trd>,
    <trd>&#12502;&#12521;&#12483;&#12489;&#12477;&#12540;&#12475;&#12540;&#12472;</trd>
  </trdgrp>
  <trd lng="nl">bloedworst</trd>
  <trd lng="pl">kaszanka</trd>
  <trd lng="ro">caltabo&scomma;i</trd>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_v;&c_ja;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_l;&c_b;&c_a;&c_s;&c_a;</trd>
  <trd lng="uk">&c_k;&c_r;&c_o;&c_v;&#8217;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#29482;&#34880;&#31957; <pr>zh&umacron;xi&ecaron;g&amacron;o</pr></trd>,
    <trd>&#34880;&#32928; <pr>xi&ecaron;ch&aacute;ng</pr></trd>,
    <trd>&#34880;&#28082;&#39321;&#32928; <pr>xi&ecaron;y&egrave;xi&amacron;ngch&aacute;ng</pr></trd>,
    <trd>&#34880;&#32928; <pr>xu&egrave;ch&aacute;ng</pr></trd>
  </trdgrp>
</drv>
</art>
