<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolbas.xml,v 1.34 2023/12/03 09:58:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kolbas</rad>/o
</kap>

<drv mrk="kolbas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kolbas.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Besta intesto, plenigita per hakita spicita viando el porko:
      <ekz>
        tri &scirc;tonaj &scirc;tupoj kondukis de la korto rekte al la kuirejo,
        <klr>...</klr> de kies kalke pentritaj traboj pendis <tld/>oj
        <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_w;&c_b;&c_a;&c_s;&c_a;</trd>,
    <trd>&c_k;&c_ib;&c_l;&c_b;&c_a;&c_s;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_a;&c_l;&c_a;&c_m;</trd>
  <trd lng="br">silzigenn</trd>
  <trdgrp lng="cs">
    <trd>klob&aacute;sa</trd>,
    <trd>p&aacute;rek</trd>,
    <trd>sal&aacute;m</trd>,
    <trd>uzenka</trd>
  </trdgrp>
  <trd lng="de">Wurst</trd>
  <trdgrp lng="en">
    <trd>sausage</trd>,
    <trd>salami</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>embutido</trd>,
    <trd>salchicha <klr>(kolbaseto)</klr></trd>
  </trdgrp>
  <trd lng="fr">saucisse<klr tip="ind"> (mets)</klr></trd>
  <trd lng="id">sosis</trd>
  <trdgrp lng="ja">
    <trd>&#x30BD;&#x30FC;&#x30BB;&#x30FC;&#x30B8;</trd>,
    <trd>&#x5C0E;&#x706B;&#x56A2;  <pr>&#x307F;&#x3061;&#x3073;&#x3072;&#x56A2;</pr></trd>
  </trdgrp>
  <trd lng="nl">worst</trd>
  <trd lng="pl">kie&lstroke;basa</trd>
  <trd lng="ro">c&acirc;rnat</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_b;&c_a;&c_s;&c_a;</trd>
  <trd lng="sk">klob&aacute;sa</trd>
  <trd lng="uk">&#x43A;&#x43E;&c_v;&c_b;&c_a;&c_s;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x9999;&#x80A0; <pr>xi&#x101;ngch&#xE1;ng</pr></trd>,
    <trd>&#x80A0;&#x4ED4; <pr>ch&#xE1;ngz&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kolbas.trip0o">
  <kap>trip<tld/>o</kap>
  <snc mrk="kolbas.trip0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="K"/>o plenigita per tripoj.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_n;&c_t;&c_r;&c_a;&c_b;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="br">anduilhenn</trd>
  <trdgrp lng="cs">
    <trd>jaternice</trd>,
    <trd>jitrnice</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Chitterlings</trd>,
    <trd>Chitlin</trd>
  </trdgrp>
  <trd lng="en">chitterling</trd>
  <trd lng="es">charcuter&iacute;a</trd>
  <trd lng="fr">andouille<klr tip="ind"> (mets)</klr></trd>
  <trd lng="ja">&#x30A2;&#x30F3;&#x30C9;&#x30A5;&#x30A4;&#x30E6;</trd>
  <trd lng="nl">rolpens</trd>
  <trdgrp lng="pl">
    <trd>pasztetowa</trd>,
    <trd>w&aogonek;trobianka</trd>
  </trdgrp>
  <trd lng="ro">leb&abreve;rvurst</trd>
  <trd lng="ru">&c_l;&c_i;&c_v;&c_je;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_l;&c_b;&c_a;&c_s;&c_a;</trd>
  <trd lng="sk">jaternica</trd>
  <trd lng="uk">&#x43B;&c_ib;&c_v;&c_je;&c_r;&#x43D;&c_a; &#x43A;&#x43E;&c_v;&c_b;&c_a;&c_s;&c_a;</trd>
</drv>

<drv mrk="kolbas.sang0o">
  <kap>sang<tld/>o, <var><kap>sango<tld/>o</kap></var></kap>
  <snc mrk="kolbas.sang0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Besta intesto plenigita per porkaj graso kaj sango kun bulboj.
      <ekz>
        Mi havas hejme sangokolbason kaj vinon. &Ccirc;u vi volas man&gcirc;eti kun
        mi?
      <fnt>
        <aut>Albert Camus, trad. Michel Duc Goninaz</aut>,
        <vrk>La Fremdulo</vrk>,
        <lok>S.A.T., 1993, p. 38</lok>
      </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_v;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_malmol;&c_r;&c_v;&c_a;&c_v;&c_i;&c_c;&c_a;</trd>
  <trd lng="br">gwadegenn</trd>
  <trd lng="de">Blutwurst</trd>
  <trdgrp lng="en">
    <trd>black pudding</trd>,
    <trd>blood sausage</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>moronga</trd>,
    <trd>morcilla</trd>
  </trdgrp>
  <trd lng="fr">boudin<klr tip="ind"> (mets)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30D6;&#x30FC;&#x30C0;&#x30F3;</trd>,
    <trd>&#x30D6;&#x30E9;&#x30C3;&#x30C9;&#x30BD;&#x30FC;&#x30BB;&#x30FC;&#x30B8;</trd>
  </trdgrp>
  <trd lng="nl">bloedworst</trd>
  <trd lng="pl">kaszanka</trd>
  <trd lng="ro">caltabo&#x219;i</trd>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_v;&c_ja;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_l;&c_b;&c_a;&c_s;&c_a;</trd>
  <trd lng="uk">&#x43A;&c_r;&#x43E;&c_v;&#x2019;&#x44F;&#x43D;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x732A;&#x8840;&#x7CD5; <pr>zh&#x16B;xi&#x11B;g&#x101;o</pr></trd>,
    <trd>&#x8840;&#x80A0; <pr>xi&#x11B;ch&#xE1;ng</pr></trd>,
    <trd>&#x8840;&#x6DB2;&#x9999;&#x80A0; <pr>xi&#x11B;y&#xE8;xi&#x101;ngch&#xE1;ng</pr></trd>,
    <trd>&#x8840;&#x80A0; <pr>xu&#xE8;ch&#xE1;ng</pr></trd>
  </trdgrp>
</drv>
</art>
