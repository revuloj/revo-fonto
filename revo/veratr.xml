<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: veratr.xml,v 1.17 2023/09/05 09:43:54 revo Exp $">
<kap>
  <rad>veratr</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="veratr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="melantiac.0oj">melantiacoj</ref>
      (<trd lng="la">Veratrum</trd>), kies radiko
      estas emetika, laksiga, sed en tro granda dozo mortiga:
      <ekz>
        Blanka veratro <klr>[&#x2026;]</klr> floras de junio &gcirc;is a&ubreve;gusto per blankaj, 12 &gcirc;is
        15&#x202F;mm grandaj floroj, kiuj formas 50&#x202F;cm longajn grapolojn, anta&ubreve; &ccirc;io kiam la
        suno brilas la floroj forte odoras
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=476758">Blanka heleboro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Flava genciano <klr>[&#x2026;]</klr> neflorante <klr>[&#x2026;]</klr> estas simila al blanka
        <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34547">Flava genciano</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="helebo.0o">heleboro</ref>
    <bld lok="&WCU;/thumb/a/a2/Veratrum_album_Rax2005.jpg/342px-Veratrum_album_Rax2005.jpg" prm="CC-BY-SA-3.0">Blanka <tld/>o (Veratrum album)
        <fnt>
          <aut>Manfred Werner</aut>
          <lok><url ref="&WCW;/File:Veratrum_album_Rax2005.jpg">&WCW;/File:Veratrum_album_Rax2005.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_m;&c_ja;&c_r;&c_y;&c_c;&c_a;</trd>
  <trd lng="de">Germer</trd>
  <trd lng="hu">z&aacute;szpa</trd>
  <trd lng="ja">&#x30B7;&#x30E5;&#x30ED;&#x30BD;&#x30A6;</trd>
  <trd lng="nl">veratrum</trd>
  <trdgrp lng="pt">
    <trd>verator</trd>,
    <trd>flor-da-verdade</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_m;&c_je;&c_r;&c_i;&c_c;&c_a;</trd>
  <trd lng="uk">&#x447;&#x435;&#x43C;&#x435;&#x440;&#x438;&#x446;&#x44F;</trd>
</drv>
</art>
