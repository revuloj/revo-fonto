<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hxalkolitik.xml,v 1.15 2023/11/15 17:08:02 revo Exp $">
  <kap><rad>&hcirc;alkolitik</rad>/o <fnt>PIV2</fnt></kap>

<drv mrk="hxalkolitik.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ARKE</uzo>
    <dif>
      <ref tip="dif" cel="epok.kupr0o">Kuprepoko</ref>:
      <ekz>
        prahistoriaj kulturoj en la bulgaraj landoj inkluzivas la Neolitikan Hamangian
        kulturon kaj <nom>Vin&ccaron;a</nom>-kulturon (6-a al 3-aj jarmiloj anta&ubreve; komuna erao), la
        <tld/>an Varnan kulturon (5a jarmilo a.k.e., vidu anka&ubreve; Varna Tombejo), kaj
        la Bronzepokan Ezeran kulturon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=443">Bulgario</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_d;&c_n;&c_y; &c_v;&c_je;&c_k;</trd>,
    <trd>&c_e;&c_n;&c_e;&c_a;&c_l;&c_ib;&c_t;</trd>,
    <trd>&c_h;&c_a;&c_l;&c_mol;&c_k;&c_a;&c_l;&c_ib;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_o;-&c_m;&c_je;&c_d;&c_n;&c_a; &c_je;&c_p;&c_o;&c_h;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_d;&c_n;&c_a; &c_je;&c_p;&c_o;&c_h;&c_a;</trd>,
    <trd>&c_h;&c_a;&c_l;&c_k;&c_o;&c_l;&c_i;&c_t;</trd>,
    <trd>&c_je;&c_n;&c_je;&c_o;&c_l;&c_i;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kupfersteinzeit</trd>,
    <trd>Kupferzeit</trd>,
    <trd>Chalkolithikum</trd>,
    <trd>&Auml;neolithikum</trd>
  </trdgrp>
  <trd lng="en">Chalcolithic period</trd>
  <trdgrp lng="fi">
    <trd>kuparikausi</trd>,
    <trd>kalkoliittinen kausi</trd>
  </trdgrp>
  <trd lng="hu">r&eacute;zkor</trd>
  <trdgrp lng="pl">
    <trd>chalkolit</trd>,
    <trd>eneolit</trd>,
    <trd>epoka miedzi</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>calcol&iacute;tico</trd>,
    <trd>eneol&iacute;tico</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_d;&c_n;&c_y;&c_j; &c_v;&c_je;&c_k;</trd>,
    <trd>&c_h;&c_a;&c_l;&c_k;&c_o;&c_l;&c_i;&c_t;</trd>,
    <trd>&c_e;&c_n;&c_je;&c_o;&c_l;&c_i;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>eneolitico</trd>,
    <trd>et&agrave; del rame</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kopertijd</trd>,
    <trd>kopersteentijd</trd>,
    <trd>chalcolithicum</trd>,
    <trd>&auml;neolithicum</trd>
  </trdgrp>
  <trd lng="uk">&c_h;&c_a;&#x43B;&#x44C;&#x43A;&#x43E;&#x43B;&c_ib;&c_t;</trd>
  <trd lng="zh">&#x7D05;&#x9285;&#x6642;&#x4EE3; <pr>h&#xF3;ngt&#xF3;ngsh&#xED;d&#xE0;i</pr></trd>
</drv>
</art>
