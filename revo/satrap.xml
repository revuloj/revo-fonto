<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: satrap.xml,v 1.18 2022/03/26 20:43:11 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>satrap</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="satrap.0o">
  <kap><tld/>o</kap>
  <snc mrk="satrap.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="sin" cel="provin.0estro.POL">Provincestro</ref> en la
      antikva Persujo:
      <ekz>
        Dario bonvolis starigi super sia regno cent dudek <tld/>ojn, por
        administri lian tutan regnon
        <fnt><bib>MT</bib>, <lok>&Dan; 6:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_t;&c_r;&c_a;&c_p;</trd>
  <trd lng="bg">&c_s;&c_a;&c_t;&c_r;&c_a;&c_p;</trd>
  <trdgrp lng="cs">
    <trd>bezohledn&#xFD; &#x10D;l.<klr>(p&#x159;en.)</klr></trd>,
    <trd>panova&#x10D;n&#xFD;</trd>,
    <trd>satrapa</trd>
  </trdgrp>
  <trd lng="de">Satrap</trd>
  <trd lng="en">satrap</trd>
  <trd lng="fr">satrape</trd>
  <trd lng="hu">szatrapa</trd>
  <trd lng="it">satrapo</trd>
  <trd lng="nl">satraap</trd>
  <trd lng="ru">&c_s;&c_a;&c_t;&c_r;&c_a;&c_p;</trd>
  <trdgrp lng="sk">
    <trd>bezoh&#x13E;adn&#xFD; &#x10D;lovek</trd>,
    <trd>panova&#x10D;n&#xFD;</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x430;&#x442;&#x440;&#x430;&#x43F;</trd>
</drv>
</art>
