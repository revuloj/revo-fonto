<vortaro>
<art mrk="$Id: gxarkand.xml,v 1.3 2022/10/24 15:42:57 revo Exp $">
<kap>
  <rad>&Gcirc;arkand</rad>/o
</kap>
<drv mrk="gxarkand.0o">
  <kap><tld/>o</kap>
  <snc mrk="gxarkand.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Gubernio en la orienta parto de
      <ref tip="malprt" cel="hind.H0ujo.POL">Hindujo</ref>:
      <ekz>
        la mundaoj estas indi&gcirc;ena etno en Barato; ili 
        devenas el la regiono <tld/>o
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Mundaoj">
          Mundaoj</url>, 2013-03-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_D;&c_zh;&c_g;&c_a;&c_r;&c_k;&c_g;&c_a;&c_n;&c_d;</trd>
  <trd lng="fr">Jharkhand</trd>
</drv>
</art>

