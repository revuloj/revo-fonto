<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>punt</rad>/o
</kap>
<drv mrk="punt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Maldensa, artisme interspacigita teksa&jcirc;o, konsistanta en delikata
      reto el silkaj a&ubreve; linaj fadenoj, servanta kiel fono por flora&jcirc;oj a&ubreve;
      ornama&jcirc;oj faritaj per kudrilo a&ubreve; &scirc;piniletoj:
      <ekz>
        la maldensa arbaro kovras la teron kvaza&ubreve; aranea
        <tld/>o<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Spitze</trd>
  <trd lng="en">lace</trd>
  <trd lng="fr">dentelle</trd>
  <trd lng="nl">kant</trd>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_zh;&c_je;&c_v;&c_o;</trd>
</drv>
</art>
