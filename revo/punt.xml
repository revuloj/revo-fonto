<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>punt</rad>/o
</kap>
<drv mrk="punt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc mrk="punt.0o.TEKS">
    <dif>
      Maldensa, artisme interspacigita teksa&jcirc;o, konsistanta en delikata
      reto el silkaj a&ubreve; linaj fadenoj, servanta kiel fono por flora&jcirc;oj
      a&ubreve;
      ornama&jcirc;oj faritaj per kudrilo a&ubreve; &scirc;piniletoj:
      <ekz>
        la maldensa arbaro kovras la teron kvaza&ubreve; aranea
        <tld/>o<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_u;&c_n;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_d;&c_a;&c_n;&c_t;&c_je;&c_l;&c_a;</trd>
  <trd lng="ca">punta</trd>
  <trd lng="de">Spitze</trd>
  <trd lng="en">lace</trd>
  <trd lng="es">encaje</trd>
  <trd lng="fr">dentelle</trd>
  <trd lng="hu">csipke</trd>
  <trd lng="nl">kant</trd>
  <trdgrp lng="pt">
   <trd>renda (tecido)</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_u;&c_zh;&c_je;&c_v;&c_o;</trd>
</drv>
</art>
