<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: retret.xml,v 1.9 2025/04/05 06:43:43 revo Exp $">
<kap>
  <rad>retret</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="retret.0o">
  <kap><tld/>o</kap>
  <snc mrk="retret.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Reveno, cedo de armeo, kiu forlasas batalpozicion a&ubreve;
      teritorion:
      <ekz>
        mi ankora&ubreve; havas du horojn anta&ubreve; <klr>ol</klr>
        mi ordonos la <tld/>on
        <fnt>
          <aut>R. Van Gulik, trad. B. Harris</aut>,
          <vrk><url ref="http://esperanto-me.org/Literaturo/cxerkoj/cxerkoj.htm">
          La &Ccirc;erkoj de la Imperiestro</url></vrk>,
          <lok>[2004]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>malanta&#365;eniro</ke>.
  </rim>

  <trd lng="be">&c_a;&c_d;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je; <klr tip="amb">(&c_v;&c_o;&c_j;&c_s;&c_k;&c_a;)</klr></trd>
  <trd lng="de">R&uuml;ckzug</trd>
  <trd lng="es">retirada</trd>
  <trd lng="fr">retraite<klr> (militaire)</klr></trd>
  <trd lng="pl">wycofanie  <klr>(wojsko)</klr></trd>
  <trd lng="ro">retragere</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_t;&c_u;&#1087;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_h;&c_ib;&c_d; (&c_v;&c_ib;&c_d; &c_s;&c_v;&c_ib;&c_t;&c_s;&#1100;&#1082;&#1086;&c_g;&#1086; &c_zh;&c_i;&c_t;&c_t;&#1103;</trd>,
    <trd>&#1087;&#1086;&#1083;&c_ib;&c_t;&c_i;&#1082;&c_i; &c_ib; &c_t;.&#1087;.)</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_t;&c_a;&c_v;&#1082;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#25289;&#22238; <pr>l&#257;hu&#237;</pr></trd>
</drv>
</art>

