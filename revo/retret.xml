<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: retret.xml,v 1.5 2022/09/23 22:38:15 revo Exp $">
<kap>
  <rad>retret</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="retret.0o">
  <kap><tld/>o</kap>
  <snc mrk="retret.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Reveno, cedo de armeo, kiu forlasas batalpozicion a&ubreve;
      teritorion:
      <ekz>
        mi ankora&ubreve; havas du horojn anta&ubreve; <klr>ol</klr>
        mi ordonos la <tld/>on
        <fnt>
          <aut>R. Van Gulik, trad. B. Harris</aut>,
          <vrk><url ref="http://esperanto-me.org/Literaturo/cxerkoj/cxerkoj.htm">
          La &Ccirc;erkoj de la Imperiestro</url></vrk>,
          <lok>[2004]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">retirada</trd>
  <trd lng="fr">retraite<klr> (militaire)</klr></trd>
  <trd lng="pl">wycofanie  <klr>(wojsko)</klr></trd>
  <trd lng="ro">retragere</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_t;&c_u;&#x43F;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_h;&c_ib;&c_d; (&c_v;&c_ib;&c_d; &c_s;&c_v;&c_ib;&c_t;&c_s;&#x44C;&#x43A;&#x43E;&c_g;&#x43E; &c_zh;&c_i;&c_t;&c_t;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&c_ib;&c_t;&c_i;&#x43A;&c_i; &c_ib; &c_t;.&#x43F;.)</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_t;&c_a;&c_v;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>
</art>

