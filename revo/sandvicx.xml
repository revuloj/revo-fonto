<art mrk="$Id: sandvicx.xml,v 1.14 2022/11/04 08:41:42 revo Exp $">
<kap>
  <ofc>8</ofc><rad>sandvi&ccirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sandvicx.0o">
  <kap><ofc>8</ofc><tld/>o <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="&AdE;/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>III. Oficialaj
Radikoj</lok></fnt></kap>
  <snc mrk="sandvicx.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Rapidman&gcirc;a&jcirc;o el pano entran&ccirc;ita a&ubreve;
      disduigita, far&ccirc;ita per
      legomoj,
      viandoj a&ubreve; sa&ubreve;coj:
      <ekz>
        mi nur frandis ian kokinan <tld/>on
        <fnt>
        <aut>knaboj ne ploras</aut>,
        <vrk><url ref="http://www.livejournal.com/users/knaboj_neploras/">[blogo]</url></vrk>,
        <lok>Soleca tago, 2004-05-14</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_n;&c_d;&c_v;&c_ib;&c_ch;</trd>,
    <trd>&c_s;&c_a;&c_n;&c_d;&c_v;&c_ib;&c_ch;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_a;&c_n;&c_d;&c_v;&c_i;&c_ch;</trd>
  <trdgrp lng="de">
    <trd>Sandwich</trd>,
    <trd>Schnitte</trd>
  </trdgrp>
  <trd lng="en">sandwich</trd>
  <trdgrp lng="es">
    <trd>bocadillo</trd>,
    <trd>emparedado</trd>,
    <trd>sanduche</trd>
  </trdgrp>
  <trd lng="fr">sandwich</trd>
  <trd lng="hu">szendvics</trd>
  <trdgrp lng="id">
    <trd><ind>roti</ind> isi</trd>,
    <trd><ind>roti</ind> lapis</trd>,
    <trd>sandwich</trd>
  </trdgrp>
  <trdgrp lng="it">
   <trd><ind>panino</ind> imbottito</trd>, 
   <trd>sandwich</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sandwich</trd>,
    <trd>belegd broodje</trd>,
    <trd>belegde boterham</trd>,
    <trd>boterham</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>sandu&iacute;che</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_a;&#x43D;&c_d;&c_v;&c_ib;&c_ch;</trd>,
    <trd>&c_s;&c_je;&#x43D;&c_d;&c_v;&c_ib;&c_ch;</trd>
  </trdgrp>
</drv>
</art>

