<drv mrk="kamp.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kamp.0isto.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="dif" cel="kultur.ter0isto.AGR">Terkulturisto</ref>:
      <ekz>
        <nom>Esav</nom> fari&gcirc;is lerta &ccirc;asisto, <tld/>isto
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 25:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        pluganta <tld/>isto
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Viktorka</nom> estas filino de <tld/>isto el
        <nom>Zernov</nom>
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K. Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/III.">
          Avineto</url></vrk>,
          <lok>1909-1914</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>paysan<klr> (subst.)</klr></trd>,
    <trd>agriculteur</trd>
  </trdgrp>
</drv>

<drv mrk="kamp.batal0o">
  <kap>batal<tld/>o</kap>
  <snc mrk="kamp.batal0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="dif" cel="batal.0ejo.MIL">Batalejo</ref>:
      <ekz>
        neniam plu detran&ccirc;u la brakojn al la malamikoj, falintaj sur la
        batal<tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvankam homo povas venki mil malamikojn sur batal<tld/>o, tamen
        estus pli glora venkinto, kiu venkis sin mem
        <fnt>
          <aut>trad. T. T. Anuruddha</aut>,
          <vrk>La Vojo al Nirvano a&ubreve; la Dhammapado</vrk>,
          <lok>1973</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mondvastaj elektronikaj, precipe komputilaj kaj interretaj,
        sistemoj &scirc;ajnas
        fari&gcirc;i batal<tld/>o de la estonteco
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007289.html">Militado sen bataliloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_l;&c_je; &c_b;&c_ib;&c_t;&c_v;&c_y;</trd>
  <trdgrp lng="br">
    <trd>aerva</trd>,
    <trd>tachenn <ind>emgann</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bitevn&iacute; pole</trd>,
    <trd>boji&scaron;t&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kampfst&auml;tte</trd>,
    <trd>Schlachtfeld</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>battlefield</trd>,
    <trd>battleground</trd>
  </trdgrp>
  <trd lng="es">campo de batalla</trd>
  <trd lng="fr">champ de bataille</trd>
  <trdgrp lng="hu">
    <trd>csatat&eacute;r</trd>,
    <trd>harcmez&odblac;</trd>
  </trdgrp>
  <trd lng="id"><ind>medan</ind> pertempuran</trd>
  <trd lng="it">campo di battaglia</trd>
  <trdgrp lng="pl">
    <trd>pole walki</trd>,
    <trd>pole bitwy</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je; &c_b;&c_i;&c_t;&c_v;&c_y;</trd>
  <trd lng="sk">bojisko</trd>
</drv>

<drv mrk="kamp.esplor0o">
  <kap>esplor<tld/>o</kap>
  <snc mrk="kamp.esplor0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Temaro, objektaro, kiun oni esploras:
      <ekz>
        aperis pluraj novaj esplor<tld/>oj en biologio (interalie
        genetiko kiel scienco
        pri heredo, ekologio, kiel scienco pri lukto por vivo, apartaj
        esplorkampoj
        kiel evolua fiziologio, evolua biokemio, ktp)
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="https://www.monato.be/2009/009858.php">La mondrevolucia verko finfine en Esperanto</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        vidu la rilatojn al aliaj grandaj kaj gravaj esplor<tld/>oj,
        ekzemple
        astronomio, genetiko a&ubreve; spacveturado
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2010/010081.php">Misteroj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia esplor<tld/>o rilatas al la analizo de la hungara lingvo 
        cele al parolsintezo
        <fnt>
          <aut>I. Koutny</aut>,
          <vrk>Tra scienca analizo al novaj perspektivoj</vrk>,
          <bib>LOdE</bib>,
          <lok>2002:11 (97)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">campo de estudio</trd>
  <trd lng="fr">domaine d'&eacute;tude</trd>
  <trd lng="id"><ind>bidang</ind> penelitian</trd>
</drv>

<drv mrk="kamp.labor0o">
  <kap>labor<tld/>o</kap>
  <snc mrk="kamp.labor0o.KOMUNE">
    <dif>
      <ref tip="super" cel="kamp.0o.tereno">Kampo</ref>,
      temaro, aferoj, pri kiuj oni laboras:
      <ekz>
        dum mia &jcirc;urnalista kariero &#x2010; kiel okazas en multaj laborkampoj &#x2010;
        jam plurfoje mi
        devis lerni novajn teknikojn
        <fnt>
          <bib>Monato</bib>
          <lok><url ref="http://steloj.de/esperanto/monato2/007058.html">http://steloj.de/esperanto/monato2/007058.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        miaj specifaj interesoj en mia labor<tld/>o estas fibroptiko 
        kaj perkomputila lernado
        <fnt>
          <aut>h. reid</aut>,
          <vrk><url ref="http://www.piloto.u-net.com/pages/esp1.htm">
          esperantaj pa&gcirc;oj de hju rid</url></vrk>,
          <lok>[1997]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">pole p&uring;sobnosti</trd>
  <trd lng="es">campo<klr> (de trabajo)</klr></trd>
  <trd lng="fr">domaine<klr> (de travail)</klr></trd>
  <trd lng="id"><ind>lapang</ind>an kerja</trd>
  <trd lng="sk">pole p&ocirc;sobnosti</trd>
</drv>

<drv mrk="kamp.vid0o">
  <kap>vid<tld/>o</kap>
  <snc mrk="kamp.vid0o.KOMUNE">
    <dif>
      La spaco, kiun rigardo sammomente ampleksas.
    </dif>
    <subsnc mrk="kamp.vid0o.FIZL">
      <uzo tip="fak">FIZL</uzo>
      <dif>
        &Ccirc;io, kion okuloj sensas:
        <ekz>
          birdrigardante de sur aviadilo super la akvo, oni havas 
          lar&gcirc;an vid<tld/>on
          <fnt>
            <aut>Zhao Xi, Wang Lihua</aut>,
            <vrk><url ref="http://www.espero.com.cn/se/txt/2013-05/06/content_539643.htm">
            Idealaj lokoj por pasigi someron</url></vrk>,
            <lok>2013-05-06</lok>
          </fnt>
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="kamp.vid0o.FOT">
      <uzo tip="fak">FOT</uzo>
      <dif>
        &Ccirc;io, kion objektivo vidigas.
      </dif>
    </subsnc>
  </snc>
  <trd lng="es">campo de visi&oacute;n</trd>
  <trd lng="fr">champ de vision</trd>
</drv>
