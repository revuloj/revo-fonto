    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_b;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol; &c_d;&c_a;&c_s;&c_mol;&c_l;&c_je;&c_d;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;&c_w;</trd>
