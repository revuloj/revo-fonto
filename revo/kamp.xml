    </dif>
    <subsnc mrk="kamp.vid0o.FIZL">
      <uzo tip="fak">FIZL</uzo>
      <dif>
        &Ccirc;io, kion okuloj sensas:
        <ekz>
          birdrigardante de sur aviadilo super la akvo, oni havas 
          lar&gcirc;an vid<tld/>on
          <fnt>
            <aut>Zhao Xi, Wang Lihua</aut>,
            <vrk><url
            ref="http://www.espero.com.cn/se/txt/2013-05/06/content_539643.htm">
            Idealaj lokoj por pasigi someron</url></vrk>,
            <lok>2013-05-06</lok>
          </fnt>
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="kamp.vid0o.FOT">
      <uzo tip="fak">FOT</uzo>
      <dif>
        &Ccirc;io, kion objektivo vidigas.
      </dif>
    </subsnc>
  </snc>
  <trd lng="es">campo de visi&oacute;n</trd>
  <trd lng="fr">champ de vision</trd>
