<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kamp</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kamp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kamp.0o.KOMUNE">
    <dif>
      Ebena a&ubreve; limigita spaco, en kiu okazas io:
    </dif>
    <subsnc mrk="kamp.0o.AGR">
      <uzo tip="fak">AGR</uzo>
      <dif>
        Terpeco, ebena a&ubreve; preska&ubreve; ebena, kulturita
        a&ubreve; kulturebla:
        <ekz>
          tritikaj kaj hordeaj <tld/>oj;
        </ekz>
        <ekz>
          <tld/>aj floroj
          <fnt>
            <bib>Fab2</bib>
            <lok>Poligono</lok>
          </fnt>;
        </ekz>
        <ekz>
          la Eternulo kreis el la tero &ccirc;iujn bestojn de la <tld/>o kaj &ccirc;iujn birdojn de la
          &ccirc;ielo
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 2:19</lok>
          </fnt>;
        </ekz>
        <ekz>
          iru sur la <tld/>on kaj &ccirc;asu por mi &ccirc;asa&jcirc;on
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 27:3</lok>
          </fnt>;
        </ekz>
        <ekz>
          plugi <klr>[&#8230;]</klr> <tld/>on
          <fnt>
            <bib>Monato</bib>
            <aut>Boris Kolker</aut>
            <vrk><url ref="http://steloj.de/esperanto/monato2/006813.html"
                >Sa&gcirc;a kapo duonvorton komprenas</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          prisemi la <tld/>on
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 47:24</lok>
          </fnt>;
        </ekz>
        <ekz>
          ni ligis garbojn meze de la <tld/>o
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 37:7</lok>
          </fnt>;
        </ekz>
        <ekz>
          sur la <tld/>o ondi&gcirc;adis la greno
          <fnt>
            <bib>Fab2</bib>
            <lok>Sambuka virineto</lok>
          </fnt>;
        </ekz>
        <ekz>
          li ser&ccirc;as la venton sur la <tld/>o
          <fnt>
            <bib>PrV</bib>
          </fnt>;
        </ekz>
        <ekz>
          la brilo de la nordlumo super la glaciaj <tld/>oj de Laplando
          <fnt>
            <bib>Fab3</bib>
            <lok>Birdo Fenikso</lok>
          </fnt>;
        </ekz>
        <ekz>
          la ne&gcirc;aj <tld/>oj
          <fnt>
            <bib>Fab2</bib>
            <lok>Ne&gcirc;a re&gcirc;ino</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="sub">
        <ref cel="agr.0o">agro</ref>
      </refgrp>
      <trdgrp lng="de">
        <trd>Feld</trd>,
        <trd>Acker</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>f&ouml;ld <klr tip="ind">(term&odblac;-)</klr></trd>,
        <trd>term&odblac;f&ouml;ld</trd>
      </trdgrp>
      <trd lng="id">ladang</trd>
     <trd lng="it">prato</trd>
      <trdgrp lng="nl">
        <trd>veld</trd>,
        <trd>akker</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kamp.0o.historia">
      <dif>
        Loko, en kiu io okazis:
        <ekz>
          <tld/>o de l' mortintoj
          <fnt>
            <bib>BdV</bib>
          </fnt>;
        </ekz>
        <ekz>
          la <tld/>o de la batalo
          <fnt>
            <bib>MT</bib>
            <lok>I. Samuel 4:2</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Feld</trd>,
        <trd>Gebiet</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>mez&odblac;</trd>,
        <trd>t&eacute;r</trd>
      </trdgrp>
      <trd lng="id">kamp</trd>
      <trdgrp lng="nl">
        <trd>veld</trd>,
        <trd>gebied</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kamp.0o.medio">
      <dif>
        Loko, inter kies limoj efikas ia ago:
        <ekz>
          operacia <tld/>o
          <klr>(parto de la korpo, kiun trafas operacio)</klr>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="medi.0o">medio</ref>
      </refgrp>
      <trdgrp lng="de">
        <trd>Feld</trd>,
        <trd>Gebiet</trd>
      </trdgrp>
      <trd lng="hu">ter&uuml;let</trd>
      <trd lng="id">medan</trd>
      <trd lng="nl">veld</trd>
    </subsnc>
    <subsnc mrk="kamp.0o.tereno">
      <dif>
        Sumo de la aferoj, kiujn atingas ia agado:
        <ekz>
          <tld/>o de laboro, de aktiveco;
        </ekz>
        <ekz>
          la <tld/>o de la historio prezentas multegajn enigmojn;
        </ekz>
        <ekz>
          politika <tld/>o
          <fnt>
            <bib>Monato</bib>
            <aut>S. Maul</aut>
            <vrk><url ref="http://steloj.de/esperanto/monato2/006831.html"
                >La&ubreve; ritmo de la rizejoj</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          tio multe vastigas la <tld/>on de gaela filologio
          <fnt>
            <bib>Monato</bib>
            <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9311-matematiko.txt"
                >Matematiko</url></vrk>
            <lok>93/11</lok>
          </fnt>;
        </ekz>
        <ekz>
          riskoj <klr>[&#8230;]</klr> trovi&gcirc;as en kvar <tld/>oj: bruo, lumo, bestoj kaj vido
          <fnt>
            <bib>Monato</bib>
            <aut>Roland Rotsaert</aut>
            <vrk><url ref="http://steloj.de/esperanto/monato2/007052.html"
                >Verda nature, science, socie</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          tiu temo malfermas <tld/>on al <klr>(ebligas)</klr> plej
          granda fantazio;
        </ekz>
        <ekz>
          prepari <tld/>on por mondpaco.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="aren.0o">areno</ref>,
        <ref cel="region.0o">regiono</ref>,
        <ref cel="spac.0o">spaco</ref>,
        <ref cel="teren.0o">tereno</ref>
      </refgrp>
      <trdgrp lng="de">
        <trd>Feld</trd>,
        <trd>Bereich</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>ter&uuml;let</trd>,
        <trd>t&eacute;r</trd>
      </trdgrp>
      <trd lng="pl">pole</trd>
      <trdgrp lng="ru">
        <trd>&c_n;&c_i;&c_v;&c_a;</trd>,
        <trd>&c_p;&c_o;&c_l;&c_je;</trd>,
        <trd>&c_p;&c_o;&c_ch;&c_v;&c_a;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <snc mrk="kamp.0o.areoparto">
    <dif>
      Difinita parto de areo:
    </dif>
    <subsnc mrk="kamp.0o.luda">
      <dif>
        <klr>(pp sportaj a&ubreve; tabulaj ludoj)</klr> Unu el la areoj,
        kie unuopa ludanto, ludfiguro a&ubreve; teamo la&ubreve;regule povas esti:
        <ekz>
          nigra, blanka, libera <tld/>o;
        </ekz>
        <ekz>
          re&gcirc;o movi&gcirc;as <klr>[&#8230;]</klr> al libervola el la najbaraj <tld/>oj
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=4074"
                >&Scirc;ako</url></lok>
          </fnt>;
        </ekz>
      </dif>
      <refgrp tip="vid">
         <ref cel="fak.0o.ujo">fako<sncref/></ref>,
         <ref cel="cxel.0o.fako">&ccirc;elo<sncref/></ref>
      </refgrp>
      <trd lng="de">Spielfeld</trd>
    </subsnc>
    <subsnc mrk="kamp.0o.formulara">
      <uzo tip="stl">KOMUNE</uzo>
      <uzo tip="fak">KOMP</uzo>
      <dif>
        Ero de <ref tip="malprt" cel="formul.0aro.radika">formularo</ref>:
        <ekz>
          en la <tld/>o <ctl>Nomo:</ctl> indiku vian familinomon en
          &gcirc;ia nacilingva formo.
        </ekz>
      </dif>
      <trdgrp lng="ru">
        <trd>&c_g;&c_r;&c_a;&c_f;&c_a;</trd>,
        <trd>&c_p;&c_o;&c_l;&c_je;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Feld</trd>,
        <trd>K&auml;stchen</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>mez&odblac;</trd>,
        <trd>kocka</trd>
      </trdgrp>
      <trd lng="nl">veld</trd>
      <trd lng="pl">pole</trd>
    </subsnc>
    <subsnc mrk="kamp.0o.blazona">
      <uzo tip="fak">HER</uzo>
      <dif>
        Kolora parto de la fono de flago a&ubreve; blazono:
        <ekz>
          blua ankro kaj blua &scirc;losilo kruci&gcirc;antaj sur blanka <tld/>o
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          blanka aglo kun krono sur ru&gcirc;a <tld/>o
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=204846"
                >Blazono de Pollando</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <ref tip="super" cel="fon.0o.KOMUNE">fono<sncref/></ref>
      <trd lng="de">Grund <klr>(Flagge, Wappen)</klr></trd>
    </subsnc>
  </snc>
  <snc mrk="kamp.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Spaco, en kiu efikas fizika forto:
      <ekz>
        kiel la elektran <tld/>on, oni povas difini la magnetan <tld/>on per la forto,
        kiun &gcirc;i produktas
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18559"
              >Magneta kampo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> kosmosondilo <klr>[&#8230;]</klr> foriras el la tera gravita <tld/>o
        kaj superas la eskapan rapidon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33143"
              >Kosmosondilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kraftfeld</trd>
  </snc>
  <snc mrk="kamp.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib>, <lok>diver&gcirc;enco</lok></fnt>
    <uzo tip="klr">(fakula &jcirc;argono, ofte uzata en fiziko)</uzo>
    <dif>
      <ref tip="super" cel="bild.0igo.MAT">Bildigo</ref> de iu parto de
      finidimensia e&ubreve;klida <ref cel="spac.euxklida0o">afina spaco
      </ref>,
      provizita per orta ununorma koordinatsistemo, al iu
      reela <ref cel="spac.vektora0o">vektora spaco</ref>:
      <ekz>
        skalara <tld/>o <klr>(la celo-aro estas la aro de reeloj)</klr>,
      </ekz>
      <ekz>
        vektora <tld/>o <klr>(la celo-aro havas pli ol unu
        dimension)</klr>;
      </ekz>
      <ekz>
        studi la <tld/>on de temperaturoj en varmigita objekto,
      </ekz>
      <ekz>
        la <tld/>on de rapidoj en movi&gcirc;anta solido;
      </ekz>
      <ekz>
        <ref cel="elektr.0akampo">elektra <tld/>o</ref>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diverg1.0o">diver&gcirc;enco</ref>,
      <ref cel="gradient.0o">gradiento</ref>,
      <ref cel="kirl.0o.MAT">kirlo</ref>,
      <ref cel="operat.laplaca0o">laplaca operatoro</ref>.
    </refgrp>
    <trd lng="de">Feld</trd>
    <trd lng="en">field</trd>
    <trd lng="fr">champ</trd>
    <trd lng="hu">mez&odblac;</trd>
    <trd lng="ku">zev&icirc;</trd>
    <trd lng="pl">pole</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_je;</trd>
  </snc>
  <snc mrk="kamp.0o.dateno">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Ano de <ref tip="malprt" cel="rikord.0o">rikordo</ref>:
      <ekz>
        havigi valoron al <klr>[&#8230;]</klr> <tld/>o de rikordo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=525701"
              >Valorizo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">pole</trd>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_l;&c_je;</trd>
  <trd lng="ca">camp</trd>
  <trdgrp lng="cs">
    <trd>are&aacute;l</trd>,
    <trd>oblast</trd>,
    <trd>p&rcaron;en. pracovn&iacute; oblast a pod.</trd>,
    <trd>ter&eacute;n</trd>
  </trdgrp>
  <trd lng="en">field</trd>
  <trd lng="es">campo</trd>
  <trd lng="fr">champ</trd>
  <trd lng="it">campo</trd>
  <trd lng="oc">camp</trd>
  <trd lng="pt">campo</trd>
  <trdgrp lng="ro">
    <trd>c&acirc;mp</trd>,
    <trd>c&acirc;mpie</trd>,
    <trd>lan</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>are&aacute;l</trd>,
    <trd>oblas&tcaron;</trd>,
    <trd>ter&eacute;n</trd>
  </trdgrp>
</drv>

<drv mrk="kamp.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Lo&gcirc;i en <tld/>oj, stariginte provizorajn lo&gcirc;ejojn
      a&ubreve; tendojn por trupo a&ubreve; armeo:
      <ekz>
        la Romanoj <tld/>is en tiu valo;
      </ekz>
      <ekz>
        la skoltoj <tld/>as en la arbaro.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_ja;&c_c;&c_mol;
  <ind>&c_l;&c_ja;&c_g;&c_je;&c_r;&c_a;&c_m;</ind></trd>
  <trdgrp lng="cs">
    <trd>kampovat</trd>,
    <trd>t&aacute;bo&rcaron;it</trd>
  </trdgrp>
  <trd lng="de">lagern</trd>
  <trd lng="es">acampar</trd>
  <trd lng="fr">camper</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;borozik</trd>,
    <trd>kempingezik</trd>
  </trdgrp>
  <trd lng="id">[<ind>ladang</ind>] berladang</trd>
  <trd lng="it">accamparsi</trd>
  <trd lng="nl">kamperen</trd>
  <trdgrp lng="pl">
    <trd>obozowa&cacute;</trd>,
    <trd>by&cacute; na <ind>kempingu</ind></trd>
  </trdgrp>
  <trd lng="ru">
 &c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol; &c_l;&c_a;&c_g;&c_je;&c_r;&c_je;&c_m;
  </trd>
  <trd lng="sk">t&aacute;bori&tcaron;</trd>
</drv>

<drv mrk="kamp.0adejo">
  <kap><tld/>adejo</kap>
  <snc mrk="kamp.0adejo.TUR">
    <dif>
      Loko, en kiu oni <tld/>as.
    </dif>
    <ref tip="vid" cel="tend.0aro">tendaro</ref>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_g;&c_je;&c_r;
  <klr tip="amb">(&c_m;&c_je;&c_s;&c_c;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>autokempink</trd>,
    <trd>kemp</trd>,
    <trd>kempink <klr>(t&aacute;bor)</klr></trd>,
    <trd>t&aacute;bo&rcaron;i&scaron;t&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lager</trd>,
    <trd>Lagerplatz</trd>,
    <trd>Lagerstelle</trd>
  </trdgrp>
  <trd lng="es">campamento</trd>
  <trdgrp lng="fr">
    <trd>camp<klr tip="ind"> (o&ugrave; camper)</klr></trd>,
    <trd>camping</trd>,
    <trd>terrain de <ind>camping</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&aacute;borhely</trd>,
    <trd>kemping</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>accampamento</trd>,
    <trd>campeggio</trd>
    <trd>camping</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kamp</trd>,
    <trd>kampeerterrein</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ob&oacute;z</trd>,
    <trd>kemping</trd>
  </trdgrp>
  <trd lng="ro">camping</trd>
  <trd lng="ru">&c_l;&c_a;&c_g;&c_je;&c_r;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kemp</trd>,
    <trd>t&aacute;borisko</trd>
  </trdgrp>
</drv>

<drv mrk="kamp.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="kamp.0aro.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Aro de la <tld/>oj, precipe kontraste al la urboj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_a;&c_ja;
    &c_m;&c_ja;&c_s;&c_c;&c_o;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_jo;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>polnosti</trd>,
    <trd>venkov</trd>
  </trdgrp>
  <trd lng="de">Land</trd>
  <trd lng="es">el campo</trd>
  <trd lng="fr">campagne<klr tip="ind"> (contr&eacute;e rurale)</klr>
  </trd>
  <trd lng="hu">mez&odblac;s&eacute;g</trd>
  <trd lng="id">ladang</trd>
  <trd lng="it">campagna</trd>
  <trd lng="nl">platteland</trd>
  <trd lng="pl">wie&sacute;</trd>
  <trd lng="ro">sat</trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_a;&c_ja;
  &c_m;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>l&aacute;ny</trd>,
    <trd>vidiek</trd>
  </trdgrp>
</drv>

<drv mrk="kamp.0arano">
  <kap><tld/>arano, <var><kap><tld/>ulo</kap></var></kap>
  <snc mrk="kamp.0arano.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Lo&gcirc;anto en la <tld/>aro, precipe terkulturisto.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_l;&c_ja;&c_n;&c_ib;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>roln&iacute;k</trd>,
    <trd>venkovan</trd>,
    <trd>zem&ecaron;d&ecaron;lec</trd>
  </trdgrp>
  <trd lng="de">Bauer</trd>
  <trd lng="es">campesino</trd>
  <trdgrp lng="fr">
    <trd>campagnard<klr> (subst.)</klr></trd>,
    <trd>paysan<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">paraszt</trd>
  <trdgrp lng="it">
