<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kamp</rad>/o
</kap>
<drv mrk="kamp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kamp.0o.agro">
    <dif>
      Terpeco, ebena a&ubreve; preskau ebena, kulturita a&ubreve; kulturebla:
      <ekz>
        tritikaj kaj hordeaj <tld/>oj;
      </ekz>
      <ekz>
        <tld/>aj floroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo> la ne&gcirc;aj <tld/>oj<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="agr.0o">agro</ref>.
    </refgrp>
    <trdgrp lng="de">
       <trd>Feld</trd>,
       <trd>Acker</trd></trdgrp>
  </snc>
  <snc mrk="kamp.0o.historia">
    <dif>
      Loko en kiu io okazis:
      <ekz>
        batal<tld/>o;
      </ekz>
      <ekz>
        <tld/>o de l' mortintoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>Feld</trd>,
       <trd>Gebiet</trd></trdgrp>
  </snc>
  <snc mrk="kamp.0o.medio">
    <dif>
      Loko, inter kies limoj efikas ia ago:
      <ekz>
        operacia <tld/>o
        <klr>(parto de la korpo, kiun trafas operacio)</klr>;
      </ekz>
      <ekz>
        magneta <tld/>o
        <klr>(parto de la spaco, kiun regas magnetismo)</klr>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="medi.0o">medio</ref>.
    </refgrp>
    <trdgrp lng="de">
       <trd>Feld</trd>,
       <trd>Gebiet</trd></trdgrp>
  </snc>
  <snc  mrk="kamp.0o.tereno">
    <dif>
      Sumo de la aferoj, kiujn atingas ia agado:
      <ekz>
        <tld/>o de laboro, de aktiveco;
      </ekz>
      <ekz>
        la <tld/>o de la historio prezentas multegajn enigmojn;
      </ekz>
      <ekz>
        tiu temo malfermas <tld/>on al (ebligas) plej granda fantazio;
      </ekz>
      <ekz>
        prepari <tld/>on por mondpaco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aren.0o">areno</ref>,
      <ref cel="teren.0o">tereno</ref>,
      <ref cel="spac.0o">spaco</ref>,
      <ref cel="region.0o">regiono</ref>.
    </refgrp>
    <trdgrp lng="de">
       <trd>Feld</trd>,
       <trd>Bereich</trd></trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_i;&c_v;&c_a;</trd>,
      <trd>&c_p;&c_o;&c_l;&c_je;</trd>,
      <trd>&c_p;&c_o;&c_ch;&c_v;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kamp.0o.formulara">
    <uzo tip="stl">KOMUNE</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Ero de <ref tip="malprt" cel="formul.0aro.radika">formularo</ref>
      a&ubreve; ano de <ref tip="malprt" cel="rikord.0o">rikordo</ref>:
      <ekz>
        en la <tld/>o <ctl>Nomo:</ctl> indiku vian familinomon en
        via nacilingva formo.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_a;&c_f;&c_a;</trd>,
      <trd>&c_p;&c_o;&c_l;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>Feld</trd>,
       <trd>K&auml;stchen</trd></trdgrp>
  </snc>
  <trd lng="en">field</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je;</trd>
</drv>
<drv mrk="kamp.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Aro de la <tld/>oj, kontraste al la urboj.
    </dif>
  </snc>
  <trd lng="de">Land</trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_a;&c_ja;
  &c_m;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
<drv mrk="kamp.0arano0ulo">
  <kap><tld/>arano, <tld/>ulo</kap>
  <snc>
    <dif>
      Terkulturisto, lo&gcirc;anta en la <tld/>aro.
    </dif>
  </snc>
  <trd lng="de">Bauer</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_t;&c_mol;&c_ja;&c_n;&c_i;&c_n;</trd>
</drv>
<drv mrk="kamp.0adi">
  <kap><tld/>(ad)i<fnt>Z</fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Lo&gcirc;i en <tld/>oj, stariginte provizorajn lo&gcirc;ejojn a&ubreve;
      tendojn
      por trupo a&ubreve; armeo:
      <ekz>
        la Romanoj <tld/>is en tiu valo;
      </ekz>
      <ekz>
        la skoltoj <tld/>as en la arbaro.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">lagern</trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol;
  &c_l;&c_a;&c_g;&c_je;&c_r;&c_je;&c_m;</trd>
</drv>
<drv mrk="kamp.0adejo">
  <kap><tld/>(ad)ejo</kap>
  <snc>
    <dif>
      Loko, en kiu oni <tld/>as; tendaro.
    </dif>
  </snc>
   <trdgrp lng="de">
      <trd>Lager</trd>,
      <trd>Lagerplatz</trd>,
      <trd>Lagerstelle</trd></trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_g;&c_je;&c_r;&c_mol;</trd>
</drv>
</art>
