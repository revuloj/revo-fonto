<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kamp.xml,v 1.49 2020/01/05 20:10:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kamp</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kamp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kamp.0o.KOMUNE">
    <dif>
      Ebena a&ubreve; limigita spaco, en kiu okazas io:
    </dif>
    <subsnc mrk="kamp.0o.AGR">
      <uzo tip="fak">AGR</uzo>
      <dif>
        Terpeco, ebena a&ubreve; preska&ubreve; ebena, kulturita
        a&ubreve; kulturebla:
        <ekz>
          tritikaj kaj hordeaj <tld/>oj;
        </ekz>
        <ekz>
          <tld/>aj floroj
          <fnt>
            <bib>Fab2</bib>
            <lok>Poligono</lok>
          </fnt>;
        </ekz>
        <ekz>
          la Eternulo kreis el la tero &ccirc;iujn bestojn de la <tld/>o kaj &ccirc;iujn birdojn de la
          &ccirc;ielo
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 2:19</lok>
          </fnt>;
        </ekz>
        <ekz>
          iru sur la <tld/>on kaj &ccirc;asu por mi &ccirc;asa&jcirc;on
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 27:3</lok>
          </fnt>;
        </ekz>
        <ekz>
          plugi <klr>[&#8230;]</klr> <tld/>on
          <fnt>
            <bib>Monato</bib>
            <aut>Boris Kolker</aut>
            <vrk><url ref="http://steloj.de/esperanto/monato2/006813.html"
                >Sa&gcirc;a kapo duonvorton komprenas</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          prisemi la <tld/>on
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 47:24</lok>
          </fnt>;
        </ekz>
        <ekz>
          ni ligis garbojn meze de la <tld/>o
          <fnt>
            <bib>MT</bib>
            <lok>Genezo 37:7</lok>
          </fnt>;
        </ekz>
        <ekz>
          sur la <tld/>o ondi&gcirc;adis la greno
          <fnt>
            <bib>Fab2</bib>
            <lok>Sambuka virineto</lok>
          </fnt>;
        </ekz>
        <ekz>
          li ser&ccirc;as la venton sur la <tld/>o
          <fnt>
            <bib>PrV</bib>
          </fnt>;
        </ekz>
        <ekz>
          la brilo de la nordlumo super la glaciaj <tld/>oj de Laplando
          <fnt>
            <bib>Fab3</bib>
            <lok>Birdo Fenikso</lok>
          </fnt>;
        </ekz>
        <ekz>
          la ne&gcirc;aj <tld/>oj
          <fnt>
            <bib>Fab2</bib>
            <lok>Ne&gcirc;a re&gcirc;ino</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="sub">
        <ref cel="agr.0o">agro</ref>
      </refgrp>
      <trdgrp lng="de">
        <trd>Feld</trd>,
        <trd>Acker</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>f&ouml;ld <klr tip="ind">(term&odblac;-)</klr></trd>,
        <trd>term&odblac;f&ouml;ld</trd>
      </trdgrp>
      <trd lng="id">ladang</trd>
     <trd lng="it">prato</trd>
      <trdgrp lng="nl">
        <trd>veld</trd>,
        <trd>akker</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kamp.0o.historia">
      <dif>
        Loko, en kiu io okazis:
        <ekz>
          <tld/>o de l' mortintoj
          <fnt>
            <bib>BdV</bib>
          </fnt>;
        </ekz>
        <ekz>
          la <tld/>o de la batalo
          <fnt>
            <bib>MT</bib>
            <lok>I. Samuel 4:2</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Feld</trd>,
        <trd>Gebiet</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>mez&odblac;</trd>,
        <trd>t&eacute;r</trd>
      </trdgrp>
      <trd lng="id">kamp</trd>
      <trdgrp lng="nl">
        <trd>veld</trd>,
        <trd>gebied</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kamp.0o.medio">
      <dif>
        Loko, inter kies limoj efikas ia ago:
        <ekz>
          operacia <tld/>o
          <klr>(parto de la korpo, kiun trafas operacio)</klr>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="medi.0o">medio</ref>
      </refgrp>
      <trdgrp lng="de">
        <trd>Feld</trd>,
        <trd>Gebiet</trd>
      </trdgrp>
      <trd lng="hu">ter&uuml;let</trd>
      <trd lng="id">medan</trd>
      <trd lng="nl">veld</trd>
    </subsnc>
    <subsnc mrk="kamp.0o.tereno">
      <dif>
        Sumo de la aferoj, kiujn atingas ia agado:
        <ekz>
          <tld/>o de laboro, de aktiveco;
        </ekz>
        <ekz>
          la <tld/>o de la historio prezentas multegajn enigmojn;
        </ekz>
        <ekz>
          politika <tld/>o
          <fnt>
            <bib>Monato</bib>
            <aut>S. Maul</aut>
            <vrk><url ref="http://steloj.de/esperanto/monato2/006831.html"
                >La&ubreve; ritmo de la rizejoj</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          tio multe vastigas la <tld/>on de gaela filologio
          <fnt>
            <bib>Monato</bib>
            <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9311-matematiko.txt"
                >Matematiko</url></vrk>
            <lok>93/11</lok>
          </fnt>;
        </ekz>
        <ekz>
          riskoj <klr>[&#8230;]</klr> trovi&gcirc;as en kvar <tld/>oj: bruo, lumo, bestoj kaj vido
          <fnt>
            <bib>Monato</bib>
            <aut>Roland Rotsaert</aut>
            <vrk><url ref="http://steloj.de/esperanto/monato2/007052.html"
                >Verda nature, science, socie</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          tiu temo malfermas <tld/>on al <klr>(ebligas)</klr> plej
          granda fantazio;
        </ekz>
        <ekz>
          prepari <tld/>on por mondpaco.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="aren.0o">areno</ref>,
        <ref cel="region.0o">regiono</ref>,
        <ref cel="spac.0o">spaco</ref>,
        <ref cel="teren.0o">tereno</ref>
      </refgrp>
      <trdgrp lng="de">
        <trd>Feld</trd>,
        <trd>Bereich</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>ter&uuml;let</trd>,
        <trd>t&eacute;r</trd>
      </trdgrp>
      <trd lng="pl">pole</trd>
      <trdgrp lng="ru">
        <trd>&c_n;&c_i;&c_v;&c_a;</trd>,
        <trd>&c_p;&c_o;&c_l;&c_je;</trd>,
        <trd>&c_p;&c_o;&c_ch;&c_v;&c_a;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <snc mrk="kamp.0o.areoparto">
    <dif>
      Difinita parto de areo:
    </dif>
    <subsnc mrk="kamp.0o.luda">
      <dif>
        <klr>(pp sportaj a&ubreve; tabulaj ludoj)</klr> Unu el la areoj,
        kie unuopa ludanto, ludfiguro a&ubreve; teamo la&ubreve;regule povas esti:
        <ekz>
          nigra, blanka, libera <tld/>o;
        </ekz>
        <ekz>
          re&gcirc;o movi&gcirc;as <klr>[&#8230;]</klr> al libervola el la najbaraj <tld/>oj
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=4074"
                >&Scirc;ako</url></lok>
          </fnt>;
        </ekz>
      </dif>
      <refgrp tip="vid">
         <ref cel="fak.0o.ujo">fako<sncref/></ref>,
         <ref cel="cxel.0o.fako">&ccirc;elo<sncref/></ref>
      </refgrp>
      <trd lng="de">Spielfeld</trd>
    </subsnc>
    <subsnc mrk="kamp.0o.formulara">
      <uzo tip="stl">KOMUNE</uzo>
      <uzo tip="fak">KOMP</uzo>
      <dif>
        Ero de <ref tip="malprt" cel="formul.0aro.radika">formularo</ref>:
        <ekz>
          en la <tld/>o <ctl>Nomo:</ctl> indiku vian familinomon en
          &gcirc;ia nacilingva formo.
        </ekz>
      </dif>
      <trdgrp lng="ru">
        <trd>&c_g;&c_r;&c_a;&c_f;&c_a;</trd>,
        <trd>&c_p;&c_o;&c_l;&c_je;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Feld</trd>,
        <trd>K&auml;stchen</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>mez&odblac;</trd>,
        <trd>kocka</trd>
      </trdgrp>
      <trd lng="nl">veld</trd>
      <trd lng="pl">pole</trd>
    </subsnc>
    <subsnc mrk="kamp.0o.blazona">
      <uzo tip="fak">HER</uzo>
      <dif>
        Kolora parto de la fono de flago a&ubreve; blazono:
        <ekz>
          blua ankro kaj blua &scirc;losilo kruci&gcirc;antaj sur blanka <tld/>o
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          blanka aglo kun krono sur ru&gcirc;a <tld/>o
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=204846"
                >Blazono de Pollando</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <ref tip="super" cel="fon.0o.KOMUNE">fono<sncref/></ref>
      <trd lng="de">Grund <klr>(Flagge, Wappen)</klr></trd>
    </subsnc>
  </snc>
  <snc mrk="kamp.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Spaco, en kiu efikas fizika forto:
      <ekz>
        kiel la elektran <tld/>on, oni povas difini la magnetan <tld/>on per la forto,
        kiun &gcirc;i produktas
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18559"
              >Magneta kampo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> kosmosondilo <klr>[&#8230;]</klr> foriras el la tera gravita <tld/>o
        kaj superas la eskapan rapidon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33143"
              >Kosmosondilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kraftfeld</trd>
  </snc>
  <snc mrk="kamp.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>PIV1</bib>, <lok>diver&gcirc;enco</lok></fnt>
    <uzo tip="klr">(fakula &jcirc;argono, ofte uzata en fiziko)</uzo>
    <dif>
      <ref tip="super" cel="bild.0igo.MAT">Bildigo</ref> de iu parto de
      finidimensia e&ubreve;klida <ref cel="spac.euxklida0o">afina spaco
      </ref>,
      provizita per orta ununorma koordinatsistemo, al iu
      reela <ref cel="spac.vektora0o">vektora spaco</ref>:
      <ekz>
        skalara <tld/>o <klr>(la celo-aro estas la aro de reeloj)</klr>,
      </ekz>
      <ekz>
        vektora <tld/>o <klr>(la celo-aro havas pli ol unu
        dimension)</klr>;
      </ekz>
      <ekz>
        studi la <tld/>on de temperaturoj en varmigita objekto,
      </ekz>
      <ekz>
        la <tld/>on de rapidoj en movi&gcirc;anta solido;
      </ekz>
      <ekz>
        <ref cel="elektr.0akampo">elektra <tld/>o</ref>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diverg1.0o">diver&gcirc;enco</ref>,
      <ref cel="gradient.0o">gradiento</ref>,
      <ref cel="kirl.0o.MAT">kirlo</ref>,
      <ref cel="operat.laplaca0o">laplaca operatoro</ref>.
    </refgrp>
    <trd lng="de">Feld</trd>
    <trd lng="en">field</trd>
    <trd lng="fr">champ</trd>
    <trd lng="hu">mez&odblac;</trd>
    <trd lng="ku">zev&icirc;</trd>
    <trd lng="pl">pole</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_je;</trd>
  </snc>
  <snc mrk="kamp.0o.dateno">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Ano de <ref tip="malprt" cel="rikord.0o">rikordo</ref>:
      <ekz>
        havigi valoron al <klr>[&#8230;]</klr> <tld/>o de rikordo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=525701"
              >Valorizo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">pole</trd>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_l;&c_je;</trd>
  <trd lng="ca">camp</trd>
  <trdgrp lng="cs">
    <trd>are&aacute;l</trd>,
    <trd>oblast</trd>,
    <trd>p&rcaron;en. pracovn&iacute; oblast a pod.</trd>,
    <trd>ter&eacute;n</trd>
  </trdgrp>
  <trd lng="en">field</trd>
  <trd lng="es">campo</trd>
  <trd lng="fr">champ</trd>
  <trd lng="it">campo</trd>
  <trd lng="oc">camp</trd>
  <trd lng="pt">campo</trd>
  <trdgrp lng="ro">
    <trd>c&acirc;mp</trd>,
    <trd>c&acirc;mpie</trd>,
    <trd>lan</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>are&aacute;l</trd>,
    <trd>oblas&tcaron;</trd>,
    <trd>ter&eacute;n</trd>
  </trdgrp>
</drv>

<drv mrk="kamp.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kamp.0i.logxi">
    <dif>
      Lo&gcirc;i en <tld/>oj, stariginte provizorajn lo&gcirc;ejojn
      a&ubreve; tendojn por trupo a&ubreve; armeo:
      <ekz>
        la Romanoj <tld/>is en tiu valo;
      </ekz>
      <ekz>
        la skoltoj <tld/>as en la arbaro;
      </ekz>
      <ekz>
        ni ankora&ubreve; <tld/>is &ccirc;e la Saskuankujo kaj atendis la tagi&gcirc;on por intertrakti kun
        la koman&ccirc;oj
        <fnt>
          <vrk><url ref="https://www.peick.de/olda-certmano/la-sendito-de-vinetuo.html"
              >La sendito de Vinetuo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_ja;&c_c;&c_mol;
  <ind>&c_l;&c_ja;&c_g;&c_je;&c_r;&c_a;&c_m;</ind></trd>
  <trdgrp lng="cs">
    <trd>kampovat</trd>,
    <trd>t&aacute;bo&rcaron;it</trd>
  </trdgrp>
  <trd lng="de">lagern</trd>
  <trd lng="es">acampar</trd>
  <trd lng="fr">camper</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;borozik</trd>,
    <trd>kempingezik</trd>
  </trdgrp>
  <trd lng="id">[<ind>ladang</ind>] berladang</trd>
  <trd lng="it">accamparsi</trd>
  <trd lng="nl">kamperen</trd>
  <trdgrp lng="pl">
    <trd>obozowa&cacute;</trd>,
    <trd>by&cacute; na <ind>kempingu</ind></trd>
  </trdgrp>
  <trd lng="ru">
 &c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol; &c_l;&c_a;&c_g;&c_je;&c_r;&c_je;&c_m;
  </trd>
  <trd lng="sk">t&aacute;bori&tcaron;</trd>
</drv>

<drv mrk="kamp.0adejo">
  <kap><tld/>adejo</kap>
  <snc mrk="kamp.0adejo.TUR">
    <dif>
      Loko, en kiu oni <tld/>as:
      <ekz>
        fortikigita vintra milit<tld/>adejo de romianoj sur la suda bordo de Danubo
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="https://www.monato.be/2014/010140.php"
              >Arkeologia parko Karnunto</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la jaro 841 la vikingoj establis okaze de rabekskurso <tld/>adejon en
        <nom>Dubhlinn</nom> (nigra lageto)
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="https://www.monato.be/2006/008699.php"
              >Kara antikva malpura Dublino</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tend.0aro">tendaro</ref>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_g;&c_je;&c_r;
  <klr tip="amb">(&c_m;&c_je;&c_s;&c_c;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>autokempink</trd>,
    <trd>kemp</trd>,
    <trd>kempink <klr>(t&aacute;bor)</klr></trd>,
    <trd>t&aacute;bo&rcaron;i&scaron;t&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lager</trd>,
    <trd>Lagerplatz</trd>,
    <trd>Lagerstelle</trd>
  </trdgrp>
  <trd lng="es">campamento</trd>
  <trdgrp lng="fr">
    <trd>camp<klr tip="ind"> (o&ugrave; camper)</klr></trd>,
    <trd>camping</trd>,
    <trd>terrain de <ind>camping</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&aacute;borhely</trd>,
    <trd>kemping</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>accampamento</trd>,
    <trd>campeggio</trd>
    <trd>camping</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kamp</trd>,
    <trd>kampeerterrein</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ob&oacute;z</trd>,
    <trd>kemping</trd>
  </trdgrp>
  <trd lng="ro">camping</trd>
  <trd lng="ru">&c_l;&c_a;&c_g;&c_je;&c_r;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kemp</trd>,
    <trd>t&aacute;borisko</trd>
  </trdgrp>
</drv>

<drv mrk="kamp.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="kamp.0aro.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Aro de la <tld/>oj, precipe kontraste al la urboj,
      pejza&gcirc;o karakterizata de <tld/>oj:
      <ekz>
        ni eliru sur la <tld/>aron, ni enlo&gcirc;i&gcirc;u en la vila&gcirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Alta kanto 7:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        kien ajn li eniris, en vila&gcirc;ojn a&ubreve; en urbojn a&ubreve; sur la <tld/>aron, ili demetis
        la malsanulojn sur la placoj
        <fnt>
          <bib>NT</bib>
          <lok>Marko 6:56</lok>
        </fnt>;
      </ekz>
      <ekz>
        la urboj de la <tld/>aro
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 27:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        multaj supreniris el la <tld/>aro al <nom>Jerusalem</nom> anta&ubreve; la Pasko, por sin sanktigi
        <fnt>
          <bib>NT</bib>
          <lok>Johano 11:55</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni lo&gcirc;is en la <tld/>aro, en bieno
        <fnt>
          <bib>Fab3</bib>
          <lok>Kora sufero</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u ni ek&gcirc;uu <tld/>aran vivon, a&ubreve; &ccirc;u ni entreprenu postedzi&gcirc;an voja&gcirc;on
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_a;&c_ja;
    &c_m;&c_ja;&c_s;&c_c;&c_o;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_jo;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>polnosti</trd>,
    <trd>venkov</trd>
  </trdgrp>
  <trd lng="de">Land</trd>
  <trd lng="es">el campo</trd>
  <trd lng="fr">campagne<klr tip="ind"> (contr&eacute;e rurale)</klr>
  </trd>
  <trd lng="hu">mez&odblac;s&eacute;g</trd>
  <trd lng="id">ladang</trd>
  <trd lng="it">campagna</trd>
  <trd lng="nl">platteland</trd>
  <trd lng="pl">wie&sacute;</trd>
  <trd lng="ro">sat</trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_a;&c_ja;
  &c_m;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>l&aacute;ny</trd>,
    <trd>vidiek</trd>
  </trdgrp>
</drv>

<drv mrk="kamp.0arano">
  <kap><tld/>arano, <var><kap><tld/>ulo</kap></var></kap>
  <snc mrk="kamp.0arano.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Lo&gcirc;anto en la <tld/>aro, precipe terkulturisto:
      <ekz>
        nia maljuna amiko renkonti&gcirc;is kun <tld/>arano, kiu pelis bovinon
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>arano saltis sur la &ccirc;evalon kaj forrajdis
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni havis la feli&ccirc;on koni <klr>[&#8230;]</klr> <tld/>araninon, al kiu ni &scirc;uldas la plej
        belajn rakontojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        nobelo promesojn disdonas, <tld/>ulo promesojn plenumas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ofte &ccirc;apo de <tld/>ulo kovras kapon de sa&gcirc;ulo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_l;&c_ja;&c_n;&c_ib;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>roln&iacute;k</trd>,
    <trd>venkovan</trd>,
    <trd>zem&ecaron;d&ecaron;lec</trd>
  </trdgrp>
  <trd lng="de">Bauer</trd>
  <trd lng="es">campesino</trd>
  <trdgrp lng="fr">
    <trd>campagnard<klr> (subst.)</klr></trd>,
    <trd>paysan<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">paraszt</trd>
  <trdgrp lng="it">
    <trd>campagnolo<klr> (sost.)</klr></trd>,
    <trd>contadino</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>boer</trd>,
    <trd>landbouwer</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wie&sacute;niak</trd>,
    <trd>ch&lstroke;op</trd>,
    <trd>w&lstroke;o&sacute;cianin<klr> (przest.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>s&abreve;tean</trd>,
    <trd>&#539;&abreve;ran</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_t;&c_mol;&c_ja;&c_n;&c_i;&c_n;
  </trd>
  <trdgrp lng="sk">
    <trd>ro&lcaron;n&iacute;k</trd>,
    <trd>sedliak</trd>
  </trdgrp>
</drv>

<drv mrk="kamp.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kamp.0isto.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="dif" cel="kultur.ter0isto.AGR">Terkulturisto</ref>:
      <ekz>
        <nom>Esav</nom> fari&gcirc;is lerta &ccirc;asisto, <tld/>isto
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 25:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        pluganta <tld/>isto
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Viktorka</nom> estas filino de <tld/>isto el <nom>Zernov</nom>
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K. Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/III.">
          Avineto</url></vrk>,
          <lok>1909-1914</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>paysan<klr> (subst.)</klr></trd>,
    <trd>agriculteur</trd>
  </trdgrp>
</drv>

<drv mrk="kamp.batal0o">
  <kap>batal<tld/>o</kap>
  <snc mrk="kamp.batal0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="dif" cel="batal.0ejo.MIL">Batalejo</ref>:
      <ekz>
        neniam plu detran&ccirc;u la brakojn al la malamikoj, falintaj sur la batal<tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvankam homo povas venki mil malamikojn sur batal<tld/>o, tamen
        estus pli glora venkinto, kiu venkis sin mem
        <fnt>
          <aut>trad. T. T. Anuruddha</aut>,
          <vrk>La Vojo al Nirvano a&ubreve; la Dhammapado</vrk>,
          <lok>1973</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mondvastaj elektronikaj, precipe komputilaj kaj interretaj, sistemoj &scirc;ajnas
        fari&gcirc;i batal<tld/>o de la estonteco
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007289.html"
              >Militado sen bataliloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_l;&c_je; &c_b;&c_ib;&c_t;&c_v;&c_y;</trd>
  <trdgrp lng="br">
    <trd>aerva</trd>,
    <trd>tachenn <ind>emgann</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bitevn&iacute; pole</trd>,
    <trd>boji&scaron;t&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kampfst&auml;tte</trd>,
    <trd>Schlachtfeld</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>battlefield</trd>,
    <trd>battleground</trd>
  </trdgrp>
  <trd lng="es">campo de batalla</trd>
  <trd lng="fr">champ de bataille</trd>
  <trdgrp lng="hu">
    <trd>csatat&eacute;r</trd>,
    <trd>harcmez&odblac;</trd>
  </trdgrp>
  <trd lng="id"><ind>medan</ind> pertempuran</trd>
  <trd lng="it">campo di battaglia</trd>
  <trdgrp lng="pl">
    <trd>pole walki</trd>,
    <trd>pole bitwy</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je; &c_b;&c_i;&c_t;&c_v;&c_y;</trd>
  <trd lng="sk">bojisko</trd>
</drv>

<drv mrk="kamp.esplor0o">
  <kap>esplor<tld/>o</kap>
  <snc mrk="kamp.esplor0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Temaro, objektaro, kiun oni esploras:
      <ekz>
        aperis pluraj novaj esplor<tld/>oj en biologio (interalie genetiko kiel scienco
        pri heredo, ekologio, kiel scienco pri lukto por vivo, apartaj esplorkampoj
        kiel evolua fiziologio, evolua biokemio, ktp)
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="https://www.monato.be/2009/009858.php"
              >La mondrevolucia verko finfine en Esperanto</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        vidu la rilatojn al aliaj grandaj kaj gravaj esplor<tld/>oj, ekzemple
        astronomio, genetiko a&ubreve; spacveturado
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2010/010081.php"
              >Misteroj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia esplor<tld/>o rilatas al la analizo de la hungara lingvo 
        cele al parolsintezo
        <fnt>
          <aut>I. Koutny</aut>,
          <vrk>Tra scienca analizo al novaj perspektivoj</vrk>,
          <bib>LOdE</bib>,
          <lok>2002:11 (97)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">campo de estudio</trd>
  <trd lng="fr">domaine d'&eacute;tude</trd>
  <trd lng="id"><ind>bidang</ind> penelitian</trd>
</drv>

<drv mrk="kamp.labor0o">
  <kap>labor<tld/>o</kap>
  <snc mrk="kamp.labor0o.KOMUNE">
    <dif>
      <ref tip="super" cel="kamp.0o.tereno">Kampo</ref>,
      temaro, aferoj, pri kiuj oni laboras:
      <ekz>
        dum mia &jcirc;urnalista kariero &dash; kiel okazas en multaj laborkampoj &dash; jam plurfoje mi
        devis lerni novajn teknikojn
        <fnt>
          <bib>Monato</bib>
          <lok><url ref="http://steloj.de/esperanto/monato2/007058.html"
              >http://steloj.de/esperanto/monato2/007058.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        miaj specifaj interesoj en mia labor<tld/>o estas fibroptiko 
        kaj perkomputila lernado
        <fnt>
          <aut>h. reid</aut>,
          <vrk><url ref="http://www.piloto.u-net.com/pages/esp1.htm">
          esperantaj pa&gcirc;oj de hju rid</url></vrk>,
          <lok>[1997]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">pole p&uring;sobnosti</trd>
  <trd lng="es">campo<klr> (de trabajo)</klr></trd>
  <trd lng="fr">domaine<klr> (de travail)</klr></trd>
  <trd lng="id"><ind>lapang</ind>an kerja</trd>
  <trd lng="sk">pole p&ocirc;sobnosti</trd>
</drv>

<drv mrk="kamp.vid0o">
  <kap>vid<tld/>o</kap>
  <snc mrk="kamp.vid0o.KOMUNE">
    <dif>
      La spaco, kiun rigardo sammomente ampleksas.
    </dif>
    <subsnc mrk="kamp.vid0o.FIZL">
      <uzo tip="fak">FIZL</uzo>
      <dif>
        &Ccirc;io, kion okuloj sensas:
        <ekz>
          birdrigardante de sur aviadilo super la akvo, oni havas 
          lar&gcirc;an vid<tld/>on
          <fnt>
            <aut>Zhao Xi, Wang Lihua</aut>,
            <vrk><url
            ref="http://www.espero.com.cn/se/txt/2013-05/06/content_539643.htm">
            Idealaj lokoj por pasigi someron</url></vrk>,
            <lok>2013-05-06</lok>
          </fnt>
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="kamp.vid0o.FOT">
      <uzo tip="fak">FOT</uzo>
      <dif>
        &Ccirc;io, kion objektivo vidigas.
      </dif>
    </subsnc>
  </snc>
  <trd lng="es">campo de visi&oacute;n</trd>
  <trd lng="fr">champ de vision</trd>
</drv>
