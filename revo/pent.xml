<kap>
  <ofc>*</ofc>
  <rad>pent</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="pent.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="pent.0i.PSI">
    <dif>
      Senti beda&ubreve;ron pro farita kulpo kun deziro ripari &gcirc;in kaj ne plu
      rekomenci:
      <ekz>
        li <tld/>is pri sia promeso
        <fnt>
          <bib>IdO</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ercu nur! vi <tld/>os iam vian konduton
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne havis okazon <tld/>i pro tio
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        Konfesu al &ccirc;ielo vian pekon, &parsl; La farita&jcirc;on <tld/>u kaj evitu &parsl; Estontajn pekojn
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiam la Eternulo <tld/>is, ke li kreis la homon sur la
        tero
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 6:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u pro viaj pekoj, &ccirc;ar malfermitaj estas la inferaj senfunda&jcirc;oj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne hontu <tld/>i pri faro, hontu persisti en eraro
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        pekinto <tld/>as, kolero silentas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bedaux.0i.malkontente">beda&ubreve;ri<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_j;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="br">morc'hedi&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>k&aacute;t se</trd>,
    <trd>litovat (&zcaron;e)</trd>,
    <trd>pykat <klr>(za)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bereuen</trd>,
    <trd><ind>Reue</ind> zeigen</trd>
  </trdgrp>
  <trd lng="en">repent</trd>
  <trd lng="es">arrepentirse</trd>
  <trd lng="fr">se <ind>repentir</ind></trd>
  <trdgrp lng="hu">
    <trd>b&aacute;n</trd>,
    <trd>megb&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F8C;&#x6094;&#x3059;&#x308B;  <pr>&#x3053;&#x3046;&#x304B;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6094;&#x609F;&#x3059;&#x308B;  <pr>&#x304F;&#x3084;&#x3055;&#x3068;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6539;&#x609B;&#x3059;&#x308B; <pr>&#x304B;&#x3044;&#x3057;&#x3085;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">berouw hebben</trd>
  <trdgrp lng="pl">
    <trd>pokutowa&cacute;</trd>,
    <trd>&zdot;a&lstroke;owa&cacute;  <klr>(za winy)</klr></trd>
  </trdgrp>
  <trd lng="pt">arrepender-se de</trd>
  <trd lng="ro">regreta</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kaja&tcaron; sa</trd>,
    <trd>&lcaron;utova&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">&aring;ngra</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_k;&c_a;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6094;&#x6539; <pr>hu&#x1D0;g&#x1CE;i</pr></trd>,
    <trd>&#x5FCF;&#x6094; <pr>ch&agrave;nhu&#x1D0;</pr></trd>,
    <trd>&#x61FA;&#x6094; <pr>ch&agrave;nhu&#x1D0;</pr></trd>,
    <trd>&#x540E;&#x6094; <pr>h&ograve;uhu&#x1D0;</pr></trd>,
    <trd>&#x5F8C;&#x6094; <pr>h&ograve;uhu&#x1D0;</pr></trd>,
    <trd>&#x6094;&#x6068; <pr>hu&#x1D0;h&egrave;n</pr></trd>,
    <trd>&#x6094;&#x609F; <pr>hu&#x1D0;w&ugrave;</pr></trd>,
    <trd>&#x6094; <pr>hu&#x1D0;</pr></trd>,
    <trd>&#x6094;&#x8FC7; <pr>hu&#x1D0;gu&ograve;</pr></trd>,
    <trd>&#x6094;&#x904E; <pr>hu&#x1D0;gu&ograve;</pr></trd>,
    <trd>&#x61CA;&#x6094; <pr>&agrave;ohu&#x1D0;</pr></trd>,
    <trd>&#x8FFD;&#x6094; <pr>zhu&imacron;hu&#x1D0;</pr></trd>,
    <trd>&#x5FCF; <pr>ch&agrave;n</pr></trd>,
    <trd>&#x61FA; <pr>ch&agrave;n</pr></trd>,
    <trd>&#x61CA;&#x6068; <pr>&agrave;oh&egrave;n</pr></trd>,
    <trd>&#x81EA;&#x6094; <pr>z&igrave;hu&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pent.0o">
  <kap><tld/>o</kap>
  <snc mrk="pent.0o.PSI">
    <dif>
      Sento de tiu, kiu <tld/>as:
      <ekz>
        dibo&ccirc;i sen <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk>Finfine oni povas dibo&ccirc;i sen pento</vrk>
        </fnt>;
      </ekz>
      <ekz>
        lia <tld/>o kaj liaj beda&ubreve;roj estis sinceraj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;Apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        tago de <tld/>o kaj pre&gcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        per <tld/>o kaj trankvileco vi savi&gcirc;us
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 30:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        Johano baptis per la bapto de <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 19:4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_a;&c_ja;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">pok&aacute;n&iacute;</trd>
  <trd lng="de">Reue</trd>
  <trdgrp lng="en">
    <trd>remorse</trd>,
    <trd>repentance</trd>
  </trdgrp>
  <trd lng="es">arrepentimiento</trd>
  <trdgrp lng="fr">
    <trd>contrition</trd>,
    <trd>repentir</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F8C;&#x6094;  <pr>&#x3053;&#x3046;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x6094;&#x609F;  <pr>&#x304B;&#x3044;&#x3054;</pr></trd>,
    <trd>&#x6539;&#x609B;  <pr>&#x304B;&#x3044;&#x3057;&#x3085;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pokuta</trd>,
    <trd>skrucha</trd>,
    <trd>&zdot;al za grzechy</trd>
  </trdgrp>
  <trd lng="ro">remu&scomma;care</trd>
  <trd lng="sk">pok&aacute;nie</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_ja;&c_t;&c_t;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_a;&c_ja;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6094;&#x6068; <pr>hu&#x1D0;h&egrave;n</pr></trd>,
    <trd>&#x540E;&#x6094; <pr>h&ograve;uhu&#x1D0;</pr></trd>,
    <trd>&#x5F8C;&#x6094; <pr>h&ograve;uhu&#x1D0;</pr></trd>,
    <trd>&#x6094;&#x610F; <pr>hu&#x1D0;y&igrave;</pr></trd>,
    <trd>&#x6C89;&#x75DB; <pr>ch&eacute;nt&ograve;ng</pr></trd>,
    <trd>&#x759A; <pr>ji&ugrave;</pr></trd>,
    <trd>&#x6094; <pr>hu&#x1D0;</pr></trd>,
    <trd>&#x5FCF; <pr>ch&agrave;n</pr></trd>,
    <trd>&#x61FA; <pr>ch&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pent.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pent.pri0i.PSI">
    <dif>
      &Ccirc;agreni&gcirc;i pro ia plenumita ago, faro, parolo;
      <ref tip="sin" cel="bedaux.0i.malkontente">beda&ubreve;ri<sncref/></ref>:
      <ekz>
        Vi mokas min, sed vi pri<tld/>os
        <fnt>
          <bib>KrM</bib>
          <lok>22. Per lerta admono alfluas la mono.</lok>
        </fnt>!
      </ekz>
      <ekz>
        kie ajn la sorto min trovis, mi pri<tld/>is nenion
        <fnt>
          <aut>Hevesi L., trad. Vizi L.</aut>,
          <vrk><url ref="http://mek.niif.hu/10600/10612/10612.htm">
          La aventuroj de Andreo Jelky</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_ja;&c_c;&c_c;&c_a; <klr>(&c_z;&c_a; &c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trd lng="cs">k&aacute;t se <klr>(z n&ecaron;&ccaron;eho)</klr></trd>
  <trd lng="de">bereuen</trd>
  <trd lng="en">regret</trd>
  <trdgrp lng="es">
    <trd>sentir remordimiento</trd>,
    <trd>sentir pesar</trd>
  </trdgrp>
  <trd lng="fr">regretter</trd>
  <trdgrp lng="pl">
    <trd>&zdot;a&lstroke;owa&cacute;</trd>,
    <trd>czu&cacute; wyrzuty sumienia</trd>
  </trdgrp>
  <trd lng="ro">poc&abreve;i</trd>
  <trd lng="sk">obanova&tcaron;</trd>
  <trdgrp lng="zh">
    <trd>&#x61BE; <pr>h&agrave;n</pr></trd>,
    <trd>&#x6094; <pr>hu&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pent.0ofari">
  <kap><tld/>ofari</kap>
  <snc>
    <dif>
      Liberi&gcirc;i de peko a&ubreve; kulpo per <tld/>aj suferoj, pia vivo, bonaj agoj:
      <ekz>
        li rifu&gcirc;is en mona&hcirc;ejon, por <tld/>ofari tie pro la junulaj pekoj
        <fnt>
          <bib>IKr</bib>
          <lok>Tri Musketeroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ofare vipante sin mem
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la pordego de l' templo haltis <klr>[&#x2026;]</klr> <tld/>faranto
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        tutajn tagojn <klr>[&#x2026;]</klr> li pasigas en la templo de <nom>Astarte</nom>, <tld/>ofarante
        kaj pre&gcirc;ante
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vulpo <tld/>ofaras, gardu la kokidojn
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_u;&c_p;&c_a;&c_c;&c_mol; <ind>&c_g;&c_r;&c_a;&c_h;&c_ib;</ind></trd>,
    <trd>&c_a;&c_d;&c_k;&c_u;&c_p;&c_a;&c_c;&c_mol; <ind>&c_g;&c_r;&c_a;&c_h;&c_ib;</ind></trd>
  </trdgrp>
  <trd lng="br">ober <ind>pinijenn</ind></trd>
  <trdgrp lng="cs">
    <trd>vykonat pok&aacute;n&iacute;</trd>,
    <trd>&ccaron;init pok&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>Bu&szlig;e</ind> tun</trd>,
    <trd>b&uuml;&szlig;en</trd>
  </trdgrp>
  <trd lng="en">do <ind>penance</ind></trd>
  <trd lng="fr">faire <ind>p&eacute;nitence</ind></trd>
  <trdgrp lng="hu">
    <trd>vezekel</trd>,
    <trd>b&udblac;nb&aacute;natot tart</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7F6A;&#x3092;&#x511F;&#x3046;  <pr>&#x3056;&#x3044;&#x3092;&#x3064;&#x3050;&#x306A;&#x3046;</pr></trd>,
    <trd>&#x6094;&#x3044;&#x6539;&#x3081;&#x308B;  <pr>&#x304F;&#x3044;&#x3042;&#x3089;&#x305F;&#x3081;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">boete doen</trd>
  <trdgrp lng="pl">
    <trd>kaja&cacute; si&eogonek;</trd>,
    <trd>odpokutowa&cacute;</trd>
  </trdgrp>
  <trd lng="ro">m&abreve;rturisesc poc&abreve;it</trd>
  <trd lng="ru">&c_i;&c_s;&c_k;&c_u;&c_p;&c_a;&c_t;&c_mol; &c_g;&c_r;&c_je;&c_h;&c_i;</trd>
  <trd lng="sk">kona&tcaron; pok&aacute;nie</trd>
  <trd lng="sv">g&ouml;ra <ind>bot</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_k;&c_u;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i; &c_p;&c_o;&c_k;&c_u;&c_t;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6094;&#x8FC7; <pr>hu&#x1D0;gu&ograve;</pr></trd>,
    <trd>&#x6094;&#x904E; <pr>hu&#x1D0;gu&ograve;</pr></trd>
  </trdgrp>
</drv>
</art>
