<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pent</rad>/i
</kap>
<drv mrk="pent.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Senti beda&ubreve;ron pro farita kulpo kun deziro ripari &gcirc;in kaj ne plu
      rekomenci:
      <ekz>
        pekinto <tld/>as, kolero silentas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i sian senpripensemon, pri, pro sia senpripensemo;
      </ekz>
      <ekz>
        tiam la Eternulo <tld/>is, ke li kreis la homon sur la
        tero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li montris sian <tld/>on batadante sian bruston.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">bereuen</trd>
  <trd lng="en">repent</trd>
  <trd lng="nl">berouw hebben</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">&aring;ngra</trd>
</drv>

<drv mrk="pent.0ofari">
  <kap><tld/>ofari<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Ripari pekon a&ubreve; kulpon per <tld/>aj suferoj kaj bonaj agoj.
    </dif>
  </snc>
  <trd lng="de">Bu&szlig;e tun</trd>
  <trd lng="nl">boete doen</trd>
  <trd lng="sv">g&ouml;ra <ind>bot</ind></trd>
  <trd lng="ru">&c_i;&c_s;&c_k;&c_u;&c_p;&c_a;&c_t;&c_mol; &c_g;&c_r;&c_je;&c_h;&c_i;</trd>
</drv>
</art>
