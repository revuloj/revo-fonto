<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>filantropi</rad>/o
</kap>
<drv mrk="filant2.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Humaneco, homaramo, bonfaremo. 
    </dif>
    <refgrp tip="vid">
      <ref cel="filant1.0o">filantropo</ref>,
      <ref cel="kompat.0o">kompato</ref>,
      <ref cel="help">helpemo</ref>.
