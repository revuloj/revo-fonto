<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: filant2.xml,v 1.29 2023/10/14 18:06:41 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>filantropi</rad>/o <fnt><bib>IK</bib></fnt>
</kap>
<drv mrk="filant2.0o">
  <kap><tld/>o</kap>
  <snc mrk="filant2.0o.MOR">
    <dif>
      <ref tip="dif" cel="am.hom0o">Homamo</ref>:
      <ekz>
        sankta kredo al la savanta efiko de la <tld/>o
        <fnt>
          <bib>IK</bib>
        </fnt>!
      </ekz>
      <ekz>
        mi devas turni min al publika <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ne studis, li kuracas pro <tld/>o... ha, ha, ha, pro <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p.
        121a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_ja;&c_n;&c_t;&c_r;&c_o;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="ca">filantropia</trd>
  <trdgrp lng="cs">
    <trd>filantropie</trd>,
    <trd>lidumilnost</trd>,
    <trd>lidumilstv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Philanthropie</trd>,
    <trd>Menschenliebe</trd>
  </trdgrp>
  <trd lng="en">philanthropy</trd>
  <trd lng="es">filantrop&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_sh;&a_r;&a_d;&a_w;&a_s;&a_t;&f_y;</trd>,
    <trd>&a_n;&a_w;&a_ayn;&#8204;&a_d;&a_w;&a_s;&a_t;&f_y;</trd>,
    <trd>&a_A;&a_n;&a_s;&a_A;&a_n;&#8204;&a_d;&a_w;&a_s;&a_t;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>philanthropie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>filantr&oacute;pia</trd>,
    <trd>emberbar&aacute;ti &eacute;rz&eacute;s</trd>
  </trdgrp>
  <trd lng="id">filantropi</trd>
  <trdgrp lng="ja">
    <trd>&#x535A;&#x611B;  <pr>&#x306F;&#x304F;&#12354;&#12356;</pr></trd>,
    <trd>&#24904;&#21892;  <pr>&#12376;&#x305C;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">filantropie</trd>
  <trdgrp lng="pl">
    <trd>filantropia</trd>,
    <trd>dobroczynno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">filantropia</trd>
  <trd lng="ro">binefacere</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_l;&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;&c_i;&c_ja;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_t;&c_v;&c_o;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">&lcaron;udomilnos&tcaron;</trd>
  <trd lng="uk">&c_f;&c_ib;&#x43B;&c_a;&#x43D;&c_t;&c_r;&#x43E;&#x43F;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="filant2.0a">
  <kap><tld/>a</kap>
  <snc mrk="filant2.0a.MOR">
    <dif>
      Homama, bonfara:
      <ekz>
        kolektoj, kiuj estis aran&gcirc;ataj por la bono de <tld/>aj institucioj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni rigardos la instituciojn <tld/>ajn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni malofte a&ubreve;das pri la <tld/>aj aferoj faritaj de budhanoj
        <fnt>
          <aut>Miaohui</aut>,
          <vrk><url ref="http://www.ebudhano.cn/espero/">Blogo de
          &Ccirc;BRE</url></vrk>,
          <lok>2007-03-31</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_ib;&c_l;&c_ja;&c_n;&c_t;&c_r;&c_a;&c_p;&c_ib;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_b;&c_r;&c_a;&c_ch;&c_y;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">filantr&ograve;pic</trd>
  <trdgrp lng="cs">
    <trd>filantropick&yacute;</trd>,
    <trd>lidumiln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caritatif</trd>,
    <trd>humanitaire</trd>,
    <trd>philantropique</trd>
  </trdgrp>
  <trd lng="id">filantropis</trd>
  <trd lng="pl">dobroczynny</trd>
  <trd lng="sk">&lcaron;udomiln&yacute;</trd>
</drv>
</art>
