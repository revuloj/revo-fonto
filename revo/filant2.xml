<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>filantropi</rad>/o
</kap>
<drv mrk="filant2.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Humaneco, homaramo, bonfaremo:
      <ekz>li ne studis, li kuracas pro <tld/>o... Ha, ha, ha, pro <tld/>o
      <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p. 121a</lok></fnt>.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="altrui.0o">altruismo</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="filant1.0o">filantropo</ref>,
      <ref cel="help">helpemo</ref>,
      <ref cel="kompat.0o">kompato</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Philantropie</trd>,
    <trd>Menschenliebe</trd>
  </trdgrp>
  <trd lng="es">filantrop&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>philantropie</trd>,
    <trd>altruisme</trd>,
    <trd>bienfaisance</trd>
  </trdgrp>
  <trd lng="nl">filantropie</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_l;&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;&c_i;&c_ja;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_t;&c_v;&c_o;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
