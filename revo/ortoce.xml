<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>ortocentr</rad>/o
  </kap>
<drv mrk="ortoce.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="angul.tri0o">triangulo</ref>)</klr>
      La komuna punkto de &gcirc;iaj tri 
      <ref cel="alt.0o.triangulo">altoj</ref>.
    </dif>
    <rim>
      PIV <fnt><bib>PIV1</bib></fnt>
      prezentas tiun vorton kiel kunmeta&jcirc;on de
      <em>ort</em> kaj <em>centr</em>, kio aspektas tre dubinda, se konsideri
      la signifon. Kelkaj preferas paroli pri <ctl>altocentro</ctl>, kio
      ja estas pli-malpli interpretebla, kvankam ne tute rigora.
    </rim>
  </snc>
  <trd lng="de">H&ouml;henpunkt</trd>
  <trd lng="en">orthocentre</trd>
  <trd lng="fr">orthocentre</trd>
  <trd lng="ru">&c_o;&c_r;&c_t;&c_o;&c_c;&c_je;&c_n;&c_t;&c_r;</trd>
</drv>
</art>

