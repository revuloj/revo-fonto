<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: balene.xml,v 1.10 2022/11/08 08:41:25 revo Exp $">
<kap>
  <rad>balened</rad>/oj
</kap>
<drv mrk="balene.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="cetac.0oj">cetacoj</ref>
      (<trd lng="la">Balaenidae</trd>), kun tre granda kapo (triono de
      la korpolongo) kaj longaj <ref tip="prt" cel="bart.0o">bartoj</ref>:
      <ekz>
        <tld/>oj estas grandaj balenoj,
        kun avera&gcirc;a longo de plenkreskuloj de 15 al 17
        metroj, kaj pezo de 50&#x202F;-&#x202F;80 tunoj, ties &ccirc;efa distingiga trajto estas ilia
        mallar&gcirc;a, arkeca, supra makzelo, kiu havigas al la animaloj tre markite
        kurban makzelolinion
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=623369">Balenedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="balen.0o.genro">baleno<sncref/></ref>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_t;&c_o;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="ca">bal&egrave;nids</trd>
  <trd lng="de">Glattwale</trd>
  <trd lng="en">whales</trd>
  <trd lng="es">bal&eacute;nidos</trd>
  <trd lng="fr">bal&eacute;nid&eacute;s</trd>
  <trd lng="hu">sima <ind>b&aacute;lnaf&eacute;l&eacute;k</ind></trd>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_d;&c_k;&c_i;&c_je;
<ind>&c_k;&c_i;&c_t;&c_y;</ind> <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x43A;&c_i;&c_t;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>

