<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parapsihxologi.xml,v 1.6 2023/04/20 09:26:58 revo Exp $">
<kap>
  <rad>parapsi&hcirc;ologi</rad>/o,
  <var><kap><rad var="k">parapsikologi</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="parapsihxologi.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <snc mrk="parapsihxologi.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="metaps.0o">Metapsikio</ref>:
      <ekz>
        <tld/>aj kaj nifulaj kongresoj
        <fnt>
        <aut>Pejno S.</aut>,
        <vrk>Simono kaj interna ideo</vrk>,
        <lok>soc.culture.esperanto, 1999-02-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld var="k"/>o metas sin ie inter mistiko kaj scienco
        <fnt>
        <aut>S. Ver&scirc;inin</aut>,
        <vrk>Re: Eksterkorpi&gcirc;o</vrk>,
        <lok>soc.culture.esperanto, 1996-01-30</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Rosmari</nom> ne interesi&gcirc;is pri <tld var="k"/>o &gcirc;is vespero en la jaro 1927, 
        kiam &scirc;i subite komencis skribi iujn strangajn vortojn sur 
        papero
        <fnt>
          <aut>V. Okc</aut>,
          <vrk><url ref="http://www.segretidipulcinella.it/sdp14/lin_01.htm">
          Telika</url></vrk>,
          <lok>2006-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_p;&c_s;&c_y;&c_h;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>metapsychologie</trd>,
    <trd>parapsychologie</trd>
  </trdgrp>
  <trd lng="de">Parapsychologie</trd>
  <trd lng="fr">parapsychologie</trd>
  <trd lng="pl">parapsychologia</trd>
  <trd lng="ro">parapsihologie</trd>
  <trd lng="sk">metaplsychol&oacute;gia</trd>
  <trd lng="uk">&#x43F;&c_a;&c_r;&c_a;&#x43F;&c_s;&c_i;&c_h;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&#x44F;</trd>
</drv>
</art>

