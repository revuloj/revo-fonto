<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: urgx.xml,v 1.25 2022/06/09 06:34:53 revo Exp $">
<kap>
<ofc>1</ofc>
<rad>ur&gcirc;</rad>/i
</kap>
<drv mrk="urgx.0i">
   <kap><tld/>i</kap>
   <snc mrk="urgx.0i.akceli">
     <gra><vspec>tr</vspec></gra>
     <uzo tip="klr">(iu, io)</uzo>
     <dif>
       Vigle plirapidigi; vive akceli; postuli tujan agon:
       <ekz>
         ne &ccirc;esu ilin <tld/>i kaj bone komprenigu al ili, ke estas tre
         grave, ke ili venu<fnt>Z</fnt>;
       </ekz>
       <ekz>
         la Egiptoj <tld/>is la popolon por pli rapide elirigi ilin el
         la
         lando<fnt>Z</fnt>;
       </ekz>
       <ekz>
         la neceso, la cirkonstancoj <tld/>as nin;
       </ekz>
       <ekz>
         <tld/>i sin, <tld/>i&gcirc;i
         <klr>(rapidi)</klr>;
       </ekz>
       <ekz>
         kial vi <tld/>as vin forkuri?
       </ekz>
       <ekz>
         tiu decido malkontentigos niajn <tld/>emulojn.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_p;&c_r;&c_y;&c_s;&c_mol;&c_p;&c_je;&c_sh;&c_v;&c_a;&c_c;&c_mol;</trd>,
       <trd>&c_p;&c_a;&c_d;&c_g;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>kiirehti&auml;</trd>,
       <trd>hoputtaa</trd>,
       <trd>patistaa</trd>
     </trdgrp>
     <trd lng="hu">s&uuml;rget</trd>
     <trdgrp lng="it">
       <trd>premere<klr tip="amb"> (urgere)</klr></trd>,
       <trd>stringere<klr tip="amb"> (urgere)</klr></trd>
     </trdgrp>
     <trd lng="nl">tot <ind>haast</ind> aanzetten</trd>
     <trd lng="ru">&c_t;&c_o;&c_r;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
   </snc>
   <snc>
     <gra><vspec>ntr</vspec></gra>
     <uzo tip="klr">(io)</uzo>
     <dif>
       Esti ne prokrastebla:
       <ekz>
         &ccirc;ar mi la miajn vidas <tld/>anta en dan&gcirc;ero<fnt>Z</fnt>
         <klr>(la teksto <tld/>ante estas preseraro)</klr>;
       </ekz>
       <ekz>
         la afero, la tempo ne <tld/>as
         (<ref tip="sin" cel="brul.0i">bruli</ref>);
       </ekz>
       <ekz>
         <tld/>as al mi revidi vin.
       </ekz>
     </dif>
    <rim>
      Se oni estas <tld/>ata de iu a&ubreve; io, oni inklinas <ref cel="hast.0i">hasti</ref>.
    </rim>
     <trdgrp lng="be">
       <trd>&c_b;&c_y;&c_c;&c_mol;
<ind>&c_n;&c_je;&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_n;&c_y;&c_m;</ind></trd>,
       <trd>&c_b;&c_y;&c_c;&c_mol;
<ind>&c_t;&c_e;&c_r;&c_m;&c_ib;&c_n;&c_o;&c_v;&c_y;&c_m;</ind></trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>dr&auml;ngen</trd>,
       <trd>antreiben</trd>,
       <trd>zur Eile mahnen</trd>,
       <trd>eilig sein</trd>,
     </trdgrp>
     <trdgrp lng="fi">
       <trd>olla kiireellinen</trd>,
       <trd>vaatia nopeaa toimintaa</trd>,
     </trdgrp>
     <trd lng="hu">s&uuml;rg&odblac;s</trd>
     <trdgrp lng="it">
       <trd>urgere</trd>,
       <trd>essere <ind>urgente</ind></trd>
     </trdgrp>
     <trdgrp lng="nl">
       <trd>dringend zijn</trd>,
       <trd>spoed hebben</trd>
     </trdgrp>
     <trd lng="pt">ser urgente</trd>
     <trdgrp lng="ru">
       <trd>&c_b;&c_y;&c_t;&c_mol;
 
<ind>&c_n;&c_je;&c_o;&c_t;&c_l;&c_o;&c_zh;&c_n;&c_y;&c_m;</ind></trd>,
       <trd>&c_b;&c_y;&c_t;&c_mol;
       <ind>&c_s;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_m;</ind></trd>,
       <trd>&c_b;&c_y;&c_t;&c_mol; &c_k;&c_r;&c_a;&c_j;&c_n;&c_je;

<ind>&c_n;&c_je;&c_o;&c_b;&c_h;&c_o;&c_d;&c_i;&c_m;&c_y;&c_m;</ind></trd>
     </trdgrp>
   </snc>
   <trd lng="en">urge</trd>
   <trdgrp lng="es">
     <trd>urgir</trd>,
     <trd>apremiar</trd>
   </trdgrp>
   <trd lng="fr">urger</trd>
   <trd lng="pl">pilnym by&cacute;</trd>
   <trd lng="pt">urgir</trd>
   <trd lng="ro">a urgenta</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_n;&c_je;&c_v;&c_ib;&c_d;&c_k;&c_l;&c_a;&c_d;&c_n;&c_i;&c_m;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_m;&c_ib;&c_n;&c_o;&c_v;&c_i;&c_m;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_a;&c_j; &c_n;&c_je;&c_o;&c_b;&c_h;&c_ib;&c_d;&c_n;&c_i;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="urgx.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Ne plu prokrastebla:
       <ekz>
         la demando pri komuna lingvo por la scienco sin prezentas kiel
         <tld/>a problemo;
       </ekz>
       <ekz>
         unue zorgi la plej <tld/>an;
       </ekz>
       <ekz>
         li havas <tld/>an bezonon da mono;
       </ekz>
       <ekz>
         <tld/>a letero;
       </ekz>
       <ekz>
         <tld/>e bezoni ion;
       </ekz>
       <ekz>
         esti pelata, premata de <tld/>eco.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_je;&c_a;&c_d;&c_k;&c_l;&c_a;&c_d;&c_n;&c_y;</trd>,
     <trd>&c_t;&c_e;&c_r;&c_m;&c_ib;&c_n;&c_o;&c_v;&c_y;</trd>
   </trdgrp>
   <trd lng="bo">&t_ba;&t_rata;&t_drengbu;&t_la;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trd lng="de">dringend</trd>
   <trd lng="en">urgent</trd>
   <trd lng="es">urgente</trd>
   <trdgrp lng="fi">
      <trd>kiireellinen</trd>,
      <trd>viipym&auml;tt&auml; teht&auml;v&auml;</trd>,
      <trd>jota ei voi lyk&auml;t&auml;</trd>
   </trdgrp>
   <trd lng="fr">urgent</trd>
   <trd lng="hu">s&uuml;rg&odblac;s</trd>
   <trd lng="it">urgente</trd>
   <trd lng="nl">urgente</trd>
   <trd lng="nl">dringend</trd>
   <trd lng="pt">urgente</trd>
   <trdgrp lng="pl">
       <trd>pilny</trd>,
       <trd>nag&lstroke;y</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_n;&c_je;&c_o;&c_t;&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_s;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_k;&c_r;&c_a;&c_j;&c_n;&c_je;
     <ind>&c_n;&c_je;&c_o;&c_b;&c_h;&c_o;&c_d;&c_i;&c_m;&c_y;&c_j;</ind></trd>,
     <trd>&c_b;&c_je;&c_z;&c_o;&c_t;&c_l;&c_a;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_v;&c_ib;&c_d;&c_k;&c_l;&c_a;&c_d;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_m;&c_ib;&c_n;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_a;&c_j; &c_n;&c_je;&c_o;&c_b;&c_h;&c_ib;&c_d;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_ib;&c_d;&c_k;&c_l;&c_a;&c_d;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="urgx.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Premsento por plenumi ion plej balda&ubreve;:
      <ekz>
        kviete kaj sen <tld/>o <klr>[la birdo]</klr> levi&gcirc;is pli kaj pli alten, glitante
        foren
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        komencante laboron, li neniam havis <tld/>on, sed komencinte li obstine da&ubreve;rigis
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni havas <tld/>on en la butikoj
        <fnt>
          <bib>LGA</bib>
          <lok>III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la psiko de soifanto, kiam temas pri soifo, celas tute simple trinki, al tio &gcirc;i
        sentas <tld/>on
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Kvar</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> ne havis tempon tralegi &gcirc;in en la kristnaska ur&gcirc;o, sed nur iom
        foliumis
        <fnt>
          <bib>Kiso</bib>
          <lok>Tombeja Arabesko</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hast.0o">hasto</ref>,
      <ref cel="bezon.0o">bezono</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Eile</trd>,
    <trd>Zeitdruck</trd>
  </trdgrp>
</drv>






<drv mrk="urgx.0igi">
   <kap><tld/>igi</kap>
   <snc>
     <dif>
       <ref cel="urgx.0i.akceli" tip="dif">Ur&gcirc;i<sncref ref="urgx.0i.akceli"/></ref>.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_r;&c_y;&c_s;&c_mol;&c_p;&c_je;&c_sh;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_a;&c_d;&c_g;&c_a;&c_n;&c_ja;&c_c;&c_mol;</trd>
   </trdgrp>
   <trd lng="es">hacer urgente</trd>
   <trd lng="fr">rendre urgent</trd>
   <trdgrp lng="it">
     <trd>sollecitare<klr tip="amb"> (urgere)</klr></trd>,
     <trd>incitare<klr tip="amb"> (urgere)</klr></trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>ponagla&cacute;</trd>,
     <trd>pop&eogonek;dza&cacute;</trd>
   </trdgrp>
</drv>
</art>
