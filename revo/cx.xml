<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>&ccirc;</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<subart>
<drv mrk="cx.0.konsonanto">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">TIP</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      <ref tip="dif" cel="konson1.0o">Konsonanto</ref>, senvo&ccirc;a <ref
      tip="dif" cel="afrika.0o">afrikata</ref> <ref tip="dif"
      cel="gingiv.0o">gingivalo</ref> [t&#643;].
    </dif>
  <trd lng="fr">&ccirc;<klr> (consonne)</klr></trd>
  </snc>
  <snc>
     <dif>
      La kvara <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto.
     </dif>
     <trd lng="fr">&ccirc;<klr> (lettre)</klr></trd>
  </snc>
  </drv>
</subart>

<subart>
<drv mrk="cx.0.nomo">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>    
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif>
    <trd lng="fr">tch</trd>
   </snc>
  </drv>
</subart>
</art>

