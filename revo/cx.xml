<kap>
    <ofc>*</ofc><ofc>7</ofc><rad>&ccirc;o</rad>
</kap>

<drv mrk="cx.0">
  <kap><tld/></kap>
  <uzo tip="fak">LIN</uzo>
  <uzo tip="fak">GRA</uzo>
  <snc> 
    <dif>
      Nomo de la kvara <ref tip="super" cel="liter.0o.propra">litero</ref>
      de la Esperanta alfabeto:
      <ekz>
          nomoj de la literoj: a, bo, co, <tld/>, do, e, <klr>[...]</klr>
          <fnt>
            <bib>F</bib>
            <lok>&FE; 1</lok>
          </fnt>
      </ekz>.
    </dif>
    <trd lng="fr">tch</trd>
    <trd lng="nl">tsj</trd>
    <trd lng="no">tsj</trd>
   </snc>
  </drv>

</art>

