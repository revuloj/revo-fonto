<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pauxs.xml,v 1.22 2024/02/16 19:24:12 revo Exp $">
<kap>
  <rad>pa&ubreve;s</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pauxs.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pauxs.0i.BELA">
    <dif>
      <ref tip="super" cel="desegn.0i.perKrajono">Desegni</ref> sur
      diafana folio la&ubreve; suba <ref tip="vid" cel="sxablo.0o">&scirc;ablono</ref>
      a&ubreve; modelo.
    </dif>
    <refgrp tip="vid">
      <ref cel="kopi.0i.BELA">kopii<sncref/></ref>,
      <ref cel="muld.0i">muldi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_l;&c_mol;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_a;&c_p;&c_ib;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>durchpausen</trd>
    </trdgrp>
    <trd lng="en">trace</trd>
    <trdgrp lng="hu">
      <trd>&aacute;tm&aacute;sol</trd>,
      <trd>pauszol</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>calqueren</trd>,
      <trd>overtrekken<klr tip="ind"> (calqueren)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_l;&c_mol;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_k;&c_o;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pauxs.0i.imiti">
    <dif>
      Ekzakte <ref tip="super" cel="imit.0i">imiti</ref>:
      <ekz>
        la Popola Partio <klr>[&#x2026;]</klr> &ccirc;iam <tld/>anta la
        e&ubreve;skan politikon de siaj rivaloj
        <fnt>
          <aut>H. Alos i Font</aut>,
          <vrk><url ref="&Monato;/2002/007862.html">Nur etaj esperoj pri balda&ubreve;a solvo</url></vrk>,
          <bib>Monato</bib>,
          <lok>2002/08, p. 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        iuj &jcirc;urnalistoj komencis skribi kaj paroli pri &leftquot;e&ubreve;rolando&rightquot;, kaj estis kelkaj
        a&ubreve;toroj de Monato, kiuj sklave <tld/>is tiun nacilingvan monstra&jcirc;on
        <fnt>
          <aut>Maul Stefan</aut>
          <vrk><url ref="&Monato;/2000/007121.php">Malbona venko</url></vrk>
              <bib>Monato</bib>
              <lok>2000/12, p. 30</lok>
        </fnt>;
      </ekz>
      <ekz>
        dupa&gcirc;a biografio, kiun la&ubreve;vorte <tld/>os la artikolo aperanta pri la a&ubreve;torino en
        la Biografia leksikono
        <fnt>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2004/008066.html">Retrorigardo al intensa vivo</url></vrk>
               <bib>Monato</bib>
          <lok>2004/04, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        verkisto-&jcirc;urnalisto <klr>[&#x2026;]</klr>, kiu iel similas kaj <tld/>as la a&ubreve;toron mem
        <fnt>
          <aut>Nicolino Rossi</aut>
          <vrk><url ref="&Monato;/2013/011144.php">Familio-sagao, kun lar&gcirc;e variigitaj psikologiaj portretoj</url></vrk>
              <bib>Monato</bib>
          <lok>2013/12, p. 24</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_l;&c_mol;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_a;&c_p;&c_ib;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_ib;&c_m;&c_ib;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="en">slavishly reproduce</trd>
    <trdgrp lng="hu">
      <trd>ut&aacute;noz</trd>,
      <trd>lekop&iacute;roz</trd>
    </trdgrp>
    <trd lng="nl">imiteren</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_i;&c_m;&c_i;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="es">calcar</trd>
  <trdgrp lng="fr">
    <trd>calquer</trd>,
    <trd>d&eacute;calquer</trd>
  </trdgrp>
  <trd lng="pl">kalkowa&cacute;</trd>
  <trdgrp lng="pt">
   <trd>decalcar</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_l;&c_mol;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_z;&c_n;&c_ib;&c_m;&c_a;&c_t;&c_i; (&c_n;&c_a; &c_p;&c_r;&c_o;&c_z;&c_o;&c_r;&c_o;&c_m;&c_u;
    &c_p;&c_a;&c_p;&c_je;&c_r;&c_ib;)</trd>,
    <trd>&c_k;&c_a;&c_l;&c_mol;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x63CF; <pr>mi&#xE1;o</pr></trd>,
    <trd>&#x6479; <pr>m&#xF3;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pauxs.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="pauxs.0ado.BELA">
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_mol;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_ib;&c_ja;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_ib;&c_m;&c_ib;&c_t;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_ib;&c_m;&c_ib;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Durchpausen</trd>
  </trdgrp>
  <trd lng="es">calco<klr tip="amb"> (imitaci&oacute;n)</klr></trd>
  <trd lng="fr">calque<klr tip="amb"> (imitation)</klr></trd>
  <trdgrp lng="hu">
    <trd>pauszol&aacute;s</trd>,
    <trd>lem&aacute;sol&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">calque</trd>
  <trd lng="pl">kalkowanie</trd>
  <trdgrp lng="pt">
    <trd>decalcagem</trd>,
    <trd>decalque</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_l;&c_mol;&c_k;&c_a;<klr tip="amb"> (&c_k;&c_o;&c_p;&c_i;&c_ja;,
    &c_i;&c_m;&c_i;&c_t;&c_a;&c_c;&c_i;&c_ja;)</klr></trd>,
    <trd>&c_k;&c_o;&c_p;&c_i;&c_ja;</trd>,
    <trd>&c_i;&c_m;&c_i;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="pauxs.0ajxo">
  <kap><tld/>a&jcirc;o, <var><kap><tld/>o</kap></var></kap>
  <snc mrk="pauxs.0ajxo.BELA">
    <dif>
      <ref tip="super" cel="kopi.0o.kopiajxo">Kopio</ref> <tld/>e
      imitanta modelon:
      <ekz>
        kiu el vi forgesis <tld/>a&jcirc;on
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 63a, volumo 2a, p.
        369a</lok></fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="faksim.0o">faksimilo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_l;&c_mol;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_p;&c_ib;&c_ja;</trd>,
      <trd>&c_ib;&c_m;&c_ib;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Pause</trd>,
      <trd>Kopie</trd>
    </trdgrp>
    <trdgrp lng="en"><trd>tracing</trd>, <trd>copy</trd></trdgrp>
    <trd lng="es">calco</trd>
    <trd lng="fr">d&eacute;calque</trd>
    <trdgrp lng="hu">
      <trd>m&aacute;solat</trd>,
      <trd>pauszm&aacute;solat</trd>,
      <trd>m&aacute;solat</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>calqueerplaat</trd>,
      <trd>calqueerblad</trd>,
      <trd>imitatie</trd>,
      <trd>model</trd>
    </trdgrp>
    <trd lng="pl">kalka</trd>
    <trdgrp lng="pt">
      <trd>decalcagem</trd>,
      <trd>decalque</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_l;&c_mol;&c_k;&c_a;<klr tip="amb"> (&c_k;&c_o;&c_p;&c_i;&c_ja;,
      &c_i;&c_m;&c_i;&c_t;&c_a;&c_c;&c_i;&c_ja;)</klr></trd>,
      <trd>&c_k;&c_o;&c_p;&c_i;&c_ja;</trd>,
      <trd>&c_i;&c_m;&c_i;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pauxs.0ajxo.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      Tradukprunto, vorto a&ubreve; esprimo konstruita per traduko de
      alilingva modelo (precipe kiam temas pri la&ubreve;vorta disa traduko
      de &ccirc;iuj komponantoj de malsimpla vorto a&ubreve; esprimo);
      <ref tip="sin" cel="kalke.0o.LIN">kalkeo</ref>:
      <ekz>
        la esperanta vorto <ctl>memstara</ctl> estas <tld/>a&jcirc;o
        de la germana &leftquot;<nac>selbstst&auml;ndig</nac>&rightquot;;
      </ekz><ekz>
        skatol-tran&ccirc;anta tran&ccirc;ilo <klr>[estas]</klr>
        mallerta <tld/>o de &leftquot;<nac>box-cutting knife</nac>&rightquot;
        <fnt>
          <aut>J. Wells</aut>,
          <vrk><url ref="http://vortaro-blogo.blogspot.com/2009/08/sovklinga-trancilo.html">
          &scirc;ovklinga tran&ccirc;ilo</url></vrk>,
          <lok>vortaro-blogo, 2009-08-17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kaj reinterpreto de la usona moto: <nac>E pluribus unum</nac> &ndash; El pluraj unu / El
        pluraj transen
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 12 (62)</lok>
        </fnt>.
      </ekz>
      <trdgrp lng="en"><trd>calque</trd>, <trd>loan
      translation</trd></trdgrp>
    </dif>
    <ref tip="vid" cel="klisx.0o">kli&scirc;o</ref>
    <ref tip="vid" cel="naci.inter0ismo.LIN">internaciismo<sncref/></ref>
    <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_k;&c_a; <klr>(&c_m;&c_o;&c_w;&c_n;&c_a;&c_ja;)</klr></trd>
    <trd lng="de">Abklatsch</trd>
    <trd lng="ru">&c_k;&c_a;&c_l;&c_mol;&c_k;&c_a;<klr tip="amb"> (&c_s;&c_je;&c_m;&c_a;&c_n;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
    &c_z;&c_a;&c_i;&c_m;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;)</klr></trd>
  </snc>
  <trd lng="uk">&c_k;&c_a;&c_l;&c_mol;&c_k;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x63CF;&#x56FE; <pr>mi&#xE1;ot&#xFA;</pr></trd>,
    <trd>&#x63CF;&#x5716; <pr>mi&#xE1;ot&#xFA;</pr></trd>,
    <trd>&#x501F;&#x8B6F; <pr>ji&#xE8;y&#xEC;</pr></trd>,
    <trd>&#x501F;&#x8BD1; <pr>ji&#xE8;y&#xEC;</pr></trd>,
    <trd>&#x501F;&#x8B6F;&#x8A5E; <pr>ji&#xE8;y&#xEC;c&#xED;</pr></trd>,
    <trd>&#x501F;&#x8BD1;&#x8BCD; <pr>ji&#xE8;y&#xEC;c&#xED;</pr></trd>,
    <trd>&#x6BDB;&#x6837; <pr>m&#xE1;oy&#xE0;ng</pr></trd>,
    <trd>&#x6BDB;&#x6A23; <pr>m&#xE1;oy&#xE0;ng</pr></trd>
  </trdgrp>
</drv>
</art>
