<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>le&ubreve;tenant</rad>/o
</kap>
<drv mrk="leuxte.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="oficir.0o">Oficiro</ref> tuj malsupera al
      kapitano:
      <ekz>
        la <tld/>o kribris per la fingroj iom da sablo anta&ubreve; ol
        respondi<fnt><aut>Alberto Garc&iacute;a Fumero</aut>:
        <vrk>Relevi&gcirc;o</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 1a,
        p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="el">&ypsilon;&pi;&omikron;&lambda;&omikron;&chi;&alfa;&gamma;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">first lieutenant <klr>(Usone)</klr></trd>
  <trdgrp lng="fr">
    <trd>lieutenant</trd>
  </trdgrp>
  <trd lng="nl">luitenant</trd>
  <trd lng="pt">tenente</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_j;&c_t;&c_je;&c_n;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_u;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">l&ouml;jtnant</trd>
</drv>

<drv mrk="leuxte.sub0o">
  <kap>sub<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Plej malsupera <ref tip="super" cel="oficir.0o">oficiro</ref>:
      <ekz>
        li i&gcirc;is sub<tld/>o en 1883 post sukcesaj studoj en la Armea
        Altlernejo<fnt><aut>Wim M. A. De Smet</aut>: <vrk>Anta&ubreve; cent
        jaroj</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 5a, p.
        25a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="leuxte.vic0o">vic<tld/>o</ref>
    </refgrp>
  </snc>
  <trd lng="en">second lieutenant <klr>(Usone)</klr></trd>
  <trdgrp lng="fr">
    <trd>sous-lieutenant</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_l;&c_a;&c_d;&c_sh;&c_i;&c_j; &c_l;&c_je;&c_j;&c_t;&c_je;&c_n;&c_a;&c_n;&c_t;</trd>
  <trd lng="nl">onderluitenant</trd>
  <trd lng="pt">subtenente</trd>
</drv>

<drv mrk="leuxte.vic0o">
  <kap>vic<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <ref tip="dif" cel="leuxte.sub0o">sub<tld/>o</ref>
  </snc>
  <trd lng="en">second lieutenant <klr>(Usone)</klr></trd>
  <trdgrp lng="fr">
    <trd>sous-lieutenant</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_l;&c_a;&c_d;&c_sh;&c_i;&c_j; &c_l;&c_je;&c_j;&c_t;&c_je;&c_n;&c_a;&c_n;&c_t;</trd>
  <trd lng="nl">onderluitenant</trd>
  <trd lng="pt">subtenente</trd>
</drv>
</art>
