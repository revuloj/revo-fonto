<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>le&ubreve;tenant</rad>/o
</kap>
<drv mrk="leuxte.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      En terarmeo kaj aerarmeo,
      <ref tip="super" cel="oficir.0o.MIL">oficiro</ref>, kiu rangas inter
      <ref cel="leuxte.sub0o">sub<tld/>o</ref> kaj
      <ref cel="kapita1.0o.MIL">kapitano</ref>:
      <ekz>
        la <tld/>o kribris per la fingroj iom da sablo anta&ubreve; ol
        respondi
        <fnt><aut>Alberto Garc&iacute;a Fumero</aut>:
        <vrk>Relevi&gcirc;o</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 1a,
        p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_j;&c_t;&c_e;&c_n;&c_a;&c_n;&c_t;</trd>
  <trd lng="br">letanant</trd>
  <trd lng="de">Leutnant</trd>
  <trd lng="el">&ypsilon;&pi;&omikron;&lambda;&omikron;&chi;&alfa;&gamma;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">first lieutenant <klr>(Usone)</klr></trd>
  <trdgrp lng="fr">
    <trd>lieutenant</trd>
  </trdgrp>
  <trd lng="hu">hadnagy</trd>
  <trd lng="nl">luitenant</trd>
  <trd lng="pt">tenente</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_j;&c_t;&c_je;&c_n;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_u;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sv">l&ouml;jtnant</trd>
</drv>

<drv mrk="leuxte.sub0o">
  <kap>sub<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      En terarmeo kaj aerarmeo,
      <ref tip="super" cel="oficir.0o.MIL">oficiro</ref>, kiu rangas sub
      <ref cel="leuxte.0o"><tld/>o</ref>;
      plej malsupera oficiro:
      <ekz>
        li i&gcirc;is sub<tld/>o en 1883 post sukcesaj studoj en la Armea
        Altlernejo
        <fnt><aut>Wim M. A. De Smet</aut>: <vrk>Anta&ubreve; cent
        jaroj</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 5a, p.
        25a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="leuxte.vic0o">vic<tld/>o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_o;&c_d;&c_sh;&c_y; <ind>&c_l;&c_je;&c_j;&c_t;&c_e;&c_n;&c_a;&c_n;&c_t;</ind></trd>
  <trdgrp lng="de">
    <trd>Fahnenjunker</trd>,
    <trd>F&auml;hnrich</trd>,
    <trd>Unterleutnant</trd>
  </trdgrp>
  <trd lng="br">isletanant</trd>
  <trd lng="en">second lieutenant <klr>(Usone)</klr></trd>
  <trdgrp lng="fr">
    <trd>sous-lieutenant</trd>
  </trdgrp>
  <trd lng="hu">alhadnagy</trd>
  <trd lng="ru">&c_m;&c_l;&c_a;&c_d;&c_sh;&c_i;&c_j; &c_l;&c_je;&c_j;&c_t;&c_je;&c_n;&c_a;&c_n;&c_t;</trd>
  <trd lng="nl">onderluitenant</trd>
  <trd lng="pt">subtenente</trd>
</drv>

<drv mrk="leuxte.vesel0o">
  <kap>vesel<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <fnt><bib>PIV1</bib><lok>veselo</lok></fnt>
  <snc>
    <dif>
      En mararmeo,
      <ref tip="super" cel="oficir.0o.MIL">oficiro</ref> kun
      rango ekvivalenta al tiu de <ref cel="kapita1.0o.MIL">kapitano</ref>.
    </dif>
    <ref tip="vid" cel="vesel.0o">veselo</ref>
  </snc>
  <trd lng="br">letanant a vor</trd>
  <trd lng="fr">lieutenant de vaisseau</trd>
  <trd lng="hu">sorhaj&oacute;hadnagy</trd>  
</drv>

<drv mrk="leuxte.vic0o">
  <kap>vic<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <ref tip="dif" cel="leuxte.sub0o">sub<tld/>o</ref>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_o;&c_d;&c_sh;&c_y; <ind>&c_l;&c_je;&c_j;&c_t;&c_e;&c_n;&c_a;&c_n;&c_t;</ind></trd>
  <trd lng="br">isletanant</trd>
  <trd lng="de">Unterleutnant</trd>
  <trd lng="en">second lieutenant <klr>(Usone)</klr></trd>
  <trdgrp lng="fr">
    <trd>sous-lieutenant</trd>
  </trdgrp>
  <trd lng="hu">alhadnagy</trd>
  <trd lng="ru">&c_m;&c_l;&c_a;&c_d;&c_sh;&c_i;&c_j; &c_l;&c_je;&c_j;&c_t;&c_je;&c_n;&c_a;&c_n;&c_t;</trd>
  <trd lng="nl">onderluitenant</trd>
  <trd lng="pt">subtenente</trd>
</drv>

<drv mrk="leuxte.vicsub0o">
  <kap>vicsub<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <fnt>
  <aut>E. Grosjean-Maupin</aut>
  <vrk>Dictionnaire complet fran&ccedil;ais-esp&eacute;ranto</vrk>
  </fnt>
  <snc>
    <ref tip="dif" cel="adjuda.0o">adjudanto</ref>
  </snc>
  <trd lng="br">adjudant</trd>
  <trd lng="de">Adjutant</trd>
  <trd lng="fr">adjudant</trd>
  <trd lng="hu">z&aacute;szl&oacute;s</trd>  
</drv>
</art>
