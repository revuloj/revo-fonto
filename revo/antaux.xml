    </subsnc>
    <subsnc>
      <dif>
        <klr>(anta&ubreve;<frm><k>X</k></frm>a: estanta anta&ubreve; la <frm><k>X</k></frm>a)</klr>
        <ekz>
          <refgrp tip="vid">
            <ref cel="last.antaux0a"><tld/>lasta</ref>...
          </refgrp>
        </ekz>
      </dif>
    </subsnc>
  </snc>
</subart>
