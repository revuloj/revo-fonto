  <drv mrk="antaux.0o">
    <kap><tld/>o</kap>
    <snc mrk="antaux.0o.loke">
      <dif>
        <tld lit="A"/>a <sncref ref="antaux.0a.pliProksima"/> parto,
        plej proksima al la rigardanto:
        <ekz>
          de la <tld/>o de la pordego de la eniro &gcirc;is la <tld/>o
          de la interna portiko de la pordego estis kvindek ulnoj
          <fnt><bib>MT</bib><lok>&Jeh; 40:15</lok></fnt>;
        </ekz>
        <ekz>
          ili demetos ankrojn el la <tld/>o <klr>(de la &scirc;ipo)</klr>
          <fnt><bib>NT</bib><lok>&Ago; 27:30</lok></fnt>;
        </ekz>
        <ekz>
          <tld/>o kaj posto sekvas sin
          <fnt><aut>La&ubreve;zio trad. Sonja Elen Kisa</aut>
          <vrk>Da&ubreve;de&gcirc;ingo</vrk>
          <lok><url
          ref="http://www.kisa.ca/taoismo.html">http://www.kisa.ca/taoismo.html</url></lok></fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">ARK</uzo>
          lia profesio metadis lin ofte en la <tld/>on de la plej
          respektindaj homoj de la urbo
          <klr>(metadis lin en ilian &ccirc;eeston, al iliaj rigardoj)</klr>
          <fnt><bib>Fab3</bib><lok>Bona humoro</lok></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="fr">
        <trd>devant<klr tip="ind"> (subst.)</klr></trd>,
        <trd>avant<klr tip="ind"> (subst.)</klr></trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>voorkant</trd>,
        <trd>voorzijde</trd>
      </trdgrp>
    </snc>
    <snc mrk="antaux.0o.tempe">
      <dif>
        <tld lit="A"/>a <sncref ref="antaux.0a.pliFrua"/> tempo:
        <ekz>
          krom la <tld/>o <klr>(krom la pasintaj spertoj)</klr> anka&ubreve;
          ektimigas nin la malfacila&jcirc;oj por fari korektajn esplorojn
          <fnt><aut>Jos&eacute; Pina Tuells</aut>
          <vrk>Teama sperto &ccirc;e priinteligentaj handikapuloj</vrk>
          <lok><url
          ref="http://storm.prohosting.com/jesuo/k44novaj.htm">http://storm.prohosting.com/jesuo/k44novaj.htm</url></lok></fnt>;
        </ekz>
        <ekz>
          al la <tld/>o <klr>(al la supre dirita)</klr>
          mi aldonu ankora&ubreve; ion
          <fnt><aut>Georgo Kama&ccirc;o</aut><vrk>Monato</vrk><lok><url
          ref="http://www.esperanto.be/fel/mon/rec/kmin.html">http://www.esperanto.be/fel/mon/rec/kmin.html</url></lok></fnt>.
        </ekz>
      </dif>
      <trd lng="fr">ce qui <ind>pr&eacute;c&egrave;de</ind></trd>
      <trd lng="nl">het <ind>vorige</ind></trd>
    </snc>
  </drv>
  <drv mrk="antaux.0i">
    <kap><tld/>i</kap>
    <gra><vspec>x</vspec></gra>
    <snc>
      <dif>
        Esti <tld/>a:
        <ekz>
          portiko <tld/>as al la palacon <klr>(a&ubreve;: la palacon)</klr>;
        </ekz>
        <ekz>
          Angolm <tld/>is min <klr>[en la ban&ccirc;ambro]</klr>
