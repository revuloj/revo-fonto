          <fnt><bib>UrdHadda</bib><lok>p. 5</lok></fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">EVI</uzo>
          <klr>(abs.)</klr> via horlo&gcirc;o <tld/>as
          <klr>(<ref cel="fru.0i.horlogxo">fruas</ref>)</klr>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="de">
