            <ref cel="lud.antaux0i"><tld/>ludi</ref>,
            <ref cel="parol.antaux0i"><tld/>paroli</ref>...
          </refgrp>
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <snc>
