<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>atripl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="atripl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="atripl.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="kenopo1.0oj">kenopodiacoj</ref>
      (<trd lng="la">Atriplex</trd>):
      <ekz>
        ili hazarde kreskis <klr>...</klr> sur nesarkita loko
        <klr>...</klr> densa <tld/>aro kaj iaj altaj herba&ccirc;oj
        &ccirc;irka&ubreve;is ilin
        <fnt>
          <aut>V. Gar&scirc;in, trad. M. Povorin</aut>,
          <vrk>La ru&gcirc;a floro</vrk>,
          <bib>LOdE</bib>,
          <lok>2006:12 (146)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_b;&c_ja;&c_d;&c_a;</trd>
  <trd lng="bg">&c_l;&c_o;&c_b;&c_o;&c_d;&c_a;</trd>
  <trd lng="de">Melde</trd>
  <trd lng="fa">&a_A;&a_s;&a_f;&a_n;&a_A;&a_j; &a_b;&a_A;&a_gh;&f_y;</trd>
  <trd lng="fr">arroche</trd>
  <trd lng="hu">laboda</trd>
  <trd lng="it">atreplice</trd>
  <trd lng="nl">melde</trd>
  <trd lng="ru">&c_l;&c_je;&c_b;&c_je;&c_d;&c_a;</trd>
  <trd lng="sv">strandm&aring;lla</trd>
</drv>
</art>
