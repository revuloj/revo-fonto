    <trd>dar velocidad</trd>,
    <trd>apurar <klr>(americanismo)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>acc&eacute;l&eacute;rer<klr tip="ind"> (tr.)</klr></trd>,
    <trd>h&acirc;ter</trd>
  </trdgrp>
  <trdgrp lng="hu">
