      <ref tip="vid" cel="akcel.0i">akceli</ref>:
      <ekz>
        <tld/>igi konstruon<fnt>B</fnt>;
      </ekz>
      <ekz>
        mi <tld/>igis mian laboron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        enkonduki novajn faciligojn kaj <tld/>igojn por la transsendo de
        la internaciaj po&scirc;ta&jcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_k;&c_o;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>popohnat</trd>,
    <trd>zrychlit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beschleunigen</trd>,
    <trd>antreiben</trd>
  </trdgrp>
  <trdgrp lng="es">
