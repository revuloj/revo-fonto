<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: izopod.xml,v 1.4 2024/11/11 09:28:46 afido Exp $">
    <kap>
      <rad>izopod</rad>/oj
    </kap>

<drv mrk="izopod.0o">
  <kap><tld/>o</kap>
  <snc mrk="izopod.0o.ZOO">
    <dif>
      Iu el <ref tip="malprt" cel="izopod.0oj"><tld/>oj</ref>.
    </dif>
    <ref tip="sub" cel="onisk.0o">onisko</ref>
    <ref tip="lst" cel="krust.0ulo" lst="voko:krustuloj">krustulo</ref>
    <bld
      lok="&WCU;/thumb/6/6f/Kellerassel_01.jpg/640px-Kellerassel_01.jpg"
      prm="PD"
      ><tld/>o (Porcellio scaber)
        <fnt>
          <aut>Arctanx, propra foto</aut>
          <lok><url ref="&WCW;/File:Kellerassel_01.jpg"
              >https://commons.wikimedia.org/wiki/File:Kellerassel_01.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_n;&c_ib;&c_k; &c_a;&c_t;&c_r;&c_a;&c_d;&c_a; <ind>&c_r;&c_a;&c_w;&c_n;&c_a;&c_n;&c_o;&c_g;&c_ib;&c_h;</ind></trd>
  <trd lng="de">Assel</trd>
  <trd lng="en">isopod</trd>
  <trd lng="fr">isopode</trd>
  <trd lng="nl">pissebed</trd>
  <trd lng="ja">&#31561;&#33050;&#39006; <pr>&#12392;&#12358;&#12365;&#12419;&#12367;&#12427;&#12356;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_v;&c_n;&c_o;&c_n;&c_o;&c_g;&c_i;&c_j;</trd>,
    <trd>&c_i;&c_z;&c_o;&c_p;&c_o;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="izopod.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="izopod.0oj.ZOO">
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_ordoj" cel="ord.0o.BIO">Ordo</ref>
      de <ref tip="malprt" cel="krust.0uloj">krustuloj</ref> (<trd lng="la">Isopoda</trd>)
      dorsventre plataj, kun sep kruroparoj,
      &ccirc;e kies lasta sidas la brankoj. Iuj adapti&gcirc;is al la vivo ekster
      akvo (vd <ref tip="prt" cel="onisk.0oj">oniskoj</ref>):
      <ekz>
        kelkaj dafnioj, izopodoj, amfipodoj kaj aliaj krustacoj havas kovopo&scirc;on por la
        ovoj
        <fnt>
          <vrk><url ref="https://scienca-revuo.info/files/327-1-465-1-10-20140426.pdf"
              >Anka&ubreve; bestoj flegas siajn idojn</url></vrk>
          <lok>Scienca Revuo, vol 40 (1989), p. 9</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la abismoj, <klr>[&#8230;]</klr> inter la senvertebruloj trovi&gcirc;as la gigantaj
        <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=762366"
              >Abisma gigantismo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_w;&c_n;&c_a;&c_n;&c_o;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="de">Asseln</trd>
  <trdgrp lng="en">
    <trd>isopods</trd>,
    <trd>Isopoda</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>isopodes</trd>,
    <trd>Isopoda</trd>
  </trdgrp>
  <trd lng="nl">pissebedden</trd>
  <trdgrp lng="ja">
    <trd>&#12527;&#12521;&#12472;&#12512;&#12471;&#30446; <pr>&#12431;&#12425;&#12376;&#12416;&#12375;&#12418;&#12367;</pr></trd>,
    <trd>&#31561;&#33050;&#30446; <pr>&#12392;&#12358;&#12365;&#12419;&#12367;&#12418;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_v;&c_n;&c_o;&c_n;&c_o;&c_g;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_z;&c_o;&c_p;&c_o;&c_d;&c_y;</trd>
  </trdgrp>
</drv>

  </art>

