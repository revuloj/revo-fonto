<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pirit</rad>/o
</kap>
<drv mrk="pirit.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Grajnoforma kombina&jcirc;o de sulfuro kaj metalo:
      <ekz>
        fera <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_ib;&c_r;&c_y;&c_t;</trd>,
      <trd>&c_s;&c_je;&c_r;&c_n;&c_y; &c_k;&c_a;&c_l;&c_ch;&c_a;&c_d;&c_a;&c_n;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Pyrit</trd>,
      <trd>Schwefelkies</trd>
    </trdgrp>
    <trd lng="hu">pirit</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_je;&c_r;&c_n;&c_y;&c_j; &c_k;&c_o;&c_l;&c_ch;&c_je;&c_d;&c_a;&c_n;</trd>,
      <trd>&c_p;&c_i;&c_r;&c_i;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      <ref tip="dif" cel="gruz.0o">Gruzo</ref>.
    </dif>
  </snc>
</drv>
</art>
