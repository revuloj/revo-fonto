<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>pirit</rad>/o
    </kap>

    <drv mrk="pirit.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="pirit.0o.MIN">
        <uzo tip="fak">MIN</uzo>
        <dif>
          Grajnoforma kombina&jcirc;o de sulfuro kaj metalo:
          <ekz>
            fera <tld/>o.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_p;&c_ib;&c_r;&c_y;&c_t;</trd>,
          <trd>&c_s;&c_je;&c_r;&c_n;&c_y; 
            &c_k;&c_a;&c_l;&c_ch;&c_a;&c_d;&c_a;&c_n;</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Pyrit</trd>,
          <trd>Schwefelkies</trd>
        </trdgrp>
        <trd lng="hu">pirit</trd>
        <trd lng="fr">pyrite</trd>
        <trdgrp lng="ru">
          <trd>&c_s;&c_je;&c_r;&c_n;&c_y;&c_j; 
            &c_k;&c_o;&c_l;&c_ch;&c_je;&c_d;&c_a;&c_n;</trd>,
          <trd>&c_p;&c_i;&c_r;&c_i;&c_t;</trd>
        </trdgrp>
      </snc>

      <snc mrk="pirit.0o.gruzo">
        <uzo tip="stl">EVI</uzo>
        <dif>
          <ref tip="dif" cel="gruz.0o">Gruzo</ref>.
        </dif>
      </snc>
    </drv>

    <drv mrk="pirit.kupro0o">
      <kap>kupro<tld/>o<fnt>PIV2</fnt></kap>
      <snc>
        <uzo tip="fak">KEM</uzo>
        <uzo tip="fak">MIN</uzo>
        <dif>
          La plej disvastigita <ref tip="super" cel="mineral.0o">mineralo</ref>
          entenata <ref cel="kupr.0o">kupron</ref>, <frm>CuFeS<sub>2</sub></frm>.
        </dif>          
      </snc>
        <trdgrp lng="ru">
          <trd>&c_m;&c_je;&c_d;&c_n;&c_y;&c_j; 
            &c_k;&c_o;&c_l;&c_ch;&c_je;&c_d;&c_a;&c_n;</trd>,
          <trd>&c_h;&c_a;&c_l;&c_mol;&c_k;&c_o;&c_p;&c_i;&c_r;&c_i;&c_t;</trd>
        </trdgrp>
    </drv>

  </art>
