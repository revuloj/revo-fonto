<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pirit.xml,v 1.28 2023/10/20 16:38:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pirit</rad>/o
</kap>
<drv mrk="pirit.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pirit.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="super" cel="minera.0o">Mineralo</ref> el kubformaj 
      kristaloj de fera sulfido <frm>FeS<sub>2</sub></frm>:
      <ekz>
        fera <tld/>o;
      </ekz>
      <ekz>
        en tiu loko originis la ora febro en 1849, kaj e&ccirc; nun oni povas vidi amason de
        ore-brilaj eroj (!) sur la sabla fundo de rivero <nom>American</nom>, <klr>[&#x2026;]</klr>
        tiel nomata &leftquot;oro de stultuloj&rightquot; (kristaloj de <tld/>o, trompe perceptataj kiel
        oraj)
        <fnt>
          <bib>LOdE</bib>
          <lok> 2002, &#x2116;7</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/4/43/Pyrit_01.jpg/640px-Pyrit_01.jpg" prm="CC BY-SA 4.0"><tld/>o
    (52&#x202F;mm diametro)
        <fnt>
          <aut>Uoaei1, Self-photographed</aut>
          <lok><url ref="&WCW;/File:Pyrit_01.jpg">&WCW;/File:Pyrit_01.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="be">
      <trd>&c_p;&c_ib;&c_r;&c_y;&c_t;</trd>,
      <trd>&c_s;&c_je;&c_r;&c_n;&c_y;
      &c_k;&c_a;&c_l;&c_ch;&c_a;&c_d;&c_a;&c_n;</trd>
    </trdgrp>
    <trd lng="bg">&c_p;&c_i;&c_r;&c_i;&c_t;</trd>
    <trd lng="ca">pirita</trd>
    <trdgrp lng="de">
      <trd>Pyrit</trd>,
      <trd>Schwefelkies</trd>
    </trdgrp>
    <trd lng="en">pyrite</trd>
    <trd lng="es">pirita</trd>
    <trd lng="hu">pirit</trd>
    <trd lng="fr">pyrite</trd>
    <trd lng="pl">piryt</trd>
    <trdgrp lng="pt">
      <trd>pirita</trd>,
      <trd>diamante-alpino</trd>,
      <trd>diamante da <ind>Pensilv&acirc;nia</ind></trd>,    
      <trd>diamante-de-enxofre</trd>,
      <trd>pedra-inca</trd>,    
      <trd>pirita-de-ferro</trd>
    </trdgrp>
    <trd lng="ro">pirit&abreve;</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_je;&c_r;&c_n;&c_y;&c_j;
      &c_k;&c_o;&c_l;&c_ch;&c_je;&c_d;&c_a;&c_n;</trd>,
      <trd>&c_p;&c_i;&c_r;&c_i;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc mrk="pirit.0o.gruzo">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="gruz.0o">Gruzo</ref>:
      <ekz>
        apena&ubreve; mi iom rekonscii&gcirc;is, mi trovas la plej klaran akvon en la <tld/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_r;&c_u;&c_z;</trd>,
      <trd>&c_zh;&c_v;&c_ib;&c_r;</trd>,
      <trd>&c_g;&c_r;&c_a;&c_v;&c_ib;&c_j;</trd>
    </trdgrp>
    <trd lng="bg">&c_d;&c_r;&c_je;&c_b;&c_je;&c_n; &c_ch;&c_a;&c_k;&c_malmol;&c_l;</trd>
    <trd lng="ca">grava</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kyz &zcaron;elezn&yacute;</trd>,
    <trd>pyrit</trd>
  </trdgrp>
  <trd lng="ja">&#x9EC4;&#x9244;&#x9271;  <pr>&#x304A;&#x3046;&#x3066;&#x3063;&#x3053;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>pyriet</trd>,
    <trd>ijzerkies</trd>,
    <trd>zwavelkies</trd>
  </trdgrp>
  <trd lng="sk">pyrit</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_ib;&c_r;&c_ch;&c_a;&#x43D;&c_i;&c_j; &#x43A;&#x43E;&#x43B;&c_ch;&c_je;&c_d;&c_a;&#x43D;</trd>,
    <trd>&#x43F;&c_ib;&c_r;&c_i;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="pirit.kupro0o">
  <kap>kupro<tld/>o <fnt>PIV2</fnt></kap>
  <snc mrk="pirit.kupro0o.MIN">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">MIN</uzo>
    <dif>
      La plej disvastigita <ref tip="super" cel="minera.0o.MIN">mineralo</ref>
      entenata <ref cel="kupr.0o">kupron</ref>,
      <frm>CuFeS<sub>2</sub></frm>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_a;&c_l;&c_mol;&c_k;&c_a;&c_p;&c_ib;&c_r;&c_y;&c_t;</trd>,
    <trd>&c_m;&c_je;&c_d;&c_n;&c_y; &c_k;&c_a;&c_l;&c_ch;&c_a;&c_d;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_h;&c_a;&c_l;&c_k;&c_o;&c_p;&c_i;&c_r;&c_i;&c_t;</trd>
  <trd lng="ca">calcopirita</trd>
  <trd lng="cs">kupropyrit</trd>
  <trd lng="en">chalcopyrite</trd>
  <trd lng="fr">chalcopyrite</trd>
  <trd lng="nl">koperkies</trd>
  <trdgrp lng="pl">
    <trd>chalkopiryt</trd>,
    <trd>piryt miedziowy</trd>
  </trdgrp>
  <trd lng="ro">calcopirit&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_d;&c_n;&c_y;&c_j; &c_k;&c_o;&c_l;&c_ch;&c_je;&c_d;&c_a;&c_n;</trd>,
    <trd>&c_h;&c_a;&c_l;&c_mol;&c_k;&c_o;&c_p;&c_i;&c_r;&c_i;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">kupropyrit</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_a;&#x43B;&#x44C;&#x43A;&#x43E;&#x43F;&c_ib;&c_r;&c_i;&c_t;</trd>,
    <trd>&#x43C;&c_ib;&c_d;&#x43D;&c_i;&c_j; &#x43A;&#x43E;&#x43B;&c_ch;&c_je;&c_d;&c_a;&#x43D;</trd>
  </trdgrp>
</drv>
</art>
