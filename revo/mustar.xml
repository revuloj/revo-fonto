<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mustar.xml,v 1.28 2024/01/26 19:05:54 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>mustard</rad>/o
</kap>
<drv mrk="mustar.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">KUI</uzo>
   <snc>
     <dif>
       <ref tip="super" cel="spic.0ajxo">Spica&jcirc;o</ref> el pistitaj
       semoj de <ref tip="vid" cel="sinap.0o">sinapo</ref> miksitaj kun
       <ref tip="prt" cel="vinagr.0o">vinagro</ref>:
      <ekz>
        en &gcirc;ia gusto estis unuigita la peza aromo de la perdrikrosta&jcirc;o kun facila
        acideto de <tld/>o
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la viandon &scirc;i &gcirc;uegis <klr>[&#x2026;]</klr> kaj samtempe observis Behemoton &scirc;miri ostron
        per <tld/>o
        <fnt>
          <bib>MkM</bib>
          <lok>24</lok>
        </fnt>;
      </ekz>
      <ekz>
        tagmeze ni ricevis bova&jcirc;on kun <tld/>a sa&ubreve;co
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 27</lok>
        </fnt>;
      </ekz>
       <ekz>
         &gcirc;i estas kiel <ind><tld/>o post la man&gcirc;o</ind>
         <klr>(tro malfrua peno)</klr>
         <fnt><bib>PrV</bib></fnt>.
         <trd lng="nl"><ind>mosterd</ind> na de maaltijd</trd>
         <trd lng="pl">musztarda po obiedzie</trd>
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_ch;&c_y;&c_c;&c_a;</trd>
  <trd lng="bg">&c_g;&c_o;&c_r;&c_ch;&c_i;&c_c;&c_a;</trd>
  <trd lng="ca">mostassa</trd>
  <trd lng="cs">ho&rcaron;&ccaron;ice</trd>
  <trd lng="de">Senf<klr tip="ind"> (Speise)</klr></trd>
  <trd lng="en">mustard</trd>
  <trd lng="es">mostaza</trd>
  <trd lng="fa">&a_kh;&a_r;&a_d;&a_l;</trd>
  <trd lng="fr">moutarde</trd>
  <trd lng="hu">must&aacute;r</trd>
  <trd lng="id">moster</trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30B9;&#x30BF;&#x30FC;&#x30C9;</trd>,
    <trd>&#x304B;&#x3089;&#x3057;</trd>
  </trdgrp>
  <trd lng="nl">mosterd</trd>
  <trd lng="pl">musztarda</trd>
  <trd lng="pt">mostarda <klr tip="amb">(cul.)</klr></trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_ch;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">hor&ccaron;ica</trd>
  <trd lng="sv">senap</trd>
  <trd lng="uk">&c_g;&c_ib;&c_r;&c_ch;&c_i;&c_c;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x82A5; <pr>ji&#xE8;</pr></trd>,
    <trd>&#x82A5;&#x672B; <pr>ji&#xE8;mo</pr></trd>
  </trdgrp>
</drv>
</art>
