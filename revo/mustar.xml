<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mustard</rad>/o
</kap>
<drv mrk="mustar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="spic.0o">Spica&jcirc;o</ref> el pistitaj semoj de
      <ref tip="vid" cel="sinap.0o">sinapo</ref> miksitaj kun vinagro:
      <ekz>
        &gcirc;i estas <tld/>o post la man&gcirc;o<fnt>Z</fnt>
        <klr>(tro malfrua peno)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Senf <klr tip="ind">(Speise)</klr></trd>
  <trd lng="nl">mosterd</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_ch;&c_i;&c_c;&c_a;</trd>
  <trd lng="sv">senap</trd>
</drv>
</art>
