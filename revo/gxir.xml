<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>&gcirc;ir</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="gxir.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="gxir.0i.valorpaperon">
    <dif>
      Meti subskribon sur la dorsa flanko de bilo, kambio a&ubreve; &ccirc;eko,
      per kiu oni cedas &gcirc;in kaj &gcirc;ian valoron al citita persono 
      a&ubreve; al &gcirc;ia prezentonto:
      <ekz>
        <tld/>i kambion al iu.
      </ekz>
    </dif>
    <trd lng="de">indossieren</trd>
    <trdgrp lng="en">
      <trd>endorse</trd>,
      <trd>indorse</trd>
    </trdgrp>
    <trd lng="fr">endosser<klr tip="ind"> (un papier-valeur &agrave; l'ordre de qn)</klr></trd>
    <trd lng="nl">endosseren</trd>
    <trdgrp lng="ru">
      <trd>&c_i;&c_n;&c_d;&c_o;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_u;&c_ju; &c_n;&c_a;&c_d;&c_p;&c_i;&c_s;&c_mol;</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="gxir.0i.sumon">
    <dif>
      Transpagi iun sumon el sia konto en la konton de iu alia:
      <ekz>
        K-do B <tld/>is hodia&ubreve; per &scirc;tata banko la sumon de 20 rubloj en la konton de SAT.
      </ekz>
    </dif>
    <trd lng="en">transfer</trd>
    <trdgrp lng="de">
      <trd>anweisen</trd>,
      <trd>&uuml;berweisen</trd>
    </trdgrp>
    <trd lng="fr">virer<klr tip="ind"> (de l'argent)</klr></trd>
    <trdgrp lng="nl">
      <trd>gireren</trd>,
      <trd>overschrijven<klr tip="ind"> (gireren)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_je;&c_s;&c_t;&c_i; &c_d;&c_je;&c_n;&c_mol;&c_g;&c_i; <klr tip="amb">(&c_s;&c_o; &c_s;&c_ch;&c_jo;&c_t;&c_a; &c_n;&c_a; &c_s;&c_ch;&c_jo;&c_t;)</klr></trd>,
      <trd>&c_zh;&c_i;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="gxir.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Subskribo de <tld/>anto.
    </dif>
    <trdgrp lng="fr">
      <trd>endos</trd>,
      <trd>endossement</trd></trdgrp> 
    <trd lng="nl">endossement</trd>
    <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_n;&c_a;&c_d;&c_p;&c_i;&c_s;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Ago <tld/>i <sncref ref="gxir.0i.sumon"/>.
    </dif>
    <trd lng="fr">virement<klr tip="ind"> (action de virer)</klr></trd>
    <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_d; &c_d;&c_je;&c_n;&c_je;&c_g; <klr tip="amb">(&c_s;&c_o; &c_s;&c_ch;&c_jo;&c_t;&c_a; &c_n;&c_a; &c_s;&c_ch;&c_jo;&c_t;)</klr></trd>
  </snc>
  <trd lng="nl">het <ind>gireren</ind></trd>
</drv>

<drv mrk="gxir.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      &Gcirc;irita papero:
      <ekz>
        bilo, kambio, &ccirc;eko.
      </ekz>
    </dif>
    <trd lng="fr">effet endoss&eacute;</trd>
  </snc>
  <snc>
    <dif>
      La &gcirc;irita sumo.
    </dif>
    <trd lng="fr">virement<klr tip="ind"> (somme transf&eacute;r&eacute;e)</klr></trd>
  </snc>
</drv>

<drv mrk="gxir.0ato">
  <kap>
    <tld/>ato
  </kap>
  <snc>
    <dif>
      La posta havanto de bilo, &ccirc;eko, kambio.
    </dif>
  </snc>
  <trd lng="fr">endossataire</trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_o;&c_s;&c_s;&c_a;&c_t;</trd>
</drv>

<drv mrk="gxir.0anto">
  <kap>
    <tld/>anto
  </kap>
  <snc>
    <dif>
      Tiu, kiu <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>endosseur</trd>
  </trdgrp> 
  <trd lng="ru">&c_i;&c_n;&c_d;&c_o;&c_s;&c_s;&c_a;&c_n;&c_t;</trd>
</drv>

<drv mrk="gxir.0ilo">
  <kap>
    <tld/>ilo
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Transpagilo.
    </dif>
    <refgrp tip="vid">
      <ref cel="skrib.sub0i">subskribi</ref>,
      <ref cel="respon.pri0i0eci0umi">respondeci</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="debet.0i">debeti</ref>,
      <ref cel="kredit.0i">krediti</ref>,
      <ref cel="pag">transpagi</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">formulaire de virement</trd>
  <trdgrp lng="nl">
    <trd>girobiljet</trd>,
    <trd>overschrijvingsformulier</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_t;&c_jo;&c_zh;&c_n;&c_o;&c_je; &c_p;&c_o;&c_r;&c_u;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
