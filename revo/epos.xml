<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>epos</rad>/o
      <fnt>Z</fnt>
    </kap>

    <drv mrk="epos.0o">
      <kap><tld/>o</kap>
      <snc mrk="epos.0o.POE">
        <uzo tip="fak">BELE</uzo>
        <uzo tip="fak">LIN</uzo>
        <dif>
          La rakonta speco de
          <ref tip="super" cel="litera.0o">literaturo</ref>
          (kontraste al <ref cel="lirik.0o">liriko</ref>
          kaj <ref cel="dram.0o.TEA">dramo</ref>):
          <ekz>
            la romanoj estas prozaj <tld/>oj.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_e;&c_p;&c_a;&c_s;</trd>
      <trd lng="de">episches Gedicht</trd>
      <trd lng="fr">&eacute;pop&eacute;e<klr tip="amb"> (genre)</klr></trd>
      <trd lng="hu">elbesz&eacute;l&odblac; <ind>k&ouml;ltem&eacute;ny</ind></trd>
  <trdgrp lng="pt">
    <trd>epos</trd>,
    <trd>epop&#233;ia</trd>
  </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_e;&c_p;&c_o;&c_s;</trd>,
       
<trd>&c_p;&c_o;&c_v;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja;
&c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
      </trdgrp>
    </drv>
  </art>
