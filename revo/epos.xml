  <art mrk="$Id: epos.xml,v 1.20 2023/08/31 11:49:30 revo Exp $">
    <kap>
      <rad>epos</rad>/o
      <fnt>Z</fnt>
    </kap>

    <drv mrk="epos.0o">
      <kap><tld/>o</kap>
      <snc mrk="epos.0o.POE">
        <uzo tip="fak">BELE</uzo>
        <uzo tip="fak">LIN</uzo>
        <dif>
          La rakonta speco de
          <ref tip="super" cel="litera.0o">literaturo</ref>
          (kontraste al <ref cel="lirik.0o">liriko</ref>
          kaj <ref cel="dram.0o.TEA">dramo</ref>):
          <ekz>
            la romanoj estas prozaj <tld/>oj.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_e;&c_p;&c_a;&c_s;</trd>
  <trd lng="ca">epopeia</trd>
  <trdgrp lng="cs">
    <trd>epos</trd>,
    <trd>v&#xFD;pravn&#xE1; b&#xE1;se&#x148;</trd>
  </trdgrp>
  <trd lng="de">episches Gedicht</trd>
  <trd lng="en">primitive narrative poetry</trd>
  <trd lng="es">epopeya</trd>
  <trd lng="fr">&eacute;pop&eacute;e<klr tip="amb">
      (genre)</klr></trd>
  <trd lng="hu">elbesz&eacute;l&odblac;
      <ind>k&ouml;ltem&eacute;ny</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x53D9;&#x4E8B;&#x8A69;  [<ind>&#x3058;&#x3087;&#x3058;&#x3057;</ind>]</trd>,
    <trd>&#x7269;&#x8A9E;&#x8A69;  [<ind>&#x3082;&#x306E;&#x304C;&#x305F;&#x308A;&#x3057;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">epos</trd>
  <trdgrp lng="pt">
        <trd>epos</trd>,
        <trd>epop&eacute;ia</trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_e;&c_p;&c_o;&c_s;</trd>,
        <trd>&c_p;&c_o;&c_v;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja;
        &c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>epos</trd>,
    <trd>v&#xFD;pravn&#xE1; b&#xE1;se&#x148;</trd>
  </trdgrp>
  <trd lng="uk">&#x435;&#x43F;&#x43E;&#x441;</trd>
</drv>
