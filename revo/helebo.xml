<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>helebor</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="helebo.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Helleborus</trd>), ornama planto, kies radiko estas
      venena.
    </dif>
  </snc>
  <trd lng="de">Nieswurz</trd>
  <trd lng="fr">hell&eacute;bore</trd>
  <trd lng="nl">nieskruid</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_z;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="helebo.nigra0o">
  <kap>
    nigra <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="helebo.0o"><tld/>o</ref>
      (<trd lng="la">Helleborus niger</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schwarze Nieswurz</trd>,
    <trd>Schneerose</trd>,
    <trd>Christrose</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>hell&eacute;bore noir</trd>,
    <trd>rose de No&euml;l</trd></trdgrp> 
  <trd lng="nl">kerstroos</trd>
</drv>
</art>
