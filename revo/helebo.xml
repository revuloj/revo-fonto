<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>helebor</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="helebo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="ranunk.0acoj">ranunkolacoj</ref>
      (<trd lng="la">Helleborus</trd>), ornama planto, kies radiko estas
      venena .
    </dif>
  </snc>
  <trd lng="de">Nieswurz</trd>
</drv>
<drv mrk="helebo.nigra0o">
  <kap>nigra <tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="helebo.0o"><tld/>o</ref>
      (<trd lng="la">Helleborus niger</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schwarze Nieswurz</trd>,
    <trd>Schneerose</trd>,
    <trd>Christrose</trd>
  </trdgrp>
</drv>
</art>
