<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ful.xml,v 1.19 2024/08/14 18:32:30 revo Exp $">
<kap>
  <rad>ful</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ful.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEKS</uzo>
  <snc mrk="ful.0i.TEKS">
    <dif>
      Premegi <ref cel="drap.0o">drapon</ref>
      por igi &gcirc;in pli densa kaj pli fortika;
      a&ubreve; harojn por fabriki <ref cel="felt.0o">felton</ref>:
      <ekz>
        neniu alkudras flika&jcirc;on el ne<tld/>ita drapo sur malnovan veston
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 9:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj vestoj fari&gcirc;is brilantaj, treege blankaj, kiel <tld/>isto sur la tero ne povas blankigi
        <fnt>
          <bib>NT</bib>
          <lok>Marko 9:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kampo de <tld/>istoj
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 18:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sapo de la <tld/>istoj
        <fnt>
          <bib>MT</bib>
          <lok>Mala&hcirc;i 3:2</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_v;&c_o;&c_w;&c_n;&c_u;, &c_l;&c_ja;&c_m;&c_je;&c_c;)</klr></trd>
  <trd lng="cs">valchovat</trd>
  <trd lng="de">walken</trd>
  <trd lng="en">full<klr tip="amb"> (treat cloth)</klr></trd>
  <trd lng="fr">fouler<klr tip="ind"> (du textile)</klr></trd>
  <trdgrp lng="hu">
    <trd>v&aacute;nyol</trd>,
    <trd>kall&oacute;z</trd>
    </trdgrp>
    <trdgrp lng="ja">
    <trd>&#22311;&#32302;&#12377;&#12427; <pr>&#12354;&#12387;&#12375;&#12421;&#12367;&#12377;&#12427;</pr></trd>,
    <trd>&#32302;&#32104;&#12377;&#12427; <pr>&#12375;&#12421;&#12367;&#12376;&#12421;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>touwen</trd>,
    <trd>vollen</trd>,
    <trd>persen</trd>,
    <trd>kneden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>foluszowa&cacute;</trd>,
    <trd>folowa&cacute;</trd>,
    <trd>spil&sacute;nia&cacute;</trd>,
    <trd>filcowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pisoar</trd>,
    <trd>apisoar</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_a;&c_l;&c_ja;&c_t;&c_mol; <klr>(&c_sh;&c_je;&c_r;&c_s;&c_t;&c_mol;, &c_v;&c_o;&c_j;&c_l;&c_o;&c_k;)</klr></trd>
  <trd lng="sk">valchova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_l;&c_ja;&c_t;&c_i; (&c_s;&c_u;&c_k;&c_n;&c_o;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_s;&c_t;&c_mol;)</trd>
  </trdgrp>
</drv>
</art>
