<vortaro>
<art mrk="$Id$">
<kap><rad>ful</rad>/i<fnt><bib>PV</bib></fnt></kap>
<drv mrk="ful.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Premegi drapon por igi &gcirc;in pli densa kaj pli fortika,
      a&ubreve; harojn por fabriki felton.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_v;&c_o;&c_w;&c_n;&c_u;, &c_l;&c_ja;&c_m;&c_je;&c_c;)</klr></trd>
  <trd lng="de">walken</trd>
  <trd lng="en">full<klr tip="amb"> (treat cloth)</klr></trd>
  <trd lng="fr">fouler<klr tip="ind"> (du textile)</klr></trd>
  <trdgrp lng="nl">
    <trd>touwen</trd>,
    <trd>vollen</trd>,
    <trd>persen</trd>,
    <trd>kneden</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pisoar</trd>,
    <trd>apisoar</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_a;&c_l;&c_ja;&c_t;&c_mol; <klr>(&c_sh;&c_je;&c_r;&c_s;&c_t;&c_mol;, &c_v;&c_o;&c_j;&c_l;&c_o;&c_k;)</klr></trd>
  <trd lng="sk">valchova≈•</trd>
</drv>
</art>
