<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: deism.xml,v 1.22 2023/11/08 09:26:30 revo Exp $">
<kap>
  <rad>deism</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="deism.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="doktri.0o">Doktrino</ref>,
      kiu asertas la ekzistadon de Dio, bazante sin nur sur
      raciaj pruvoj kaj malakceptante <ref cel="revela.0o">revelacion</ref>:
      <ekz>
        la Epoko de Racio enhavas argumentojn tipe <tld/>ajn, ekzemple, &gcirc;i reliefigas la
        koruptecon de la kristana eklezio kaj &gcirc;i kritikas ties penon por akiri
        politikan povon, <klr>[&#8230;]</klr> pledas por la racio anstata&ubreve; revelacio,
        <klr>[kaj vidas]</klr> la Biblion kiel simpla literatura verko kaj ne kiel
        teksto inspirita de la dio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=351495">La Epoko de Racio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="di.0ismo">diismo</ref>
    </refgrp>
    <refgrp tip="super">
      <ref cel="teism.0o">teismo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_d;&c_je;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="ca">deisme</trd>
  <trd lng="cs">deizmus</trd>
  <trdgrp lng="de">
     <trd>Deismus</trd>,
     <trd>Gottglaube</trd>
  </trdgrp>
  <trd lng="en">deism</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_d;&a_A;&f_g;&a_r;&a_A;&f_y;&f_y;</trd>,
    <trd>&a_kh;&a_d;&a_A;&a_b;&a_A;&a_w;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;isme</trd>
  <trd lng="hu">deizmus</trd>
  <trd lng="id">deisme</trd>
  <trd lng="it">deismo</trd>
  <trdgrp lng="ja">
    <trd/>,
    <trd>&#29702;&#x795E;&#x8AD6;  <pr>&#x308A;&#12375;&#12435;&#x308D;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">deizm</trd>
  <trd lng="pt">de&iacute;smo</trd>
  <trd lng="ru">&c_d;&c_je;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">deizmus</trd>
  <trdgrp lng="zh">
    <trd>&#x81EA;&#28982;&#x795E;&#x8AD6; <pr>z&#xEC;r&#xE1;nsh&#xE9;nl&#xF9;n</pr></trd>,
    <trd>&#x4FE1;&#22857; <pr>x&#xEC;nf&#xE8;ng</pr></trd>
  </trdgrp>
</drv>
</art>
