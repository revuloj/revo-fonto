<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>deism</rad>/o
    <fnt>Z</fnt>
  </kap>
  <drv mrk="deism.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">FIL</uzo>
    <snc>
      <dif>
        Doktrino, kiu asertas la ekzistadon de Dio, bazante sin nur sur
        raciaj pruvoj k malakceptante revelacion.
      </dif>
      <refgrp tip="sin">
        <ref cel="di.0ismo">diismo</ref>;
      </refgrp>
      <refgrp tip="vid">
        <ref cel="teism.0o">teismo</ref>.
      </refgrp>
      </snc>
    <trd lng="be">&c_d;&c_e;&c_ib;&c_z;&c_m;</trd>
    <trdgrp lng="de">
       <trd>Deismus</trd>,
       <trd>Gottglaube</trd>
    </trdgrp>
    <trd lng="en">deism</trd>
    <trdgrp lng="fa">
      <trd>&a_kh;&a_d;&a_A;&f_g;&a_r;&a_A;&f_y;&f_y;</trd>,
      <trd>&a_kh;&a_d;&a_A;&a_b;&a_A;&a_w;&a_r;&f_y;</trd>
    </trdgrp>
    <trd lng="fr">d&eacute;isme</trd>
    <trd lng="hu">deizmus</trd>
    <trd lng="it">deismo</trd>
    <trd lng="pl">deizm</trd>
    <trd lng="pt">de&iacute;smo</trd>
    <trd lng="ru">&c_d;&c_je;&c_i;&c_z;&c_m;</trd>
    <trd lng="sk">deizmus</trd>
  </drv>
</art>
