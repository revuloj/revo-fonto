<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>psikiatr</rad>/o
</kap>
<drv mrk="psikia.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">PSI</uzo>
  <snc>
    <dif>
      Fakulo pri <ref tip="vid" cel="psikia1.0o">psikiatrio</ref>,
      ordinare anka&ubreve; kuracisto de mensaj malsanoj:
      <ekz>
        mi rekomendis al &scirc;i konsulti <tld/>on kaj, &ccirc;agrenite, &scirc;i
        foriris<fnt><aut>Werner Schad</aut>: <vrk>Nenio perfektas</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 4a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_s;&c_ib;&c_h;&c_ib;&c_ja;&c_t;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Psychiater</trd>,
    <trd>Nervenarzt</trd>,
    <trd>Irrenarzt<klr> (arkaika)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>psychiatre</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pszichi&aacute;ter</trd>,
    <trd>idegorvos</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>psychiater</trd>,
    <trd>zenuwarts</trd>
  </trdgrp>
  <trd lng="pt">psiquiatra</trd>
  <trd lng="ru">&c_p;&c_s;&c_i;&c_h;&c_i;&c_a;&c_t;&c_r;</trd>
</drv>
</art>
