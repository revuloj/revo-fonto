<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: psikia.xml,v 1.23 2024/02/17 11:11:10 revo Exp $">
<kap>
  <rad>psikiatr</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="&hcirc;">psi&hcirc;iatr</rad>/o</kap></var>
</kap>
<drv mrk="psikia.0o">
  <kap><tld/>o,
    <var><kap><tld var="&hcirc;"/>o</kap></var>
  </kap>
  <snc mrk="psikia.0o.MED">
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="super" cel="kurac.0isto">Kuracisto</ref>, kies fako
      estas <ref tip="vid" cel="psikia1.0o"><tld/>io</ref>:
      <ekz>
        mi rekomendis al &scirc;i konsulti <tld/>on kaj, &ccirc;agrenite, &scirc;i foriris
        <fnt><aut>Werner Schad</aut>: <vrk>Nenio perfektas</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 4a, p. 9a</lok></fnt>;
      </ekz>
      <ekz>
        voli trudi la feli&ccirc;on al iu anstata&ubreve; li, la&ubreve; certaj 
        <tld var="&hcirc;"/>oj estas formo de perverso
        <fnt>
          <aut>A.-C. Robert, trad. V. Lutermano</aut>,
          <vrk><url ref="&Mondediplo;article2479.html">Stumpigitaj
          parlamentoj</url></vrk>,
          <lok>Le Monde diplomatique, 2017-04</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_s;&c_ib;&c_h;&c_ib;&c_ja;&c_t;&c_r;</trd>
  <trd lng="cs">psychiatr</trd>
  <trdgrp lng="de">
    <trd>Psychiater</trd>,
    <trd>Nervenarzt</trd>,
    <trd>Irrenarzt<klr> (arkaika)</klr></trd>
  </trdgrp>
  <trd lng="en">psychiatrist</trd>
  <trd lng="fr">psychiatre</trd>
  <trd lng="fy">psychiater</trd>
  <trdgrp lng="hu">
    <trd>pszichi&aacute;ter</trd>,
    <trd>idegorvos</trd>
  </trdgrp>
  <trd lng="ja">&#x7CBE;&#x795E;&#x79D1;&#x533B;  <pr>&#x305B;&#x3044;&#x3057;&#x3093;&#x304B;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>psychiater</trd>,
    <trd>zenuwarts</trd>
  </trdgrp>
  <trd lng="pl">psychiatra</trd>
  <trd lng="pt">psiquiatra</trd>
  <trd lng="ro">psihiatru</trd>
  <trd lng="ru">&c_p;&c_s;&c_i;&c_h;&c_i;&c_a;&c_t;&c_r;</trd>
  <trd lng="sk">psychiater</trd>
  <trd lng="uk">&#x43F;&c_s;&c_i;&c_h;&c_ib;&c_a;&c_t;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x7CBE;&#x795E;&#x79D1;&#x533B;&#x751F; <pr>j&#x12B;ngsh&#xE9;nk&#x113;y&#x12B;sh&#x113;ng</pr></trd>,
    <trd>&#x7CBE;&#x795E;&#x79D1;&#x91AB;&#x751F; <pr>j&#x12B;ngsh&#xE9;nk&#x113;y&#x12B;sh&#x113;ng</pr></trd>,
    <trd>&#x5FC3;&#x7406;&#x533B;&#x751F; <pr>x&#x12B;nl&#x1D0;y&#x12B;sh&#x113;ng</pr></trd>,
    <trd>&#x5FC3;&#x7406;&#x91AB;&#x751F; <pr>x&#x12B;nl&#x1D0;y&#x12B;sh&#x113;ng</pr></trd>
  </trdgrp>
</drv>
</art>
