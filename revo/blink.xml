<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: blink.xml,v 1.4 2022/04/22 10:40:28 afido Exp $">
    <kap>
      <rad>blink</rad>/i
    </kap>

<drv mrk="blink.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Intermite <ref tip="super" cel="lum.0i">lumi</ref>; <ref tip="sin"
      cel="puls.lum0i">lumpulsi</ref>:
      <ekz>
        la ru&gcirc;a lumeto de la faksilo <tld/>as
        <fnt>
          <aut>Jorge Camacho</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=la-majstro-kaj-martinelli">La Majstro kaj
          Martinelli</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi &ccirc;iam uzas la <tld/>ilojn por indiki, &ccirc;u la veturilon turnos dekstren a&ubreve;
        maldekstren
        <fnt>
          <vrk><url ref="https://tekstaro.com/t?nomo=kontakto-2011-2019">Blogo de
          supervivanto</url></vrk>
          <lok>Kontakto 2011-2019</lok>
        </fnt>.
      </ekz>  
    </dif>    
    <refgrp tip="vid">
       <ref cel="flagr.0i">flagri</ref>,
       <ref cel="bril.trem0i">trembrili</ref>
    </refgrp>
    <trd lng="de">blinken</trd>
    <trdgrp lng="en">
      <trd>blink (light)</trd>,
      <trd>flash (repeatedly)</trd>
    </trdgrp>
    <trd lng="fr">clignoter</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&c_b;&c_l;&c_i;&c_m;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_i;&c_g;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_i;&c_g;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

  </art>

