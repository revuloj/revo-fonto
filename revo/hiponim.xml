<drv mrk="hiponim.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="noci.sub0o">Subnocio</ref>:
      <ekz>
        skarlata <klr>[&#8230;]</klr>, karmezina, <klr>[&#8230;]</klr> amba&ubreve; estas <tld/>oj de ru&gcirc;a
        <fnt>
          <bib>LAE</bib>
          <lok>5. Leksiko Kaj Semantiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiaj rilatoj estas sinonimeco, antonimeco, plursignifo, paronimeco, <tld/>eco
        kaj &gcirc;ia malo, kaj inverseco
        <fnt>
          <bib>LAE</bib>
          <lok>5. Leksiko Kaj Semantiko</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Hyponym</trd>
    <trd lng="en">hyponym</trd>
    <trd lng="fr">hyponyme</trd>
    <trd lng="pl">hiponim</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_p;&c_a;&c_n;&c_ja;&c_c;&c_mol;&c_c;&c_je;</trd>
  <trd lng="ja">&#x4E0B;&#x4F4D;&#x8A9E;  <pr>&#x304B;&#12356;&#12372;</pr></trd>
  <trd lng="zh">&#x4E0B;&#x4E49;&#x8BCD; <pr>xi&#xE0;y&#xEC;c&#xED;</pr></trd>
</drv>

