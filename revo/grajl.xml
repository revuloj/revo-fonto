<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: grajl.xml,v 1.17 2020/03/30 17:10:23 revo Exp $">
<kap>
  <rad>grajl</rad>/o
</kap>
<drv mrk="grajl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Speco de <ref tip="sub" cel="hajl.0o">hajlo</ref>
      en formo de tre malgrandaj grajnoj:  
      <ekz>
        firno esti&gcirc;as, kiam ne&gcirc;kristaloj per ripeta degelado 
        kaj refrosti&gcirc;o kunfandi&gcirc;as al pli grandaj <tld/>aj kaj grajnecaj
        objektoj
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=21831">Firno</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> formi&gcirc;as kiam subfridigantaj faloj de
       akvo kondens<klr>[i&gcirc;]</klr>as sur ne&gcirc;ero, ke formigas 2-5 mm de
       prujno
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=28581">Grajlo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_u;&c_g;&c_r;&c_a;&c_sh;&c_i;&c_c;&c_a;</trd>
  <trd lng="ca">aiguaneu</trd>
  <trdgrp lng="cs">
    <trd>krupobit&iacute;</trd>,
    <trd>sn&ecaron;hov&eacute; kroupy</trd>,
    <trd>sn&ecaron;hov&eacute; krupky</trd>
  </trdgrp>
  <trd lng="de">Graupeln</trd>
  <trd lng="en">sleet</trd>
  <trd lng="es">aguanieve</trd>
  <trd lng="fr">gr&eacute;sil<klr tip="ind"> (fine gr&ecirc;le)</klr></trd>
  <trdgrp lng="nl">
    <trd>stofhagel</trd>,
    <trd>korrelhagel</trd>
  </trdgrp>
  <trd lng="pl">krupy</trd>
  <trdgrp lng="pt">
   <trd>granizo mi&uacute;do</trd>, 
   <trd>saraiva mi&uacute;da</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_u;&c_p;&c_a;</trd>,
    <trd>&c_m;&c_je;&c_l;&c_k;&c_i;&c_j; &c_g;&c_r;&c_a;&c_d;</trd>
  </trdgrp>
  <trd lng="sk">krupobitie <klr>(jemnej&scaron;ie)</klr></trd>
</drv>
</art>
