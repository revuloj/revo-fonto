<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kabrio.xml,v 1.32 2025/01/12 12:47:19 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>kabriolet</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kabrio.0o">
   <kap><tld/>o</kap>
   <snc mrk="kabrio.0o.TRA">
     <uzo tip="fak">TRA</uzo>
     <dif>
       <ref tip="super" cel="vetur.0ilo.TRA">Veturilo</ref> kun
       surtirebla tegmento.
     </dif>
     <subsnc mrk="kabrio.0o.senmotora">
       <dif>
         Malpeza durada unu&ccirc;evala veturilo
         malfermita &ccirc;e la anta&ubreve;a parto kaj
         provizita per faldebla leda kovrilo:
         <ekz>
           sinjor Denique estis <klr>...</klr> grimpinta kun energia
           decido sur sian <tld/>on por hejmenveturi
           <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>.
         </ekz>
       </dif>
       <trd lng="ca">cabriol&eacute;</trd>
       <trd lng="de">Kabriolett</trd>
       <trdgrp lng="en">
         <trd>cabriolet</trd>,
         <trd>chaise</trd>
       </trdgrp>
       <trdgrp lng="fi">
         <trd>kuomulliset <ind>kiesit</ind></trd>,
         <trd>kabrioletti</trd>
       </trdgrp>
       <trd lng="he">&kaf;&resh;&kaf;&resh;&he;</trd>
       <trdgrp lng="it">
         <trd>calesse<klr> (con mantice)</klr></trd>,
         <trd>calessino<klr> (con mantice)</klr></trd>
       </trdgrp>
       <trd lng="nl">cabriolet</trd>
       <trd lng="pt">cabriol&eacute;</trd>
       <trdgrp lng="ru">
         <trd>&c_k;&c_a;&c_b;&c_r;&c_i;&c_o;&c_l;&c_je;&c_t;</trd>,
         <trd>&c_l;&c_a;&c_n;&c_d;&c_o;</trd>,
         <trd>&c_o;&c_d;&c_n;&c_o;&c_k;&c_o;&c_l;&c_k;&c_a;</trd>
       </trdgrp>
       <trdgrp lng="tr">
         <trd>&uuml;st&uuml; a&ccedil;&inodot;lan araba</trd>,
         <trd>tek atl&inodot; arabas&inodot;</trd>
       </trdgrp>
     </subsnc>

     <subsnc mrk="kabrio.0o.auxto">
       <uzo tip="fak">AUT</uzo>
       <dif>
         <ref tip="super" cel="auxt.0o.AUT">A&ubreve;to</ref>
         kun faldebla tegmento (<ref tip="vid" cel="kapucx.0o.AUT">kapu&ccirc;o</ref>):
         <ekz>
           mi eltiris po&scirc;lampon <klr>(&#8230;)</klr> por rigardi la a&ubreve;ton;
           &gcirc;i estis <nom>Packard</nom>-kabrioleto, ka&scirc;tan- a&ubreve; malhel-bruna
           <fnt>
             <aut>R. Chandler, trad. A. Neilson</aut>
             <vrk>el: <url ref="https://www.adoneilson.com/eo/prozo/la_dormego.html">La
             Dormego</url></vrk>
             <lok>[vidita en 2025]</lok>
           </fnt>.
         </ekz>
       </dif>
       <trd lng="ca">descapotable</trd>
       <trdgrp lng="en">
         <trd>cabriolet</trd>,
         <trd>convertible</trd>
       </trdgrp>
       <trdgrp lng="es">
         <trd>descapotable</trd>,
         <trd>convertible</trd>
       </trdgrp>
       <trd lng="de">Kabrio</trd>
       <trdgrp lng="fi">
          <trd>avoauto</trd>
       </trdgrp>
       <trdgrp lng="it">
         <trd>decapottabile<klr> (auto)</klr></trd>,
         <trd>cabriolet</trd>,
         <trd>cabrio</trd>,
         <trd>convertibile<klr> (cabrio)</klr></trd>
       </trdgrp>
       <trdgrp lng="nl">
         <trd>cabriolet</trd>,
         <trd>cabrio</trd>,
         <trd>convertible</trd>
       </trdgrp>
       <trd lng="pl">kabriolet</trd>
       <trd lng="pt">autom&oacute;vel <ind>convers&iacute;vel</ind>
       </trd>
       <trdgrp lng="ru">
         <trd>&c_k;&c_a;&c_b;&c_r;&c_i;&c_o;&c_l;&c_je;&c_t;</trd>,
         <trd>&c_l;&c_a;&c_n;&c_d;&c_o;</trd>,
         <trd>
         &c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_mol; &c_s;
         &c_o;&c_t;&c_k;&c_i;&c_d;&c_n;&c_y;&c_m;
         &c_v;&c_je;&c_r;&c_h;&c_o;&c_m;</trd>
       </trdgrp>
     </subsnc>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_b;&c_r;&c_y;&c_ja;&c_l;&c_je;&c_t;</trd>
  <trd lng="ca">cabriol&eacute;</trd>
  <trd lng="cs">kabriolet</trd>
  <trd lng="fr">cabriolet</trd>
  <trd lng="hu">kabriolet</trd>
  <trd lng="ja">&#x30AB;&#x30D6;&#x30EA;&#x30AA;&#x30EC;</trd>
  <trd lng="nl">cabriolet</trd>
  <trd lng="sk">kabriolet</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&c_b;&c_r;&c_ib;&#x43E;&#x43B;&c_je;&c_t;</trd>,
    <trd>&#x43E;&c_d;&#x43D;&#x43E;&#x43A;&#x43E;&#x43B;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>
</art>
