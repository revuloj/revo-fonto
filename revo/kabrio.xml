<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kabriolet</rad>/o
</kap>
<drv mrk="kabrio.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Durada unu&ccirc;evala kale&scirc;o, malfermita &ccirc;e la anta&ubreve;a parto kaj
      provizita per faldebla leda kovrilo.  
    </dif>
    <trd lng="de">Kabriolett</trd>
    <trd lng="en">cabriolet</trd>
    <trd lng="fr">cabriolet</trd>
    <trd lng="nl">cabriolet</trd>
    <trdgrp lng="ru">
     <trd>&c_k;&c_a;&c_b;&c_r;&c_i;&c_o;&c_l;&c_je;&c_t;</trd>,
     <trd>&c_o;&c_d;&c_n;&c_o;&c_k;&c_o;&c_l;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      A&ubreve;tomobilo kun faldebla tegmento.
    </dif>
    <trdgrp lng="en">
      <trd>cabriolet</trd>,
      <trd>convertible</trd>
    </trdgrp>
    <trd lng="de">Kabrio</trd>
  </snc>
</drv>
</art>
