<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>2</ofc>
   <rad>kabriolet</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kabrio.0o">
   <kap><tld/>o</kap>
   <snc mrk="kabrio.0o.TRA">
     <uzo tip="fak">TRA</uzo>
     <dif>
       <ref tip="super" cel="vetur.0ilo.TRA">Veturilo</ref> kun
       surtirebla tegmento.
     </dif>
     <subsnc mrk="kabrio.0o.senmotora">
       <dif>
         Malpeza durada unu&ccirc;evala veturilo
         malfermita &ccirc;e la anta&ubreve;a parto kaj
         provizita per faldebla leda kovrilo:
         <ekz>
           sinjor Denique estis <klr>...</klr> grimpinta kun energia
           decido sur sian <tld/>on por hejmenveturi
           <fnt><bib>KielAkvo</bib>, <lok>1:1</lok></fnt>.
         </ekz>
       </dif>
       <trd lng="de">Kabriolett</trd>
       <trd lng="en">cabriolet</trd>
       <trdgrp lng="fi">
         <trd>kuomulliset <ind>kiesit</ind></trd>,
         <trd>kabrioletti</trd>
       </trdgrp>
       <trd lng="he">&kaf;&resh;&kaf;&resh;&he;</trd>
       <trd lng="nl">cabriolet</trd>
       <trd lng="pt">cabriol&eacute;</trd>
       <trdgrp lng="ru">
         <trd>&c_k;&c_a;&c_b;&c_r;&c_i;&c_o;&c_l;&c_je;&c_t;</trd>,
         <trd>&c_l;&c_a;&c_n;&c_d;&c_o;</trd>,
         <trd>&c_o;&c_d;&c_n;&c_o;&c_k;&c_o;&c_l;&c_k;&c_a;</trd>
       </trdgrp>
       <trdgrp lng="tr">
         <trd>&uuml;st&uuml; a&ccedil;&inodot;lan araba</trd>,
         <trd>tek atl&inodot; arabas&inodot;</trd>
       </trdgrp>
     </subsnc>

     <subsnc mrk="kabrio.0o.auxto">
       <uzo tip="fak">AUT</uzo>
       <dif>
         <ref tip="super" cel="auxtom1.0o">A&ubreve;tomobilo</ref>
         kun faldebla tegmento.
       </dif>
       <trdgrp lng="en">
         <trd>cabriolet</trd>,
         <trd>convertible</trd>
       </trdgrp>
       <trdgrp lng="es">
         <trd>descapotable</trd>,
         <trd>convertible</trd>
       </trdgrp>
       <trd lng="de">Kabrio</trd>
       <trdgrp lng="fi">
          <trd>avoauto</trd>
       </trdgrp>
       <trdgrp lng="nl">
         <trd>cabriolet</trd>,
         <trd>cabrio</trd>,
         <trd>convertible</trd>
       </trdgrp>
       <trd lng="pl">kabriolet</trd>
       <trd lng="pt">autom&oacute;vel <ind>convers&iacute;vel</ind>
       </trd>
       <trdgrp lng="ru">
         <trd>&c_k;&c_a;&c_b;&c_r;&c_i;&c_o;&c_l;&c_je;&c_t;</trd>,
         <trd>&c_l;&c_a;&c_n;&c_d;&c_o;</trd>,
         <trd>
         &c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_mol; &c_s; 
         &c_o;&c_t;&c_k;&c_i;&c_d;&c_n;&c_y;&c_m; 
         &c_v;&c_je;&c_r;&c_h;&c_o;&c_m;</trd>
       </trdgrp>
     </subsnc>
   </snc>
   <trd lng="be">&c_k;&c_a;&c_b;&c_r;&c_y;&c_ja;&c_l;&c_je;&c_t;</trd>
   <trd lng="fr">cabriolet</trd>
   <trd lng="hu">kabriolet</trd>
</drv>
</art>
