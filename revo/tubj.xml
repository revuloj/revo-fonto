<art mrk="$Id: tubj.xml,v 1.9 2023/09/04 19:01:54 revo Exp $">
<kap>
    <rad>tubj</rad>/o <fnt><bib>RokPop</bib></fnt>
</kap>
<drv mrk="tubj.0o">
  <kap><tld/>o</kap>
  <snc mrk="tubj.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Granda latuna <ref tip="lst" lst="voko:blovinstrumentoj" cel="instru2.blov0o">blovinstrumento</ref> kun valvoj:
      <ekz>
        la <tld/>o <klr>[&#x2026;]</klr> ludas la plej malaltajn notojn en la
        latuna blovinstrumenta familio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=230845">Tubjo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/e/eb/VDC_2519_%2839110503025%29.jpg/640px-VDC_2519_%2839110503025%29.jpg" prm="PD"><tld/>isto
        <fnt>
          <aut>La Usona Armea Bando, VDC_2519</aut>
          <lok><url ref="&WCW;/File:VDC_2519_(39110503025).jpg">&WCW;/File:VDC_2519_(39110503025).jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Tuba</trd>
  </snc>
  <trd lng="ca">tuba</trd>
  <trdgrp lng="cs">
    <trd>basov&aacute; trubka</trd>,
    <trd>tuba</trd>
  </trdgrp>
  <trd lng="en">tuba</trd>
  <trdgrp lng="it">
   <trd>tuba</trd>, 
   <trd>flicorno<klr> (basso o contrabbasso)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x30C1;&#x30E5;&#x30FC;&#x30D0;</trd>
  <trd lng="pl">tuba</trd>
  <trdgrp lng="sk">
    <trd>tr&uacute;ba</trd>,
    <trd>tuba</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x443;&#x431;&#x430;</trd>,
    <trd>&#x432;&#x435;&#x43B;&#x438;&#x43A;&#x430; &#x431;&#x430;&#x441;&#x43E;&#x432;&#x430; &#x442;&#x440;&#x443;&#x431;&#x430;</trd>
  </trdgrp>
</drv>
