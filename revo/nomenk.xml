<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nomenk.xml,v 1.25 2022/05/01 16:20:54 revo Exp $">
<kap>
  <rad>nomenklatur</rad>/o
</kap>
<drv mrk="nomenk.0o">
  <kap><tld/>o</kap>
  <snc mrk="nomenk.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Regula <ref tip="super" cel="nom.0aro">nomaro</ref> por &ccirc;iuj
      a&jcirc;oj de
      iu scienco, arto, industrio:
      <ekz>
        scienca <tld/>o
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        multaj laikoj <klr>[&#x2026;]</klr>, pensas, ke tiuj <tld/>oj estas absolute ekzaktaj
        kaj rigoraj
        <fnt>
          <bib>Monato</bib>
          <aut>Manuel Halvelik</aut>
          <vrk><url ref="&Monato;/2007/008949.php">Atentinda solvo por kompleksega problemo</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
       <ref cel="termin.0aro">terminaro</ref>,
       <ref cel="terminologi.0o.terminaro">terminologio<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_m;&c_e;&c_n;&c_k;&c_l;&c_ja;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>nomenklatura</trd>,
    <trd>odborn&eacute; n&aacute;zvoslov&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nomenklatur</trd>,
    <trd>Benennungssystem</trd>,
    <trd>Bezeichnungssystem</trd>
  </trdgrp>
  <trd lng="el">&omikron;&ny;&omikron;&my;&alfa;&tau;&omikron;&lambda;&omikron;&gamma;&jota_ton;&alfa;</trd>
  <trd lng="en">nomenclature</trd>
  <trd lng="es">nomenclatura</trd>
  <trdgrp lng="fr">
    <trd>nomenclature</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>n&oacute;menklat&uacute;ra</trd>,
    <trd>n&eacute;vmutat&oacute;</trd>
  </trdgrp>
  <trd lng="it">nomenclatura</trd>
  <trd lng="nl">nomenclatuur</trd>
  <trdgrp lng="pl">
    <trd>nazewnictwo</trd>,
    <trd>nomenklatura</trd>
  </trdgrp>
  <trd lng="pt">nomenclatura</trd>
  <trd lng="ro">nomenclatur&abreve;</trd>
  <trd lng="ru">&c_n;&c_o;&c_m;&c_je;&c_n;&c_k;&c_l;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>nomenklat&uacute;ra</trd>,
    <trd>odborn&eacute; n&aacute;zvoslovie</trd>
  </trdgrp>
  <trd lng="uk">&#x43D;&#x43E;&#x43C;&#x435;&#x43D;&#x43A;&#x43B;&#x430;&#x442;&#x443;&#x440;&#x430;</trd>
</drv>

<drv mrk="nomenk.Nova_Biologia_N0o">
  <kap>Nova Biologia <tld lit="N"/>o</kap>
  <mlg>NBN</mlg>
  <snc mrk="nomenk.Nova_Biologia_N0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      <tld lit="N"/>o de viva&jcirc;oj proponita ekde 1971 alternative al la
      linea sistemo:
      <ekz>
        la nomoj indikitaj inter asteriskoj estas nomoj de Nova Biologia <tld lit="N"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Izabel C. O. Santiago</aut>
          <vrk><url ref="&Steloj;monato2/006900.html">Fa&ubreve;no: La "drakoj" de Komodo: Gigantaj bestoj sur izola insulo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Nova Biologia <tld lit="N"/>o (N.B.N.) la nomo estas
        *Kapdelfeno familitipa*
        <fnt>
          <aut>Wim M. A. De Smet</aut>,
          <vrk>Gigantoj sur strandoj</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1998a, numero 11a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">Nouvelle Nomenclature Biologique</trd>
  <trd lng="hu">&Uacute;j Biol&oacute;giai <ind>N&oacute;menklat&uacute;ra</ind></trd>
  <trd lng="nl">Nieuwe Biologische <ind>Nomenclatuur</ind></trd>
  <trd lng="pl">Nowa  <ind>Nomenklatura</ind> Biologiczna</trd>
  <trdgrp lng="pt">
    <trd>Nova <ind>Nomenclatura</ind> Biol&oacute;gica</trd>,
    <trd>NNB</trd>
  </trdgrp>
  <trd lng="ro">Noua  <ind>Nomenclatur&abreve;</ind> Biologic&abreve;</trd>
</drv>
</art>
