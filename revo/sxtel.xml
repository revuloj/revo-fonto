<drv mrk="sxtel.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxtel.0i.JUR">
    <dif>
      Ka&scirc;e forkapti kaj proprigi al si poseda&jcirc;on de aliulo:
      <ekz>
        gro&scirc;on <tld/>is, ho <tld/>isto, milojn <tld/>is,
        financisto
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        unu al alia <tld/>is la butonojn de la vestoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <tld/>as de najbaro por doni al altaro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vi <tld/>is al mi mian filon el la brakoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi <tld/>is de mi karan horon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pren.0i">preni</ref>,
      <ref cel="rab.0i">rabi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_r;&c_a;&c_d;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_kata;&t_shabchu;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>furtar</trd>,
    <trd>sostreure</trd>,
    <trd>sostraure</trd>,
    <trd>robar</trd>,
    <trd>pispar</trd>,
    <trd>fotre</trd>
  </trdgrp>
  <trd lng="de">stehlen</trd>
  <trd lng="en">steal</trd>
  <trdgrp lng="es">
     <trd>robar</trd>,
     <trd>hurtar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>voler <klr tip="ind">(d&eacute;rober)</klr></trd>,
    <trd>d&eacute;rober</trd>
  </trdgrp>
  <trd lng="hu">lop</trd>
  <trdgrp lng="id">
    <trd><baz>curi</baz>mencuri</trd>,
    <trd><baz>colong</baz>mencolong</trd>,
    <trd><baz>maling</baz>memaling</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>rubare</trd>,
    <trd>sottrarre<klr> (rubare)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#30423;&#12416;  <pr>&#12396;&#12377;&#12416;</pr></trd>,
    <trd>&#12371;&#12387;&#12381;&#12426;&#22890;&#12358;  <pr>&#12371;&#12387;&#12381;&#12426;&#12358;&#12400;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stelen</trd>,
    <trd>pikken</trd>,
    <trd>jatten</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kra&sacute;&cacute;</trd>,
    <trd>buchn&aogonek;&cacute;<klr>  (ukra&sacute;&cacute; pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>furtar</trd>,
    <trd>roubar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_a;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_i;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">stj&auml;la</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_r;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20599; <pr>t&omacron;u</pr></trd>
    <trd>&#31363; <pr>qi&egrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxtel.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxtel.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Ago <tld/>i:
      <ekz>
        faras rabon kaj <tld/>on, por oferi al Dio kandelon
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_d;&c_z;&c_jo;&c_zh;</trd>,
    <trd>&c_z;&c_l;&c_a;&c_d;&c_z;&c_je;&c_j;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>furt</trd>,
    <trd>sostracci&oacute;</trd>,
    <trd>robatori</trd>
  </trdgrp>
  <trd lng="de">Diebstahl</trd>
  <trd lng="en">theft</trd>
  <trd lng="es">robo</trd>
  <trd lng="fr">vol <klr tip="ind">(du bien d'autrui)</klr></trd>
  <trd lng="hu">lop&aacute;s</trd>
  <trdgrp lng="id">
    <trd><baz>curi</baz>pencurian</trd>,
    <trd><baz>colong</baz>pencolongan</trd>,
    <trd><baz>maling</baz>pemalingan</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>furto</trd>,
    <trd>sottrazione<klr> (furto)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#31363;&#30423;  <pr>&#12379;&#12387;&#12392;&#12358;</pr></trd>,
    <trd>&#30423;&#12415;  <pr>&#12396;&#12377;&#12415;</pr></trd>
  </trdgrp>
  <trd lng="nl">diefstal</trd>
  <trdgrp lng="pl">
    <trd>kradzie&zdot;</trd>,
    <trd>grabie&zdot;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>furto</trd>,
    <trd>roubo</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_zh;&c_a;</trd>
  <trd lng="sv">st&ouml;ld</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_r;&c_a;&c_d;&c_ib;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_d;&c_ib;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_r;&c_a;&c_d;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22833;&#31363; <pr>sh&imacron;qi&egrave;</pr></trd>,
    <trd>&#22833;&#31434; <pr>sh&imacron;qi&egrave;</pr></trd>,
    <trd>&#30423;&#26696; <pr>d&agrave;o&agrave;n</pr></trd>,
    <trd>&#30428;&#26696; <pr>d&agrave;o&agrave;n</pr></trd>,
    <trd>&#20599;&#30423; <pr>t&omacron;ud&agrave;o</pr></trd>,
    <trd>&#20599;&#30428; <pr>t&omacron;ud&agrave;o</pr></trd>,
    <trd>&#20599;&#31363; <pr>t&omacron;uqi&egrave;</pr></trd>,
    <trd>&#20599;&#31434; <pr>t&omacron;uqi&egrave;</pr></trd>,
    <trd>&#31363;&#21462; <pr>qi&egrave;q&#468;</pr></trd>,
    <trd>&#31434;&#21462; <pr>qi&egrave;q&#468;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxtel.0e">
  <kap><tld/>e</kap>
  <snc mrk="sxtel.0e.JUR">
    <dif>
      Per <tld/>o:
      <ekz>
        <tld/>e proprigi ion al si;
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;-&c_z;&c_l;&c_a;&c_d;&c_z;&c_je;&c_j;&c_s;&c_k;&c_u;</trd>
    <trd lng="de">durch Diebstahl</trd>
    <trd lng="hu">lop&aacute;ssal</trd>
    <trd lng="nl">steels</trd>
    <trd lng="pl">z&lstroke;odziejsko</trd>
    <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_v;&c_s;&c_k;&c_i;</trd>
  </snc>
  <snc mrk="sxtel.0e.kasxe">
    <dif>
      Ka&scirc;e kaj sekrete, kvaza&ubreve; <tld/>isto:
      <ekz>
        &scirc;i <tld/>e forkondukis lin el la domo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e en&scirc;ovi&gcirc;i kaj embuski;
      </ekz>
      <ekz>
        <tld/>move eliri<fnt>Z</fnt>.
      </ekz>
      <trdgrp lng="ca">
        <trd>d'<ind>amagat</ind></trd>,
        <trd>d&#8217;<ind>amagatons</ind></trd>,
        <trd>d&#8217;<ind>amagatotis</ind></trd>,
        <trd>d&#8217;<ind>estranquis</ind></trd>,
        <trd>furtivament</trd>,
        <trd>clandestinament</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>ladronamente</trd>,
        <trd>a <ind>hurtadillas</ind></trd>
      </trdgrp>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_r;&c_a;&c_d;&c_k;&c_o;&c_m;</trd>,
      <trd>&c_k;&c_r;&c_a;&c_d;&c_u;&c_ch;&c_y;&c_s;&c_ja;</trd>,
      <trd>&c_u;&c_p;&c_o;&c_t;&c_a;&c_j;</trd>,
      <trd>&c_u;&c_p;&c_o;&c_t;&c_a;&c_j;&c_k;&c_ib;</trd>,
      <trd>&c_c;&c_ib;&c_sh;&c_k;&c_o;&c_m;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>unbemerkt</trd>,
      <trd>heimlich</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>furtivement</trd>,
      <trd>&agrave; la <ind>d&eacute;rob&eacute;e</ind></trd>
    </trdgrp>
    <trd lng="hu">lopva</trd>
    <trdgrp lng="id">
      <trd><ind>curi</ind>-curi</trd>,
      <trd><ind>diam</ind>-diam</trd>,
      <trd><ind>sembunyi</ind>-sembunyi</trd>
    </trdgrp>
    <trd lng="it">furtivamente</trd>
    <trd lng="nl">heimelijk</trd>
    <trdgrp lng="pl">
      <trd>niepostrze&zdot;enie</trd>,
      <trd>ukradkiem</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_a;&c_d;&c_u;&c_ch;&c_i;&c_s;&c_mol;</trd>,
      <trd>&c_u;&c_k;&c_r;&c_a;&c_d;&c_k;&c_o;&c_j;</trd>
    </trdgrp>
    <trd lng="sv">i <ind>smyg</ind></trd>
  </snc>
  <trdgrp lng="en">
    <trd>furtively</trd>,
    <trd>stealthily</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#30423;&#12435;&#12391;  <pr>&#12396;&#12377;&#12435;&#12391;</pr></trd>,
    <trd>&#12371;&#12387;&#12381;&#12426;&#12392;</trd>,
    <trd>&#12402;&#12381;&#12363;&#12395;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;-&c_z;&c_l;&c_o;&c_d;&c_ib;&c_j;&c_s;&c_mol;&c_k;&c_i;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_d;&c_mol;&c_k;&c_o;&c_m;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_a;&c_j;&c_k;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sxtel.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="sxtel.0ajxo.JUR">
    <dif>
      Io <tld/>ita:
      <ekz>
        <ind><tld/>a&jcirc;oka&scirc;isto</ind> estas mem
        <tld/>isto<fnt>Z</fnt>.
        <trd lng="fr">receleur</trd>
        <trd lng="hu">orgazda</trd>
        <trdgrp lng="id">
          <trd><baz>tadah</baz>penadah</trd>,
          <trd><ind>tukang</ind> tadah</trd>
        </trdgrp>
        <trd lng="nl">heler</trd>
