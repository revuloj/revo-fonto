    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_d;&c_z;&c_je;&c_n;&c_a;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>roberia</trd>,
    <trd>bot&iacute;</trd>
  </trdgrp>
  <trd lng="de">Diebesgut</trd>
  <trdgrp lng="en">
    <trd>loot</trd>,
    <trd>plunder</trd>,
    <trd>stolen goods</trd>
  </trdgrp>
  <trd lng="fr">butin</trd>
  <trd lng="hu">lopott holmi</trd>
  <trdgrp lng="id">
    <trd><baz>curi</baz>curian</trd>,
    <trd><baz>colong</baz>colongan</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>refurtiva</trd>,
    <trd>maltolto</trd>
