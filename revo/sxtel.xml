<drv mrk="sxtel.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="sxtel.0isto.JUR">
    <dif>
      Homo, kiu ser&ccirc;as vivrimedojn en <tld/>ado:
      <ekz>
        <tld/>etiston oni batas, <tld/>egiston oni &scirc;atas
        <fnt><bib>PrV</bib></fnt>.
        <trdgrp lng="zh">
          <trd>&#x7A83;&#x94A9;&#x8005;&#x8BDB; <pr>qi&egrave; g&omacron;u zh&ecaron; zh&umacron;</pr></trd>,
          <trd>&#x7A83;&#x56FD;&#x8005;&#x4FAF; <pr>qi&egrave;gu&oacute; zh&ecaron; h&oacute;u</pr></trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_l;&c_o;&c_d;&c_z;&c_je;&c_j;</trd>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_d;&c_je;&c_c;</trd>
  <trd lng="bo">&t_ra;&t_kata;&t_shabchu;&t_tsheg;&t_ma;&t_tsheg;</trd>
  <trd lng="ca">lladre</trd>
  <trd lng="de">Dieb</trd>
  <trd lng="en">thief</trd>
  <trdgrp lng="es">
     <trd>ladr&oacute;n</trd>,
     <trd>ratero</trd>,
     <trd>chorizo</trd>
  </trdgrp>
  <trd lng="fr">voleur</trd>
  <trd lng="hu">tolvaj</trd>
  <trdgrp lng="id">
    <trd><baz>curi</baz>pencuri</trd>,
    <trd>maling</trd>,
    <trd><baz>colong</baz>pencolong</trd>,
    <trd><ind>tukang</ind> curi</trd>,
    <trd><ind>tukang</ind> colong</trd>
