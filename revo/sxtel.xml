  <trd lng="it">ladro</trd>
  <trdgrp lng="ja">
    <trd>&#30423;&#20154;  <pr>&#12396;&#12377;&#12403;&#12392;</pr></trd>,
    <trd>&#27877;&#26834;  <pr>&#12393;&#12429;&#12412;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">dief</trd>
  <trdgrp lng="pl">
    <trd>z&lstroke;odziej</trd>,
    <trd>andrus</trd>,
    <trd>rzezimieszek</trd>,
    <trd>w&lstroke;amywacz</trd>
  </trdgrp>
  <trd lng="pt">ladr&atilde;o</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;</trd>
  <trd lng="sv">tjuv</trd>
  <trd lng="uk">&c_z;&c_l;&c_o;&c_d;&c_ib;&c_j;</trd>
  <trd lng="zh">&#23567;&#20599; <pr>xi&#462;ot&omacron;u</pr></trd>
</drv>

