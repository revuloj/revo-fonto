  <trd lng="ru">&c_k;&c_r;&c_a;&c_d;&c_je;&c_n;&c_o;&c_je;</trd>
  <trd lng="sv">st&ouml;ldgods</trd>
  <trd lng="uk">&c_k;&c_r;&c_a;&c_d;&c_je;&c_n;&c_je; (&c_u;
  &c_z;&c_n;&c_a;&c_ch;.&c_ib;&c_m;.)</trd>
  <trdgrp lng="zh">
    <trd>&#36115;&#29289; <pr>z&amacron;ngw&ugrave;</pr></trd>,
    <trd>&#36163;&#29289; <pr>z&amacron;ngw&ugrave;</pr></trd>,
    <trd>&#36115; <pr>z&amacron;ng</pr></trd>,
    <trd>&#36163; <pr>z&amacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxtel.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="sxtel.0isto.JUR">
    <dif>
      Homo, kiu ser&ccirc;as vivrimedojn en <tld/>ado:
      <ekz>
        <tld/>etiston oni batas, <tld/>egiston oni &scirc;atas
        <fnt><bib>PrV</bib></fnt>.
        <trdgrp lng="zh">
          <trd>&#31363;&#38057;&#32773;&#35803; <pr>qi&egrave; g&omacron;u zh&ecaron; zh&umacron;</pr></trd>,
          <trd>&#31363;&#22269;&#32773;&#20399; <pr>qi&egrave;gu&oacute; zh&ecaron; h&oacute;u</pr></trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_l;&c_o;&c_d;&c_z;&c_je;&c_j;</trd>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_d;&c_je;&c_c;</trd>
  <trd lng="bo">&t_ra;&t_kata;&t_shabchu;&t_tsheg;&t_ma;&t_tsheg;</trd>
  <trd lng="ca">lladre</trd>
  <trd lng="de">Dieb</trd>
  <trd lng="en">thief</trd>
  <trdgrp lng="es">
     <trd>ladr&oacute;n</trd>,
     <trd>ratero</trd>,
     <trd>chorizo</trd>
  </trdgrp>
  <trd lng="fr">voleur</trd>
  <trd lng="hu">tolvaj</trd>
  <trdgrp lng="id">
    <trd><baz>curi</baz>pencuri</trd>,
    <trd>maling</trd>,
    <trd><baz>colong</baz>pencolong</trd>,
    <trd><ind>tukang</ind> curi</trd>,
    <trd><ind>tukang</ind> colong</trd>
