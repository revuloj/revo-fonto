<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxtel.xml,v 1.40 2022/05/05 14:43:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;tel</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sxtel.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxtel.0i.JUR">
    <dif>
      Ka&scirc;e forkapti kaj proprigi al si poseda&jcirc;on de aliulo:
      <ekz>
        gro&scirc;on <tld/>is, ho <tld/>isto, milojn <tld/>is,
        financisto
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        unu al alia <tld/>is la butonojn de la vestoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <tld/>as de najbaro por doni al altaro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vi <tld/>is al mi mian filon el la brakoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi <tld/>is de mi karan horon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pren.0i">preni</ref>,
      <ref cel="rab.0i">rabi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_r;&c_a;&c_d;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="bo">&#xF62;&#xF90;&#xF74;&#xF0B;&#xF56;&#xF0B;</trd>
  <trd lng="de">stehlen</trd>
  <trd lng="en">steal</trd>
  <trdgrp lng="es">
     <trd>robar</trd>,
     <trd>hurtar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>voler <klr tip="ind">(d&eacute;rober)</klr></trd>,
    <trd>d&eacute;rober</trd>
  </trdgrp>
  <trd lng="hu">lop</trd>
  <trd lng="id">mencuri</trd>
  <trdgrp lng="it">
    <trd>rubare</trd>,
    <trd>sottrarre<klr> (rubare)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stelen</trd>,
    <trd>pikken</trd>,
    <trd>jatten</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>furtar</trd>,
    <trd>roubar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_a;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_i;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">stj&auml;la</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x440;&#x430;&#x441;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43A;&#x440;&#x430;&#x434;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5077; [<ind>t&#x14D;u</ind>]</trd>
    <trd>&#x7A83; [<ind>qi&egrave;</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="sxtel.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxtel.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Ago <tld/>i:
      <ekz>
        faras rabon kaj <tld/>on, por oferi al Dio kandelon
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_d;&c_z;&c_jo;&c_zh;</trd>,
    <trd>&c_z;&c_l;&c_a;&c_d;&c_z;&c_je;&c_j;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Diebstahl</trd>
  <trd lng="en">theft</trd>
  <trd lng="es">robo</trd>
  <trd lng="fr">vol <klr tip="ind">(du bien d'autrui)</klr></trd>
  <trd lng="hu">lop&aacute;s</trd>
  <trd lng="id">[<ind>curi</ind>] pencurian</trd>
  <trdgrp lng="it">
    <trd>furto</trd>,
    <trd>sottrazione<klr> (furto)</klr></trd>
  </trdgrp>
  <trd lng="nl">diefstal</trd>
  <trdgrp lng="pt">
    <trd>furto</trd>,
    <trd>roubo</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_zh;&c_a;</trd>
  <trd lng="sv">st&ouml;ld</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x440;&#x430;&#x434;&#x456;&#x436;&#x43A;&#x430;</trd>,
    <trd>&#x437;&#x43B;&#x43E;&#x434;&#x456;&#x439;&#x441;&#x442;&#x432;&#x43E;</trd>,
    <trd>&#x432;&#x438;&#x43A;&#x440;&#x430;&#x434;&#x435;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="sxtel.0e">
  <kap><tld/>e</kap>
  <snc mrk="sxtel.0e.JUR">
    <dif>
      Per <tld/>o:
      <ekz>
        <tld/>e proprigi ion al si;
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;-&c_z;&c_l;&c_a;&c_d;&c_z;&c_je;&c_j;&c_s;&c_k;&c_u;</trd>
    <trd lng="de">durch Diebstahl</trd>
    <trd lng="hu">lop&aacute;ssal</trd>
    <trd lng="nl">steels</trd>
    <trd lng="ru">&c_v;&c_o;&c_r;&c_o;&c_v;&c_s;&c_k;&c_i;</trd>
  </snc>
  <snc mrk="sxtel.0e.kasxe">
    <dif>
      Ka&scirc;e kaj sekrete, kvaza&ubreve; <tld/>isto:
      <ekz>
        &scirc;i <tld/>e forkondukis lin el la domo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e en&scirc;ovi&gcirc;i kaj embuski;
      </ekz>
      <ekz>
        <tld/>move eliri<fnt>Z</fnt>.
      </ekz>
      <trdgrp lng="es">
        <trd>ladronamente</trd>,
        <trd>a <ind>hurtadillas</ind></trd>
      </trdgrp>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_r;&c_a;&c_d;&c_k;&c_o;&c_m;</trd>,
      <trd>&c_k;&c_r;&c_a;&c_d;&c_u;&c_ch;&c_y;&c_s;&c_ja;</trd>,
      <trd>&c_u;&c_p;&c_o;&c_t;&c_a;&c_j;</trd>,
      <trd>&c_u;&c_p;&c_o;&c_t;&c_a;&c_j;&c_k;&c_ib;</trd>,
      <trd>&c_c;&c_ib;&c_sh;&c_k;&c_o;&c_m;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>unbemerkt</trd>,
      <trd>heimlich</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>furtivement</trd>,
      <trd>&agrave; la <ind>d&eacute;rob&eacute;e</ind></trd>
    </trdgrp>
    <trd lng="hu">lopva</trd>
    <trd lng="it">furtivamente</trd>
    <trd lng="nl">heimelijk</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_a;&c_d;&c_u;&c_ch;&c_i;&c_s;&c_mol;</trd>,
      <trd>&c_u;&c_k;&c_r;&c_a;&c_d;&c_k;&c_o;&c_j;</trd>
    </trdgrp>
    <trd lng="sv">i <ind>smyg</ind></trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;-&#x437;&#x43B;&#x43E;&#x434;&#x456;&#x439;&#x441;&#x44C;&#x43A;&#x438;</trd>,
    <trd>&#x43A;&#x440;&#x430;&#x434;&#x44C;&#x43A;&#x43E;&#x43C;&#x430;</trd>,
    <trd>&#x43F;&#x43E;&#x442;&#x430;&#x439;&#x43A;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="sxtel.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="sxtel.0ajxo.JUR">
    <dif>
      Io <tld/>ita:
      <ekz>
        <ind><tld/>a&jcirc;oka&scirc;isto</ind> estas mem
        <tld/>isto<fnt>Z</fnt>.
        <trd lng="fr">receleur</trd>
        <trd lng="hu">orgazda</trd>
        <trd lng="nl">heler</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_d;&c_z;&c_je;&c_n;&c_a;&c_je;</trd>
  <trd lng="de">Diebesgut</trd>
  <trdgrp lng="en">
    <trd>loot</trd>,
    <trd>plunder</trd>,
    <trd>stolen goods</trd>
  </trdgrp>
  <trd lng="fr">butin</trd>
  <trd lng="hu">lopott holmi</trd>
  <trd lng="id"><ind>curi</ind>an</trd>
  <trdgrp lng="it">
    <trd>refurtiva</trd>,
    <trd>maltolto</trd>
  </trdgrp>
  <trd lng="nl">buit</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_d;&c_je;&c_n;&c_o;&c_je;</trd>
  <trd lng="sv">st&ouml;ldgods</trd>
  <trd lng="uk">&#x43A;&#x440;&#x430;&#x434;&#x435;&#x43D;&#x435; (&#x443; &#x437;&#x43D;&#x430;&#x447;.&#x456;&#x43C;.)</trd>
</drv>

<drv mrk="sxtel.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="sxtel.0isto.JUR">
    <dif>
      Homo, kiu ser&ccirc;as vivrimedojn en <tld/>ado:
      <ekz>
        <tld/>etiston oni batas, <tld/>egiston oni &scirc;atas
        <fnt><bib>PrV</bib></fnt>.
        <trdgrp lng="zh">
          <trd>&#x7A83;&#x94A9;&#x8005;&#x8BDB; [<ind>qi&egrave; g&#x14D;u zh&ecaron; zh&#x16B;</ind>]</trd>,
          <trd>&#x7A83;&#x56FD;&#x8005;&#x4FAF; [<ind>qi&egrave;gu&oacute; zh&ecaron; h&oacute;u</ind>]</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_l;&c_o;&c_d;&c_z;&c_je;&c_j;</trd>
  <trd lng="bg">&c_k;&c_r;&c_a;&c_d;&c_je;&c_c;</trd>
  <trd lng="bo">&#xF62;&#xF90;&#xF74;&#xF0B;&#xF58;&#xF0B;</trd>
  <trd lng="de">Dieb</trd>
  <trd lng="en">thief</trd>
  <trdgrp lng="es">
     <trd>ladr&oacute;n</trd>,
     <trd>ratero</trd>,
     <trd>chorizo</trd>
  </trdgrp>
  <trd lng="fr">voleur</trd>
  <trd lng="hu">tolvaj</trd>
  <trdgrp lng="id">
    <trd>[<ind>curi</ind>] pencuri</trd>,
    <trd>maling</trd>
  </trdgrp>
  <trd lng="it">ladro</trd>
  <trd lng="nl">dief</trd>
  <trd lng="pt">ladr&atilde;o</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;</trd>
  <trd lng="sv">tjuv</trd>
  <trd lng="uk">&#x437;&#x43B;&#x43E;&#x434;&#x456;&#x439;</trd>
  <trd lng="zh">&#x5C0F;&#x5077; [<ind>xi&#x1CE;ot&#x14D;u</ind>]</trd>
</drv>

<drv mrk="sxtel.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sxtel.0umi.sxteliri">
    <ref tip="dif" cel="ir.sxtel0i.kasxe"><tld/>iri</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_k;&c_r;&c_a;&c_d;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="hu">lopakodik</trd>
  <trdgrp lng="it">
    <trd>sgattaiolare</trd>, 
    <trd>muoversi<klr> furtivamente</klr></trd>
  </trdgrp>
  <trd lng="nl">sluipen</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_k;&c_r;&c_a;&c_s;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_k;&c_r;&c_a;&c_d;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;
  </trd>
  </trdgrp>
  <trd lng="sv">smyga</trd>
</drv>

<drv mrk="sxtel.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <snc mrk="sxtel.el0igxi.KOMUNE">
    <dif>
      Nerimarkite eliri:
      <ekz>
        senbrue li el<tld/>i&gcirc;is el la salono.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">sich hinausstehlen</trd>
  <trdgrp lng="es">
    <trd>irse a <ind>hurtadillas</ind></trd>,
    <trd>irse sigilosamente</trd>
  </trdgrp>
  <trd lng="hu">kilopakodik</trd>
  <trd lng="nl">wegsluipen</trd>
  <trd lng="fr">s'<ind>&eacute;clipser</ind> <klr tip="ind">(partir)</klr>
  </trd>
  <trdgrp lng="it">
    <trd>sgattaiolare<klr> fuori</klr></trd>, 
    <trd>uscire<klr> furtivamente</klr></trd>,
    <trd>squagliarsi<klr> (uscire senza essere visto)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sxtel.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <snc mrk="sxtel.en0igxi.KOMUNE">
    <dif>
      Nerimarkite en&scirc;ovi&gcirc;i:
      <ekz>
        perfidulo en<tld/>i&gcirc;is en niajn vicojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la ekzemplon de kalkulado en<tld/>i&gcirc;is mal&gcirc;usta
        nombro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kun printempo en<tld/>i&gcirc;as nova espero en la korojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_c;&c_je;&c_r;&c_c;&c_ib;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">sich <ind>einschleichen</ind></trd>
  <trd lng="es">colarse</trd>
  <trd lng="fr">se <ind>glisser</ind> 
<klr tip="ind">(furtivement)</klr></trd>
  <trd lng="hu">belopakodik</trd>
  <trdgrp lng="it">
    <trd>infiltrarsi</trd>, 
    <trd>entrare<klr> furtivamente</klr></trd>, 
    <trd>sgattaiolare<klr> dentro</klr></trd>,
    <trd>penetrare<klr> (entrare furtivamente)</klr></trd>
    <trd>intrufolarsi</trd>
  </trdgrp>
  <trd lng="nl">binnensluipen</trd>
  <trd lng="ru">&c_v;&c_k;&c_r;&c_a;&c_s;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="sxtel.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxtel.pri0i.JUR">
    <dif>
      Senigi iun a&ubreve; ion de io per <tld/>o:
      <ekz>
        oni pri<tld/>as ne ri&ccirc;ulon, sed sengardulon
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        strataj buboj pri<tld/>is la budon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_r;&c_a;&c_b;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">bestehlen</trd>
  <trd lng="en">steal <klr>from someone</klr></trd>
  <trdgrp lng="fr">
    <trd>d&eacute;valiser</trd>,
    <trd>d&eacute;pouiller <klr tip="ind">(d&eacute;valiser)</klr></trd>
  </trdgrp>
  <trd lng="hu">meglop</trd>
  <trdgrp lng="it">
    <trd>derubare</trd>,
    <trd>svaligiare</trd>
  </trdgrp>
  <trd lng="nl">bestelen</trd>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_k;&c_r;&c_a;&c_s;&c_t;&c_mol;</trd>
  <trd lng="uk">&#x43E;&#x431;&#x43A;&#x440;&#x430;&#x434;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="sxtel.dom0isto">
  <kap>dom<tld/>isto</kap>
  <snc mrk="sxtel.dom0isto.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="super" cel="sxtel.0isto.JUR"><tld lit="&Scirc;"/>isto</ref>
      kiu rabas en domoj:
      <ekz>
        dom<tld/>istoj <klr>...</klr> fari&gcirc;as pli kaj pli sentimaj
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://claudepiron.free.fr/livres/dankonamiko.htm">
          Dankon, amiko!</url></vrk>,
          <lok>1990</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">cambrioleur</trd>
</drv>

<drv mrk="sxtel.posxo0isto">
  <kap>po&scirc;o<tld/>isto</kap>
  <snc mrk="sxtel.posxo0isto.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="super" cel="sxtel.0isto.JUR"><tld lit="&Scirc;"/>isto</ref>
      kiu prenas el po&scirc;oj kaj sakoj en publikaj lokoj:
      <ekz>
        po&scirc;o<tld/>isto sukcese faris lerta&jcirc;on, kaj la 
        pri<tld/>ito senretene sputis koleregajn blasfemojn  
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="&EduInf;lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Taschendieb</trd>
  <trd lng="es">carterista</trd>
  <trd lng="fr">pickpocket</trd>
  <trd lng="uk">&#x43A;&#x438;&#x448;&#x435;&#x43D;&#x44C;&#x43A;&#x43E;&#x432;&#x438;&#x439; &#x437;&#x43B;&#x43E;&#x434;&#x456;&#x439;</trd>
</drv>
</art>
