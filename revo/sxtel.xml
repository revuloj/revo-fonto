<drv mrk="sxtel.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxtel.0i.JUR">
    <dif>
      Ka&scirc;e forkapti kaj proprigi al si poseda&jcirc;on de aliulo:
      <ekz>
        kie vi <tld/>is la belan hundon
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>?
      </ekz>
      <ekz>
        unu al alia <tld/>as la butonojn de la pantalono
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>is al mi mian filon el la brakoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la dancejo ili <tld/>as niajn virinojn per mensoga &gcirc;entileco
        <fnt>
          <bib>KrM</bib>
          <lok>31. Ricevis Bandito la&ubreve; sia Merito.</lok>
        </fnt>;
      </ekz>
      <ekz>
        se iu <tld/>as homon por vendi lin <klr>[&#x2026;]</klr>, li devas esti mortigita
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 21:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen Mi turnos Min kontra&ubreve; la profetojn, diras la Eternulo, kiuj <tld/>as Miajn
        vortojn
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 23:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>as en temploj kaj tomboj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>!
      </ekz>
      <ekz>
        soldatoj <klr>[&#x2026;]</klr> revenante en sian patrujon en la komenco petis almozon,
        poste <tld/>is kaj fine komencis rabi
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        sciureto a&ubreve;dace venis <tld/>i nukson falintan el sako piknika
        <fnt>
          <bib>ChB</bib>
          <lok>16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi <tld/>is de mi karan horon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        jen vi <tld/>as <klr>(detruas)</klr> ilian estontecon anta&ubreve; iliaj propraj okuloj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiuj, kiuj eduki&gcirc;is dum la diktaturo <klr>[&#x2026;]</klr> sentas, ke oni <tld/>is
        ilian historion, kaj volas reakiri &gcirc;in
        <fnt>
          <bib>Kon11</bib>
          <lok>Historio</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj fiprogramoj <klr>[&#x2026;]</klr> <tld/>as pasvortojn
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>as de najbaro por doni al altaro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        gro&scirc;on <tld/>is, ho <tld/>isto, milojn <tld/>is,
        financisto
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
    <ref cel="kapt.for0i">forkapti</ref>
      <ref cel="pren.0i.perforte">preni<sncref/></ref>,
      <ref cel="rab.0i">rabi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ja;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_r;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_o;&c_t;&c_k;&c_r;&c_a;&c_d;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_kata;&t_shabchu;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>furtar</trd>,
    <trd>sostreure</trd>,
    <trd>sostraure</trd>,
    <trd>robar</trd>,
    <trd>pispar</trd>,
    <trd>fotre</trd>
  </trdgrp>
  <trd lng="de">stehlen</trd>
  <trd lng="en">steal</trd>
  <trdgrp lng="es">
     <trd>robar</trd>,
     <trd>hurtar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>voler <klr tip="ind">(d&eacute;rober)</klr></trd>,
    <trd>d&eacute;rober</trd>
  </trdgrp>
  <trd lng="hu">lop</trd>
  <trdgrp lng="id">
    <trd><baz>curi</baz>mencuri</trd>,
    <trd><baz>colong</baz>mencolong</trd>,
    <trd><baz>maling</baz>memaling</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>rubare</trd>,
    <trd>sottrarre<klr> (rubare)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x76D7;&#x3080;  <pr>&#x306C;&#x3059;&#x3080;</pr></trd>,
    <trd>&#x3053;&#x3063;&#x305D;&#x308A;&#x596A;&#x3046;  <pr>&#x3053;&#x3063;&#x305D;&#x308A;&#x3046;&#x3070;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stelen</trd>,
    <trd>pikken</trd>,
    <trd>jatten</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kra&sacute;&cacute;</trd>,
    <trd>buchn&aogonek;&cacute;<klr>  (ukra&sacute;&cacute; pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>furtar</trd>,
    <trd>roubar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_a;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_i;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_h;&c_i;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">stj&auml;la</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_r;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5077; <pr>t&omacron;u</pr></trd>
    <trd>&#x7A83; <pr>qi&egrave;</pr></trd>
  </trdgrp>
</drv>

