<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sturne.xml,v 1.13 2023/11/19 01:40:31 revo Exp $">
<kap>
  <rad>sturned</rad>/oj
</kap>
<drv mrk="sturne.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Sturnidae</trd>), kun
      fortaj piedoj kaj ofte metaleca brilo:
      <ekz>
        <tld/>oj havas diversajn kaj komplikajn vo&ccirc;ojn, kaj <klr>[&#8230;]</klr>
        enmetas sonojn el sia &ccirc;irka&ubreve;a&jcirc;o en siajn proprajn alvokojn, inklude
        hupojn, kaj homajn parolmanierojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18710">Sturnedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="sturn.0o">sturno</ref>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_a;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">est&uacute;rnids</trd>
  <trd lng="cs">&scaron;pa&ccaron;kovit&iacute;</trd>
  <trd lng="de">Stare<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">sturnid&eacute;s</trd>
  <trd lng="hu">sereg&eacute;lyf&eacute;l&eacute;k</trd>
  <trd lng="nl">spreeuwen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">szpakowate</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_v;&c_o;&c_r;&c_c;&c_y;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_s;&c_k;&c_v;&c_o;&c_r;&c_c;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">&scaron;korcovit&eacute;</trd>
  <trd lng="uk">&c_sh;&#x43F;&c_a;&#x43A;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>
