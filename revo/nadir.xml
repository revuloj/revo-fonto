<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nadir.xml,v 1.21 2023/09/02 12:44:19 revo Exp $">
<kap>
   <rad>nadir</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="nadir.0o">
   <kap><tld/>o</kap>
   <snc mrk="nadir.0o.AST">
     <uzo tip="fak">AST</uzo>
     <dif>
         Punkto de la &ccirc;iela sfero trovi&gcirc;anta sur la
         vertikalo de l' observanto
         kaj direkte malsupre sub liaj piedoj.
     </dif>
     <refgrp tip="ant">
       <ref cel="zenit.0o.AST">zenito</ref>
     </refgrp>
    <trd lng="be">&c_n;&c_a;&c_d;&c_z;&c_ib;&c_r;</trd>
    <trd lng="bg">&c_n;&c_a;&c_d;&c_i;&c_r;</trd>
    <trd lng="ca">nadir</trd>
    <trdgrp lng="cs">
      <trd>nadir</trd>,
      <trd>podno&zcaron;n&iacute;k</trd>
    </trdgrp>
    <trdgrp lng="de">
       <trd>Nadir</trd>,
       <trd>Fu&szlig;punkt</trd>,
       <trd>Nullpunkt</trd>
     </trdgrp>
    <trd lng="el">&ny;&alfa;&delta;&jota_ton;&rho;</trd>
    <trd lng="en">nadir</trd>
    <trdgrp lng="fa">
       <trd>&f_p;&a_A;&a_s;&a_w;</trd>,
       <trd>&a_s;&a_m;&a_t;&zwnj;&a_A;&a_l;&a_q;&a_d;&a_m;</trd>
     </trdgrp>
    <trd lng="fr">nadir</trd>
    <trdgrp lng="hu">
       <trd>nad&iacute;r</trd>,
       <trd>talppont</trd>
     </trdgrp>
    <trd lng="nl">nadir</trd>
    <trd lng="pl">nadir</trd>
    <trd lng="pt">nadir</trd>
    <trd lng="ru">&c_n;&c_a;&c_d;&c_i;&c_r;</trd>
    <trdgrp lng="sk">
      <trd>nadir</trd>,
      <trd>podno&zcaron;n&iacute;k</trd>
    </trdgrp>
  </snc>
  <snc mrk="nadir.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mal<ref tip="ant" cel="kulmin.0o.FIG">kulmino</ref>,
      plej malsukcesa, plej deprima stato de afero:
      <ekz>
        iam devos veni la <tld/>o de la milito kaj, post &gcirc;i, la vojo 
        kondukanta al paco
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_j;&c_n;&c_ib;&c_zh;&c_e;&c_j;&c_sh;&c_y; <ind>&c_p;&c_u;&c_n;&c_k;&c_t;</ind></trd>,
      <trd>&c_d;&c_n;&c_o; <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>
    </trdgrp>
    <trd lng="fr">point le <ind>plus bas</ind></trd>
  </snc>
  <trd lng="ja">&#x5929;&#x5E95;  [<ind>&#x3066;&#x3093;&#x3066;&#x3044;</ind>]</trd>
  <trd lng="uk">&#x43D;&c_a;&c_d;&c_i;&c_r;</trd>
</drv>
</art>
