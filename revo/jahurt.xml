<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: jahurt.xml,v 1.23 2020/09/09 06:10:37 revo Exp $">
<kap>
  <rad>jahurt</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="g">jogurt</rad>/o <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="jahurt.0o">
  <kap><tld/>o,
    <var><kap><tld var="g"/>o</kap></var>
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="lakt.0o.mambesto">Lakto</ref>
      acidigita per speciala fermento, ofte kun aldono de fruktoj,
      tre saniga por la intestoj:
      <ekz>
        laktejo fabrikas <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=304667">Sainte-Marie-la-Blanche</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        haveblas pluraj specoj de acidlakto, kefiro kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51484">Finnlanda kuirarto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        poroduktoj reklamataj televide, kiel <tld var="g"/>o kaj aliaj lakta&jcirc;oj ri&ccirc;igitaj je
        vegetalaj steroloj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/011124.php">Atentu la koron</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve;igu ilin per plantdevena lakto (kiel sojlakto), sojo<tld var="g"/>o a&ubreve; sojka&ccirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van De Velde</aut>
          <vrk><url ref="&Monato;/2013/010934.php">Sablohorlo&gcirc;o for&scirc;ovas piramidon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kefir.0o">kefiro</ref>
  </snc>
  <trd lng="be">&c_jo;&c_g;&c_u;&c_r;&c_t;</trd>
  <trd lng="bg">&c_k;&c_i;&c_s;&c_je;&c_l;&c_o; &c_m;&c_l;&c_ja;&c_k;&c_o;</trd>
  <trd lng="ca">iogurt</trd>
  <trd lng="cs">jogurt</trd>
  <trd lng="de">Joghurt</trd>
  <trd lng="el">
  &gamma;&jota;&alfa;&omikron;&ypsilon_ton;&rho;&tau;&jota;</trd>
  <trd lng="en">yogurt</trd>
  <trd lng="es">yogur</trd>
  <trdgrp lng="fr">
    <trd>yaourt</trd>,
    <trd>yoghourt</trd>,
    <trd>yogourt</trd>
  </trdgrp>
  <trd lng="hu">joghurt</trd>
  <trd lng="it">yoghurt</trd>
  <trd lng="nl">yoghurt</trd>
  <trd lng="pl">jogurt</trd>
  <trd lng="pt">iogurte</trd>
  <trd lng="ru">&c_j;&c_o;&c_g;&c_u;&c_r;&c_t;</trd>
  <trd lng="sk">jogurt</trd>
  <trd lng="sv">yoghurt</trd>
  <trd lng="tr">yo&gbreve;urt</trd>
</drv>
</art>
