<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>jahurt</rad>/o
</kap>
<drv mrk="jahurt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Lakto acidigita per speciala fermento, ofte kun aldono de fruktoj,
      tre saniga por la intestoj;
      <ref tip="sin" cel="jogurt.0o">jogurto</ref>.  
    </dif>
    <ref tip="vid" cel="kefir.0o">kefiro</ref>
  </snc>
  <trd lng="be">&c_jo;&c_g;&c_u;&c_r;&c_t;</trd>
  <trd lng="de">Joghurt</trd>
  <trd lng="nl">yoghurt</trd>
  <trd lng="ru">&c_j;&c_o;&c_g;&c_u;&c_r;&c_t;</trd>
  <trd lng="sv">yoghurt</trd>
</drv>
</art>
