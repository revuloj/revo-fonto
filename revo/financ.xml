<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>financ</rad>/o
</kap>
<uzo tip="fak">EKON</uzo>
<drv mrk="financ.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Monaj rimedoj, pri kiuj disponas iu &scirc;tato a&ubreve; privatulo;
      stato de la en- k elspezoj:
      <ekz>havi prosperajn <tld/>ojn;</ekz>
      <ekz>la gazeto falis pro manko de <tld/>oj<fnt>Z</fnt>;</ekz> 
      <ekz>ministro pri la <tld/>oj.</ekz>
    </dif>
    <trd lng="de">Finanzen</trd>
  </snc>
  <snc num="2">
    <dif>
      Fako de tiu, kiu sin okupas pri <tld/>aj aferoj;
      arto profitigi monon:
      <ekz><tld/>o dependas de industrio k reciproke.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="budgxe.0o">bud&gcirc;eto</ref>,
      <ref cel="bilanc.0o">bilanco</ref>,
      <ref cel="kapita.0o">kapitalo</ref>,
      <ref cel="ekonom1.0o">ekonomio</ref>.
    </refgrp>
    <trd lng="de">Finanzwesen</trd>
  </snc>
  <trd lng="ru">&c_f;&c_i;&c_n;&c_a;&c_n;&c_s;&c_y;</trd>
</drv>
<drv mrk="financ.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la <tld/>aj malfacila&jcirc;oj de la a&ubreve;toro prokrastis la eldonon.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">finanziell</trd>
  <trd lng="ru">&c_f;&c_i;&c_n;&c_a;&c_n;&c_s;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>
<drv mrk="financ.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      <tld/>e subteni:
      <ekz><tld/>i entreprenon, al entrepreno, propagandon, esploron.</ekz>
    </dif>
  </snc>
  <trd lng="de">finanzieren</trd>
  <trd lng="ru">&c_f;&c_i;&c_n;&c_a;&c_n;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="financ.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Specialisto pri <tld/>aj aferoj, kondukanto de gravaj <tld/>aj aferoj&c_ju;
    </dif>
    <refgrp tip="sin">
      <ref cel="kapita.0isto">kapitalisto</ref>,
      <ref cel="bank.0isto">bankisto</ref>,
      <ref cel="bankie.0o">bankiero</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="sxtel.0i">&scirc;teli</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Finanzier</trd>,
    <trd>Kapitalgeber</trd></trdgrp>
  <trd lng="ru">&c_f;&c_i;&c_n;&c_a;&c_n;&c_s;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
