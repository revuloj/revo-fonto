<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jakv.xml,v 1.9 2023/11/11 09:24:19 revo Exp $">
<kap>
  <rad>jakv</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="jakv.0o">
  <kap><tld/>o</kap>
  <snc mrk="jakv.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref> de
      <tld/>arbo, granda, kun malglata, helverda &scirc;elo, forta odoro,
      mildagusta karno:
      <ekz>
        durioj kaj <tld/>oj staras en plenplenaj korboj el plekta&jcirc;o
        <fnt>
          <aut>Riskemulo</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/mondovojagxanto/420194">
          Prapradaeng - La verda pulmo de Bankoko</url></vrk>,
          <lok>2012-10-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_o;&c_d; <ind>&c_h;&c_l;&c_je;&c_b;&c_n;&c_a;&c_g;&c_a;</ind> &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>jakva</trd>,
    <trd>plod chlebovn&#xED;ku</trd>
  </trdgrp>
  <trd lng="en">jackfruit</trd>
  <trdgrp lng="fr">
    <trd>jacque<klr> (fruit)</klr></trd>,
    <trd>jaque<klr> (fruit)</klr></trd>
  </trdgrp>
  <trd lng="pl">d&zdot;akfrut</trd>
  <trd lng="pt">jaca<klr> (fruta)</klr></trd>
  <trd lng="sk">plod chlebovn&#xED;ka</trd>
  <trd lng="uk">&#x43F;&#x43B;&c_ib;&c_d; &c_h;&#x43B;&c_ib;&c_b;&#x43D;&#x43E;&c_g;&#x43E; &c_d;&c_je;&c_r;&c_je;&c_v;&c_a;</trd>
</drv>

<drv mrk="jakv.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="jakv.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Hinddevena, tropika
      <ref tip="lst" cel="arb.0o" lst="voko:rozoparencoj">arbo</ref>
      kun densa bran&ccirc;aro,
      kultivata pro la fruktoj, rekte kreskantaj &ccirc;e la trunko, kaj pro
      la ligno;
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">specio</ref>
       el familio <ref tip="malprt" cel="morusa.0oj.BOT">morusacoj</ref>
      (<trd lng="la">Artocarpus heterophyllus</trd>).
    </dif>
  </snc>
  <trd lng="be"><ind>&c_h;&c_l;&c_je;&c_b;&c_n;&c_a;&c_je;</ind> &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trd lng="de">Jackfruchtbaum</trd>
  <trdgrp lng="fr">
    <trd>jacquier</trd>,
    <trd>jaquier</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>chlebowiec r&oacute;&zdot;nolistny</trd>,
    <trd>drzewo bochenkowe</trd>
</trdgrp>
<trd lng="pt">jaqueira</trd>
  <trd lng="uk">&c_h;&#x43B;&c_ib;&c_b;&#x43D;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&#x43E; (&c_r;&c_ib;&c_z;&#x43D;&#x43E;&#x43B;&c_i;&c_s;&c_t;&c_je;)</trd>
  <trdgrp lng="zh">
    <trd>&#x83E0;&#x841D;&#x871C; <pr>b&#x14D;lu&#xF3;m&#xEC;</pr></trd>,
    <trd>&#x83E0;&#x863F;&#x871C; <pr>b&#x14D;lu&#xF3;m&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>

