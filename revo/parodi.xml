<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parodi.xml,v 1.20 2023/09/03 12:45:04 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>parodi</rad>/o
</kap>
<drv mrk="parodi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Ridiga imito de serioza verko:
      <ekz>
        en amba&ubreve; rakontojn, Carroll inkludas <tld/>ojn de popularaj
        kantoj
        por infanoj kaj de moraligaj poemoj<fnt><aut>Lewis
        Carroll</aut>:
        <vrk>Trans la spegulo kaj kion Alico trovis tie</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 1a, p. 25a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lauxd.mal0i">malla&ubreve;di</ref>,
      <ref cel="satir.0o">satiro</ref>,
      <ref cel="mok.0i">moki</ref>,
      <ref cel="parodi.0o">parodio</ref>,
      <ref cel="paskvi.0o">paskvilo</ref>,
      <ref cel="vip.0i.mallauxdi">vipi</ref>,
      <ref cel="pastic.0o">pasti&ccirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_o;&c_d;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_p;&c_a;&c_r;&c_o;&c_d;&c_i;&c_ja;</trd>
  <trd lng="cs">parodie</trd>
  <trd lng="de">Parodie</trd>
  <trdgrp lng="en">
    <trd>travesty</trd>,
    <trd>parody</trd>,
    <trd>burlesque</trd>
  </trdgrp>
  <trd lng="es">parodia</trd>
  <trdgrp lng="fr">
    <trd>parodie</trd>,
    <trd>pastiche</trd>
  </trdgrp>
  <trd lng="hu">par&oacute;dia</trd>
  <trdgrp lng="ja">
    <trd>&#x30D1;&#x30ED;&#x30C7;&#x30A3;&#x30FC;</trd>,
    <trd>&#x3082;&#x3058;&#x308A;&#x8A69;&#x6587;  [<ind>&#x3082;&#x3058;&#x308A;&#x3057;&#x3076;&#x3093;</ind>]</trd>,
    <trd>&#x3053;&#x3063;&#x3051;&#x3044;&#x306A;&#x6A21;&#x5023;  [<ind>&#x3053;&#x3063;&#x3051;&#x3044;&#x306A;&#x3082;&#x307B;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">parodie</trd>
  <trd lng="pl">parodia</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_d;&c_i;&c_ja;</trd>
  <trd lng="sk">par&#xF3;dia</trd>
  <trd lng="uk">&#x43F;&#x430;&#x440;&#x43E;&#x434;&#x456;&#x44F;</trd>
</drv>

<drv mrk="parodi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Verki, spektigi mokimiton de serioza verko, a&ubreve;toro a&ubreve; stilo.
    </dif>
    <refgrp tip="sin">
      <ref cel="traves.0i">travestii</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_d;&c_y;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>parodizovat</trd>,
    <trd>parodovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>travestieren</trd>,
    <trd>parodieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>travesty</trd>,
    <trd>parody</trd>,
    <trd>burlesque</trd>
  </trdgrp>
  <trd lng="es">parodiar</trd>
  <trdgrp lng="fr">
    <trd>parodier</trd>,
    <trd>pasticher</trd>
  </trdgrp>
  <trd lng="hu">parodiz&aacute;l</trd>
  <trdgrp lng="ja">
    <trd>&#x30D1;&#x30ED;&#x30C7;&#x30A3;&#x30FC;&#x5316;&#x3059;&#x308B;  [<ind>&#x30D1;&#x30ED;&#x30C7;&#x30A3;&#x30FC;&#x304B;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x3082;&#x3058;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>travesteren</trd>,
    <trd>parodi&euml;ren</trd>,
    <trd>verkleden</trd>
  </trdgrp>
  <trd lng="pl">parodiowa&cacute;</trd>
  <trd lng="pt">parodiar</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>parodova&#x165;</trd>,
    <trd>zosmie&#x161;&#x148;ova&#x165;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x430;&#x440;&#x43E;&#x434;&#x456;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>
</art>
