<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>parodi</rad>/o
</kap>
<drv mrk="parodi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ridiga imito de serioza verko.
    </dif>
  </snc>
  <trd lng="de">Parodie</trd>
  <trd lng="nl">parodie</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_d;&c_i;&c_ja;</trd>
</drv>

<drv mrk="parodi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ridige imiti, travestii.
    </dif>
  </snc>
  <trd lng="de">parodieren</trd>
  <trd lng="nl">parodi&euml;ren</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
