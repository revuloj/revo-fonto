<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>parodi</rad>/o
</kap>
<drv mrk="parodi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Ridiga imito de serioza verko:
      <ekz>
        en amba&ubreve; rakontojn, Carroll inkludas <tld/>ojn de popularaj kantoj
        por infanoj kaj de moraligaj poemoj<fnt><aut>Lewis Carroll</aut>:
        <vrk>Trans la spegulo kaj kion Alico trovis tie</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 1a, p. 25a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pastic.0o">pasti&ccirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="de">Parodie</trd>
  <trd lng="es">parodia</trd>
  <trdgrp lng="fr">
    <trd>parodie</trd>,
    <trd>pastiche</trd>
  </trdgrp>
  <trd lng="nl">parodie</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_d;&c_i;&c_ja;</trd>
</drv>

<drv mrk="parodi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Verki, spektigi mokimiton de serioza verko, a&ubreve;toro a&ubreve; stilo.
    </dif>
    <refgrp tip="sin">
      <ref cel="traves.0i">travestii</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>travestieren</trd>,
    <trd>parodieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>travesty</trd>,
    <trd>parody</trd>,
    <trd>burlesque</trd>
  </trdgrp>
  <trd lng="es">parodiar</trd>
  <trdgrp lng="fr">
    <trd>parodier</trd>,
    <trd>pasticher</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>travesteren</trd>,
    <trd>parodi&euml;ren</trd>,
    <trd>verkleden</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_o;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
