<art mrk="$Id: bimeta.xml,v 1.17 2023/10/14 08:39:48 revo Exp $">
<kap>
<rad>bimetalism</rad>/o
</kap>
<drv mrk="bimeta.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sistem.mon0o">Mona sistemo</ref>
      bazita sur duobla normo kaj en kiu la valora rilato inter
      la <ref cel="or.0o">oro</ref>
      kaj la <ref cel="argxen.0o">ar&gcirc;ento</ref>
      estas precize fiksita:
      <ekz>
        <nom>William Jennings Bryan</nom> <klr>[&#x2026;]</klr> volis &ccirc;esigi al nuran orbazon kaj volis
        <tld/>on (oron kaj ar&gcirc;enton)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=121555">William Jennings Bryan</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_m;&c_e;&c_t;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="cs">bimetalizmus</trd>
  <trdgrp lng="de">
   <trd>Doppelw&auml;hrung</trd>,
   <trd>Bimetallismus</trd></trdgrp>
  <trdgrp lng="en">
   <trd>double standard</trd>,
   <trd>bimetallism</trd></trdgrp>
  <trd lng="es">bimetalismo</trd>
  <trd lng="fr">bim&eacute;tallisme</trd>
  <trd lng="he">&#x5D1;&#x5D9;&#x5DE;&#x5D8;&#x5D0;&#x5DC;&#x5D9;&#x5D6;&#x5DD;</trd>
  <trdgrp lng="hu">
   <trd>k&eacute;tf&eacute;mrendszer</trd>,
   <trd>kett&odblac;s <ind>valutarendszer</ind></trd>,
   <trd>bimetallizmus</trd></trdgrp>
  <trd lng="ja">&#x8907;&#x672C;&#x4F4D;&#x5236;  <pr>&#x3075;&#x304F;&#x307B;&#x3093;&#x3044;&#x305B;&#x3044;</pr></trd>
  <trd lng="nl">bimetallisme</trd>
  <trd lng="ru">&c_b;&c_i;&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">bimetalizmus</trd>
  <trd lng="uk">&#x431;&#x456;&#x43C;&#x435;&#x442;&#x430;&#x43B;&#x456;&#x437;&#x43C;</trd>
</drv>
</art>
