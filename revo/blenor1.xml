<vortaro>
<art mrk="$Id: blenor1.xml,v 1.22 2024/08/04 09:26:21 revo Exp $">
<kap>
  <rad>blenore</rad>/o <fnt><bib>V</bib></fnt>
</kap>
<drv mrk="blenor1.0o">
  <kap><tld/>o</kap>
  <snc mrk="blenor1.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kronika <ref tip="super" cel="blenor.0o">blenoragio</ref>,
      karakterizata per inflamo kun
      pusa elfluo de <ref cel="mukoz.0o">mukozo</ref>:
      <ekz>
        reteni&gcirc;o de larmoj en sekvo de <tld/>o de la larma sako a&ubreve; de nesufi&ccirc;a
        trempi&gcirc;ado de la larmaj punktoj en la larman lagon estas ofta ka&ubreve;zo de la
        kataro, tial oni neniam devas forgesi &ccirc;e unuflanka kataro esplori, &ccirc;u ne
        ekzistas ia sufero de la larmaj vojoj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="gonore.0o">gonoreo</ref>
    <trd lng="de">Blennorrhoe</trd>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_je;&c_n;&c_a;&c_r;&c_e;&c_ja;</trd>
  <trd lng="ca">blennorrea</trd>
  <trd lng="cs">kapavka</trd>
  <trd lng="es">blenorrea</trd>
  <trd lng="fa">&a_s;&a_w;&a_z;&a_A;&f_k;</trd>
  <trd lng="fr">blennorrh&eacute;e</trd>
  <trd lng="he">&dalet;&lamed;&qof;&tav; &he;&shin;&lamed;&pe;&vav;&het;&yod;&tav;</trd>
  <trdgrp lng="hu">
    <trd>gennycsorg&aacute;s</trd>,
    <trd>blennorrhea</trd>
  </trdgrp>
  <trd lng="id">blenorea</trd>
  <trdgrp lng="ja">
    <trd>&#33215;&#28431;  <pr>&#12358;&#12415;&#12418;</pr></trd>,
    <trd>&#24930;&#24615;&#28107;&#30142;  <pr>&#12414;&#12435;&#12379;&#12356;&#12426;&#12435;&#12375;&#12388;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rze&zdot;&aogonek;czka</trd>,
    <trd>tryper<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_l;&c_je;&c_n;&c_n;&c_o;&c_r;&c_je;&c_ja;</trd>
  <trd lng="sk">kvapavka</trd>
  <trd lng="uk">&c_b;&c_l;&c_je;&c_n;&c_o;&c_r;&c_je;&c_ja;</trd>
</drv>
</art>
