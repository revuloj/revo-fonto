<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>provinc</rad>/o
</kap>
<drv mrk="provin.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Parto de regno, administrata de speciala reprezentanto de la centra
      registaro:
      <ekz>
        <tld/>estro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="distri1.0o">distrikto</ref>,
      <ref cel="depart.0o">departemento</ref>,
      <ref cel="gubern1.0o">gubernio</ref>.
    </refgrp>
    <trd lng="nl">provincie</trd>
    <trd lng="ru">&c_o;&c_b;&c_l;&c_a;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Cetero de la lando, kontraste al la &ccirc;efurbo:
      <ekz>
        naiva <ind><tld/>ano</ind><fnt>Z</fnt>;
        <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_i;&c_n;&c_c;&c_i;&c_a;&c_l;</trd>
      </ekz>
      <ekz>
        izolitaj esperantistoj el la <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Provinz</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_i;&c_n;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="provin.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a oficisto, urbo, gazeto, grupo.
      </ekz>
    </dif>
    <trd lng="ru">&c_o;&c_b;&c_l;&c_a;&c_s;&c_t;&c_n;&c_o;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Nemoderna, neeleganta:
      <ekz>
        <tld/>a modo, tono;
      </ekz>
      <ekz>
        <tld/>aj manieroj.
      </ekz>
    </dif>
    <trd lng="de">provinziell</trd>
    <trd lng="nl">provinciaal</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_i;&c_n;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>
</art>
