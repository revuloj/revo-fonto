<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mastod.xml,v 1.20 2025/02/24 22:26:56 revo Exp $">
<kap>
  <rad>mastodont</rad>/o <fnt><bib>PVEV</bib></fnt>,
  <var><kap><rad var="n">mastodon</rad>/o <fnt><bib>JED</bib></fnt></kap></var>
</kap>
<drv mrk="mastod.0o">
  <kap><tld/>o, <var><kap><tld var="n"/>o</kap></var></kap>
  <snc mrk="mastod.0o.ZOO">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_familioj" cel="famili.0o.BIO">Familio</ref> de grandaj
      dikha&ubreve;taj fosiliaj <ref tip="malprt" cel="rostr.0uloj">rostruloj</ref>
      (genro <trd lng="la">Mammut</trd>, sin. <trd lng="la">Mastodon</trd> k.a.),
      pli granda ol elefanto, kun karakteriza dentaro, vivinta en mioceno kaj
      plioceno:
      <ekz>
        oni renkontas <tld/>ojn ankora&ubreve; en plejstoceno kune kun homaj
        resta&jcirc;oj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;i tiu <tld/>o en jupo konfuzas min, ke mi ru&gcirc;i&gcirc;as
        <fnt>
          <bib>IKr</bib>
          <lok>Bol&ccirc;jo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dinote.0o">dinoterio</ref>,
      <ref cel="mamut.0o">mamuto</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/0/04/Mammut_americanum_Sergiodlarosa.jpg/640px-Mammut_americanum_Sergiodlarosa.jpg"
      prm="CC BY-SA 3.0"
      >grafika rekonstruo de <tld/>o (Mammut americanum)
        <fnt>
          <aut>Sergiodlarosa, propra verko</aut>
          <lok><url ref="&WCW;/File:Mammut_americanum_Sergiodlarosa.jpg"
              >https://commons.wikimedia.org/wiki/File:Mammut_americanum_Sergiodlarosa.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_d;&c_o;&c_n;&c_t;</trd>
  <trd lng="ca">mastodont</trd>
  <trd lng="cs">mastodont</trd>
  <trd lng="de">Mastodon</trd>
  <trd lng="en">mastodon</trd>
  <trd lng="es">mastodonte</trd>
  <trd lng="fr">mastodonte</trd>
  <trd lng="hu">masztodon</trd>
  <trd lng="ja">&#12510;&#12473;&#12488;&#12489;&#12531;</trd>
  <trd lng="nl">mastodont</trd>
  <trd lng="pl">mastodont</trd>
  <trd lng="pt">mastodonte</trd>
  <trd lng="ro">mastodont</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_t;&c_o;&c_d;&c_o;&c_n;&c_t;</trd>
  <trd lng="sk">mastodont</trd>
  <trd lng="uk">&c_m;&c_a;&c_s;&c_t;&c_o;&c_d;&c_o;&c_n;&c_t;</trd>
</drv>
</art>
