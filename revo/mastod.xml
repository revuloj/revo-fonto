<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mastodont</rad>/o
</kap>
<drv mrk="mastod.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MIN</uzo>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      F. de grandaj dikha&ubreve;taj fosiliaj rostruloj, pli granda ol 
elefanto,
      kun karakteriza dentaro, vivinta en mioceno kaj plioceno:
      <ekz>
        oni renkontas <tld/>ojn ankora&ubreve; en pleistoceno kune kun homaj
        resta&jcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_d;&c_o;&c_n;&c_t;</trd>
  <trd lng="de">Mastodon</trd>
  <trd lng="en">mastodon</trd>
  <trd lng="es">mastodonte</trd>
  <trd lng="hu">masztodon</trd>
  <trd lng="nl">mastodont</trd>
  <trd lng="pt">mastodonte</trd>
  <trd lng="ru">&c_m;&c_a;&c_s;&c_t;&c_o;&c_d;&c_o;&c_n;&c_t;</trd>
</drv>
</art>
