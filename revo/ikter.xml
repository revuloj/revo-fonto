<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ikter</rad>/o
</kap>
<drv mrk="ikter.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malsano karakterizata per flavi&gcirc;o de la ha&ubreve;to kaj de la okula konjunktivo:
      <ekz>
        simpla, infekta, katara, obstrukcia <tld/>o
        <klr>(= <ref tip="sin" cel="flav.0malsano">flava malsano</ref>)</klr>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="de">Gelbsucht</trd>
  <trd lng="en">jaundice</trd>
  <trd lng="nl">geelzucht</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_u;&c_h;&c_a;</trd>
</drv>
</art>
