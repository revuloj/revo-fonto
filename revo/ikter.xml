<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ikter</rad>/o
</kap>
<drv mrk="ikter.0o">
  <kap><tld/>o</kap>
  <snc mrk="ikter.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Malsano karakterizata per flavi&gcirc;o de la ha&ubreve;to kaj de la
okula
      konjunktivo pro <ref tip="vid" cel="gal.0o">galo</ref>:
      <ekz>
        simpla, infekta, katara, obstrukca <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="san.flavmal0o">flavmalsano</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_u;&c_h;&c_a;</trd>
  <trd lng="ca">icter&iacute;cia</trd>
  <trd lng="de">Gelbsucht</trd>
  <trd lng="en">jaundice</trd>
  <trd lng="es">ictericia</trd>
  <trdgrp lng="fr">
    <trd>ict&egrave;re</trd>,
    <trd>jaunisse</trd>
  </trdgrp>
  <trd lng="hu">s&aacute;rgas&aacute;g</trd>
  <trd lng="nl">geelzucht</trd>
  <trd lng="pl">&zdot;&oacute;&lstroke;taczka</trd>
  <trd lng="pt">icter&iacute;cia</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_u;&c_h;&c_a;</trd>
  <trd lng="sv">gulsot</trd>
</drv>
</art>
