    <trd>fare <ind>scoppiare</ind></trd>,
    <trd>fare <ind>detonare</ind></trd>,
    <trd>brillare<klr> (esplosivo)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7206;&#x767A;&#x3055;&#x305B;&#x308B;  <pr>&#x3070;&#x304F;&#x306F;&#x3064;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x7206;&#x7834;&#x3059;&#x308B;  <pr>&#x3070;&#x304F;&#x306F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">doen <ind>ontploffen</ind></trd>
  <trdgrp lng="pl">
    <trd>detonowa&cacute;</trd>,
