<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>eksplod</rad>/i
</kap>
<drv mrk="eksplo.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc num="1">
    <dif>
      Forte k brue skui aeron, pro subita plivolumeni&gcirc;o de gaso:
      <ekz>
        dinamito facile <tld/>as.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flam">ekflami</ref>,
      <ref cel="flagr.0i">flagri</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <dif>
      Brue diskrevi, pro granda interna premo de l' gasoj:
      <ekz>
        bombo, mino, vaporma&scirc;ino, torpedo <tld/>as.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="romp">disrompi&gcirc;i</ref>,
      <ref cel="salt.dis0i">dissalti</ref>,
      <ref cel="split">disspliti&gcirc;i</ref>.
    </refgrp>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Subite k brue eksterigi fortan senton a&ubreve; pasion, kiun oni ne povas
      plu deteni:
      <ekz>
        <tld/>i de furiozo, de kolero, pro &gcirc;ojo, per
        rido<fnt>B</fnt>, per larmoj;
      </ekz>
      <ekz>
        li <tld/>is, dirante, ke...<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_v;&c_s;&c_p;&c_y;&c_l;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc num="4">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Subite k brue komenci&gcirc;i:
      <ekz>
        balda&ubreve; la ventego devis <tld/>i, rompi la glacion, kaj 
        &ccirc;iuj tie devis nesaveble droni
        <fnt><bib>Fab3</bib>, <lok>Io</lok></fnt>;
      </ekz>
      <ekz>
        la milito <tld/>is neatendite;
      </ekz>
      <ekz>
        <tld/>as &gcirc;ojo<fnt>Z</fnt>, striko, ribelo, katastrofo, krio.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_v;&c_s;&c_p;&c_y;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
    <trd>explodieren</trd>,
    <trd>ausbrechen</trd>
  </trdgrp>
  <trd lng="en">explode</trd>
  <trdgrp lng="fr">
    <trd>&eacute;clater</trd>,
    <trd>exploser</trd>,
    <trd>sauter<klr tip="ind"> (exploser)</klr></trd></trdgrp> 
  <trd lng="ru">&c_v;&c_z;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sv">explodera</trd>
</drv>
<drv mrk="eksplo.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Brua subita plivolumeni&gcirc;o de naski&gcirc;antaj gasoj:
      <ekz>
        <tld/>o de lumgaso, de mingaso;
      </ekz>
      <ekz>
        <ind><tld/>motoro</ind>.
        <trd lng="fr">moteur &agrave; explosion</trd>
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Brua subita diskrevo pro interna premado de gasoj: 
      <ekz>
        <tld/>o de kaldronego, de vulkano.
      </ekz>
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo> 
    <dif>
      Subita brua eksteri&gcirc;o de forta sento a&ubreve; pasio: 
      <ekz>
        <tld/>o da kolero, &gcirc;ojo, amo; 
      </ekz>
      <ekz>
        vo&ccirc;o plena de flamecaj <tld/>oj;
      </ekz>
      <ekz>
        la kamparano trankviligis la <tld/>ojn de sia edzino
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 12</lok></fnt>.
      </ekz> 
    </dif>
    <trd lng="ru">&c_v;&c_s;&c_p;&c_y;&c_sh;&c_k;&c_a;</trd>
  </snc>
  <snc num="4">
    <uzo tip="stl">FIG</uzo> 
    <dif>
      Subita brua komenci&gcirc;o: 
      <ekz>
        <tld/>o de milito, ribelo, kriego. 
      </ekz>
    </dif>
    <trd lng="ru">&c_v;&c_s;&c_p;&c_y;&c_sh;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Explosion</trd>,
    <trd>Ausbruch</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;tonation</trd>,
    <trd>explosion<klr tip="ind"> (bombe, rire,...)</klr></trd>,
    <trd>flamb&eacute;e<klr tip="ind"> (explosion, fig.)</klr></trd></trdgrp> 
  <trd lng="ru">&c_v;&c_z;&c_r;&c_y;&c_v;</trd>
  <trd lng="sv">explosion</trd>
</drv>
<drv mrk="eksplo.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Substanco, kiu povas <tld/>i:
      <ekz>
        dinamito, nitroglicerino estas <tld/>a&jcirc;oj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &dash; &Ccirc;u malbona nova&jcirc;o, &Ccirc;efo?
        &dash; Ne, knabo, nur iom tro tu&scirc;otima <tld/>a&jcirc;o
        <klr>(tro dan&gcirc;era informo)</klr>
        <fnt><bib>UrdHadda</bib>, <lok>p. 16</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sprengstoff</trd>
  <trd lng="fr">explosif<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_r;&c_y;&c_v;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_z;&c_r;&c_y;&c_v;&c_ch;&c_a;&c_t;&c_o;&c_je; &c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>
<drv mrk="eksplo.0ema">
  <kap><tld/>ema</kap>
  <snc num="1">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Facile <tld/>anta:
      <ekz>
        <tld/>emaj substancoj.
      </ekz>
    </dif>
    <trd lng="de">explosiv</trd>
    <trd lng="ru">&c_v;&c_z;&c_r;&c_y;&c_v;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Facile inciti&gcirc;anta:
      <ekz>
        <tld/>ema karaktero;
      </ekz>
      <ekz><klr>[li estas]</klr> <tld/>ema kiel pulvo
        <fnt><bib>PrV</bib></fnt></ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flam.0igxema">flami&gcirc;ema</ref>,
      <ref cel="fulm">fulma</ref>,
      <ref cel="bol.0i">boli</ref>.
    </refgrp>
    <trd lng="de">kolerisch</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>bouillant<klr tip="ind"> (caract&egrave;re)</klr></trd>,
    <trd>explosif<klr tip="ind"> (adj.)</klr></trd>,
    <trd>volcanique<klr tip="ind"> (caract&egrave;re)</klr></trd></trdgrp> 
  <trd lng="ru">&c_v;&c_s;&c_p;&c_y;&c_l;&c_mol;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
</drv>
<drv mrk="eksplo.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on; fari, ke io <tld/>as:
      <ekz>
        unu fajrero sufi&ccirc;as por <tld/>igi pulvon;
      </ekz>
      <ekz>
        <tld/>igi bombon, koleron, ribelon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la senhonta aroganteco de &Scirc;ankhatum preska&ubreve; <tld/>igis la 
        re&gcirc;on de kolerego
        <fnt><bib>I&scirc;tar</bib>,<lok>&ccirc;ap. 19</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
  Indus precizigi, &ccirc;u la objekto povas esti io alia ol ~ajxo. 
  Alidire, &ccirc;u oni "~igas domon"? La figursenca ekzemplo &scirc;ajne
  tion sugestas. <aut>MB</aut>
  </adm>
  <trd lng="de">z&uuml;nden</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;cha&icirc;ner<klr tip="ind"> (le rire, la col&egrave;re,...)</klr></trd>,
    <trd>d&eacute;clencher<klr tip="ind"> (le rire, la col&egrave;re,...)</klr></trd>,
    <trd>faire <ind>exploser</ind></trd></trdgrp> 
  <trd lng="ru">&c_v;&c_z;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="eksplo.0igilo">
  <kap><tld/>igilo</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Aparato, per kiu oni <tld/>igas minon, torpedon ktp.  
    </dif>
  </snc>
  <trd lng="de">Z&uuml;nder</trd>
  <trd lng="fr">d&eacute;tonateur</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_r;&c_y;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_t;&c_o;&c_n;&c_a;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
</drv>
</art>
