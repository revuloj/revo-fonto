  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_r;&c_y;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_d;&c_e;&c_t;&c_a;&c_n;&c_a;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">detonador</trd>
  <trd lng="de">Z&uuml;nder</trd>
  <trd lng="fr">d&eacute;tonateur</trd>
  <trd lng="pt">detonador</trd>
  <trdgrp lng="hu">
    <trd>gy&uacute;jt&oacute;szerkezet</trd>,
    <trd>deton&aacute;tor</trd>,
    <trd>robban&oacute;szerkezet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>innesco</trd>,
    <trd>detonatore</trd>
  </trdgrp>
  <trd lng="nl">ontstekingsmechanisme</trd>
  <trdgrp lng="pl">
    <trd>detonator</trd>,
    <trd>zapalnik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_r;&c_y;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_t;&c_o;&c_n;&c_a;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="eksplo.pra0o">
  <kap>pra<tld/>o</kap>
  <snc mrk="eksplo.pra0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Grandenergia fenomeno, kiu markas la naski&gcirc;on de la
      <ref cel="univer.0o">universo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_ib;
  <ind>&c_V;&c_y;&c_b;&c_u;&c_h;</ind></trd>
  <trd lng="ca">Big Bang</trd>
  <trd lng="de">Urknall</trd>
  <trd lng="en">Big Bang</trd>
  <trd lng="fi">Alkur&auml;j&auml;hdys</trd>
  <trd lng="fr">Big bang</trd>
  <trd lng="hu">&odblac;srobban&aacute;s</trd>
  <trd lng="nl">oerknal</trd>
  <trd lng="pl">Wielki Wybuch</trd>
  <trd lng="pt">Grande Explos&atilde;o</trd>
  <trd lng="ru">&c_B;&c_o;&c_l;&c_mol;&c_sh;&c_o;&c_j; &c_V;&c_z;&c_r;&c_y;&c_v;</trd>
  <trd lng="tr">B&uuml;y&uuml;k Patlama</trd>
</drv>

<drv mrk="eksplo.0akonsonanto">
  <kap><tld/>a konsonanto <fnt><bib>PMEG</bib></fnt></kap>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      Momenta <ref tip="super" cel="konson1.0o">konsonanto</ref>
      produktata per abrupta malfermo de la bu&scirc;kanalo, kiu lasas
      la spiron eliri nur post momente kompleta fermi&gcirc;o
      (en Esperanto <em>k, g, t, d, p, b</em>):
      <ekz>
        se du vokalojn apartigas du konsonantoj, la unua el kiuj estas
        <tld/>a kaj la dua estas flua (/l/ a&ubreve; /r/), la silabolimo situas
        anta&ubreve; la <tld/>a (/p&aacute;-tro/, /ci-tr&oacute;-no/, /ci-pr&eacute;-so/,
        /ge-o-gra-f&iacute;-o/)
        <fnt>
          <aut>Marc Bavant</aut>: <vrk>Silabo kaj silabado</vrk>.
          <lok><url
 ref="http://lingvakritiko.com/2006/12/20/silabo-kaj-silabado/"
              >Lingva kritiko</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="super" cel="bru.0anto.FON">bruanto</ref>
  <trd lng="be">&c_v;&c_y;&c_b;&c_u;&c_h;&c_o;&c_v;&c_y; <ind>&c_z;&c_y;&c_ch;&c_n;&c_y;</ind></trd>
  <trdgrp lng="de">
    <trd>Plosiv</trd>,
    <trd>Verschlusslaut</trd>,
    <trd>Okklusiv</trd>,
    <trd>Klusil</trd>
  </trdgrp>
  <trd lng="en">plosive</trd>
  <trd lng="es">oclusiva</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_m;&a_kh;&a_w;&a_A;&a_n; <ind>&a_b;&a_n;&a_d;&a_sh;&f_y;</ind></trd>,
    <trd>&a_h;&a_m;&a_kh;&a_w;&a_A;&a_n; <ind>&a_A;&a_n;&a_s;&a_d;&a_A;&a_d;&f_y;</ind></trd>
  </trdgrp>
  <trd lng="fr">occlusive<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>z&aacute;rhang</trd>,
    <trd>exploz&iacute;va</trd>,
    <trd>okkluz&iacute;va</trd>
  </trdgrp>
  <trd lng="nl">plofklank</trd>
  <trdgrp lng="pt">
    <trd>explosiva <klr>(subst.)</klr></trd>,
    <trd>consoante explosiva</trd>,
    <trd>oclusiva <klr>(subst.)</klr></trd>,    
    <trd>consoante oclusiva</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_r;&c_y;&c_v;&c_n;&c_o;&c_j; &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
