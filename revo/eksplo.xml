<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>eksplod</rad>/i</kap> <fnt><bib>PV</bib></fnt>
<drv mrk="eksplo.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Forte kaj brue skui aeron, pro subita plivolumeni&gcirc;o de gaso:
      <ekz>
        dinamito facile <tld/>as.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flam">ekflami</ref>,
      <ref cel="flagr.0i">flagri</ref>
    </refgrp>
  </snc>
  <snc>
    <dif>
      Brue diskrevi, pro granda interna premo de l' gasoj:
      <ekz>
        bombo, mino, vaporma&scirc;ino, torpedo <tld/>as.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="romp">disrompi&gcirc;i</ref>,
      <ref cel="salt.dis0i">dissalti</ref>,
      <ref cel="split">disspliti&gcirc;i</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Subite kaj brue eksterigi fortan senton a&ubreve; pasion, kiun oni
      ne
      povas
      plu deteni:
      <ekz>
        <tld/>i de furiozo, de kolero, pro &gcirc;ojo, per
        rido<fnt>B</fnt>, per larmoj;
      </ekz>
      <ekz>
        li <tld/>is, dirante, ke...<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_v;&c_s;&c_p;&c_y;&c_l;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Subite kaj brue komenci&gcirc;i:
      <ekz>
        balda&ubreve; la ventego devis <tld/>i, rompi la glacion, kaj
        &ccirc;iuj tie devis nesaveble droni
        <fnt><bib>Fab3</bib>, <lok>Io</lok></fnt>;
      </ekz>
      <ekz>
        la milito <tld/>is neatendite;
      </ekz>
      <ekz>
        <tld/>as &gcirc;ojo<fnt>Z</fnt>, striko, ribelo, katastrofo,
        krio.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_v;&c_s;&c_p;&c_y;&c_h;&c_n;&c_u;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_b;&c_u;&c_h;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>esclatar</trd>,
    <trd>explotar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>explodieren</trd>,
    <trd>ausbrechen</trd>
  </trdgrp>
  <trd lng="es">explotar</trd>
  <trd lng="en">explode</trd>
  <trdgrp lng="fr">
    <trd>&eacute;clater</trd>,
    <trd>exploser</trd>,
    <trd>sauter<klr tip="ind"> (exploser)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>esplodere</trd>,
    <trd>scoppiare</trd>,
    <trd>detonare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ontploffen</trd>,
    <trd>exploderen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>eksplodowa&cacute;</trd>,
    <trd>wybucha&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>explodir</trd>,
    <trd>estourar</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_z;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sv">explodera</trd>
</drv>

<drv mrk="eksplo.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Brua subita plivolumeni&gcirc;o de naski&gcirc;antaj gasoj:
      <ekz>
        <tld/>o de lumgaso, de mingaso;
      </ekz>
      <ekz>
        <ind><tld/>motoro</ind>.
        <trd lng="fr">moteur &agrave; explosion</trd>
        <trdgrp lng="hu">
          <trd>robban&oacute;motor</trd>,
          <trd>bels&odblac; &eacute;g&eacute;s&udblac;
          <ind>motor</ind></trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Brua subita diskrevo pro interna premado de gasoj:
      <ekz>
        <tld/>o de kaldronego, de vulkano.
      </ekz>
    </dif>
    <trd lng="pt">estouro</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Subita brua eksteri&gcirc;o de forta sento a&ubreve; pasio:
      <ekz>
        <tld/>o da kolero, &gcirc;ojo, amo;
      </ekz>
      <ekz>
        vo&ccirc;o plena de flamecaj <tld/>oj;
      </ekz>
      <ekz>
        la kamparano trankviligis la <tld/>ojn de sia edzino
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 12</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_v;&c_s;&c_p;&c_y;&c_sh;&c_k;&c_a;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Subita brua komenci&gcirc;o:
      <ekz>
        <tld/>o de milito, ribelo, kriego.
      </ekz>
    </dif>
    <trd lng="ru">&c_v;&c_s;&c_p;&c_y;&c_sh;&c_k;&c_a;</trd>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_b;&c_u;&c_h;</trd>
  <trdgrp lng="ca">
    <trd>esclat</trd>,
    <trd>explosi&oacute;</trd>
  </trdgrp>
  <trd lng="es">explosi&oacute;n</trd>
  <trdgrp lng="de">
    <trd>Explosion</trd>,
    <trd>Ausbruch</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;tonation</trd>,
    <trd>explosion<klr tip="ind"> (bombe, rire,...)</klr></trd>,
    <trd>flamb&eacute;e<klr tip="ind"> (explosion, fig.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ontploffing</trd>,
    <trd>explosie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>robban</trd>,
    <trd>felrobban</trd>,
    <trd>kirobban</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>esplosione</trd>,
    <trd>scoppio</trd>,
    <trd>detonazione</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>eksplozja</trd>,
    <trd>wybuch</trd>
  </trdgrp>
  <trd lng="pt">explos&atilde;o</trd>
  <trd lng="ru">&c_v;&c_z;&c_r;&c_y;&c_v;</trd>
  <trd lng="sv">explosion</trd>
</drv>

<drv mrk="eksplo.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="eksplo.0ajxo.KEM">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Substanco, kiu povas <tld/>i:
      <ekz>
        dinamito, nitroglicerino estas <tld/>a&jcirc;oj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &#8208; &Ccirc;u malbona nova&jcirc;o, &Ccirc;efo?
        &#8208; Ne, knabo, nur iom tro tu&scirc;otima <tld/>a&jcirc;o
        <klr>(tro dan&gcirc;era informo)</klr>
        <fnt><bib>UrdHadda</bib>, <lok>p. 16</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_b;&c_u;&c_h;&c_o;&c_w;&c_k;&c_a;</trd>
  <trd lng="ca">explosiu<klr> (subst.)</klr></trd>
  <trd lng="de">Sprengstoff</trd>
  <trd lng="es">explosivo</trd>
  <trd lng="fr">explosif<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>robban&oacute;anyag</trd>,
    <trd>robban&oacute;szer</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>esplosivo<klr> (sost.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>springstof</trd>,
    <trd>explosief<klr tip="ind"> (z.nw.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>materia&lstroke; <ind>wybuchowy</ind></trd>
  </trdgrp>
  <trd lng="pt">explosivo</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_r;&c_y;&c_v;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_z;&c_r;&c_y;&c_v;&c_ch;&c_a;&c_t;&c_o;&c_je; &c_v;&c_je;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="eksplo.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <uzo tip="fak">KEM</uzo>
    <dif>
      Facile <tld/>anta:
      <ekz>
        <tld/>emaj substancoj.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_y;&c_b;&c_u;&c_h;&c_o;&c_v;&c_y;</trd>
    <trd lng="de">explosiv</trd>
    <trd lng="hu">k&ouml;nnyen <ind>robban&oacute;</ind></trd>
    <trd lng="it">esplosivo<klr> (agg.)</klr></trd>
    <trd lng="ru">&c_v;&c_z;&c_r;&c_y;&c_v;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Facile inciti&gcirc;anta:
      <ekz>
        <tld/>ema karaktero;
      </ekz>
      <ekz>
        <klr>[li estas]</klr> <tld/>ema kiel pulvo
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_ch;&c_y;&c_v;&c_y;</trd>
    <refgrp tip="vid">
      <ref cel="flam.0igxema">flami&gcirc;ema</ref>,
      <ref cel="fulm">fulma</ref>,
      <ref cel="bol.0i">boli</ref>.
    </refgrp>
    <trd lng="de">cholerisch</trd>
    <trd lng="hu">robban&eacute;kony</trd>
    <trd lng="ru">&c_v;&c_s;&c_p;&c_y;&c_l;&c_mol;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </snc>
  <trd lng="ca">explosiu<klr> (adj.)</klr></trd>
  <trdgrp lng="fr">
    <trd>bouillant<klr tip="ind"> (caract&egrave;re)</klr></trd>,
    <trd>explosif<klr tip="ind"> (adj.)</klr></trd>,
    <trd>volcanique<klr tip="ind"> (caract&egrave;re)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>esplosivo<klr> (fig.)</klr></trd>,
    <trd>prorompente<klr> (fig.)</klr></trd>,
    <trd>infiammabile<klr> (fig.)</klr></trd>,
    <trd>collerico<klr> (fig.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ontplofbaar</trd>,
    <trd>explosief<klr tip="ind"> (b.nw.)</klr></trd>
  </trdgrp>
  <trd lng="pl">wybuchowy</trd>
  <trd lng="pt">explosivo</trd>
</drv>

<drv mrk="eksplo.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on; fari, ke io <tld/>as:
      <ekz>
        unu fajrero sufi&ccirc;as por <tld/>igi pulvon;
      </ekz>
      <ekz>
        <tld/>igi bombon, koleron, ribelon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la senhonta aroganteco de &Scirc;ankhatum preska&ubreve;
        <tld/>igis la re&gcirc;on de kolerego
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Indus precizigi, &ccirc;u la objekto povas esti io alia ol ~a&jcirc;o.
    Alidire, &ccirc;u oni &leftquot;~igas domon&rightquot;? La figursenca ekzemplo &scirc;ajne
    tion sugestas. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>detonar</trd>,
    <trd>fer esclatar</trd>
  </trdgrp>
  <trd lng="de">z&uuml;nden</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;chainer<klr tip="ind"> (le rire, la
    col&egrave;re,...)</klr></trd>,
    <trd>d&eacute;clencher<klr tip="ind"> (le rire, la
    col&egrave;re,...)</klr></trd>,
    <trd>faire <ind>exploser</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fare <ind>esplodere</ind></trd>,
    <trd>fare <ind>scoppiare</ind></trd>,
    <trd>fare <ind>detonare</ind></trd>,
    <trd>brillare<klr> (esplosivo)</klr></trd>
  </trdgrp>
  <trd lng="nl">doen <ind>ontploffen</ind></trd>
  <trdgrp lng="pl">
    <trd>detonowa&cacute;</trd>,
    <trd>odpala&cacute;<klr> (np. bomb&eogonek;)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>detonar</trd>,
    <trd>explodir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felrobbant</trd>,
    <trd>kirobbant</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_o;&c_r;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_z;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

