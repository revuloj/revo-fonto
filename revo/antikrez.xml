<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>antikrez</rad>/o</kap>
<drv mrk="antikrez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Hipoteka <ref tip="super" cel="kontra3.0o">kontrakto</ref>
      la&ubreve; kiu la hipotekkreditoro prenas en
      posedon la hipotekitan proprieta&jcirc;on, &gcirc;uante &gcirc;iajn fruktojn
      a&ubreve; profitojn anstata&ubreve; renton:
      <ekz>
        en Babilonio dum la regtempo de Hamurabo la interezo por prunto
        de greno estis triono kaj de ar&gcirc;ento kvinono, kiun oni povis pagi
        per sklava laboro kadre de <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frukt.0uzo">fruktuzo</ref>,
      <ref cel="garant.0ajxo">garantia&jcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>antichresis</trd>,
    <trd>z&aacute;stavn&iacute; nucen&eacute; pr&aacute;vo</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nutzungspfandrecht</trd>,
    <trd>Antichrese</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>antichresis</trd>,
    <trd>Welsh <ind>mortgage</ind></trd>,
    <trd>assignment of rents</trd>,
    <trd>assignment of profits</trd>,
    <trd>pledging of the income from real property</trd>
  </trdgrp>
  <trd lng="es">anticresis</trd>
  <trd lng="fa">&a_r;&a_h;&a_n; &a_t;&a_S;&a_r;&a_shadda;&a_f;</trd>
  <trdgrp lng="hu">
    <trd>haszn&aacute;lati <ind>z&aacute;log</ind></trd>,
    <trd>haszon&eacute;lvezeti <ind>z&aacute;log</ind></trd>
  </trdgrp>
  <trd lng="it">anticresi</trd>
  <trdgrp lng="nl">
    <trd>antichresis</trd>,
    <trd>verpanding van het vruchtgebruik</trd>
  </trdgrp>
  <trd lng="pt">anticrese</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_t;&c_i;&c_h;&c_r;&c_je;&c_z;</trd>,
    <trd>&c_r;&c_u;&c_ch;&c_n;&c_o;&c_j; &c_z;&c_a;&c_l;&c_o;&c_g; &c_n;&c_je;&c_d;&c_v;&c_i;&c_zh;&c_i;&c_m;&c_o;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;stavn&eacute; n&uacute;ten&eacute; pr&aacute;vo</trd>
  <trd lng="sv">pants&auml;ttning av fast egendoms avkastning</trd>
</drv>
</art>
