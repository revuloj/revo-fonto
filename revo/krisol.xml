<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krisol.xml,v 1.19 2023/12/03 09:59:01 revo Exp $">
<kap>
  <rad>krisol</rad>/o
</kap>
<drv mrk="krisol.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vaz.0o">Vazo</ref>
      el fajrorezista tero, en kiu oni fandas mineralojn:
      <ekz>
        por fari laboratoriajn <tld/>ojn, oni kutime uzas porcelanon a&ubreve; inertan metalon,
        ekzemple platenon, nikelon, a&ubreve; zirkonion
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22066">Krisolo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        heterogena popolaro, kunigita kaj kune fandita en la <tld/>o de la nova nacio
        kun novaj ideoj kaj konceptoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2008/009404.php">Inspira antologio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="vaz.fand0o">fandvazo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_y;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">gresol</trd>
  <trd lng="de">Schmelztiegel</trd>
  <trd lng="en">crucible</trd>
  <trd lng="es">crisol</trd>
  <trd lng="fa"><klr tip="ind"> (&a_Z;&a_r;&a_f; &a_n;&a_s;&a_w;&a_z;)</klr>&a_b;&a_w;&a_t;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>creuset</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;nt&odblac;&uuml;st</trd>,
    <trd>olvaszt&oacute;t&eacute;gely</trd>
  </trdgrp>
  <trd lng="ja">&#x308B;&#x3064;&#x307C;</trd>
  <trd lng="nl">smeltkroes</trd>
  <trd lng="pl">tygiel</trd>
  <trdgrp lng="pt">
    <trd>crisol</trd>,
    <trd>cadinho</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_i;&c_g;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_n;&c_i;&c_l;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x438;&#x433;&#x435;&#x43B;&#x44C;</trd>,
    <trd>&#x433;&#x43E;&#x440;&#x43D;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5769; <pr>g&#x101;n</pr></trd>,
    <trd>&#x57DA; <pr>gu&#x14D;</pr></trd>,
    <trd>&#x581D; <pr>gu&#x14D;</pr></trd>,
    <trd>&#x5769;&#x57DA; <pr>g&#x101;ngu&#x14D;</pr></trd>,
    <trd>&#x5769;&#x581D; <pr>g&#x101;ngu&#x14D;</pr></trd>
  </trdgrp>
</drv>
</art>
