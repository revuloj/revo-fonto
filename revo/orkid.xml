<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>orkid</rad>/o
</kap>
<drv mrk="orkid.0o">
  <kap><tld/>o</kap>
  <snc mrk="orkid.0o.BOT">
  <uzo tip="fak">BOT</uzo>
      <dif>
      Genro el familio <ref tip="super" cel="orkid.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Orchis</trd>).
    </dif>
  <trdgrp lng="de">
    <trd>Knabenkraut</trd>,
    <trd>Orchis</trd>
  </trdgrp>
  <trd lng="nl">standelkruit</trd>
  </snc>
  <snc mrk="orkid.0o.ANA">
  <uzo tip="fak">ANA</uzo>
      <dif>
      Scienca nomo de <ref tip="sin" cel="testik.0o">testiko</ref>.
    </dif>
  <trdgrp lng="nl">
    <trd>testikel</trd>,
    <trd>teelbal</trd>
  </trdgrp>
  </snc>
  </drv>
<drv mrk="orkid.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (<trd lng="la">Orchidacea</trd>), kun okulfrapaj, ofte
      strangformaj floroj.
    </dif>
    <ref tip="prt" cel="orkide.0o">orkideo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Orchideen</trd>,
    <trd>Knabenkrautgew&auml;chse</trd>
  </trdgrp>
</drv>
</art>
