<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>orkid</rad>/o
</kap>
<drv mrk="orkid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="orkid.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Orchis</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Knabenkraut</trd>,
    <trd>Orchis</trd>
  </trdgrp>
</drv>
<drv mrk="orkid.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de unukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Orchidacea</trd>), kun okulfrapaj, ofte
      strangformaj floroj.
    </dif>
    <ref tip="prt" cel="orkide.0o">orkideo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Orchideen</trd>,
    <trd>Knabenkrautgew&auml;chse</trd>
  </trdgrp>
</drv>
</art>
