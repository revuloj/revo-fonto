<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: orkid.xml,v 1.25 2024/01/30 16:09:26 revo Exp $">
<kap><rad>orkid</rad>/o</kap>
<drv mrk="orkid.0o">
  <kap><tld/>o</kap>
  <snc mrk="orkid.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="orkidac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Orchis</trd>):
      <ekz>
        la purpura <tld/>o (Orchis purpurea) estas unu el la plej grandaj meze&ubreve;ropaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=580683">Purpura orkido</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/76/Orchis_purpurea_plant.jpg/275px-Orchis_purpurea_plant.jpg" prm="CC-BY-SA-3.0">purpura <tld/>o (Orchis purpurea)
        <fnt>
          <lok><url ref="&WCW;/File:Orchis_purpurea_plant.jpg">&WCW;/File:Orchis_purpurea_plant.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_z;&c_ja;&c_z;&c_ju;&c_l;&c_mol;&c_k;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
    <trdgrp lng="de">
      <trd>Knabenkraut</trd>,
      <trd>Orchis</trd>
    </trdgrp>
    <trd lng="el">&omikron;&rho;&chi;&jota;&delta;&epsilon_ton;&alfa;</trd>
    <trd lng="en">orchid</trd>
    <trd lng="fr">orchis</trd>
    <trd lng="hu">kosbor</trd>
    <trdgrp lng="it">
      <trd>orchide</trd>,
      <trd>orchidea</trd>
    </trdgrp>
    <trd lng="nl">standelkruit</trd>
    <trd lng="pl">storczyk</trd>   
    <trd lng="pt">orqu&iacute;dea</trd>
    <trd lng="ru">&c_ja;&c_t;&c_r;&c_y;&c_sh;&c_n;&c_i;&c_k;</trd>
    <trdgrp lng="zh">
    <trd>&#x5170;&#x82B1; <pr>l&#xE1;nhu&#x101;</pr></trd>,
    <trd>&#x862D;&#x82B1; <pr>l&#xE1;nhu&#x101;</pr></trd>,
    <trd>&#x5170; <pr>l&#xE1;n</pr></trd>,
    <trd>&#x862D; <pr>l&#xE1;n</pr></trd>,
    <trd>&#x4F69;&#x5170; <pr>p&#xE8;il&#xE1;n</pr></trd>,
    <trd>&#x4F69;&#x862D; <pr>p&#xE8;il&#xE1;n</pr></trd>,
    <trd>&#xF91F; <pr>l&#xE1;n</pr></trd>,
    <trd>&#x85BD; <pr>zh&#x113;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="orkid.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Scienca nomo de <ref tip="sin" cel="testik.0o">testiko</ref>.
    </dif>
    <trd lng="be">&c_ja;&c_je;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
    <trd lng="el">&omikron_ton;&rho;&chi;&jota;&sigma_fina;</trd>
    <trd lng="en">testicle</trd>
    <trd lng="hu">here<klr tip="ind"> (ivarmirigy)</klr></trd>
    <trd lng="it">testicolo</trd>
    <trdgrp lng="nl">
      <trd>testikel</trd>,
      <trd>teelbal</trd>
    </trdgrp>
    <trd lng="pl">j&aogonek;dro m&eogonek;skie</trd>   
    <trd lng="pt">test&iacute;culo</trd>
    <trd lng="zh">&#x777E;&#x4E38; <pr>g&#x101;ow&#xE1;n</pr></trd>
</snc>
  <trd lng="cs">varlata</trd>
  <trdgrp lng="ja">
    <trd>&#x30CF;&#x30AF;&#x30B5;&#x30F3;&#x30C1;&#x30C9;&#x30EA;</trd>,
    <trd>&#x30AA;&#x30EB;&#x30AD;&#x30B9;</trd>,
    <trd>&#x7CBE;&#x5DE3;  <pr>&#x305B;&#x3044;&#x305D;&#x3046;</pr></trd>,
    <trd>&#x777E;&#x4E38;  <pr>&#x3053;&#x3046;&#x304C;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="sk">semenn&iacute;ky</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x43E;&#x437;&#x443;&#x43B;&#x438;&#x43D;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x44F;&#x454;&#x447;&#x43A;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
