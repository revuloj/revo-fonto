<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>orkid</rad>/o
</kap>
<drv mrk="orkid.0o">
  <kap><tld/>o</kap>
  <snc mrk="orkid.0o.BOT">
  <uzo tip="fak">BOT</uzo>
      <dif>
      Genro el familio <ref tip="super" cel="orkidac.0oj"><tld/>acoj</ref>
      (<trd lng="la">Orchis</trd>).
    </dif>
  <trdgrp lng="de">
    <trd>Knabenkraut</trd>,
    <trd>Orchis</trd>
  </trdgrp>
  <trd lng="nl">standelkruit</trd>
  </snc>
  <snc mrk="orkid.0o.ANA">
  <uzo tip="fak">ANA</uzo>
      <dif>
      Scienca nomo de <ref tip="sin" cel="testik.0o">testiko</ref>.
    </dif>
  <trdgrp lng="nl">
    <trd>testikel</trd>,
    <trd>teelbal</trd>
  </trdgrp>
  </snc>
  </drv>

</art>
