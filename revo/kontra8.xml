<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kontrast</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kontra8.0o">
  <kap><tld/>o</kap>
  <snc mrk="kontra8.0o.KOMUNE">
    <dif>
      Malsameco, maleco de du objektoj a&ubreve; ideoj, kiuj reciproke pli klare
      distingi&gcirc;as pro la apudmeti&gcirc;o mem:
      <ekz>
        <tld/>o de la ombro kaj de la lumo;
      </ekz>
      <ekz>
        &ccirc;iu <tld/>o vekas atenton;
      </ekz>
      <ekz>
        kiam oni el ardanta forno saltas en glacian akvon, oni ne povas
        senti la <tld/>on tiel forte kiel mi<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="difere.0o">diferenco</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_s;&c_t;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;&c_l;&c_je;&c_g;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kontrast</trd>,
    <trd>Gegensatz</trd>,
    <trd>starker <ind>Unterschied</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>contrast</trd>,
    <trd>difference <klr> (p. p. koloroj, formoj, stiloj,
    ktp.)</klr></trd>,
    <trd>antithesis</trd>,
    <trd>opposition</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>contraste</trd>,
    <trd>opposition<klr tip="amb"> (contraste)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kontraszt</trd>,
    <trd>&eacute;les <ind>ellent&eacute;t</ind></trd>,
    <trd>szembet&udblac;n&odblac; <ind>k&uuml;l&ouml;nbs&eacute;g</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kontra8.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="kontra8.0i.KOMUNE">
    <dif>
      Esti en <tld/>o kun, tre <ref tip="vid"
      cel="distin.0igxi">distingi&gcirc;i</ref> de alia objekto per <tld/>o:
      <ekz>
        <tld/>i ion, kun<fnt>Z</fnt>,
      </ekz>
      <ekz>
        al, kontra&ubreve; io;
      </ekz>
      <ekz>
        kun la grizaj muroj forte <tld/>is la blankaj tukoj;
      </ekz>
      <ekz>
        &scirc;ia eta figuro akre <tld/>is la fonon &scirc;in
        &ccirc;irka&ubreve;antan<fnt>B</fnt>;
      </ekz>
      <ekz>
        la ru&gcirc;o tiel bele <tld/>is la blankan ne&gcirc;on
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_t;&c_r;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kontrastieren</trd>,
    <trd>sich <ind>abheben</ind></trd>,
    <trd>sich <ind>unterscheiden</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>exhibit <ind>contrast</ind></trd>,
    <trd>show <ind>difference</ind></trd>,
    <trd>stand out <klr> (kontraste kun fono)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire <ind>contraste</ind> avec</trd>,
    <trd>se <ind>d&eacute;tacher</ind> sur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;les <ind>ellent&eacute;tben</ind> &aacute;ll</trd>,
    <trd>szembet&udblac;n&odblac;en <ind>k&uuml;l&ouml;nb&ouml;zik</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_t;&c_r;&c_a;&c_s;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_z;&c_k;&c_o; &c_o;&c_t;&c_l;&c_i;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_je;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_n;&c_a; &c_f;&c_o;&c_n;&c_je;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kontra8.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kontra8.0igi.KOMUNE">
    <dif>
      Fari, ke du objektoj <tld/>u.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>kontrastieren</trd>,
    <trd>gegen&uuml;berstellen</trd>,
    <trd>Unterschiede <ind>herausarbeiten</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>compare</trd>,
    <trd>point out difference between</trd>,
    <trd>set off</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mettre en <ind>regard</ind></trd>,
    <trd>opposer<klr tip="amb"> (mettre en contraste)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szembe&aacute;ll&iacute;t</trd>,
    <trd>&eacute;lesen <ind>megk&uuml;l&ouml;nb&ouml;ztet</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol; &c_r;&c_a;&c_z;&c_l;&c_i;&c_ch;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_p;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kontra8.0eal">
  <kap><tld/>e al</kap>
  <snc mrk="kontra8.0eal.KOMUNE">
    <dif>
      <ref tip="super" cel="kontraux.0.kompare">Kontra&ubreve;</ref>, kun iu
      forta diferenco:
      <ekz>
        kvintona <klr>...</klr> gamo <klr>...</klr> enhavas nur kvin
        tonojn, <tld/>e al ma&jcirc;oraj kaj minoraj gamoj, kiuj enhavas po sep
        tonojn
        <fnt><bib>RokPop</bib>, <lok>kapvorto &leftquot;kvintona&rightquot;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>&agrave; la diff&eacute;rence de</trd>,
    <trd>par opposition &agrave;</trd>
  </trdgrp>
</drv>
</art>
