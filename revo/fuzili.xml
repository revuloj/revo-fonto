<vortaro>
<art mrk="$Id: fuzili.xml,v 1.21 2023/11/12 09:22:23 revo Exp $">
<kap>
  <rad>fuzilier</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="fuzili.0o">
  <kap><tld/>o</kap>
  <snc mrk="fuzili.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="dif" cel="fusil.0isto">Fusilisto</ref>:
      <ekz>
        anta&ubreve; la lo&gcirc;ietoj de la belaj knabinoj kun
        kolor&scirc;minkitaj okuloj vagadas <klr>[&#8230;]</klr>
        arabaj <tld/>oj kun turbano kaj ru&gcirc;a zono
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>Rotterdam, 1956</lok>
        </fnt>;
      </ekz>
      <ekz>
        Arme&#8217;! la&ubreve;dire pola! sed <tld/>oj, &parsl; Saperoj, grenadiroj kaj kanonieroj: &parsl; En &ccirc;i
        amas&#8217; a&ubreve;di&gcirc;as da fremdaj titoloj &parsl; Pli, ol polaj
        <fnt>
          <bib>SrT</bib>
          <lok>Libro XII.</lok>
        </fnt>!
      </ekz>
      <ekz>
        anta&ubreve; la Dua Mondmilito li militservis en la Regimento de <tld lit="F"/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=431417">Leon Machowski</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_l;&c_o;&c_k;</trd>
  <trd lng="cs">st&rcaron;elec z pu&scaron;ky</trd>
  <trdgrp lng="de">
   <trd>Sch&uuml;tze</trd>,
   <trd>F&uuml;silier</trd></trdgrp>
  <trd lng="en">fusilier</trd>
  <trd lng="fr">fusilier</trd>
  <trdgrp lng="hu">
   <trd>pusk&aacute;s</trd>,
   <trd>l&ouml;v&eacute;sz</trd></trdgrp>
  <trd lng="it">fuciliere</trd>
  <trd lng="nl">schutter<klr tip="ind"> (met geweer)</klr></trd>
  <trd lng="pl">fizylier</trd>
  <trd lng="pt">fuzileiro</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_r;&c_je;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_f;&c_u;&c_z;&c_i;&c_l;&c_jo;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">strelec z pu&scaron;ky</trd>
  <trd lng="zh">&#23556;&#20987;&#36816;&#21160;&#21592; <pr>sh&egrave;j&iacute;y&ugrave;nd&ograve;ngyu&aacute;n</pr></trd>
</drv>
</art>
