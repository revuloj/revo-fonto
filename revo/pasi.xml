<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pasi.xml,v 1.26 2022/03/25 10:18:30 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pasi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="pasi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pasi.0o.dezirego">
    <dif>
      Fervorega <ref tip="super" cel="inklin.0o.KOMUNE">inklino</ref> al io; 
      arda <ref tip="super" cel="dezir.0o.ago">deziro</ref>:
      <ekz>
        malpermesoj neniom helpis kaj la <tld/>o fumi tabakon disvolvi&gcirc;is &ccirc;iam
        pli
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        amo, avideco, adorado, malamo, timoj, esperoj, plej diversaj interesoj kaj
        <tld/>oj, plej diversaj deziroj kaj celadoj volvi&gcirc;adis kaj kruci&gcirc;adis
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        doloraj estis la malamo kaj &scirc;ovinistaj <tld/>oj &ccirc;ie nun ellasataj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[jen]</klr> tiuj, kiuj volas forpremi ribelan 
        <tld/>on a&ubreve; kuraci profundan doloron de sia
        brusto, retiras sin en eternan solecon
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        en lia animo reveki&gcirc;is la <tld/>oj, dormigitaj por unu momento: malamo
        <klr>[&#x2026;]</klr> kaj &jcirc;aluzo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la mezo de la <klr>[&#x2026;]</klr> uragano de via <tld/>o vi devas observi certan
        moderecon, kiu donas al &ccirc;io harmonion
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        malpla&ccirc;as nenio, se taksas <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="mani.0o">manio</ref>,
      <ref cel="forges.mem0o">memforgeso</ref>,
      <ref cel="obsed.0o">obsedo</ref>
    </refgrp>
    <trd lng="pl">pasja</trd>
  </snc>
  <snc mrk="pasi.0o.amo">
    <dif>
      Arda kaj senbrida <ref tip="super" cel="am.0o">amo</ref>:
      <ekz>
        Mi transdonis &scirc;in en la manojn de &scirc;iaj amistoj, <klr>[&#x2026;]</klr> al kiuj &scirc;i havis
        voluptan <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 23:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vin amegas per profunda kaj nepereebla amo, se mi havus feli&ccirc;on inspiri al vi
        <tld/>on egalan al la mia ni amba&ubreve; estus tre fortaj por batali kontra&ubreve; la
        malfacila&jcirc;oj de la vivo
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        ebria pro <tld/>o, freneza, <klr>[&#x2026;]</klr> Fernando altiris la junulinon al siaj
        genuoj: kaj iliaj lipoj kuni&gcirc;is
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">pasi&oacute;n amorosa</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_r;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">passi&oacute;</trd>
  <trd lng="cs">v&aacute;&scaron;e&ncaron;</trd>
  <trdgrp lng="de">
    <trd>Passion</trd>,
    <trd>Leidenschaft</trd>
  </trdgrp>
  <trd lng="en">passion</trd>
  <trd lng="es">pasi&oacute;n</trd>
  <trd lng="fr">passion</trd>
  <trd lng="hu">szenved&eacute;ly</trd>
  <trdgrp lng="id">
    <trd>berahi</trd>,
    <trd>gairah</trd>,
    <trd>hasrat</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>passie</trd>,
    <trd>hartstocht</trd>
  </trdgrp>
  <trd lng="pl">nami&eogonek;tno&sacute;&cacute;</trd>
  <trd lng="pt">paix&atilde;o</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">v&aacute;&scaron;e&ncaron;</trd>
  <trd lng="sv">passion</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x441;&#x442;&#x440;&#x430;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x437;&#x430;&#x445;&#x43E;&#x43F;&#x43B;&#x435;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x437;&#x430;&#x43F;&#x430;&#x43B;</trd>,
    <trd>&#x430;&#x437;&#x430;&#x440;&#x442;</trd>
  </trdgrp>
</drv>

<drv mrk="pasi.0a">
  <kap><tld/>a</kap>
  <snc mrk="pasi.0a.posedata">
    <dif>
      Plena de <tld/>o:
      <ekz>
        ili fari&gcirc;is kiel voluptemaj <tld/>aj &ccirc;evaloj
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 5:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun formetu <klr>[vian]</klr> <tld/>an indignon
        <fnt>
          <bib>NT</bib>
          <lok>Koloseanoj 3:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> a&ubreve;dis la <tld/>ajn strofojn de la Greko
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kion &scirc;i devas fari, se &scirc;in kaptas <tld/>a deziro vivi
        <fnt>
          <bib>Marta</bib>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="emoci.0a">emocia</ref>,
       <ref cel="entuzi.0a">entuziasma</ref>,
       <ref cel="fervor.0a">fervora</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_r;&c_s;&c_n;&c_y;</trd>
  <trd lng="ca">apassionat</trd>
  <trd lng="cs">v&aacute;&scaron;niv&yacute;</trd>
  <trd lng="de">leidenschaftlich</trd>
  <trd lng="es">apasionado</trd>
  <trd lng="fr">passionn&eacute;</trd>
  <trd lng="id">ber<ind>gairah</ind></trd>
  <trdgrp lng="nl">
     <trd>passioneel</trd>,
     <trd>hartstochtelijk</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>pe&lstroke;en <ind>pasji</ind></trd>,
     <trd>zapalony</trd>
   </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>n&aacute;ru&zcaron;iv&yacute;</trd>,
    <trd>ohniv&yacute;</trd>,
    <trd>v&aacute;&scaron;niv&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x432;&#x437;&#x44F;&#x442;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x441;&#x442;&#x440;&#x430;&#x441;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="pasi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a (pri io):
      <ekz>
        tamen, ju pli la libido <tld/>as je io <klr>&#x2026;</klr> des pli
        &gcirc;i malforti&gcirc;as pri aliaj aferoj
	<fnt>
	  <aut>Platono, tr. D. Broadribb</aut>:
	  <vrk>La Respubliko</vrk>. <lok>Parto 6a</lok>.
	</fnt>;
      </ekz><ekz>
        Tro bone, kiom la virin' <tld/>as! //
        Ili fidelas same kiel ni
        <fnt><aut>&Scirc;ekspiro, tr. W. Auld</aut>:
          <vrk>La Festo de la Tri Re&gcirc;oj a&ubreve;, Kiel Vi Volas</vrk>.
          <lok>2:5. En: The British Esperantist, novembro 1967.</lok>
        </fnt>;
      </ekz><ekz>
        li proponas al tiuj, kiuj <tld/>as pri horoskopoj, legi ne nur
        la hodia&ubreve;an, sed anka&ubreve; la pasintsemajnan horoskopon
        <fnt><vrk>Astrologoj progresas</vrk>. <bib>Monato</bib>.
          <lok>2006</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ard.0i.FIG">ardi<sncref/></ref>,
       <ref cel="brul.0i.FIG">bruli<sncref/></ref>,
       <ref cel="flam.0i">flami</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_c;&c_c;&c_a; <klr>(&c_zh;&c_a;&c_r;&c_s;&c_mol;&c_c;&c_ju;)</klr></trd>
  </trdgrp>
  <trd lng="de">sich <ind>verzehren</ind></trd>
  <trdgrp lng="ru">
     <trd>&c_u;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol;, &c_p;&c_y;&c_l;&c_a;&c_t;&c_mol; <ind>&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_mol;&c_ju;</ind></trd>
   </trdgrp>
</drv>

</art>
