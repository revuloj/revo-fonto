<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vina</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="vina.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Sudhinda <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>,
      granda liuto kun longa kolo,
      4 &ccirc;efaj, 3 ritmaj, pin&ccirc;e sonigataj kordoj, 24 fretoj,
      du sonkestoj, el kiuj la pli eta fiksitas supre &ccirc;e la kolo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>sarasvat&icirc; <ind>v&icirc;n&acirc;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vina</trd>,
    <trd>cs&odblac;citera</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_a;<klr tip="amb"> (&c_m;&c_u;&c_z;. &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
</drv>
</art>

