<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vina.xml,v 1.12 2023/11/25 18:29:23 revo Exp $">
<kap>
  <rad>vina</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="vina.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Sudhinda <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>,
      granda liuto kun longa kolo,
      4 &ccirc;efaj, 3 ritmaj, pin&ccirc;e sonigataj kordoj, 24 fretoj,
      du sonkestoj, el kiuj la pli eta fiksitas supre &ccirc;e la Kolo:
      <ekz>
        Rudra <tld/>o el Norda Barato
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=575602">Vinao</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/3/35/Veena.png/600px-Veena.png" prm="CC BY 3.0">desegno de <tld/>o la&ubreve;
    malnova foto el persona kolekto
        <fnt>
          <aut>Sreejithk2000 el: Gringer (talk)</aut>
          <lok><url ref="&WCW;/File:Veena.png">&WCW;/File:Veena.png</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_n;&c_a; <klr tip="amb">(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;)</klr></trd>
  <trd lng="ca">vina</trd>
  <trd lng="cs">v&iacute;na</trd>
  <trdgrp lng="de">
    <trd>Vina</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>sarasvat&icirc; <ind>v&icirc;n&acirc;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vina</trd>,
    <trd>cs&odblac;citera</trd>
  </trdgrp>
  <trd lng="nl">vina</trd>
  <trd lng="ru">&c_v;&c_i;&c_n;&c_a;<klr tip="amb"> (&c_m;&c_u;&c_z;.
  &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
  <trd lng="sk">v&iacute;na</trd>
  <trd lng="uk">&c_v;&c_i;&#x43D;&c_a; (&#x43C;&c_u;&c_z;. &c_ib;&#x43D;&c_s;&c_t;&c_r;&c_u;&#x43C;&c_je;&#x43D;&c_t;)</trd>
</drv>
</art>

