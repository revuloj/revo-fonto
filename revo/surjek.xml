<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>surjekci</rad>/o
</kap>
<drv mrk="enjekc.0o">
  <kap><tld/>o
  <fnt><bib>MatVort</bib></fnt>
  </kap>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <ref tip="dif" cel="jxet.sur0o">sur&jcirc;eto.</ref>
  </snc>
  <trd lng="en">surjection</trd>
  <trd lng="de">Surjektion</trd>
  <trdgrp lng="fr">
    <trd>surjection</trd>,
    <trd>application surjective</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_ju;&c_r;&c_malmol;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
</drv>
<drv mrk="surjekc.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(p.p. <ref cel="bild.0igo">bildigo</ref>)</klr>
      Havanta la ecojn de <tld/>o:
      <ekz>
        la bildigo, kiu &jcirc;etas reelon al &gcirc;ia kvadrato ne estas
        <tld/>a.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">surjective</trd>
  <trd lng="de">surjektiv</trd>
  <trd lng="fr">surjectif</trd>
  <trd lng="ru">&c_s;&c_ju;&c_r;&c_malmol;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>
</art>

<!--
