<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>surjekci</rad>/o
</kap>
<drv mrk="surjek.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <ref tip="dif" cel="jxet.sur0o">Sur&jcirc;eto.</ref>
  </snc>
  <trd lng="en">surjection</trd>
  <trd lng="de">Surjektion</trd>
  <trdgrp lng="fr">
    <trd>surjection</trd>,
    <trd>application surjective</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je; "&c_n;&c_a;"</trd>,
    <trd>&c_s;&c_ju;&c_r;&c_malmol;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="surjek.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="bild.0igo.MAT">bildigo</ref>)</klr>
      Havanta la ecojn de <tld/>o:
      <ekz>
        la bildigo, kiu &jcirc;etas reelon al &gcirc;ia kvadrato ne estas
        <tld/>a.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">surjective</trd>
  <trd lng="de">surjektiv</trd>
  <trd lng="fr">surjectif</trd>
  <trd lng="ru">&c_s;&c_ju;&c_r;&c_malmol;&c_je;&c_k;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
