<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>trumpet</rad>/o
</kap>
<drv mrk="trumpe.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <uzo tip="fak">MUZ</uzo>
     <dif>
       Metala senklava <ref tip="super"
       cel="instru2.blov0o">blovinstrumento</ref>.
     </dif>
     <refgrp tip="sin">
       <ref cel="korn.0o">korno</ref>,
       <ref cel="klario.0o">klariono</ref>,
       <ref cel="trombo.0o">trombono</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_t;&c_r;&c_u;&c_b;&c_a; 
<klr>(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
   <trd lng="de">Trompete</trd>
   <trd lng="en">trumpet</trd>
   <trd lng="hu">trombita</trd>   
   <trd lng="nl">trompet</trd>
   <trd lng="fr">trompette</trd>
   <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_a;</trd>
</drv>

<drv mrk="trumpe.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <uzo tip="fak">MUZ</uzo>
     <dif>
       Ludi <tld/>on.
     </dif>
   </snc>
   <trd lng="be">&c_t;&c_r;&c_u;&c_b;&c_ib;&c_c;&c_mol;</trd>
   <trd lng="de">trompeten</trd>
   <trdgrp lng="fr"><trd>jouer de la 
<ind>trompette</ind></trd><trd>sonner de la 
<ind>trompette</ind></trd></trdgrp>
   <trd lng="hu">trombit&aacute;l</trd>
   <trd lng="nl">trompetspelen</trd>
   <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="trumpe.dis0i">
   <kap>dis<tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Blue disanonci, disfamigi:
       <ekz>
         niaj kontra&ubreve;uloj dis<tld/>ados al la mondo, ke la delegacio
         konsistis el maskitaj esp-istoj, kiuj aran&gcirc;is 
komedion<fnt>Z</fnt>
         (<refgrp tip="sin">
           <ref cel="tamtam.0i">tamtami</ref>,
           <ref cel="bluf.0i">blufi</ref>
         </refgrp>).
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_r;&c_a;&c_s;&c_t;&c_r;&c_u;&c_b;&c_ib;&c_c;&c_mol;</trd>
   <trd lng="de">ausposaunen</trd>
   <trd lng="hu">sz&eacute;tk&uuml;rt&ouml;l</trd>   
   <trd lng="nl">uitbazuinen</trd>
   <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_r;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
