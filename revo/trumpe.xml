<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: trumpe.xml,v 1.33 2024/05/10 10:43:47 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>trumpet</rad>/o
</kap>
<drv mrk="trumpe.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="trumpe.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Brilsona latuna <ref tip="lst" cel="instru2.blov0o" lst="voko:blovinstrumentoj">blovinstrumento</ref> kun longa
      ordinare kunrulita tubo, senvalva a&ubreve; kun tri valvoj:
      <ekz>
        faru al vi du ar&gcirc;entajn <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 10:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        se la <tld/>o donos necertan vo&ccirc;on, kiu preti&gcirc;os por
        batalo
        <fnt><bib>NT</bib>, <lok>&Ko1; 14:8</lok></fnt>?
      </ekz>
      <ekz>
        sonoris kornetoj, <tld/>oj kaj flutoj
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="korn.cxas0o">&ccirc;askorno</ref>,
      <ref cel="kornet.0o.blovinstrumento">korneto<sncref/></ref>,
      <ref cel="trombo.0o">trombono</ref>,
      <ref cel="tubj.0o">tubjo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_u;&c_b;&c_a; 
<klr>(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="ca">trompeta</trd>
  <trdgrp lng="cs">
    <trd>trouba</trd>,
    <trd>trubka</trd>,
    <trd>trumpeta</trd>
  </trdgrp>
  <trd lng="de">Trompete</trd>
  <trd lng="en">trumpet</trd>
  <trd lng="fr">trompette</trd>
  <trd lng="hu">trombita</trd>
  <trd lng="id">trompet</trd>
  <trd lng="io">trumpeto</trd>
  <trd lng="it">tromba</trd>
  <trdgrp lng="ja">
    <trd>&#x3089;&#x3063;&#x3071;</trd>,
    <trd>&#x30C8;&#x30E9;&#x30F3;&#x30DA;&#x30C3;&#x30C8;</trd>
  </trdgrp>
  <trd lng="nl">trompet</trd>
  <trdgrp lng="pl">
    <trd>tr&aogonek;ba</trd>,
    <trd>tr&aogonek;bka</trd>
  </trdgrp>
  <trd lng="pt">trompete</trd>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_a;<klr tip="ind">
  (&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;
  &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
  <trd lng="sk">tr&uacute;bka</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_r;&c_m;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5C0F;&#x53F7; <pr>xi&#x1CE;oh&#xE0;o</pr></trd>,
    <trd>&#x5C0F;&#x865F; <pr>xi&#x1CE;oh&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="trumpe.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trumpe.0i.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="lud.0i.MUZ">Ludi</ref> per <tld/>o; signali per <tld/>sono:
      <ekz>
        du ar&gcirc;entaj <tld/>oj <klr>[&#x2026;]</klr> servu al vi por kunvokado <klr>[&#x2026;]</klr>,
        kiam oni simple <tld/>os per ili, tiam kolekti&gcirc;u al vi la tuta komunumo
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 10:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la gardostaranto de la turo <klr>[&#x2026;]</klr> <tld/>is melankolian melodion
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="trumpe.0i.signali">
    <dif>
      <ref tip="super" cel="signal.0i">Signali</ref> <tld/>sone,
      i.a. per <ref cel="korn.0o.MUZ">korno<sncref/></ref>:
      <ekz>
        oni <tld/>is por batalo
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 17:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ek<tld/>is per korno sur la monto
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 3:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial vi <tld/>as alarmon, anstata&ubreve; mar&scirc;i
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>?
      </ekz>
      <ekz>
        la &scirc;ipo komencis <tld/>i sian potencan signalon
        <fnt>
          <bib>FdO</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="alarm.0i">alarmi</ref>,
      <ref cel="vok.kun0i">kunvoki</ref>  
    </refgrp>
  </snc>
  <snc mrk="trumpe.0i.disanonci">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Disanonci, <ref cel="trumpe.dis0i">dis<tld/>i</ref>:
      <ekz>
        &ccirc;u vi volas ku&scirc;i en la malliberejo <klr>[&#x2026;]</klr> &gcirc;is oni <tld/>os la tagon de
        lasta ju&gcirc;o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>?
      </ekz>
      <ekz>
        <tld/>i pri <klr>[invento]</klr> al la leganta mondo &mdash; &ccirc;io tio &ccirc;i estas tre
        facila
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        simio staris sur alta traba&jcirc;o, energie <tld/>is pri sia propra lerteco
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> sen&ccirc;ese <tld/>is en niajn orelojn <klr>[sian reklamon]</klr>
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>El Odeso &gcirc;is Konstantinopolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fanfar1.0i">fanfaroni</ref>,
      <ref cel="propag.0i">propagandi</ref>
    </refgrp>
    <trd lng="de">verk&uuml;nden</trd>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_u;&c_b;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="ca">tocar la trompeta</trd>
  <trd lng="de">trompeten</trd>
  <trd lng="en">play trumpet</trd>
  <trdgrp lng="fr">
    <trd>jouer de la <ind>trompette</ind></trd>,
    <trd>sonner de la <ind>trompette</ind></trd>
  </trdgrp>
  <trd lng="hu">trombit&aacute;l</trd>
  <trdgrp lng="io">
    <trd>trumpetagar</trd>,
    <trd>plear trumpeto</trd>
  </trdgrp>
  <trd lng="it">suonare con la <ind>tromba</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x3089;&#x3063;&#x3071;&#x3092;&#x5439;&#x304F;  <pr>&#x3089;&#x3063;&#x3071;&#x3092;&#x3075;&#x304F;</pr></trd>,
    <trd>&#x30C8;&#x30E9;&#x30F3;&#x30DA;&#x30C3;&#x30C8;&#x3092;&#x5439;&#x304F;  <pr>&#x30C8;&#x30E9;&#x30F3;&#x30DA;&#x30C3;&#x30C8;&#x3092;&#x3075;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>trompetten</trd>,
    <trd>trompet spelen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tr&aogonek;bi&cacute;</trd>,
    <trd>zatr&aogonek;bi&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">tr&uacute;bi&tcaron;</trd>
  <trd lng="cs">troubit</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_r;&c_m;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_b;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="trumpe.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trumpe.dis0i.FIG">
    <dif>
      Brue dis<ref tip="super" cel="anonc.0i.publike">anonci</ref>, 
      <ref cel="fam.dis0igi">disfamigi</ref>:
      <ekz>
        fiera postiljono de sur pompa kale&scirc;o kornotone dis<tld/>is sian alvenon
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2013/010812.php">Po&scirc;to perdita</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        li dis<tld/>is, ke mi a&ccirc;etis la lo&gcirc;ejon el mono, akirita per banditismo
        <fnt>
          <bib>SatirRak</bib>
          <lok>La fluidumo de la &ccirc;iovido</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u ne &ccirc;iuj, anka&ubreve; Usono kaj Ruslando, dis<tld/>as sian pacemon
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007173.php">Morti pro presti&gcirc;o</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 5</lok>
          </fnt>?
      </ekz>
      <ekz>
        li dis<tld/>is mensogojn pri <klr>[la]</klr> filo de la murdita pakistana
        eks&ccirc;efministro
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009430.php">Fido kaj skeptiko</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kon.dis0igi">diskonigi</ref>,
      <ref cel="fanfar1.0i">fanfaroni</ref>,
      <ref cel="klacx.0i">kla&ccirc;i</ref>,
      <ref cel="fam.mis0igi">misfamigi</ref>,
      <ref cel="prokla.0i">proklami</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_t;&c_r;&c_u;&c_b;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="ca">vilipendiar p&uacute;blicament</trd>
  <trd lng="cs">vytroubit</trd>
  <trd lng="de">ausposaunen</trd>
  <trdgrp lng="en">
    <trd>trumpet</trd>,
    <trd>herald</trd>,
    <trd>proclaim loudly</trd>
  </trdgrp>
  <trd lng="fr">claironner<klr tip="amb"> (fig.)</klr></trd>
  <trd lng="hu">sz&eacute;tk&uuml;rt&ouml;l</trd>
  <trdgrp lng="it">
    <trd>strombazzare</trd>,
    <trd>spifferare</trd>,
    <trd>spiattellare</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3089;&#x3063;&#x3071;&#x3092;&#x5439;&#x3044;&#x3066;&#x77E5;&#x3089;&#x305B;&#x308B;  <pr>&#x3089;&#x3063;&#x3071;&#x3092;&#x3075;&#x3044;&#x3066;&#x3057;&#x3089;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x5439;&#x8074;&#x3059;&#x308B;  <pr>&#x3075;&#x3044;&#x3061;&#x3087;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5927;&#x9A12;&#x304E;&#x3067;&#x8A00;&#x3044;&#x3075;&#x3089;&#x3059;  <pr>&#x304A;&#x304A;&#x3055;&#x308F;&#x304E;&#x3067;&#x3044;&#x3044;&#x3075;&#x3089;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>rondbazuinen</trd>,
    <trd>uitbazuinen</trd>
  </trdgrp>
  <trd lng="pl">roztr&aogonek;bi&cacute;</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_r;&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>rozhl&aacute;si&tcaron;</trd>,
    <trd>roztr&uacute;bi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_t;&c_r;&c_u;&c_b;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_d;&c_z;&c_v;&c_o;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
