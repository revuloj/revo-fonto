<art mrk="$Id: gicxet.xml,v 1.22 2026/02/15 03:20:15 revo Exp $">
<kap>
  <ofc>9</ofc><rad>gi&ccirc;et</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="gicxet.0o">
  <kap><tld/>o</kap>
  <snc mrk="gicxet.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Fenestreto en pordo, mureto, destinita por interparoli
      a&ubreve; pasigi ion:
      <ekz>
        la ir-kaj-reveno de pladoj kaj cetera man&gcirc;ilaro okazis tra
        <tld/>o tiucele aran&gcirc;ita en la pordeto
        <fnt><bib>KielAkvo</bib>, <lok>1:2</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="luk.0o">luko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_je;&c_n;&c_c;&c_a;</trd>
  <trd lng="bg">&c_g;&c_i;&c_sh;&c_je;</trd>
  <trdgrp lng="de">
     <trd>Fensterchen</trd>,
     <trd>Guckfensterchen</trd>,
     <trd>Durchreiche</trd>,
     <trd>Schalter</trd>,
     <trd>Abfertigungsschalter</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ticket <ind>window</ind></trd>,
    <trd>hatch</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ventanilla</trd>,
    <trd>taquilla</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>guichet</trd>,
    <trd>passe-plat</trd>
  </trdgrp>
  <trd lng="hu">p&eacute;nzt&aacute;rablak</trd>
  <trdgrp lng="ja">
    <trd>&#x7A93;&#x53E3;  <pr>&#x307E;&#x3069;&#x3050;&#x3061;</pr></trd>,
    <trd>&#x5207;&#x7B26;&#x58F2;&#x308A;&#x5834;  <pr>&#x304D;&#x3063;&#x3077;&#x3046;&#x308A;&#x3070;</pr></trd>
  </trdgrp>
  <trd lng="nl">loket</trd>
  <trd lng="pl">okienko <klr>(urz&aogonek;d,kasa,sto&lstroke;&oacute;wka)</klr></trd>
  <trd lng="pt">guich&ecirc;</trd>
  <trd lng="ru">&c_o;&c_k;&c_o;&c_sh;&c_k;&c_o;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_k;&c_o;&c_n;&c_c;&c_je;&nbsp;(&c_k;&c_a;&c_s;&c_i; &c_ib; &c_t;. &c_p;.)</trd>,
    <trd>&c_k;&c_v;&c_a;&c_t;&c_i;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
