<drv mrk="arb.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="arb.0a.rilata">
    <dif>
      De <tld/>o, rilata al <tld/>o:
      <ekz>
        <tld/>a verda&jcirc;o;
      </ekz>
      <ekz>
        <tld/>a ombro;
      </ekz>
      <ekz>
        <tld/>an frukto, kiu semas semon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 1:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        pom<tld/>a flora&jcirc;o, kiun la vento forpelas
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi rigardis la fendojn de la <tld/>a &scirc;elo
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_e;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>arb&oacute;reo</trd>,
    <trd>del &aacute;rbol</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_je;&c_r;&c_je;&c_v;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="arb.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="arb.0ajxo.dehakitaj">
    <dif>
      Dehakitaj a&ubreve; defalintaj arboj a&ubreve; arbo- kaj arbustopartoj:
      <ekz>
        bruligi <tld/>a&jcirc;on;
      </ekz>
      <ekz>
        &leftquot;la kale&scirc;o veturas tra l' <tld/>a&jcirc;o&rightquot;, ekkriis <nom>Britain</nom>
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="lign.0o">ligno</ref>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_o;&c_v;&c_y;</trd>
