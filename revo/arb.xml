<drv mrk="arb.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="arb.0eto.arbo">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">HOR</uzo>
    <dif>
      Malgranda <tld/>o:
      <ekz>
        ofte alkuradis leporo kaj saltadis pluen &gcirc;uste trans
        la <tld/>eton
        <fnt><bib>Fab2</bib>, <lok>abio</lok></fnt>;
      </ekz>
      <ekz>
        granata <tld/>eto
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mimozo-<tld/>etoj
        <fnt>
       <aut>Thomas Eccardt</aut>
          <vrk><url ref="&Monato;/2000/006859.php">Urbeto a&ubreve; sendependa lando?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &leftquot;sova&gcirc;a <tld/>aro&rightquot; <klr>[&#8230;]</klr> konsistis el multaj
        <tld/>etoj: interalie
        kverkoj, fagoj, fraksenoj, betuloj, salikoj
        <fnt>
          <aut>M&aacute;ire NicAoidh</aut>
          <vrk><url ref="&Monato;/2000/006878.php">&Gcirc;ardenumi</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 20</lok>
          </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_r;&c_e;&c_w;&c_c;&c_a;</trd>
    <trd lng="de">B&auml;umchen</trd>
    <trd lng="es">&aacute;rbol</trd>
    <trd lng="fa">&a_n;&a_h;&a_A;&a_l;</trd>
