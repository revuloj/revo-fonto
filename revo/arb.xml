<drv mrk="arb.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="arb.0eto.arbo">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">HOR</uzo>
    <dif>
      Malgranda <tld/>o:
      <ekz>
        ofte alkuradis leporo kaj saltadis pluen &gcirc;uste trans
        la <tld/>eton
        <fnt><bib>Fab2</bib>, <lok>abio</lok></fnt>;
      </ekz>
      <ekz>
        granata <tld/>eto
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mimozo-<tld/>etoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006859.html">Seborgo:
          Urbeto a&ubreve; sendependa lando?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &leftquot;sova&gcirc;a <tld/>aro&rightquot; <klr>[&#8230;]</klr> konsistis el multaj
        <tld/>etoj: interalie
        kverkoj, fagoj, fraksenoj, betuloj, salikoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006878.html">Dommastrumo:
          &Gcirc;ardenumi</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_r;&c_e;&c_w;&c_c;&c_a;</trd>
    <trd lng="de">B&auml;umchen</trd>
    <trd lng="es">&aacute;rbol</trd>
    <trd lng="fa">&a_n;&a_h;&a_A;&a_l;</trd>
    <trd lng="fy">beamke</trd>
    <trd lng="nl">boompje</trd>
    <trd lng="pl">drzewko</trd>
    <trd lng="ru">&c_d;&c_je;&c_r;&c_je;&c_v;&c_c;&c_je;</trd>
  </snc>
  <snc mrk="arb.0eto.arbusto">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">HOR</uzo>
    <dif>
      <ref tip="sin" cel="arbust.0o">Arbusto</ref> a&ubreve;
      <ref tip="sin" cel="arbed.0o">arbedo</ref>:
      <ekz>
        la siring<tld/>etoj odoris kaj klinis siajn longajn
        verdajn bran&ccirc;ojn al la kviete serpentumantaj
        riveretoj
        <fnt><bib>Fab2</bib>, <lok>malbela anasido</lok></fnt>;
      </ekz>
      <ekz>
        la &ccirc;evalo <klr>[&#8230;]</klr> movetadis la longajn trunketojn de la
        rubus<tld/>eto tiel ke la maturaj sukoplenaj fruktoj falis
        <fnt>
          <bib>Fab4</bib>,
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_u;&c_s;&c_t;</trd>
    <trd lng="de">Strauch</trd>
    <trd lng="en">bush</trd>
    <trd lng="es">arbusto</trd>
    <trd lng="fa">&a_b;&a_w;&a_t;&a_h;</trd>
    <trd lng="fy">str&uacute;k</trd>
    <trd lng="nl">struik</trd>
    <trd lng="no">busk</trd>
    <trdgrp lng="pl">
      <trd>krzak</trd>,
      <trd>krzew</trd>,
      <trd>krzewina</trd>,
      <trd>kierz<klr> (dawn.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_u;&c_s;&c_t;</trd>
  </snc>
  <trd lng="br">brousgwezenn</trd>
  <trd lng="ca">arbust</trd>
  <trdgrp lng="cs">
    <trd>stromek</trd>,
    <trd>strome&ccaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Strauch</trd>,
    <trd>Busch</trd>
  </trdgrp>
  <trd lng="fr">arbuste</trd>
  <trd lng="he">&#x5E2;&#x5E5; &#x5D2;&#x5DE;&#x5D3;&#x5D9;</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_c;&c_je;</trd>,
    <trd>&c_k;&c_u;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">strom&ccaron;ek</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_c;&c_je;</trd>,
    <trd>&#x43A;&c_u;&c_shch;</trd>
  </trdgrp>
</drv>

<drv mrk="arb.0etaro">
