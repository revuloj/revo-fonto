<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>arb</rad>/o
    </kap>
    <drv mrk="arb.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="arb.0o.BOT">
        <uzo tip="fak">BOT</uzo>
        <dif>
          Ligna <ref tip="super" cel="plant.dauxra0o">da&ubreve;ra
            planto</ref> kun alta trunko supre disbran&ccirc;i&gcirc;anta:
          <ekz>
            pino, tilio, kverko, platano, pomujo, &ccirc;erizujo
            estas <tld/>oj.
          </ekz>
        </dif>
      </snc>
      <snc mrk="arb.0o.MAT">
        <uzo tip="fak">MAT</uzo>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Finia aro, en kiu unu elementon oni rigardas <em>radiko</em>,
          kaj la ceteraj formas disajn arojn (nomatajn <em>subarboj</em>),
          &ccirc;iu el kiuj mem estas <tld/>o:
          <ekz>
            la <ref cel="grafei.0o">grafeika</ref> terminaro
            pri la <tld/>oj estas &ccirc;erpita el genealogio
            (praulo, posteulo, patro, ido, frato) kaj el botaniko
            (radiko, folio);
          </ekz><ekz>
            en grafeiko oni apartigas orientitajn, ordajn <tld/>ojn;
          </ekz><ekz>
            senradika <tld/>o
            <klr>(grafeo kiu i&gcirc;as <tld/>o se iun &gcirc;ian verticon oni
              deklaras radiko)</klr>,
            radikhava <tld/>o
            <klr>(proprasenca <tld/>o kontraste al senradika)</klr>;
          </ekz><ekz>
            <uzo tip="fak">KOMP</uzo>
            duuma <tld/>o, B-<tld/>o, deriv<tld/>o, ser&ccirc;<tld/>o.
          </ekz>
        </dif>
        <refgrp tip="super">
          <ref cel="grafe.0o">grafeo</ref>,
          <ref cel="klasif.0o">klasifiko</ref>,
          <ref cel="hierar.0o">hierarkio</ref>,
          <ref cel="datum.0strukturo">datumstrukturo</ref>.
        </refgrp>
        <rim>
          Oni ofte prezentas <tld/>on per grafeo simila al genealogia
          <tld/>o; tia grafeo povas esti <em>alradika</em>
          <klr>(ekz-e
            genealogia <tld/>o prezentanta la praulojn de koncerna
            individuo)</klr>
          a&ubreve; <em>elradika</em>
          <klr>(ekz-e genealogia <tld/>o prezentanta la posteularon
            de koncerna individuo)</klr>.
        </rim>
      </snc>
      <trd lng="cs">strom</trd>
      <trd lng="de">Baum</trd>
      <trd lng="en">tree</trd>
      <trd lng="fr">arbre</trd>
      <trd lng="ru">&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
      <trd lng="sv">tr&auml;d</trd>
    </drv>
    <drv mrk="arb.0aro">
      <kap><tld/>aro</kap>
      <snc>
        <dif>
          Areo, kie dense kreskas 
          <ref tip="prt" cel="arb.0o"><tld/>oj</ref>.
        </dif>
      </snc>
      <trd lng="de">Wald</trd>
      <trd lng="en">wood</trd>
      <trd lng="fr">for&ecirc;t</trd>
      <trd lng="ru">&c_l;&c_je;&c_s;</trd>
      <trd lng="sv">skog</trd>
    </drv>
    <drv mrk="arb.sub0o">
      <kap>sub<tld/>o</kap>
      <snc>
        <uzo tip="fak">MAT</uzo>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          <ref tip="malprt" cel="arb.0o.MAT">Arbo</ref> konsiderata
          kiel parto de alia arbo.
        </dif>
      </snc>
      <trd lng="de">Teilbaum</trd>
      <trd lng="en">subtree</trd>
      <trd lng="ru">&c_p;&c_o;&c_d;&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
      <trd lng="sv">deltr&auml;d</trd>
    </drv>
    <drv mrk="arb.0eto">
      <kap><tld/>eto</kap>
      <snc mrk="arb.0eto.arbo">
        <dif>
          Malgranda arbo:
          <ekz>
            ofte alkuradis leporo kaj saltadis pluen &gcirc;uste trans
            la arbeton
            <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
                Abio</vrk>)</fnt>.
          </ekz>
        </dif>
        <trd lng="de">B&auml;umchen</trd>
      </snc>
      <snc mrk="arb.0eto.arbusto">
        <dif>
          <ref tip="sin" cel="arbust.0o">Arbusto</ref> a&ubreve;
          <ref tip="sin" cel="arbed.0o">arbedo</ref>:
          <ekz>
            la siring<tld/>etoj odoris kaj klinis siajn longajn
            verdajn bran&ccirc;ojn al la kviete serpentumantaj
            riveretoj
            <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
                Malbela anasido</vrk>)</fnt>;
          </ekz><ekz>
            la &ccirc;evalo <klr>[...]</klr> movetadis la longajn
            trunketojn de la rubus<tld/>eto tiel ke la maturaj sukoplenaj
            fruktoj falis
            <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
                Filino de la mar&ccirc;a re&gcirc;o</vrk>)</fnt>.
          </ekz>     
        </dif>
        <trd lng="de">Strauch</trd>
      </snc>
      <trdgrp lng="ru">
        <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_c;&c_je;</trd>,
        <trd>&c_k;&c_u;&c_s;&c_t;</trd>
      </trdgrp>
    </drv>
    <drv mrk="arb.0etaro">
      <kap><tld/>etaro</kap>
      <snc>
        <dif>
          Kolekto da <tld/>etoj kune kreskantaj.
        </dif>
      </snc>
      <trd lng="de">Geb&uuml;sch</trd>
      <trd lng="ru">&c_k;&c_u;&c_s;&c_t;&c_a;&c_r;&c_n;&c_i;&c_k;</trd>
    </drv>
    <drv mrk="arb.0etajxo">
      <kap><ofc>*</ofc><tld/>eta&jcirc;o</kap>
      <snc>
        <dif>
          Arbustotufo, vepro.
        </dif>
      </snc>
    </drv>
  </art>
