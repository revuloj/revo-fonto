<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: delfen1.xml,v 1.11 2022/03/15 15:50:54 revo Exp $">
<kap>
  <rad>delfened</rad>/oj
</kap>
<drv mrk="delfen1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref>
      el la ordo <ref tip="malprt" cel="cetac.0oj">cetacoj</ref>
      (<trd lng="la">Delphinidae</trd>):
      <ekz>
        dentocetacoj formas subordon de cetacoj, <klr>[&#x2026;]</klr> multaj havas e&ccirc; &gcirc;is 100
        dentojn kiel ekzemple <tld/>oj, male al ili narvalo havas nur unu longan
        pu&scirc;denton
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50889">Dentocetacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="delfen.0o">delfeno</ref>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_l;&c_mol;&c_f;&c_ib;&c_n;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Delphine <klr tip="ind">(Familie)</klr></trd>
  <trd lng="fr">delphinid&eacute;s</trd>
  <trd lng="he">&#x5D3;&#x5D5;&#x5DC;&#x5E4;&#x5D9;&#x5E0;&#x5D9;&#x5DD;</trd>
  <trd lng="hu">delfinf&eacute;l&eacute;k</trd>
  <trd lng="pt">delfin&iacute;deos</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_mol;&c_f;&c_i;&c_n;&c_o;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_mol;&c_f;&c_i;&c_n;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x434;&#x435;&#x43B;&#x44C;&#x444;&#x456;&#x43D;&#x43E;&#x432;&#x456;</trd>
</drv>
</art>

