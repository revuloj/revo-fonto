<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: proces.xml,v 1.33 2026/02/14 13:11:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>proces</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="proces.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="proces.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="super" cel="afer.jugx0o">Ju&gcirc;afero</ref> jure
      submetita al <ref cel="tribun1.0o">tribunalo</ref>:
      <ekz>
        esti prava en sia <tld/>o
	<fnt><bib>MT</bib>, <lok>&Sen; 18:17</lok></fnt>;
      </ekz>
      <ekz>
        ju&gcirc;a afero inter sango kaj sango, inter <tld/>o kaj <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 17:8</lok>
        </fnt>;
      </ekz><ekz>
        oni demandis pentriston, kiel oni povas <klr>[&#x2026;]</klr>
        prezenti du <tld/>antojn, el kiuj unu gajnis la <tld/>on
        kaj la dua &gcirc;in perdis
        <fnt>
          <bib>FK</bib>,
          <lok>III. Anekdotoj</lok>
        </fnt>;
      </ekz><ekz>
        komenci&gcirc;is <tld/>o, mono fluas sen &ccirc;eso
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="en">lawsuit</trd>
    <trd lng="fr">proc&egrave;s</trd>
    <trdgrp lng="hu">
      <trd>per</trd>,
      <trd>p&ouml;r</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>dakwa</trd>,
      <trd>dakwaan</trd>,
      <trd>gugatan</trd>,
      <trd>kasus hukum</trd>,
      <trd>perkara hukum</trd>,
      <trd>tuntutan</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>proces</trd>,
      <trd>rechtsgeding</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>proces</trd>,
      <trd>rozprawa s&aogonek;dowa</trd>
    </trdgrp>
    <trd lng="pt">processo</trd>
    <trd lng="ro">proces</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;<klr> (&c_s;&c_u;&c_d;&c_je;&c_b;&c_n;&c_y;&c_j;)</klr></trd>,
      <trd>&c_t;&c_ja;&c_zh;&c_b;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x5B98;&#x53F8; <pr>gu&amacron;nsi</pr></trd>,
    <trd>&#x8A34;&#x8A1F; <pr>s&ugrave;s&ograve;ng</pr></trd>,
    <trd>&#x8BC9;&#x8BBC; <pr>s&ugrave;s&ograve;ng</pr></trd>,
    <trd>&#x8A1F; <pr>s&ograve;ng</pr></trd>,
    <trd>&#x8BBC; <pr>s&ograve;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="proces.0o.SCI">
    <uzo tip="stl">ARK</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="dif" cel="procez.0o.SCI">Procezo</ref>:
      <ekz>
        tiu <tld/>o de unuformi&gcirc;ado iros en &gcirc;i
        <klr>(en la lingvo internacia)</klr>
        multe pli rapide
        <fnt>
          <aut>&Z;</aut>:
          <vrk>Aldono al la Dua Libro de l' Lingvo Internacia</vrk>.
        </fnt>;
      </ekz><ekz>
        senskue kaj regule da&ubreve;ras kaj disvolvi&gcirc;as tiu <tld/>o de la
        homa movi&gcirc;ado, pensado kaj sentado, kiu estas nomata vivo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="en">process</trd>
  <trd lng="id">proses</trd>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;</trd>
  <trd lng="bo">&t_kha;&t_tsheg;&t_ma;&t_cha;&t_shabchu;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>judicium</trd>,
    <trd>proces</trd>,
    <trd>soudn&iacute; p&rcaron;e</trd>,
    <trd>soudn&iacute; spor</trd>,
    <trd>soudn&iacute; &rcaron;&iacute;zen&iacute; <klr>(obecn&ecaron;)</klr> (z&aacute;va&zcaron;n&ecaron;j&scaron;&iacute;)</trd>,
    <trd>sporn&eacute; &rcaron;&iacute;zen&iacute; soudn&iacute;</trd>
  </trdgrp>
  <trd lng="de">Prozess</trd>
  <trd lng="es">proceso</trd>
  <trd lng="hu">folyamat</trd>
  <trd lng="ja">&#x8A34;&#x8A1F;  <pr>&#x305D;&#x3057;&#x3087;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>proces</trd>,
    <trd>verloop</trd>,
    <trd>ontwikkelingsgang</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>proces</trd>,
    <trd>przebieg zjawiska</trd>
  </trdgrp>
  <trd lng="ro">proces</trd>
  <trd lng="sk">najviac vy&ccaron;nievaj&uacute;ca &ccaron;as&tcaron; org&aacute;nu</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_o;&c_v;&c_a; &c_s;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_o;&c_s;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_z;&c_m;&c_ib;&c_n;&c_a; &c_p;&c_o;&c_d;&c_ib;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_n;&c_ib;&c_v;</trd>
  </trdgrp>
</drv>

<drv mrk="proces.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="proces.0i.jugxe">
    <dif>
      Ju&gcirc;e persekuti iun:
      <ekz>
        kiu povas <tld/>i kontra&ubreve; mi
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 13:19</lok>
        </fnt>?
      </ekz>
      <ekz>
        neniu <tld/>as honeste, kaj neniu ju&gcirc;as juste
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 59:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        frato <tld/>as kontra&ubreve; frato
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 6:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        se iu deziras <tld/>i kontra&ubreve; vi, por forpreni vian tunikon, lasu lin preni
        anka&ubreve; vian mantelon
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 5:40</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>to sue</trd>,
    <trd>litigate</trd>,
    <trd>bring suit</trd>
  </trdgrp>
  <trd lng="be">&c_s;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;t se <klr>(soudn&ecaron;)</klr></trd>,
    <trd>soudit se</trd>,
    <trd>soudn&ecaron; st&iacute;hat</trd>,
    <trd>v&eacute;st proces</trd>,
    <trd>v&eacute;st spor soudn&iacute;</trd>
  </trdgrp>
  <trd lng="de">prozessieren</trd>
  <trd lng="es">procesar</trd>
  <trd lng="fr">faire un proc&egrave;s</trd>
  <trdgrp lng="hu">
    <trd>pereskedik</trd>,
    <trd>beperel</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>gugat</trd>,
    <trd><baz>gugat</baz>menggugat</trd>,
    <trd><baz>perkara</baz>memerkarakan</trd>,
    <trd><baz>proses</baz>memproses scr hukum</trd>,
    <trd>tuntut</trd>,
    <trd><baz>tuntut</baz>menuntut</trd>
  </trdgrp>
  <trd lng="ja">&#x8A34;&#x8A1F;&#x3092;&#x8D77;&#x3053;&#x3059;  <pr>&#x305D;&#x3057;&#x3087;&#x3046;&#x3092;&#x304A;&#x3053;&#x3059;</pr></trd>
  <trd lng="nl">procederen</trd>
  <trd lng="pl">procesowa&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_s;&c_t;&c_i;, &c_v;&c_o;&c_z;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol;
      <ind>&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;</ind>
      <klr>(&c_s;&c_u;&c_d;&c_je;&c_b;&c_n;&c_y;&c_j;)</klr></trd>,
    <trd>&c_s;&c_u;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pravoti&tcaron; sa</trd>,
    <trd>vies&tcaron; spor</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_je;&c_s;&c_t;&c_i; &c_p;&c_r;&c_o;&c_c;&c_je;&c_s; (&c_u; &c_s;&c_u;&c_d;&c_ib;)</trd>
  <trdgrp lng="zh">
    <trd>&#x6253;&#x5B98;&#x53F8; <pr>d&#x1CE;gu&amacron;nsi</pr></trd>,
    <trd>&#x544A; <pr>g&agrave;o</pr></trd>,
    <trd>&#x72B6;&#x544A; <pr>zhu&agrave;ngg&agrave;o</pr></trd>,
    <trd>&#x72C0;&#x544A; <pr>zhu&agrave;ngg&agrave;o</pr></trd>,
    <trd>&#x8D77;&#x8A34; <pr>q&#x1D0;s&ugrave;</pr></trd>,
    <trd>&#x8D77;&#x8BC9; <pr>q&#x1D0;s&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="proces.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="proces.0ema.amanta">
    <dif>
      Amanta <tld/>i:
      <ekz>
        <klr>[ili]</klr> malamas &scirc;ian <tld/>emon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=138112">La Kabano de Onklo Tom</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxikan.0ema">&ccirc;ikanema</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_c;&c_ja;&c_zh;&c_n;&c_ib;&c_c;&c_k;&c_ib;</trd>
  <trdgrp lng="en">
    <trd>litigious</trd>,
    <trd>sue-happy <klr>(slang)</klr></trd>
  </trdgrp>
  <trd lng="de">prozessfreudig</trd>
  <trd lng="hu">peresked&odblac;</trd>
  <trd lng="id">suka menuntut</trd>
  <trd lng="ja">&#x8A34;&#x8A1F;&#x597D;&#x304D;&#x306E;
  <pr>&#x305D;&#x3057;&#x3087;&#x3046;&#x3059;&#x304D;&#x306E;</pr></trd>
  <trd lng="nl">tot <ind>procederen</ind> geneigd</trd>
  <trd lng="pl">pieniacki</trd>
  <trd lng="ru">&c_s;&c_u;&c_t;&c_ja;&c_zh;&c_n;&c_y;&c_j;</trd>
  <trd lng="uk">&c_s;&c_u;&c_t;&c_ja;&c_zh;&c_n;&c_i;&c_c;&c_mol;&c_k;&c_i;&c_j;</trd>
</drv>
</art>

