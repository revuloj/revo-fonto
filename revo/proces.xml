<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>proces</rad>/o
    </kap>
    <drv mrk="proces.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="proces.0o.JUR">
        <uzo tip="fak">JUR</uzo>
        <dif>
          Ju&gcirc;afero jure submetita al tribunalo:
          <ekz>
            esti prava en sia <tld/>o<fnt>Z</fnt>;
          </ekz><ekz>
            gajni, perdi sian <tld/>on<fnt>Z</fnt>;
          </ekz><ekz>komenci&gcirc;is <tld/>o, mono fluas sen &ccirc;eso
<fnt><bib>PrV</bib></fnt>.</ekz>
        </dif>
        <trdgrp lng="hu">
          <trd>per</trd>,
          <trd>p&ouml;r</trd>
        </trdgrp>
        <trdgrp lng="nl">
    <trd>proces</trd>,
    <trd>rechtsgeding</trd>
  </trdgrp>
<trdgrp lng="ru">
          <trd>&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s; <klr>(&c_s;&c_u;&c_d;&c_je;&c_b;&c_n;&c_y;&c_j;)</klr></trd>,
          <trd>&c_t;&c_ja;&c_zh;&c_b;&c_a;</trd></trdgrp>
      </snc>
      <snc mrk="proces.0o.SCI">
        <uzo tip="fak">SCI</uzo>
        <dif>
          <ref tip="dif" cel="procez.0o.SCI">Procezo</ref>:
          <ekz>
            la <tld/>o de unuformi&gcirc;ado iros en la lingvo internacia
            multe pli rapide<fnt>Z</fnt>;
          </ekz><ekz>
            senskue kaj regule disvolvi&gcirc;as tiu <tld/>o de la homa
            movi&gcirc;ado, pensado kaj sentado, kiu estas nomata
            vivo<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_a;&c_c;&c_e;&c_s;</trd>
      <trd lng="de">Prozess</trd> 
      <trd lng="es">proceso</trd>
      <trd lng="hu">folyamat</trd> 
    <trdgrp lng="nl">
    <trd>proces</trd>,
    <trd>verloop</trd>,
    <trd>ontwikkelingsgang</trd>
  </trdgrp>
</drv>
    <drv mrk="proces.0i">
      <kap><tld/>i</kap>
      <gra><vspec>x</vspec></gra>
      <snc>
        <dif>
          Ju&gcirc;e persekuti iun:
          <ekz>
            <tld/>i iun, kontra&ubreve; iu, pri kompenso de la perdoj<fnt>B</fnt>;
          </ekz><ekz>
            la re&gcirc;o kaj mi <tld/>as pri mia kapo<fnt>B</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_s;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a;</trd>
      <trd lng="de">prozessieren</trd>
      <trd lng="es">procesar</trd>
      <trdgrp lng="hu">
        <trd>pereskedik</trd>,
        <trd>beperel</trd></trdgrp>
      <trd lng="nl">procederen</trd>
      <trdgrp lng="ru">
        <trd>&c_v;&c_je;&c_s;&c_t;&c_i;, &c_v;&c_o;&c_z;&c_b;&c_u;&c_d;&c_i;&c_t;&c_mol; <ind>&c_p;&c_r;&c_o;&c_c;&c_je;&c_s;&c_s;</ind> <klr>(&c_s;&c_u;&c_d;&c_je;&c_b;&c_n;&c_y;&c_j;)</klr></trd>,
        <trd>&c_s;&c_u;&c_d;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd></trdgrp>
    </drv>
    <drv mrk="proces.0ema">
      <kap><tld/>ema</kap>
      <snc>
        <dif>
          Amanta <tld/>i.
        </dif>
        <refgrp tip="sin">
          <ref cel="cxikan">&ccirc;ikanema</ref>.
        </refgrp>
      </snc>
      <trd lng="de">prozessfreudig</trd>
      <trd lng="hu">peresked&odblac;</trd>
      <trd lng="ru">&c_s;&c_u;&c_t;&c_ja;&c_zh;&c_n;&c_y;&c_j;</trd>
    </drv>
  </art>
