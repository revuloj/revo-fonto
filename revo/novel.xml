<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>novel</rad>/o
</kap>
<drv mrk="novel.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Mallonga beletra rakonto.
    </dif>
    <refgrp tip="sin">
      <ref cel="roman.0o">romano</ref>,
      <ref cel="fabel.0o">fabelo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Novelle</trd>
  <trd lng="nl">novelle</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_v;&c_je;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_o;&c_v;&c_je;&c_l;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">novell</trd>
</drv>
</art>
