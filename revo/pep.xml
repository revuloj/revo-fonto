<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pep</rad>/i
</kap>
<drv mrk="pep.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pep.0i.bleki">
    <dif>
      <ref tip="super" cel="blek.0i.beste">Bleki</ref> per malla&ubreve;taj
      ripetataj krietoj, kiel kokidetoj, paseroj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>aj krioj de la virinoj<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="pep.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Babile, abunde, rapide <ref tip="super"
      cel="parol.0i.vocxi">paroli</ref> per akuta vo&ccirc;o:
      <ekz>
        sinjoro Aleksandro <klr>...</klr> sekvis la movojn de la
        rozokoloraj lipetoj, kiuj <tld/>is tiujn vortojn
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">piepen</trd>
  <trd lng="en">chirp</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;pier</trd>,
    <trd>piailler</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>piepen</trd>,
    <trd>sjilpen</trd>,
    <trd>kwetteren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_je;&c_b;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_ch;&c_i;&c_r;&c_i;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_a;&c_h;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pep.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc>
    <dif>
      Krieto de malgranda birdo:
      <ekz>
        &leftquot;<tld/>!&rightquot; diris unu pasero al alia
        <fnt><bib>Fab3</bib>, <lok>historio de la jaro</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">cui<klr> (cri d'oiseau)</klr></trd>
</drv>

<drv mrk="pep.plor0i">
  <kap>plor<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Plende, pete <tld/>i, kiel birdido:
      <ekz>
        kiel hirundo plendanta mi plor<tld/>is
        <fnt><bib>MT</bib>, <lok>&Jer; 38:14</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">p&eacute;pier<klr> (oisillon)</klr></trd>
</drv>
</art>
