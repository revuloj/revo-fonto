<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pep</rad>/i
</kap>
<drv mrk="pep.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      A&ubreve;digi malla&ubreve;tajn ripetitajn krietojn, kiel kokidetoj, paseroj:
      (f)
      <ekz>
        la <tld/>aj krioj de la virinoj<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Babili.
    </dif>
  </snc>
  <trd lng="de">piepen</trd>
  <trd lng="en">chirp</trd>
  <trdgrp lng="nl">
    <trd>piepen</trd>,
    <trd>sjilpen</trd>,
    <trd>kwetteren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_je;&c_b;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_ch;&c_i;&c_r;&c_i;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_a;&c_h;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
