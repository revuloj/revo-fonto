<drv mrk="skem.0a">
  <kap><tld/>a</kap>
  <snc mrk="skem.0a.propre">
    <dif>
      Rilata al <tld/>o:
      <ekz>
	la <tld/>aj konvencioj
	<klr>(kiel fari a&ubreve; legi <tld/>ojn)</klr>;
      </ekz>
      <ekz>
	<tld/>a metodo <klr>(por solvi problemon ktp)</klr>;
      </ekz>
      <ekz>
        tiel klare kaj <tld/>e li ne rezonadis, sed <klr>[&#x2026;]</klr>
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a numerado
        <fnt>
          <vrk><url ref="https://help.libreoffice.org/6.0/eo/text/swriter/01/06060000.html">&Ccirc;apitra
          numerado</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        privilegioj kiujn oni donas la&ubreve; la <tld/>a aparteno al la certa klaso
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 7 (69)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="model.0a">modela</ref>,
      <ref cel="tekton1.0a">tektona</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_h;&c_je;&c_m;&c_n;&c_y;</trd>
    <trdgrp lng="hu">
      <trd>s&eacute;ma-</trd>,
      <trd>v&aacute;zlat</trd>
    </trdgrp>
    <trd lng="pl">schematyczny</trd>
    <trd lng="ru">&c_s;&c_h;&c_je;&c_m;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="skem.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="skem.0eca"><tld/>eca</ref>
    <trd lng="be">&c_s;&c_h;&c_je;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
    <trdgrp lng="hu">
