    <trd>Bild</trd>,
    <trd>Darstellung</trd>,
    <trd>Schema<ind>zeichnung</ind></trd>,
    <trd>Modell</trd>,
    <trd>Struktur</trd>,
    <trd>Muster</trd>,
    <trd>Organigramm</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>schema</trd>,
    <trd>diagram</trd>,
    <trd>model</trd>,
    <trd>structure</trd>,
    <trd>pattern</trd>
