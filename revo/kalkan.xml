<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kalkan</rad>/o
</kap>
<drv mrk="kalkan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <dif>
      Posta parto de la homa a&ubreve; simia piedo:
      <ekz>li turnis sin sur la <tld/>oj.</ekz>
    </dif>
  </snc>
  <trd lng="de">Ferse</trd>
  <trd lng="en">heel</trd>
  <trd lng="fr">talon</trd>
  <trd lng="nl">hiel</trd>
  <trd lng="pl">pi&eogonek;ta</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_ja;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ja;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">h&auml;l</trd>
</drv>
<drv mrk="kalkan.0umo">
  <kap><tld/>umo</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Posta suba parto de la &scirc;uo:
      <ekz>
        li skuas la pugnon kaj batas per <tld/>umo la
        plankon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ka&ubreve;&ccirc;ukaj, turni&gcirc;eblaj sub<tld/>umoj
        <klr>(moveblaj <tld/>umoj, kiujn oni fiksas sub la ordinarajn,
        por &scirc;irmi ilin kontra&ubreve; tro rapida foruzo)</klr>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="de">Absatz</trd>
  <trd lng="en">heel</trd>
  <trd lng="fr">talon<klr> (de chaussure)</klr></trd>
  <trd lng="nl">hak</trd>
  <trd lng="pl">obcas</trd>
  <trd lng="ru">&c_k;&c_a;&c_b;&c_l;&c_u;&c_k;</trd>
  <trd lng="sv">klack</trd>
</drv>
<drv mrk="kalkan.0sidi">
  <kap><tld/>sidi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="kauxr.0i">Ka&ubreve;ri</ref>.
    </dif>
  </snc>
</drv>
<trd lng="fr">s'<ind>accroupir</ind></trd>
</art>
