<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kalkan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kalkan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kalkan.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Posta parto de la homa a&ubreve; simia <ref tip="malprt"
      cel="pied.0o.ANA">piedo</ref>:
      <ekz>
        li turnis sin sur la <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_t;&c_a;</trd>,
    <trd>&c_p;&c_ja;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_je;&c_t;&c_a;</trd>
  <trd lng="br">seul<klr tip="ind"> (troad)</klr></trd>
  <trd lng="ca">tal&oacute;<klr> (anat.)</klr></trd>
  <trd lng="cs">kalk&aacute;n</trd>
  <trd lng="de">Ferse</trd>
  <trd lng="en">heel</trd>
  <trd lng="es">tal&oacute;n</trd>
  <trd lng="fr">talon<klr> (du pied)</klr></trd>
  <trd lng="hu">sarok <klr tip="ind"> (l&aacute;b&eacute;)</klr></trd>
  <trd lng="id">tumit</trd>
  <trdgrp lng="it">
    <trd>calgagno</trd>,
    <trd>tallone</trd>
  </trdgrp>
  <trd lng="nl">hiel</trd>
  <trd lng="pl">pi&eogonek;ta</trd>
  <trd lng="pt">calcanhar</trd>
  <trd lng="ro">c&abreve;lc&acirc;i</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_ja;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ja;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">kalk&aacute;n</trd>
  <trd lng="sv">h&auml;l</trd>
</drv>

<drv mrk="kalkan.0umo">
  <kap><ofc>1</ofc><tld/>umo</kap>
  <snc mrk="kalkan.0umo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Posta suba parto de la <ref tip="malprt"
      cel="sxu.0o.TEKS">&scirc;uo</ref>:
      <ekz>
        li skuas la pugnon kaj batas per <tld/>umo la
        plankon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_c;&c_a;&c_s;</trd>
  <trd lng="bg">&c_t;&c_o;&c_k;<klr tip="amb"> (&c_n;&c_a; &c_o;&c_b;&c_u;&c_v;&c_k;&c_i;)</klr></trd>
  <trd lng="br">seul<klr tip="ind"> (botez)</klr></trd>
  <trdgrp lng="ca">
    <trd>tal&oacute; <klr>(cal&ccedil;.)</klr></trd>,
    <trd>tac&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kramflek</trd>,
    <trd>podpatek</trd>
  </trdgrp>
  <trd lng="de">Absatz</trd>
  <trd lng="en">heel</trd>
  <trd lng="fr">talon<klr> (de chaussure)</klr></trd>
  <trd lng="hu">cip&odblac;sarok</trd>
  <trd lng="id">hak (sepatu)</trd>
  <trd lng="it">tacco<klr> (scarpa)</klr></trd>
  <trd lng="nl">hak</trd>
  <trd lng="pl">obcas</trd>
  <trd lng="ro">toc</trd>
  <trd lng="ru">&c_k;&c_a;&c_b;&c_l;&c_u;&c_k;</trd>
  <trd lng="sk">op&auml;tok</trd>
  <trd lng="sv">klack</trd>
</drv>

<drv mrk="kalkan.sub0umo">
  <kap>sub<tld/>umo</kap>
  <snc mrk="kalkan.sub0umo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Fiksita a&ubreve; demovebla <tld/>umo, kiun oni
      almetas sub la ordinaran,
      por &scirc;irmi &gcirc;in kontra&ubreve; tro rapida foruzo:
      <ekz>
        ka&ubreve;&ccirc;ukaj sub<tld/>umoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bonbout</trd>,
    <trd>embout de talon</trd>
  </trdgrp>
</drv>

<drv mrk="kalkan.0sidi">
  <kap><tld/>sidi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <ref tip="dif" cel="kauxr.0i">ka&ubreve;ri</ref>
    <ekz>
      de kelka tempo ili <tld/>sidis sur la trotuar-rando
      <fnt>
        <aut>R. Schwartz</aut>,
        <vrk><url ref="http://esperanto.net/literaturo/noveloj/10.html">
        La Puto de Paupel&egrave;che</url></vrk>,
        <lok>La Nica Literatura Revuo, 1957 (2:6)</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_d;&c_z;&c_je;&c_c;&c_mol; &c_n;&c_a;
  <ind>&c_k;&c_u;&c_k;&c_ib;&c_sh;&c_k;&c_a;&c_h;</ind></trd>
  <trdgrp lng="br">
    <trd>beza&ntilde; <ind>kluchet</ind></trd>,
    <trd>beza&ntilde; <ind>puchet</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>seure ajupit</trd>,
    <trd>estar emponat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>sed&ecaron;t na bobku</trd>,
    <trd>sed&ecaron;t v d&rcaron;epu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kauern</trd>,
    <trd>hocken</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>squat</trd>,
    <trd>sit on one's heels</trd>
  </trdgrp>
  <trd lng="es">ponerse de cuclillas</trd>
  <trd lng="fr">se tenir <ind>accroupi</ind></trd>
  <trdgrp lng="it">
    <trd>accosciarsi</trd>,
    <trd><ind>sedersi</ind> sui talloni</trd>
  </trdgrp>
  <trd lng="nl">hurken</trd>
  <trd lng="pl">kuca&cacute;</trd>
  <trd lng="ro">se st&acirc;rci</trd>
  <trd lng="ru">&c_s;&c_i;&c_d;&c_je;&c_t;&c_mol; &c_n;&c_a;
  <ind>&c_k;&c_o;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a;&c_h;</ind></trd>
  <trdgrp lng="sk">
    <trd>sedie&tcaron; drepom</trd>,
    <trd>&ccaron;upie&tcaron;</trd>
  </trdgrp>
</drv>
</art>
