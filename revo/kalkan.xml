<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalkan.xml,v 1.44 2024/02/09 09:23:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kalkan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kalkan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kalkan.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Posta parto de la homa a&ubreve; simia <ref tip="malprt" cel="pied.0o.ANA">piedo</ref>:
      <ekz>
        <ind>li sin turnis sur la <tld/>o</ind>
        <fnt>
          <bib>Marta</bib>
        </fnt>;
        <trd lng="de">er drehte sich auf dem <ind>Absatz</ind> um</trd>
      </ekz>
      <ekz>
        la ma&scirc;o enkro&ccirc;igos lian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 18:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sidi&gcirc;is rajde sur la kapro, frapis &gcirc;iajn flankojn per siaj
        <tld/>oj kaj
        forgalopis
        <fnt>
          <bib>Fab3</bib>
          <lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la knabino fortran&ccirc;is pecon de la <tld/>o, superforte enpu&scirc;is la
        piedon en la &scirc;uon, kunpremis pro doloro la dentojn kaj iris renkonte al la
        princo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>de la peruko &gcirc;is la nudaj <tld/>oj</ind>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
        <trd lng="de">vom Scheitel bis zur Sohle</trd>
      </ekz>
      <ekz>
        <nom>Em&egrave;ric</nom> demandis La Majstron pri febla punkto,
        <ref cel="vund.0aloko.ebla">vunda loko</ref> a&ubreve;
        <ind>a&hcirc;ila <tld/>o</ind> de la
        insolenta madridano
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 6</lok>
        </fnt>.
        <trd lng="de">Achillesferse</trd>
        <trd lng="en">Achilles' heel</trd>
        <trd lng="fr">talon d'Achille</trd>
        <trd lng="ru">&c_A;&c_h;&c_i;&c_l;&c_l;&c_je;&c_s;&c_o;&c_v;&c_a; &c_p;&c_ja;&c_t;&c_a;</trd>
        <trd lng="uk">&c_A;&c_h;&c_ib;&c_l;&c_l;&c_je;&c_s;&c_o;&c_v;&c_a; &c_p;'&c_ja;&c_t;&c_a;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_t;&c_a;</trd>,
    <trd>&c_p;&c_ja;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_je;&c_t;&c_a;</trd>
  <trd lng="br">seul<klr tip="ind"> (troad)</klr></trd>
  <trd lng="ca">tal&oacute;<klr> (anat.)</klr></trd>
  <trd lng="cs">kalk&aacute;n</trd>
  <trdgrp lng="de">
    <trd>Ferse</trd>,
    <trd>Hacke <klr>(Ferse)</klr></trd>
  </trdgrp>
  <trd lng="en">heel</trd>
  <trd lng="es">tal&oacute;n</trd>
  <trd lng="fr">talon<klr> (du pied)</klr></trd>
  <trd lng="hu">sarok <klr tip="ind"> (l&aacute;b&eacute;)</klr></trd>
  <trd lng="id">tumit</trd>
  <trdgrp lng="it">
    <trd>calgagno</trd>,
    <trd>tallone</trd>
  </trdgrp>
  <trd lng="ja">&#x8E35;  <pr>&#x304B;&#x304B;&#12392;</pr></trd>
  <trd lng="nl">hiel</trd>
  <trd lng="pl">pi&eogonek;ta</trd>
  <trd lng="pt">calcanhar</trd>
  <trd lng="ro">c&abreve;lc&acirc;i</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_ja;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ja;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">kalk&aacute;n</trd>
  <trd lng="sv">h&auml;l</trd>
  <trd lng="uk">&c_p;&#8217;&c_ja;&c_t;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x811A;&#x8DDF; <pr>ji&#x1CE;og&emacron;n</pr></trd>,
    <trd>&#x8DDF; <pr>g&emacron;n</pr></trd>,
    <trd>&#x811A;&#x540E;&#x8DDF; <pr>ji&#x1CE;oh&#xF2;ug&emacron;n</pr></trd>,
    <trd>&#x540E;&#x8DDF; <pr>h&#xF2;ug&emacron;n</pr></trd>,
    <trd>&#x8E35; <pr>zh&#x1D2;ng</pr></trd>,
    <trd>&#x8DDF;&#x9AA8; <pr>g&emacron;ng&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kalkan.0umo">
  <kap><ofc>1</ofc><tld/>umo</kap>
  <snc mrk="kalkan.0umo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Posta suba parto de la <ref tip="malprt" cel="sxu.0o.TEKS">&scirc;uo</ref>:
      <ekz>
        li skuas la pugnon kaj batas per <tld/>umo la
        plankon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        ha, mia bela juna fra&ubreve;lino! <klr>[&#8230;]</klr> vi per alteco de unu
        <tld/>umo
        fari&gcirc;is pli proksima al la &ccirc;ielo
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj <tld/>umetoj frapetis la asfalton
        <fnt>
          <aut>Konstanten Krysakov</aut>
          <vrk><url ref="&Monato;/2000/006805.php">Kota akvo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 28</lok>
          </fnt>;
      </ekz>
      <ekz>
        &scirc;ikaj alt<tld/>umaj &scirc;uoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/008760.php">En
          fremda lando</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_c;&c_a;&c_s;</trd>
  <trd lng="bg">&c_t;&c_o;&c_k;<klr tip="amb"> (&c_n;&c_a; &c_o;&c_b;&c_u;&c_v;&c_k;&c_i;)</klr></trd>
  <trd lng="br">seul<klr tip="ind"> (botez)</klr></trd>
  <trdgrp lng="ca">
    <trd>tal&oacute; <klr>(cal&ccedil;.)</klr></trd>,
    <trd>tac&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kramflek</trd>,
    <trd>podpatek</trd>
  </trdgrp>
  <trd lng="de">Absatz <klr>(Schuh)</klr></trd>
  <trd lng="en">heel</trd>
  <trd lng="fr">talon<klr> (de chaussure)</klr></trd>
  <trd lng="hu">cip&odblac;sarok</trd>
  <trd lng="id">hak<klr tip="amb"> (sepatu)</klr></trd>
  <trd lng="it">tacco<klr> (scarpa)</klr></trd>
  <trd lng="ja">&#x304B;&#x304B;&#12392;</trd>
  <trd lng="nl">hak</trd>
  <trd lng="pl">obcas</trd>
  <trd lng="ro">toc</trd>
  <trd lng="ru">&c_k;&c_a;&c_b;&c_l;&c_u;&c_k;</trd>
  <trd lng="sk">op&auml;tok</trd>
  <trd lng="sv">klack</trd>
  <trd lng="uk">&c_k;&c_a;&c_b;&c_l;&c_u;&c_k;</trd>
  <trd lng="zh">&#x978B;&#x8DDF; <pr>xi&#xE9;g&emacron;n</pr></trd>
</drv>

<drv mrk="kalkan.sub0umo">
  <kap>sub<tld/>umo</kap>
  <snc mrk="kalkan.sub0umo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Fiksita a&ubreve; demovebla <tld/>umo, kiun oni
      almetas sub la ordinaran,
      por &scirc;irmi &gcirc;in kontra&ubreve; tro rapida foruzo:
      <ekz>
        ka&ubreve;&ccirc;ukaj sub<tld/>umoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_b;&c_o;&c_j;&c_k;&c_a; <klr>(&c_n;&c_a; &c_a;&c_b;&c_c;&c_a;&c_s;)</klr></trd>
  <trdgrp lng="fr">
    <trd>bonbout</trd>,
    <trd>embout de talon</trd>
  </trdgrp>
</drv>

<drv mrk="kalkan.0sidi">
  <kap><tld/>sidi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="kauxr.0i">Ka&ubreve;ri</ref>:
      <ekz>
        de kelka tempo ili <tld/>sidis sur la trotuar-rando
        <fnt>
          <aut>R. Schwartz</aut>,
          <vrk><url ref="http://esperanto.net/literaturo/noveloj/10.html">
          La Puto de Paupel&egrave;che</url></vrk>,
          <lok>La Nica Literatura Revuo, 1957 (2:6)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_d;&c_z;&c_je;&c_c;&c_mol; &c_n;&c_a;
  <ind>&c_k;&c_u;&c_k;&c_ib;&c_sh;&c_k;&c_a;&c_h;</ind></trd>
  <trdgrp lng="br">
    <trd>beza&ntilde; <ind>kluchet</ind></trd>,
    <trd>beza&ntilde; <ind>puchet</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>seure ajupit</trd>,
    <trd>estar emponat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>sed&ecaron;t na bobku</trd>,
    <trd>sed&ecaron;t v d&rcaron;epu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kauern</trd>,
    <trd>hocken</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>squat</trd>,
    <trd>sit on one's heels</trd>
  </trdgrp>
  <trd lng="es">ponerse de cuclillas</trd>
  <trd lng="fr">se tenir <ind>accroupi</ind></trd>
  <trdgrp lng="it">
    <trd>accosciarsi</trd>,
    <trd><ind>sedersi</ind> sui talloni</trd>
  </trdgrp>
  <trd lng="ja">&#12375;&#12419;&#x304C;&#12416;</trd>
  <trd lng="nl">hurken</trd>
  <trd lng="pl">kuca&cacute;</trd>
  <trd lng="ro">se st&acirc;rci</trd>
  <trd lng="ru">&c_s;&c_i;&c_d;&c_je;&c_t;&c_mol; &c_n;&c_a;
  <ind>&c_k;&c_o;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a;&c_h;</ind></trd>
  <trdgrp lng="sk">
    <trd>sedie&tcaron; drepom</trd>,
    <trd>&ccaron;upie&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_i;&c_d;&c_ib;&c_t;&c_i; &c_n;&c_a;&c_v;&c_p;&c_o;&c_ch;&c_ib;&c_p;&c_k;&c_i;/&c_n;&c_a;&c_p;&c_o;&c_ch;&c_ib;&c_p;&c_k;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x8E1E; <pr>j&#xF9;</pr></trd>,
    <trd>&#x8DC1; <pr>p&#xE1;</pr></trd>,
    <trd>&#x8E72;&#x8E1E; <pr>d&#x16B;nj&#xF9;</pr></trd>,
    <trd>&#x8E06; <pr>q&#x16B;n</pr></trd>,
    <trd>&#x8DCD; <pr>k&#x16B;</pr></trd>,
    <trd>&#x8DF1; <pr>zh&#xEC;</pr></trd>,
    <trd>&#x84CC; <pr>cu&#xF2;</pr></trd>,
    <trd>&#18432; <pr>ch&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>
</art>
