<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skarifik</rad>/i <fnt>Z</fnt>
</kap>
<drv mrk="skarifik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="skarifik.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Tratran&ccirc;eti la ha&ubreve;ton por kuraca celo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_a;&c_r;&c_y;&c_f;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_n;&c_a;&c_s;&c_je;&c_ch;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">skarifizieren</trd>
  <trd lng="fr">scarifier</trd>
  <trd lng="nl">schrapen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_a;&c_r;&c_i;&c_f;&c_i;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_n;&c_a;&c_s;&c_je;&c_ch;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>narezať kožu</trd>,
    <trd>skarifikovať</trd>,
    <trd>urobiť zárez</trd>
  </trdgrp>
</drv>
</art>

