<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>brokant</rad>/i
</kap>
<drv mrk="brokan.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      A&ccirc;eti kaj revendi jam uzitajn a&jcirc;ojn.
    </dif>
    <trdgrp lng="de">
      <trd>mit Altware <ind>handeln</ind></trd>,
      <trd>mit Gebrauchtware <ind>handeln</ind></trd>,
      <trd>tr&ouml;deln</trd>
    </trdgrp>
    <trd lng="fr">brocanter</trd>
    <trd lng="ru">&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_p;&c_o;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_n;&c_y;&c_m;&c_i;
    &c_v;&c_je;&c_shch;&c_a;&c_m;&c_i;</trd>
  </snc>
</drv>

<drv mrk="brokan.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Jam uzita objekto, kiun oni vendas denove, &ccirc;ar &gcirc;i
      ankora&ubreve; funkcias a&ubreve; i&gcirc;is rara&jcirc;o
      interesa por kolektantoj.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="antikv.0ajxo">antikva&jcirc;o</ref>,
    <ref cel="kolekt.0i">kolekti</ref>.
  </refgrp>
  <trdgrp lng="de">
    <trd>Altware</trd>,
    <trd>Gebrauchtware</trd>,
    <trd>Tr&ouml;del</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>antiquit&eacute;</trd>,
    <trd>article d'<ind>occasion</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_n;&c_a;&c_ja; &c_v;&c_je;&c_shch;&c_mol;</trd>
</drv>

<drv mrk="brokan.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="komerc.0isto">Komercisto</ref>, kiu <tld/>adas.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Antiquar</trd>,
    <trd>Altwarenh&auml;ndler</trd>,
    <trd>Gebrauchth&auml;ndler</trd>,
    <trd>Tr&ouml;dler</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>antiquaire</trd>,
    <trd>brocanteur</trd>,
    <trd>fripier</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_mol;&c_jo;&c_v;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_a;&c_n;&c_t;&c_i;&c_k;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
</drv>
</art>
