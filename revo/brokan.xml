<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>brokant</rad>/i
</kap>
<drv mrk="brokan.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="fak">KOME</uzo>
  <snc>
    <dif>
      A&ccirc;eti kaj revendi jam uzitajn a&jcirc;ojn.
    </dif>
    <trd lng="fr">brocanter</trd>
    <trd lng="ru">&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_p;&c_o;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_n;&c_y;&c_m;&c_i;
&c_v;&c_je;&c_shch;&c_a;&c_m;&c_i;</trd>
  </snc>
</drv>

<drv mrk="brokan.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Jam uzita objekto, kiun oni vendas denove, &ccirc;ar &gcirc;i
      ankora&ubreve; funkcias a&ubreve; i&gcirc;is rara&jcirc;o
      interesa por kolektantoj.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="antikv.0ajxo">antikva&jcirc;o</ref>,
    <ref cel="kolekt.0i">kolekti</ref>.
  </refgrp>
  <trdgrp lng="fr">
    <trd>antiquit&eacute;</trd>,
    <trd>article d'<ind>occasion</ind></trd>
  </trdgrp>
</drv>

<drv mrk="brokan.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Komercisto, kiu brokantadas.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>antiquaire</trd>,
    <trd>brocanteur</trd>,
    <trd>fripier</trd>
  </trdgrp>
</drv>
</art>
