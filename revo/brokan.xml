<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>brokant</rad>/i
</kap>
<drv mrk="brokan.0i">
<kap><tld/>i</kap>
<gra><vspec>x</vspec></gra>
<snc num="1">
<dif>
A&ccirc;eti k vendi <tld/>a&jcirc;ojn.
</dif>
<trd lng="fr">brocanter</trd>
<trd lng="ru">&c_t;&c_o;&c_r;&c_g;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_p;&c_o;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_n;&c_n;&c_y;&c_m;&c_i; &c_v;&c_je;&c_shch;&c_a;&c_m;&c_i;</trd>
</snc>
<snc num="2">
<dif>
Komerca&ccirc;i. 
</dif>
<refgrp tip="vid">
<ref cel="kolpor.0i">kolporti</ref>.
</refgrp>
</snc>
</drv>
    
    <drv mrk="brokan.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
        <dif>
          Jam uzita objekto, kiun oni vendas denove, &ccirc;ar &gcirc;i
          povas ankora&ubreve; servi a&ubreve; i&gcirc;is rara&jcirc;o
          interesa por kolektantoj.
        </dif>
      </snc>
      <refgrp tip="vid">
        <ref cel="antikv.0ajxo">antikva&jcirc;o</ref>,
        <ref cel="kolekt.0i">kolekti</ref>.
      </refgrp>
      <trd lng="fr">occasion <klr tip="ind">(article d')</klr></trd>
    </drv>
    
</art>
