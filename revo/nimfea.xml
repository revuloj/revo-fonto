<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nimfea.xml,v 1.14 2024/01/29 15:59:10 revo Exp $">
<kap>
  <rad>nimfeac</rad>/oj
</kap>
<drv mrk="nimfea.0oj">
  <kap><tld/>oj
    <fnt><bib>PV</bib><lok>nimfeo</lok></fnt>
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      akvoplantoj (<trd lng="la">Nymphaeaceae</trd>):
      <ekz>
        la <tld/>aj estas mar&ccirc;aj plantoj, kies radiktigoj loki&gcirc;as en la &scirc;limo, 
        subakve, la folioj etendi&gcirc;as sur la akva surfaco, la floroj estas grandaj, okulfrapaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=258424">Nimfeacoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la viktorio-domo malfermi&gcirc;is en septembro 1998, &gcirc;i havas la 
        nomon de la tie kreskanta <tld lit="N"/>oj, la specio viktorio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=440096">Erfurta
          &Gcirc;ardenekspozicio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="nimfe.0o">nimfeo</ref>,
       <ref cel="nufar.0o">nufaro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_l;&c_a;&c_ch;&c_y;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_v;&c_o;&c_d;&c_n;&c_i; &c_r;&c_o;&c_z;&c_i;</trd>
  <trdgrp lng="cs">
    <trd>lekn&iacute;novit&eacute;</trd>,
    <trd>nocenkovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  </trdgrp>
  <trd lng="de">Seerosengew&auml;chse</trd>
  <trd lng="en">water lilies</trd>
  <trd lng="fr">Nymph&eacute;ac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>t&uuml;nd&eacute;rr&oacute;zsaf&eacute;l&eacute;k</trd>,
    <trd>tavir&oacute;zsaf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">waterleliefamilie</trd>
  <trd lng="pl">grzybieniowate</trd>
  <trd lng="pt">ninfe&aacute;ceas</trd>
  <trd lng="ro">nimfeacee</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_v;&c_sh;&c_i;&c_n;&c_k;&c_o;&c_v;&c_y;&c_je;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_n;&c_i;&c_m;&c_f;&c_je;&c_j;&c_n;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">leknovit&eacute;</trd>
  <trdgrp lng="zh">
    <trd>&#x7761;&#x83B2;&#x79D1; <pr>shu&#xEC;li&#xE1;nk&#x113;</pr></trd>,
    <trd>&#x7761;&#x84EE;&#x79D1; <pr>shu&#xEC;li&#xE1;nk&#x113;</pr></trd>
  </trdgrp>
</drv>
</art>
