<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>konsul</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="konsul.0o">
  <kap><tld/>o</kap>
  <snc mrk="konsul.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Diplomatia agento komisiita por protekti en fremda lando la
      aferojn de la samregnanoj.
    </dif>
  </snc>
  <snc mrk="konsul.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      &Ccirc;iu el la du unujaraj regnestroj de la antikva roma respubliko.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_s;&c_u;&c_l;</trd>
  <trd lng="de">Konsul</trd>
  <trd lng="en">consul</trd>
  <trd lng="es">C&oacute;nsul</trd>
  <trdgrp lng="fr">
    <trd>consul</trd>
  </trdgrp>
  <trd lng="hu">konzul</trd>
  <trd lng="nl">consul</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_u;&c_l;</trd>
</drv>

<drv mrk="konsul.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="konsul.0o.ejo">
    <uzo tip="fak">POL</uzo>
    <dif>
      Administraciejo kaj laborloko de <tld/>o en fremda lando,
      kie samregnanoj de la <tld/>o povas viziti lin:
      <ekz>
        samtage estis malfermita en Edinburgo <tld/>ejo de la Irlanda
        Respubliko<fnt><aut>Irl</aut>: <vrk>Historia pa&scirc;o</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 7a, p. 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_n;&c_s;&c_u;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_s;&c_u;&c_l;&c_ja;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Konsulat</trd>
  <trd lng="es">Consulado</trd>
  <trdgrp lng="fr">
    <trd>consulat</trd>
  </trdgrp>
  <trd lng="hu">konzul&aacute;tus</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_u;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>
</art>
