<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>2</ofc>
      <rad>cirkumfleks</rad>/o
    </kap>

    <drv mrk="cirkum1.0o">
      <kap><tld/>o</kap>
      <snc mrk="cirkum1.0o.FON">
        <uzo tip="fak">FON</uzo>
	<dif>
           <ref tip="super" cel="akcent.ton0o">Tonakcento</ref> en la klasika
           greka (levi&gcirc;anta-falanta), serba-kroata (falanta-levi&gcirc;anta),
           sveda k.a. lingvoj, en transskriboj indikata per
           <ref cel="cxapel.0o.supersigno">&ccirc;apelo</ref>,
           <ref cel="tild.0o">tildo</ref> a&ubreve; supersigna arketo.
        </dif>
        <trdgrp lng="ru">
           <trd>&c_c;&c_i;&c_r;&c_k;&c_u;&c_m;&c_f;&c_l;&c_je;&c_k;&c_s;</trd>,
           <trd>&c_o;&c_b;&c_l;&c_je;&c_g;&c_ch;&c_jo;&c_n;&c_n;&c_o;&c_je; &c_u;&c_d;&c_a;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
        </trdgrp>
      </snc>

      <snc mrk="cirkum1.0o.supersigno">
        <uzo tip="stl">EVI</uzo>
        <uzo tip="fak">GRA</uzo>
        <ref tip="dif" cel="cxapel.0o.supersigno">&Ccirc;apelo</ref>
      </snc>

      <trd lng="es">circunflejo</trd>
      <trd lng="fr">accent <ind>circonflexe</ind></trd>
      <trd lng="nl">circumflex</trd>
    </drv>
  </art>
