<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>frag</rad>/o
</kap>
<drv mrk="frag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Frukto de la <tld/>ujo, ru&gcirc;a kaj bongusta,
      uzata en deserto a&ubreve; konfita&jcirc;o.
    </dif>
    <ref tip="super" cel="frukt.plur0o">plurfrukto</ref>
  </snc>
  <bld lok="../bld/frag1.jpg"><tld/>o</bld>
  <trd lng="de">Erdbeere</trd>
  <trd lng="es">fresa</trd>
  <trd lng="fr">fraise</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldieper</trd>,
    <trd>szam&oacute;ca</trd>
  </trdgrp>
  <trd lng="nl">aardbei<klr tip="ind"> (vrucht)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_u;&c_b;&c_n;&c_i;&c_k;&c_a; <klr>(&c_ja;&c_g;&c_o;&c_d;&c_a;)</klr></trd>,
    <trd>&c_z;&c_je;&c_m;&c_l;&c_ja;&c_n;&c_i;&c_k;&c_a; <klr>(&c_ja;&c_g;&c_o;&c_d;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="tr">&ccedil;ilek</trd>
</drv>
<drv mrk="frag.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Fragaria</trd>), kun ru&gcirc;aj
      plurfruktoj.
    </dif>
  </snc>
  <bld lok="../bld/frag.jpg">floroj kaj folioj</bld>
  <trd lng="de">Erdbeere</trd>
  <trd lng="es">fresero</trd>
  <trd lng="fr">fraisier</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldieper<klr> (n&ouml;v&eacute;ny)</klr></trd>,
    <trd>szam&oacute;ca<klr> (n&ouml;v&eacute;ny)</klr></trd>
  </trdgrp>
  <trd lng="nl">aardbei<klr tip="ind"> (plant)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_u;&c_b;&c_n;&c_i;&c_k;&c_a; <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_z;&c_je;&c_m;&c_l;&c_ja;&c_n;&c_i;&c_k;&c_a; <klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
</drv>
</art>
