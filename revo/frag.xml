<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>frag</rad>/o
</kap>
<drv mrk="frag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      &Scirc;ajnfrukto de la <tld/>ujo, ru&gcirc;a kaj bongusta.
    </dif>
  </snc>
  <bld lok="../bld/frag1.jpg"><tld/>o</bld>
  <trd lng="de">Erdbeere</trd>
  <trd lng="fr">fraise</trd>
  <trd lng="tr">&ccedil;ilek</trd>
</drv>
<drv mrk="frag.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="roz.0acoj">rozacoj</ref>
      (<trd lng="la">Fragaria</trd>), kies
      &scirc;ajnfruktoj estas man&gcirc;ataj kiel deserto a&ubreve;
      konfita&jcirc;o.
    </dif>
  </snc>
  <bld lok="../bld/frag.jpg">floroj kaj folioj</bld>
  <trd lng="de">Erdbeere</trd>
  <trd lng="fr">fraisier</trd>
</drv>
</art>
