<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: frag.xml,v 1.35 2023/08/31 13:11:31 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>frag</rad>/o <fnt><bib>Fab2</bib></fnt>
</kap>

<drv mrk="frag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="frag.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>
      de la <tld/>ujo, ru&gcirc;a kaj bongusta,
      uzata en deserto a&ubreve; konfita&jcirc;o:
      <ekz>
        la vila&gcirc;anaj infanoj <klr>[&#x2026;]</klr> sin turnadis en la arbaro,
        kolektante <tld/>ojn kaj frambojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie sur la erikejo ekzistis aliaj trezoroj, &ccirc;i tie estis
        plene de eriko kaj <tld/>oj kaj mirteloj, grandaj kaj dol&ccirc;aj ili
        &ccirc;ie montri&gcirc;is en granda abundo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la somero ni havas <tld/>ojn kaj persikojn, kiam ili estas
        maturaj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="frukt.plur0o">plurfrukto</ref>
  </snc>
  <bld lok="&WCU;/thumb/c/c1/Fragaria_vesca_-_metsmaasikas.jpg/320px-Fragaria_vesca_-_metsmaasikas.jpg" prm="CC BY-SA 3.0">sova&gcirc;aj <tld/>oj (Fragaria vesca), duoninsulo Pakri
       <fnt>
         <aut>Ivar Leidus, propra verko</aut>
         <lok><url ref="&WCW;/File:Fragaria_vesca_-_metsmaasikas.jpg">&WCW;/File:Fragaria_vesca_-_metsmaasikas.jpg</url></lok>
       </fnt>
  </bld>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_n;&c_ib;&c_c;&c_a;
    <klr>(&c_ja;&c_g;&c_a;&c_d;&c_a;)</klr></trd>,
    <trd>&c_t;&c_r;&c_u;&c_s;&c_k;&c_a;&c_w;&c_k;&c_a;
    <klr>(&c_ja;&c_g;&c_a;&c_d;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_ja;&c_g;&c_o;&c_d;&c_a;</trd>
  <trd lng="de">Erdbeerstrauch</trd>
  <trd lng="en">strawberry</trd>
  <trd lng="es">fresa</trd>
  <trd lng="fr">fraise</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldieper</trd>,
    <trd>szam&oacute;ca</trd>
  </trdgrp>
  <trd lng="id">stroberi</trd>
  <trdgrp lng="ja">
    <trd>&#x30AA;&#x30E9;&#x30F3;&#x30C0;&#x30A4;&#x30C1;&#x30B4;</trd>,
    <trd>&#x30A4;&#x30C1;&#x30B4;</trd>
  </trdgrp>
  <trd lng="nl">aardbei<klr tip="ind"> (vrucht)</klr></trd>
  <trdgrp lng="pl">
    <trd>truskawka</trd>,
    <trd>poziomka</trd>
  </trdgrp>
  <trd lng="pt">morango</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_u;&c_b;&c_n;&c_i;&c_k;&c_a;
    <klr>(&c_ja;&c_g;&c_o;&c_d;&c_a;)</klr></trd>,
    <trd>&c_z;&c_je;&c_m;&c_l;&c_ja;&c_n;&c_i;&c_k;&c_a;
    <klr>(&c_ja;&c_g;&c_o;&c_d;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="tr">&ccedil;ilek</trd>
  <trd lng="sk">jahoda <klr>(plod)</klr></trd>
  <trd lng="cs">jahoda</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&#x43D;&c_i;&c_c;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&c_u;&#x43D;&c_i;&c_c;&#x44F; (&#x44F;&c_g;&#x43E;&c_d;&c_i;)</trd>
  </trdgrp>
  <trd lng="zh">&#x8349;&#x8393;</trd>
</drv>

<drv mrk="frag.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="frag.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Malalta 
      <ref tip="lst" cel="veget.0ajxo" lst="voko:rozoparencoj">vegeta&jcirc;o</ref>
      kun ru&gcirc;aj plurfruktoj kaj tripartaj folioj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="rozac.0oj">rozacoj</ref>
      (<trd lng="la">Fragaria</trd>):
      <ekz>
        li sciis pri tri florantaj <tld/>ujoj, kiuj promesis al li ri&ccirc;an
        rikolton
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;to&hcirc;oriaj plantoj a&ubreve; memdissemantoj estas ekzemple oficina 
        taraksako, poplo, arbara <tld/>ujo, saliko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=522755">A&ubreve;to&hcirc;orio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/2/25/FragariaChileonesisVarPatagonia.jpg/570px-FragariaChileonesisVarPatagonia.jpg" prm="PD">folioj kaj floroj de <tld/>ujo (Fragaria chiloensis subsp.
     chiloensis)
       <fnt>
         <aut>Dr. Kim Hummer</aut>
         <lok><url ref="&WCW;/File:FragariaChileonesisVarPatagonia.jpg">&WCW;/File:FragariaChileonesisVarPatagonia.jpg</url></lok>
       </fnt>
  </bld>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_n;&c_ib;&c_c;&c_a;
<klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>,
    <trd>&c_t;&c_r;&c_u;&c_s;&c_k;&c_a;&c_w;&c_k;&c_a;
<klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_ja;&c_g;&c_o;&c_d;&c_a;</trd>
  <trd lng="de">Erdbeere</trd>
  <trd lng="es">fresero</trd>
  <trd lng="fr">fraisier</trd>
  <trdgrp lng="hu">
    <trd>f&ouml;ldieper<klr> (n&ouml;v&eacute;ny)</klr></trd>,
    <trd>szam&oacute;ca<klr> (n&ouml;v&eacute;ny)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x30AA;&#x30E9;&#x30F3;&#x30C0;&#x30A4;&#x30C1;&#x30B4;</trd>
  <trd lng="nl">aardbei<klr tip="ind"> (plant)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_u;&c_b;&c_n;&c_i;&c_k;&c_a;
<klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_z;&c_je;&c_m;&c_l;&c_ja;&c_n;&c_i;&c_k;&c_a;
<klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="sk">jahodn&iacute;k</trd>
  <trd lng="cs">jahodn&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_u;&c_shch; &c_s;&c_u;&#x43D;&c_i;&c_c;&c_ib;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&c_u;&#x43D;&c_i;&c_c;&c_ib;</trd>
  </trdgrp>
</drv>
</art>
