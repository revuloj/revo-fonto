<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>osmant</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="osmant.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="oleac.0oj">oleacoj</ref>
      (<trd lng="la">Osmanthus</trd>), arbustoj el Azio, Polinezio a&ubreve;
      Ameriko kun tutjaraj folioj, bonodoraj blankaj floroj uzataj por
      fari parfumojn:
      <ekz>
        balda&ubreve; &scirc;i atingis la ombron de granda <tld/>o
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 71a, p. 514a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_m;&c_a;&c_n;&c_t;&c_u;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Duftblume</trd>,
    <trd>Duftbl&uuml;te</trd>
  </trdgrp>
  <trd lng="fr">osmanthe</trd>
  <trdgrp lng="hu">
    <trd>illatvir&aacute;g</trd>,
    <trd>illatosfa</trd>
  </trdgrp>
  <trd lng="it">osmanto</trd>
  <trd lng="ru">&c_o;&c_s;&c_m;&c_a;&c_n;&c_t;&c_u;&c_s;</trd>
</drv>
</art>

