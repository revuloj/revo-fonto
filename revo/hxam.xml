<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
    <art mrk="$Id$">
      <kap><rad>&Hcirc;am</rad>/o</kap>
      <drv mrk="hxam.Hx0o">
	<kap><tld/>o</kap>
	<snc>
	  <uzo tip="fak">BIB</uzo>
	  <dif>
	    La dua filo de Noa, prapatro de multaj popoloj en
	    la biblia mito.
	  </dif>
	</snc>
	<trd lng="be">&c_H;&c_a;&c_m;</trd>
        <trd lng="bg">&c_H;&c_a;&c_m;</trd>
	<trd lng="de">Ham</trd>
	<trd lng="en">Ham</trd>
	<trd lng="fi">Haam</trd>
	<trd lng="fr">Cham</trd>
	<trdgrp lng="hu">
	  <trd>H&aacute;m</trd>,
	  <trd>K&aacute;m</trd>
	</trdgrp>
	<trd lng="pl">Cham</trd>
	<trd lng="ru">&c_H;&c_a;&c_m;</trd>
        <trd lng="it">Cam</trd>
      </drv>
      <drv mrk="hxam.0ido">
	<kap><tld lit="&hcirc;"/>ido</kap>
	<snc>
	  <dif>
	    Posteulo de <tld/>o; eksmoda nomo por indiki parolanton
	    de &leftquot;<tld lit="&hcirc;"/>ida lingvo&rightquot;, lingvo el la
	    azia-afrika lingvofamilio (lingvoj kopta, berbera, etiopa ktp).
	  </dif>
	</snc>
	<trd lng="de">Hamite</trd>
	<trdgrp lng="fi">
	  <trd>Haamin j&auml;lkel&auml;inen</trd>,
	  <trd>haamilainen <klr tip="amb"> (subst.)</klr></trd>
	</trdgrp>
	<trd lng="hu">hamita</trd>
	<trd lng="pl">chamita</trd>
	<trd lng="ru">&c_h;&c_a;&c_m;&c_i;&c_t;</trd>
        <trd lng="it">camita</trd>
      </drv>
    </art>

