<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>hieroglif</rad>/o
</kap>
<drv mrk="hierog.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Skriba sistemo, kiu uzas figurojn de homo, bestoj, vegeta&jcirc;oj,
      objektoj por reprezenti ideon a&ubreve; sonon:
      <ekz>egiptaj, meksikaj <tld/>oj</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ideogr.0o">ideografio</ref>,
      <ref cel="foneti.0o">fonetiko 2</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Nelegebla skribo, nekomprenebla stilo:
      <ekz>tio estas por mi puraj <tld/>oj.</ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_k;&c_o;&c_r;&c_ju;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_i;&c_t;&c_a;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_g;&c_r;&c_a;&c_m;&c_o;&c_t;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Hieroglyphe</trd>
  <trd lng="ru">&c_i;&c_je;&c_r;&c_o;&c_g;&c_l;&c_i;&c_f;</trd>
</drv>
</art>
