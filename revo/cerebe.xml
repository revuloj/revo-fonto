<kap>
<rad>cerebel</rad>/o
</kap>
<drv mrk="cerebe.0o">
<kap><tld/>o</kap>
<uzo tip="fak">ANA</uzo>
<snc>
  <dif>
    <ref tip="dif" cel="cerb.0eto">Cerbeto</ref>:
      <ekz>
        la viruso anta&ubreve;enmovi&gcirc;as la&ubreve; nervaj fibroj en la cerbon, 
        kie &gcirc;i frapas la sensajn kaj movajn centrojn de la kortiko 
        kaj same la <tld/>on kaj multajn subkortikajn zonojn
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo
          kontra&ubreve; mortiga malsano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_z;&c_zh;&c_a;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_m;&c_o;&c_z;&c_a;&c_ch;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">cerebel</trd>
  <trdgrp lng="cs">
    <trd>cerebellum</trd>,
    <trd>mal&yacute; mozek</trd>,
    <trd>moze&ccaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kleinhirn</trd>,
    <trd>Cerebellum</trd>
  </trdgrp>
  <trd lng="en">cerebellum</trd>
  <trd lng="he">&#x5DE;&#x5D5;&#x5B9;&#x5D7;&#x5B7;
  &#x5DE;&#x5B0;&#x5D0;&#x5D5;&#x5BC;&#x5E8;&#x5B8;&#x5DA;</trd>
  <trd lng="ja">&#x5C0F;&#x8133;  <pr>&#x3057;&#x3087;&#x3046;&#x306E;&#x3046;</pr></trd>
  <trd lng="sk">mal&yacute; mozog</trd>
  <trd lng="uk">&#x43C;&#x43E;&c_z;&#x43E;&c_ch;&#x43E;&#x43A;</trd>
  <trd lng="zh">&#x5C0F;&#x8111; <pr>xi&#x1CE;on&#x1CE;o</pr></trd>
</drv>
