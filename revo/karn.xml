  <trd lng="nl">tandvlees</trd>
  <trd lng="pl">dzi&aogonek;s&lstroke;o</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_n;&c_a;</trd>
  <trd lng="sk">&dcaron;asno</trd>
  <trd lng="sv">tandk&ouml;tt</trd>
  <trd lng="uk">&#x44F;&c_s;&#x43D;&c_a;</trd>
  <trd lng="zh">&#x7259;&#x9F66; <pr>y&#xE1;y&#xED;n</pr></trd>
</drv>

<drv mrk="karn.0okolora">
  <kap><tld/>okoloro</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="karn.0okolora.rozkolora">
    <dif>
      <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">Koloro</ref> de <ref cel="hauxt.0o">ha&ubreve;to</ref> de ne sunbruni&gcirc;inta blankulo:
      <ekz>
        <tld/>okoloraj &scirc;trumpoj;
