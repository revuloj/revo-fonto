    <trd>&c_u;&c_c;&c_je;&c_l;&c_a;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_ib;&c_n;&c_k;&c_a;&c_r;&c_n;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
