  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio de
      <ref tip="super" cel="strige.0oj">strigedoj</ref>
      (<trd lng="la">Bubo</trd>), granda noktobirdo.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_g;&c_a;&c_ch;</trd>
  <trd lng="cs">v&yacute;r velk&yacute;</trd>
  <trd lng="de">Uhu</trd>
  <trd lng="en">eagle-owl</trd>
  <trd lng="fr">hibou<klr tip="ind"> (oiseau)</klr></trd>
  <trdgrp lng="nl">
    <trd>oehoe</trd>,
    <trd>grote <ind>uil</ind></trd>,
    <trd>ooruil</trd>
  </trdgrp>
  <trd lng="hu">uhu</trd>
  <trd lng="pl">puchacz</trd>
  <trd lng="pt">bufo</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_i;&c_n;</trd>
</drv>
</art>
