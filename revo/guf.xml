<vortaro>
<art mrk="$Id: guf.xml,v 1.27 2023/11/11 11:36:37 revo Exp $">
<kap><ofc>*</ofc>
<rad>guf</rad>/o</kap>
<drv mrk="guf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Granda noktobirdo;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio de
      <ref tip="malprt" cel="strige.0oj">strigedoj</ref>
      (<trd lng="la">Bubo</trd>):
      <ekz>
        la neordinara silento &ccirc;irka&ubreve;e estis interrompata nur de la malbela kriado de
        <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur sova&gcirc;aj korvoj &ccirc;i tie krias kaj noktomezaj <tld/>oj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujare minimume 150 breditaj <tld/>idoj povus senprobleme pligrandigi la
        familion de litovaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2012/010875.php">Pereon de specioj malhelpas homoj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="strig.0o">strigo</ref>
    <bld lok="&WCU;/thumb/c/ca/Bubo_bubo_1_(Martin_Mecnarowski).jpg/559px-Bubo_bubo_1_(Martin_Mecnarowski).jpg" prm="CC BY-SA 3.0"><tld/>o kun &ccirc;asita marteso, &Zcaron;&dcaron;&aacute;rsk&eacute; vrchy, &Ccirc;e&hcirc;io
        <fnt>
          <aut>Martin Mecnarowski (http://www.photomecan.eu/), propra verko</aut>
          <lok><url ref="&WCW;/File:Bubo_bubo_1_(Martin_Mecnarowski).jpg">&WCW;/File:Bubo_bubo_1_(Martin_Mecnarowski).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_g;&c_a;&c_ch;</trd>
  <trd lng="bg">&c_b;&c_u;&c_h;&c_a;&c_l;</trd>
  <trd lng="ca">duc</trd>
  <trd lng="cs">v&yacute;r velk&yacute;</trd>
  <trd lng="de">Uhu</trd>
  <trd lng="en">eagle-owl</trd>
  <trd lng="es">b&uacute;ho real</trd>
  <trd lng="fr">hibou<klr tip="ind"> (oiseau)</klr></trd>
  <trd lng="hu">uhu</trd>
  <trd lng="ja">&#x30EF;&#x30B7;&#x30DF;&#x30DF;&#x30BA;&#x30AF; <pr>&#x308F;&#x3057;&#x307F;&#x307F;&#x305A;&#x304F;</pr></trd>
  <trd lng="nl">oehoe</trd>
  <trd lng="pl">puchacz</trd>
  <trd lng="pt">bufo</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_i;&c_n;</trd>
  <trd lng="sk">v&yacute;r ve&lcaron;k&yacute;</trd>
  <trd lng="uk">&c_p;&c_u;&c_g;&c_a;&c_ch;</trd>
  <trdgrp lng="zh">
    <trd>&#x96D5;&#x9D1E; <pr>di&#x101;oxi&#x101;o</pr></trd>,
    <trd>&#x4E00;&#x79CD;&#x732B;&#x5934;&#x9E70; <pr>y&#x12B;zh&#x1D2;ngm&#x101;ot&#xF3;uy&#x12B;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="guf.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="guf.0ujo.ESP">
    <uzo tip="fak">ESP</uzo>
    <dif>
      Dumnokta te<ref tip="super" cel="trink.0ejo.KOMUNE">trinkejo</ref>
      kun trankvila etoso, populara en esperantistaj, precipe
      junularaj kongresoj:
      <ekz>
        <tld lit="G"/>ujo <klr>[&#x2026;]</klr> estis inventita de <nom>Nathalie S.</nom> kaj <nom>Rolfo
        E.</nom>
        kiel alternativa trinkejo (strikte sen fumado kaj sen alkoholo), &gcirc;i unuafoje
        okazis fine de 1995 dum Internacia Seminario
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32409">Gufujo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam mi nun pensas pri la festivalo, mi plej bone
        memoras la etoson <klr>[&#x2026;]</klr> kaj la malfruajn horojn
        kiujn mi pasigis en la <tld/>ujo
        <fnt>
          <aut>Argilo</aut>
          <vrk><url ref="https://www.argilo.net/ijf-artikolo">
          &Ccirc;u esti a&ubreve; ne esti Festivalano?</url></vrk>
          <lok>Tereno de Argilo, 2006-04-18</lok>
        </fnt>;
      </ekz>
      <ekz>
        bonis diskotekoj kaj logis <tld lit="G"/>ujo per siaj bongustegaj teoj kaj
        noktomezaj literaturaj legadoj
        <fnt>
          <bib>LOdE</bib>
          <lok><url ref="&LOdE;65-lode.htm">2000, No 3 (65)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="knajp.0o">knajpo</ref>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_g;&c_a;&c_r;&c_b;&c_a;&c_t;&c_n;&c_ja;</ind> <klr>(&c_n;&c_a; &c_s;&c_h;&c_o;&c_d;&c_k;&c_a;&c_h; &c_e;&c_s;&c_p;&c_e;&c_r;&c_a;&c_n;&c_t;&c_y;&c_s;&c_t;&c_a;&c_w;)</klr></trd>
  <trd lng="de"><ind>Teestube</ind> &leftquot;Gufujo&rightquot;</trd>
  <trd lng="en">tea lounge<klr tip="amb"> (esp. at Esperanto
  gatherings)</klr></trd>
  <trd lng="nl">theesalon<klr tip="amb"> (m.n. bij
  Esperanto-bijeenkomsten)</klr></trd>
</drv>
</art>
