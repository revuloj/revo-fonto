<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brikabrak.xml,v 1.8 2024/05/13 21:22:28 revo Exp $">
<kap>
  <rad>brikabrak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="brikabrak.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="brikabrak.0o.KOMUNE">
    <dif>
      Amasa a&ubreve; malorda kolekto de malnovaj, eluzitaj a&jcirc;oj
      eventuale vendotaj:
      <ekz>
        ni sciis, ke tie estis nur malnova meblaro, <tld/>o kaj
        konfuza&jcirc;oj
        <fnt>
          <aut>A. Munro, trad. W. Kirschstein</aut>,
          <vrk><url ref="&LOdE;Diverse/230nobel.htm">
          Knaboj kaj knabinoj</url></vrk>,
          <lok>La Ondo de Esperanto, 2013:12 (230)</lok>
        </fnt>;
      </ekz>
      <ekz>
        turista <tld/>o: T-&ccirc;emizoj, kru&ccirc;oj, magnetoj por fridujo kaj bildpo&scirc;tkartoj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2015/011540.php"
              >Ampolaj paretoj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        kion mi proceduru nun el &ccirc;i rekviziciita <tld/>o
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 11</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brokan.0ajxo">brokanta&jcirc;o</ref>,
      <ref cel="fatras.0o">fatraso</ref>,
      <ref cel="kicx.0ajxo">ki&ccirc;a&jcirc;o</ref>,
      <ref cel="pelmel.0o">pelmelo</ref>,
      <ref cel="bazar.pul0o">pulbazaro</ref>,
      <ref cel="valor.sen0ajxo">senvalora&jcirc;o</ref>,
      <ref cel="um.0ilo">umilo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_y;&c_z;&c_mol;&c_z;&c_jo;</trd>,
      <trd>&c_l;&c_a;&c_m;&c_a;&c_ch;&c_ch;&c_a;</trd>
    </trdgrp>
    <trd lng="en">bric-a-brac</trd>
    <trdgrp lng="fr">
      <trd>bric-&agrave;-brac</trd>,
      <trd>brocante<klr> (objets)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bibeloty</trd>,
      <trd>starzyzna</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Tr&ouml;del</trd>,
    <trd>Krimskrams</trd>,
    <trd>Kram</trd>,
    <trd>Plunder</trd>,
    <trd>Zeug</trd>,
    <trd>Ramsch<klr>(ware)</klr></trd>,
    <trd>Sammelsurium</trd>,
    <trd>Firlefanz</trd>,
    <trd>Nippes</trd>,
    <trd>Schnickschnack</trd>
  </trdgrp>
  <trd lng="uk">&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n; &c_v;&c_i;&c_p;&c_a;&c_d;&c_k;&c_o;&c_v;&c_i;&c_h; &c_r;&c_je;&c_ch;&c_je;&c_j;</trd>
</drv>
</art>

