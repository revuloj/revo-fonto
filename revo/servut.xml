<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>servut</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="servut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="servut.0o.POL_stato">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Duonsklaveca stato de kamparano nehavanta la rajton transmigri sen
      la konsento de la mastrosinjoro el la bieno, kies &ccirc;iama hereda
      farmanto li estas, kaj devanta plie senpage kulturi la
      mastrosinjoran bienon.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_y;&c_g;&c_o;&c_n;&c_mol;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_g;&c_o;&c_n;</trd>
    </trdgrp>
    <trd lng="en">serfdom</trd>
    <trd lng="fr">servitude</trd>
    <trd lng="hu">jobb&aacute;gys&aacute;g</trd>
    <trd lng="it"><ind>servit&ugrave;</ind> della gleba</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_n;&c_o;&c_je; &c_p;&c_r;&c_a;&c_v;&c_o;</trd>,
      <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
  </snc>
  <snc mrk="servut.0o.POL_servo">
    <dif>
      Laboro, kiun kamparano &scirc;uldas senpage al la mastrosinjoro a&ubreve; al
      la &scirc;tato.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_n;&c_sh;&c_ch;&c_y;&c_n;&c_a;</trd>,
      <trd>&c_p;&c_a;&c_n;&c_sh;&c_ch;&c_y;&c_z;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="fr">corv&eacute;e</trd>
    <trd lng="hu">&uacute;rb&eacute;r</trd>
    <trd lng="ru">&c_b;&c_a;&c_r;&c_shch;&c_i;&c_n;&c_a;</trd>
  </snc>
  <snc mrk="servut.0o.FIG">
    <dif>
      Sklaveco:
      <ekz>
        <tld/>aj manieroj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="en">slavery</trd>
    <trd lng="hu">jobb&aacute;gysors</trd>
    <trd lng="it">servit&ugrave;<klr> (schiavit&ugrave;)</klr></trd>
    <trd lng="ru">&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Frondienst</trd>,
    <trd>Leibeigenschaft</trd>,
    <trd>Knechtschaft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>forced labor</trd>
  </trdgrp>
  <trd lng="nl">lijfeigenschap</trd>
</drv>

<drv mrk="servut.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="servut.0ulo.POL">
    <dif>
      Homo submetita al <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_g;&c_o;&c_n;&c_n;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  <trd lng="de">Leibeigener</trd>
  <trd lng="en">serf</trd>
  <trd lng="fr">serf</trd>
  <trd lng="hu">jobb&aacute;gy</trd>
  <trd lng="it"><ind>servo</ind> della gleba</trd>
  <trd lng="nl">lijfeigene</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_n;&c_o;&c_j; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
</drv>

<drv mrk="servut.en0igi">
  <kap>en<tld/>igi</kap>
  <snc mrk="servut.en0igi.POL">
    <dif>
      Submeti al <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu volo de superrego ne povus kvieti&gcirc;i alie
        ol en la hegemonio de la plej forta &scirc;tato
        super &ccirc;iuj ceteraj en<tld/>igitaj
        <fnt>
          <aut>A. Spinelli, E. Rossi, trad. U. Broccatelli</aut>,
          <vrk><url
          ref="http://www.altierospinelli.org/manifesto/eo/html/manifesto1944eo.html">
          La Manifesto de Ventotene</url></vrk>,
          <lok>[2007]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">asservir</trd>
</drv>
</art>
