<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>le&ubreve;kemi</rad>/o
</kap>
<drv mrk="leuxke.0o">
  <kap>
    <ofc>9</ofc><tld/>o
  </kap>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsano</ref> karakterizata per tromulti&gcirc;o de
      blankaj globetoj en la
      sango.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_j;&c_k;&c_je;&c_m;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
     <trd>Blutkrebs</trd>,
     <trd>Leuk&auml;mie</trd>,
     <trd>Wei&szlig;bl&uuml;tigkeit<klr> [ark]</klr></trd>
  </trdgrp>
  <trd lng="el">&lambda;&epsilon;&ypsilon;&chi;&alfa;&jota;&my;&jota_ton;&alfa;</trd>
  <trd lng="en">leukemia</trd>
  <trd lng="es">leucemia</trd>
  <trdgrp lng="fr">
    <trd>leuc&eacute;mie</trd>,
    <trd>leucose</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>leuk&eacute;mia</trd>,
    <trd>feh&eacute;rv&eacute;r&udblac;s&eacute;g</trd>
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>bloedkanker</trd>,
    <trd>leukemie</trd>,
    <trd>leukose</trd>
  </trdgrp>
  <trd lng="pt">leucemia</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_j;&c_k;&c_je;&c_m;&c_i;&c_ja;</trd>,
    <trd>&c_b;&c_je;&c_l;&c_o;&c_k;&c_r;&c_o;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
