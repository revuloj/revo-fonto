<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>badminton</rad>/o <fnt><bib>NePiv</bib></fnt>
    </kap>

    <drv mrk="badminton.0o">
      <kap><tld/>o</kap>
      <snc mrk="badminton.0o.SPO">
        <uzo tip="fak">SPO</uzo>
        <dif>
          <ref tip="super" cel="sport.0o">Sportludo</ref>
          por du a&ubreve; kvar partoprenantoj, kiuj per batiloj re&jcirc;etas
          flugilhavan pilketon trans mezan reton:
          <ekz>
            en la korto ludis <tld/>on malliberuloj
            <fnt><bib>HomojMan&gcirc;antaj</bib>,
              <lok>&leftquot;man&gcirc;o interne de la &ccirc;irka&ubreve;barilo&rightquot;, p.
67a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <ref tip="vid" cel="tenis.0o">teniso</ref>
      <trd lng="de">Badminton</trd>
      <trd lng="fr">badminton</trd>
      <trd lng="ru">&c_b;&c_a;&c_d;&c_m;&c_i;&c_n;&c_t;&c_o;&c_n;</trd>
    </drv>
  </art>

