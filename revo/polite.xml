<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: polite.xml,v 1.19 2019/06/30 18:21:49 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>politeism</rad>/o
</kap>
<drv mrk="polite.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="super" cel="religi.0o.REL">Religio</ref> kredanta je
      ekzisto de multaj <ref tip="dif" cel="di.0o.politeisma">dioj</ref>:
      <ekz>
        en &ccirc;iuj homaj rilatoj okazis malamoj kaj krimoj, e&ccirc; tie kie svarmis <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2006/008871.php">&Scirc;tato,
          eklezio kaj ideologia laikeco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="di.plur0ismo">plurdiismo</ref>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_l;&c_ib;&c_t;&c_e;&c_ib;&c_z;&c_m;</trd>
  <trd lng="cs">mnohobo&zcaron;stv&iacute;</trd>
  <trd lng="de">Polytheismus</trd>
  <trd lng="en">polytheism</trd>
  <trd lng="es">polite&iacute;smo</trd>
  <trd lng="fr">polyth&eacute;isme</trd>
  <trdgrp lng="hu">
    <trd>politeizmus</trd>,
    <trd>t&ouml;bbistenhit</trd>,
    <trd>sokistenh&iacute;v&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>polythe&iuml;sme</trd>,
    <trd>veelgodendom</trd>
  </trdgrp>
  <trd lng="pt">polite&iacute;smo</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_i;&c_t;&c_je;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_m;&c_n;&c_o;&c_g;&c_o;&c_b;&c_o;&c_zh;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">mnohobo&zcaron;stvo</trd>
</drv>
</art>
