<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alen.xml,v 1.20 2022/03/11 13:58:40 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>alen</rad>/o
</kap>
<drv mrk="alen.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="kudr.0ilo">Kudrilego</ref>
      uzata de ledlaboristoj, &scirc;uistoj:
      <ekz>
        sako <tld/>on ne tenas, &gcirc;i balda&ubreve; elvenas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        prenu <tld/>on kaj enpiku en lian orelon kaj en la pordon, kaj li estos sklavo
        al vi por &ccirc;iam
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 15:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        per la <tld/>o li trapikis al si fingron, sed tio ne estis grava por li
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_l;&c_a;</trd>
  <trd lng="bg">&c_sh;&c_i;&c_l;&c_o;</trd>
  <trd lng="cs">&scaron;&iacute;dlo</trd>
  <trdgrp lng="de">
     <trd>Ahle</trd>,
     <trd>Pfriem</trd>
  </trdgrp>
  <trd lng="en">awl</trd>
  <trd lng="fr">al&egrave;ne</trd>
  <trd lng="he">&#x5DE;&#x5E8;&#x5E6;&#x5E2;</trd>
  <trdgrp lng="nl">
    <trd>els<klr tip="ind"> (priem)</klr></trd>,
    <trd>priem</trd>
  </trdgrp>
  <trd lng="pt">sovela</trd>
  <trd lng="ru">&c_sh;&c_i;&c_l;&c_o;</trd>
  <trd lng="sk">&scaron;idlo</trd>
  <trd lng="sv">syl</trd>
  <trd lng="uk">&#x448;&#x438;&#x43B;&#x43E;</trd>
</drv>
</art>
