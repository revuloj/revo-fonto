<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>rosmaren</rad>/o
</kap>
<drv mrk="rosmar1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Rosmarinus</trd>), unu specio estas la oficina
      <tld/>o,
      arbusto, kun aroma odoro, uzata kiel 
      <ref tip="super" cel="spic.0o">spico</ref> kaj en farmacio.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_m;&c_a;&c_r;&c_y;&c_n;</trd>
  <trd lng="de">Rosmarin</trd>
  <trd lng="en">rosemary</trd>
  <trd lng="es">romero (bot&aacute;nica)</trd>
  <trd lng="fr">romarin</trd>
  <trd lng="hu">rozmarin(g)</trd>
  <trd lng="nl">rozemarijn</trd>
  <trdgrp lng="pt">
    <trd>rosmaninho</trd>,
    <trd>alecrim</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_m;&c_a;&c_r;&c_i;&c_n;</trd>
  <trd lng="tr">biberiye</trd>
</drv>
</art>
