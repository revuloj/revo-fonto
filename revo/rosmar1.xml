<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>rosmaren</rad>/o
</kap>
<drv mrk="rosmar1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lami.0acoj">lamiacoj</ref>
      (<trd lng="la">Rosmarinus</trd>), 
      arbedo, kun aroma odoro, uzata kiel spica&jcirc;o
      kaj en farmacio.
    </dif>
  </snc>
  <trd lng="de">Rosmarin</trd>
  <trd lng="fr">romarin</trd>
  <trd lng="tr">biberiye</trd>
</drv>
</art>
