<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plafon.xml,v 1.25 2023/12/10 17:26:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>plafon</rad>/o
</kap>
<drv mrk="plafon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="plafon.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Horizontala a&ubreve; kurba surfaco, formanta la supran internan parton
      de
      &ccirc;ambro, galerio a&ubreve; alia konstrua&jcirc;o:
      <ekz>
        lampo, pendanta de la <tld/>o, brulis
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la porcio da
        glacia&jcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        La longorelaj vespertoj <klr>...</klr> volonte
        dormas inter la ligna <tld/>o kaj ardeza tegmento de katolika
        pre&gcirc;ejo
        <fnt>
        <aut>Marjorie Boulton</aut>,
        <vrk>Vesproj por vespertoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1997a, numero 8a, p. 30a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_o;&c_l;&c_mol;</trd>
  <trd lng="bo">&#xF50;&#xF7C;&#xF42;&#xF0B;&#xF54;&#xF44;&#xF0B;</trd>
  <trd lng="ca">sostre</trd>
  <trdgrp lng="cs">
    <trd>nejvy&scaron;&scaron;&#xED; dosa&#x17E;iteln&#xE1; v&#xFD;&scaron;ka</trd>,
    <trd>strop</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Decke</trd>,
    <trd>Zimmerdecke</trd>
  </trdgrp>
  <trd lng="en">ceiling</trd>
  <trd lng="es">techo</trd>
  <trdgrp lng="fr">
    <trd>plafond</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mennyezet</trd>,
    <trd>plafon</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#22825;&#x4E95;  <pr>&#12390;&#12435;&#12376;&#12423;&#12358;</pr></trd>,
    <trd>&#x96F2;&#x9AD8;  <pr>&#12358;&#12435;&#12371;&#12358;</pr></trd>,
    <trd>&#x4E0A;&#26119;&#38480;&#x5EA6;  <pr>&#12376;&#12423;&#12358;&#12375;&#12423;&#12358;&#12370;&#12435;&#12393;</pr></trd>,
    <trd>&#26368;&#x9AD8;&#38480;&#x5EA6;  <pr>&#12373;&#12356;&#12371;&#12358;&#12370;&#12435;&#12393;</pr></trd>,
    <trd>&#x4E0A;&#38480;  <pr>&#12376;&#12423;&#12358;&#12370;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">plafond</trd>
  <trdgrp lng="pl">
    <trd>sufit</trd>,
    <trd>strop  <klr>(budownictwo)</klr></trd>
  </trdgrp>
  <trd lng="pt">teto</trd>
  <trd lng="ro">tavan</trd>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_o;&c_l;&c_o;&c_k;</trd>
  <trd lng="sk">najvy&scaron;&scaron;ia dosiahnute&#x13E;n&#xE1; v&#xFD;&scaron;ka</trd>
  <trd lng="sv"><klr>(inner)</klr>tak</trd>
  <trd lng="uk">&c_s;&c_t;&c_je;&#x43B;&#x44F;</trd>
</drv>
</art>
