    <trd>&c_p;&c_o;&c_r;&c_r;&c_i;&c_d;&c_zh;</trd>,
    <trd>&c_o;&c_v;&c_s;&c_ja;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_sh;&c_a;</trd>
