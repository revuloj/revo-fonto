<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: porigx.xml,v 1.9 2022/05/06 22:31:05 revo Exp $">
<kap>
  <rad>pori&gcirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="porigx.0o">
  <kap><tld/>o</kap>
  <snc mrk="porigx.0o.KUI">
  <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="gri.0ajxo">Gria&jcirc;o</ref>, precipe avena, preparita kun lakto a&ubreve; akvo:
      <ekz>
        &scirc;iaj intestoj kaj stomako &scirc;ajnis kunglui&gcirc;i, kaj fine &scirc;i e&ccirc;
        <tld/>on ne povis gluti
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 89a, volumo 3a, p.
        142a</lok></fnt>;
      </ekz>
      <ekz>
        lando de ekstravagancaj insulanoj kun mal&gcirc;ustaflanka trafiko, dueta&gcirc;aj
        omnibusoj, mejloj, pajntoj, futoj, <tld/>o, elo
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 6 (104)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_o;&c_v;&c_je;&c_s;&c_je;&c_n;&c_a; &c_k;&c_a;&c_sh;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Porridge</trd>,
    <trd>Haferbrei</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>porridge</trd>,
    <trd>oatmeal</trd>
  </trdgrp>
  <trd lng="fr">porridge</trd>
  <trdgrp lng="nl">
    <trd>pap</trd>,
    <trd>havermoutpap</trd>
  </trdgrp>
  <trd lng="pl">owsianka  <klr>(potrawa)</klr></trd>
  <trd lng="pt">mingau</trd>
  <trdgrp lng="ro">
    <trd>terciul de ov&abreve;z</trd>,
    <trd>porridge</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_r;&c_r;&c_i;&c_d;&c_zh;</trd>,
    <trd>&c_o;&c_v;&c_s;&c_ja;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_sh;&c_a;</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_ib;&c_v;&c_s;&c_ja;&c_n;&c_a; &c_k;&c_a;&c_sh;&c_a;</trd>
</drv>
</art>

