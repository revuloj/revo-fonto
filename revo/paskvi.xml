<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: paskvi.xml,v 1.18 2018/11/22 20:10:15 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>paskvil</rad>/o
</kap>
<drv mrk="paskvi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      Porokaza <ref tip="super" cel="verk.0o">verketo</ref>, kiu akre atakas iun a&ubreve; ion:
      <ekz>
        <nom>Jos&eacute; Guadalupe Posada</nom>, <tld/>a ilustristo kies gravura&jcirc;oj pri meksika kulturo
        kaj politiko &scirc;an&gcirc;is la pens-manieron de meksikanoj pri post-revolucia Meksiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=535865"
              >Jos&eacute; Clemente Orozco</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;La Vortaro de la Diablo&rightquot; <klr>[&#8230;]</klr> interpretas anglajn frazojn kaj vortojn
        por <tld/>i &jcirc;argonon kaj politikan trompparolon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49951"
              >Ambrose Bierce</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pamfle.0o">pamfleto</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="satir.0o">satiro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_k;&c_v;&c_ib;&c_l;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>hanopis</trd>,
    <trd>paskvil</trd>
  </trdgrp>
  <trd lng="de">Pasquill</trd>
  <trdgrp lng="en">
    <trd>pasquinade</trd>,
    <trd>lampoon</trd>,
    <trd>satire</trd>
  </trdgrp>
  <trd lng="fa">&a_h;&a_j;&a_w;&a_n;&a_A;&a_m;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>libelle</trd>,
    <trd>pamphlet</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>g&uacute;nyirat</trd>,
    <trd>paszkvillus</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>libello</trd>,
    <trd>pasquinata</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>paskwil</trd>,
    <trd>schotschrift</trd>
  </trdgrp>
  <trd lng="pl">paszkwil</trd>
  <trd lng="pt">pasquim</trd>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_k;&c_v;&c_i;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hanopis</trd>,
    <trd>pa&scaron;kvil</trd>
  </trdgrp>
</drv>
</art>
