<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>paskvil</rad>/o
</kap>
<drv mrk="paskvi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      Porokaza verketo, kiu akre atakas iun a&ubreve; ion.
    </dif>
    <refgrp tip="sin">
      <ref cel="pamfle.0o">pamfleto</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="satir.0o">satiro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_k;&c_v;&c_ib;&c_l;&c_mol;</trd>
  <trd lng="de">Pasquill</trd>
  <trdgrp lng="fr">
    <trd>libelle</trd>,
    <trd>pamphlet</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>g&uacute;nyirat</trd>,
    <trd>paszkvillus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>paskwil</trd>,
    <trd>schotschrift</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_s;&c_k;&c_v;&c_i;&c_l;&c_mol;</trd>
</drv>
</art>
