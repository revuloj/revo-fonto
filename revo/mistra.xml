<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mistra.xml,v 1.18 2022/03/23 14:02:26 revo Exp $">
<kap>
  <rad>mistral</rad>/o
</kap>
<drv mrk="mistra.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Denordokcidenta <ref tip="super" cel="vent.0o.MET">vento</ref>
      kiu trablovas la mediteranean marbordon de Francujo:
      <ekz>
        <tld/>o povas blovi je pli ol 90&nbsp;km/h, &ccirc;efe en la rodana valo
       pro la kuvefiko de la Centra Masivo kaj de la Alpoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=248830">Mistralo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_s;&c_t;&c_r;&c_a;&c_l;&c_mol;</trd>
  <trd lng="ca">mestral</trd>
  <trd lng="cs">mistr&aacute;l</trd>
  <trd lng="de">Mistral</trd>
  <trd lng="en">mistral</trd>
  <trd lng="es">mistral</trd>
  <trd lng="fr">mistral</trd>
  <trd lng="hu">misztr&aacute;l</trd>
  <trd lng="nl">mistral</trd>
  <trd lng="pl">mistral</trd>
  <trd lng="pt">mistral</trd>
  <trd lng="ru">&c_m;&c_i;&c_s;&c_t;&c_r;&c_a;&c_l;&c_mol;</trd>
  <trd lng="sk">mistr&aacute;l</trd>
  <trd lng="uk">&#x43C;&#x456;&#x441;&#x442;&#x440;&#x430;&#x43B;&#x44C; (&#x432;&#x456;&#x442;&#x435;&#x440;)</trd>
</drv>
</art>
