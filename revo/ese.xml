<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>ese</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ese.0o">
   <kap><tld/>o</kap>
   <snc mrk="ese.0o.BELE">
     <uzo tip="fak">BELE</uzo>
     <dif>
       Mallonga literatura verko, en kiu la a&ubreve;toro ne intencas
       funde pritrakti sian temon.
     </dif>
   </snc>
   <trd lng="be">&c_e;&c_s;&c_e;</trd>
   <trdgrp lng="de">
     <trd>Essai</trd>,
     <trd>Essay</trd>
   </trdgrp>
   <trd lng="en">essay</trd>
   <trd lng="fr">essai<klr tip="ind"> (litt.)</klr></trd>
   <trd lng="hu">essz&eacute;</trd>
   <trd lng="ru">&c_e;&c_s;&c_s;&c_je;</trd>
</drv>

<drv mrk="ese.0aro">
   <kap><tld/>o</kap>
   <snc>
   <uzo tip="fak">BELE</uzo>
     <dif>
       <ref tip="super" cel="tekst.0aro">Tekstaro</ref> el <tld/>oj,
       ofte sed ne nepre sama&ubreve;toraj a&ubreve; samtemaj:
       <ekz>
         <ctl>Japana <tld lit="E"/>aro n-ro 1</ctl>
         <fnt><vrk>Japana Esaro n-ro 1</vrk>, <lok>Tokio: Libroteko Tokio, 1994</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="fr">recueil d'essais</trd>
</drv>

<drv mrk="ese.0isto">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       A&ubreve;toro konata pro verkado de <tld/>oj.
     </dif>
   </snc>
   <trd lng="fr">essayiste</trd>
</drv>
</art>
