<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tarantas</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="tarantas.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Rusa kampara <ref tip="super" cel="kalesx.0o">kale&scirc;o</ref>:
      <ekz>
        sur la vojo sin montris <tld/>o, al kiu estis jungitaj tri &ccirc;evaloj
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 1a, p. 8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_r;&c_a;&c_n;&c_t;&c_a;&c_s;</trd>
  <trd lng="fr">tarantass</trd>
  <trd lng="hu">tarant&aacute;sz</trd>
</drv>
</art>

