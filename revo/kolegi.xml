<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kolegi</rad>/o
</kap>
<drv mrk="kolegi.0o">
  <kap><tld/>o</kap>
  <snc mrk="kolegi.0o.PED">
    <dif>
      Duagrada <ref tip="super" cel="lern.0ejo.loko">lernejo</ref>, malpli grava ol gimnazio.
    </dif>
    <trd lng="de">Mittelschule</trd>
    <trd lng="en">trade school</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_l;&c_l;&c_je;&c_d;&c_zh;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_l;&c_je;&c_zh;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kolegi.0o.PED.universitato">
    <uzo tip="fak">PED</uzo>
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="univer2.0o">universitato</ref>
    <ekz>
      la Franca <tld/>o;
    </ekz>
    <ekz>
      la <tld/>oj de Kembridge.
    </ekz>
    <trd lng="de">Hochschule</trd>
    <trd lng="en">college</trd>
  </snc>
  <snc mrk="kolegi.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Aro da personoj plenumantaj religian oficon:
      <ekz>
        la <tld/>o de la pontifikoj, de la a&ubreve;guroj, de la kardinaloj.
      </ekz>
    </dif>
    <trd lng="de">Kollegium</trd>
    <trd lng="en">college</trd>
    <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_je;&c_g;&c_i;&c_ja;</trd>
  </snc>
  <trd lng="cs">kolej (&scaron;koln&iacute;)</trd>
  <trdgrp lng="fr">
    <trd>coll&egrave;ge</trd>
  </trdgrp>
  <trd lng="nl">college</trd>
</drv>
</art>
