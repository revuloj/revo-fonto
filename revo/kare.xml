<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kare.xml,v 1.18 2022/06/22 09:30:37 revo Exp $">
<kap>
  <rad>kare</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kare.0o">
  <kap><tld/>o</kap>
  <snc mrk="kare.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="spic.0o.KUI">Spico</ref>, deveninta
      el Orienta Azio, konsistanta el diversaj pulvorigitaj spicoj
      kiel
      <refgrp tip="prt">
        <ref cel="kurkum.0o">kurkumo</ref>,
        <ref cel="zingib.0o">zingibro</ref>, 
        <ref cel="pipr.0o">pipro</ref>,
        <ref cel="muskat.0o">muskatfloro</ref>,
        <ref cel="kariof.0o">kariofilo</ref>, 
        <ref cel="piment.0o">pimento</ref>,
        <ref cel="paprik.0o.pulvoro">papriko</ref>, 
        <ref cel="cinam.0o">cinamo</ref>
      </refgrp>
      k. a.:
      <ekz>
        la <tld/>o estas spicomiksa&jcirc;o venanta el Orienta Azio
        <fnt><bib>Viki</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Alia esperantigo: <ctl>kureo</ctl>.
    <ekz>
      mi <klr>...</klr> entuziasmis pri la perfekta kuirado
      de rizkureo per mia kuirilo
        <fnt>
          <aut>C. Rossetti</aut>,
          <vrk>Kredu Min, Sinjorino!</vrk>,
          <lok>1950</lok>
        </fnt>.
    </ekz>
  </rim>
  <trd lng="be">&c_k;&c_a;&c_r;&c_y; <klr>(&c_z;&c_a;&c_k;&c_r;&c_a;&c_s;&c_a;)</klr></trd>
  <trd lng="bg">&c_k;&c_malmol;&c_r;&c_i;</trd>
  <trd lng="ca">curry</trd>
  <trdgrp lng="cs">
    <trd>kar&eacute;</trd>,
    <trd>pe&ccaron;&iacute;nka</trd>
  </trdgrp>
  <trd lng="de">Curry</trd>
  <trd lng="en">curry</trd>
  <trd lng="es">curry</trd>
  <trd lng="fa">&f_k;&a_A;&a_r;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>curry<klr tip="ind"> (&eacute;pice)</klr></trd>,
    <trd>carry<klr tip="ind"> (&eacute;pice)</klr></trd>,
    <trd>cary<klr tip="ind"> (&eacute;pice)</klr></trd>,
    <trd>cari<klr tip="ind"> (&eacute;pice)</klr></trd>
  </trdgrp>
  <trd lng="hu">curry</trd>
  <trd lng="nl">kerrie</trd>
  <trd lng="pl">curry</trd>
  <trdgrp lng="sk">
    <trd>kar&eacute;</trd>,
    <trd>pe&ccaron;ienka</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>hint baharat&inodot;</trd>,
    <trd>k&ouml;ri</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&c_r;&c_ib;</trd>,
    <trd>&#x43A;&c_je;&c_r;&c_ib;</trd>
  </trdgrp>
</drv>
</art>
