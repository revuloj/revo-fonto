<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kare</rad>/o
</kap>
<drv mrk="kare.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="spic.0o">Spico</ref>, deveninta
      el Orienta Azio, konsistanta el diversaj pulvorigitaj spicoj
      kiel
      <refgrp tip="sub">
        <ref cel="kurkum.0o">kurkumo</ref>,
        <ref cel="zingib.0o">zingibro</ref>, <ref cel="pipr.0o">pipro</ref>,
        <ref cel="muskat.0o">muskatfloro</ref>,
        <ref cel="kariof.0o">kariofilo</ref>, <ref
        cel="piment.0o">pimento</ref>,
        <ref cel="kapsik.0o">kapsiko</ref>, <ref cel="cinam.0o">cinamo</ref>
      </refgrp>
      k.a.:
      <ekz>
        la <tld/>o estas spicomiksa&jcirc;o venanta el Orienta Azio
        <fnt><bib>Viki</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Curry</trd>
  <trd lng="en">curry</trd>
</drv>
</art>
