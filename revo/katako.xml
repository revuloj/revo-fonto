<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: katako.xml,v 1.25 2023/12/03 08:00:24 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>katakomb</rad>/o
</kap>
<drv mrk="katako.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Subteraj galerioj, pasintece uzataj kiel <ref tip="super" cel="tomb.0ejo">tombejo</ref>
      a&ubreve; ostejo:
      <ekz>
        la monar&hcirc;o de Egipto kaj de &ccirc;iuj landoj, kiujn lumigas la suno,
        estis okuponta
        lokon inter siaj respektindaj anta&ubreve;uloj, en la tebaj <tld/>oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; el tombo de pra<tld/>o plorventas kantik'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;atataj de turistoj estas anka&ubreve; la <tld/>oj, precipe tiuj de la
        kapucenoj el la
        16a jarcento
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011265.php">En Palermo subtere</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_k;&c_o;&c_m;&c_b;&c_a;</trd>
  <trd lng="ca">catacumba</trd>
  <trdgrp lng="cs">
    <trd>katakomba</trd>,
    <trd>katakomby</trd>,
    <trd>podzemn&iacute; poh&rcaron;ebi&scaron;t&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Katakombe</trd>
  </trdgrp>
  <trd lng="en">catacomb</trd>
  <trd lng="es">catacumba</trd>
  <trd lng="fr">catacombe</trd>
  <trd lng="hu">katakomba</trd>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30BF;&#x30B3;&#x30F3;&#x30D9;</trd>,
    <trd>&#x5730;&#x4E0B;&#x5893;&#x5730;  <pr>&#x3061;&#x304B;&#x307C;&#x3061;</pr></trd>
  </trdgrp>
  <trd lng="nl">catacombe</trd>
  <trd lng="pl">katakumby</trd>     
  <trd lng="pt">catacumba</trd>
  <trd lng="ro">catacombe</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_a;&c_k;&c_o;&c_m;&c_b;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>katakomba</trd>,
    <trd>katakomby</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x430;&#x442;&#x430;&#x43A;&#x43E;&#x43C;&#x431;&#x430;</trd>
  <trd lng="zh">&#x5893;&#x7A96; <pr>m&#xF9;ji&#xE0;o</pr></trd>
</drv>
</art>
