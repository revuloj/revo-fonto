<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: majork.xml,v 1.4 2022/03/23 14:01:05 revo Exp $">
    <kap>
      <rad>Majork</rad>/o
    </kap>

<drv mrk="majork.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:insuloj" cel="insul.0o.GEOG">Insulo</ref>
      en okcidenta <ref tip="malprt" cel="mediterane.M0o">Mediteraneo</ref>,
      apartenanta al <ref tip="malprt" cel="hispan.H0ujo">Hispanujo</ref>, situanta proksimume 170&#x202F;km
      sude de Barcelono:
      <ekz>
        en <tld/>o oni fermis la tunelon de <nom>S&oacute;ller</nom> pro la
        lavango da uzantoj rifuzantaj
        pagi
        <fnt>
          <bib>Monato</bib>
          <aut>Jaume Clotet</aut>
          <vrk><url ref="https://www.monato.net/2012/010745.php">Mi ne volas pagi ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a Unio, la partio reginta en <tld/>o inter 1995 kaj 2007
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="https://www.monato.net/2011/010471.php">Koruptado demisiigas prezidanton</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_M;&c_a;&c_jo;&c_r;&c_k;&c_a;</trd>
    <trd lng="de">Mallorca</trd>
    <trd lng="pl">Majorka</trd>     
    <trd lng="ro">Mallorca</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x41C;&#x430;&#x43B;&#x44C;&#x439;&#x43E;&#x440;&#x43A;&#x430;</trd>,
    <trd>&#x41C;&#x430;&#x439;&#x43E;&#x440;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>

  </art>

