<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>cili</rad>/o
</kap>
<drv mrk="cili.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="cili.0o.BOT.ZOO">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Rigidaj haroj, kiuj trovi&gcirc;as sur la korpo de iuj insektoj, a&ubreve; &ccirc;e
      randoj de vegeta&jcirc;aj organoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="har">okulharo</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Wimperhaar</trd>,
      <trd>Zilie</trd>
    </trdgrp>
  </snc>
  <snc mrk="cili.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Moveblaj harformaj elstara&jcirc;oj de protoplasmo, kiuj trovi&gcirc;as &ccirc;e iuj
      &ccirc;eloj.
    </dif>
  </snc>
  <trd lng="en">cilia</trd>
  <trd lng="fr">cil</trd>
  <trd lng="ru">&c_r;&c_je;&c_s;&c_n;&c_i;&c_ch;&c_k;&c_a;</trd>
</drv>
</art>
