<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>cili</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cili.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="cili.0o.BIO">
    <dif>
      Rigida haro de vivulo.
    </dif>
    <refgrp tip="vid">
      <ref cel="har">okulharo</ref>.
    </refgrp>
    <subsnc mrk="cili.0o.ZOO">
      <uzo tip="fak">ZOO</uzo>
      <dif>
        Rigida haro, kiu trovi&gcirc;as sur la korpo de iuj insektoj.
      </dif>
    </subsnc>
    <subsnc mrk="cili.0o.BOT">
      <uzo tip="fak">BOT</uzo>
      <dif>
        Rigida haro, kiu trovi&gcirc;as &ccirc;e randoj de iuj
        vegeta&jcirc;aj organoj.
      </dif>
    </subsnc>
    <subsnc mrk="cili.0o.BAK">
      <uzo tip="fak">BAK</uzo>
      <dif>
        Moveblaj harformaj elstara&jcirc;oj de iuj &ccirc;eloj.
      </dif>
      <refgrp tip="vid">
        <ref cel="flagel.0o.BIO">flagelo</ref>
      </refgrp>
      <trd lng="pl">rz&eogonek;ska</trd>
    </subsnc>
    <trdgrp lng="de">
      <trd>Wimperhaar</trd>,
      <trd>Zilie</trd>
    </trdgrp>
    <ekz>
<ind><tld/>oj</ind>;
<trd lng="de">Wimpern</trd>
</ekz> 
    <trdgrp lng="pl">
      <trd>rz&eogonek;ska</trd>,
      <trd>rz&eogonek;sa</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_v;&c_je;&c_j;&c_k;&c_a;</trd>
  <trd lng="en">cilia</trd>
  <trd lng="fr">cil</trd>
  <trdgrp lng="hu">
    <trd>pillasz&odblac;r</trd>,
    <trd>csill&oacute;</trd>,
    <trd>csillang&oacute;</trd>
  </trdgrp>
  <trd lng="id">silium</trd>
  <trd lng="pt">c&iacute;lio</trd>
  <trd lng="ru">&c_r;&c_je;&c_s;&c_n;&c_i;&c_ch;&c_k;&c_a;</trd>
</drv>

<drv mrk="cili.0uloj">
  <kap>ciliuloj</kap>
  <snc mrk="cili.0uloj.BAK">
    <uzo tip="fak">BAK</uzo>
    <dif>
      E&ubreve;kariotaj unu&ccirc;eluloj kun organetoj la&ubreve;funkcie
      grupigitaj, kiuj movi&gcirc;as per <tld/>oj;
      klaso el filumo
      <ref tip="super" cel="protoz.0o.BAK">protozooj</ref>
      (<trd lng="la">Ciliata</trd>):
      <ekz>
        la <tld/>a movo estas karakteriza por la <tld/>uloj
        <fnt>
          <aut>C. St&oslash;p-Bowitz, M. B&eacute;la</aut>,
          <vrk><url
          ref="http://www.freeweb.hu/eventoj/steb/biologio/enkond-fokuso/f874-p08.htm">
          Enkonduko en la biologion</url></vrk>,
          <lok>Fokuso, numero 1987-04, p. 8a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Wimpertierchen<klr> (biol., Ciliophora,
  Ciliata)</klr></trd>
  <trd lng="fr">cili&eacute;s<klr> (biol., classe)</klr></trd>
  <trd lng="id">ciliata</trd>
</drv>
</art>
