  <trd lng="hi">&#x92C;&#x948;&#x902;&#x915;</trd>
  <trd lng="hr">banka</trd>
  <trd lng="hu">bank</trd>
  <trd lng="hy">&#x562;&#x561;&#x576;&#x56F;</trd>
