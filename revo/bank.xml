<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>bank</rad>/o
</kap>
<drv mrk="bank.0o">
<kap><tld/>o</kap>
<snc>
<dif>
&Scirc;tata institucio a&ubreve; privata firmo por financaj operacioj (pruntoj, kambioj, &ccirc;ekoj, vendo k a&ccirc;eto de valora&jcirc;oj).  
</dif>
</snc>
<trd lng="ru">&c_b;&c_a;&c_n;&c_k;</trd>
</drv>
<drv mrk="bank.0isto">
<kap><tld/>isto</kap>
<snc>
<dif>
Oficisto a&ubreve; estro kiu sin okupas je <tld/>aj aferoj. 
</dif>
</snc>
<trd lng="ru">&c_s;&c_l;&c_u;&c_zh;&c_a;&c_shch;&c_i;&c_j; &c_b;&c_a;&c_n;&c_k;&c_a;</trd>
</drv>
<drv mrk="bank.0estro">
<kap><tld/>estro</kap>
<snc>
<dif>
Direktoro de <tld/>o. 
</dif>
</snc>
<trd lng="ru">&c_d;&c_i;&c_r;&c_je;&c_k;&c_t;&c_o;&c_r; &c_b;&c_a;&c_n;&c_k;&c_a;</trd>
</drv>
<drv mrk="bank.0bileto">
<kap><tld/>bileto</kap>
<snc>
<dif>
Papera mono eldonita de <tld/>o.  
</dif>
</snc>
<trdgrp lng="ru">
  <trd>&c_b;&c_a;&c_n;&c_k;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j; &c_b;&c_i;&c_l;&c_je;&c_t;</trd>,
  <trd>&c_b;&c_a;&c_n;&c_k;&c_n;&c_o;&c_t;</trd>
</trdgrp>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
