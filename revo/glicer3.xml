<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>glicerin</rad>/o
</kap>
<drv mrk="glicer3.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      C<sub>3</sub>H<sub>8</sub>O<sub>3</sub>.
      Densa, dol&ccirc;a flua&jcirc;o, ekstraktata el grasoj, kaj uzata por
&scirc;miri
      la ha&ubreve;ton, fabriki eksploda&jcirc;ojn ktp.  
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ib;&c_c;&c_e;&c_r;&c_y;&c_n;</trd>
  <trdgrp lng="de">
     <trd>Glyzerin</trd>,
     <trd>&Ouml;ls&uuml;&szlig;</trd></trdgrp>
  <trd lng="es">glicerina</trd>
  <trdgrp lng="fr">
    <trd>glyc&eacute;rine</trd>,
    <trd>glyc&eacute;rol</trd></trdgrp> 
  <trd lng="hu">glicerin</trd>
  <trdgrp lng="nl">
    <trd>glycerol</trd>,
    <trd>glycerine</trd>
  </trdgrp> 
  <trdgrp lng="pt">
    <trd>glicerina</trd>, 
    <trd>glicerol</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_l;&c_i;&c_c;&c_je;&c_r;&c_i;&c_n;</trd>
  <trd lng="tr">gliserin</trd>
</drv>
</art>
