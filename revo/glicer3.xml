<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glicer3.xml,v 1.19 2022/03/19 19:18:23 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>glicerin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="glicer3.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      C<sub>3</sub>H<sub>8</sub>O<sub>3</sub>.
      Densa, dol&ccirc;a flua&jcirc;o, ekstraktata el grasoj,
      kaj uzata por &scirc;miri
      la ha&ubreve;ton, fabriki eksploda&jcirc;ojn ktp:  
      <ekz>
        55 gramoj da <tld/>o estas solvataj en unu litro da 
        63-procenta alkoholo <klr>...</klr>
        <fnt>
          <aut>-</aut>
          <vrk>El la mondo</vrk>
          <lok><url ref="http://anno.onb.ac.at/cgi-content/anno-plus?aid=e0a&amp;datum=1892&amp;page=23">
          La esperantisto, 1892:2, p. 23a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ib;&c_c;&c_e;&c_r;&c_y;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>glicerina</trd>,
    <trd>glicerol</trd>
  </trdgrp>
  <trd lng="cs">glycerin</trd>
  <trdgrp lng="de">
     <trd>Glyzerin</trd>,
     <trd>&Ouml;ls&uuml;&szlig;</trd></trdgrp>
  <trd lng="es">glicerina</trd>
  <trdgrp lng="fr">
    <trd>glyc&eacute;rine</trd>,
    <trd>glyc&eacute;rol</trd></trdgrp>
  <trd lng="hu">glicerin</trd>
  <trdgrp lng="nl">
    <trd>glycerol</trd>,
    <trd>glycerine</trd>
  </trdgrp>
  <trd lng="pl">gliceryna</trd>
  <trdgrp lng="pt">
    <trd>glicerina</trd>, 
    <trd>glicerol</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_l;&c_i;&c_c;&c_je;&c_r;&c_i;&c_n;</trd>
  <trd lng="sk">glycer&iacute;n</trd>
  <trd lng="tr">gliserin</trd>
  <trd lng="uk">&#x433;&#x43B;&#x456;&#x446;&#x435;&#x440;&#x438;&#x43D;</trd>
</drv>
</art>
