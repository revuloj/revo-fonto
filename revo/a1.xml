<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>a</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="a1.0.vokalo">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      Unu el la kvin <ref tip="dif" cel="vokal.0o">vokaloj</ref> de
      Esperanto, la centra, plej vasta, kaj
      nomo de tiu <ref tip="dif" cel="fonem.0o">fonemo</ref> [<tld/>].
    </dif>
    <trd lng="da">a<klr> (vokal)</klr></trd>
    <trd lng="fr">a<klr> (voyelle)</klr></trd>
    <trd lng="nl">a<klr> (klinker)</klr></trd>
    <trd lng="no">a<klr> (vokal)</klr></trd>
    <trd lng="pt">a<klr> (vogal)</klr></trd>
  </snc>
  <snc>
     <dif>
      La unua <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta
      alfabeto, uzata i.a. kiel:
     </dif>
     <trd lng="da">a<klr> (bogstav)</klr></trd>
     <trd lng="fr">a<klr> (lettre)</klr></trd>
     <trd lng="nl">a<klr> (letter)</klr></trd>
     <trd lng="no">a<klr> (bokstav)</klr></trd>
     <trd lng="pt">a<klr> (letra)</klr></trd>
     <subsnc>
     <uzo tip="fak">MUZ</uzo>
     <dif>
     (maj.) nomo de la sesa tono de la C-ma&jcirc;ora
     gamo (440 Hz; absolut-solfe&gcirc;a la), konvencie uzata kiel bazo por
     agordi instrumenton kaj por kunagordi la instrumentojn de
     orkestro:<ekz> <tld lit="A"/> bemola, diesa</ekz>;
     </dif>
     <trd lng="da">a<klr> (tone)</klr></trd>
     <trd lng="fr">la</trd>
     <trd lng="nl">la<klr> (muzieknoot)</klr></trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">KEM</uzo>
     <dif>
     (maj.) simbolo de argono &gcirc;is 1957:<ekz> <tld
     lit="A"/></ekz>;
     </dif>
     <trd lng="fr">argon</trd>
     <trd lng="nl">argon</trd>
     <trd lng="no">argon</trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">ELE</uzo>
     <dif>
     (maj.) simbolo de ampero:<ekz> <tld lit="A"/></ekz>;
     </dif>
     <trd lng="da">A<klr> (Ampere)</klr></trd>
     <trd lng="fr">Amp&egrave;re</trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">TIP</uzo>
     <dif>
     (maj.) kodo de normigitaj formatoj de folioj: <ekz><tld lit="A"/>4
     (297 &#215; 210 mm)</ekz>;
     </dif>
     <trd lng="da">A4</trd>
     <trd lng="fr">A4</trd>
     <trd lng="nl">A4</trd>
     <trd lng="no">A4</trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">GEOG</uzo>
     <dif>
     (min.) simbolo de aro: <ekz><tld/></ekz>;
     </dif>
     <trd lng="da">ar<klr> (fladem&aring;l)</klr></trd>
     <trd lng="fr">are<klr> (unit&eacute; de surface)</klr></trd>
     </subsnc>   
  </snc>
  <snc>
  <ref tip="vid" cel="a.0">-<tld/> (fina&jcirc;o)</ref>
  <trd lng="da">-a<klr> (endelse)</klr></trd>
  <trd lng="fr">-a<klr> (terminaison)</klr></trd>
  <trd lng="no">-a<klr> (endelse)</klr></trd>
  </snc>
</drv>

<drv mrk="a1.0.volvita">
  <kap><tld/> volvita</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="fak">TIP</uzo>
    <refgrp tip="dif">
      <ref cel="po.0-signo">po-signo</ref>,
      <ref cel="helik.0o.ascii">heliko</ref>
    </refgrp>
    <dif>
    </dif>
  </snc>
   <trd lng="da">snabel-a</trd>
 <trdgrp lng="fr">
   <trd>arobase</trd>,
   <trd>@</trd>
 </trdgrp>
 <trd lng="nl">apestaart</trd>
 <trdgrp lng="no">
   <trd>kr&oslash;llalfa</trd>,
   <trd>alfakr&oslash;ll</trd>
 </trdgrp>
 <trd lng="pt">arroba</trd>
</drv>
</art>

