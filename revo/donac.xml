<drv mrk="donac.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senkompense <ref tip="super" cel="don.0i.disponigi">doni</ref>,
      kiel ateston de bonvolemo, amikeco, favoro, ktp:
      <ekz>
        mi ja ne <tld/>as al vi tiun &ccirc;i monon, mi &gcirc;in pruntas
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen, mi <tld/>os unu pomon al vi
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>os al li la vivon, se li alportos <klr>[&#8230;]</klr> &ccirc;evalon, kiu kuras pli
        rapide ol la vento
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u &scirc;in al mi, &ccirc;ar mi ne povos vivi
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        naztukon kun sia nomo &scirc;i <tld/>is al la &ccirc;asisto
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la naturo <tld/>is al ili neordinaran forton
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is al la re&gcirc;o cent dudek kikarojn da oro
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 10:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        la estroj de la Levidoj, <tld/>is al la Levidoj por la Paskoj kvin mil &scirc;afidojn
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 35:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li akiris alian koron, ankora&ubreve; anta&ubreve; ol mi havis koron, por <tld/>i al li
        <klr>(t.e. ekami lin)</klr>
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fra&ubreve;lino, <tld/>u al mi vian manon
        <klr>(<ref cel="edz.0igi">edzigu</ref> min)</klr>
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>!
      </ekz>
      <ekz>
        la re&gcirc;o <tld/>is <klr>(kp <ref cel="atribu.0i.asigni">atribui</ref>)</klr>
        al la amba&ubreve; <klr>[&#8230;]</klr> kavaliran krucon kaj la titolon de
        sekretaj teksistoj de la kortego
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>as por speso kaj laborigas por spesmilo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        al &ccirc;evalo <tld/>ita oni bu&scirc;on ne
        esploras
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dot.0i">doti</ref>,
      <ref cel="ofer.0i">oferi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_d;&c_a;&c_r;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>darovat</trd>,
    <trd>d&aacute;t <klr>(n&ecaron;co n&ecaron;komu)</klr></trd>,
    <trd>obdarovat (n&ecaron;koho n&ecaron;&ccaron;&iacute;m)</trd>
  </trdgrp>
  <trd lng="de">schenken</trd>
  <trd lng="en">donate</trd>
  <trdgrp lng="es">
    <trd>donar</trd>,
    <trd>regalar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>donner en cadeau</trd>,
    <trd>offrir<klr tip="ind"> (un cadeau)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>derma</baz>mendermakan</trd>,
    <trd><baz>donasi</baz>mendonasikan</trd>,
    <trd><baz>hadiah</baz>menghadiahkan</trd>,
    <trd><baz>infak</baz>menginfakkan</trd>,
    <trd><baz>sumbang</baz>menyumbangkan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8D08;&#x308B;  <pr>&#x304A;&#x304F;&#x308B;</pr></trd>,
    <trd>&#x5BC4;&#x8D08;&#12377;&#x308B;  <pr>&#x304D;&#x305E;&#12358;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>darowa&cacute;<klr tip="amb"> (dawa&cacute;)</klr></trd>,
    <trd>podarowa&cacute;</trd>,
    <trd>ofiarowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>presentear</trd>,
    <trd>doar</trd>,
    <trd>brindar</trd>,
    <trd>dar</trd>,
    <trd>oferecer</trd>,
    <trd>fazer <ind>doa&ccedil;&atilde;o</ind> de</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">darova&tcaron;</trd>
  <trd lng="uk">&c_d;&c_a;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x8D08;&#x4E88; <pr>z&#xE8;ngy&#x1D4;</pr></trd>,
    <trd>&#x8D08;&#36865; <pr>z&#xE8;ngs&#xF2;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="donac.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="D"/>ita&jcirc;o:
      <ekz>
        vi estas tiel <klr>[&#8230;]</klr> bona kaj tiel honesta, ke mi devas fari al vi
        <tld/>on
        <fnt>
          <bib>F</bib>
          <lok>&FE; 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> dankis la maljunulinon pro la <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi pardonemigos lin per la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 32:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        anonima <tld/>o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi helpis min per konsilo kaj <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> donis al li returne la dokumenton de <tld/>o
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        per favoroj kaj <tld/>oj oni solvas multajn malfacila&jcirc;ojn
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kiu <tld/>on prenas, tiu sin katenas</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de"><ind>Geschenk</ind>e verpflichten</trd>
      </ekz>
      <ekz>
        <tld/>o nesincera estas <tld/>o infera
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_r;&c_u;&c_n;&c_a;&c_k;</trd>
  <trd lng="bg">&c_p;&c_o;&c_d;&c_a;&c_r;&c_malmol;&c_k;</trd>
  <trd lng="bo">&t_da;&t_naro;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>dar</trd>,
    <trd>d&aacute;rek</trd>,
    <trd>nad&iacute;lka</trd>,
    <trd>v&yacute;sluha</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geschenk</trd>,
    <trd>Gabe</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>donaci&oacute;n</trd>,
    <trd>regalo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cadeau</trd>,
    <trd>don</trd>,
    <trd>pr&eacute;sent<klr tip="ind"> (cadeau)</klr></trd>
  </trdgrp>
  <trd lng="he">&mem;&tav;&nun;&he;</trd>
  <trdgrp lng="id">
    <trd>derma</trd>,
    <trd>donasi</trd>,
    <trd>hadiah</trd>,
    <trd>infak</trd>,
    <trd><baz>sumbang</baz>sumbangan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8D08;&#x308A;&#29289;  <pr>&#x304A;&#x304F;&#x308A;&#12418;&#x306E;</pr></trd>,
    <trd>&#x8D08;&#x4E0E;  <pr>&#x305E;&#12358;&#12424;</pr></trd>,
    <trd>&#x8D08;&#21576;  <pr>&#x305E;&#12358;&#12390;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>prezent</trd>,
    <trd>dar</trd>,
    <trd>podarunek</trd>,
    <trd>upominek</trd>
  </trdgrp>
  <trd lng="ro">cadou</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_r;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">obdarovanie</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_r;</trd>,
    <trd>&c_d;&c_a;&c_r;&c_u;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_r;&c_u;&c_n;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x793C;&#29289; <pr>l&#x1D0;w&#xF9;</pr></trd>,
    <trd>&#x63A8;&#33616;&#x4E66; <pr>tu&#x12B;ji&#xE0;nsh&#x16B;</pr></trd>,
    <trd>&#x793C;&#x54C1; <pr>l&#x1D0;p&#x1D0;n</pr></trd>,
    <trd>&#x994B;&#x8D08;&#x54C1; <pr>ku&#xEC;z&#xE8;ngp&#x1D0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="donac.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Kiel <tld/>o(n):
      <ekz>
       <tld/>e ricevi, prezenti;
      </ekz>
      <ekz>
        la ar&gcirc;enton <klr>[li]</klr> sendis <klr>[&#8230;]</klr> donace al la re&gcirc;o de Asirio
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 16:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estis lia poeta talento, kiun Dio <tld/>e metis al li en la lulilon
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        eminenta sinjoro <klr>[&#8230;]</klr> forte la&ubreve;dadis la &ccirc;evalon <klr>[&#8230;]</klr>
        dezirante ricevi &gcirc;in <tld/>e
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;
  <ind>&c_p;&c_a;&c_d;&c_a;&c_r;&c_u;&c_n;&c_a;&c_k;</ind></trd>
  <trd lng="cs">darem</trd>
  <trdgrp lng="de">
    <trd>geschenkt</trd>,
    <trd>als <ind>Geschenk</ind></trd>
  </trdgrp>
  <trd lng="es">de <ind>regalo</ind></trd>
  <trd lng="fr">en <ind>cadeau</ind></trd>
  <trd lng="he">&bet;&mem;&tav;&nun;&he;</trd>
  <trd lng="id"><baz>bagai</baz>sebagai hadiah</trd>
  <trd lng="ja">&#x8D08;&#x308A;&#29289;&#12392;&#12375;&#12390;  <pr>&#x304A;&#x304F;&#x308A;&#12418;&#x306E;&#12392;&#12375;&#12390;</pr></trd>
  <trdgrp lng="pl">
    <trd>w <ind>prezencie</ind></trd>,
    <trd>w <ind>darze</ind></trd>,
    <trd>jako <ind>podarunek</ind></trd>,
    <trd>jako <ind>upominek</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_v;
  <ind>&c_p;&c_o;&c_d;&c_a;&c_r;&c_o;&c_k;</ind></trd>
  <trd lng="sk">darom</trd>
</drv>

<drv mrk="donac.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Volonte <tld/>anta:
      <ekz>
        mi &gcirc;ojas konstati, ke vi estas <tld/>ema
        <fnt>
          <bib>KrB</bib>
          <lok>L&#8217; avarulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ni]</klr> dankinte foriris, impresite de la <tld/>ema malavareco
        <fnt>
       <aut>Werner Schad</aut>
          <vrk><url ref="&Monato;/2000/006868.php">Inter maro kaj arbarego</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        kampanjo necesis havigi subtenon apelaciante al la <tld/>emo de la membraro
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="avar.mal0a">malavara</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_o;&c_d;&c_r;&c_y; <klr>(&c_n;&c_a; &c_p;&c_a;&c_d;&c_a;&c_r;&c_u;&c_n;&c_k;&c_ib;, &c_a;&c_h;&c_v;&c_ja;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ib;)</klr></trd>
  <trd lng="cs">&scaron;t&ecaron;dr&yacute;</trd>
  <trdgrp lng="de">
    <trd>gener&ouml;s</trd>,
    <trd>freigiebig</trd>
  </trdgrp>
  <trd lng="es">generoso</trd>
  <trd lng="fr">g&eacute;n&eacute;reux</trd>
  <trd lng="he">&nun;&dalet;&yod;&bet;</trd>
  <trdgrp lng="id">
    <trd>dermawan</trd>,
    <trd><ind>murah</ind> hati</trd>,
    <trd>karim</trd>
  </trdgrp>
  <trd lng="ja">&#x6C17;&#x524D;&#x306E;&#12356;&#12356;  <pr>&#x304D;&#x307E;&#12360;&#x306E;&#12356;&#12356;</pr></trd>
  <trdgrp lng="pl">
    <trd>hojny</trd>,
    <trd>szczodry</trd>
  </trdgrp>
  <trd lng="ru">&c_shch;&c_je;&c_d;&c_r;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>radodajn&yacute;</trd>,
    <trd>&scaron;tedr&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_t;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_shch;&c_je;&c_d;&c_r;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x96CD;&#x5BB9;&#22823;&#x5EA6; <pr>y&#x14D;ngr&#xF3;ngd&#xE0;d&#xF9;</pr></trd>,
    <trd>&#24936; <pr>k&#x1CE;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="donac.mon0o">
  <kap>
    mon<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="D"/>o de mono, ordinare por iu bonafara celo, projekto ks:
      <ekz>
        li promesis anka&ubreve; al la aliaj iomete mon<tld/>i
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> ricevis de Nacia Hereda&jcirc;a Memoriga Fonda&jcirc;o rekordan
        mon<tld/>egon de pli ol 17 milionoj da pundoj
        <fnt>
          <bib>Monato</bib>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2004/008282.html">Volontuli en Tyntesfield</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        temploj <klr>[&#8230;]</klr>j dependas de mon<tld/>o de siaj kredantoj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2008/009633.php">Religio en Japanio. &Ccirc;u budhismo?</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &leftquot;Obolo de Sankta Petro&rightquot; <klr>[estas]</klr> mon<tld/>oj al la papo fare de
        milionoj da fideluloj
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2007/009297.php">Spezokonto por la jaro 2006 pozitiva</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="almoz.0o">almozo</ref>,
      <ref cel="benefi.0o">benefico</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_h;&c_v;&c_ja;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je; <klr>(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_je;)</klr></trd>
  <trdgrp lng="de">
    <trd>Spende</trd>,
    <trd>Geldzuwendung</trd>
  </trdgrp>
  <trd lng="en">donation</trd>
  <trdgrp lng="id">
    <trd>donasi</trd>,
    <trd>infak</trd>,
    <trd>sedekah</trd>,
    <trd><baz>sumbang</baz>sumbangan</trd>
  </trdgrp>
  <trd lng="ja">&#x5BC4;&#x4ED8;  <pr>&#x304D;&#12405;</pr></trd>
  <trdgrp lng="zh">
    <trd>&#25424;&#x6B3E; <pr>ju&amacron;nku&#x1CE;n</pr></trd>,
    <trd>&#20851;&#29031; <pr>gu&amacron;nzh&#xE0;o</pr></trd>,
    <trd>&#25424;&#x732E; <pr>ju&amacron;nxi&#xE0;n</pr></trd>,
    <trd>&#25424;&#x52A9; <pr>ju&amacron;nzh&#xF9;</pr></trd>
  </trdgrp>
</drv>

</art>
