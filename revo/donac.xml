<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: donac.xml,v 1.22 2021/05/28 12:52:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>donac</rad>/i
</kap>

<drv mrk="donac.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senkompense doni, kiel ateston de bonvolemo, amikeco, favoro, ktp:
      <ekz>
        mia patro <tld/>is al mi belan libron;
      </ekz>
      <ekz>
        oni povas multe <tld/>i k ne esti amata;
      </ekz>
      <ekz>al &ccirc;evalo <tld/>ita oni bu&scirc;on ne
      esploras<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dot.0i">doti</ref>,
      <ref cel="ofer.0i">oferi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_d;&c_a;&c_r;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>darovat</trd>,
    <trd>d&aacute;t <klr>(n&ecaron;co n&ecaron;komu)</klr></trd>,
    <trd>obdarovat (n&ecaron;koho n&ecaron;&ccaron;&iacute;m)</trd>
  </trdgrp>
  <trd lng="de">schenken</trd>
  <trd lng="en">donate</trd>
  <trdgrp lng="es">
    <trd>donar</trd>,
    <trd>regalar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>donner en cadeau</trd>,
    <trd>offrir<klr tip="ind"> (un cadeau)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>darowa&cacute;<klr tip="amb"> (dawa&cacute;)</klr></trd>,
    <trd>podarowa&cacute;</trd>,
    <trd>ofiarowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>presentear</trd>,
    <trd>doar</trd>,
    <trd>brindar</trd>,
    <trd>dar</trd>,
    <trd>oferecer</trd>,
    <trd>fazer <ind>doa&ccedil;&atilde;o</ind> de</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">darova&tcaron;</trd>
</drv>

<drv mrk="donac.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="D"/>ita&jcirc;o:
      <ekz>
        novjaraj <tld/>oj;
      </ekz>
      <ekz>
        doni, fari <tld/>on;
      </ekz>
      <ekz>
        elokvento estis en li natura <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_r;&c_u;&c_n;&c_a;&c_k;</trd>
  <trd lng="bg">&c_p;&c_o;&c_d;&c_a;&c_r;&c_malmol;&c_k;</trd>
  <trd lng="bo">&#xF51;&#xF7C;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>dar</trd>,
    <trd>d&aacute;rek</trd>,
    <trd>nad&iacute;lka</trd>,
    <trd>v&yacute;sluha</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geschenk</trd>,
    <trd>Gabe</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>donaci&oacute;n</trd>,
    <trd>regalo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cadeau</trd>,
    <trd>don</trd>,
    <trd>pr&eacute;sent<klr tip="ind"> (cadeau)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5EA;&#x5E0;&#x5D4;</trd>
  <trdgrp lng="pl">
    <trd>prezent</trd>,
    <trd>dar</trd>,
    <trd>podarunek</trd>,
    <trd>upominek</trd>
  </trdgrp>
  <trd lng="ro">cadou</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_r;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">obdarovanie</trd>
</drv>

<drv mrk="donac.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Kiel <tld/>o(n):
      <ekz>
       <tld/>e ricevi, prezenti.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;
  <ind>&c_p;&c_a;&c_d;&c_a;&c_r;&c_u;&c_n;&c_a;&c_k;</ind></trd>
  <trd lng="cs">darem</trd>
  <trdgrp lng="de">
    <trd>geschenkt</trd>,
    <trd>als <ind>Geschenk</ind></trd>
  </trdgrp>
  <trd lng="es">de <ind>regalo</ind></trd>
  <trd lng="fr">en <ind>cadeau</ind></trd>
  <trd lng="he">&#x5D1;&#x5DE;&#x5EA;&#x5E0;&#x5D4;</trd>
  <trdgrp lng="pl">
    <trd>w <ind>prezencie</ind></trd>,
    <trd>w <ind>darze</ind></trd>,
    <trd>jako <ind>podarunek</ind></trd>,
    <trd>jako <ind>upominek</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_v;
  <ind>&c_p;&c_o;&c_d;&c_a;&c_r;&c_o;&c_k;</ind></trd>
  <trd lng="sk">darom</trd>
</drv>

<drv mrk="donac.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Volonte <tld/>anta.
    </dif>
    <refgrp tip="vid">
      <ref cel="avar.mal0a">malavara</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">&scaron;t&ecaron;dr&yacute;</trd>
  <trdgrp lng="de">
    <trd>gener&ouml;s</trd>,
    <trd>freigiebig</trd>
  </trdgrp>
  <trd lng="es">generoso</trd>
  <trd lng="fr">g&eacute;n&eacute;reux</trd>
  <trd lng="he">&#x5E0;&#x5D3;&#x5D9;&#x5D1;</trd>
  <trdgrp lng="pl">
    <trd>hojny</trd>,
    <trd>szczodry</trd>
  </trdgrp>
  <trd lng="ru">&c_shch;&c_je;&c_d;&c_r;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>radodajn&yacute;</trd>,
    <trd>&scaron;tedr&yacute;</trd>
  </trdgrp>
</drv>
</art>
