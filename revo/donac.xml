<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>donac</rad>/i
</kap>
<drv mrk="donac.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senkompense doni, kiel ateston de bonvolemo, amikeco, favoro, ktp:
      <ekz>
        mia patro <tld/>is al mi belan libron;
      </ekz>
      <ekz>
        oni povas multe <tld/>i k ne esti amata;
      </ekz>
      <ekz>al &ccirc;evalo <tld/>ita oni bu&scirc;on ne esploras<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dot.0i">doti</ref>,
      <ref cel="ofer.0i">oferi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">schenken</trd>
  <trd lng="en">donate</trd>
  <trd lng="es">donar</trd>
  <trdgrp lng="fr">
    <trd>donner en cadeau</trd>,
    <trd>offrir<klr tip="ind"> (un cadeau)</klr></trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="donac.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="D"/>ita&jcirc;o:
      <ekz>
        novjaraj <tld/>oj;
      </ekz>
      <ekz>
        doni, fari <tld/>on;
      </ekz>
      <ekz>
        elokvento estis en li natura <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_r;&c_u;&c_n;&c_a;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Geschenk</trd>,
    <trd>Gabe</trd>
  </trdgrp>
  <trd lng="es">donaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>cadeau</trd>,
    <trd>don</trd>,
    <trd>pr&eacute;sent<klr tip="ind"> (cadeau)</klr></trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_r;&c_o;&c_k;</trd>
  </trdgrp>
</drv>
<drv mrk="donac.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Kiel <tld/>o(n):
      <ekz>
       <tld/>e ricevi, prezenti.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u; <ind>&c_p;&c_a;&c_d;&c_a;&c_r;&c_u;&c_n;&c_a;&c_k;</ind></trd>
  <trd lng="fr">en <ind>cadeau</ind></trd>
  <trd lng="ru">&c_v; <ind>&c_p;&c_o;&c_d;&c_a;&c_r;&c_o;&c_k;</ind></trd>
</drv>
<drv mrk="donac.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Volonte <tld/>anta.
    </dif>
    <refgrp tip="vid">
      <ref cel="avar.mal0a">malavara</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">g&eacute;n&eacute;reux</trd>
  <trd lng="ru">&c_shch;&c_je;&c_d;&c_r;&c_y;&c_j;</trd>
</drv>
</art>
