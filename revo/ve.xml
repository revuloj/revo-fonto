<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ve</rad>
</kap>
<drv mrk="ve.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <dif>
    <ref tip="dif" cel="kri.ek0o">Ekkrio</ref> esprimanta:
  </dif>
  <snc>
    <dif>
      Plendon pro malfeli&ccirc;o:
      <ekz>
        ve al ni, ni estas ruinigitaj!<fnt>Z</fnt>
      </ekz>
    </dif>
    <trd lng="de">weh</trd>
    <trdgrp lng="en">
      <trd>wo</trd>,
      <trd>woe</trd>,
      <trd>alas</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_w;&a_A;&f_y;!</trd>
      <trd>&a_A;&a_f;&a_s;&a_w;&a_s;!</trd>
      <trd>&a_d;&a_r;&f_y;&a_gh;!</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>a&iuml;e</trd>,
      <trd>h&eacute;las</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>ai de mim!</trd>
    </trdgrp>
    <trd lng="ru">&c_u;&c_v;&c_y;</trd>
    <trd lng="tp">ike</trd>
  </snc>
  <snc>
    <dif>
      Minacon pri malfeli&ccirc;o:
      <ekz>
        ve al la popolo peka, <klr>[...]</klr> al la idaro 
        krima<fnt><bib>MT</bib>, <lok>Jesaja 1:4</lok></fnt>;
      </ekz>
      <ekz>
        <ind>ve al la venkitoj!</ind>
        <trd lng="fa">&a_w;&a_A;&f_y; &a_b;&a_r;
        &a_sh;&f_k;&a_s;&a_t;&zwnj;&a_kh;&a_w;&a_r;&a_d;&f_g;&a_A;&a_n;!</trd>
        <trd lng="hu">jaj a legy&odblac;z&ouml;tteknek!</trd>
        <trd lng="ru">&c_g;&c_o;&c_r;&c_je; &c_p;&c_o;&c_b;&c_je;&c_zh;&c_d;&c_jo;&c_n;&c_n;&c_y;&c_m;!</trd>
      </ekz>
    </dif>
    <trd lng="de">wehe</trd>
    <trd lng="fa">&a_w;&a_A;&f_y;!</trd>
    <trdgrp lng="fr">
      <trd>malheur !</trd>
    </trdgrp>
    <trd lng="hu">jaj</trd>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_je;! <klr 
tip="amb">(&c_k;&c_o;&c_m;&c_u;-&c_l;.)</klr></trd>
  </snc>
  <trd lng="nl">wee</trd>
  <trd lng="sk">beda</trd>
</drv>

<drv mrk="ve.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Plenda ekkrio:
      <ekz>
        lamentoj, &gcirc;emoj kaj <tld/>oj<fnt><bib>MT</bib>, 
        <lok>Je&hcirc;ezkel 2:10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>plaint</trd>,
    <trd>lamentation</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_sh;&f_k;&a_w;&a_h;</trd>
    <trd>&a_A_madda;&a_h; &a_w; &a_n;&a_A;&a_l;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lamentation</trd>,
    <trd>plainte</trd>
  </trdgrp>
  <trd lng="hu">jajgat&aacute;s</trd>
  <trd lng="nl">weeklacht</trd>
  <trdgrp lng="de">
    <trd>Wehklage</trd>,
    <trd>Wehruf</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_i;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_ch;</trd>
  </trdgrp>
  <trd lng="sk">bed&aacute;kanie</trd>
</drv>

<drv mrk="ve.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ekkrii <tld/>ojn:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la vento <tld/>adas.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>lament</trd>,
    <trd>wail</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_sh;&f_k;&a_w;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
    <trd>&a_n;&a_A;&a_l;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&eacute;mir</trd>,
    <trd>se <ind>lamenter</ind></trd>,
    <trd>se <ind>plaindre</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jajgat</trd>,
    <trd>jajong</trd>
  </trdgrp>
  <trd lng="nl">weeklagen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_n;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">bed&aacute;ka&tcaron;</trd>
</drv>

<drv mrk="ve.hejm0o">
  <kap>hejm<tld/>o</kap>
  <snc>
    <dif>
      Dolora sopiro al hejmo.
    </dif>
    <ref tip="sin" cel="nostal.0o">nostalgio</ref>
  </snc>
  <trd lng="de">Heimweh</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_r;&a_d; &a_gh;&a_r;&a_b;&a_t;</trd>
    <trd>&a_d;&a_r;&a_d; &a_w;&a_T;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mal du pays</trd>
    <trd>nostalgie</trd>
  </trdgrp>
  <trd lng="hu">honv&aacute;gy</trd>
  <trd lng="sv">heml&auml;ngtan</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_s;&c_k;&c_a; &c_p;&c_o; &c_d;&c_o;&c_m;&c_u;</trd>,
    <trd>&c_n;&c_o;&c_s;&c_t;&c_a;&c_l;&c_mol;&c_g;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
