<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ve</rad>
</kap>
<drv mrk="ve.0">
  <kap><ofc>*</ofc><tld/></kap>
  <dif>
    Ekkria vorteto esprimanta:
  </dif>
  <snc>
    <dif>
      Plendon pro malfeli&ccirc;o:
      <ekz>
        ve al ni, ni estas ruinigitaj!<fnt>Z</fnt>
      </ekz>
    </dif>
    <trd lng="de">weh</trd>
    <trdgrp lng="fr">
      <trd>a&iuml;e</trd>,
      <trd>h&eacute;las</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>ai de mim!</trd>
    </trdgrp>
    <trd lng="ru">&c_u;&c_v;&c_y;</trd>
  </snc>
  <snc>
    <dif>
      Minacon pri malfeli&ccirc;o:
      <ekz>
        ve al la popolo peka, al la idaro krima!<fnt>Z</fnt>
      </ekz>
      <ekz>
        <ind>ve al la venkitoj!</ind>
        <trd lng="hu">jaj a legy&odblac;z&ouml;tteknek!</trd>
        <trd lng="ru">&c_g;&c_o;&c_r;&c_je; &c_p;&c_o;&c_b;&c_je;&c_zh;&c_d;&c_jo;&c_n;&c_n;&c_y;&c_m;!</trd>
      </ekz>
    </dif>
    <trd lng="de">wehe</trd>
    <trdgrp lng="fr">
      <trd>malheur !</trd>
    </trdgrp>
    <trd lng="hu">jaj</trd>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_je;! <klr tip="amb">(&c_k;&c_o;&c_m;&c_u;-&c_l;.)</klr></trd>
  </snc>
  <trd lng="nl">wee</trd>
  <trd lng="sk">beda</trd>
</drv>

<drv mrk="ve.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Plenda ekkrio:
      <ekz>
        lamentoj, &gcirc;emoj kaj <tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>lamentation</trd>,
    <trd>plainte</trd>
  </trdgrp>
  <trd lng="nl">weeklacht</trd>
  <trdgrp lng="de">
    <trd>Wehklage</trd>,
    <trd>Wehruf</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_i;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_ch;</trd>
  </trdgrp>
  <trd lng="sk">bed&aacute;kanie</trd>
</drv>

<drv mrk="ve.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ekkrii <tld/>ojn:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la vento <tld/>adas.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>g&eacute;mir</trd>,
    <trd>se <ind>lamenter</ind></trd>,
    <trd>se <ind>plaindre</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>g&eacute;mir</trd>,
    <trd>jajgat</trd>,
    <trd>jajong</trd>
  </trdgrp>
  <trd lng="nl">weeklagen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_n;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">bed&aacute;ka&tcaron;</trd>
</drv>

<drv mrk="ve.hejm0o">
  <kap>hejm<tld/>o</kap>
  <snc>
    <dif>
      Dolora sopiro al hejmo.
    </dif>
    <ref tip="sin" cel="nostal.0o">nostalgio</ref>
  </snc>
  <trd lng="de">Heimweh</trd>
  <trdgrp lng="fr">
    <trd>mal du pays</trd>
    <trd>nostalgie</trd>
  </trdgrp>
  <trd lng="hu">honv&aacute;gy</trd>
  <trd lng="sv">heml&auml;ngtan</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_s;&c_k;&c_a; &c_p;&c_o; &c_d;&c_o;&c_m;&c_u;</trd>,
    <trd>&c_n;&c_o;&c_s;&c_t;&c_a;&c_l;&c_mol;&c_g;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
