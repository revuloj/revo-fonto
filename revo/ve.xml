<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ve</rad>
</kap>
<drv mrk="ve.0">
  <kap>
    <ofc>*</ofc><tld/>
  </kap>
  <dif>
    Ekkria vorteto esprimanta:
  </dif>
  <snc>
    <dif>
      Plendon pro malfeli&ccirc;o:
      <ekz>
        ve al ni, ni estas ruinigitaj!<fnt>Z</fnt>
      </ekz>
    </dif>
    <trd lng="de">weh</trd>
  <trdgrp lng="fr">
    <trd>a&iuml;e</trd>,
    <trd>h&eacute;las</trd>
  </trdgrp>
  </snc>
  <snc>
    <dif>
      Minacon pri malfeli&ccirc;o:
      <ekz>
        ve al la popolo peka, al la idaro krima!<fnt>Z</fnt>
      </ekz>
      <ekz>
        ve al la venkitoj!
      </ekz>
    </dif>
    <trd lng="de">wehe</trd>
    <trdgrp lng="fr">
      <trd>malheur !</trd>
    </trdgrp>
  </snc>
  <trd lng="nl">wee</trd>
</drv>

<drv mrk="ve.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Plenda ekkrio:
      <ekz>
        lamentoj, &gcirc;emoj kaj <tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>lamentation</trd>,
    <trd>plainte</trd>
  </trdgrp>
  <trd lng="nl">weeklacht</trd>
</drv>

<drv mrk="ve.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ekkrii <tld/>ojn: (f)
      <ekz>
        la vento <tld/>adas.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>g&eacute;mir</trd>,
    <trd>se <ind>lamenter</ind></trd>,
    <trd>se <ind>plaindre</ind></trd>
  </trdgrp>
  <trd lng="nl">weeklagen</trd>
</drv>

<drv mrk="ve.hejm0o">
  <kap>
    hejm<tld/>o
  </kap>
  <snc>
    <dif>
      Dolora sopiro al hejmo.
    </dif>
    <ref tip="sin" cel="nostal.0o">nostalgio</ref>
  </snc>
  <trd lng="de">Heimweh</trd>
  <trdgrp lng="fr">
    <trd>mal du pays</trd>
    <trd>nostalgie</trd>
  </trdgrp>
  <trd lng="sv">heml&auml;ngtan</trd>
</drv>

</art>
