<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">

<kap>
  <rad>piranj</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="piranj.0o">
  <kap><tld/>o</kap>

  <snc mrk="piranj.0o.specio">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="serasalm.0o">serasalmo</ref>
      (<trd lng="la">Serrasalmus piraya</trd>), fi&scirc;o kun mallonga,
      alta korpo
      kaj akregaj dentoj, per kiuj &gcirc;i rapide povas el&scirc;iri
      karnopecojn, e&ccirc; de
      grandaj mamuloj kaj homoj; vivas en riveroj de Suda kaj Centra
      Amerikoj.
    </dif>
    <trd lng="ca">piranya</trd>
    <trdgrp lng="it">
      <trd>pira&ntilde;a</trd>,
      <trd>piraha<klr> (pesce)</klr></trd>
    </trdgrp>
    <trd lng="pl">pirania</trd>
  </snc>

  <snc mrk="piranj.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="serasalm.0o.genro">serasalmo</ref>
    <trd lng="ca">Serrasalmus</trd>
    <trdgrp lng="it">
      <trd>pira&ntilde;as</trd>,
      <trd>pirahas<klr> (pesci)</klr></trd>,
      <trd>serrasalmini</trd>
    </trdgrp>
    <trd lng="pl">pirania</trd>
  </snc>

  <snc mrk="piranj.0o.rivero">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Rivero en <ref tip="vid" cel="brazil.B0o">Brazilo</ref>.
    </dif>
    <trd lng="ca">Rio Piranhas</trd>
    <trd lng="it">Rio Pirahas</trd>
  </snc>
  <trd lng="sk">pira≈àa</trd>
</drv>

</art>

