<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: piranj.xml,v 1.13 2024/02/17 06:43:08 revo Exp $">

<kap>
  <rad>piranj</rad>/o
  <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="piranj.0o">
  <kap><tld/>o</kap>

  <snc mrk="piranj.0o.specio">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="super" cel="serasalm.0o">serasalmo</ref>
      (<trd lng="la">Pygocentrus piraya</trd>), <ref tip="lst" lst="voko:fi&scirc;oj" cel="fisx.0o">fi&scirc;o</ref>
      kun mallonga, alta korpo kaj akregaj dentoj, per kiuj &gcirc;i rapide
      povas el&scirc;iri
      karnopecojn, e&ccirc; de grandaj mamuloj; &gcirc;i vivas en riveroj de Suda
      kaj Centra
      Amerikoj:
      <ekz>
        <tld/>oj a&ubreve; Segildentaj salmoj <klr>[&#x2026;]</klr> lo&gcirc;as en riveroj
        de Sudameriko,
        &ccirc;efe en Amazonio, &ccirc;e la rivero Orinoko, la Gujanoj kaj
        Paragvajo, temas pri
        malgrandaj fi&scirc;oj kun rondoforma kapo, kiuj estas helbluaj
        havante nigrecajn makulojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=296439">Piranjo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/27/Schwarzerpiranha-01.jpg/699px-Schwarzerpiranha-01.jpg" prm="CC BY-SA     2.0 de">nigravosta <tld/>o (Pygocentrus piraya), Zoo Frankfurto
        <fnt>
          <aut>propra foto de Petra Karstedt / www.Tiermotive.de</aut>
          <lok><url ref="&WCW;/File:Schwarzerpiranha-01.jpg">&WCW;/File:Schwarzerpiranha-01.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_p;&c_ib;&c_r;&c_a;&c_n;&c_mol;&c_ja;</trd>
    <trd lng="ca">piranya</trd>
    <trdgrp lng="de">
      <trd>Piraya</trd>,
      <trd>Schwarzer Piranha</trd>,
      <trd>Chupita</trd>
    </trdgrp>
    <trd lng="fr">piranha</trd>
    <trdgrp lng="it">
      <trd>pira&ntilde;a</trd>,
      <trd>piraha<klr> (pesce)</klr></trd>
    </trdgrp>
    <trd lng="pl">pirania</trd>
    <trd lng="pt">piranha</trd>
  </snc>

  <snc mrk="piranj.0o.genroj">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el kvin genroj de rabofi&scirc;oj el la familio serasalmedoj
      kun akregaj dentoj kaj rapida vundosani&gcirc;o, i.a.
      <ref tip="dif" cel="serasalm.0o.genro">serasalmo</ref>,
      pigocentro kaj pristobriko.
    </dif>
    <trd lng="be">&c_p;&c_ib;&c_r;&c_a;&c_n;&c_mol;&c_ib;</trd>
    <trd lng="ca">Serrasalmus</trd>
    <trd lng="de">Piranha (div. Gattungen)</trd>
    <trdgrp lng="it">
      <trd>pira&ntilde;as</trd>,
      <trd>pirahas<klr> (pesci)</klr></trd>,
      <trd>serrasalmini</trd>
    </trdgrp>
    <trd lng="pl">pirania</trd>
  </snc>

  <snc mrk="piranj.0o.rivero">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Rivero en <ref tip="vid" cel="brazil.B0o">Brazilo</ref>.
    </dif>
    <trd lng="be">&c_P;&c_ib;&c_r;&c_a;&c_n;&c_mol;&c_ja;&c_s; <klr>(&c_r;&c_a;&c_k;&c_a;)</klr></trd>
    <trd lng="ca">Rio Piranhas</trd>
    <trd lng="it">Rio Pirahas</trd>
    <trd lng="pt">Rio Piranhas</trd>
  </snc>
  <trd lng="cs">pira&ncaron;a</trd>
  <trd lng="sk">pira&ncaron;a</trd>
  <trd lng="uk">&#x43F;&c_ib;&c_r;&c_a;&#x43D;&#x44C;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x98DF;&#x4EBA;&#x9B5A; <pr>sh&#xED;r&#xE9;ny&#xFA;</pr></trd>,
    <trd>&#x98DF;&#x4EBA;&#x9C7C; <pr>sh&#xED;r&#xE9;ny&#xFA;</pr></trd>
  </trdgrp>
</drv>

</art>

