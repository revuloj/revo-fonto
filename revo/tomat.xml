<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>tomat</rad>/o
</kap>

<drv mrk="tomat.0o">
  <kap><tld/>o</kap>
  <snc mrk="tomat.0o.KUI">
    <dif>
      Frukto de <tld/>ujo:
      <ekz>
        mia viza&gcirc;o i&gcirc;os ru&gcirc;a kiel <tld/>o
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 8a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>tomaca</trd>,
      <trd>tom&agrave;quet</trd>
    </trdgrp>
  </snc>
  <snc mrk="tomat.0o.BOT">
    <dif>
      <ref tip="dif" cel="tomat.0ujo"><tld lit="T"/>ujo</ref>.
    </dif>
    <trd lng="ca">tomaquera</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_m;&c_a;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_ib;&c_d;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_o;&c_m;&c_a;&c_t;</trd>
  <trd lng="de">Tomate</trd>
  <trd lng="en">tomato</trd>
  <trd lng="es">tomate</trd>
  <trd lng="fa">&f_g;&a_w;&a_j;&a_h;&zwnj;&a_f;&a_r;&a_n;&f_g;&f_y;</trd>
  <trd lng="fr">tomate<klr tip="amb"> (fruit)</klr></trd>
  <trd lng="hu">paradicsom</trd>
  <trd lng="nl">tomaat</trd>
  <trdgrp lng="pl">
    <trd>pomidor</trd>,
    <trd>tomat<klr> (rzadko)</klr></trd>
  </trdgrp>
  <trd lng="pt">tomate</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_m;&c_a;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_i;&c_d;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="sv">tomat</trd>
  <trd lng="tr">domates</trd>
</drv>

<drv mrk="tomat.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="tomat.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="solana.0oj">solanacoj</ref>
      (<trd lng="la">Lycopersicon</trd>), kun man&gcirc;eblaj brile
      ru&gcirc;aj
      fruktoj. Unu specio (<trd lng="la">Lycopersicon
      lycopersicum</trd>)
      estas multe kulturata.
    </dif>
  </snc>
  <bld lok="../bld/tomat.jpg">
    <tld/>ujo kun floroj kaj nematuraj fruktoj
  </bld>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_m;&c_a;&c_t;
    <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>,
    <trd>&c_p;&c_a;&c_m;&c_ib;&c_d;&c_o;&c_r;
    <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>tomaquera</trd>,
    <trd></trd>
  </trdgrp>
  <trd lng="es">tomatera</trd>
  <trd lng="fr">tomate<klr tip="amb"> (plante)</klr></trd>
  <trd lng="hu">paradicsomn&ouml;v&eacute;ny</trd>
  <trd lng="pl">pomidor<klr> (krzew)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_m;&c_a;&c_t; <klr
    tip="ind">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_p;&c_o;&c_m;&c_i;&c_d;&c_o;&c_r; <klr
    tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
</drv>
</art>
