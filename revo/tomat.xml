<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>tomat</rad>/o
</kap>
<drv mrk="tomat.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Frukto de <tld/>ujo.
    </dif>
  </snc>
  <snc num="2">
    <dif>
      <ref tip="dif" cel="tomat.0ujo"><tld lit="T"/>ujo</ref>.
    </dif>
  </snc>
  <trd lng="de">Tomate</trd>
  <trd lng="fr">tomate</trd>
  <trd lng="tr">domates</trd>
</drv>
<drv mrk="tomat.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="solan.0acoj">solanacoj</ref> 
      (<trd lng="la">Lycopersicon</trd>), kun man&gcirc;eblaj brile ru&gcirc;aj
      fruktoj. Unu specio (<trd lng="la">Lycopersicon lycopersicum</trd>)
      estas multe kulturata.
    </dif>
  </snc>
  <bld lok="../bld/tomat.jpg">
    <tld/>ujo kun floroj kaj nematuraj fruktoj
  </bld>
</drv>
</art>
