<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: protez.xml,v 1.18 2022/05/04 18:30:34 revo Exp $">
    <kap>
      <rad>protez</rad>/o <fnt><bib>SPV</bib></fnt>
    </kap>

    <drv mrk="protez.0o">
      <kap><tld/>o</kap>
      <snc mrk="protez.0o.MED">
        <uzo tip="fak">MED</uzo>
        <uzo tip="fak">TEK</uzo>
        <dif>
          Artefarita peco destinita anstata&ubreve;i naturan organon a&ubreve;
          korpoparton:
          <ekz>
            denta <tld/>o,
          </ekz><ekz>
            krura <tld/>o;
          </ekz><ekz>
            se vi trapikas vian koron mem, vi ricevas motoran
            kor<tld/>on
            <fnt><aut>S&aacute;ndor Szathm&aacute;ri</aut>,
            <vrk>Honorigo</vrk></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_t;&c_e;&c_z;</trd>
  <trd lng="ca">pr&ograve;tesi</trd>
  <trdgrp lng="cs">
    <trd>prot&eacute;za</trd>,
    <trd>um&ecaron;l&yacute; &uacute;d</trd>
  </trdgrp>
  <trd lng="de">Prothese</trd>
  <trdgrp lng="en">
        <trd>prosthesis</trd>,
        <trd>prosthetics</trd>
      </trdgrp>
      <trdgrp lng="fa">
        <trd>&a_ayn;&a_D;&a_w; &a_m;&a_S;&a_n;&a_w;&a_ayn;&f_y;</trd>,
        <trd>&f_p;&a_r;&a_w;&a_t;&a_kasra;&a_z;</trd>
      </trdgrp>
  <trd lng="fr">proth&egrave;se</trd>
  <trd lng="hu">prot&eacute;zis</trd>
  <trd lng="nl">prothese</trd>
  <trd lng="pt">pr&oacute;tese <klr>(med.)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_je;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>prot&eacute;za</trd>,
    <trd>umel&aacute; n&aacute;hrada <klr>(zubov, kon&ccaron;at&iacute;n)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_t;&c_je;&c_z;</trd>
  </trdgrp>
</drv>

    <drv mrk="protez.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <uzo tip="fak">MED</uzo>
      <uzo tip="fak">TEK</uzo>
      <snc>
        <dif>
          Fari kaj instali <tld/>on:
          <ekz>
            Sed kio pri <klr>...</klr> tio kiel oni <tld/>is
            viajn dentojn en Parizo <klr>...</klr>?
            <fnt><aut>Dostojevskij, tr. A. Kor&jcirc;enkov</aut>:
              <vrk><url ref="&LOdE;Rn-dost3.htm">Iom pri la
              mensogado</url></vrk>.
              <bib>LOdE</bib>, <lok>2000. &#8470;11 (73)</lok>
            </fnt>;
          </ekz><ekz>
            helpe de tiuj &scirc;tataj institucioj oni sukcesis <tld/>i
            sociajn ligojn, disfalantajn sub premo de bonstato
            <fnt>
              <aut>S. Lem, tr. J. Finkel</aut>:
              <vrk><url ref="http://www.esperanto.mv.ru/Lem/wl_2.html">Surloka esploro</url></vrk>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_a;&c_t;&c_e;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">implantar, posar una pr&ograve;tesi</trd>
  <trdgrp lng="cs">
    <trd>vlo&zcaron;it prot&eacute;zu</trd>,
    <trd>vyrobit prot&eacute;zu</trd>
  </trdgrp>
  <trd lng="nl">een <ind>prothese</ind> aanbrengen</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_je;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>vlo&zcaron;i&tcaron; prot&eacute;zu</trd>,
    <trd>vyrobi&tcaron; prot&eacute;zu</trd>
  </trdgrp>
</drv>

    <drv mrk="protez.0isto">
      <kap><tld/>isto</kap>
      <uzo tip="fak">MED</uzo>
      <uzo tip="fak">TEK</uzo>
      <snc>
        <dif>
          Fakulo kiu faras kaj instalas <tld/>ojn.
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_a;&c_t;&c_e;&c_z;&c_y;&c_s;&c_t;</trd>
      <trdgrp lng="ca">
        <trd>prot&egrave;tic</trd>,
        <trd>protetista</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>prosthetist</trd>,
        <trd>prostheticist</trd>
      </trdgrp>
      <trd lng="nl">prothesemaker</trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_je;&c_z;&c_i;&c_s;&c_t;</trd>
    </drv>

  </art>
