<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>protez</rad>/o <fnt><bib>SPV</bib></fnt>
    </kap>

    <drv mrk="protez.0o">
      <kap><tld/>o</kap>
      <snc mrk="protez.0o.MED">
        <uzo tip="fak">MED</uzo>
        <uzo tip="fak">TEK</uzo>
        <dif>
          Artefarita peco destinita anstata&ubreve;i naturan organon a&ubreve;
          korpoparton:
          <ekz>
            denta <tld/>o,
          </ekz><ekz>
            krura <tld/>o;
          </ekz><ekz>
            se vi trapikas vian koron mem, vi ricevas motoran kor<tld/>on
            <fnt><aut>S&aacute;ndor Szathm&aacute;ri</aut>, <vrk>Honorigo</vrk></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_a;&c_t;&c_e;&c_z;</trd>
      <trd lng="de">Prothese</trd>
      <trd lng="fr">proth&egrave;se</trd>
      <trd lng="hu">prot&eacute;zis</trd>
      <trd lng="nl">prothese</trd>
      <trd lng="pt">pr&oacute;tese <klr>(med.)</klr></trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_je;&c_z;</trd>
    </drv>

    <drv mrk="protez.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <uzo tip="fak">MED</uzo>
      <uzo tip="fak">TEK</uzo>
      <snc>
        <dif>
          Fari kaj instali <tld/>on:
          <ekz>
            Sed kio pri <klr>...</klr> tio kiel oni <tld/>is
            viajn dentojn en Parizo <klr>...</klr>?
            <fnt><aut>Dostojevskij, tr. A. Kor&jcirc;enkov</aut>:
              <vrk><url ref="http://www.esperanto.org/Ondo/Rn-dost3.htm"
                  >Iom pri la mensogado</url></vrk>.
              <bib>LOdE</bib>, <lok>2000. &#x2116;11 (73)</lok>
            </fnt>;
          </ekz><ekz>
            helpe de tiuj &scirc;tataj institucioj oni sukcesis <tld/>i
            sociajn ligojn, disfalantajn sub premo de bonstato
            <fnt>
              <aut>S. Lem, tr. J. Finkel</aut>:
              <vrk><url ref="http://www.esperanto.mv.ru/Lem/wl_2.html"
                  >Surloka esploro</url></vrk>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_je;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="protez.0isto">
      <kap><tld/>isto</kap>
      <uzo tip="fak">MED</uzo>
      <uzo tip="fak">TEK</uzo>
      <snc>
        <dif>
          Fakulo kiu faras kaj instalas <tld/>ojn.
        </dif>
      </snc>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_je;&c_z;&c_i;&c_s;&c_t;</trd>
    </drv>

  </art>
