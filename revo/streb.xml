<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: streb.xml,v 1.30 2025/06/13 12:59:15 revo Exp $">
<kap>
  <ofc>6</ofc>
  <rad>streb</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="streb.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="streb.0i.KOMUNE">
    <dif>
      Energie stre&ccirc;i la fortojn de la korpo a&ubreve; de la spirito por
      atingi iun celon:
      <ekz>
        li sciis, al kiu celo la flamo <tld/>is
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi luktis kaj <tld/>is por la infano
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur al la bono kaj vero mi <tld/>as
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[al]</klr> malpieco <tld/>as ilia animo
        <fnt>
          <bib>MT</bib>
          <lok>Ho&scirc;ea 4:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvankam mi estas nur malri&ccirc;a &scirc;uista submajstro, mi povas ja fari&gcirc;i majstro, mi
        laboros kaj mi <tld/>os anta&ubreve;en
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>!
      </ekz>
      <ekz>
        li devas konsideri la aferojn ne nur la&ubreve; ties rigida realeco, sed li devas
        <tld/>i pli alten
        <fnt>
          <bib>VaK</bib>
          <lok>Deka &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspir.0i">aspiri</ref>,
      <ref cel="cel.0i">celi</ref>,
      <ref cel="dezir.0i.porSiMem">deziri<sncref/></ref>,
      <ref cel="klopod.0i">klopodi</ref>,
      <ref cel="pen.0i">peni</ref>,
      <ref cel="sopir.0i">sopiri</ref>,
      <ref cel="vol.0i.strebi">voli<sncref/></ref>
    </refgrp>
    <trd lng="de">trachten</trd>
    <trd lng="en">seek</trd>
    <trdgrp lng="es">
      <trd>tener <ind>af&aacute;n</ind></trd>,
      <trd>afanarse</trd>,
      <trd>sentir <ind>inclinaci&oacute;n</ind> </trd>,
    </trdgrp>
    <trdgrp lng="fr">
      <trd>tendre (&agrave;)</trd>,
      <trd>viser (&agrave;)</trd>,
      <trd>avoir pour <ind>but</ind> (de)</trd>,
      <trd>aspirer (&agrave;)</trd>
    </trdgrp>
    <trd lng="hu">t&ouml;rekszik</trd>
    <trdgrp lng="nl">
      <trd>inspannen</trd>,
      <trd>streven</trd>
    </trdgrp>
    <trd lng="pl">stara&cacute; si&eogonek;</trd>
    <trdgrp lng="pt">
      <trd>esfor&ccedil;ar-se</trd>,
      <trd>empenhar-se</trd>
    </trdgrp>
  </snc>
  <snc mrk="streb.0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      Senlime proksimi&gcirc;i:
      <ekz>
        la funkcio logaritmo <tld/>as al malfinio, kiam &gcirc;ia
        argumento <tld/>as al nulo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konver.0i.MAT">konver&gcirc;i</ref>,
      <ref cel="limes.0o">limeso</ref>
    </refgrp>
    <trd lng="en">tend<klr tip="ind"> (to a limit)</klr></trd>
    <trd lng="es">tender<klr tip="amb"> (hacia un l&iacute;mite)</klr></trd>
    <trd lng="fr">tendre<klr tip="ind"> (vers une limite)</klr></trd>
    <trd lng="pl">d&aogonek;&zdot;y&cacute;</trd>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>sna&zcaron;it se</trd>,
    <trd>usilovat</trd>
  </trdgrp>
  <trd lng="de">streben</trd>
  <trd lng="hu">tart<klr tip="ind"> (hat&aacute;r fel&eacute;)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#20840;&#21147;&#12434;&#12388;&#12367;&#12377;  <pr>&#12380;&#12435;&#12426;&#12423;&#12367;&#12434;&#12388;&#12367;&#12377;</pr></trd>,
    <trd>&#25080;&#21629;&#12395;&#21162;&#21147;&#12377;&#12427;
    <pr>&#12369;&#12435;&#12417;&#12356;&#12395;&#12393;&#12426;&#12423;&#12367;&#12377;&#12427;</pr></trd>,
    <trd>&#22894;&#38360;&#12377;&#12427;  <pr>&#12405;&#12435;&#12392;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">streven</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>ba&zcaron;i&tcaron;</trd>,
    <trd>pren.: driapa&tcaron; sa</trd>,
    <trd>sna&zcaron;i&tcaron; sa</trd>,
    <trd>usilova&tcaron;<klr>(sa)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_ja;&c_g;&c_n;&c_u;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21147;&#20105;&#19978;&#28216; <pr>l&igrave;zh&emacron;ngsh&agrave;ngy&oacute;u</pr></trd>,
    <trd>&#21147;&#29229;&#19978;&#28216; <pr>l&igrave;zh&emacron;ngsh&agrave;ngy&oacute;u</pr></trd>,
    <trd>&#21162;&#21147;&#20105;&#21462; <pr>n&#468;l&igrave;zh&emacron;ngq&#468;</pr></trd>,
    <trd>&#21162;&#21147;&#29229;&#21462; <pr>n&#468;l&igrave;zh&emacron;ngq&#468;</pr></trd>,
    <trd>&#35584;&#27714; <pr>m&oacute;uqi&uacute;</pr></trd>,
    <trd>&#35851;&#27714; <pr>m&oacute;uqi&uacute;</pr></trd>,
    <trd>&#35584;&#21462; <pr>m&oacute;uq&#468;</pr></trd>,
    <trd>&#35851;&#21462; <pr>m&oacute;uq&#468;</pr></trd>,
    <trd>&#21147;&#27714; <pr>l&igrave;qi&uacute;</pr></trd>,
    <trd>&#20105;&#21462;&#36798;&#21040; <pr>zh&emacron;ngq&#468;d&aacute;d&agrave;o</pr></trd>,
    <trd>&#29229;&#21462;&#36948;&#21040; <pr>zh&emacron;ngq&#468;d&aacute;d&agrave;o</pr></trd>,
    <trd>&#20105;&#38155; <pr>zh&emacron;ngf&emacron;ng</pr></trd>,
    <trd>&#29229;&#37586; <pr>zh&emacron;ngf&emacron;ng</pr></trd>,
    <trd>&#30446;&#30340;&#22312; <pr>m&ugrave;d&igrave;z&agrave;i</pr></trd>,
    <trd>&#30446;&#30340;&#22312;&#20110; <pr>m&ugrave;d&igrave;z&agrave;iy&uacute;</pr></trd>,
    <trd>&#30446;&#30340;&#22312;&#26044; <pr>m&ugrave;d&igrave;z&agrave;iy&uacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="streb.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="streb.0o.KOMUNE">
    <dif>
      Kuni&gcirc;o de <ref cel="vol.0o.volado">volo</ref> kaj forto por atingi celon:
      <ekz>
        en sia <tld/>ado <klr>[li]</klr> estas pli feli&ccirc;a ol liaj du pli a&gcirc;aj fratoj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kulturaj a&ubreve; politikaj <tld/>adoj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oratoro komprenigas siajn pensojn, sentojn kaj <tld/>adojn per la lingvo
        <fnt>
          <bib>Ret</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cel.0ado">celado</ref>,
      <ref cel="strecx.forto0o.KOMUNE">fortostre&ccirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_m;&c_k;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_y;&c_w;</trd>,
    <trd>&c_ib;&c_m;&c_p;&c_e;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>snaha</trd>,
    <trd>sna&zcaron;en&iacute;</trd>,
    <trd>&uacute;sil&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Streben</trd>,
    <trd>Trachten</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>impulso</trd>,
    <trd>af&aacute;n</trd>,
    <trd>contrafuerte<klr> (arquitectura)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aspiration<klr> (&eacute;lan)</klr></trd>,
    <trd>&eacute;lan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25080;&#21629;&#12398;&#21162;&#21147;  <pr>&#12369;&#12435;&#12417;&#12356;&#12398;&#12393;&#12426;&#12423;&#12367;</pr></trd>,
    <trd>&#22894;&#38360;  <pr>&#12405;&#12435;&#12392;&#12358;</pr></trd>,
    <trd>&#31934;&#36914;  <pr>&#12375;&#12423;&#12358;&#12376;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">usi&lstroke;owanie</trd>
  <trdgrp lng="sk">
    <trd>dravos&tcaron;</trd>,
    <trd>drav&aacute; t&uacute;&zcaron;ba</trd>,
    <trd>n&aacute;por</trd>,
    <trd>&uacute;silie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_ja;&c_g;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_i;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#39493;&#20351; <pr>q&umacron;sh&#464;</pr></trd>,
    <trd>&#39537;&#20351; <pr>q&umacron;sh&#464;</pr></trd>,
    <trd>&#21183;&#22836; <pr>sh&igrave;t&oacute;u</pr></trd>,
    <trd>&#21218;&#38957; <pr>sh&igrave;t&oacute;u</pr></trd>,
    <trd>&#25265;&#36000; <pr>b&agrave;of&ugrave;</pr></trd>,
    <trd>&#25265;&#36127; <pr>b&agrave;of&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="streb.0ema">
  <kap>
    <tld/>ema
  </kap>
  <snc>
    <dif>
      Persista en sia <tld/>ado:
      <ekz>
        estas klare ke <tld/>emo &ccirc;iam celas superforti kaj venki kaj akiri bonan
        reputacion
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Na&ubreve;</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povas legi &ccirc;iam denove en la diman&ccirc;aj &jcirc;urnaloj <klr>[&#8230;]</klr>, ke nur per
        tiu libervola aldona laboro, kiu evidentigas ambicion, <tld/>emon kaj la
        deziron komandi, jam kelkaj simplaj honestaj laboristoj fari&gcirc;is &gcirc;enerala
        direktoro a&ubreve; miliardulo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ambici.0a">ambicia</ref>,
      <ref cel="sukces.0ema">sukcesema</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_l;&c_ib;&c_v;&c_y; <klr>(&c_u; &c_p;&c_a;&c_m;&c_k;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_ja;&c_h;)</klr></trd>
  <trdgrp lng="de">
    <trd>strebsam</trd>,
    <trd>zielstrebig</trd>,
    <trd>ehrgeizig</trd>
  </trdgrp>
</drv>

</art>
