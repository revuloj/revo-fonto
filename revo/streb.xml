<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: streb.xml,v 1.28 2024/03/25 07:28:36 revo Exp $">
<kap>
  <ofc>6</ofc>
  <rad>streb</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="streb.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="streb.0i.KOMUNE">
    <dif>
      Energie stre&ccirc;i la fortojn de la korpo a&ubreve; de la spirito por
      atingi iun celon:
      <ekz>
        li sciis, al kiu celo la flamo <tld/>is
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi luktis kaj <tld/>is por la infano
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur al la bono kaj vero mi <tld/>as
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[al]</klr> malpieco <tld/>as ilia animo
        <fnt>
          <bib>MT</bib>
          <lok>Ho&scirc;ea 4:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvankam mi estas nur malri&ccirc;a &scirc;uista submajstro, mi povas ja fari&gcirc;i majstro, mi
        laboros kaj mi <tld/>os anta&ubreve;en
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>!
      </ekz>
      <ekz>
        li devas konsideri la aferojn ne nur la&ubreve; ties rigida realeco, sed li devas
        <tld/>i pli alten
        <fnt>
          <bib>VaK</bib>
          <lok>Deka &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspir.0i">aspiri</ref>,
      <ref cel="cel.0i">celi</ref>,
      <ref cel="dezir.0i.porSiMem">deziri<sncref/></ref>,
      <ref cel="klopod.0i">klopodi</ref>,
      <ref cel="pen.0i">peni</ref>,
      <ref cel="sopir.0i">sopiri</ref>,
      <ref cel="vol.0i.strebi">voli<sncref/></ref>
    </refgrp>
    <trd lng="de">trachten</trd>
    <trd lng="en">seek</trd>
    <trdgrp lng="es">
      <trd>tener <ind>af&aacute;n</ind></trd>,
      <trd>afanarse</trd>,
      <trd>sentir <ind>inclinaci&oacute;n</ind> </trd>,
    </trdgrp>
    <trdgrp lng="fr">
      <trd>tendre (&agrave;)</trd>,
      <trd>viser (&agrave;)</trd>,
      <trd>avoir pour <ind>but</ind> (de)</trd>,
      <trd>aspirer (&agrave;)</trd>
    </trdgrp>
    <trd lng="hu">t&ouml;rekszik</trd>
    <trdgrp lng="nl">
      <trd>inspannen</trd>,
      <trd>streven</trd>
    </trdgrp>
    <trd lng="pl">stara&cacute; si&eogonek;</trd>
    <trdgrp lng="pt">
      <trd>esfor&ccedil;ar-se</trd>,
      <trd>empenhar-se</trd>
    </trdgrp>
  </snc>
  <snc mrk="streb.0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      Senlime proksimi&gcirc;i:
      <ekz>
        la funkcio logaritmo <tld/>as al malfinio, kiam &gcirc;ia
        argumento <tld/>as al nulo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konver.0i.MAT">konver&gcirc;i</ref>,
      <ref cel="limes.0o">limeso</ref>
    </refgrp>
    <trd lng="en">tend<klr tip="ind"> (to a limit)</klr></trd>
    <trd lng="es">tender<klr tip="amb"> (hacia un l&iacute;mite)</klr></trd>
    <trd lng="fr">tendre<klr tip="ind"> (vers une limite)</klr></trd>
    <trd lng="pl">d&aogonek;&zdot;y&cacute;</trd>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>sna&zcaron;it se</trd>,
    <trd>usilovat</trd>
  </trdgrp>
  <trd lng="de">streben</trd>
  <trd lng="hu">tart<klr tip="ind"> (hat&aacute;r fel&eacute;)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x5168;&#x529B;&#x3092;&#x3064;&#x304F;&#x3059;  <pr>&#x305C;&#x3093;&#x308A;&#x3087;&#x304F;&#x3092;&#x3064;&#x304F;&#x3059;</pr></trd>,
    <trd>&#x61F8;&#x547D;&#x306B;&#x52AA;&#x529B;&#x3059;&#x308B; 
    <pr>&#x3051;&#x3093;&#x3081;&#x3044;&#x306B;&#x3069;&#x308A;&#x3087;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x596E;&#x95D8;&#x3059;&#x308B;  <pr>&#x3075;&#x3093;&#x3068;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">streven</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>ba&zcaron;i&tcaron;</trd>,
    <trd>pren.: driapa&tcaron; sa</trd>,
    <trd>sna&zcaron;i&tcaron; sa</trd>,
    <trd>usilova&tcaron;<klr>(sa)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_ja;&c_g;&c_n;&c_u;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x529B;&#x4E89;&#x4E0A;&#x6E38; <pr>l&#xEC;zh&#x113;ngsh&#xE0;ngy&#xF3;u</pr></trd>,
    <trd>&#x529B;&#x722D;&#x4E0A;&#x6E38; <pr>l&#xEC;zh&#x113;ngsh&#xE0;ngy&#xF3;u</pr></trd>,
    <trd>&#x52AA;&#x529B;&#x4E89;&#x53D6; <pr>n&#x1D4;l&#xEC;zh&#x113;ngq&#x1D4;</pr></trd>,
    <trd>&#x52AA;&#x529B;&#x722D;&#x53D6; <pr>n&#x1D4;l&#xEC;zh&#x113;ngq&#x1D4;</pr></trd>,
    <trd>&#x8B00;&#x6C42; <pr>m&#xF3;uqi&#xFA;</pr></trd>,
    <trd>&#x8C0B;&#x6C42; <pr>m&#xF3;uqi&#xFA;</pr></trd>,
    <trd>&#x8B00;&#x53D6; <pr>m&#xF3;uq&#x1D4;</pr></trd>,
    <trd>&#x8C0B;&#x53D6; <pr>m&#xF3;uq&#x1D4;</pr></trd>,
    <trd>&#x529B;&#x6C42; <pr>l&#xEC;qi&#xFA;</pr></trd>,
    <trd>&#x4E89;&#x53D6;&#x8FBE;&#x5230; <pr>zh&#x113;ngq&#x1D4;d&#xE1;d&#xE0;o</pr></trd>,
    <trd>&#x722D;&#x53D6;&#x9054;&#x5230; <pr>zh&#x113;ngq&#x1D4;d&#xE1;d&#xE0;o</pr></trd>,
    <trd>&#x4E89;&#x950B; <pr>zh&#x113;ngf&#x113;ng</pr></trd>,
    <trd>&#x722D;&#x92D2; <pr>zh&#x113;ngf&#x113;ng</pr></trd>,
    <trd>&#x76EE;&#x7684;&#x5728; <pr>m&#xF9;d&#xEC;z&#xE0;i</pr></trd>,
    <trd>&#x76EE;&#x7684;&#x5728;&#x4E8E; <pr>m&#xF9;d&#xEC;z&#xE0;iy&#xFA;</pr></trd>,
    <trd>&#x76EE;&#x7684;&#x5728;&#x65BC; <pr>m&#xF9;d&#xEC;z&#xE0;iy&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="streb.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="streb.0o.KOMUNE">
    <dif>
      Kuni&gcirc;o de <ref cel="vol.0o.volado">volo</ref> kaj forto por atingi celon:
      <ekz>
        en sia <tld/>ado <klr>[li]</klr> estas pli feli&ccirc;a ol liaj du pli a&gcirc;aj fratoj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kulturaj a&ubreve; politikaj <tld/>adoj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oratoro komprenigas siajn pensojn, sentojn kaj <tld/>adojn per la lingvo
        <fnt>
          <bib>Ret</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cel.0ado">celado</ref>,
      <ref cel="strecx.forto0o.KOMUNE">fortostre&ccirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_m;&c_k;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_y;&c_w;</trd>,
    <trd>&c_ib;&c_m;&c_p;&c_e;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>snaha</trd>,
    <trd>sna&zcaron;en&iacute;</trd>,
    <trd>&uacute;sil&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Streben</trd>,
    <trd>Trachten</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>impulso</trd>,
    <trd>af&aacute;n</trd>,
    <trd>contrafuerte<klr> (arquitectura)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aspiration<klr> (&eacute;lan)</klr></trd>,
    <trd>&eacute;lan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x61F8;&#x547D;&#x306E;&#x52AA;&#x529B;  <pr>&#x3051;&#x3093;&#x3081;&#x3044;&#x306E;&#x3069;&#x308A;&#x3087;&#x304F;</pr></trd>,
    <trd>&#x596E;&#x95D8;  <pr>&#x3075;&#x3093;&#x3068;&#x3046;</pr></trd>,
    <trd>&#x7CBE;&#x9032;  <pr>&#x3057;&#x3087;&#x3046;&#x3058;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">usi&lstroke;owanie</trd>
  <trdgrp lng="sk">
    <trd>dravos&tcaron;</trd>,
    <trd>drav&aacute; t&uacute;&zcaron;ba</trd>,
    <trd>n&aacute;por</trd>,
    <trd>&uacute;silie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_a;&c_g;&c_n;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_ja;&c_g;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_i;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9A45;&#x4F7F; <pr>q&#x16B;sh&#x1D0;</pr></trd>,
    <trd>&#x9A71;&#x4F7F; <pr>q&#x16B;sh&#x1D0;</pr></trd>,
    <trd>&#x52BF;&#x5934; <pr>sh&#xEC;t&#xF3;u</pr></trd>,
    <trd>&#x52E2;&#x982D; <pr>sh&#xEC;t&#xF3;u</pr></trd>,
    <trd>&#x62B1;&#x8CA0; <pr>b&#xE0;of&#xF9;</pr></trd>,
    <trd>&#x62B1;&#x8D1F; <pr>b&#xE0;of&#xF9;</pr></trd>
  </trdgrp>
</drv>
