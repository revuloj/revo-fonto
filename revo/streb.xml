<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>streb</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="streb.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="streb.0i.KOMUNE">
    <dif>
      Energie stre&ccirc;i la fortojn de la korpo a&ubreve; de la spirito por
      atingi iun celon:
      <ekz>
        <tld/>i al iu celo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        al malpieco <tld/>as ilia animo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspir.0i">aspiri</ref>,
      <ref cel="cel.0i">celi</ref>,
      <ref cel="pen.0i">peni</ref>,
      <ref cel="sopir.0i">sopiri</ref>.
    </refgrp>
    <trd lng="en">seek</trd>
    <trdgrp lng="es">
      <trd>tener <ind>af&aacute;n</ind></trd>,
      <trd>afanarse</trd>,
      <trd>sentir <ind>inclinaci&oacute;n</ind> </trd>,
    </trdgrp>
    <trdgrp lng="fr">
      <trd>tendre (&agrave;)</trd>,
      <trd>viser (&agrave;)</trd>,
      <trd>avoir pour <ind>but</ind> (de)</trd>,
      <trd>aspirer (&agrave;)</trd>
    </trdgrp>
    <trd lng="hu">t&ouml;rekszik</trd>    
    <trdgrp lng="nl">
      <trd>inspannen</trd>,
      <trd>streven</trd>
    </trdgrp>
  </snc>
  <snc mrk="streb.0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      Senlime proksimi&gcirc;i:
      <ekz>
        la funkcio logaritmo <tld/>as al malfinio, kiam &gcirc;ia
        argumento <tld/>as al nulo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konver.0i.MAT">konver&gcirc;i</ref>,
      <ref cel="limes.0o">limeso</ref>.
    </refgrp>
    <trd lng="en">tend<klr tip="ind"> (to a limit)</klr></trd>
    <trd lng="es">tender<klr tip="amb"> (hacia un l&iacute;mite)</klr></trd>
    <trd lng="fr">tendre<klr tip="ind"> (vers une limite)</klr></trd>
    <trd lng="pl">d&aogonek;&zdot;y&cacute;</trd>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_k;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">streben</trd>
  <trd lng="hu">tart<klr tip="ind"> (hat&aacute;r fel&eacute;)</klr></trd>
  <trd lng="nl">streven</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>bažiť</trd>,
    <trd>pren.: driapať sa</trd>,
    <trd>snažiť sa</trd>,
    <trd>usilovať<klr>(sa)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="streb.0o">
  <kap><tld/>o</kap>
  <snc mrk="streb.0o.KOMUNE">
    <dif>
      Kuni&gcirc;o de volo kaj forto por atingi celon:
      <ekz>
        ekzistas homoj, kiuj de tempo al tempo sentas
        <tld/>adon al poezio<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strecx.forto0o.KOMUNE">fortostre&ccirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>aspiration<klr> (&eacute;lan)</klr></trd>,
    <trd>&eacute;lan</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dravosť</trd>,
    <trd>dravá túžba</trd>,
    <trd>nápor</trd>,
    <trd>úsilie</trd>
  </trdgrp>
</drv>
</art>
