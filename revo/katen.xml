<drv mrk="katen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="katen.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Fortika <ref tip="super" cel="cxen.0o">&ccirc;eno</ref>, uzata por ligi
      malliberulojn a&ubreve; bestojn kaj malhelpi ilin forkuri:
      <ekz>
        <nom>Jozef</nom> estis vendita kiel sklavo: oni enpremis liajn
        piedojn en
        <tld/>ojn
        <fnt><bib>MT</bib>, <lok>&Psa; 105:18</lok></fnt>;
      </ekz>
      <ekz>
        venis al li <nom>Agag</nom> en <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Sa1; 15:32</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>fetter</trd>,
      <trd>chain</trd>,
      <trd>shackle</trd>
    </trdgrp>
    <trd lng="es">cadena</trd>
    <trdgrp lng="hu">
      <trd>bilincs</trd>,
      <trd>b&eacute;kly&oacute;</trd>,
      <trd>rabl&aacute;nc</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>keten<klr tip="ind"> (boei)</klr></trd>,
      <trd>boei</trd>
    </trdgrp>
  </snc>
  <snc mrk="katen.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;io, kio malhelpas libere agadi, forkuri ktp:
      <ekz>
        vi povas flugi, kien vi volas, sed ni havas <tld/>ojn sur la
        piedoj!
        <klr>(ni ne rajtas forlasi nian laboron)</klr>
        <fnt><bib>Fab1</bib>, <lok>Metamorfozo de la
        skribisto</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la virino]</klr>, kies manoj estas <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Pre; 7:26</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;u ne devis &ccirc;i tiu virino <klr>[&#x2026;]</klr>
        esti malligita el &ccirc;i tiu kateno <klr>(kripleco)</klr> en la
        sabata tago?
        <fnt><bib>NT</bib>, <lok>&Luk; 13:16</lok></fnt>;
      </ekz>
      <ekz>
        via spirito estas <tld/>ita de memfor&gcirc;itaj <tld/>oj
        <fnt><bib>I&scirc;tar</bib>,<lok>&ccirc;apitro 1a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>cadena</trd>,
      <trd>restricci&oacute;n</trd>,
      <trd>atadura</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>korl&aacute;toz</trd>,
      <trd>g&aacute;tol</trd>
    </trdgrp>
    <trd lng="nl">keten<klr tip="ind"> (fig.)</klr></trd>
  </snc>
  <snc mrk="katen.0o.linio">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib>, <lok>p. 35</lok></fnt>
    <dif>
      Ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref> la&ubreve; la
      formo, kiun alprenas peza &scirc;nuro, kies du ekstremoj estas ligitaj
      al fiksaj punktoj:
      <ekz>
        la kartezia ekvacio de <tld/>o estas de la tipo
        <frm am="y=a(e^(x//a)+e^(-x//a))//2"><k>y</k> =
        <k>a</k>(<k>e</k><sup><k>x</k>/<k>a</k></sup>+<k>e</k><sup>-<k>x</k>/<k>a</k></sup>)/2</frm>.
      </ekz>
    </dif>
    <bld lok="../bld/k_kateno.svg" tip="svg" alt="400" lrg="400">
      <tld/>o
    </bld>
    <trd lng="de">Kettenlinie</trd>
    <trd lng="en">catenary<klr tip="ind"> (curve)</klr></trd>
    <trd lng="fr">cha&icirc;nette<klr tip="ind"> (courbe)</klr></trd>
    <trd lng="hu">l&aacute;ncg&ouml;rbe</trd>
    <trd lng="nl">kettinglijn</trd>
    <trd lng="pl">&lstroke;a&nacute;cuchowa<klr tip="ind"> (linia)</klr></trd>
    <trd lng="ru">&c_c;&c_je;&c_p;&c_n;&c_a;&c_ja;
    <ind>&c_l;&c_i;&c_n;&c_i;&c_ja;</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_n;&c_c;&c_u;&c_g;</trd>,
    <trd>&c_k;&c_a;&c_j;&c_d;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>okovy</trd>,
    <trd>pouta</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kette</trd>,
    <trd>Fessel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>restraint</trd>,
    <trd>restriction</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cha&icirc;nes</trd>,
    <trd>fers</trd>,
    <trd>entraves</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x624B;&#x9396;  <pr>&#x3066;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x8DB3;&#x9396;  <pr>&#x3042;&#x3057;&#x304F;&#x3055;&#x308A;</pr></trd>,
    <trd>&#x624B;&#x304B;&#x305B;  <pr>&#x3066;&#x304B;&#x305B;</pr></trd>,
    <trd>&#x8DB3;&#x304B;&#x305B;  <pr>&#x3042;&#x3057;&#x304B;&#x305B;</pr></trd>,
    <trd>&#x675F;&#x7E1B;  <pr>&#x305D;&#x304F;&#x3070;&#x304F;</pr></trd>,
    <trd>&#x62D8;&#x675F;  <pr>&#x3053;&#x3046;&#x305D;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wi&eogonek;zy</trd>,
    <trd>okowy</trd>,
    <trd>p&eogonek;to</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>lan&#x21B;</trd>,
    <trd>leg&abreve;tur&abreve;</trd>,
    <trd>obliga&#x21B;iuni</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_d;&c_a;&c_l;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>okovy</trd>,
    <trd>put&aacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43A;&#x43E;&c_v;&c_i;</trd>,
    <trd>&#x43A;&c_a;&c_j;&c_d;&c_a;&#x43D;&c_i;</trd>,
    <trd>&#x43B;&c_a;&#x43D;&c_c;&#x44E;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7F81;&#x7ECA; <pr>j&#x12B;b&#xE0;n</pr></trd>,
    <trd>&#x9410; <pr>li&#xE0;o</pr></trd>,
    <trd>&#x9396;&#x93C8; <pr>su&#x1D2;li&#xE0;n</pr></trd>,
    <trd>&#x91F1; <pr>d&#xEC;</pr></trd>,
    <trd>&#x67B7;&#x9396; <pr>ji&#x101;su&#x1D2;</pr></trd>,
    <trd>&#x811A;&#x9563; <pr>ji&#x1CE;oli&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="katen.0i">
  <kap><tld/>i, <var><kap>en<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="katen.0i.JUR">
    <dif>
      Ligi per <tld/>o:
      <ekz>
        <tld/>ita besto ne timigas
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujn <klr>[&#x2026;]</klr> altrangulojn oni <tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Na&hcirc;um 3:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        se li eble trovos <klr>[ilin]</klr>, li alkonduku ilin 
        <tld/>itajn al <nom>Jerusalem</nom>
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 9:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estis <tld/>ita, kiam mi staris en la akuzitejo
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 2a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="cxen.0i">&ccirc;eni, en&ccirc;enigi</ref>
    <refgrp tip="vid">
       <ref cel="arest.0i">aresti</ref>,
       <ref cel="fiks.0i.flagon">fiksi<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>anketten</trd>,
      <trd>in <ind>Ketten</ind> legen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>megbilincsel</trd>,
      <trd>b&eacute;kly&oacute;ba ver</trd>,
      <trd>l&aacute;ncraver</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>ketenen</trd>,
      <trd>boeien</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_k;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_v;
      &c_k;&c_a;&c_n;&c_d;&c_a;&c_l;&c_y; <klr>(&c_o;
      &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_je;)</klr></trd>,
      <trd>&c_s;&c_a;&c_zh;&c_a;&c_t;&c_mol; &c_n;&c_a;
      &c_c;&c_je;&c_p;&c_mol; <klr>(&c_o;
      &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_m;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="katen.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malhelpi ies liberan agadon:
      <ekz>
        ili sidis en mallumo <klr>[&#x2026;]</klr>, <tld/>itaj de mizero
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 107:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        se tian grandampleksan fiksitan vortaregon <klr>[&#x2026;]</klr> ni volus pretigi
        teorie kaj &leftquot;en rapideco&rightquot;, ni tre dan&gcirc;ere en<tld/>igus nian lingvon
        <fnt>
          <bib>LR</bib>
          <lok>4. Vortaroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        en lingvaj demandoj &ccirc;iu superflua &leftquot;decido&rightquot; 
        <tld/>as
        <fnt>
          <bib>LR</bib>
          <lok>2. La verkaro de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial do ni devas senbezone <tld/>i nin
        <fnt>
          <bib>LR</bib>
          <lok>1. Vortfarado</lok>
        </fnt>?
      </ekz>
      <ekz>
        balonfarado ne vere apartenas al miaj fortaj interesoj, sed la farado de la
        oktopodo tamen sukcesis kapti mian atenton kaj <tld/>is min al la ekrano &gcirc;is
        la lasta bildo
        <fnt>
          <bib>Monato</bib>
          <aut>pp</aut>
          <vrk><url ref="&Monato;/2005/008738.php">La flugigo de oktopodo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu sian langon <tld/>as, Dio lin benas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>kiu donacon prenas, tiu sin <tld/>as</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="de">Geschenke verpflichten</trd>
      </ekz>
    </dif>
    <trd lng="es">constre&ntilde;ir</trd>
    <trdgrp lng="hu">
      <trd>korl&aacute;toz</trd>,
      <trd>g&aacute;tol</trd>
    </trdgrp>
    <trd lng="nl">ketenen<klr tip="ind"> (fig.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; &c_u; <ind>&c_l;&c_a;&c_n;&c_c;&c_u;&c_g;&c_ib;</ind></trd>,
    <trd>&c_b;&c_r;&c_a;&c_c;&c_mol; &c_n;&c_a; <ind>&c_l;&c_a;&c_n;&c_c;&c_u;&c_g;</ind></trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; &c_u; <ind>&c_k;&c_a;&c_j;&c_d;&c_a;&c_n;&c_y;</ind></trd>
  </trdgrp>
  <trd lng="cs">d&aacute;t spoutat</trd>
  <trdgrp lng="en">
    <trd>fetter</trd>,
    <trd>shackle</trd>,
    <trd>restrain</trd>,
    <trd>handcuff</trd>
  </trdgrp>
  <trd lng="es">encadenar</trd>
  <trdgrp lng="fr">
    <trd>encha&icirc;ner</trd>,
    <trd>entraver</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9396;&#x306B;&#x3064;&#x306A;&#x3050;  <pr>&#x304F;&#x3055;&#x308A;&#x306B;&#x3064;&#x306A;&#x3050;</pr></trd>,
    <trd>&#x304B;&#x305B;&#x3092;&#x304B;&#x3051;&#x308B;</trd>,
    <trd>&#x675F;&#x7E1B;&#x3059;&#x308B;  <pr>&#x305D;&#x304F;&#x3070;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x62D8;&#x675F;&#x3059;&#x308B;  <pr>&#x3053;&#x3046;&#x305D;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>skuwa&cacute;</trd>,
    <trd>wi&eogonek;zi&cacute;</trd>,
    <trd>zaku&cacute; w &lstroke;a&nacute;cuchy</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>&icirc;nc&abreve;tu&#x219;a</trd>,
    <trd>&icirc;nchisoare</trd>
  </trdgrp>
  <trd lng="sk">da&tcaron; sp&uacute;ta&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#x43A;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i; &c_v; &#x43A;&c_a;&c_j;&c_d;&c_a;&#x43D;&c_i;</trd>,
    <trd>&c_s;&c_a;&c_d;&c_zh;&c_a;&c_t;&c_i; &#x43D;&c_a; &#x43B;&c_a;&#x43D;&c_c;&#x44E;&c_g;</trd>,
    <trd>&c_s;&#x43A;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_v;&#x2019;&#x44F;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x91F1; <pr>d&#xEC;</pr></trd>,
    <trd>&#x675F;&#x7E1B; <pr>sh&#xF9;f&#xF9;</pr></trd>,
    <trd>&#x811A;&#x9563; <pr>ji&#x1CE;oli&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="katen.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>ita:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dum originala verkisto povas esprimi penson per diversaj parolturnoj, tradukisto
        devas kateni&gcirc;i al la originaj sencoj
        <fnt>
          <bib>Kon11</bib>
          <vrk>Gerrit Berveling &ndash; Plivalorigante la Esperantan literaturon</vrk>
        </fnt>;
      </ekz>
      <ekz>
        tro rapida edzi&gcirc;o, por&ccirc;iama <ind><tld/>i&gcirc;o</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">esclavage</trd>
        <trd lng="nl">onderworpenheid</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">s'encha&icirc;ner</trd>
  <trd lng="nl">in de <ind>boeien</ind> geslagen worden</trd>
  <trd lng="pl">katowa&cacute; si&eogonek;</trd>     
  <trd lng="ro">tortura</trd>
</drv>

<drv mrk="katen.al0i">
  <kap>al<tld/>i</kap>
  <snc mrk="katen.al0i.ligi">
    <dif>
      Ligi al io per <tld/>o:
      <ekz>
        al<tld/>i hundon al pordo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi estas al<tld/>ita, sed al pan-dona arbo! <klr>(al bona
        ofico)</klr>
        <fnt><bib>Fab1</bib> <lok>Metamorfozo de la
        skribisto</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[la patrino]</klr> de multaj monatoj estis al<tld/>ita al
        la
        lito
        <fnt><bib>Fab3</bib> <lok>Juda knabino</lok></fnt>;
      </ekz>
      <ekz>
        la venkitajn re&gcirc;ojn li al<tld/>igis per oraj &ccirc;enoj al sia
        kale&scirc;o
        <fnt><bib>Fab4</bib> <lok>Malbona princo</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="forgx.al0i">alfor&gcirc;i.</ref>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">p&rcaron;ipoutat</trd>
  <trdgrp lng="de">
    <trd>anketten</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>attacher (&agrave;)</trd>,
    <trd>clouer (&agrave;)<klr tip="ind"> (immobiliser)</klr></trd>,
    <trd>immobiliser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>odal&aacute;ncol</trd>,
    <trd>hozz&aacute;l&aacute;ncol</trd>
  </trdgrp>
  <trd lng="ja">&#x9396;&#x3067;&#x3064;&#x306A;&#x3050;  <pr>&#x304F;&#x3055;&#x308A;&#x3067;&#x3064;&#x306A;&#x3050;</pr></trd>
  <trd lng="nl">vastketenen</trd>
  <trd lng="pl">przyku&cacute;</trd>
  <trdgrp lng="ro">
    <trd>captur&abreve;</trd>,
    <trd>re&#x21B;ine</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">prip&uacute;ta&tcaron;</trd>
</drv>

<drv mrk="katen.mal0i">
  <kap>mal<tld/>i, <var><kap>sen<tld/>igi</kap></var></kap>
  <snc>
    <dif>
      Forpreni la <tld/>ojn:
      <ekz>
        oni forpardonu &ccirc;iujn punojn, sen<tld/>igu la homojn
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        liaj pensoj sen<tld/>i&gcirc;is, kiel ondoj, tordataj de ventego
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Houdini</nom> <klr>[&#x2026;]</klr>, la usona sen<tld/>igisto kaj eskapisto
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007096.php">Ekskapo eskapas</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 8</lok>
          </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="cxen.el0igi.TEK">de&ccirc;enigi, sen&ccirc;enigi</ref>
