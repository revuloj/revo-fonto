<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>katen</rad>/o
</kap>
<drv mrk="katen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">JUR</uzo>
    <dif>
      Fortika <ref tip="super" cel="cxen.0o">&ccirc;eno</ref>, uzata por ligi
      malliberulojn a&ubreve; bestojn kaj malhelpi ilin forkuri:
      <ekz>
        Jozef estis vendita kiel sklavo: oni enpremis liajn piedojn en
        <tld/>ojn
        <fnt><bib>M</bib>, <lok>&Psa; 105:18</lok></fnt>;
      </ekz>
      <ekz>
        venis al li Agag en <tld/>oj
        <fnt><bib>M</bib>, <lok>&Sa1; 15:32</lok></fnt>.
      </ekz>
    </dif>
   <trdgrp lng="en">
     <trd>fetter</trd>,
     <trd>chain</trd>,
     <trd>shackle</trd>
    </trdgrp> 
   <trd lng="nl">keten</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      &Ccirc;io, kio malhelpas libere agadi, forkuri ktp:
      <ekz>
        vi povas flugi, kien vi volas, sed ni havas <tld/>ojn sur la piedoj!
        <klr>(ni ne rajtas forlasi nian laboron)</klr>
        <fnt><bib>Fab1</bib>, <lok>Metamorfozo de la skribisto</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[la virino]</klr>, kies manoj estas <tld/>oj
        <fnt><bib>M</bib>, <lok>&Pre; 7:26</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;u ne devis &ccirc;i tiu virino <klr>[...]</klr>
        esti malligita el &ccirc;i tiu kateno <klr>(kripleco)</klr> en la
        sabata tago?
        <fnt><bib>X</bib>, <lok>&Luk; 13:16</lok></fnt>;
      </ekz>
      <ekz>
        via spirito estas <tld/>ita de memfor&gcirc;itaj <tld/>oj
        <fnt><bib>I&scirc;tar</bib>,<lok>&ccirc;ap. 1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>restraint</trd>,
    <trd>restriction</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cha&icirc;nes</trd>,
    <trd>fers</trd>,
    <trd>entraves</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_d;&c_a;&c_l;&c_y;</trd>
  </trdgrp>
</drv>
<drv mrk="katen.0i">
  <kap><tld/>i, en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="katen.0i.konkrete">
    <dif>
      Ligi per <tld/>o:
      <ekz>
        <tld/>i krimulon, beston.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_z;&c_a;&c_k;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_v; &c_k;&c_a;&c_n;&c_d;&c_a;&c_l;&c_y; <klr>(&c_o; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_je;)</klr></trd>,
      <trd>&c_s;&c_a;&c_zh;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_c;&c_je;&c_p;&c_mol; <klr>(&c_o; &c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_m;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="katen.0i.figure">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malhelpi ies liberan agadon:
      <ekz>kiu donacon prenas, tiu sin <tld/>as
        <fnt><bib>PrV</bib></fnt></ekz>
      <ekz>
        ili sidis en mallumo, <tld/>itaj de mizero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kial do ni devas senbezone <tld/>i nin? <fnt>Z</fnt>;
      </ekz>
      <ekz>kiu sian langon <tld/>as, Dio lin benas
        <fnt><bib>PrV</bib></fnt></ekz>
      <ekz>
        en lingvaj demandoj &ccirc;iu superflua decido malutile
        <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        dan&gcirc;ere en<tld/>igi la lingvon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li staris, kvaza&ubreve; <tld/>ite al la tero;
      </ekz>
      <ekz>
        <tld/>is min tiu grandioza spektaklo <klr>(mi ne povis foriri
        rezignante tiun spektaklon)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_k;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>encha&icirc;ner</trd>,
    <trd>entraver</trd>
  </trdgrp>
  <trd lng="nl">ketenen</trd>
</drv>
<drv mrk="katen.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i <tld/>ita:
      <ekz>tro rapida edzi&gcirc;o, por&ccirc;iama <ind><tld/>i&gcirc;o</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">esclavage</trd></ekz>
    </dif>
  </snc>
</drv>
<drv mrk="katen.al0i">
  <kap>al<tld/>i</kap>
  <snc>
    <dif>
      Ligi al io per <tld/>o:
      <ekz>
        al<tld/>i hundon al pordo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi estas al<tld/>ita, sed al pan-dona arbo! <klr>(al bona ofico)</klr>
        <fnt><bib>Fab1</bib>, <lok>Metamorfozo de la skribisto</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[la patrino]</klr> de multaj monatoj estis al<tld/>ita al la lito
        <fnt><bib>Fab3</bib>, <lok>Juda knabino</lok></fnt>;
      </ekz>
      <ekz>
        la venkitajn re&gcirc;ojn li al<tld/>igis per oraj &ccirc;enoj al sia
        kale&scirc;o
        <fnt><bib>Fab4</bib>, <lok>Malbona princo</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="forgx.al0i">alfor&gcirc;i.</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>attacher (&agrave;)</trd>,
    <trd>clouer (&agrave;)<klr tip="ind"> (immobiliser)</klr></trd>,
    <trd>immobiliser</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="katen.mal0i">
  <kap>mal<tld/>i, sen<tld/>igi</kap>
  <snc>
    <dif>
      Forpreni la <tld/>ojn:
      <ekz>
        mal<tld/>i sklavon.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;cha&icirc;ner</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="katen.man0o">
  <kap>man<tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <tld lit="K"/>o kun du &scirc;loseblaj ringoj, per kiuj oni ligas
      la manartikojn de arestito.
    </dif>
  </snc>
  <trd lng="fr">menottes</trd>
  <trd lng="ru">&c_n;&c_a;&c_r;&c_u;&c_ch;&c_n;&c_i;&c_k;&c_i;</trd>
</drv>
<drv mrk="katen.prem0i">
  <kap>prem<tld/>i</kap>
  <snc>
    <dif>
      <tld lit="K"/>i <sncref ref="katen.0i.figure"/> per forta morala premo:
      <ekz>
        Mustafa Kemal Pa&scirc;a persiste provas disrompi la religian kirason,
        kiu prem<tld/>as la popolon
        <fnt><bib>Lanti</bib>, <lok>Leteroj el Turkio</lok></fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
