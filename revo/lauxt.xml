  <trd lng="th">&#xE40;&#xE2A;&#xE35;&#xE22;&#xE07;&#xE14;&#xE31;&#xE07;</trd>
  <trd lng="tl">malakas</trd>
  <trd lng="ur">&#x628;&#x644;&#x646;&#x62F; &#x622;&#x648;&#x627;&#x632;</trd>
