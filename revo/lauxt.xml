<drv mrk="lauxt.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="lauxt.0a.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Forte, intense a&ubreve;debla:
      <ekz>
        <tld/>a vo&ccirc;o, &gcirc;emo, krio, sono, bruo, rido, &gcirc;ojo;
      </ekz>
      <ekz>
        &ccirc;i tie precipe la printempo estas <tld/>ega, plena je bruo de
        la riveroj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>e ridi, plori;
      </ekz>
      <ekz>
        <tld/>e legi;
      </ekz>
      <ekz>
        <tld/>e eldiri sian opinion.
      </ekz>
    </dif>
    <trd lng="ca">alt</trd>
    <trd lng="en">loud <klr>(volume)</klr></trd>
    <trd lng="es">alto</trd>
    <trdgrp lng="pl">
      <trd>g&lstroke;o&sacute;ny</trd>,
      <trd>gromki</trd>
    </trdgrp>
    <trd lng="pt">alto <klr tip="amb">(som)</klr></trd>
  </snc>
  <snc mrk="lauxt.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Emfaza, efekta:
      <ekz>
        <tld/>aj frazoj<fnt>Z</fnt>,
      </ekz>
      <ekz>
        projektoj;
      </ekz>
      <ekz>
        <nom>Volap&uuml;k</nom> povis nur pase entuziasmigi la homojn sub
        influo de <tld/>aj reklamoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  <trd lng="en">loud <klr>(emphatic)</klr></trd>
  </snc>
  <trd lng="af">hard</trd>
  <trd lng="am">&#4910;&#4781; &#4709;&#4622;</trd>
