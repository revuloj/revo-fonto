<drv mrk="lauxt.0ilo">
  <kap><tld/>ilo, <var><kap><tld/>parolilo</kap></var></kap>
  <snc mrk="lauxt.0ilo.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Aparato por <tld/>igi vo&ccirc;on a&ubreve; sonon:
      <ekz>
        la generaloj per <tld/>paroliloj informis &ccirc;iujn,
        ke ili ne zorgu, &ccirc;ar &ccirc;io estas en ordo
        <fnt>
          <aut>S. Strunjak</aut>,
          <vrk>Kiel formikbebo savis...</vrk>,
          <bib>LOdE</bib>,
          <lok>2003:1</lok>
         </fnt>;
      </ekz>
      <ekz>
        oni a&ubreve;digis dum la tuta tempo la himnon
        <ctl>La Espero</ctl> dissendita per <tld/>iloj
        <fnt>
          <aut>-</aut>,
          <vrk>89.a Universala Kongreso en Pekino</vrk>,
          <lok><url ref="http://www.ueci.it/k_sento/ks_2004/ks_2004_i5.htm">
          Katolika Sento, 2004:5</url></lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">speaker <klr>(sound emitter)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_ch;&c_n;&c_a;&c_g;&c_a;&c_v;&c_a;&c_r;&c_y;&c_c;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_r;&c_e;&c_p;&c_r;&c_a;&c_d;&c_u;&c_k;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_r;&c_u;&c_p;&c_a;&c_r;</trd>,
    <trd>&c_d;&c_y;&c_n;&c_a;&c_m;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="br">uhelgomzer</trd>
  <trd lng="ca">altaveu</trd>
  <trd lng="de">Lautsprecher</trd>
  <trdgrp lng="en">
    <trd>loud speaker</trd>,
    <trd>public announcement system</trd>
  </trdgrp>
  <trd lng="es">altavoz</trd>
  <trdgrp lng="fr">
    <trd>baffle</trd>,
    <trd>enceinte<klr> (baffle)</klr></trd>,
