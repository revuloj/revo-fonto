<drv mrk="lauxt.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lauxt.mal0igi.SON">
    <dif>
      Igi mal<tld/>a, sone malforta,
      <ref tip="vid" cel="damp.0i.MUZ">dampi</ref>:
      <ekz>
        &scirc;i komencis per vo&ccirc;o sufi&ccirc;e mal<tld/>a, tamen ne intence mal<tld/>igita nek
        tremanta
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&ccirc;u vi]</klr> konas la sonobtuzigajn potojn, kiuj e&ccirc; la plej
        orel&scirc;iran bruegon mal<tld/>igas &gcirc;is tolerebla
        <fnt>
          <bib>VaK</bib>
          <lok>Sepa &Ccirc;apitro</lok>
        </fnt>?
      </ekz>
    </dif>
  <trd lng="en">soften <klr>(volume)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_sh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_l;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>leiser</ind> machen</trd>,
    <trd><klr>(die Stimme)</klr> <ind>senken</ind></trd>,
    <trd>die <ind>Lautst&auml;rke</ind> reduzieren</trd>
  </trdgrp>
  <trd lng="en">decrescendo</trd>
  <trdgrp lng="fr">
    <trd>assourdir</trd>,
    <trd>&eacute;touffer<klr> (un son)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>lirih</baz>melirihkan</trd>,
    <trd><baz>lembut</baz>melembutkan suara</trd>,
    <trd><baz>pelan</baz>memelankan suara</trd>
  </trdgrp>
  <trd lng="pl">&sacute;cisza&cacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_i;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_t;&c_i;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_g;&c_l;&c_u;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_g;&c_l;&c_u;&c_sh;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="lauxt.mal0igxi">
  <kap>
    mal<tld/>i&gcirc;i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="lauxt.mal0igxi.SON">
    <dif>
      I&gcirc;i mal<tld/>a, malpli <tld/>a ol anta&ubreve;e:
      <ekz>
        la flustrado kaj la tintado pli kaj pli mal<tld/>i&gcirc;is
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akorda akompano, malklari&gcirc;is, mal<tld/>i&gcirc;is kaj diskuris
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>i&gcirc;is la motorzumoj
        <fnt>
          <aut>Okada H., trad. Konisi G.</aut>
          <vrk>Notoj pri la Delto</vrk>
          <lok>1992</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">soften <klr>(volume)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_sh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_l;&c_u;&c_sh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de"><ind>leiser</ind> werden</trd>
  <trd lng="fr">s'<ind>assourdir</ind></trd>
  <trdgrp lng="id">
    <trd><baz>lirih</baz>melirih</trd>,
    <trd><baz>lembut</baz>melembut<klr tip="amb"> (ttg suara)</klr></trd>,
    <trd><baz>pelan</baz>memelan<klr tip="amb"> (ttg suara)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>uciszy&cacute; si&eogonek;</trd>,
    <trd>cichn&aogonek;&cacute;</trd>
  </trdgrp>
</drv>
