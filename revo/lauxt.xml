<drv mrk="lauxt.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="lauxt.0igi.SON">
    <dif>
      Igi <tld/>a, sone pli forta, pli a&ubreve;debla:
      <ekz>
        <tld/>igu potence vian vo&ccirc;on<fnt>Z</fnt>.
      </ekz>
    </dif>
  <trd lng="en">amplify <klr>(volume)</klr></trd>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_m;&c_a;&c_c;&c_n;&c_ja;&c_c;&c_mol; <klr>(&c_g;&c_u;&c_k;)</klr></trd>
  <trd lng="fr">hausser<klr> (la voix)</klr></trd>
  <trd lng="pl">podg&lstroke;a&sacute;nia&cacute;</trd>
