<art mrk="$Id: lauxt.xml,v 1.66 2021/06/19 05:58:46 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>la&ubreve;t</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lauxt.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="lauxt.0a.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Forte, intense a&ubreve;debla:
      <ekz>
        <tld/>a vo&ccirc;o, &gcirc;emo, krio, sono, bruo, rido, &gcirc;ojo;
      </ekz>
      <ekz>
        &ccirc;i tie precipe la printempo estas <tld/>ega, plena je bruo de
        la riveroj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>e ridi, plori;
      </ekz>
      <ekz>
        <tld/>e legi;
      </ekz>
      <ekz>
        <tld/>e eldiri sian opinion.
      </ekz>
    </dif>
    <trd lng="ca">alt</trd>
    <trd lng="es">alto</trd> 
    <trdgrp lng="pl">
      <trd>g&lstroke;o&sacute;ny</trd>,
      <trd>gromki</trd>
    </trdgrp>
    <trd lng="pt">alto <klr tip="amb">(som)</klr></trd>
  </snc>
  <snc mrk="lauxt.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Emfaza, efekta:
      <ekz>
        <tld/>aj frazoj<fnt>Z</fnt>,
      </ekz>
      <ekz>
        projektoj;
      </ekz>
      <ekz>
        <nom>Volap&uuml;k</nom> povis nur pase entuziasmigi la homojn sub
        influo de <tld/>aj reklamoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="pl">g&lstroke;o&sacute;ny</trd>
  </snc>          
  <trd lng="af">hard</trd>
  <trd lng="am">&#x132E;&#x12AD; &#x1265;&#x120E;</trd>
  <trd lng="ar">&#x628;&#x635;&#x648;&#x62A; &#x639;&#x627;&#x644;</trd>
  <trd lng="az">y&#xFC;ks&#x259;k</trd>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_a;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bn">&#x985;&#x99F;&#x9CD;&#x99F;</trd>
  <trd lng="bo">&#xF66;&#xF90;&#xF51;&#xF0B;&#xF46;&#xF7A;&#xF53;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>trouzus</trd>,
    <trd>kre&ntilde;v<klr tip="ind"> (mouezh, trouz)</klr></trd>,
    <trd>uhel<klr tip="ind"> (mouezh, trouz)</klr></trd>
  </trdgrp>
  <trd lng="bs">glasno</trd>
  <trd lng="cs">hlasit&yacute;</trd>
  <trd lng="da">h&#xF8;jt</trd>
  <trd lng="de">laut</trd>
  <trd lng="en">loud</trd>   
  <trd lng="et">vali</trd>
  <trd lng="eu">ozen</trd>
  <trdgrp lng="fr">
    <trd>fort<klr tip="amb"> (bruit)</klr></trd>,
    <trd>haute<klr tip="amb"> (voix)</klr></trd>
  </trdgrp>
  <trd lng="fy">l&#xFB;d</trd>
  <trd lng="gl">alto</trd>
  <trd lng="gu">&#xAAE;&#xACB;&#xA9F;&#xABE;</trd>
  <trd lng="hi">&#x91C;&#x94B;&#x930;</trd>
  <trd lng="hr">glasno</trd>
  <trd lng="hu">hangos</trd>
  <trd lng="hy">&#x562;&#x561;&#x580;&#x571;&#x580;&#x561;&#x571;&#x561;&#x575;&#x576;</trd>
  <trd lng="is">h&#xE1;v&#xE6;r</trd>
  <trd lng="ja">&#x9A12;&#x3005;&#x3057;&#x3044;</trd>
  <trd lng="jw">sora</trd>
  <trd lng="ka">&#x10EE;&#x10DB;&#x10D0;&#x10DB;&#x10D0;&#x10E6;&#x10D0;&#x10DA;&#x10D8;</trd>
  <trd lng="km">&#x1781;&#x17D2;&#x179B;&#x17B6;&#x17C6;&#x1784;</trd>
  <trd lng="ko">&#xD070; &#xC18C;&#xB9AC;&#xB85C;</trd>
  <trd lng="lo">&#xE94;&#xEB1;&#xE87;</trd>
  <trd lng="ml">&#xD09;&#xD1A;&#xD4D;&#xD1A;&#xD24;&#xD4D;&#xD24;&#xD3F;&#xD32;&#xD41;&#xD33;&#xD4D;&#xD33;</trd>
  <trd lng="mr">&#x92E;&#x94B;&#x920;&#x93E; &#x906;&#x935;&#x93E;&#x91C;</trd>
  <trd lng="my">&#x1021;&#x101E;&#x1036;&#x1000;&#x103B;&#x101A;&#x103A;&#x101E;&#x1031;&#x102C;</trd>
  <trd lng="ne">&#x91A;&#x930;&#x94D;&#x915;&#x94B;</trd>
  <trd lng="nl">luid</trd>
  <trd lng="ny">mokweza</trd>
  <trd lng="pa">&#xA09;&#xA71;&#xA1A;&#xA40;</trd>
  <trd lng="ps">&#x644;&#x648;&#x693;</trd>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_m;&c_k;&c_i;&c_j;</trd>
  <trd lng="sd">&#x628;&#x644;&#x646;&#x62F;</trd>
  <trd lng="si">&#xDC1;&#xDB6;&#xDCA;&#xDAF; &#xDC0;&#xDD2;&#xD9A;&#xDCF;&#xDC1;&#xDB1; &#xDBA;&#xDB1;&#xDCA;&#xDAD;&#xDCA;&#xDBB;</trd>
  <trd lng="sq">me z&#xEB; t&#xEB; lart&#xEB;</trd>
