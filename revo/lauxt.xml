<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>la&ubreve;t</rad>/a
</kap>
<drv mrk="lauxt.0a">
  <kap>
    <ofc>*</ofc><tld/>a
  </kap>
  <snc>
    <dif>
      Forte, intense a&ubreve;debla:
      <ekz>
        <tld/>a vo&ccirc;o, &gcirc;emo, krio, sono, bruo, rido, &gcirc;ojo;
      </ekz>
      <ekz>
        &ccirc;i tie precipe la printempo estas <tld/>ega, plena je bruo de la
        riveroj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>e ridi, plori;
      </ekz>
      <ekz>
        <tld/>e legi;
	<trd lng="ca">llegir en veu alta</trd>
	<trd lng="es">leer en voz alta</trd>
      </ekz>
      <ekz>
        <tld/>e eldiri sian opinion;
      </ekz>
      <ekz>
        <tld/>igu potence vian vo&ccirc;on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ca">alt</trd>
    <trd lng="es">alto</trd>
    <trd lng="pt">alto</trd>
  </snc>
  <snc>
    <dif>
      Emfaza, efekta:
      <ekz>
        <tld/>aj frazoj<fnt>Z</fnt>,
      </ekz>
      <ekz>
        projektoj;
      </ekz>
      <ekz>
        Volap&uuml;k povis nur pase entuziasmigi la homojn sub
        influo de <tld/>aj reklamoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_u;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_a;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">hlasit&yacute;</trd>
  <trd lng="de">laut</trd>
  <trd lng="en">loud</trd>
  <trd lng="hu">hangos</trd>
  <trd lng="nl">luid</trd>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_m;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="lauxt.mal0a">
  <kap>
    mal<tld/>a
  </kap>
  <snc>
    <dif>
      Malforte, apena&ubreve; a&ubreve;debla:
      <ekz>
        mal<tld/>e flustri en la orelo;
	<trd lng="es">susurrar al o&iacute;do en voz baja</trd>
      </ekz>
      <ekz>
        la ne&gcirc;o mal<tld/>igas la bruon de la pa&scirc;oj.
	<trd lng="es">la nieve silencia el ruido de los pasos</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_ib;&c_h;&c_ib;</trd>
  <trd lng="de">leise</trd>
  <trdgrp lng="es">
    <trd>bajo<klr> (voz)</klr></trd>
    <trd>silencioso</trd>
  </trdgrp>
  <trd lng="hu">halk</trd>
  <trd lng="nl">zacht<klr tip="ind"> (v.geluid)</klr></trd>
  <trd lng="pt">baixo</trd>
  <trd lng="ru">&c_t;&c_i;&c_h;&c_i;&c_j;<klr tip="ind"> (&c_o; &c_z;&c_v;&c_u;&c_k;&c_je;)</klr></trd>
</drv>

<drv mrk="lauxt.0parolilo">
  <kap>
    <tld/>parolilo
  </kap>
  <snc>
    <dif>
      Aparato por <tld/>igi vo&ccirc;on a&ubreve; sonon.
    </dif>
  </snc>
  <trd lng="ca">altaveu</trd>
  <trd lng="de">Lautsprecher</trd>
  <trd lng="es">altavoz</trd>
  <trd lng="hu">hangsz&oacute;r&oacute;</trd>
  <trd lng="nl">luidspreker</trd>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_m;&c_k;&c_o;&c_g;&c_o;&c_v;&c_o;&c_r;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
