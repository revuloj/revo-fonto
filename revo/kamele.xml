<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kameleon</rad>/o
</kap>
<drv mrk="kamele.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kamele.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Chamaeleon</trd>).
    </dif>
  </snc>
  <trd lng="de">Cham&auml;leon</trd>
  <trd lng="en">chameleon</trd>
  <trd lng="fr">cam&eacute;l&eacute;on</trd>
</drv>
<drv mrk="kamele.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Chamaeleonidae</trd>), kun envolvebla vosto,
      el&jcirc;etebla lango por kapti insektojn kaj
      kolor&scirc;an&gcirc;ema ha&ubreve;to.
    </dif>
  </snc>
  <trd lng="de">Cham&auml;leons <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
