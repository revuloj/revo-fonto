<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: marsk.xml,v 1.3 2024/01/25 20:10:35 afido Exp $">
    <kap>
      <rad>marsk</rad>/o
    </kap>

<drv mrk="marsk.0o">
  <kap><tld/>o</kap>
  <snc mrk="marsk.0o.aluvio">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="super" cel="aluvi.0o.GEOL">Aluvia</ref> herbeja grundo, 
      esti&gcirc;inta post la lasta glaciepoko, fekunda kaj malseka, proksima de maroj kaj riverdeltoj:
      <ekz>
        mi vidis <klr>[&#x2026;]</klr> la dezertan, de &ccirc;iuj bestoj jam forlasitan <tld/>on
        <fnt>
          <aut>Theodor Storm, trad. Heino Heitm&uuml;ller</aut>
          <vrk><url ref="http://i-espero.info/files/elibroj/eo%20-%20storm,%20theodor%20-%20la%20blankcxevala%20rajdanto.pdf">La Blank&ccirc;evala Rajdanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Norda E&ubreve;ropo, socioj povis vivi bone danke al abundaj man&gcirc;a&jcirc;provizadoj de la
        <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=23553">Prahistorio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="polder.0o">poldero</ref>
    <trdgrp lng="de">
      <trd>Marschland</trd>,
      <trd>Schwemmland</trd>
    </trdgrp>
    <trd lng="en">tidal marsh</trd>
    <trd lng="fr">marais maritime</trd>
    <trdgrp lng="zh">
    <trd>&#x85AE; <pr>s&#x1D2;u</pr></trd>,
    <trd>&#x85EA; <pr>s&#x1D2;u</pr></trd>,
    <trd>&#x51B2;&#x79EF;&#x5730; <pr>ch&#xF2;ngj&#x12B;de</pr></trd>,
    <trd>&#x6C96;&#x7A4D;&#x5730; <pr>ch&#xF2;ngj&#x12B;de</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x440;&#x448;</trd>,
    <trd>&#x43D;&#x438;&#x437;&#x43E;&#x432;&#x438;&#x43D;&#x43D;&#x435; &#x43C;&#x43E;&#x440;&#x441;&#x44C;&#x43A;&#x435; &#x443;&#x437;&#x431;&#x435;&#x440;&#x435;&#x436;&#x436;&#x44F;</trd>
  </trdgrp>
</drv>

  </art>

