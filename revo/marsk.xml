<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: marsk.xml,v 1.4 2025/04/02 18:50:41 afido Exp $">
    <kap>
      <rad>marsk</rad>/o
    </kap>

<drv mrk="marsk.0o">
  <kap><tld/>o</kap>
  <snc mrk="marsk.0o.aluvio">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="super" cel="aluvi.0o.GEOL">Aluvia</ref> herbeja grundo, 
      esti&gcirc;inta post la lasta glaciepoko, fekunda kaj malseka, proksima de maroj kaj riverdeltoj:
      <ekz>
        mi vidis <klr>[&#8230;]</klr> la dezertan, de &ccirc;iuj bestoj jam forlasitan <tld/>on
        <fnt>
          <aut>Theodor Storm, trad. Heino Heitm&uuml;ller</aut>
          <vrk><url ref="http://i-espero.info/files/elibroj/eo%20-%20storm,%20theodor%20-%20la%20blankcxevala%20rajdanto.pdf">La Blank&ccirc;evala Rajdanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Norda E&ubreve;ropo, socioj povis vivi bone danke al abundaj man&gcirc;a&jcirc;provizadoj de la
        <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=23553">Prahistorio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="polder.0o">poldero</ref>
    <trdgrp lng="de">
      <trd>Marschland</trd>,
      <trd>Schwemmland</trd>
    </trdgrp>
    <trd lng="en">tidal marsh</trd>
    <trd lng="fr">marais maritime</trd>
    <trdgrp lng="zh">
      <trd>&#34222; <pr>s&#466;u</pr></trd>,
      <trd>&#34282; <pr>s&#466;u</pr></trd>,
      <trd>&#20914;&#31215;&#22320; <pr>ch&#242;ngj&#299;de</pr></trd>,
      <trd>&#27798;&#31309;&#22320; <pr>ch&#242;ngj&#299;de</pr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>&#349;limebena&#309;o</ke>.
  </rim>

  <trdgrp lng="uk">
    <trd>&#1084;&#1072;&#1088;&#1096;</trd>,
    <trd>&#1085;&#1080;&#1079;&#1086;&#1074;&#1080;&#1085;&#1085;&#1077; &#1084;&#1086;&#1088;&#1089;&#1100;&#1082;&#1077; &#1091;&#1079;&#1073;&#1077;&#1088;&#1077;&#1078;&#1078;&#1103;</trd>
  </trdgrp>
</drv>

  </art>


