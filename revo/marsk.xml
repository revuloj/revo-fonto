<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: marsk.xml,v 1.5 2026/02/16 20:49:26 afido Exp $">
    <kap>
      <rad>marsk</rad>/o
    </kap>

<drv mrk="marsk.0o">
  <kap><tld/>o</kap>
  <snc mrk="marsk.0o.aluvio">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="super" cel="aluvi.0o.GEOL">Aluvia</ref> herbeja grundo,
      esti&gcirc;inta post la lasta glaciepoko, fekunda kaj malseka, proksima de maroj kaj riverdeltoj:
      <ekz>
        mi vidis <klr>[&#8230;]</klr> la dezertan, de &ccirc;iuj bestoj jam forlasitan <tld/>on
        <fnt>
          <aut>Theodor Storm, trad. Heino Heitm&uuml;ller</aut>
          <vrk><url
          ref="http://i-espero.info/files/elibroj/eo%20-%20storm,%20theodor%20-%20la%20blankcxevala%20rajdanto.pdf">La
          Blank&ccirc;evala Rajdanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Norda E&ubreve;ropo, socioj povis vivi bone danke al abundaj man&gcirc;a&jcirc;provizadoj de la
        <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=23553">Prahistorio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="polder.0o">poldero</ref>
    <trdgrp lng="de">
      <trd>Marschland</trd>,
      <trd>Schwemmland</trd>
    </trdgrp>
    <trd lng="en">tidal marsh</trd>
    <trd lng="fr">marais maritime</trd>
    <trd lng="pl">teren nizinny</trd>
    <trdgrp lng="zh">
      <trd>&#34222; <pr>s&#466;u</pr></trd>,
      <trd>&#34282; <pr>s&#466;u</pr></trd>,
      <trd>&#20914;&#31215;&#22320; <pr>ch&ograve;ngj&imacron;de</pr></trd>,
      <trd>&#27798;&#31309;&#22320; <pr>ch&ograve;ngj&imacron;de</pr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>&scirc;limebena&jcirc;o</ke>.
  </rim>

  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_r;&c_sh;</trd>,
    <trd>&c_n;&c_i;&c_z;&c_o;&c_v;&c_i;&c_n;&c_n;&c_je; &c_m;&c_o;&c_r;&c_s;&c_mol;&c_k;&c_je; &c_u;&c_z;&c_b;&c_je;&c_r;&c_je;&c_zh;&c_zh;&c_ja;</trd>
  </trdgrp>
</drv>
</art>


