    <trd><baz>singkir</baz>menyingkirkan</trd>
  </trdgrp>
  <trd lng="ja">&#36960;&#12374;&#12369;&#x308B;  <pr>&#12392;&#x304A;&#12374;&#12369;&#x308B;</pr></trd>
  <trd lng="sk">vzdiali&tcaron;</trd>
