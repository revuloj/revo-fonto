    <trd>&c_n;&c_a;&c_b;&c_l;&c_ib;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_h;&c_o;&c_d;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_b;&c_y;&c_c;&c_mol;&c_c;&c_jo;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_y;&c_h;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="de">Ann&auml;herung</trd>
