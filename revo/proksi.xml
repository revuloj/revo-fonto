<drv mrk="proksi.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="proksi.mal0igi.KOMUNE">
    <dif>
      Fari mal<tld/>a:
      <ekz>
        oni <klr>[&#8230;]</klr> forpelis jam en mal<tld/>igitajn bu&ccirc;ejojn la scenojn de
        mortigado de bestoj, kiuj ne estas eblaj sen fluoj de sango
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        miajn fratojn Li mal<tld/>igis de mi kaj miaj konatoj forfremdi&gcirc;is de mi
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 19:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi mal<tld/>igis de mi amanton kaj amikon
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 88:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        post tri minutoj oni mal<tld/>igu la &scirc;ra&ubreve;bon el la fajro
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011332.php">Dilati&gcirc;o, kuntiri&gcirc;o ... kaj
          eksperimento por infana amuzi&gcirc;o</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="for.0igi">forigi</ref>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_d;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="cs">odd&aacute;lit</trd>
