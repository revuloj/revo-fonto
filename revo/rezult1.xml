  </trdgrp>
  <trd lng="pl">skutkowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_je;&c_s;&c_t;&c_i; <klr>(&c_k; &c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;&c_u;,
