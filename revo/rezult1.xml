<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rezult1.xml,v 1.24 2026/02/15 08:49:40 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>1</ofc><ofc>7</ofc>
  <rad>rezultat</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="rezult1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Fina sekvo de ia agado:
      <ekz>
        sed ni ne povis atingi ian <tld/>on kaj ni disiris
        <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 42</lok></fnt>;
      </ekz>
      <ekz>
        la ju&gcirc;a esploro restas sen <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kia estas la <tld/>o de iliaj pre&gcirc;oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>?
      </ekz>
      <ekz>
        <klr>[li]</klr> dum sep tagoj fosis kaj &scirc;ovelis seninterrompe, sed post sep
        tagoj li vidis, ke la <tld/>o estas nula
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        l' afero dependas de l' <tld/>oj de la vo&ccirc;dono
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni rigardu, &ccirc;u la Kongreso donis iajn <tld/>ojn
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la ferma ceremonio de la Sesa Kongreso Esperantista en Washington en la 20a de a&ubreve;gusto 1910 (a)</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> publikigis la <tld/>ojn de siaj esploroj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rezult.0o">rezulto</ref>
    <refgrp tip="vid">
      <ref cel="ating.0ajxo">atinga&jcirc;o</ref>,
      <ref cel="bilanc.0o">bilanco</ref>,
      <ref cel="efik.0o">efiko</ref>,
      <ref cel="frukt.0o">frukto</ref>,
      <ref cel="produk.0o">produkto</ref>,
      <ref cel="rikolt.0o">rikolto</ref>
    </refgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Anka&ubreve; eblas diri <ke>rezulto</ke>.
  </rim>

  <trd lng="be">&c_v;&c_y;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>rezult&aacute;t</trd>,
    <trd>v&yacute;sledek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ergebnis</trd>,
    <trd>Resultat</trd>
  </trdgrp>
  <trd lng="en">result</trd>
  <trd lng="fa">&a_n;&a_t;&f_y;&a_j;&a_h;</trd>
  <trd lng="fr">r&eacute;sultat</trd>
  <trdgrp lng="hu">
    <trd>eredm&eacute;ny</trd>,
    <trd>k&ouml;vetkezm&eacute;ny</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>resultaat</trd>,
    <trd>uitkomst</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wynik</trd>,
    <trd>rezultat</trd>
  </trdgrp>
  <trd lng="pt">resultado</trd>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>rezult&aacute;t</trd>,
    <trd>v&yacute;sledok</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x4EA7;&#x7269; <pr>ch&#x1CE;nw&ugrave;</pr></trd>,
    <trd>&#x7522;&#x7269; <pr>ch&#x1CE;nw&ugrave;</pr></trd>,
    <trd>&#x679C; <pr>gu&#x1D2;</pr></trd>
  </trdgrp>
</drv>

