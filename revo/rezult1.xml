<drv mrk="rezult1.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      <ref cel="util.ne0asen0a">Senutila</ref>, <ref cel="van.0a">vana</ref>:
      <ekz>
        sen<tld/>a peno;
      </ekz>
      <ekz>
        sen<tld/>aj iroj al la informa oficejo
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vant.0a">vanta</ref>,
      <ref cel="flu.super0a">superflua</ref>,
      <ref cel="cel.sen0a">sencela</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_z;&c_v;&c_y;&c_n;&c_ib;&c_k;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&f_y;&zwnj;&a_n;&a_t;&f_y;&a_j;&a_h;</trd>,
    <trd>&a_b;&f_y;&zwnj;&a_th;&a_m;&a_r;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>eredm&eacute;nytelen</trd>,
    <trd>medd&odblac;</trd>,
    <trd>hi&aacute;baval&oacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bezskuteczny</trd>,
    <trd>bez  <ind>wyniku</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_r;&c_je;&c_z;&c_u;&c_l;&c_mol;&c_t;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
