<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rezultat</rad>/o
</kap>
<drv mrk="rezult1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fina sekvo de ia agado:
      <ekz>
        doni, alporti bonan <tld/>on;
      </ekz>
      <ekz>
        akiri, ricevi, trafi <tld/>on;
      </ekz>
      <ekz>
        brila, riĉa <tld/>o;
      </ekz>
      <ekz>
        sed ni ne povis atingi ian <tld/>on kaj ni disiris
        <fnt><bib>F</bib>, <lok>Ekzercaro, ¶ 42</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="rezult.0o">rezulto</ref>
    <refgrp tip="vid">
      <ref cel="frukt.0o">frukto</ref>,
      <ref cel="produk">produkto</ref>,
      <ref cel="rezult.0o">rezulto</ref>,
      <ref cel="efik.0o">efiko</ref>,
      <ref cel="ating">atingaĵo</ref>,
      <ref cel="bilanc.0o">bilanco</ref>,
      <ref cel="rikolt">rikolto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">вынік</trd>
  <trdgrp lng="de">
    <trd>Ergebnis</trd>,
    <trd>Resultat</trd>
  </trdgrp>
  <trd lng="en">result</trd>
  <trdgrp lng="hu">
    <trd>eredmény</trd>,
    <trd>következmény</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>resultaat</trd>,
    <trd>uitkomst</trd>
  </trdgrp>
  <trd lng="pt">resultado</trd>
  <trd lng="ru">результат</trd>
</drv>

<drv mrk="rezult1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Sekvi <tld/>e el io.
    </dif>
  </snc>
  <trd lng="be">вынікаць</trd>
  <trd lng="en">result</trd>
  <trdgrp lng="hu">
    <trd>adódik</trd>,
    <trd>kijön <klr tip="ind">(eredményként)</klr></trd>,
    <trd>következik</trd>,
    <trd>származik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>получаться</trd>,
    <trd>получиться</trd>,
    <trd>следовать</trd>,
    <trd>являться <ind>результатом</ind></trd>
  </trdgrp>
</drv>

<drv mrk="rezult1.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Sekvigi kiel <tld/>on
    </dif>
    <refgrp tip="vid">
      <ref cel="kauxz.0i">kaŭzi</ref>,
      <ref cel="vek.0i">veki</ref>,
      <ref cel="instig.0i">instigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>спрычыніцца</trd>,
    <trd>вызваць</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>eredményez</trd>,
    <trd>okoz</trd>,
    <trd>hoz <klr tip="ind">(eredményül)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>вызвать</trd>,
    <trd>привести <klr>(к результату, следствию)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="rezult1.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Senutila, vana:
      <ekz>
        sen<tld/>a peno
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vant.0a">vanta</ref>,
      <ref cel="flu.super0a">superflua</ref>,
      <ref cel="cel.sen0a">sencela</ref>.
    </refgrp>
  </snc>
  <trd lng="be">безвыніковы</trd>
  <trdgrp lng="hu">
    <trd>eredménytelen</trd>,
    <trd>meddő</trd>,
    <trd>hiábavaló</trd>
  </trdgrp>
  <trd lng="ru">безрезультатный</trd>
</drv>
</art>
