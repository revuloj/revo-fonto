<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glutamat.xml,v 1.13 2023/11/11 11:36:35 revo Exp $">
<kap>
  <rad>glutamat</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="glutamat.0o">
  <kap><tld/>o</kap>
  <snc mrk="glutamat.0o.KUI">
    <uzo tip="fak">FIZL</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Salo de glutaminacido funkcianta kiel
      <ref tip="super" cel="trans.nervo0igilo.FIZL">nervotransigilo</ref>,
      anka&ubreve; uzata en kuirarto kiel gustofortigilo:
      <ekz>
        vaporumita ovokrema&jcirc;o: Kirli du ovojn en bovlo, aldoni iom da  
        salo, <tld/>o kaj porkograso  
        <fnt>
          <aut>Xiang Hong</aut>,
          <vrk><url ref="http://www.china.org.cn/EL/EL/ElPopolaCxinio/2000-02/bd2000-2-11.htm">Iom
          pri &ccirc;inaj pladoj</url></vrk>,
          <lok>El Popola &Ccirc;inio, 2000-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="glutamat.0o.acido">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">BIO</uzo>
    <dif>
      Glutaminacido.
    </dif>
  </snc>
  <trd lng="ca">glutamat</trd>
  <trd lng="cs">glutam&aacute;t</trd>
  <trdgrp lng="de">
    <trd>Glutamins&auml;ure</trd>,
    <trd>Glutamat</trd>
  </trdgrp>
  <trd lng="fa">&f_g;&a_l;&a_w;&a_t;&a_A;&a_m;&a_A;&a_t;</trd>
  <trd lng="fr">glutamate</trd>
  <trd lng="id">glutamat</trd>
  <trd lng="pt">glutamato</trd>
  <trd lng="sk">glutam&aacute;t</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x43B;&#x443;&#x442;&#x430;&#x43C;&#x456;&#x43D;&#x430;&#x442;</trd>,
    <trd>&#x441;&#x456;&#x43B;&#x44C; &#x433;&#x43B;&#x443;&#x442;&#x430;&#x43C;&#x456;&#x43D;&#x43E;&#x432;&#x43E;&#x457; &#x43A;&#x438;&#x441;&#x43B;&#x43E;&#x442;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7A40;&#x6C28;&#x9178; <pr>g&#x1D4;&#x101;nsu&#x101;n</pr></trd>,
    <trd>&#x7A40;&#x80FA;&#x9178; <pr>g&#x1D4;&#x101;nsu&#x101;n</pr></trd>,
    <trd>&#x5473;&#x7CBE; <pr>w&#xE8;ij&#x12B;ng</pr></trd>
  </trdgrp>
</drv>
</art>

