<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glutamat.xml,v 1.15 2024/08/06 15:29:38 revo Exp $">
<kap>
  <rad>glutamat</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="glutamat.0o">
  <kap><tld/>o</kap>
  <snc mrk="glutamat.0o.KUI">
    <uzo tip="fak">FIZL</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Salo de glutaminacido funkcianta kiel
      <ref tip="super" cel="trans.nervo0igilo.FIZL">nervotransigilo</ref>,
      anka&ubreve; uzata en kuirarto kiel gustofortigilo:
      <ekz>
        vaporumita ovokrema&jcirc;o: Kirli du ovojn en bovlo, aldoni iom da
        salo, <tld/>o kaj porkograso
        <fnt>
          <aut>Xiang Hong</aut>,
          <vrk><url ref="http://www.china.org.cn/EL/EL/ElPopolaCxinio/2000-02/bd2000-2-11.htm">Iom
          pri &ccirc;inaj pladoj</url></vrk>,
          <lok>El Popola &Ccirc;inio, 2000-02</lok>
        </fnt>.
      </ekz>
    </dif>
     <trd lng="be">&c_g;&c_l;&c_ju;&c_t;&c_a;&c_m;&c_a;&c_t; <klr>(&c_a;&c_s;&c_a;&c_b;&c_l;&c_ib;&c_v;&c_a; &c_n;&c_a;&c_t;&c_r;&c_y;&c_ju;)</klr></trd>
     <trd lng="pl">glutaminian sodu </trd>
  </snc>
  <snc mrk="glutamat.0o.acido">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">BIO</uzo>
    <dif>
      Glutaminacido.
    </dif>
       <trd lng="be">&c_g;&c_l;&c_ju;&c_t;&c_a;&c_m;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_ja; <ind>&c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</ind></trd>
       <trd lng="pl">kwas<ind> glutaminowy</ind></trd>
  </snc>
  <trd lng="ca">glutamat</trd>
  <trd lng="cs">glutam&aacute;t</trd>
  <trdgrp lng="de">
    <trd>Glutamins&auml;ure</trd>,
    <trd>Glutamat</trd>
  </trdgrp>
  <trd lng="fa">&f_g;&a_l;&a_w;&a_t;&a_A;&a_m;&a_A;&a_t;</trd>
  <trd lng="fr">glutamate</trd>
  <trd lng="id">glutamat</trd>
  <trd lng="pt">glutamato</trd>
  <trd lng="sk">glutam&aacute;t</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43B;&c_u;&c_t;&c_a;&#x43C;&c_ib;&#x43D;&c_a;&c_t;</trd>,
    <trd>&c_s;&c_ib;&#x43B;&#x44C; &c_g;&#x43B;&c_u;&c_t;&c_a;&#x43C;&c_ib;&#x43D;&#x43E;&c_v;&#x43E;&c_ji; &#x43A;&c_i;&c_s;&#x43B;&#x43E;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7A40;&#x6C28;&#37240; <pr>g&#x1D4;&amacron;nsu&amacron;n</pr></trd>,
    <trd>&#x7A40;&#x80FA;&#37240; <pr>g&#x1D4;&amacron;nsu&amacron;n</pr></trd>,
    <trd>&#21619;&#x7CBE; <pr>w&#xE8;ij&#x12B;ng</pr></trd>
  </trdgrp>
</drv>
</art>

