<drv mrk="funkci.hevisida0o">
  <kap>hevisida <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Reela <ref tip="super" cel="funkci.0o.MAT"><tld/>o</ref>, kiuj
      &jcirc;etas strikte negativan argumenton al <frm>0</frm> kaj 
      pozitivan al <frm>1</frm>:
      <ekz>
        la hevisidan <tld/>on oni foje signas per <frm>H(<k>x</k>)</frm> 
        a&ubreve; <frm>Y(<k>x</k>)</frm> kaj
        foje nomas anka&ubreve; <ctl>unuo-&scirc;tupo</ctl>.
      </ekz>
    </dif>
    <ref tip="vid" cel="hevisi.0o">Hevisido.</ref>
  </snc>
  <trd lng="de">Heavisidesche <ind>Funktion</ind></trd>
  <trd lng="en">Heaviside['s] <ind>function</ind></trd>
  <trdgrp lng="fr">
    <trd>fonction de Heaviside</trd>,
    <trd>&eacute;chelon unit&eacute;</trd>
  </trdgrp>
  <trd lng="nl">functie van Heaviside</trd>
  <trd lng="pl">funkcja Heaviside'a</trd>
  <trd lng="ru">&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;
  &c_H;&c_je;&c_v;&c_i;&c_s;&c_a;&c_j;&c_d;&c_a;</trd>
</drv>

