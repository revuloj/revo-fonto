    <trd>&a_m;&a_r;&a_d;&a_m;&zwnj;&a_f;&a_r;&f_y;&a_b;</trd>,
    <trd>&a_ayn;&a_w;&a_A;&a_m;&zwnj;&a_f;&a_r;&f_y;&a_b;</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;magogue</trd>
  <trd lng="hu">demag&oacute;g</trd>
  <trd lng="pl">demagog</trd>
  <trd lng="pt">demagogo</trd>
  <trd lng="ru">&c_d;&c_je;&c_m;&c_a;&c_g;&c_o;&c_g;</trd>
  <trd lng="sk">demag&oacute;g</trd>
