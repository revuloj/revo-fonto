<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Lati</rad>/o <fnt>PIV2</fnt>
    </kap>

    <drv mrk="lati.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc mrk="lati.0o.HIS">
        <uzo tip="fak">HIS</uzo>
        <dif>
          Regiono en la okcidento de la meza Italujo, &ccirc;irka&ubreve; Tibero kaj
          <ref tip="prt" cel="rom.0o">Romo</ref>,
          la centro de la frua historio de
          <ref tip="malprt" cel="romi.0o">Romio</ref>:
          <ekz>
            <ref cel="latin.0o">Latino</ref> estis lingvo parolata
            en <tld/>o, kaj kiu disvasti&gcirc;is en la okcidenta parto
            de Romio;
          </ekz><ekz>
            tiu speco de demokratio <klr>(la Romia) ...</klr> tute
            sendube sukcesintus, ke la grandbienuloj de <tld/>o
            &scirc;an&gcirc;us sian opinion, kaj sekve fari&gcirc;us la plej fervoraj
            demokratoj
            <fnt>
              <vrk>Kio restas de la demokratio?</vrk>.
              <lok><url ref="http://eo.mondediplo.com/article926.html"
                  >&leftquot;Le Monde diplomatique&rightquot; en Esperanto</url></lok>
            </fnt>.
          </ekz>
        </dif>
        <trdgrp lng="ru">
          <trd>&c_L;&c_a;&c_c;&c_i;&c_j;</trd>,
          <trd>&c_L;&c_a;&c_c;&c_i;&c_u;&c_m;</trd>
        </trdgrp>
      </snc>
      <snc mrk="lati.0o.POL">
        <uzo tip="fak">POL</uzo>
        <dif>
          Administra regiono en la moderna Italujo:
          <ekz>
            la regionoj koncernataj de la nova perturbi&gcirc;o
            <klr>(vetera)</klr> estas <tld/>o, Kampanio,
            Kalabrio kaj Bazilikato
            <fnt>
              <lok><url ref="http://radioarkivo.org/vidu.php/1893"
                  >RAI INTERNATIONAL - trasmissione in lingua
                  esperanto di sabato 5.3.2005</url></lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="ru">&c_L;&c_a;&c_c;&c_i;&c_o;</trd>
      </snc>
      <trd lng="la">Latium</trd>
    </drv>

    <drv mrk="lati.0a">
      <kap><tld lit="l"/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>o:
          <ekz>
            <uzo tip="fak">HIS</uzo>
            <ind><tld lit="l"/>a juro</ind>;
            <trdgrp lng="la">
              <trd>jus Latii</trd>, <trd>jus sine suffragio</trd>
            </trdgrp>
            <trd lng="ru">&c_l;&c_a;&c_t;&c_i;&c_n;&c_s;&c_k;&c_o;&c_je;
              &c_p;&c_r;&c_a;&c_v;&c_o;</trd>
          </ekz><ekz>
            <uzo tip="fak">HIS</uzo>
            <ind><tld/>a ligo</ind>.
            <trd lng="en">Latin Ligue</trd>
            <trd lng="fr">Ligue latine</trd>
            <trd lng="ru">&c_L;&c_a;&c_t;&c_i;&c_n;&c_s;&c_k;&c_i;&c_j;
              &c_s;&c_o;&c_ju;&c_z;</trd>
          </ekz>
        </dif>
      </snc>
      <trd lng="ru">&c_l;&c_a;&c_t;&c_i;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>

    <drv mrk="lati.0ano">
      <kap><tld lit="l"/>ano</kap>
      <snc>
        <dif>
          Enlo&gcirc;anto de <tld/>o.
        </dif>
      </snc>
      <trd lng="la">Latinus</trd>
      <trdgrp lng="ru">
        <trd>&c_l;&c_a;&c_t;&c_i;&c_n;</trd>,
        <trd>&c_l;&c_a;&c_t;&c_i;&c_n;&c_ja;&c_n;&c_i;&c_n;</trd>
      </trdgrp>
    </drv>
  </art>


