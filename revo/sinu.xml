<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sinu</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sinu.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;iu el la kurboj de serpentuma vojo, rivero, onduma streko, kaj
      similaj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>m&eacute;andre</trd>,
    <trd>sinuosit&eacute;</trd>
  </trdgrp>
</drv>

<drv mrk="sinu.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="serpen.0a">serpenta</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>ondoyant</trd>,
    <trd>sinueux</trd>,
    <trd>tortueux</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slingerend</trd>,
    <trd>kronkelend</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_m;&c_je;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
</drv>

<drv mrk="sinu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <ref tip="dif" cel="serpen.0i">serpenti</ref>
    <ekz>
      kreskis &ccirc;irka&ubreve;e fragmitoj, tra kiuj <tld/>is pado
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 49a, p. 123a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>serpenter</trd>,
    <trd>sinuer</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
