<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sinu.xml,v 1.13 2019/10/19 16:10:27 revo Exp $">
<kap>
  <rad>sinu</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sinu.0o">
  <kap><tld/>o</kap>
  <snc mrk="sinu.0o.FORM">
    <dif>
      &Ccirc;iu el la <ref tip="super" cel="kurb.0o.KOMUNE">kurbo</ref>j 
      de serpentuma vojo, rivero, onduma streko ks:
      <ekz>
        la vojo el Ta&hcirc;enbako al <nom>Gerton</nom> <klr>[&#8230;]</klr> turni&gcirc;as &ccirc;e la maro per kruta
        <tld/>o kaj eliras sur ebena&jcirc;on
        <fnt>
          <aut>Aleksandro Grin</aut>:
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/vojo_2_01.html"
          >La vojo nenien</url></vrk>
          <lok>Parto 2. &Ccirc;apitro 1</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="meandr.0o.GEOG">meandro</ref>,
      <ref cel="serpen.0umo.forma">serpentumo</ref>,
      <ref cel="zigzag.0o.FORM">zigzago</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_g;&c_ib;&c_b;</trd>,
    <trd>&c_v;&c_y;&c_g;&c_ib;&c_n;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ib;&c_l;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_g;&c_ib;&c_b;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_l;&c_u;&c_k;&c_a;&c_v;&c_ib;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>sinuositat</trd>,
    <trd>ondulaci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>klikatina</trd>,
    <trd>zat&aacute;&ccaron;ka</trd>,
    <trd>z&aacute;hyb</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Biegung</trd>,
     <trd>Kr&uuml;mmung</trd>,
     <trd>Windung</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>seno</trd>,
     <trd>ondulaci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>m&eacute;andre</trd>,
    <trd>sinuosit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kanyar</trd>,
    <trd>fordul&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_g;&c_i;&c_b;</trd>,
    <trd>&c_i;&c_z;&c_l;&c_u;&c_ch;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_i;&c_l;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>krivolakos&tcaron;</trd>,
    <trd>z&aacute;hyb</trd>,
    <trd>z&aacute;kruta</trd>
  </trdgrp>
</drv>

<drv mrk="sinu.0a">
  <kap><tld/>a</kap>
  <snc mrk="sinu.0a.FORM">
    <dif>
      <ref tip="dif" cel="serpen.0a.forma">Serpenta</ref>:
      <ekz>
        la&ubreve; <tld/>aj vojoj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Maljunulo de la Migranta Monto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> trovi&gcirc;is en mallar&gcirc;a kaverno, kiu kiel <tld/>a tubo kondukis
        tra la rokmasiva&jcirc;o
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Igramul', la Multulo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_v;&c_ib;&c_l;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_r;&c_u;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_r;&c_u;&c_ch;&c_a;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>sinu&oacute;s</trd>,
    <trd>ondulant</trd>
  </trdgrp>
  <trd lng="cs">k&rcaron;ivolak&yacute;</trd>
  <trdgrp lng="de">
    <trd>geschl&auml;ngelt</trd>,
    <trd>gekr&uuml;mmt</trd>,
    <trd>gewunden</trd>,
    <trd>gebogen</trd>
  </trdgrp>
  <trd lng="es">sinuoso</trd>
  <trdgrp lng="fr">
    <trd>ondoyant</trd>,
    <trd>sinueux</trd>,
    <trd>tortueux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kanyarg&oacute;s</trd>,
    <trd>k&iacute;gy&oacute;z&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slingerend</trd>,
    <trd>kronkelend</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_v;&c_i;&c_l;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_m;&c_je;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">krivolak&yacute;</trd>
</drv>

<drv mrk="sinu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sinu.0i.FORM">
    <dif>
      <ref tip="dif" cel="serpen.0umi.forme">Serpentumi</ref>:
      <ekz>
        kreskis &ccirc;irka&ubreve;e fragmitoj, tra kiuj <tld/>is pado
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 49a, p. 123a</lok></fnt>.
      </ekz>
      <ekz>
        vasta, iom onda grespejza&gcirc;o, tra kiu <tld/>is rivero
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Kunvoja&gcirc;antoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; kiam ni promenis la&ubreve; la trotuaro, &scirc;i pa&scirc;is <tld/>e por surtreti kiel eble
        plej multajn
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <lok><url ref="http://steloj.de/esperanto/monato2/006918.html">http://steloj.de/esperanto/monato2/006918.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_g;&c_ib;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">serpentejar</trd>
  <trdgrp lng="cs">
    <trd>klikatit se</trd>,
    <trd>vlnit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>sich </klr>winden</trd>,
    <trd><klr>sich </klr>schl&auml;ngeln</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>serpenter</trd>,
    <trd>sinuer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kanyarog</trd>,
    <trd>k&iacute;gy&oacute;zik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_t;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_m;&c_je;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>k&lcaron;ukati&tcaron; sa</trd>,
    <trd>vlni&tcaron; sa</trd>
  </trdgrp>
</drv>
</art>

