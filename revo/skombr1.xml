<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skombr1.xml,v 1.12 2022/03/27 17:06:35 revo Exp $">
<kap>
  <rad>skombred</rad>/oj
</kap>
<drv mrk="skombr1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="perk.0oformaj">perkoformaj
      fi&scirc;oj</ref> (<trd lng="la">Scombridae</trd>):
      <ekz>
        <tld/>oj estas fi&scirc;oj <klr>[kiel]</klr> <ref tip="prt" cel="skombr.0o">skombroj</ref>,
        <ref tip="prt" cel="tinus.0o">tinusoj</ref>,
        kaj veraj bonitoj <klr>[kaj]</klr> inkludas multajn el la plej gravaj
        kaj familiaraj man&gcirc;ofi&scirc;oj, <klr>[ili]</klr> havas du dorsajn na&gcirc;ilojn,
        kaj serion de finaj na&gcirc;iletoj malanta&ubreve; la fina dorsa na&gcirc;ilo kaj anusan na&gcirc;ilon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=422919">Skombredoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_u;&c_m;&c_b;&c_r;&c_y;&c_je;&c_v;&c_y;&c_ja;
<klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="cs">makrely <klr>(rod)</klr></trd>
  <trd lng="de">Makrelen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="hu">makr&eacute;laf&eacute;l&eacute;k</trd>
  <trd lng="nl">makreelachtigen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">makrelowate</trd>
  <trd lng="ru">&c_s;&c_k;&c_u;&c_m;&c_b;&c_r;&c_i;&c_je;&c_v;&c_y;&c_je;
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">makrely</trd>
  <trd lng="uk">&#x441;&#x43A;&#x443;&#x43C;&#x431;&#x440;&#x456;&#x454;&#x432;&#x456;</trd>
</drv>
</art>
