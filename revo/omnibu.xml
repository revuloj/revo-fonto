<art mrk="$Id: omnibu.xml,v 1.27 2024/01/30 16:09:26 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>omnibus</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="omnibu.0o">
  <kap><tld/>o</kap>
  <snc mrk="omnibu.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Publika surstrata veturilo portanta homojn de
      haltejo al haltejo;
      precipe tia &ccirc;evaltirata veturilo kontraste
      al la posta motora
      <ref tip="vid" cel="auxtob1.0o.AUT">a&ubreve;tobuso</ref>,
      a&ubreve; <ref tip="vid" cel="bus.0o.AUT">buso</ref>:
      <ekz>
        grandega tramvoja <tld/>o, tirata de kvar grandaj, fortaj &ccirc;evaloj, plena de homoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estis ko&ccirc;ero de funebra veturilo <klr>[&#x2026;]</klr>, la <tld/>o de la morto
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o komenci&gcirc;is mia propra persona konateco kun li
        <fnt>
          <aut>J. K. Jerome, trad. R. Williams</aut>,
          <vrk><url ref="&Sezonoj;2013/10/jerome">
          La viro kiu volis direkti</url></vrk>,
          <lok>La Ondo de Esperanto, 2013:10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_o;&c_m;&c_n;&c_ib;&c_b;&c_u;&c_s;</trd>
  <trd lng="bg">&c_a;&c_v;&c_t;&c_o;&c_b;&c_u;&c_s;</trd>
  <trd lng="ca">&ograve;mnibus</trd>
  <trdgrp lng="cs">
    <trd>m&ecaron;stsk&yacute; dostavn&iacute;k</trd>,
    <trd>omnibus</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Omnibus</trd>,
    <trd>Bus</trd>
  </trdgrp>
  <trd lng="el">&lambda;&epsilon;&omega;&phi;&omikron;&rho;&epsilon;&jota_ton;&omikron;</trd>
  <trdgrp lng="en">
    <trd>omnibus</trd>,
    <trd>horsecar</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_t;&a_w;&a_b;&a_w;&a_s;</trd>
  <trd lng="fr">omnibus</trd>
  <trdgrp lng="hu">
    <trd>omnibusz</trd>,
    <trd>busz</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E57;&#x5408;&#x99AC;&#x8ECA;  <pr>&#x306E;&#x308A;&#x3042;&#x3044;&#x3070;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x30AA;&#x30E0;&#x30CB;&#x30D0;&#x30B9;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>omnibus</trd>,
    <trd>bus</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>omibus<klr> (przest.)</klr></trd>,
    <trd>tramwaj konny</trd>,
    <trd>autobus</trd>
  </trdgrp>
  <trd lng="pt">&ocirc;nibus</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_m;&c_n;&c_i;&c_b;&c_u;&c_s;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_k;&c_a;</trd>
    <trd>&c_a;&c_v;&c_t;&c_o;&c_b;&c_u;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">omnibus</trd>
  <trd lng="sv">buss</trd>
  <trd lng="uk">&#x43E;&#x43C;&#x43D;&#x456;&#x431;&#x443;&#x441;</trd>
  <trdgrp lng="zh">
    <trd>&#x5BA2;&#x8ECA; <pr>k&#xE8;ch&#x113;</pr></trd>,
    <trd>&#x5BA2;&#x8F66; <pr>k&#xE8;ch&#x113;</pr></trd>
  </trdgrp>
</drv>
