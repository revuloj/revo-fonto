<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kloroplast.xml,v 1.7 2022/06/17 11:27:01 revo Exp $">
<kap>
  <rad>kloroplast</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kloroplast.0o">
  <kap><tld/>o</kap>
  <snc mrk="kloroplast.0o.FIZL">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Iu el la <ref tip="super" cel="organ.0eto.FIZL">organetoj</ref>
      troveblaj en vegeta&jcirc;aj &ccirc;eloj, kiuj plenumas
      <ref tip="vid" cel="fotosintez.0o.BOT">fotosintezon</ref>:
      <ekz>
        iuj kredas ke la prapatro de la <tld/>oj estis parenco de
        Prokloro
        <fnt>
          <aut>J. Badger</aut>,
          <vrk><url ref="http://groups.google.be/group/soc.culture.esperanto/msg/3a164e3a487ee13c">
          biologia klasifiko</url></vrk>,
          <lok>soc.culture.esperanto, 1999-12-04</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ubreve;kariotoj de la Planta Regno kapablas fotosintezi, pro
        verdaj 
        organetoj kiuj nomi&gcirc;as <tld/>oj
        <fnt>
          <aut>K. Lendon</aut>,
          <vrk>Proterozoika Erao</vrk>,
          <lok><url ref="http://groups.yahoo.com/group/aeko/message/845">
          aeko</url>, 2002-06-20</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="klorof.0o">klorofilo</ref>
    <trd lng="be">&c_h;&c_l;&c_jo;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_s;&c_t;</trd>
    <trd lng="ca">cloroplast</trd>
    <trd lng="de">Chloroplast</trd>
    <trd lng="en">chloroplast</trd>
    <trd lng="fr">chloroplaste</trd>
    <trd lng="pl">chloroplast</trd>
  </snc>
  <trd lng="uk">&c_h;&#x43B;&#x43E;&c_r;&#x43E;&#x43F;&#x43B;&c_a;&c_s;&c_t;</trd>
</drv>
</art>

