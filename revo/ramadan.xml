<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ramadan.xml,v 1.17 2024/04/13 20:15:51 revo Exp $">
<kap>
  <rad>ramadan</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ramadan.0o">
  <kap><tld/>o</kap>
  <snc mrk="ramadan.0o.KAL">
    <uzo tip="fak">ISL</uzo>
    <uzo tip="fak">KAL</uzo>
    <dif>
      La na&ubreve;a <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj" val="9">monato</ref> de la
      <ref tip="vid" cel="islam.0o">islama</ref>
      jaro, dum kies &ccirc;iuj taghoroj la
      islamanoj strikte fastas:
      <ekz>
        ni havas la sanktan monaton, kiu nomi&gcirc;as <tld/>o
        <fnt>
          <aut>M'hammed</aut>,
          <vrk>Ekster Esperantio mi spertas rasismon</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fast.0o">fasto</ref>,
      <ref cel="karesm.0o">Karesmo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_m;&c_a;&c_d;&c_a;&c_n;</trd>,
    <trd>&c_r;&c_a;&c_m;&c_a;&c_z;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">ramad&agrave;</trd>
  <trdgrp lng="cs">
    <trd>ramad&#xE1;n</trd>,
    <trd>ramaz&#xE1;n</trd>
  </trdgrp>
  <trd lng="en">Ramadan</trd>
  <trd lng="es">ramad&aacute;n</trd>
  <trd lng="fr">ramadan</trd>
  <trd lng="hu">ramad&aacute;n</trd>
  <trd lng="id">Ramadan</trd>
  <trd lng="ja">&#x30E9;&#x30DE;&#x30C0;&#x30F3;</trd>
  <trd lng="nl">ramadan</trd>
  <trd lng="pl">ramadan</trd>
  <trd lng="pt">ramad&atilde;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_m;&c_a;&c_d;&c_a;&c_n;</trd>,
    <trd>&c_r;&c_a;&c_m;&c_a;&c_z;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ramad&#xE1;n</trd>,
    <trd>ramaz&#xE1;n</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x658B;&#x6212;&#x8282; <pr>Zh&#x101;iji&#xE8;ji&#xE9;</pr></trd>,
    <trd>&#x9F4B;&#x6212;&#x7BC0; <pr>Zh&#x101;iji&#xE8;ji&#xE9;</pr></trd>,
    <trd>&#x62C9;&#x99AC;&#x4E39; <pr>L&#x101;m&#x1CE;d&#x101;n</pr></trd>,
    <trd>&#x62C9;&#x9A6C;&#x4E39; <pr>L&#x101;m&#x1CE;d&#x101;n</pr></trd>,
    <trd>&#x83B1;&#x9EA6;&#x4E39; <pr>L&#xE1;im&#xE0;id&#x101;n</pr></trd>,
    <trd>&#x840A;&#x9EA5;&#x4E39; <pr>L&#xE1;im&#xE0;id&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>
