<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>graci</rad>/a
</kap>
<drv mrk="graci.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="super" cel="bel.0a">Bela</ref>, plaĉa per facilaj
      kaj harmoniaj movoj aŭ bele proporciaj formoj:
      <ekz>
        <tld/>a virino, vazo, stilo;
      </ekz>
      <ekz>
        <tld/>aj lipoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="delika.0a">delikata</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>грацыёзны</trd>,
    <trd>зграбны</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>graziös</trd>,
    <trd>anmutig</trd>,
    <trd>zierlich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>graceful</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gracieux</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kecses</trd>,
    <trd>bájos</trd>,
    <trd>könnyed mozgású</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gracieus</trd>,
    <trd>bevallig</trd>,
    <trd>bekoorlijk</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pełny <ind>gracji</ind></trd>,
    <trd>zgrabny</trd>,
    <trd>powabny</trd>,
    <trd>przystojny</trd>,
    <trd>nadobny</trd>,
    <trd>lekki</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>gracioso</trd>,
   <trd>esbelto</trd>
  </trdgrp>
  <trd lng="es">gracioso, -a<klr> (bello, hermoso)</klr></trd>
  <trdgrp lng="ru">
    <trd>грациозный</trd>,
    <trd>изящный</trd>
  </trdgrp>
</drv>

<drv mrk="graci.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Laŭ <tld/>a maniero:
      <ekz>
        <tld/>e prezenti tason al vizitanto;
      </ekz>
      <ekz>
        li saltas <tld/>e, kiel urso ebria
        <klr>(ironie)</klr><fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>грацыёзна</trd>,
    <trd>зграбна</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>graziös</trd>,
    <trd>anmutig</trd>,
    <trd>zierlich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gracefully</trd>
  </trdgrp>
  <trd lng="fr">gracieusement<klr tip="ind"> (avec grâce)</klr>
  </trd>
  <trdgrp lng="hu">
    <trd>kecsesen</trd>,
    <trd>bájosan</trd>,
    <trd>könnyedén</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sierlijk</trd>,
    <trd>met <ind>gratie</ind></trd>,
    <trd>op <ind>hoffelijke</ind> wijze</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>z <ind>gracją</ind></trd>,
    <trd>zgrabnie</trd>,
    <trd>powabnie</trd>,
    <trd>przystojnie</trd>,
    <trd>nadobnie</trd>,
    <trd>lekko</trd>
  </trdgrp>
  <trd lng="pt">graciosamente</trd>
  <trdgrp lng="es">
    <trd>graciosamente</trd>,
    <trd>con gracia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>грациозно</trd>,
    <trd>изящно</trd>
  </trdgrp>
</drv>

<drv mrk="graci.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Kvalito de iu aŭ io <tld/>a:
      <ekz>
        <tld/>eco de infana vizaĝo, de antikva statufragmento.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>
  грацыёзнасьць
    </trd>,
    <trd>
    зграбнасьць</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Grazie</trd>,
    <trd>Anmut</trd>,
    <trd>Liebreiz</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gracefulness</trd>
  </trdgrp>
  <trd lng="fr">grâce<klr tip="ind"> (charme)</klr></trd>
  <trdgrp lng="hu">
    <trd>kecsesség</trd>,
    <trd>bájosság</trd>,
    <trd>könnyedség</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gratie</trd>,
    <trd>bevalligheid</trd>
  </trdgrp>
  <trd lng="pt">graciosidade</trd>
<trd lng="es">gracia<klr> (elegancia)</klr></trd>
  <trdgrp lng="pl">
    <trd>gracja</trd>,
    <trd>zgrabność</trd>,
    <trd>powab</trd>,
    <trd>przystojność</trd>,
    <trd>nadobność</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>грация</trd>,
    <trd>грациозность
    </trd>,
    <trd>изящество</trd>
  </trdgrp>
</drv>

<drv mrk="graci.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Havanta senharmoniajn movojn aŭ formojn.
    </dif>
    <ref tip="vid" cel="plump.0a">plumpa</ref>
  </snc>
  <trdgrp lng="be">
    <trd>нязграбны</trd>,
    <trd>нехлямяжы</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>plump</trd>,
    <trd>derb</trd>,
    <trd>schwerfällig</trd>,
    <trd>ungeschlacht</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>awkward</trd>,
    <trd>clumsy</trd>
  </trdgrp>
  <trd lng="fr">disgracieux</trd>
  <trdgrp lng="hu">
    <trd>mackós</trd>,
    <trd>otromba</trd>,
    <trd>nehézkes</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>onbekoorlijk</trd>,
    <trd>onbevallig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ociężały</trd>,
    <trd>niezgrabny</trd>
  </trdgrp>
  <trd lng="pt">desgracioso</trd>
  <trdgrp lng="es">
    <trd>torpe</trd>,
    <trd>feo</trd>
  </trdgrp>
  <trd lng="ru">неуклюжий</trd>
</drv>

<drv mrk="graci.G0o">
  <kap><tld lit="G"/>o</kap>
  <uzo tip="fak">MIT</uzo>
  <snc>
    <dif>
      Ĉiu el la tri antikvaj diaĵoj, reprezentantaj la plej
      gracian belecon.
    </dif>
    <trd lng="be">грацыя</trd>
    <trd lng="en">Grace</trd>
    <trd lng="fr">Grâce<klr> (les 3 ~s)</klr></trd>
    <trd lng="hu">Grácia</trd>
    <trd lng="nl">gratie</trd>
    <trd lng="pl">Gracja</trd>
    <trd lng="ru">грация</trd>
  </snc>
</drv>
</art>
