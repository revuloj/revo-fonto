<drv mrk="graci.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="super" cel="bel.0a">Bela</ref>, <ref tip="super" cel="placx.0a">pla&ccirc;a</ref> 
      per facilaj kaj harmoniaj movoj a&ubreve; bele proporciaj formoj:
      <ekz>
        <nom>Jozef</nom> estis <tld/>a kaj bela
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 39:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i havis la plej <tld/>ajn malgrandajn blankajn piedetojn, kiujn bela knabino
        nur povas havi
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <klr>[&#x2026;]</klr> admiris &scirc;ian <tld/>an flugetantan iradon
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda buldogo <klr>[&#x2026;]</klr> kaptis la re&gcirc;inon je &scirc;ia <tld/>a korpo tiel, ke oni
        klare a&ubreve;dis sonon de rompi&gcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu vesperon venis Aftanido, <tld/>a kiel kano, forta kaj bruna
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen staris <tld/>a, belega rozarbeta&jcirc;o, sed malbona mano rompis la trunkon
        <fnt>
          <bib>Fab2</bib>
          <lok>An&gcirc;elo</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia eleganta kaj <tld/>a silueto, lia profilo kun delikataj brovoj kaj kun
        maldikaj lipoj ombritaj de ore blondaj lipharoj mirinde kontrastis la bluan
        &ccirc;ambron
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        knabino kun la <tld/>a kaj elasta talio, kun la fizionomio plena de libereco kaj
        inteligenteco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;evaloj kun flirtantaj kolharoj kaj <tld/>aj kruroj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno lumis tra la pentritaj vitroj inter la altaj <tld/>aj kolonoj en la
        internon
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="statur.bel0a">belstatura</ref>,
      <ref cel="cxarm.0a">&ccirc;arma</ref>,
      <ref cel="delika.0a">delikata</ref>,
      <ref cel="mov.facil0a">facilmova</ref>,
      <ref cel="lert.0a.korpo">lerta<sncref/></ref>,
      <ref cel="svelt.0a">svelta</ref>,
      <ref cel="vigl.0a.rapidmova">vigla<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_c;&c_y;&c_jo;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>graci&eacute;zn&iacute;</trd>,
