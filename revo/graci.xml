  <trdgrp lng="ja">
    <trd>&#x512A;&#x7F8E;  <pr>&#x3086;&#x3046;&#x3073;</pr></trd>,
    <trd>&#x3057;&#x3068;&#x3084;&#x304B;&#x3055;</trd>,
    <trd>&#x6C17;&#x54C1;  <pr>&#x304D;&#x3072;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
