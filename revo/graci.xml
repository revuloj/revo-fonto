</drv>

<drv mrk="graci.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      La&ubreve; <tld/>a maniero:
      <ekz>
        nenia dancistino povos flirtadi tiel <tld/>e kiel vi
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        letero skribita sur rozokolora papero tre delikata <klr>[&#x2026;]</klr>, <tld/>e &gcirc;i
        estis skribita, tio estis skribkaraktero de sinjorino
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> sen la e&ccirc; plej malgranda konfuzi&gcirc;o, tre <tld/>e riverencis al
        sia estonta instruantino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        beleta sorparbo enigis tie radikojn kaj staris <tld/>e kaj elegante alte supre
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;ipo malrapide kaj <tld/>e na&gcirc;is la&ubreve;longe la riveron
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        li saltas <tld/>e, kiel urso ebria
        <klr>(ironie)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_a;&c_c;&c_y;&c_jo;&c_z;&c_n;&c_a;</trd>,
    <trd>&c_z;&c_g;&c_r;&c_a;&c_b;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>graci&eacute;zn&ecaron;</trd>,
    <trd>graci&oacute;zn&ecaron;</trd>,
    <trd>ladn&ecaron;</trd>,
    <trd>p&uring;vabn&ecaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>grazi&ouml;s</trd>,
    <trd>anmutig</trd>,
    <trd>zierlich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>gracefully</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>graciosamente</trd>,
    <trd>con gracia</trd>
  </trdgrp>
  <trd lng="fr">gracieusement<klr tip="ind"> (avec gr&acirc;ce)</klr>
  </trd>
  <trdgrp lng="hu">
    <trd>kecsesen</trd>,
    <trd>b&aacute;josan</trd>,
    <trd>k&ouml;nnyed&eacute;n</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x512A;&#x96C5;&#x306B;  <pr>&#x3086;&#x3046;&#x304C;&#x306B;</pr></trd>,
    <trd>&#x3057;&#x3068;&#x3084;&#x304B;&#x306B;</trd>
  </trdgrp>
  <trdgrp lng="nl">
