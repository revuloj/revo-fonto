<drv mrk="graci.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Havanta sen<ref tip="ant" cel="harmon1.0a">harmonia</ref>jn movojn a&ubreve; formojn:
      <ekz>
        vidante &ccirc;i tiujn fremdulojn, fortajn, mal<tld/>ajn, vulgare ridantajn
        <klr>[&#x2026;]</klr> <nom>Ramzes</nom> ekbolis
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kamenoj mal<tld/>aj, el verdaj kaheloj
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patrino ku&scirc;is, vestita, sur la apuda lito, kun la mal<tld/>a, pala viza&gcirc;o
        kontra&ubreve; la lumo
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>Du blinduloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        monotonaj muroj kaj mal<tld/>aj arboj
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krud.0a.akra">kruda<sncref/></ref>,
      <ref cel="plump.0a">plumpa</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
