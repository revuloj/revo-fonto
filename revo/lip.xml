<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lip</rad>/o
</kap>
<drv mrk="lip.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="lip.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Movebla rando de la bu&scirc;o, kiu kovras la dentojn:
      <ekz>
        supra <tld/>o;
      </ekz>
      <ekz>
        ru&gcirc;aj, koralaj <tld/>oj;
      </ekz>
      <ekz>
        mallar&gcirc;aj kunpremitaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lip.0o.parolo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tiu parto de la viza&gcirc;o, konsiderata kiel organo de la parolo:
      <ekz>
        li &ccirc;iam havas tiun vorton sur la <tld/>oj;
      </ekz>
      <ekz>
        kiu retenas siajn <tld/>ojn estas sa&gcirc;a<fnt>Z</fnt>;
      </ekz>
      <ekz>
        viaj <tld/>oj parolos &gcirc;usta&jcirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        veramaj <tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="busx.0o">bu&scirc;o</ref>,
      <ref cel="lang.0o">lango</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Lippe</trd>
  <trd lng="en">lip</trd>
  <trd lng="es">labio</trd>
  <trd lng="fr">l&egrave;vre</trd>
  <trd lng="nl">lip</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_a;</trd>
  <trd lng="sv">l&auml;pp</trd>
</drv>

<drv mrk="lip.0haroj">
  <kap>
    <tld/>haroj
  </kap>
  <snc>
    <dif>
      Haroj kreskantaj &ccirc;e la supra <tld/>o: (f)
      <ekz>
        bruligi al si la <tld/>harojn<fnt>Z</fnt>
        <klr>(ne atingi la esperatan rezultaton.
        <ref tip="sin" cel="traf.mal0i">maltrafi</ref>,
        <ref tip="vid" cel="naz.0o">nazo</ref>)</klr>
      </ekz>
    </dif>
  </snc>
  <trd lng="es">bigote</trd>
  <trd lng="fr">moustache</trd>
  <trd lng="nl">snor</trd>
  <trd lng="ru">&c_u;&c_s;&c_y;</trd>
  <trd lng="sv">mustasch</trd>
</drv>

<drv mrk="lip.0mieni">
  <kap>
    <tld/>mieni
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="lip.faridik0on">Fari dik<tld/>on</ref>.
    </dif>
  </snc>
  <trd lng="nl">pruilen</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_u;&c_t;&c_mol; &c_g;&c_u;&c_b;&c_y;</trd>
  </trdgrp>
</drv>

<drv mrk="lip.faridik0on">
  <kap>
    fari dik<tld/>on
  </kap>
  <snc>
    <dif>
      Montri malkontenti&gcirc;on, anta&ubreve;enigante la <tld/>ojn; bu&scirc;pintigi.
    </dif>
  </snc>
  <trd lng="fr">faire la <ind>moue</ind></trd>
  <trd lng="nl">pruilen</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_u;&c_t;&c_mol; &c_g;&c_u;&c_b;&c_y;</trd>
  </trdgrp>
</drv>
</art>
