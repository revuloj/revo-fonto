<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lip.xml,v 1.45 2023/09/01 19:11:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lip</rad>/o
</kap>

<drv mrk="lip.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lip.0o.ANA_busxa">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Movebla rando de la
      <ref tip="malprt" cel="busx.0o.ANA">bu&scirc;o</ref>,
      kiu kovras la dentojn:
      <ekz>
        konfuzita, li nur mordis la <tld/>ojn kaj silentis
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>;
      </ekz><ekz>
        supra <tld/>o;
      </ekz><ekz>
        mallar&gcirc;aj kunpremitaj <tld/>oj;
      </ekz>
      <ekz>
        leprulo <klr>[&#x2026;]</klr> &gcirc;is la <tld/>oj devas esti &ccirc;irka&ubreve;kovrita
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:45</lok>
        </fnt>;
      </ekz>
      <ekz>
        de la manoj &gcirc;is <tld/>oj la sup' elver&scirc;i&gcirc;is
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        inter pokalo kaj <tld/>oj povas multe okazi
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Karen</nom> genustari&gcirc;is anta&ubreve; la altaro kaj almetis al la
        <tld/>oj la oran kalikon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kise alpremis siajn <tld/>ojn al la floro
        <fnt>
          <bib>Fab3</bib>
          <lok>Folio el la &ccirc;ielo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj <tld/>oj tremante movi&gcirc;is
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dol&ccirc;e &scirc;miri al iu la <tld/>ojn <klr>(flati)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        plenblovi la <tld/>ojn
        <klr>(vd dika <tld/>o)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lip.0o.parolilo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sama parto de la homa kapo, konsiderata kiel servanta por
      paroli:
      <ekz>
        li &ccirc;iam havas tiun vorton sur la <tld/>oj;
      </ekz><ekz>
        kiu retenas siajn <tld/>ojn estas sa&gcirc;a<fnt>Z</fnt>;
      </ekz><ekz>
        kaj puran scion eldiros miaj <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Ijo; 33:3</lok></fnt>;
      </ekz><ekz>
        veramaj <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <nom>Ijob</nom> ne pekis per siaj <tld/>oj
        <klr>(ne plendis)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 2:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        li mutigas la <tld/>ojn de fidinduloj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 12:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;ojas miaj <tld/>oj, kiam mi kantas al Vi
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 71:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur &scirc;iaj <tld/>oj movi&gcirc;adis, sed &scirc;ian vo&ccirc;on oni ne a&ubreve;dis
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 1:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        veneno de vipuro estas sub iliaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 140:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj de piulo gvidas multajn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 10:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unua vorto, kiu eliros el viaj <tld/>oj, frapos kiel mortiga
        ponardo la koron
        de viaj fratoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kio tra l' dentoj travenis, tion la <tld/>oj ne retenos
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; unu vorteto ne transpa&scirc;os niajn <tld/>ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        meti la fingrojn sur la <tld/>ojn
        <klr>(silentigi)</klr>
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>&scirc;ercemaj <tld/>oj</ind>
        <fnt>
          <bib>IK</bib>
        </fnt>.
        <trd lng="de">kesse <ind>Lippe</ind></trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="busx.0o.parolilo">bu&scirc;o<sncref/></ref>,
      <ref cel="lang.0o.parolkapablo">lango<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_b;&c_a;</trd>
  <trd lng="bg">&c_u;&c_s;&c_t;&c_n;&c_a;</trd>
  <trd lng="bo">&#xF58;&#xF46;&#xF74;&#xF0B;&#xF4F;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>pysk</trd>,
    <trd>ret</trd>
  </trdgrp>
  <trd lng="de">Lippe</trd>
  <trd lng="el">&chi;&epsilon;&jota_ton;&lambda;&jota;</trd>
  <trd lng="en">lip</trd>
  <trd lng="es">labio</trd>
  <trd lng="fr">l&egrave;vre</trd>
  <trd lng="he">&shin;&pe;&he;<klr tip="amb"> (&alef;&het;&tav;
  &he;&shin;&pe;&tav;&yod;&yod;&fmem;)</klr></trd>
  <trd lng="hu">ajak</trd>
  <trd lng="id">bibir</trd>
  <trd lng="it">labbro</trd>
  <trdgrp lng="ja">
    <trd>&#x5507;  [<ind>&#x304F;&#x3061;&#x3073;&#x308B;</ind>]</trd>,
    <trd>&#x53E3;  [<ind>&#x304F;</ind>]</trd>,
    <trd>&#x5507;&#x5F01;  [<ind>&#x304F;&#x3061;&#x3073;&#x308B;&#x3079;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">lip</trd>
  <trd lng="pl">warga</trd>
  <trd lng="pt">l&aacute;bio</trd>
  <trd lng="ro">buz&abreve;</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_a;</trd>
  <trd lng="sk">pysk</trd>
  <trd lng="sv">l&auml;pp</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&c_b;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_b;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="lip.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o(j):
      <ekz>
        <uzo tip="fak">FON</uzo>
        <tld/>a parolsono
        <klr>(= <ref cel="lip.0sono"><tld/>sono</ref>)</klr>;
      </ekz><ekz>
        <uzo tip="fak">FON</uzo>
        <tld/>a artikulacio;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a kulto <klr>(formala, ne elkora)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">labial</trd>
  <trd lng="fr">labial</trd>
  <trd lng="ja">&#x5507;&#x306E;  [<ind>&#x304F;&#x3061;&#x3073;&#x308B;&#x306E;</ind>]</trd>
  <trd lng="pl">wargowy</trd>
  <trd lng="pt">labial</trd>
  <trd lng="ro">labial</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_n;&c_o;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&c_b;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43B;&c_a;&c_b;&c_ib;&c_a;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="lip.0sono">
  <kap><tld/>sono</kap>
  <snc mrk="lip.0sono.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="super" cel="son.parol0o">Parolsono</ref> en kies
      artikulacio partoprenas unu a&ubreve; amba&ubreve; <tld/>oj.
    </dif>
  </snc>
  <refgrp tip="sub">
    <ref cel="lip.0akonsonanto"><tld/>a konsonanto</ref>,
    <ref cel="lip.0avokalo"><tld/>a vokalo</ref>
  </refgrp>
  <trdgrp lng="de">
      <trd>Labial</trd>,
      <trd>Labiallaut</trd>
  </trdgrp>
  <trd lng="ja">&#x5507;&#x97F3;  [<ind>&#x3057;&#x3093;&#x304A;&#x3093;</ind>]</trd>
  <trd lng="pl">sp&oacute;&lstroke;g&lstroke;oska <ind>wargowa</ind></trd>
  <trd lng="ro">consoan&abreve; labial&abreve;</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_n;&c_o;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>
  <trd lng="uk">&c_g;&c_u;&c_b;&#x43D;&c_i;&c_j; &#x43F;&c_r;&c_i;&c_g;&#x43E;&#x43B;&#x43E;&c_s;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="lip.0akonsonanto">
  <kap><tld/>a konsonanto</kap>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="super" cel="konson1.0o">Konsonanto</ref>
      <ref tip="sub" cel="lip.00akonsonanto"><tld/>-<tld/>a</ref>
      a&ubreve;
      <ref tip="sub" cel="lip.0dentakonsonanto"><tld/>-denta</ref>.
    </dif>
  </snc>
  <ref tip="super" cel="lip.0sono"><tld/>sono</ref>
  <trd lng="be">&c_g;&c_u;&c_b;&c_n;&c_y;
  &c_z;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">labial</trd>
  <trdgrp lng="de">
    <trd>Labial</trd>,
    <trd>Labiallaut</trd>,
    <trd>Lippenlaut</trd>
  </trdgrp>
  <trd lng="en">labial</trd>
  <trd lng="es">labial</trd>
  <trd lng="fr">labiale</trd>
  <trdgrp lng="hu">
    <trd>ajakhang</trd>,
    <trd>labi&aacute;lis</trd>
  </trdgrp>  
  <trdgrp lng="nl">
    <trd>labiaal</trd>,
    <trd>lipletter</trd>
  </trdgrp>
  <trd lng="pl">sp&oacute;&lstroke;g&lstroke;oska <ind>wargowa</ind></trd>
  <trdgrp lng="pt">
    <trd>labial <klr tip="amb">(fon.)</klr></trd>,
    <trd>consoante labial <klr tip="amb">(fon.)</klr></trd>
  </trdgrp>
  <trd lng="ro">consoan&abreve; labial&abreve;</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_n;&c_o;&c_j;
  &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
  <trd lng="uk">&c_g;&c_u;&c_b;&#x43D;&c_i;&c_j; &#x43F;&c_r;&c_i;&c_g;&#x43E;&#x43B;&#x43E;&c_s;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="lip.00akonsonanto">
  <kap><tld/>-<tld/>a konsonanto</kap>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="super" cel="konson1.0o">Konsonanto</ref>
      artikulaciata per alproksimigo de la du <tld/>oj:
      <ekz>
        Esperanto havas 3 <tld/>-<tld/>ajn konsonantojn:
        /p/, /b/, /m/.
      </ekz>
    </dif>
  </snc>
  <ref tip="super" cel="lip.0sono"><tld/>sono</ref>
  <trdgrp lng="de">
      <trd>Bilabial</trd>,
      <trd>bilabialer Konsonant</trd>
  </trdgrp>
  <trd lng="en">bilabial</trd>
  <trd lng="pl">sp&oacute;&lstroke;g&lstroke;oska dwuwargowa</trd>
  <trd lng="ro">consoan&abreve; bilabial&abreve;</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_n;&c_o;-&c_g;&c_u;&c_b;&c_n;&c_o;&c_j;
  &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="lip.0dentakonsonanto">
  <kap><tld/>-denta konsonanto</kap>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="super" cel="konson1.0o">Konsonanto</ref>
      artikulaciata per alproksimigo de la malsupra <tld/>o
      al supraj dentoj:
      <ekz>
        Esperanto havas 2 <tld/>-dentajn konsonantojn:
        /f/ kaj /v/.
      </ekz>
    </dif>
  </snc>
  <ref tip="super" cel="lip.0sono"><tld/>sono</ref>
  <trdgrp lng="de">
      <trd>Labiodental</trd>,
      <trd>labiodentaler Konsonant</trd>
  </trdgrp>
  <trd lng="en">labiodental</trd>
  <trd lng="pl">sp&oacute;&lstroke;g&lstroke;oska wargowo-z&eogonek;bowa</trd>
  <trd lng="ro">consoan&abreve; labiodental&abreve;</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_n;&c_o;-&c_z;&c_u;&c_b;&c_n;&c_o;&c_j;
  &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="lip.0avokalo">
  <kap><tld/>a vokalo</kap>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref tip="super" cel="vokal.0o">Vokalo</ref>
      artikulaciata per anta&ubreve;en&scirc;ovo kaj rondigo de la
      <tld/>oj:
      <ekz>
        Esperanto havas 2 <tld/>ajn vokalojn:
        /o/ kaj /u/.
      </ekz>
    </dif>
  </snc>
  <ref tip="super" cel="lip.0sono"><tld/>sono</ref>
    <trdgrp lng="de">
      <trd>gerundeter Vokal</trd>,
      <trd>labialisierter Vokal</trd>
    </trdgrp>
  <trdgrp lng="en">
    <trd>labialized vowel</trd>,
    <trd>rounded vowel</trd>
  </trdgrp>
  <trd lng="pl">samog&lstroke;oska <ind>wargowa</ind></trd>
  <trd lng="ro">vocal labial</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_n;&c_o;&c_j;
  &c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="lip.lepor0o">
  <kap>lepor<tld/>o</kap>
  <snc mrk="lip.lepor0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Vertikala fendo en la supra lipo
      pro plej ofte denaska manko en viza&gcirc;a &ccirc;elaro:
      <ekz>
        grandkapa, lepor<tld/>a junulo
        <fnt>
          <aut>A. &Ccirc;e&hcirc;ov, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;Libroj/V_bibl02.pdf">
              Sveda alumeto</url></vrk>,
          <lok>2004-2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Hasenscharte</trd>
  </snc>
  <trd lng="bg">&c_z;&c_a;&c_je;&c_sh;&c_k;&c_a; &c_u;&c_s;&c_t;&c_n;&c_a;</trd>
  <trd lng="cs">zaje&ccaron;&iacute; pysk</trd>
  <trdgrp lng="fr">
    <trd>bec-de-li&egrave;vre</trd>,
    <trd>fente labiale</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x307F;&#x3064;&#x53E3;  [<ind>&#x307F;&#x3064;&#x3053;&#x3046;</ind>]</trd>,
    <trd>&#x514E;&#x5507;  [<ind>&#x3044;&#x3050;&#x3061;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zaj&eogonek;cza <ind>warga</ind></trd>,
    <trd>rozszczep <ind>wargi</ind> i podniebienia</trd>
  </trdgrp>
  <trd lng="ro">despic&abreve;turi labiale &#x219;i palatine</trd>
  <trd lng="sk">zaja&ccaron;ia pera</trd>
  <trd lng="uk">&c_z;&c_a;&#x44F;&c_ch;&c_a; &c_g;&c_u;&c_b;&c_a;</trd>
</drv>

<drv mrk="lip.dika0o">
  <kap>dika <tld/>o</kap>
  <uzo tip="stl">FIG</uzo>
  <snc mrk="lip.dika0o.fanfarona">
    <dif>
      Fanfarono:
      <ekz>
        se <tld/>o diki&gcirc;as, ventro maldiki&gcirc;as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">dicke <ind>Lippe</ind> <klr>(Angeberei)</klr></trd>
  </snc>
  <snc mrk="lip.dika0o.malkontenta">
    <ref tip="dif" cel="pauxt.0o">pa&ubreve;to</ref>
    <rim>
      Kontra&ubreve;e, oni povas anka&ubreve; rezignacie montri malkontenton,
      humili&gcirc;on per
      kunpremitaj <tld/>oj:
      <ekz>
        <nom>Mefres</nom> kunpremis siajn maldikajn <tld/>ojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
      </ekz>
    </rim>
    <trdgrp lng="de">
      <trd>Schmollmund</trd>,
      <trd>Schnute</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_z;&c_mol;&c_m;&c_u;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_b;&c_y;
    &c_n;&c_a;&c_d;&c_z;&c_mol;&c_m;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_u;&c_b;&c_y;
    &c_z;&c_a;&c_k;&c_a;&c_p;&c_y;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_y;&c_b;&c_y;
    &c_n;&c_a;&c_d;&c_z;&c_mol;&c_m;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schmollen</trd>,
    <trd>einen Schmollmund machen</trd>
  </trdgrp>
  <trd lng="en">pout</trd>
  <trdgrp lng="fr">
    <trd>faire la <ind>moue</ind></trd>,
    <trd>bouder</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ajk&aacute;t biggyeszti</trd>,
    <trd>duzzog</trd>
  </trdgrp>
  <trd lng="it">fare il <ind>broncio</ind></trd>
  <trd lng="nl">pruilen</trd>
  <trdgrp lng="pl">
    <trd>wyd&eogonek;ta warga</trd>,
    <trd>grymas niezadowolenia</trd>
  </trdgrp>
  <trd lng="ro">buzele m&abreve;run&#x21B;ite</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_u;&c_t;&c_mol; &c_g;&c_u;&c_b;&c_y;</trd>
  </trdgrp>
</drv>

  </art>
