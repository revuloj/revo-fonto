<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lip</rad>/o
</kap>
<drv mrk="lip.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lip.0o.ANA_busxa">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Movebla rando de la <ref tip="malprt" cel="busx.0o.ANA">bu&scirc;o</ref>,
      kiu kovras la dentojn:
      <ekz>
        konfuzita, li nur mordis la <tld/>ojn kaj silentis
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>;
      </ekz>
      <ekz>
        supra <tld/>o;
      </ekz>
      <ekz>
        mallar&gcirc;aj kunpremitaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lip.0o.parolilo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sama parto de la homa kapo, konsiderata kiel servanta por paroli:
      <ekz>
        li &ccirc;iam havas tiun vorton sur la <tld/>oj;
      </ekz>
      <ekz>
        kiu retenas siajn <tld/>ojn estas sa&gcirc;a<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kaj puran scion eldiros miaj <tld/>oj
        <fnt><bib>MT</bib>, <lok>&Ijo; 33:3</lok></fnt>;
      </ekz>
      <ekz>
        veramaj <tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="busx.0o.parolilo">bu&scirc;o <sncref/></ref>,
      <ref cel="lang.0o.parolkapablo">lango <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_b;&c_a;</trd>
  <trd lng="de">Lippe</trd>
  <trd lng="el">&chi;&epsilon;&jota_ton;&lambda;&jota;</trd>
  <trd lng="en">lip</trd>
  <trd lng="es">labio</trd>
  <trd lng="fr">l&egrave;vre</trd>
  <trd lng="he">&shin;&pe;&he;<klr tip="amb"> (&alef;&het;&tav; &he;&shin;&pe;&tav;&yod;&yod;&fmem;)</klr></trd>
  <trd lng="hu">ajak</trd>
  <trd lng="nl">lip</trd>
  <trd lng="pt">l&aacute;bio</trd>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_a;</trd>
  <trd lng="sv">l&auml;pp</trd>
</drv>

<drv mrk="lip.0mieni">
  <kap><tld/>mieni</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <ref tip="dif" cel="lip.faridik0on">fari dik<tld/>on</ref>
  </snc>
  <trdgrp lng="hu">
    <trd>ajk&aacute;t biggyeszti</trd>,
    <trd>duzzog</trd>
  </trdgrp>
  <trd lng="nl">pruilen</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_u;&c_t;&c_mol; &c_g;&c_u;&c_b;&c_y;</trd>
  </trdgrp>
</drv>

<drv mrk="lip.faridik0on">
  <kap>fari dik<tld/>on</kap>
  <snc>
    <dif>
      Montri malkontenti&gcirc;on, anta&ubreve;enigante la <tld/>ojn; bu&scirc;pintigi.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_z;&c_mol;&c_m;&c_u;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_g;&c_u;&c_b;&c_y; &c_n;&c_a;&c_d;&c_z;&c_mol;&c_m;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_u;&c_b;&c_y; &c_z;&c_a;&c_k;&c_a;&c_p;&c_y;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_y;&c_b;&c_y; &c_n;&c_a;&c_d;&c_z;&c_mol;&c_m;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de"><klr>eine </klr>Schnute machen</trd>
  <trd lng="fr">faire la <ind>moue</ind></trd>
  <trdgrp lng="hu">
    <trd>ajk&aacute;t biggyeszti</trd>,
    <trd>duzzog</trd>
  </trdgrp>
  <trd lng="nl">pruilen</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_u;&c_t;&c_mol; &c_g;&c_u;&c_b;&c_y;</trd>
  </trdgrp>
</drv>
</art>
