<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kaprimulg</rad>/o
</kap>
<drv mrk="kaprim.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <tld/>edoj (<trd lng="la">Caprimulgus</trd>),
      birdo kun lar&gcirc;a beko, grandaj okuloj kaj arb&scirc;elkolora
      plumaro (= noktohirundo).
    </dif>
  </snc>
  <trd lng="cs">lelek</trd>
  <trdgrp lng="de">
    <trd>Ziegenmelker</trd>,
    <trd>Nachtschwalbe</trd>
  </trdgrp>
  <trd lng="en">nightjar</trd>
  <trd lng="hu">kecskefej&odacute;</trd>
  <trd lng="pl">lelek</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_d;&c_o;&c_o;&c_j;</trd>
</drv>
</art>

