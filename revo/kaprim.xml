<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaprim.xml,v 1.17 2022/05/01 08:09:23 revo Exp $">
<kap>
  <rad>kaprimulg</rad>/o
</kap>
<drv mrk="kaprim.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <tld/>edoj (<trd lng="la">Caprimulgus</trd>),
      <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref>
      kun lar&gcirc;a beko, grandaj okuloj kaj arb&scirc;elkolora plumaro:
      <ekz>
        <tld/>oj vivas en malfermaj kaj malsekaj ebena&jcirc;oj kaj ondaj terenoj kie abundas
        veprejoj kaj filikejoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26022">Kaprimulgo</url></lok>
        </fnt>.
      </ekz>
    </dif>  
    <refgrp tip="sin">
      <ref cel="hirund.nokto0o">noktohirundo</ref>
    </refgrp>   
    <bld lok="&WCU;/thumb/e/ee/Long-tailed_nightjar_(Caprimulgus_climacurus_climacurus)_male.jpg/640px-Long-tailed_nightjar_(Caprimulgus_climacurus_climacurus)_male.jpg" prm="CC BY-SA 4.0"><tld/>o (Caprimulgus climacurus), Gambio
        <fnt>
          <aut>Charles J Sharp, propra verko</aut>
          <lok><url ref="&WCW;/File:Long-tailed_nightjar_(Caprimulgus_climacurus_climacurus)_male.jpg">&WCW;/File:Long-tailed_nightjar_(Caprimulgus_climacurus_climacurus)_male.jpg</url></lok>
        </fnt>
    </bld>
  </snc>              
  <trd lng="be">&c_l;&c_ja;&c_l;&c_ja;&c_k;</trd>
  <trd lng="br">adren</trd>
  <trd lng="ca">g&egrave;nere Caprimulgus</trd>
  <trd lng="cs">lelek</trd>
  <trdgrp lng="de">
    <trd>Ziegenmelker</trd>,
    <trd>Nachtschwalbe</trd>
  </trdgrp>
  <trd lng="en">nightjar</trd>
  <trdgrp lng="fr">
    <trd>engoulevent</trd>
    <trd><ind>crapaud</ind> volant<klr tip="ind">
    (engoulevent)</klr></trd>
  </trdgrp>
  <trd lng="hu">kecskefej&odblac;</trd>
  <trdgrp lng="nl">
    <trd>geitenmelker<klr tip="ind"> (zwaluw)</klr></trd>,
    <trd>nachtzwaluw</trd>
  </trdgrp>
  <trd lng="pl">lelek kozod&oacute;j</trd>
  <trd lng="pt">noitib&oacute;</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_o;&c_d;&c_o;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x440;&#x456;&#x43C;&#x43B;&#x44E;&#x433;&#x430;</trd>,
    <trd>&#x441;&#x43F;&#x43B;&#x44E;&#x448;&#x43A;&#x430;</trd>,
    <trd>&#x441;&#x43F;&#x43B;&#x44E;&#x445;&#x430;</trd>
  </trdgrp>
</drv>
</art>
