<art mrk="$Id: ru.xml,v 1.17 2025/04/05 10:02:33 revo Exp $">
<kap>
  <rad>ru</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ru.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="macer.0i">Maceri</ref> tekseblan planton en likva&jcirc;o por apartigi &gcirc;iajn
      fibrojn de
      aliaj substancoj:
      <ekz>
        oni plukis &gcirc;in, <tld/>is, sekigis kaj jam rompis, sed jen la lino ek&#64258;amis
        <fnt>
          <aut>Alois Jirasek</aut>
          <vrk><url ref="http://corpora.uni-leipzig.de/de/res?corpusId=epo_mixed_2012&amp;word=ruis">Malnovaj
          mitoj &ccirc;e&hcirc;aj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi mem pistis kaj <tld/>is la jugland-fungojn, per kiuj ni brunigis la blankajn
        felojn
        <fnt>
          <aut>Vizi L&aacute;szl&oacute;</aut>
          <vrk><url ref="http://www.ipernity.com/blog/eszperanto/388896">La Lasta
          Kudrero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>trempi</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_l;&c_jo;&c_n;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mo&ccaron;it <klr>(len, konop&iacute;)</klr></trd>,
    <trd>rosit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>r&ouml;sten <klr>(Pflanzenfasern)</klr></trd>,
    <trd>rotten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ret</trd>,
    <trd>steep</trd>
  </trdgrp>
  <trd lng="hu">&aacute;ztat <klr tip="amb">(pl. kendert)</klr></trd>
  <trd lng="ja">&#28024;&#27700;&#12377;&#12427;  <pr>&#12375;&#12435;&#12377;&#12356;&#12377;&#12427;</pr></trd>
  <trd lng="nl">roten</trd>
  <trdgrp lng="pl">
    <trd>rosi&cacute;</trd>,
    <trd>moczy&cacute; <klr>(len, konopie)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_ch;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_l;&c_jo;&c_n;, &c_k;&c_o;&c_n;&c_o;&c_p;&c_l;&c_ju;)</klr></trd>
  <trdgrp lng="sk">
    <trd>mo&ccaron;i&tcaron;</trd>,
    <trd>rosi&tcaron; <klr>(&lcaron;an, konope)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1084;&#1086;&c_ch;&c_i;&c_t;&c_i; (&#1083;&#1100;&#1086;&#1085;</trd>,
    <trd>&#1082;&#1086;&#1085;&#1086;&#1087;&#1083;&c_ib;)</trd>
  </trdgrp>
</drv>
</art>
