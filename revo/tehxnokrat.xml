<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>teĥnokrat</rad>/o
</kap>
<drv mrk="tehxnokrat.0o">
  <kap><tld/>o</kap>
  <snc mrk="tehxnokrat.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Homo, kiu regas aŭ administras fidante fakulojn,
      <tld/>isme:
      <ekz>
        kiel multaj aliaj aferoj de sana prudento, ĝi perdigis ĉe la
        modernaj <tld/>oj
        <fnt>
        <aut>Roland</aut>,
        <vrk><url ref="http://raporto.info/2006/01/29">Falantaj
        konstruaĵoj..., komentoj</url></vrk>,
        <lok>raporto.info, 2006-01-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">technocrate</trd>
  <trd lng="pl">technokrata</trd>
  <trd lng="pt">tecnocrata</trd>
</drv>

<drv mrk="tehxnokrat.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="tehxnokrat.0aro.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Tuto de la <tld/>oj.
    </dif>
    <refgrp tip="vid">
      <ref cel="burokr.0aro.POL">burokrataro</ref>
    </refgrp>
  </snc>
  <trd lng="fr">technocratie <klr>(personnel)</klr></trd>
</drv>

<drv mrk="tehxnokrat.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="tehxnokrat.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Administrado laŭ la opinioj de teĥnikistoj kaj fakuloj pli,
ol laŭ
      la opinioj de ordinaruloj.
    </dif>
    <refgrp tip="vid">
      <ref cel="burokr.0ismo.POL">burokratismo</ref>
    </refgrp>
  </snc>
  <trd lng="fr">technocratie<klr> (système)</klr></trd>
  <trd lng="pl">technokracja</trd>
</drv>
</art>

