<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>te&hcirc;nokrat</rad>/o
</kap>
<drv mrk="tehxnokrat.0o">
  <kap><tld/>o</kap>
  <snc mrk="tehxnokrat.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Homo, kiu regas a&ubreve; administras fidante fakulojn,
      <tld/>isme:
      <ekz>
        kiel multaj aliaj aferoj de sana prudento, &gcirc;i perdigis &ccirc;e la
        modernaj <tld/>oj
        <fnt>
        <aut>Roland</aut>,
        <vrk><url ref="http://raporto.info/2006/01/29">Falantaj
        konstrua&jcirc;oj..., komentoj</url></vrk>,
        <lok>raporto.info, 2006-01-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">technocrate</trd>
  <trd lng="pl">technokrata</trd>
  <trd lng="pt">tecnocrata</trd>
  <trd lng="ru">&c_t;&c_je;&c_h;&c_n;&c_o;&c_k;&c_r;&c_a;&c_t;</trd>
</drv>

<drv mrk="tehxnokrat.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="tehxnokrat.0aro.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Tuto de la <tld/>oj.
    </dif>
    <refgrp tip="vid">
      <ref cel="burokr.0aro.POL">burokrataro</ref>
    </refgrp>
  </snc>
  <trd lng="fr">technocratie <klr>(personnel)</klr></trd>
  <trd lng="ru">&c_t;&c_je;&c_h;&c_n;&c_o;&c_k;&c_r;&c_a;&c_t;&c_i;&c_ja; 
<klr>(&c_l;&c_ju;&c_d;&c_i;)</klr></trd>
</drv>

<drv mrk="tehxnokrat.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="tehxnokrat.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Administrado la&ubreve; la opinioj de te&hcirc;nikistoj kaj fakuloj pli,
      ol la&ubreve; la opinioj de ordinaruloj.
    </dif>
    <refgrp tip="vid">
      <ref cel="burokr.0ismo.POL">burokratismo</ref>
    </refgrp>
  </snc>
  <trd lng="fr">technocratie<klr> (syst&egrave;me)</klr></trd>
  <trd lng="pl">technokracja</trd>
  <trd lng="ru">&c_t;&c_je;&c_h;&c_n;&c_o;&c_k;&c_r;&c_a;&c_t;&c_i;&c_ja; 
<klr>(&c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a;)</klr></trd>
</drv>
</art>

