<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>interfer</rad>/i
</kap>
<drv mrk="interf.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on:
      <ekz>
        kiam oni reflektigas sur du metalaj ebenaj
        reflektiloj, formantaj angulon de preska&ubreve; 180&ring;, luman faskon,
        tiamaniere, ke, post reflekti&gcirc;o, okazu du faskoj sin
        tran&ccirc;antaj la&ubreve; tre akra angulo, la radioj
        renkonti&gcirc;ante <tld/>as;
      </ekz>
      <ekz>
        kiam oni ricevas ilin sur blanka ekrano, aperas brilaj kaj
        malbrilaj strioj nomataj <tld/>aj fran&gcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_r;&c_f;&c_e;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">interferieren</trd>
  <trd lng="fr">interf&eacute;rer</trd>
  <trd lng="hu">interfer&aacute;l</trd>
  <trd lng="nl">interfereren</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_f;&c_je;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="interf.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Plimalforti&gcirc;o de la lumo ka&ubreve;zata en kelkaj okazoj per
      interkruci&gcirc;o de lumaj radioj.  
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_r;&c_f;&c_e;&c_r;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Interferenz</trd>
  <trd lng="fr">interf&eacute;rence</trd>
  <trd lng="hu">interferencia</trd>
  <trd lng="nl">interferentie</trd>
<trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
</drv>
<drv mrk="interf.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta <tld/>on: <ekz><tld/>aj fran&gcirc;oj, strioj..</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_r;&c_f;&c_e;&c_r;&c_e;&c_n;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
   <trd lng="de">interferierend</trd>
  <trdgrp lng="hu">
    <trd>interfer&aacute;l&oacute;</trd>,
    <trd>interferens</trd>,
    <trd>interferencia-</trd>
  </trdgrp>
   <trd lng="nl">interfererend</trd>
<trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="interf.0ajfrangxoj">
  <kap><tld/>aj fran&gcirc;oj</kap>
  <snc>
    <dif>
      Mallumaj strioj, kiuj aperas en luma <tld/>o. 
    </dif>
  </snc>
  <trd lng="fr">franges d'interf&eacute;rence</trd>
  <trd lng="hu">interferenciacs&iacute;kok</trd>
</drv>
<drv mrk="interf.0igajspeguloj">
  <kap><tld/>igaj speguloj</kap>
  <snc>
    <dif>
      La speciale aran&gcirc;itaj speguloj, per kiuj oni reflektigas la
      radiojn de la fasko, por ke <tld/>o okazu.  
    </dif>
  </snc>
  <trd lng="hu">interferenciat&uuml;kr&ouml;k</trd>
</drv>
</art>
