<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jxig.xml,v 1.16 2023/12/03 07:09:19 revo Exp $">
<kap>
  <rad>&jcirc;ig</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="&gcirc;">&gcirc;ig</rad>/o <fnt><bib>MuzTerm</bib></fnt></kap></var>
</kap>

<drv mrk="jxig.0o">
  <kap><tld/>o,
    <var><kap><tld var="&gcirc;"/>o</kap></var>
  </kap>
  <snc mrk="jxig.0o.danco">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Vigla e&ubreve;ropa <ref tip="lst" cel="danc.0o" lst="voko:dancoj">danco</ref> popola, du- a&ubreve; tritakta, plu
      populara en irlando kaj skotlando:
      <ekz>
        li dancis <tld/>on por distri la grupon
        <fnt><bib>DOz</bib>, <lok>&leftquot;Zeb reiras al la
ran&ccirc;o&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Jig</trd>
    <trd lng="pt">jiga<klr tip="amb">(dan&ccedil;a)</klr></trd>
    <trd lng="ru">&c_d;&c_zh;&c_i;&c_g;&c_a;</trd>
  </snc>
  <snc mrk="jxig.0o.muzikajxo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>odanco, ofta fina parto de
      baroka <ref tip="malprt" cel="suit.0o.MUZ">suito</ref>:
      <ekz>
        en 1789 <nom>Mozart</nom> a&ubreve;skultis dum vizito en la <nom>Thomas</nom>-pre&gcirc;ejo de
        <nom>Bach</nom> la moteton
        &leftquot;S<nom>inget dem Herrn ein neues Lied</nom>&rightquot; <klr>[&#x2026;]</klr> spuroj de &ccirc;i tiu renkonto
        estas spontane komponita <tld var="&gcirc;"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19633">Johann Sebastian Bach</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Gigue</trd>
    <trd lng="pt">giga <klr tip="amb">(composi&ccedil;&atilde;o musical)</klr></trd>
    <trd lng="ru">&c_zh;&c_i;&c_g;&c_a;</trd>
    <trd lng="zh">&#x57FA;&#x683C; <pr>j&#x12B;g&#xE9;</pr></trd>
</snc>
  <ref tip="hom" cel="jxig1.0o">&jcirc;igo (puzlo)</ref>
  <trd lng="be">&c_d;&c_zh;&c_y;&c_g;&c_a;</trd>
  <trd lng="en">jig</trd>
  <trd lng="es">giga</trd>
  <trd lng="fa">&f_zh;&f_y;&f_g;</trd>
  <trd lng="fr">gigue</trd>
  <trd lng="hu">gigue</trd>
  <trd lng="it">giga</trd>
  <trdgrp lng="ja">
    <trd>&#x30B8;&#x30B0;</trd>,
    <trd>&#x30B8;&#x30FC;&#x30B0;</trd>
  </trdgrp>
  <trd lng="nl">gigue</trd>
  <trd lng="pl">gigue</trd>
  <trd lng="sv">jig</trd>
  <trdgrp lng="uk">
    <trd>&#x436;&#x438;&#x433;&#x430;</trd>,
    <trd>&#x434;&#x436;&#x438;&#x433;&#x430;&#xA0;(&#x442;&#x430;&#x43D;&#x435;&#x446;&#x44C;)</trd>
  </trdgrp>
</drv>
</art>

