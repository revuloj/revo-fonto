    <trd>plus exactement</trd>,
    <trd>plut&ocirc;t<klr> (plus exactement)</klr></trd>
  </trdgrp>
 <trd lng="id">lebih <ind>tepat</ind></trd>
 <trdgrp lng="it">
    <trd>pi&ugrave; <ind>esattamente</ind></trd>,
    <trd>pi&ugrave; <ind>precisamente</ind></trd>,
    <trd>pi&ugrave; <ind>propriamente</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
