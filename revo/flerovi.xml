  <trd lng="la">flerovium</trd>
  <trd lng="nl">flerovium</trd>
  <trd lng="pl">flerow</trd>
  <trd lng="pt">fler&oacute;vio</trd>
  <trd lng="ru">&c_f;&c_l;&c_je;&c_r;&c_o;&c_v;&c_i;&c_j;</trd>
