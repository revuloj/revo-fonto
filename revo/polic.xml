    <trd>intelijen</trd>,
    <trd>polisi tanpa seragam</trd>
  </trdgrp>
  <trd lng="ja">&#31169;&#26381;&#35686;&#23448;  <pr>&#12375;&#12405;&#12367;&#12369;&#12356;&#12363;&#12435;</pr></trd>
