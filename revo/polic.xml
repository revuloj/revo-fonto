    <trd>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_a;&c_r;
    &c_p;&c_o;&c_l;&c_i;&c_c;&c_i;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_k;
    &c_o;&c_t;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">n&aacute;&ccaron;eln&iacute;k pol&iacute;cie</trd>
