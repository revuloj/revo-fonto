<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>polic</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="polic.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="polic.0o.POL">
  <uzo tip="fak">POL</uzo>
    <dif>
      Administracio komisiita por prizorgi la publikan ordon kaj
      sendan&gcirc;erecon:
      <ekz>
        li ordonis al sia veturigisto veturi al la estro de la <tld/>o
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la karaj
        braceletoj</lok></fnt>;
      </ekz>
      <ekz>
        oni diras, ke el najbaraj stratoj estas senditaj grandaj
        ta&ccirc;mentoj
        de la <tld/>o
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">policia</trd>
  <trd lng="de">Polizei</trd>
  <trd lng="en">police</trd>
  <trd lng="es">polic&iacute;a<klr tip="amb"> (cuerpo de)</klr></trd>
  <trd lng="fr">police</trd>
  <trd lng="nl">politie</trd>
  <trd lng="pl">policja</trd>
  <trd lng="pt">pol&iacute;cia</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">polícia</trd>
  <trd lng="sv">polis</trd>
</drv>

<drv mrk="polic.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="polic.0ano.POL">
    <dif>
      Homo oficanta en <tld/>o:
      <ekz>
        nia urbo havas bonajn <tld/>anojn
        <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Polizist</trd>
  <trdgrp lng="es">
    <trd>guardia</trd>,
    <trd>polic&iacute;a</trd>
  </trdgrp>
  <trd lng="fr">policier</trd>
  <trd lng="nl">politieagent</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_c;&c_je;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>policajt</trd>,
    <trd>strážnik</trd>
  </trdgrp>
</drv>

<drv mrk="polic.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="polic.0ejo.POL">
    <dif>
      <tld lit="P"/>a administrejo gardanta distrikton:
      <ekz>
        mi rekte iros kun plendo, por ke oni prenu lin en la <tld/>ejon
        kaj tuj en la malliberejon
        <fnt><bib>Revizoro</bib>, <lok>akto 2a, sceno 2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Polizeirevier</trd>
  <trd lng="fr">commissariat de police</trd>
  <trd lng="nl">politiekantoor</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je; &c_p;&c_o;&c_l;&c_i;&c_c;&c_i;&c_i;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_a;&c_r;&c_i;&c_a;&c_t; &c_p;&c_o;&c_l;&c_i;&c_c;&c_i;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="polic.0estro">
  <kap><tld/>estro</kap>
  <snc mrk="polic.0estro.POL">
    <dif>
      <ref tip="super" cel="polic.0ano.POL"><tld lit="P"/>ano</ref>,
      &ccirc;efulo de <tld/>ejo a&ubreve; de <tld/>a departemento:
      <ekz>
        nia urbo havas <klr>...</klr> ne sufi&ccirc;e energian <tld/>estron
        <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Polizeikommissar</trd>
  <trd lng="fr">commissaire de police</trd>
  <trd lng="nl">commisaris</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_i;&c_c;&c_m;&c_je;&c_j;&c_s;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_a;&c_r; &c_p;&c_o;&c_l;&c_i;&c_c;&c_i;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_k; &c_o;&c_t;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">náčelník polície</trd>
</drv>

<drv mrk="polic.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="polic.0isto.POL">
    <dif>
      <ref tip="super" cel="polic.0ano.POL"><tld lit="P"/>ano</ref>,
      precipe tiu, kiu laboras enkete, garde, helpe por la publiko.
      <ekz>
        aperis en la hotelo kvara gasto, <klr>...</klr> kriminala
        <tld/>isto el Parizo
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Polizist</trd>
  <trd lng="fr">policier</trd>
  <trd lng="nl">politiebeambte</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_c;&c_je;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>policajt</trd>,
    <trd>strážnik</trd>
  </trdgrp>
</drv>

<drv mrk="polic.kasx0ano">
  <kap>ka&scirc;<tld/>ano</kap>
  <snc>
    <refgrp tip="vid">
      Vd rimarkon &ccirc;e
      <ref cel="detekt1.0o.RIM">detektivo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>stille <ind>politieagent</ind></trd>,
    <trd><ind>politieagent</ind> in burger</trd>,
    <trd>geheime <ind>politieagent</ind></trd>,
    <trd>detective</trd>
  </trdgrp>
</drv>

<drv mrk="polic.kontrauxtumulta0o">
  <kap>kontra&ubreve;tumulta <tld/>o</kap>
  <snc mrk="polic.kontrauxtumulta0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <tld lit="P"/>o taskata pri sekurigo de publikaj lokoj
      okaze de manifestacio, ribelo, tumulto, eventuale per uzo
      de forto:
      <ekz>
        la kontra&ubreve;tumulta <tld/>o <klr>...</klr> arestis kelkajn homojn 
        kaj evakuis iom perforte la konstrua&jcirc;on
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://www.eventeo.net/web/index.php/euxropo/politiko/264-usona-ambasadejo-bruligita">
          Usona ambasadejo bruligita</url></vrk>,
          <lok>eventeo, 2008-02-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>CRS</trd>,
    <trd>police anti&eacute;meute</trd>,
  </trdgrp>
</drv>
</art>
