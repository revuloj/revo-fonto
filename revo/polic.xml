<drv mrk="polic.kasx0ano">
  <kap>ka&scirc;<tld/>ano, <var><kap>ka&scirc;<tld/>isto</kap></var></kap>
  <snc>
    <dif>
      <tld lit="P"/>ano persekutanta krimulojn sen uniformo:
      <ekz>
        <klr>[li]</klr> anka&ubreve; en la ekzilo restis kritikanto de la nazioj nesciante ke
        ka&scirc;<tld/>ano observadis lin
        <fnt>
          <vrk><url ref="&Viki;/Carl_Lampert">Carl Lampert</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>&Scirc;vejk</nom> kaj la ka&scirc;<tld/>isto
        <fnt>
          <vrk><url
          ref="https://www.kava-pech.cz/procvicujsiesperanto/sites/citanka/citanka.html">&Ccaron;&iacute;tanka -
          Procvi&ccaron;uj si esperanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="persek.0isto">persekutisto</ref>,
       <ref cel="spion.0o.MIL">spiono<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      Vd rimarkon &ccirc;e
      <ref cel="detekt1.0o.RIM">detektivo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_c;&c_y;&c_ja;&c_n;&c_t; &c_u; &c_c;&c_y;&c_v;&c_ib;&c_l;&c_mol;&c_n;&c_y;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Zivilstreife</trd>,
  </trdgrp>
