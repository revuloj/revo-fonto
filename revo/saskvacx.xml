<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: saskvacx.xml,v 1.6 2023/12/02 21:23:09 revo Exp $">
<kap>
    <rad>saskva&ccirc;</rad>/o
</kap>
<drv mrk="saskvacx.0o">
  <kap><tld/>o</kap>
  <snc mrk="saskvacx.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
       La&ubreve; indianaj legendoj, <ref tip="super" cel="est.0ajxo.ulo">esta&jcirc;o</ref> loganta en arbaroj,
       kiu
       similas grandan simion a&ubreve; sova&gcirc;an vilan homon:
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> estas usona tipo de ne&gcirc;viro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=52331"
              >Saskva&ccirc;o</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        en siaj publikigitaj taglibroj <nom>Thompson</nom> notis ke li vidis kelkajn grandajn
        piedsignojn <klr>[&#8230;]</klr>, oni sugestis ke &ccirc;i tiuj piedsignoj estis de
        <klr>[&#8230;]</klr> <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=122155"
              >David Thompson (esploristo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="jeti.0o">jetio</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_s;&c_k;&c_v;&c_a;&c_ch;</trd>,
    <trd>&c_b;&c_ib;&c_g;&c_f;&c_u;&c_t;</trd>,
    <trd>&c_s;&c_mol;&c_n;&c_je;&c_zh;&c_n;&c_y; &c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">Peus Grans</trd>
  <trdgrp lng="de">
    <trd>Sasquatch</trd>,
    <trd>Bigfoot</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Sasquatch</trd>
    <trd>Bigfoot</trd>
  </trdgrp>
  <trd lng="fr">Sasquatch</trd>
  <trd lng="it">bigfoot</trd>
  <trd lng="pl">Wielka Stopa</trd>
</drv>
</art>

