<drv mrk="mev.trifingra0o">
  <kap>trifingra <tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      Specioj (<trd lng="la">Rissa tridactyla</trd>)
      de <ref tip="malprt" cel="lared.0oj">laredoj</ref> sen posta
      piedfingro:
      <ekz>
        estas du rasoj de Nigrokrura trifingra <tld/>o: <klr>[unu]</klr> en la Norda
        Atlantiko, kiu estas la ununura mevo inter &ccirc;iuj el la Laredoj kiu havas nur
        tre etan a&ubreve; e&ccirc; nenian malanta&ubreve;an fingron <klr>[kaj alia]</klr> en la Norda
        Pacifiko, kiu havas normalan malanta&ubreve;an fingron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22445">Trifingra mevo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kiti.0o">kitio, kitivako</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/0/04/Kittywake_(Rissa_tridactyla)%2C_Svalbard.jpg/320px-Kittywake_(Rissa_tridactyla)%2C_Svalbard.jpg" prm="CC BY-SA 4.0">trifingra <tld/>o (Rissa tridactyla) &ccirc;e Svalbard
        <fnt>
          <aut>AWeith, propra verko</aut>
          <lok><url ref="&WCW;/File:Kittywake_(Rissa_tridactyla),_Svalbard.jpg">&WCW;/File:Kittywake_(Rissa_tridactyla),_Svalbard.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="be">
      <trd>&c_k;&c_ib;&c_r;&c_l;&c_ja; &c_t;&c_r;&c_o;&c_h;&c_p;&c_a;&c_l;&c_mol;&c_c;&c_a;&c_ja;</trd>,
      <trd>&c_m;&c_y;&c_je;&c_w;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">gavineta de tres dits</trd>
    <trd lng="en">kittywake</trd>
    <trd lng="pl">mewa tr&oacute;jpalczasta</trd>
    <trd lng="sv">tret&aring;ig <ind>m&aring;s</ind></trd>
  </snc>
</drv>

