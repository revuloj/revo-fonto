<art mrk="$Id: anorma.xml,v 1.25 2026/01/29 10:56:56 revo Exp $">
<kap>
  <rad>anormal</rad>/a
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="anorma.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="normal.ne0a">Nenormala</ref>, <ref
      cel="norm.kontraux0a">eksternorma</ref>:
      <ekz>
        originala am-rakonto inter 30-jara viro kaj 11-jara knabineto ... konformistoj
        kaj idiotoj opinios &gcirc;in <tld/>a fiamo &ndash; e&ccirc; se mi tenis min sufi&ccirc;e konformista
        ene de la heteroseksa kampo
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2003/007837.html">Ne al &ccirc;iuj pla&ccirc;os &ccirc;i literatura
          gemeto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="anomal.0a">anomalia</ref>
    </refgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke><ref cel="normal.ne0a">nenormala</ref></ke>.
  </rim>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_n;&c_a;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_n;&c_a;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_n;&c_a;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>abnorm&aacute;ln&iacute;</trd>,
    <trd>nenorm&aacute;ln&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unnormal</trd>,
    <trd>anormal</trd>,
    <trd>ungew&ouml;hnlich</trd>
  </trdgrp>
  <trd lng="en">abnormal</trd>
  <trd lng="es">anormal</trd>
  <trd lng="fr">anormal</trd>
  <trd lng="he">&lamed;&#1465;&alef; &nun;&vav;&#1465;&resh;&mem;&#1464;&lamed;&#1460;&yod;</trd>
  <trd lng="hu">abnorm&aacute;lis</trd>
  <trd lng="ja">&#30064;&#24120;&#12394;  <pr>&#12356;&#12376;&#12423;&#12358;&#12394;</pr></trd>
  <trd lng="nl">abnormaal</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_n;&c_o;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_n;&c_o;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_n;&c_o;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">nenorm&aacute;lny</trd>
  <trd lng="uk">&c_a;&c_n;&c_o;&c_r;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
