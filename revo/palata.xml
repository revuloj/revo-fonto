<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>palatal</rad>/o</kap>
<drv mrk="palata.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      Konsonanto a&ubreve; kvaza&ubreve;vokalo, kies brueron naskas fermo a&ubreve; malvastigo
      de bu&scirc;kanalo per alproksimigo de langa dorso al palato (j, nj, &scirc;, &jcirc;,
      k, g).
    </dif>
    <refgrp tip="sin">
      <ref cel="velar.0o">velaro</ref>,
      <ref cel="labial.0o">labialo</ref>,
      <ref cel="dental.0o">dentalo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_g;&c_u;&c_k;</ind></trd>
  <trdgrp lng="de">
    <trd>Palatal</trd>,
    <trd>Gaumenlaut</trd>
  </trdgrp>
  <trd lng="es">palatal</trd>
  <trdgrp lng="hu">
    <trd>&iacute;nyhang</trd>,
    <trd>palat&aacute;lis hang</trd>
  </trdgrp>
  <trd lng="nl">palataal</trd>
  <trdgrp lng="pt">
    <trd>palatal</trd>,
    <trd>consoante <ind>palatal</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_l;&c_a;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>,
    <trd>&c_n;&c_jo;&c_b;&c_n;&c_y;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>
  </trdgrp>
</drv>
</art>
