<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>embriologi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="embrio2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc>
    <dif>
      Scienco, parto de <ref tip="malprt" cel="biolog1.xml">biologio</ref>, kiu pritraktas la disvolvi&gcirc;on de organismoj ekde ilia
      stato de <ref tip="vid" cel="embri.0o">embrio</ref> &gcirc;is ilia naski&gcirc;o.
    </dif>
  </snc>
  <trd lng="fr">embryologie</trd>
  <trd lng="ru">&c_e;&c_m;&c_b;&c_r;&c_i;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="embrio2.0isto">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fakulo pri <tld/>o.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="embrio1.0o">embriologo</ref>
  </refgrp>
  <trd lng="fr">embryologue</trd>
  <trd lng="ru">&c_e;&c_m;&c_b;&c_r;&c_i;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
