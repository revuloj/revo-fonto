<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&ccirc;igong</rad>/o
</kap>
<drv mrk="cxigong.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;indevena mensgvida kaj gimnastika metodo por favori sanon
      per malstre&ccirc;i&gcirc;o, profunda spirado, sintenoj, sinmovoj
      kaj procedoj diversaj la&ubreve; la skoloj:
      <ekz>
        estas diversaj <tld/>-ekzercoj, inter kiuj la &ccirc;efaj estas teni&gcirc;o,
        spirado, memmasa&gcirc;o kaj movi&gcirc;o de membroj<fnt><bib>EncCxi</bib>
        <lok>&ccirc;ap. &leftquot;&Ccirc;igong-o&rightquot;, p. 282a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>chi gong</trd>,
    <trd>qigong</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>qi-gong</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csi-kung</trd>,
    <trd>qigong</trd>
  </trdgrp>
</drv>
</art>

