<art mrk="$Id: acetat.xml,v 1.27 2023/10/12 16:57:42 revo Exp $">
<kap>
  <rad>acetat</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="acetat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sal.0o.KEM">Salo</ref>
      el vinagra acido kaj bazo:
      <ekz>
        natria <tld/>o, <tld/>o de natrio a&ubreve; <frm>H<sub>3</sub>C-COO-Na</frm> estas neorganika
        natria salo de
        <tld/>a acido
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=531591">Natria acetato</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        celuloza <tld/>o respektive celuloza tri<tld/>o estas speco de plasto,
        produktita el celulozo pere de kemia reago kun aceta acido
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=284454">Celuloza acetato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_c;&c_e;&c_t;&c_a;&c_t;</trd>
  <trd lng="bg">&c_o;&c_c;&c_je;&c_t;&c_n;&c_a; &c_k;&c_i;&c_s;&c_je;&c_l;&c_i;&c_n;&c_a;</trd>
  <trd lng="ca">acetat</trd>
  <trdgrp lng="cs">
    <trd>acet&aacute;t</trd>,
    <trd>octan</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Essigs&auml;uresalz</trd>,
    <trd>Azetat</trd>
  </trdgrp>
  <trd lng="en">acetate</trd>
  <trd lng="es">acetato</trd>
  <trd lng="fa">&a_A;&a_s;&a_t;&a_A;&a_t;</trd>
  <trd lng="fr">ac&eacute;tate</trd>
  <trd lng="he">&#x5D0;&#x5B2;&#x5E6;&#x5B5;&#x5D8;&#x5B7;&#x5D8;</trd>
  <trd lng="hu">acet&aacute;t</trd>
  <trd lng="it">acetato</trd>
  <trd lng="ja">&#37218;&#37240;&#22633;  <pr>&#12373;&#12367;&#12373;&#12435;&#12360;&#12435;</pr></trd>
  <trd lng="nl">acetaat</trd>
  <trd lng="pl">octan</trd>
  <trd lng="pt">acetato</trd>
  <trd lng="ru">&c_a;&c_c;&c_je;&c_t;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>acet&aacute;t</trd>,
    <trd>octan</trd>
  </trdgrp>
  <trd lng="sv">acetat</trd>
</drv>
</art>
