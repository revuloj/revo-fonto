<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>paradize</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="paradize.0o">
  <kap><tld/>o</kap>
  <snc mrk="paradize.0o.SNC">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="bird.paradiz0o">Paradizbirdo</ref>
    </dif>
  </snc>
  <trdgrp lng="fr"><trd>paradisier</trd><trd>oiseau de
  paradis</trd></trdgrp>
</drv>
<drv mrk="paradize.P0o">
  <kap><tld lit="P"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt></kap>
  <mlg>Aps</mlg>
  <snc mrk="paradize.P0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro"
      lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Apus</trd>).
    </dif>
  </snc>
  <trd lng="en">Apus</trd>
  <trd lng="fr">Oiseau de Paradis<klr tip="ind">
  (constellation)</klr></trd>
</drv>
</art>

