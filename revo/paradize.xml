<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: paradize.xml,v 1.3 2018/11/18 19:10:15 revo Exp $">
<kap>
    <rad>paradize</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="paradize.0o">
  <kap><tld/>o</kap>
  <snc mrk="paradize.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <ref tip="dif" cel="bird.paradiz0o">paradizbirdo</ref>
    <trd lng="de">Paradiesvogel</trd>
  </snc>
  <trdgrp lng="fr"><trd>paradisier</trd><trd>oiseau de
  paradis</trd></trdgrp>
</drv>
<drv mrk="paradize.P0o">
  <kap><tld lit="P"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt></kap>
  <mlg>Aps</mlg>
  <snc mrk="paradize.P0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Apus</trd>):
      <ekz>
        NGC 5612 estas galaksio en konstelacio <tld lit="P"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=118677">NGC 5612</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Paradiesvogel <klr>(Sternbild)</klr></trd>
  </snc>
  <trd lng="en">Apus</trd>
  <trd lng="fr">Oiseau de Paradis<klr tip="ind">
  (constellation)</klr></trd>
</drv>
</art>

