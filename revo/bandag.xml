<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bandag.xml,v 1.24 2023/10/14 08:39:48 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>banda&gcirc;</rad>/o
</kap>
<drv mrk="bandag.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Longa tola <ref tip="super" cel="stri.0o.peco">strio</ref> por 
      &ccirc;irka&ubreve;volvi a&ubreve; senmovigi vunditan korpoparton:
      <ekz>
        la vundoj <klr>[&#x2026;]</klr> tu&scirc;etis nur la ha&ubreve;ton, dank&#x2019; al konvenaj <tld/>oj ili
        rapide cikatri&gcirc;is
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj manoj tremis kaj li ne povis ligi la <tld/>on
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        se &scirc;i ne restos &ccirc;i tie, tiam per tiu &ccirc;i sana mano mi de&scirc;iros la <tld/>ojn de la
        brako, akceptos nek man&gcirc;on, nek trinkon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne sufi&ccirc;as la pin&ccirc;ilo, medikamento kaj <tld/>o, la &ccirc;efafero estas la animo
        <fnt>
          <bib>VaK</bib>
          <lok>Kvina &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="zon.0o">zono</ref>,
       <ref cel="bend.0o">bendo</ref>,
       <ref cel="lig.cxirkaux0i">&ccirc;irka&ubreve;liga&jcirc;o</ref>,
       <ref cel="pans.0ajxo">pansa&jcirc;o</ref>
     </refgrp>
   </snc>
   <trd lng="bo">&t_ra;&t_mata;&t_tsheg;&t_ra;&t_sa;&t_tsheg;</trd>
  <trdgrp lng="ca">
     <trd>bena</trd>,
     <trd>embenat</trd>,
     <trd>embenatge</trd>
   </trdgrp>
   <trdgrp lng="be">
     <trd>&c_b;&c_a;&c_n;&c_d;&c_a;&c_zh;</trd>,
     <trd>&c_b;&c_ib;&c_n;&c_t;</trd>,
     <trd>&c_p;&c_a;&c_v;&c_ja;&c_z;&c_k;&c_a;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Verband</trd>,
     <trd>Binde</trd>,
     <trd>Bandage</trd>
   </trdgrp>
   <trd lng="en">bandage</trd>
   <trdgrp lng="es">
     <trd>venda</trd>,
     <trd>vendaje</trd>
   </trdgrp>
   <trdgrp lng="fa">
     <trd>&a_n;&a_w;&a_A;&a_r; &a_z;&a_kh;&a_m;&zwnj;&a_b;&a_n;&a_d;&f_y;</trd>,
     <trd>&a_t;&a_n;&a_z;&f_y;&a_b;</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>bande<klr tip="ind"> (bandage)</klr></trd>,
     <trd>bandage</trd></trdgrp>
   <trd lng="he">&tav;&#x5B7;&het;&#x5B0;&bet;&#x5BC;&vav;&#x5B9;&shin;&#x5C1;&#x5B6;&tav;</trd>
  <trdgrp lng="hu">
     <trd>k&ouml;t&eacute;s<klr tip="ind"> (seben)</klr></trd>,
     <trd>k&ouml;tszer</trd></trdgrp>
    <trdgrp lng="it">
     <trd>benda<klr> (bendaggio)</klr></trd>,
     <trd>fascia<klr> (bendaggio)</klr></trd>,
     <trd>bendaggio<klr> (fascia, benda)</klr></trd></trdgrp>
   <trd lng="ja">&#x5305;&#x5E2F;  <pr>&#x307B;&#x3046;&#x305F;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>verband</trd>,
    <trd>bandage</trd>
  </trdgrp>
  <trd lng="pl">banda&zdot;</trd>
  <trdgrp lng="ru">
     <trd>&c_b;&c_a;&c_n;&c_d;&c_a;&c_zh;</trd>,
     <trd>&c_p;&c_o;&c_v;&c_ja;&c_z;&c_k;&c_a;</trd>,
     <trd>&c_b;&c_i;&c_n;&c_t;</trd>
   </trdgrp>
  <trd lng="sk">obv&auml;z</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_n;&c_d;&c_a;&c_zh;</trd>,
    <trd>&c_p;&c_o;&c_v;&#x2019;&c_ja;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_i;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
<drv mrk="bandag.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Garni per <tld/>o:
      <ekz>
        malsanan vi ne kuracas, vunditan vi ne <tld/>as, <klr>[&#x2026;]</klr> vi regas super
        ili maldelikate kaj kruele
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 34:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patrino <tld/>is al li la brakon, la vundo estis profunda
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la sojlo aperis <nom>Sara</nom> kun <tld/>ita kapo, apogita sur la <klr>[&#x2026;]</klr>
        servistino
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        permesu, mi nur <tld/>os vian piedon, via vundo ne estas dan&gcirc;era, tamen oni
        devas haltigi la sangon
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pans.0i">pansi</ref>,
      <ref cel="vind.0i">vindi</ref>
    </refgrp>
  </snc>
   <trdgrp lng="be">
     <trd>&c_b;&c_ib;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_ja;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
   <trd lng="bo">&t_ra;&t_mata;&t_tsheg;&t_ra;&t_sa;&t_tsheg;&t_sa;&t_bata;&t_yata;&t_ra;&t_tsheg;</trd>
  <trd lng="ca">embenar</trd>
   <trdgrp lng="de">
     <trd>verbinden <klr>(Wunde)</klr></trd>,
     <trd>bandagieren</trd>
   </trdgrp>
   <trd lng="en">bandage</trd>
   <trd lng="es">vendar</trd>
   <trd lng="fa">&a_b;&a_A;&a_n;&a_d;&f_p;&f_y;&f_ch;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
   <trd lng="fr">bander<klr tip="ind"> (avec une bande)</klr></trd>
   <trd lng="he">&lamed;&het;&bet;&vav;&shin;</trd>
  <trd lng="hu">bek&ouml;t&ouml;z<klr tip="ind"> (sebet)</klr></trd>
   <trdgrp lng="it">
     <trd>bendare</trd>,
     <trd>fasciare<klr> (bendare)</klr></trd>
   </trdgrp>
   <trd lng="ja">&#x5305;&#x5E2F;&#x3059;&#x308B;  <pr>&#x307B;&#x3046;&#x305F;&#x3044;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="nl">
     <trd>verbinden<klr tip="ind"> (omzwachtelen)</klr></trd>,
     <trd>omzwachtelen</trd>,
     <trd>omwikkelen</trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>banda&zdot;owa&cacute;</trd>,
     <trd>obanda&zdot;owywa&cacute;</trd>,
     <trd>obanda&zdot;owa&cacute;</trd>,
     <trd>zabanda&zdot;owywa&cacute;</trd>,
     <trd>zabanda&zdot;owa&cacute;</trd>
   </trdgrp>
   <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_ja;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
   <trd lng="sk">obviaza&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&#x2019;&c_ja;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_p;&c_je;&c_r;&c_je;&c_v;&#x2019;&c_ja;&c_z;&c_k;&c_u;</trd>
  </trdgrp>
</drv>
</art>
