<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>banda&gcirc;</rad>/o
</kap>
<drv mrk="bandag.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Longa tola strio por &ccirc;irka&ubreve;volvi a&ubreve; senmovigi
      vunditan korpoparton:
      <ekz>
        hernia <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="zon.0o">zono</ref>,
      <ref cel="lig">&ccirc;irka&ubreve;liga&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de"><trd>Verband</trd>, <trd>Binde</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_n;&c_d;&c_a;&c_zh;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_ja;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_i;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
<drv mrk="bandag.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Garni per <tld/>o:
      <ekz>
        <tld/>i vundojn
        <fnt>(<vrk>Nova testamento</vrk>,
        <lok>Luko 10, 34</lok>)</fnt>;
      </ekz>
      <ekz>
        <tld/> vunditon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/> la piedon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        Sara aperis kun <tld/>ita kapo
        <fnt>(<aut>Kabe</aut>: <vrk>La Faraono, 1a vol.</vrk>,
        <lok>&ccirc;ap. 10</lok>)</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="vind.0i">vindi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">verbinden</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_v;&c_ja;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
