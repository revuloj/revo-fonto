<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fenici.xml,v 1.18 2023/11/09 19:13:16 revo Exp $">
<kap>
  <rad>Fenici</rad>/o <fnt><bib>Far1</bib></fnt>,
  <var><kap><rad var="k">Feniki</rad>/o  <fnt><bib>NT</bib></fnt></kap></var>
</kap>
<drv mrk="fenici.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <snc mrk="fenici.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="dif" cel="fenic.0ujo">Fenicujo</ref>:
      <ekz>
        ili do, forkondukite de la eklezio, trairis Fenikion kaj Samarion
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 15:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        se <tld/>o havus unu komunan estron, kiu scius, kio okazas en &ccirc;iuj urboj kaj
        komandus en ili, &ccirc;i tiu lando ne pagus al ni e&ccirc; unu utenon da tributo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiom da ri&ccirc;a&jcirc;oj posedas <tld/>o, &ccirc;ion ni metos anta&ubreve; viaj piedoj
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 6a</lok></fnt>;
      </ekz>
      <ekz>
        la urbo estis malgranda antikva komercejo de <tld/>o konkerita de Antikva Romo
        <fnt>
          <vrk><url ref="https://eo.advisor.travel/poi/Tipasa-25760">Tipasa in Tipasa</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_F;&c_ib;&c_n;&c_ib;&c_k;&c_ib;&c_ja;</trd>
  <trd lng="ca">Fen&iacute;cia</trd>
  <trd lng="cs">F&eacute;nicie</trd>
  <trd lng="de">Ph&ouml;nizien</trd>
  <trd lng="es">Fenicia</trd>
  <trd lng="fr">Ph&eacute;nicie</trd>
  <trd lng="hu">F&ouml;n&iacute;cia</trd>
  <trd lng="ja">&#x30D5;&#x30A7;&#x30CB;&#x30AD;&#x30A2;</trd>
  <trd lng="pl">Fenicja</trd>
  <trd lng="pt">Fen&iacute;cia</trd>
  <trd lng="ru">&c_F;&c_i;&c_n;&c_i;&c_k;&c_i;&c_ja;</trd>
  <trd lng="sk">Fen&iacute;cia</trd>
  <trd lng="uk">&#x424;&#x456;&#x43D;&#x456;&#x43A;&#x456;&#x44F;</trd>
</drv>

<drv mrk="fenici.0ano">
  <kap>
    <tld/>ano
  </kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <tld/>o:
      <ekz>
        ili min ne komprenas, &ccirc;i tiuj transmaraj barbuloj <klr>[&#x2026;]</klr> fratoj de
        <tld/>anoj kaj Hebreoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="fenic.0o">fenico</ref>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_n;&c_ib;&c_k;&c_ib;&c_je;&c_c;</trd>
  <trd lng="de">Ph&ouml;nizier</trd>
  <trdgrp lng="zh">
    <trd>&#x8153;&#x5C3C;&#x57FA;&#x4EBA; <pr>f&#xE9;in&#xED;j&#x12B;r&#xE9;n</pr></trd>,
    <trd>&#x8153;&#x5C3C;&#x57FA; <pr>f&#xE9;in&#xED;j&#x12B;</pr></trd>
  </trdgrp>
</drv>




</art>
