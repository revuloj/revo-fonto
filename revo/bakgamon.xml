<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bakgamon.xml,v 1.6 2024/03/04 21:19:28 revo Exp $">
<kap>
  <rad>bakgamon</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="bakgamon.0o">
  <kap><tld/>o</kap>
  <snc mrk="bakgamon.0o.SPO">
    <dif>
      <ref tip="dif" cel="triktrak.0o.SPO">Triktrako</ref>:
      <ekz>
        la&ubreve; ilia studo la &Ccirc;ina &scirc;ako kaj <tld/>o amba&ubreve; devenas el la pli malnova
        &Ccirc;ina ludo <ctl>Liubo</ctl> de anta&ubreve; 3500 jaroj
        <fnt>
          <aut>E. Friedlander, trad. G. D&eacute;nes</aut>,
          <vrk><url ref="http://www.ludanto.org/sxako/cxina_sxako.shtml">&Ccirc;ina
          &scirc;ako</url></vrk>,
          <lok>2006-08-20</lok>
        </fnt>;
      </ekz>
      <ekz>
        la magazino enhavis nova&jcirc;ojn kaj enigmojn pri klasikaj ludoj (&scirc;ako, goo,
        <tld/>o, damludo, skrablo...)
        <fnt>
          <vrk><url ref="https://dominiko.livejournal.com/12841.html">Polg&aacute;r fratinoj kaj Esperanto
          - Ur yezh hepken n'eo ket a ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <!-- prefere traduku nur &ccirc;e triktrako -->
  <trd lng="be">&c_t;&c_r;&c_y;&c_k;&c_t;&c_r;&c_a;&c_k;</trd>
  <trd lng="en">backgammon</trd>
  <trd lng="uk">&c_t;&c_r;&c_i;&c_k;&c_t;&c_r;&c_a;&c_k;&nbsp;(&c_g;&c_r;&c_a;)</trd>
</drv>
</art>

