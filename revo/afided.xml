<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: afided.xml,v 1.15 2022/03/11 12:15:12 revo Exp $">
<kap>
  <rad>afided</rad>/oj
</kap>
<drv mrk="afided.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="hemipt.0oj">hemipteroj</ref>
      (<trd lng="la">Aphididae</trd>), su&ccirc;antaj la sukon de plantoj:
      <ekz>
        <ref tip="prt" cel="afid.0o.genro">afido</ref>
        estas malgranda insekto el la ordo de hemipteroj, <klr>[&#x2026;]</klr>
        familio de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=209566">Afido</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        junkokanbirdo ser&ccirc;as lokojn kun granda nombro de <tld/>oj kaj
        restas ne tre for de tiaj lokoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=307695">Junkokanbirdo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_l;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_l;&c_i;&c_s;&c_t;&c_n;&c_i; <ind>&c_v;&c_malmol;&c_sh;&c_k;&c_i;</ind></trd>
  <trd lng="ca">af&iacute;dids</trd>
  <trd lng="de">R&ouml;hrenl&auml;use</trd>
  <trd lng="en">Aphididae</trd>
  <trdgrp lng="es">
    <trd>af&iacute;didos</trd>,
    <trd>&aacute;fidos</trd>,
    <trd>Aphididae</trd>
  </trdgrp>
  <trd lng="he">&#x5DB;&#x5E0;&#x5D9;&#x5DE;&#x5D5;&#x5EA;</trd>
  <trd lng="hu">igazi
    <ind>lev&eacute;ltet&udblac;f&eacute;l&eacute;k</ind></trd>
  <trd lng="it">afididi</trd>
  <trd lng="nl">bladluizen</trd>
  <trd lng="pt">afid&iacute;deos</trd>
  <trd lng="fr">aphidiens</trd>
  <trd lng="ru">&c_t;&c_l;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43F;&#x435;&#x43B;&#x438;&#x446;&#x435;&#x432;&#x456;</trd>
</drv>
</art>
