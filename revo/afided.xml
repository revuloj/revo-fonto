  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt"
      cel="homopt.0oj">homopteroj</ref>
      (<trd lng="la">Aphididae</trd>), su&ccirc;antaj la sukon de plantoj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_l;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_l;&c_i;&c_s;&c_t;&c_n;&c_i; <ind>&c_v;&c_malmol;&c_sh;&c_k;&c_i;</ind></trd>
  <trd lng="de">R&ouml;hrenl&auml;use</trd>
  <trd lng="en">Aphididae</trd>
  <trd lng="hu">igazi
<ind>lev&eacute;ltet&udblac;f&eacute;l&eacute;k</ind></trd>
  <trd lng="it">afididi</trd>
  <trd lng="nl">bladluizen</trd>
  <trd lng="pt">afid&iacute;deos</trd>
  <trd lng="fr">aphidiens</trd>
  <trd lng="ru">&c_t;&c_l;&c_i; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
