<art mrk="$Id: antigen.xml,v 1.10 2023/10/12 18:05:09 revo Exp $">
<kap>
  <rad>antigen</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="antigen.0o">
  <kap><tld/>o</kap>
  <snc mrk="antigen.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Proteino a&ubreve; alia substanco, kiu ka&ubreve;zas imunigan
      reagon
      de viva korpo per responda
      <ref tip="vid" cel="antikorp.0o.FIZL">antikorpo</ref>:
      <ekz>
        T-&ccirc;ela <tld/>o
        <fnt>
          <aut>A. Lucas</aut>,
          <vrk><url ref="http://www.teleskopo.com/ekzemplo_de_genetika_terapio_en_francujo.pdf">
          Ekzemplo de genetika Terapio...</url></vrk>,
          <lok>Teleskopo, 2010 (2)</lok>
        </fnt>;
        </ekz>
        <ekz><tld/>oj estas molekuloj, kiuj estas eksponataj sur la surfaco de &ccirc;elo: ili estas
        iaspeca &leftquot;etikedo&#8221;
        <fnt>
        <aut>Stefano Moroso</aut>
        <vrk><url ref="&Monato;/2023/013357.php">Kiom da vero pri mRNA-vakcinoj kontra&ubreve;
        kancero?</url></vrk>
        <bib>Monato</bib>
        <lok>2023/08-09, p. 27</lok>
        </fnt>
      </ekz>
    </dif>
    <trd lng="fr">antig&egrave;ne</trd>
    <trd lng="it">antigene</trd>
    <trd lng="nl">antigeen</trd>
    <trd lng="pl">antygen</trd>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_g;&c_je;&c_n;</trd>
  <trd lng="cs">antigen</trd>
  <trd lng="ja">&#25239;&#x539F;  <pr>&#12371;&#12358;&#12370;&#12435;</pr></trd>
  <trd lng="sk">antig&#xE9;n</trd>
  <trd lng="uk">&c_a;&#x43D;&c_t;&c_i;&c_g;&c_je;&#x43D;</trd>
</drv>
</art>

