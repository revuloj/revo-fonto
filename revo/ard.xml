  <ofc>3</ofc>
  <rad>ard</rad>/i
</kap>

<drv mrk="ard.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ard.0i.brulegi">
    <dif>
      Videble, senflame elradii <ref tip="prt" cel="lum.0o.FIZ">lumo</ref>n
      pro varmego:
      <ekz>
        <tld/>anta forno
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 3:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        li prenos plenan incensujon da <tld/>antaj karboj de la altaro
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 16:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u iu povas mar&scirc;i sur <tld/>antaj karboj, ne bruligante siajn piedojn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 6:28</lok>
        </fnt>?
      </ekz>
      <ekz>
        kiam la varmega cindro ku&scirc;is alte kaj <tld/>ante, oni bakadis en &gcirc;i la panon
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        por havi bonan odoron, oni metas <klr>[la rozojn]</klr> sur <tld/>antan feran
        tabulon
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>anta fandoforno en la fabriko
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno prezenti&gcirc;is kiel <tld/>anta globo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="inkand.0i">inkandeski</ref>,
      <ref cel="flam.0i">flami</ref>.
    </refgrp>
  </snc>
  <snc mrk="ard.0i.brili">
    <dif>
      <ref tip="super" cel="bril.0i.lumo">Brili</ref> per intensa koloro kvaza&ubreve; &gcirc;i <tld/>as:
      <ekz>
        la nuboj ru&gcirc;e <tld/>is
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rozo <tld/>is pli bele ol anta&ubreve;e
        <fnt>
          <bib>Fab2</bib>
          <lok>Rozo de la tombo de Homero</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>antaj vangoj de mia dol&ccirc;a infano
        <fnt>
          <bib>Fab3</bib>
          <lok>Plej bela rozo de la mondo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suproj de la montoj <tld/>is en la ru&gcirc;a vespera suno
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>

  <snc mrk="ard.0i.senti">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti varmega:
      <ekz>
        la sablo elspiris <tld/>antan varmegon de malsupre
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen mi <tld/>as de varmego, jen mi tremas de frosto
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno &jcirc;etadis sur la arbon siajn plej <tld/>antajn radiojn
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ard.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti tre <ref cel="pasi.0a">pasia</ref>,
      tre <ref cel="fervor.0a">fervora</ref>, ekstreme viveca:
      <ekz>
        la mallumaj okuloj esprimadis <tld/>antajn pensojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro <klr>[&#8230;]</klr> <tld/>a intereso al romano
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9506-kiso.txt">de Shi
          Chengtai</url></vrk>
          <lok>95/06</lok>
        </fnt>;
      </ekz>
      <ekz>
        la paseo <klr>[&#8230;]</klr> tute sinkis en la <tld/>an kisadon
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9506-kiso.txt">de Shi
          Chengtai</url></vrk>
          <lok>95/06</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>antaj lipoj de <nom>Emil</nom> tu&scirc;is &scirc;iajn brovojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007215.html">La peko de Ros</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bol.0i">boli</ref>,
      <ref cel="febr.0i.anime">febri</ref>,
      <ref cel="fervor.0i">fervori</ref>.
    </refgrp>
    <trd lng="en">be ardent</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>planout</trd>,
    <trd>&zcaron;hnout</trd>
  </trdgrp>
  <trd lng="de">gl&uuml;hen</trd>
  <trd lng="en">glow</trd>
  <trd lng="es">arder</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>ardent</ind></trd>,
    <trd>&ecirc;tre <ind>incandescent</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5DC;&#x5D4;&#x5D5;&#x5D8;</trd>
  <trd lng="hu">izzik</trd>
  <trd lng="ru">&c_p;&c_y;&c_l;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>&zcaron;iari&tcaron;</trd>,
    <trd>plan&uacute;&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">gl&ouml;da</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&#x43B;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_a;&#x43B;&c_a;&c_h;&#x43A;&#x43E;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_g;&#x43E;&c_r;&c_ib;&c_t;&c_i; &c_s;&c_i;&#x43B;&#x44C;&#x43D;&c_i;&#x43C;
    &#x43F;&#x43E;&#x43B;&c_u;&#x43C;&#8217;&#x44F;&#x43C;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_r;&#x43E;&c_z;&#x43F;&c_je;&c_ch;&c_je;&#x43D;&c_i;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="ard.0a">
  <kap><tld/>a</kap>
  <snc mrk="ard.0a.varmega">
    <dif>
      Tia, ke &gcirc;i <tld/>as:
      <ekz>
        li ordonis <tld/>igi la fornon sepoble pli forte, ol oni faradis ordinare
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 3:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la metalo brilis <tld/>e en la ru&gcirc;aj radioj
        <fnt>
          <bib>Fab4</bib>
          <lok>Sonorila profundo</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="inkand.0a">inkandeska</ref>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_l;&c_k;&c_ib;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="en">glowing</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_y;&c_l;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_k;&c_a;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ard.0a.fervora">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pasia, fervora; tre viveca:
      <ekz>
        <tld/>a &ccirc;evalo, atako, intereso, atento, deziro;
      </ekz><ekz>
        <tld/>aj paroloj;
      </ekz><ekz>
        homo <tld/>a en la diskutado.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flam.0a.anime">flama</ref>,
      <ref cel="varm.0ega.anime">varmega</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_w;&c_z;&c_ja;&c_t;&c_y;</trd>,
      <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="en">ardent</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_y;&c_l;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_a;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>horouc&iacute;</trd>,
    <trd>vrouc&iacute;</trd>,
    <trd>&rcaron;e&rcaron;av&yacute;</trd>,
    <trd>&zcaron;&aacute;rn&yacute;</trd>
  </trdgrp>
  <trd lng="de">gl&uuml;hend</trd>
  
  <trd lng="es">ardiente</trd>
  <trdgrp lng="fr">
    <trd>ardent</trd>,
    <trd>chaud<klr tip="ind"> (de temp&eacute;rament)</klr></trd>,
    <trd>chauff&eacute; &agrave; blanc</trd>,
    <trd>incandescent</trd>,
    <trd>passionn&eacute;</trd>,
    <trd>torride</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5DC;&#x5D4;&#x5D5;&#x5D8;</trd>,
    <trd>&#x5E0;&#x5DC;&#x5D4;&#x5D1;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>izz&oacute;</trd>,
    <trd>par&aacute;zs<klr tip="amb"> (mell&eacute;kn&eacute;v)</klr></trd>
    <trd>par&aacute;zsl&oacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&zcaron;erav&yacute;</trd>,
    <trd>rozp&aacute;len&yacute;</trd>
  </trdgrp>
  <trd lng="sv">gl&ouml;dande</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&#x43B;&c_a;&c_h;&#x43A;&#x43E;&c_t;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&c_je;&c_ch;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_a;&#x43B;&#x43A;&c_i;&c_j;</trd>,
    <trd>&c_zh;&c_a;&c_g;&c_u;&c_ch;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&#x43F;&c_a;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_z;&#x44F;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="ard.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>anta:
      <ekz>
        karbo servas por <tld/>a&jcirc;o, ligno por fajro
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 26:21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Glut</trd>
  <trd lng="en">ember</trd>
  <trd lng="he">&#x5DC;&#x5B7;&#x5D4;&#x5B7;&#x5D8;</trd>
  <trdgrp lng="hu">
    <trd>par&aacute;zs</trd>,
    <trd>zsar&aacute;tnok</trd>
  </trdgrp>
  <trd lng="sv">gl&ouml;d</trd>
  <trd lng="es">brasa</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_a;&c_r;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&c_u;&#x43C;&#8217;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="ard.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="A"/>a elradiado:
      <ekz>
        kiu el ni eltenos &ccirc;e la eterna <tld/>ego
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 33:14</lok>
        </fnt>?
      </ekz>
      <ekz>
        Moskvo estis redonanta la <tld/>on akumulitan en la
        asfalto dum la tago
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 5a, p.
        48a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="inkand.0o">inkandesko;</ref>
    <refgrp tip="vid">
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="ekzalt">ekzalteco</ref>,
      <ref cel="fajr.0o.fervoro">fajro</ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="flam.0o.fervoro">flamo</ref>,
      <ref cel="pasi.0o">pasio</ref>,
      <ref cel="verv.0o">vervo</ref>,
      <ref cel="varm.0o">varmo</ref>,
      <ref cel="vigl">vigleco</ref>.
    </refgrp>
  </snc>
  <snc mrk="ard.0o.pasio">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pasio, granda fervoro:
      <ekz>
        tro granda <tld/>o de amo
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        li elver&scirc;is sur lin la <tld/>on de sia kolero
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 42:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidas tiujn brulantajn okulojn, tiun <tld/>on kaj kvaza&ubreve; animan soifon
        <fnt>
          <bib>Fab3</bib>
          <lok>Juda knabino</lok>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="en">ardor</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;</trd>,
    <trd>&c_z;&c_a;&c_w;&c_z;&c_ja;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>v&yacute;he&ncaron;</trd>,
    <trd>&zcaron;eh</trd>,
    <trd>&zcaron;&aacute;r</trd>
  </trdgrp>
  <trd lng="de">Gl&uuml;hen</trd>
  
  <trd lng="es">ardor</trd>
  <trdgrp lng="fr">
    <trd>ardeur</trd>,
    <trd>incandescence</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5DC;&#x5D4;&#x5D8;</trd>,
    <trd>&#x5D4;&#x5EA;&#x5DC;&#x5D4;&#x5D1;&#x5D5;&#x5EA;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_y;&c_l;</trd>,
    <trd>&c_zh;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_k;&c_a;&c_l;</trd>,
    <trd>&c_r;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vrelos&tcaron; <klr>(pren.)</klr></trd>,
    <trd>vr&uacute;cnos&tcaron;</trd>,
    <trd>&zcaron;eravos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#x43F;&c_a;&#x43B;</trd>,
    <trd>&c_zh;&c_a;&c_r;</trd>,
    <trd>&c_zh;&c_a;&c_g;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_z;&#x44F;&c_t;&c_t;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="ard.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      <tld lit="A"/>anta fajrejo:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la hinda duonkontinento estis konstanta <tld/>ejo de &hcirc;olero dum kelkaj jarmiloj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2010/010071.php">Antikva, sed ankora&ubreve; malica
          gasto</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="centr.0o">centro</ref>,
      <ref cel="fokus.0o">fokuso</ref>,
      <ref cel="kern.0o">kerno</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_g;&c_n;&c_ib;&c_sh;&c_ch;&c_a;</trd>,
    <trd>&c_a;&c_g;&c_n;&c_ib;&c_sh;&c_ch;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ohnisko</trd>,
    <trd>&zcaron;&aacute;rovi&scaron;t&ecaron;</trd>
  </trdgrp>
  <trd lng="de">Glutnest</trd>
  <trd lng="en">kiln</trd>
  <trd lng="fr">foyer</trd>
  <trd lng="sk">ohnisko</trd>
</drv>

<drv mrk="ard.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="ard.0ilo.gasmufo">
    <dif>
      <ref tip="dif" cel="gas.0mufo">Gasmufo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; <ind>&c_s;&c_je;&c_t;&c_k;&c_a;</ind></trd>
  <trd lng="de">Gl&uuml;hk&ouml;rper</trd>
  <trd lng="en">kiln</trd>
  <trd lng="fr">manchon<klr tip="ind"> (de flamme)</klr></trd>
  <trd lng="ru">&c_p;&c_je;&c_ch;&c_mol;<klr tip="amb"> (&c_t;&c_je;&c_h;.)</klr></trd>
</drv>

<drv mrk="ard.blank0a">
  <kap>blank<tld/>a</kap>
  <snc mrk="ard.blank0a.KOL">
    <dif>
      <ref tip="vid" cel="blank.0a">Blanka</ref> pro <tld/>o:
      <ekz>
        gas-sen&scirc;argaj lampoj anstata&ubreve;igas blank<tld/>ajn lumojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=512465">Gas-&scirc;argiga lampo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        oni kaptis kaj torturis lin, per <klr>[&#8230;]</klr> blank<tld/>a (inkandeska) krono,
        &gcirc;is morto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=108024">Guillaume Caillet</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_y; &c_d;&c_a;&c_b;&c_ja;&c_l;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>&zcaron;hav&yacute;</trd>,
    <trd>&zcaron;houc&iacute;</trd>
  </trdgrp>
  <trd lng="de">wei&szlig;gl&uuml;hend</trd>
  <trd lng="en">ashen</trd>
  <trd lng="es">(ardiendo) al blanco</trd>
