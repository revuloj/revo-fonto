<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: orator.xml,v 1.22 2024/01/30 16:09:26 revo Exp $">
<kap><ofc>1</ofc> <rad>orator</rad>/o</kap>
<drv mrk="orator.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo, kiu kutimas publike paroladi:
      <ekz>
        ju&gcirc;eja, politika, eklezia <tld/>o;
      </ekz>
      <ekz>
        unu el la gastoj <klr>[&#x2026;]</klr> frapetis sian glason kaj, ho ve &mdash; tosto! pensis la plimulto
        <klr>[&#x2026;]</klr> kun &gcirc;emoj sin pretigante al a&ubreve;skultado de la <tld/>o
        <fnt>
          <aut>B. von Suttner, trad. A. Caumont kaj F. Luin</aut>
          <vrk>For la batalilojn!</vrk>
          <lok>1914, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">orador</trd>
    <trdgrp lng="de">
      <trd><klr>(</klr>&ouml;ffentlicher) <ind>Redner</ind></trd>,
      <trd>Orator</trd>,
      <trd>Rhetor</trd>
    </trdgrp>
    <trd lng="hu">sz&oacute;nok</trd>
    <trd lng="nl">redenaar</trd>
    <trd lng="en">orator</trd>
    <trd lng="ru">&c_o;&c_r;&c_a;&c_t;&c_o;&c_r;</trd>
  </snc>
  <snc>
    <dif>
      Tiu, kiu parolas en la nomo de kolegoj:
      <ekz>
        li estis la <tld/>o de la grupo.
      </ekz>
    </dif>
    <trd lng="ca">portaveu</trd>
    <trd lng="de">Sprecher</trd>
    <trd lng="en">spokesperson</trd>
    <trd lng="hu">sz&oacute;sz&oacute;l&oacute;</trd>
    <trd lng="nl">spreekbuis</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_a;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_m;&c_o;&c_w;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>or&aacute;tor</trd>,
    <trd>&rcaron;e&ccaron;n&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>&omikron;&my;&jota;&lambda;&eta;&tau;&eta_ton;&sigma_fina;</trd>,
    <trd>&rho;&eta_ton;&tau;&omikron;&rho;&alfa;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">speaker</trd>
  <trd lng="fr">orateur</trd>
  <trdgrp lng="ja">
    <trd>&#x5F01;&#x58EB;  <pr>&#x3079;&#x3093;&#x3057;</pr></trd>,
    <trd>&#x96C4;&#x5F01;&#x5BB6;  <pr>&#x3086;&#x3046;&#x3079;&#x3093;&#x304B;</pr></trd>,
    <trd>&#x6F14;&#x8AAC;&#x5BB6;  <pr>&#x3048;&#x3093;&#x305C;&#x3064;&#x304B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>orator</trd>,
    <trd>retor</trd>,
    <trd>m&oacute;wca</trd>,
    <trd>krasom&oacute;wca</trd>
  </trdgrp>
  <trd lng="pt">orador</trd>
  <trdgrp lng="ro">
    <trd>orator</trd>,
    <trd>retor</trd>,
    <trd>vorbitor</trd>
  </trdgrp>
  <trd lng="sk">re&ccaron;n&iacute;k</trd>
  <trd lng="uk">&#x43E;&c_r;&c_a;&c_t;&#x43E;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x6F14;&#x8AAA;&#x8005; <pr>y&#x1CE;nshu&#x14D;zh&#x11B;</pr></trd>,
    <trd>&#x6F14;&#x8BF4;&#x8005; <pr>y&#x1CE;nshu&#x14D;zh&#x11B;</pr></trd>,
    <trd>&#x8A9E;&#x8005; <pr>y&#x1D4;zh&#x11B;</pr></trd>,
    <trd>&#x8BED;&#x8005; <pr>y&#x1D4;zh&#x11B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="orator.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a arto, frazo, periodo kadenco.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_a;&c_t;&c_a;&c_r;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">oratori (adj.)</trd>
  <trd lng="en">oratorical</trd>
  <trd lng="fr">oratorique</trd>
  <trd lng="hu">sz&oacute;noki</trd>
  <trdgrp lng="ja">
    <trd>&#x5F01;&#x8AD6;&#x306E;  <pr>&#x3079;&#x3093;&#x308D;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x96C4;&#x5F01;&#x306A;  <pr>&#x3086;&#x3046;&#x3079;&#x3093;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">redenaars-</trd>
  <trdgrp lng="pl">
    <trd>oratorski</trd>,
    <trd>krasom&oacute;wczy</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>oratoric</trd>,
    <trd>craciunului</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_r;&c_a;&c_t;&c_o;&c_r;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="uk">&#x43E;&c_r;&c_a;&c_t;&#x43E;&c_r;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
</drv>

<drv mrk="orator.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Paroladi kiel <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;&c_a;&c_c;&c_mol; <klr>(&c_z; &c_p;&c_u;&c_b;&c_l;&c_ib;&c_ch;&c_n;&c_a;&c_j;
  &c_p;&c_r;&c_a;&c_m;&c_o;&c_v;&c_a;&c_j;)</klr></trd>
  <trdgrp lng="ca">
    <trd>orar</trd>,
    <trd>discursejar</trd>
  </trdgrp>
  <trd lng="cs">&rcaron;e&ccaron;nit</trd>
  <trdgrp lng="de">
      <trd><klr>(</klr>&ouml;ffentlich) <ind>reden</ind></trd>,
      <trd><klr>(</klr>&ouml;ffentlich) <ind>sprechen</ind></trd>
    </trdgrp>
  <trd lng="el">&pi;&rho;&omikron;&phi;&epsilon_ton;&rho;&omega; &omikron;&my;&jota;&lambda;&jota_ton;&alfa;</trd>
  <trd lng="en">deliver an oration</trd>
  <trd lng="fr">orationner</trd>
  <trd lng="hu">sz&oacute;nokol</trd>
  <trd lng="ja">&#x6F14;&#x8AAC;&#x3059;&#x308B;  <pr>&#x3048;&#x3093;&#x305C;&#x3064;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">een <ind>redevoering</ind> houden</trd>
  <trdgrp lng="pl">
    <trd>przemawia&cacute;</trd>,
    <trd>popisywa&cacute; si&eogonek; krasom&oacute;wstwem</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>discursar</trd>,
    <trd>orar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>vorbi</trd>,
    <trd>ar&abreve;t&acirc;ndu-&#x219;i oratoria</trd>
  </trdgrp>
  <trd lng="sk">re&ccaron;ni&tcaron;</trd>
  <trd lng="uk">&c_v;&c_i;&c_g;&#x43E;&#x43B;&#x43E;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i; &#x43F;&c_r;&#x43E;&#x43C;&#x43E;&c_v;&c_u;</trd>
</drv>

<drv mrk="orator.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Anta&ubreve;publika parolata&jcirc;o.
    </dif>
    <refgrp tip="sin">
      <ref cel="diskur.0o">diskurso</ref>,
      <ref cel="oraci.0o">oracio</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_o;&c_v;&c_a; <klr>(&c_p;&c_u;&c_b;&c_l;&c_ib;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
  <trdgrp lng="ca">
    <trd>discurs</trd>,
    <trd>(per)oraci&oacute;</trd>
  </trdgrp>
  <trd lng="el">&omikron;&my;&jota;&lambda;&jota_ton;&alfa;</trd>
  <trdgrp lng="en">
    <trd>oration</trd>,
    <trd>speech</trd>
  </trdgrp>
  <trd lng="fr">oration</trd>
  <trd lng="hu">sz&oacute;noklat</trd>
  <trdgrp lng="ja">
    <trd>&#x5F01;&#x8AD6;  <pr>&#x3079;&#x3093;&#x308D;&#x3093;</pr></trd>,
    <trd>&#x6F14;&#x8AAC;  <pr>&#x3048;&#x3093;&#x305C;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">redevoering</trd>
  <trd lng="pl">mowa krasom&oacute;wcza</trd>
  <trdgrp lng="pt">
    <trd>discurso</trd>,
    <trd>ora&ccedil;&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>discurs oratoric</trd>,
    <trd>cuv&acirc;nt cheie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_r;&c_a;&c_s;&#x43D;&#x43E;&#x43C;&#x43E;&c_v;&#x43D;&c_i;&c_j; &c_t;&c_v;&c_ib;&c_r;</trd>,
    <trd>&#x43A;&c_r;&c_a;&c_s;&#x43D;&#x43E;&#x43C;&#x43E;&c_v;&#x43D;&c_i;&c_j; &c_u;&c_r;&c_i;&c_v;&#x43E;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6F14;&#x8AAA; <pr>y&#x1CE;nshu&#x14D;</pr></trd>,
    <trd>&#x6F14;&#x8BF4; <pr>y&#x1CE;nshu&#x14D;</pr></trd>,
    <trd>&#x8A71; <pr>hu&#xE0;</pr></trd>,
    <trd>&#x8BDD; <pr>hu&#xE0;</pr></trd>,
    <trd>&#x9648;&#x8BCD; <pr>ch&#xE9;nc&#xED;</pr></trd>,
    <trd>&#x9673;&#x8A5E; <pr>ch&#xE9;nc&#xED;</pr></trd>
  </trdgrp>
</drv>
</art>
