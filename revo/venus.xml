<vortaro>
<art mrk="$Id: venus.xml,v 1.25 2021/05/29 14:33:26 revo Exp $">
<kap>
  <rad>Venus</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="venus.0o">
  <kap><tld/>o</kap>
  <mlg>&#x2640;</mlg>
  <snc mrk="venus.0o.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
      <ref tip="dif" cel="vener1.0o">Venero</ref>:
      <ekz>
        Amoro venas helpe al Amanto kaj per plia helpo de <tld/>o
        bruligas la kastelon
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2008/009455.php">Mezepokaj
          sekret(igit)aj sonetoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">Venus</trd>    
  </snc>
  <snc mrk="venus.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      La dua (post Merkuro, la&ubreve; distanco de la 
      <ref tip="vid" cel="sun.0o.nia">Suno</ref>)
      <ref tip="lst" cel="planed.0o" lst="voko:planedoj">planedo</ref>
      de la <ref tip="malprt" cel="sun.0sistemo">sunsistemo</ref>:
      <ekz>
        scienc-fikcia dramo, celanta kompari la nombron de esperantistoj
        sur la tero kaj sur <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2007/009300.php">Sukcesoplena,
          &scirc;ercoplena kaj tre malkutima</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/e/e0/Mercury%2C_Venus_and_the_Moon_Align.jpg/405px-Mercury%2C_Venus_and_the_Moon_Align.jpg" prm="CC BY 4.0">luno, <tld/>o kaj Merkuro vidita de Paranal Observejo
        <fnt>
          <aut>ESO/Y. Beletsky,
          http://www.eso.org/public/images/potw1025a/</aut>
          <lok><url ref="&WCW;/File:Mercury,_Venus_and_the_Moon_Align.jpg">&WCW;/File:Mercury,_Venus_and_the_Moon_Align.jpg</url></lok>
        </fnt>
    </bld>
    <bld lok="&WCU;/thumb/8/85/Venus_globe.jpg/480px-Venus_globe.jpg" prm="PD">bildo de <tld/>o konstruita el fotoj de Magellan kaj orbitilo
      Pioneer Venus,
    simulitaj koloroj surbaze de spac&scirc;ipaj fotoj de Venera 13 kaj 14
        <fnt>
          <aut>NASA,
          http://photojournal.jpl.nasa.gov/catalog/PIA00104</aut>
          <lok><url ref="&WCW;/File:Venus_globe.jpg">&WCW;/File:Venus_globe.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="hu">V&eacute;nusz</trd>
    <trd lng="zh">&#x91D1;&#x661F; [<ind>j&#x12B;nx&#x12B;ng</ind>]</trd>
  </snc>
  <trd lng="be">&c_V;&c_e;&c_n;&c_e;&c_r;&c_a;</trd>
  <trd lng="bg">&c_V;&c_je;&c_n;&c_je;&c_r;&c_a;</trd>
  <trd lng="bo">&#xF54;&#xF0B;&#xF66;&#xF44;&#xF66;&#xF0B;</trd>
  <trd lng="ca">Venus</trd>
  <trdgrp lng="cs">
    <trd>Venu&scaron;e</trd>,
    <trd>ve&ccaron;ernice</trd>
  </trdgrp>
  <trd lng="de">Venus</trd>
  <trd lng="el">&Alfa;&phi;&rho;&omikron;&delta;&jota_ton;&tau;&eta;
  </trd>
  <trd lng="en">Venus</trd>
  <trd lng="es">Venus</trd>
  <trd lng="fr">V&eacute;nus</trd>
  <trd lng="id">Venus</trd>
  <trd lng="it">Venere</trd>
  <trd lng="la">Venus</trd>
  <trd lng="nl">Venus</trd>
  <trd lng="no">Venus</trd>
  <trd lng="pl">Wenus</trd>
  <trd lng="pt">V&ecirc;nus</trd>
  <trd lng="ru">&c_V;&c_je;&c_n;&c_je;&c_r;&c_a;</trd>
  <trd lng="sk">Venu&scaron;a</trd>
</drv>

