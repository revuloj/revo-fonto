<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Venus</rad>/o
</kap>
<drv mrk="venus.0o">
  <kap><tld/>o</kap>
  <snc mrk="venus.0o.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Romia <ref tip="super" cel="di.0o.politeisma">diino</ref> de amo,
      printempo kaj &gcirc;ardenoj.
    </dif>
  </snc>
  <snc mrk="venus.0o.planedo">
    <uzo tip="fak">AST</uzo>
    <dif>
      La dua (la&ubreve; distanco de la <ref tip="vid"
      cel="sun.0o.nia">Suno</ref>)
      <ref tip="super" cel="planed.0o">planedo</ref>
      de la <ref tip="malprt" cel="sun.0sistemo">sunsistemo</ref>.
    </dif>
    <trd lng="zh">&#37329;&#26143;<klr> [j&#299;nx&#299;ng]</klr></trd>
  </snc>
  <trd lng="de">Venus</trd>
  <trd lng="el">&Alfa;&phi;&rho;&omikron;&delta;&jota_acute;&tau;&eta;</trd>
  <trd lng="en">Venus</trd>
  <trd lng="es">Venus</trd>
  <trd lng="fr">V&eacute;nus</trd>
  <trd lng="it">Venere</trd>
  <trd lng="ru">&c_V;&c_je;&c_n;&c_je;&c_r;&c_a;</trd>
</drv>

<drv mrk="venus.0a">
  <kap><tld/>o</kap>
  <snc mrk="venus.0a.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Rilata al <tld/>o <sncref ref="venus.0o.mita"/>.
    </dif>
    <trd lng="ru">&c_v;&c_je;&c_n;&c_je;&c_r;&c_i;&c_n;</trd>
  </snc>
  <snc mrk="venus.0a.planeda">
    <uzo tip="fak">AST</uzo>
    <dif>
      Rilata al <tld/>o <sncref ref="venus.0o.planedo"/>.
    </dif>
    <trd lng="ru">&c_v;&c_je;&c_n;&c_je;&c_r;&c_i;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <trd lng="de">Venus-</trd>
  <trd lng="en">Venusian</trd>
  <trd lng="fr">v&eacute;nusien</trd>
</drv>
</art>

<!--
$Log$
