<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tak.xml,v 1.3 2025/03/03 12:53:29 afido Exp $">
<kap>
  <rad>tak</rad>/o <fnt><bib>PIV2</bib></fnt>,
  <var><kap><rad var="a">taka</rad>/o <fnt><bib>NePiv</bib></fnt></kap></var>
</kap>
<drv mrk="tak.0o">
  <kap><tld/>o,
  <var><kap><tld var="a"/>o</kap></var></kap>
  <snc mrk="tak.0o.MON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">Monunuo</ref> de Banglade&scirc;o:
      <ekz>
        ili deklaris strikon por postuli tagan salajron de 300 <tld/>oj (3,16 dolaroj)
        <fnt>
          <aut>&#2474;&#2494;&#2472;&#2509;&#2469; &#2480;&#2489;&#2478;&#2494;&#2472; &#2480;&#2503;&#2460;&#2494; (Pantha), trad. A. Delgado Ordu&ntilde;o</aut>
          <vrk><url ref="https://eo.globalvoices.org/2023/01/13488/">
          Kiel banglade&scirc;aj teolaboristoj povas travivi kun taga salajro de malpli ol du
          dolaroj?</url></vrk>
          <lok>GlobalVoices, 2023-01-02</lok>
        </fnt>;
      </ekz>
      <ekz>
        paginte unu <tld var="a"/>on mi envagoni&gcirc;is
        <fnt><bib>HomojMan&gcirc;antaj</bib> <lok>man&gcirc;i rubman&gcirc;a&jcirc;on, p.
        16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="tak1.0o">tako (man&gcirc;a&jcirc;o)</ref>
  <trd lng="be">&c_t;&c_a;&c_k;&c_a; <klr>(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_ja; &c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;)</klr></trd>
  <trd lng="de">Taka <klr>(W&auml;hrung)</klr></trd>
  <trd lng="fr">taka<klr> (monnaie)</klr></trd>
</drv>
</art>

