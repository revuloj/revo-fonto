<drv mrk="lamp.strat0o">
  <kap>strat<tld/>o</kap>
  <snc mrk="lamp.strat0o.LUM">
    <dif>
      <ref tip="super" cel="lamp.0o.LUM"><tld lit="L"/>o</ref>
      prilumanta urban straton dum la nokto:
      <ekz>
	&ccirc;e strat<tld/>o staris maljunulino etendante sian manon,
	sed ka&scirc;ante la viza&gcirc;on
	<fnt><bib>Kiso</bib>, <lok>son&gcirc;o pri maljuneco</lok>
	</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_l;&c_ib;&c_ch;&c_n;&c_y; &c_l;&c_ib;&c_h;&c_t;&c_a;&c_r;</trd>
