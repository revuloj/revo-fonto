  <trd lng="sk">ne&oacute;nov&aacute; &zcaron;iarivka</trd>
  <trd lng="uk">&c_n;&c_je;&c_o;&c_n;&c_o;&c_v;&c_a; &c_l;&c_a;&c_m;&c_p;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#38675;&#34425;&#28783; <pr>n&iacute;h&oacute;ngd&emacron;ng</pr></trd>,
    <trd>&#38675;&#34425;&#29128; <pr>n&iacute;h&oacute;ngd&emacron;ng</pr></trd>
  </trdgrp>
