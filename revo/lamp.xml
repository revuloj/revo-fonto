<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lamp</rad>/o
</kap>
<drv mrk="lamp.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="lamp.0o.vazo">
    <dif>
      Aparato por lumigi, konsistanta el vazo kun me&ccirc;o trempi&gcirc;anta en ia
      brulebla flua&jcirc;o:
      <ekz>
        petrola, olea <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lamp.0o.ampolo">
    <dif>
      Aparato por lumigi:
      <ekz>
        elektra, minista <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ampol.0o">ampolo</ref>,
      <ref cel="lampio.0o">lampiono</ref>,
      <ref cel="lanter.0o">lanterno</ref>,
      <ref cel="lucern.0o">lucerno</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>lampa</trd>,
    <trd>sv&iacute;tilna</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Lampe</trd>,
     <trd>Leuchte</trd>
  </trdgrp>
  <trd lng="el">&lambda;&alfa_ton;&my;&pi;&alfa;</trd>
  <trd lng="en">lamp</trd>
  <trd lng="fr">lampe</trd>
  <trd lng="nl">lamp</trd>
  <trd lng="pt">l&acirc;mpaa</trd>
  <trd lng="pl">lampa</trd>
  <trd lng="ru">&c_l;&c_a;&c_m;&c_p;&c_a;</trd>
  <trd lng="sv">lampa</trd>
</drv>
</art>
