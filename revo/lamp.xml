  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_e;&c_o;&c_n;&c_a;&c_v;&c_a;&c_ja; &c_l;&c_ja;&c_m;&c_p;&c_a;</trd>,
    <trd>&c_n;&c_e;&c_o;&c_n;&c_a;&c_v;&c_y; &c_l;&c_ib;&c_h;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
