<vortaro>
<art mrk="$Id: lamp.xml,v 1.41 2024/02/10 15:20:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lamp</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="lamp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lamp.0o.LUM">
    <dif>
      <ref tip="super" cel="lum.0igilo">Lumigilo</ref>
      produktanta lumon per transformado de energio:
      <ekz>
        fi&scirc;olea <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        fulganta petrola <tld/>o
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 28</lok>
        </fnt>;
      </ekz>
      <ekz>
	elektra, minista <tld/>o;
      </ekz>
      <ekz>
        la malprudentaj, prenante la <tld/>ojn, ne prenis oleon kun si
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 25:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie brulas <tld/>o en la ununura &ccirc;ambro kaj &jcirc;etas siajn flavajn striojn tra la
        fenestroj
        <fnt>
          <bib>IdO</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        pendanta granda vitra rozokolora globo, kiu entenis en si <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la klas&ccirc;ambro la <tld/>oj lumis de mateno &gcirc;is vespero
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni sciis nenion pri la ekzisto de acetilenaj <tld/>oj, lampoj de fabrikita
        gaso, gazolinlanternoj, alkoholaj <tld/>oj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 30</lok>
        </fnt>;
      </ekz>
      <ekz>
        eta karbonfadena <tld/>o mizere prilumis la vastan, terure malzorgitan
        anta&ubreve;&ccirc;ambron
        <fnt>
          <bib>MkM</bib>
          <lok>4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lumo de la ankora&ubreve; ne mal&scirc;altitaj &scirc;oseaj <tld/>oj estis jam senutila
        <fnt>
          <bib>MkM</bib>
          <lok>6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la luno estis grandega nokta <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ampol.0o.elektra">ampolo<sncref/></ref>,
      <ref cel="kandel.0o">kandelo</ref>,
      <ref cel="lanter.0o.LUM">lanterno<sncref/></ref>,
      <ref cel="lucern.0o">lucerno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_m;&c_p;&c_a;</trd>
  <trd lng="bg">&c_l;&c_a;&c_m;&c_p;&c_a;</trd>
  <trd lng="bo">&t_ba;&t_zha;&t_shabchu;&t_tsheg;&t_ma;&t_ra;&t_tsheg;</trd>
  <trd lng="br">lamp</trd>
  <trd lng="ca">l&agrave;mpada</trd>
  <trdgrp lng="cs">
    <trd>lampa</trd>,
    <trd>sv&iacute;tilna</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lampe</trd>,
    <trd>Leuchte</trd>
  </trdgrp>
  <trd lng="el">&lambda;&alfa_ton;&my;&pi;&alfa;</trd>
  <trd lng="en">lamp</trd>
  <trd lng="es">l&aacute;mpara</trd>
  <trd lng="fr">lampe</trd>
  <trd lng="hu">l&aacute;mpa</trd>
  <trd lng="id">lampu</trd>
  <trdgrp lng="ja">
    <trd>&#12521;&#12531;&#12503;</trd>,
    <trd>&#28783;&#28779;  <pr>&#12392;&#12418;&#12375;&#12403;</pr></trd>,
    <trd>&#38651;&#28783;  <pr>&#12391;&#12435;&#12392;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">lamp</trd>
  <trd lng="pl">lampa</trd>
  <trd lng="pt">l&acirc;mpada</trd>
  <trd lng="ru">&c_l;&c_a;&c_m;&c_p;&c_a;</trd>
  <trd lng="sk">lampa</trd>
  <trd lng="sv">lampa</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_m;&c_p;&c_a;</trd>,
    <trd>&c_l;&c_ib;&c_h;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="zh">&#29031;&#20142; <pr>zh&agrave;oli&agrave;ng</pr></trd>
</drv>

<drv mrk="lamp.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="lamp.0ingo.KOMUNE">
    <dif>
      Objekto, sur kiu oni starigas a&ubreve; fiksas <tld/>on:
      <ekz>
	&ccirc;u <tld/>o enporti&gcirc;as, por esti metita sub
	grenmezurilon a&ubreve; sub liton, kaj ne por stari sur la
	<tld/>ingo
	<fnt><bib>NT</bib>, <lok>&Mar; 4:21</lok></fnt>?
      </ekz>
    </dif>
    <ref tip="vid" cel="lum.0ingo">lumingo</ref>
    <trd lng="de">Lampenhalter</trd>
  </snc>
  <snc mrk="lamp.0ingo.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Objekto, en kiun oni fiksas elektran
      <tld/>on kaj per kiu &gcirc;i ricevas elektron;
      <ref tip="dif" cel="ampol.0ingo">ampolingo</ref>:
      <ekz>
        depreni el <tld/>ingo restojn de rompita ampolo
        <fnt>
          <vrk><url ref="http://www.esperanto.cba.pl/raportoj.html">Mia laboro en Bona Espero</url></vrk>
          <lok>Esperanto en Gliwice, raportoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Lampenfassung</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_r;&c_o;&c_n; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_l;&c_ja;&c_m;&c_p;&c_y;)</klr></trd>
  <trdgrp lng="ca">
    <trd>suport</trd>,
    <trd>base</trd>
  </trdgrp>
  <trd lng="cs">obj&iacute;mka lampy</trd>
  <trdgrp lng="en">
  <trd>lamp holder</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chandelier</trd>,
    <trd>support de lampe</trd>
  </trdgrp>
  <trd lng="hu">l&aacute;mpafoglalat</trd>
  <trd lng="ja">&#12477;&#12465;&#12483;&#12488;</trd>
  <trdgrp lng="nl">
    <trd>lamphouder</trd>,
    <trd>fitting</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>klosz</trd>
  </trdgrp>
  <trd lng="sk">obj&iacute;mka lampy</trd>
  <trd lng="uk">&c_p;&c_a;&c_t;&c_r;&c_o;&c_n; (&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_n;&c_o;&c_ji;
  &c_l;&c_a;&c_m;&c_p;&c_o;&c_ch;&c_k;&c_i;)</trd>
</drv>

<drv mrk="lamp.halogen0o">
  <kap>halogen<tld/>o, <var><kap>halogena <tld/>o</kap></var></kap>
  <snc mrk="lamp.halogen0o.LUM">
    <dif>
      Elektro<ref tip="super" cel="lamp.0o.LUM"><tld/>o</ref> kun
      ampoleto entenanta halogenon, kiu konservas la filamenton:
      <ekz>
	malgranda hejtilo kun halogenaj <tld/>oj
	<fnt>
	<aut>Vastalto</aut>,
	<vrk><url ref="http://www.vastalto.com/blog/">Akvosfero</url></vrk>,
	<lok>Halogena hejtilo, 2006-01-04</lok>
	</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_ja;&c_g;&c_je;&c_n;&c_a;&c_v;&c_a;&c_ja; &c_l;&c_ja;&c_m;&c_p;&c_a;</trd>
  <trd lng="ca"><ind>l&agrave;mpada</ind> d'halogen</trd>
  <trd lng="de">Halogenlampe</trd>
  <trdgrp lng="en">
  <trd>halogen lamp</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>halog&egrave;ne<klr tip="amb"> (lampe)</klr></trd>,
    <trd>lampe &agrave; halog&egrave;ne</trd>
  </trdgrp>
  <trd lng="id"><ind>lampu</ind> halogen</trd>
  <trd lng="ja">&#12495;&#12525;&#12466;&#12531;&#12521;&#12531;&#12503;</trd>
  <trd lng="nl">halogeenlamp</trd>
  <trd lng="pl">lampa halogenowa</trd>
  <trdgrp lng="zh">
    <trd>&#21348;&#32032;&#28783; <pr>l&#468;s&ugrave;d&emacron;ng</pr></trd>,
    <trd>&#28407;&#32032;&#29128; <pr>l&#468;s&ugrave;d&emacron;ng</pr></trd>,
    <trd>&#37794;&#32114;&#28407;&#32032;&#29128; <pr>w&umacron;s&imacron;l&#468;s&ugrave;d&emacron;ng</pr></trd>,
    <trd>&#38056;&#19997;&#21348;&#32032;&#28783; <pr>w&umacron;s&imacron;l&#468;s&ugrave;d&emacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lamp.neon0o">
  <kap>neon<tld/>o</kap>
  <snc mrk="lamp.neon0o.LUM">
    <dif>
      Elektro<ref tip="super" cel="lamp.0o.LUM"><tld/>o</ref>
      funkcianta per neono:
