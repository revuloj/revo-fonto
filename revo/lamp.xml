    <trd>&#34903;&#28783; <pr>ji&emacron;d&emacron;ng</pr></trd>,
    <trd>&#34903;&#29128; <pr>ji&emacron;d&emacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lamp.tablo0o">
  <kap>tablo<tld/>o</kap>
  <snc mrk="lamp.tablo0o.LUM">
    <dif>
      Surtabla <ref tip="super" cel="lamp.0o.LUM"><tld/>o</ref>
      prilumanta la tablan surfacon:
      <ekz>
	la tablo<tld/>o estas envolvita en la ru&gcirc;a &scirc;tofo,
	lasante nur mallar&gcirc;an strion de la radioj, falantaj rekte
	la manuskripton
	<fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_l;&c_ja;&c_m;&c_p;&c_a;</trd>
  <trd lng="ca">l&agrave;mpada de taula</trd>
  <trd lng="cs">stoln&iacute; lampa</trd>
  <trd lng="de">Tischlampe</trd>
  <trdgrp lng="en">
  <trd>table lamp</trd>
  </trdgrp>
  <trd lng="fr">lampe de bureau</trd>
  <trd lng="id"><ind>lampu</ind> meja</trd>
  <trd lng="nl">bureaulamp</trd>
  <trd lng="pl">lampa biurkowa</trd>
  <trdgrp lng="pt">
   <trd>l&acirc;mpada de mesa</trd>
   </trdgrp>
  <trd lng="sk">stoln&aacute; lampa</trd>
  <trdgrp lng="zh">
    <trd>&#21488;&#28783; <pr>t&aacute;id&emacron;ng</pr></trd>,
    <trd>&#27311;&#29128; <pr>t&aacute;id&emacron;ng</pr></trd>
  </trdgrp>
</drv>
