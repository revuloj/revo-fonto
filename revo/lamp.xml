    <trd>Neonlampe</trd>
  </trdgrp>
  <trdgrp lng="en">
  <trd>neon lamp</trd>
  </trdgrp>
  <trd lng="fr">n&eacute;on<klr> (lampe)</klr></trd>
  <trd lng="id"><ind>lampu</ind> neon</trd>
  <trd lng="ja">&#12493;&#12458;&#12531;&#12521;&#12531;&#12503;</trd>
  <trd lng="nl">neonlamp</trd>
  <trdgrp lng="pl">
    <trd>lampa neonow</trd>,
