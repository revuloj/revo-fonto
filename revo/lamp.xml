<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lamp.xml,v 1.39 2024/01/15 16:14:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lamp</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="lamp.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="lamp.0o.LUM">
    <dif>
      <ref tip="super" cel="lum.0igilo">Lumigilo</ref>
      produktanta lumon per transformado de energio:
      <ekz>
        fi&scirc;olea <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        fulganta petrola <tld/>o
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 28</lok>
        </fnt>;
      </ekz>
      <ekz>
	elektra, minista <tld/>o;
      </ekz>
      <ekz>
        la malprudentaj, prenante la <tld/>ojn, ne prenis oleon kun si
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 25:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie brulas <tld/>o en la ununura &ccirc;ambro kaj &jcirc;etas siajn flavajn striojn tra la
        fenestroj
        <fnt>
          <bib>IdO</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        pendanta granda vitra rozokolora globo, kiu entenis en si <tld/>on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en la klas&ccirc;ambro la <tld/>oj lumis de mateno &gcirc;is vespero
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni sciis nenion pri la ekzisto de acetilenaj <tld/>oj, lampoj de fabrikita
        gaso, gazolinlanternoj, alkoholaj <tld/>oj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 30</lok>
        </fnt>;
      </ekz>
      <ekz>
        eta karbonfadena <tld/>o mizere prilumis la vastan, terure malzorgitan
        anta&ubreve;&ccirc;ambron
        <fnt>
          <bib>MkM</bib>
          <lok>4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lumo de la ankora&ubreve; ne mal&scirc;altitaj &scirc;oseaj <tld/>oj estis jam senutila
        <fnt>
          <bib>MkM</bib>
          <lok>6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la luno estis grandega nokta <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ampol.0o.elektra">ampolo<sncref/></ref>,
      <ref cel="kandel.0o">kandelo</ref>,
      <ref cel="lanter.0o.LUM">lanterno<sncref/></ref>,
      <ref cel="lucern.0o">lucerno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_m;&c_p;&c_a;</trd>
  <trd lng="bg">&c_l;&c_a;&c_m;&c_p;&c_a;</trd>
  <trd lng="bo">&t_ba;&t_zha;&t_shabchu;&t_tsheg;&t_ma;&t_ra;&t_tsheg;</trd>
  <trd lng="br">lamp</trd>
  <trd lng="ca">l&agrave;mpada</trd>
  <trdgrp lng="cs">
    <trd>lampa</trd>,
    <trd>sv&iacute;tilna</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lampe</trd>,
    <trd>Leuchte</trd>
  </trdgrp>
  <trd lng="el">&lambda;&alfa_ton;&my;&pi;&alfa;</trd>
  <trd lng="en">lamp</trd>
  <trd lng="es">l&aacute;mpara</trd>
  <trd lng="fr">lampe</trd>
  <trd lng="hu">l&aacute;mpa</trd>
  <trd lng="id">lampu</trd>
  <trdgrp lng="ja">
    <trd>&#x30E9;&#x30F3;&#x30D7;</trd>,
    <trd>&#x706F;&#x706B;  <pr>&#x3068;&#x3082;&#x3057;&#x3073;</pr></trd>,
    <trd>&#x96FB;&#x706F;  <pr>&#x3067;&#x3093;&#x3068;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">lamp</trd>
  <trd lng="pl">lampa</trd>
  <trd lng="pt">l&acirc;mpada</trd>
  <trd lng="ru">&c_l;&c_a;&c_m;&c_p;&c_a;</trd>
  <trd lng="sk">lampa</trd>
  <trd lng="sv">lampa</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_m;&c_p;&c_a;</trd>,
    <trd>&c_l;&c_ib;&c_h;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="zh">&#x7167;&#x4EAE; <pr>zh&#xE0;oli&#xE0;ng</pr></trd>
</drv>

<drv mrk="lamp.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="lamp.0ingo.KOMUNE">
    <dif>
      Objekto, sur kiu oni starigas a&ubreve; fiksas <tld/>on:
      <ekz>
	&ccirc;u <tld/>o enporti&gcirc;as, por esti metita sub
	grenmezurilon a&ubreve; sub liton, kaj ne por stari sur la
	<tld/>ingo
	<fnt><bib>NT</bib>, <lok>&Mar; 4:21</lok></fnt>?
      </ekz>
    </dif>
    <ref tip="vid" cel="lum.0ingo">lumingo</ref>
    <trd lng="de">Lampenhalter</trd>
  </snc>
  <snc mrk="lamp.0ingo.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Objekto, en kiun oni fiksas elektran
      <tld/>on kaj per kiu &gcirc;i ricevas elektron;
      <ref tip="dif" cel="ampol.0ingo">ampolingo</ref>:
      <ekz>
        depreni el <tld/>ingo restojn de rompita ampolo
        <fnt>
          <vrk><url ref="http://www.esperanto.cba.pl/raportoj.html">Mia laboro en Bona Espero</url></vrk>
          <lok>Esperanto en Gliwice, raportoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Lampenfassung</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_r;&c_o;&c_n; <klr tip="amb">(&c_d;&c_l;&c_ja; &c_l;&c_ja;&c_m;&c_p;&c_y;)</klr></trd>
  <trdgrp lng="ca">
    <trd>suport</trd>,
    <trd>base</trd>
  </trdgrp>
  <trd lng="cs">obj&iacute;mka lampy</trd>
  <trdgrp lng="fr">
    <trd>chandelier</trd>,
    <trd>support de lampe</trd>
  </trdgrp>
  <trd lng="hu">l&aacute;mpafoglalat</trd>
  <trd lng="ja">&#x30BD;&#x30B1;&#x30C3;&#x30C8;</trd>
  <trdgrp lng="nl">
    <trd>lamphouder</trd>,
    <trd>fitting</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>klosz</trd>
  </trdgrp>
  <trd lng="sk">obj&iacute;mka lampy</trd>
  <trd lng="uk">&c_p;&c_a;&c_t;&c_r;&c_o;&c_n; (&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_n;&c_o;&c_ji;
  &c_l;&c_a;&c_m;&c_p;&c_o;&c_ch;&c_k;&c_i;)</trd>
</drv>

<drv mrk="lamp.halogen0o">
  <kap>halogen<tld/>o, <var><kap>halogena <tld/>o</kap></var></kap>
  <snc mrk="lamp.halogen0o.LUM">
    <dif>
      Elektro<ref tip="super" cel="lamp.0o.LUM"><tld/>o</ref> kun
      ampoleto entenanta halogenon, kiu konservas la filamenton:
      <ekz>
	malgranda hejtilo kun halogenaj <tld/>oj
	<fnt>
	<aut>Vastalto</aut>,
	<vrk><url ref="http://www.vastalto.com/blog/">Akvosfero</url></vrk>,
	<lok>Halogena hejtilo, 2006-01-04</lok>
	</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_ja;&c_g;&c_je;&c_n;&c_a;&c_v;&c_a;&c_ja; &c_l;&c_ja;&c_m;&c_p;&c_a;</trd>
  <trd lng="ca"><ind>l&agrave;mpada</ind> d'halogen</trd>
  <trd lng="de">Halogenlampe</trd>
  <trdgrp lng="fr">
    <trd>halog&egrave;ne<klr tip="amb"> (lampe)</klr></trd>,
    <trd>lampe &agrave; halog&egrave;ne</trd>
  </trdgrp>
  <trd lng="id"><ind>lampu</ind> halogen</trd>
  <trd lng="ja">&#x30CF;&#x30ED;&#x30B2;&#x30F3;&#x30E9;&#x30F3;&#x30D7;</trd>
  <trd lng="nl">halogeenlamp</trd>
  <trd lng="pl">lampa halogenowa</trd>
  <trdgrp lng="zh">
    <trd>&#x5364;&#x7D20;&#x706F; <pr>l&#x1D4;s&#xF9;d&#x113;ng</pr></trd>,
    <trd>&#x6EF7;&#x7D20;&#x71C8; <pr>l&#x1D4;s&#xF9;d&#x113;ng</pr></trd>,
    <trd>&#x93A2;&#x7D72;&#x6EF7;&#x7D20;&#x71C8; <pr>w&#x16B;s&#x12B;l&#x1D4;s&#xF9;d&#x113;ng</pr></trd>,
    <trd>&#x94A8;&#x4E1D;&#x5364;&#x7D20;&#x706F; <pr>w&#x16B;s&#x12B;l&#x1D4;s&#xF9;d&#x113;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lamp.neon0o">
  <kap>neon<tld/>o</kap>
  <snc mrk="lamp.neon0o.LUM">
    <dif>
      Elektro<ref tip="super" cel="lamp.0o.LUM"><tld/>o</ref>
      funkcianta per neono:
      <ekz>
	&gcirc;ia neon<tld/>o kun skarlataj literoj melankolie prilumas
	la straton
	<fnt><aut>Ueyama M.</aut>, <vrk>Pardonon!</vrk>, <lok>l'
	omnibuso, 1970</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_e;&c_o;&c_n;&c_a;&c_v;&c_a;&c_ja; &c_l;&c_ja;&c_m;&c_p;&c_a;</trd>,
    <trd>&c_n;&c_e;&c_o;&c_n;&c_a;&c_v;&c_y; &c_l;&c_ib;&c_h;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="ca"><ind>l&agrave;mpada</ind> de ne&oacute;</trd>
  <trdgrp lng="cs">
    <trd>neonov&aacute; lampa</trd>,
    <trd>neonov&aacute; z&aacute;&rcaron;ivka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Leuchtr&ouml;hre</trd>,
    <trd>Neonlampe</trd>
  </trdgrp>
  <trd lng="fr">n&eacute;on<klr> (lampe)</klr></trd>
  <trd lng="id"><ind>lampu</ind> neon</trd>
  <trd lng="ja">&#x30CD;&#x30AA;&#x30F3;&#x30E9;&#x30F3;&#x30D7;</trd>
  <trd lng="nl">neonlamp</trd>
  <trdgrp lng="pl">
    <trd>lampa neonow</trd>,
    <trd>neon&oacute;wka</trd>,
    <trd>neon</trd>
  </trdgrp>
  <trd lng="sk">ne&oacute;nov&aacute; &zcaron;iarivka</trd>
  <trd lng="uk">&c_n;&c_je;&c_o;&c_n;&c_o;&c_v;&c_a; &c_l;&c_a;&c_m;&c_p;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x9713;&#x8679;&#x706F; <pr>n&#xED;h&#xF3;ngd&#x113;ng</pr></trd>,
    <trd>&#x9713;&#x8679;&#x71C8; <pr>n&#xED;h&#xF3;ngd&#x113;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lamp.ole0o">
  <kap>ole<tld/>o, <var><kap>oleo<tld/>o</kap></var></kap>
  <snc mrk="lamp.ole0o.LUM">
    <dif>
      <ref tip="super" cel="lamp.0o.LUM"><tld lit="L"/>o</ref>
      kun me&ccirc;o &ccirc;erpanta brula&jcirc;on el olea rezervujo:
      <ekz>
	ole<tld/>oj estas la komunaj lumigiloj de romianoj;
      </ekz>
      <ekz>
	por kompletigi niajn preparojn, ni bezonas malgrandan
	oleo<tld/>on, sankta pastro
	<fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 20a, Lemuel
	faras eltrovon</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_je;&c_j;&c_n;&c_a;&c_ja; <ind>&c_l;&c_ja;&c_m;&c_p;&c_a;</ind></trd>,
    <trd>&c_a;&c_l;&c_je;&c_j;&c_n;&c_a;&c_ja; <ind>&c_s;&c_mol;&c_v;&c_ja;&c_c;&c_ib;&c_l;&c_mol;&c_n;&c_ja;</ind></trd>
  </trdgrp>
  <trd lng="ca">ll&agrave;ntia</trd>
  <trd lng="de">&Ouml;llampe</trd>
  <trd lng="fr">lampe &agrave; huile</trd>
  <trdgrp lng="id">
    <trd><ind>lampu</ind> minyak</trd>,
    <trd><ind>lampu</ind> tempel</trd>,
    <trd><ind>lampu</ind> teplok</trd>
  </trdgrp>
  <trd lng="nl">olielamp</trd>
  <trdgrp lng="pl">
    <trd>lampa olejowa</trd>,
    <trd>sztorm&oacute;wka</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6CB9;&#x706F; <pr>y&#xF3;ud&#x113;ng</pr></trd>,
    <trd>&#x6CB9;&#x71C8; <pr>y&#xF3;ud&#x113;ng</pr></trd>,
    <trd>&#x706F;&#x76CF; <pr>d&#x113;ngzh&#x1CE;n</pr></trd>,
    <trd>&#x71C8;&#x76DE; <pr>d&#x113;ngzh&#x1CE;n</pr></trd>,
    <trd>&#x91ED; <pr>g&#x101;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lamp.posx0o">
  <kap>po&scirc;<tld/>o</kap>
  <snc mrk="lamp.posx0o.LUM">
    <dif>
      Malgranda, facile kunportebla <ref tip="super" cel="lamp.0o.LUM"><tld/>o</ref>:
      <ekz>
	du akre brilaj po&scirc;<tld/>oj celis al mi
	<fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
	8a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_h;&c_t;&c_a;&c_r;&c_y;&c_k; <klr>(&c_k;&c_ib;&c_sh;&c_e;&c_n;&c_n;&c_y;)</klr></trd>
  <trdgrp lng="ca">
    <trd>llanterna</trd>,
    <trd>far&oacute;</trd>
  </trdgrp>
  <trd lng="de">Taschenlampe</trd>
  <trd lng="fr">lampe de poche</trd>
  <trd lng="id">senter</trd>
  <trd lng="ja">&#x61D0;&#x4E2D;&#x96FB;&#x706F;  <pr>&#x304B;&#x3044;&#x3061;&#x3085;&#x3046;&#x3067;&#x3093;&#x3068;&#x3046;</pr></trd>
  <trd lng="nl">zaklamp</trd>
  <trd lng="pl">latarka</trd>
  <trd lng="uk">&c_k;&c_i;&c_sh;&c_je;&c_n;&c_mol;&c_k;&c_o;&c_v;&c_i;&c_j; &c_l;&c_ib;&c_h;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x624B;&#x7535; <pr>sh&#x1D2;udi&#xE0;n</pr></trd>,
    <trd>&#x624B;&#x96FB; <pr>sh&#x1D2;udi&#xE0;n</pr></trd>,
    <trd>&#x624B;&#x7535;&#x7B52; <pr>sh&#x1D2;udi&#xE0;nt&#x1D2;ng</pr></trd>,
    <trd>&#x624B;&#x96FB;&#x7B52; <pr>sh&#x1D2;udi&#xE0;nt&#x1D2;ng</pr></trd>,
    <trd>&#x721D; <pr>ju&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lamp.strat0o">
  <kap>strat<tld/>o</kap>
  <snc mrk="lamp.strat0o.LUM">
    <dif>
      <ref tip="super" cel="lamp.0o.LUM"><tld lit="L"/>o</ref>
      prilumanta urban straton dum la nokto:
      <ekz>
	&ccirc;e strat<tld/>o staris maljunulino etendante sian manon,
	sed ka&scirc;ante la viza&gcirc;on
	<fnt><bib>Kiso</bib>, <lok>son&gcirc;o pri maljuneco</lok>
	</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_l;&c_ib;&c_ch;&c_n;&c_y; &c_l;&c_ib;&c_h;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">fanal</trd>
  <trd lng="cs">pouli&ccaron;n&iacute; sv&iacute;tilna</trd>
  <trd lng="de">Stra&szlig;enlaterne</trd>
  <trdgrp lng="fr">
    <trd>&eacute;clairage public<klr tip="amb">
    (r&eacute;verb&egrave;re)</klr></trd>,
    <trd>r&eacute;verb&egrave;re</trd>
  </trdgrp>
  <trd lng="id"><ind>lampu</ind> jalan</trd>
  <trd lng="nl">straatlantaarn</trd>
  <trd lng="pl">latarnia<klr> uliczna</klr></trd>
  <trd lng="sk">pouli&ccaron;n&aacute; lampa</trd>
  <trdgrp lng="zh">
    <trd>&#x8DEF;&#x706F; <pr>l&#xF9;d&#x113;ng</pr></trd>,
    <trd>&#x8DEF;&#x71C8; <pr>l&#xF9;d&#x113;ng</pr></trd>,
    <trd>&#x8857;&#x706F; <pr>ji&#x113;d&#x113;ng</pr></trd>,
    <trd>&#x8857;&#x71C8; <pr>ji&#x113;d&#x113;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lamp.tablo0o">
  <kap>tablo<tld/>o</kap>
  <snc mrk="lamp.tablo0o.LUM">
    <dif>
      Surtabla <ref tip="super" cel="lamp.0o.LUM"><tld/>o</ref>
      prilumanta la tablan surfacon:
      <ekz>
	la tablo<tld/>o estas envolvita en la ru&gcirc;a &scirc;tofo,
	lasante nur mallar&gcirc;an strion de la radioj, falantaj rekte
	la manuskripton
	<fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_l;&c_ja;&c_m;&c_p;&c_a;</trd>
  <trd lng="ca">l&agrave;mpada de taula</trd>
  <trd lng="cs">stoln&iacute; lampa</trd>
  <trd lng="de">Tischlampe</trd>
  <trd lng="fr">lampe de bureau</trd>
  <trd lng="id"><ind>lampu</ind> meja</trd>
  <trd lng="nl">bureaulamp</trd>
  <trd lng="pl">lampa biurkowa</trd>
  <trdgrp lng="pt">
   <trd>l&acirc;mpada de mesa</trd>
   </trdgrp>
  <trd lng="sk">stoln&aacute; lampa</trd>
  <trdgrp lng="zh">
    <trd>&#x53F0;&#x706F; <pr>t&#xE1;id&#x113;ng</pr></trd>,
    <trd>&#x6AAF;&#x71C8; <pr>t&#xE1;id&#x113;ng</pr></trd>
  </trdgrp>
</drv>
</art>
