<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pulover.xml,v 1.13 2020/07/19 11:10:37 revo Exp $">
<kap>
  <rad>pulover</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="pulover.0o">
  <kap><tld/>o</kap>
  <snc mrk="pulover.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Trikita <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="vest.0o">vesto</ref>
      kovranta la torson kaj la brakojn, senfenda, kiun
      oni surmetas tra&scirc;ovante la kapon:
      <ekz>
        la knaboj havis longajn hararojn kaj striitajn <tld/>ojn
        <fnt>
          <bib>DOz</bib>
          <lok>&leftquot;la na&ubreve; porketoj&rightquot;</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_u;&c_l;&c_o;&c_v;&c_je;&c_r;</trd>
  <trd lng="cs">pulovr</trd>
  <trdgrp lng="de">
    <trd>Pullover</trd>,
    <trd>Pulli</trd>
  </trdgrp>
  <trd lng="en">pullover</trd>
  <trd lng="es">jersey</trd>
  <trdgrp lng="fr">
    <trd>pull</trd>,
    <trd>pullover</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pull</trd>,
    <trd>pullover</trd>,
    <trd>trui</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>su&eacute;ter</trd>,  
    <trd>pul&ocirc;ver</trd>
  </trdgrp>
  <trd lng="sk">pul&oacute;ver</trd>
  <trdgrp lng="sv">
    <trd>pullover</trd>,
    <trd>tr&ouml;ja</trd>
  </trdgrp>
</drv>

<drv mrk="pulover.kapucx0o">
  <kap>kapu&ccirc;<tld/>o</kap>
  <snc mrk="pulover.kapucx0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="pulover.0o">Pulovero</ref>
      provizita per kapu&ccirc;o:
      <ekz>
        li estas 24-jara, havas bluan kapu&ccirc;<tld/>on
        kaj bluan &gcirc;inzon
        <fnt>
          <aut>K. Kniivil&auml;</aut>
          <vrk>Homoj de Putin</vrk>
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">pullover &agrave; capuche</trd>
</drv>


</art>

