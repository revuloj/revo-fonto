<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pulover.xml,v 1.16 2022/08/11 07:30:52 revo Exp $">
<kap>
  <rad>pulover</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="pulover.0o">
  <kap><tld/>o</kap>
  <snc mrk="pulover.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Trikita <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="vest.0o">vesto</ref>
      kovranta la torson kaj la brakojn, senfenda, kiun
      oni surmetas tra&scirc;ovante la kapon:
      <ekz>
        la knaboj havis longajn hararojn kaj striitajn <tld/>ojn
        <fnt>
          <bib>DOz</bib>
          <lok>&leftquot;la na&ubreve; porketoj&rightquot;</lok>
        </fnt>;
      </ekz>
      <ekz>
        lokaj viroj nun vestas sin per &scirc;aflanaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2012/010778.php">Falklandaj Insuloj prosperas</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i surhavis mallongan jakon, sub kiu estis malhelru&gcirc;a <tld/>o kaj tenera blanka
        bluzo
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2013/010962.php">La iluziisto</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_l;&c_o;&c_v;&c_e;&c_r;</trd>
  <trd lng="bg">&c_p;&c_u;&c_l;&c_o;&c_v;&c_je;&c_r;</trd>
  <trd lng="cs">pulovr</trd>
  <trdgrp lng="de">
    <trd>Pullover</trd>,
    <trd>Pulli</trd>
  </trdgrp>
  <trd lng="en">pullover</trd>
  <trd lng="es">jersey</trd>
  <trd lng="fi">villapusero</trd>
  <trdgrp lng="fr">
    <trd>pull</trd>,
    <trd>pullover</trd>
  </trdgrp>
  <trd lng="gd">geansaidh</trd>
  <trd lng="hr">pulover</trd>
  <trdgrp lng="hu">
    <trd>pul&oacute;ver</trd>,
    <trd>garb&oacute;</trd>
  </trdgrp>
  <trd lng="hy">&#1405;&#1406;&#1387;&#1407;&#1381;&#1408;</trd>
  <trd lng="id">sweater</trd>
  <trd lng="io">trikoturo</trd>
  <trd lng="it">maglione</trd>
  <trd lng="ka">&#4318;&#4323;&#4314;&#4317;&#4309;&#4308;&#4320;&#4312;</trd>
  <trd lng="ko">&#49828;&#50920;&#53552;</trd>
  <trd lng="mt">&#289;ersi</trd>
  <trdgrp lng="nl">
    <trd>pull</trd>,
    <trd>pullover</trd>,
    <trd>trui</trd>
  </trdgrp>
  <trd lng="pl">sweter</trd>
  <trdgrp lng="pt">
    <trd>su&eacute;ter</trd>,  
    <trd>pul&ocirc;ver</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_u;&c_l;&c_o;&c_v;&c_je;&c_r;</trd>
  <trd lng="sk">pul&oacute;ver</trd>
  <trdgrp lng="sv">
    <trd>pullover</trd>,
    <trd>tr&ouml;ja</trd>
  </trdgrp>
  <trd lng="th">&#3648;&#3626;&#3639;&#3657;&#3629;&#3612;&#3657;&#3634;&#3607;&#3637;&#3656;&#3651;&#3594;&#3657;&#3626;&#3623;&#3617;&#3627;&#3619;&#3639;&#3629;&#3604;&#3638;&#3591;&#3629;&#3629;&#3585;&#3607;&#3634;&#3591;&#3588;&#3629;</trd>
  <trd lng="tr">kazak</trd>
  <trd lng="uk">&c_p;&c_u;&c_l;&c_o;&c_v;&c_je;&c_r;</trd>
</drv>

<drv mrk="pulover.kapucx0o">
  <kap>kapu&ccirc;<tld/>o</kap>
  <snc mrk="pulover.kapucx0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="pulover.0o">Pulovero</ref>
      provizita per kapu&ccirc;o:
      <ekz>
        li estas 24-jara, havas bluan kapu&ccirc;<tld/>on
        kaj bluan &gcirc;inzon
        <fnt>
          <aut>K. Kniivil&auml;</aut>
          <vrk>Homoj de Putin</vrk>
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/f/fc/Cory_Carroll.jpg/180px-Cory_Carroll.jpg"
      prm="CC BY-SA 3.0"
      >pokerante en kapu&ccirc;<tld/>o, Cory Carroll, 2007
        <fnt>
          <aut>Photos by flipchip / LasVegasVegas.com,
          http://www.lasvegasvegas.com/viewer/v/2007WSOP/2007+WSOP+Day+4/Cory+Carroll.jpg.html</aut>
          <lok><url ref="&WCW;/File:Cory_Carroll.jpg"
              >&WCW;/File:Cory_Carroll.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_h;&c_u;&c_d;&c_z;&c_ib;</trd>
  <trd lng="de">Kapuzenpulli</trd>
  <trd lng="en">hoodie</trd>
  <trd lng="fr">pullover &agrave; capuche</trd>
</drv>


</art>

