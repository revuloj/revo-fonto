<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pulover</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="pulover.0o">
  <kap><tld/>o</kap>
  <snc mrk="pulover.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Trikita vesto kovranta la torson kaj la brakojn, senfenda, kiun
      oni surmetas tra&scirc;ovante la kapon:
      <ekz>
        la knaboj havis longajn hararojn kaj striitajn <tld/>ojn
        <fnt><bib>DOz</bib>, <lok>&leftquot;la na&ubreve;
porketoj&rightquot;</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_u;&c_l;&c_o;&c_v;&c_je;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Pullover</trd>,
    <trd>Pulli</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pull</trd>,
    <trd>pullover</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pull</trd>,
    <trd>pullover</trd>,
    <trd>trui</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>su&eacute;ter</trd>,  
    <trd>pul&ocirc;ver</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>pullover</trd>,
    <trd>tr&ouml;ja</trd>
  </trdgrp>
</drv>
</art>

