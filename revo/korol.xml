<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: korol.xml,v 1.22 2023/12/03 09:58:58 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>korol</rad>/o
</kap>
<drv mrk="korol.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="korol.0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      En <ref tip="malprt" cel="flor.0o.BOT">floro</ref> la
      interna cirklo de florfolioj, ofte delikate maldikaj kaj kun
      okulfrapaj koloroj:
      <ekz>
        la <tld/>o de la floro plej velura
        <fnt>
       <aut>Manoel BORGES DOS SANTOS</aut>
          <vrk><url ref="&Monato;/2000/006863.php">Poemoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 32</lok>
        </fnt>;
      </ekz>
      <ekz>
        salvio: <klr>[&#x2026;]</klr> dulipa <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4009">Salvio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_n;&c_o;&c_ch;&c_a;&c_k; <klr tip="amb">(&c_k;&c_a;&c_l;&c_ja;&c_k;&c_v;&c_je;&c_t;&c_n;&c_ib;&c_k;&c_a;)</klr></trd>
  <trd lng="ca">corol.la</trd>
  <trd lng="de">Krone <klr>(Bl&uuml;te)</klr></trd>
  <trd lng="en">corolla</trd>
  <trd lng="es">corola</trd>
  <trd lng="fr">corolle</trd>
  <trd lng="hu">p&aacute;rta<klr tip="amb"> (vir&aacute;gzatban)</klr></trd>
  <trd lng="ja">&#x82B1;&#x51A0;  <pr>&#x304B;&#x304B;&#x3093;</pr></trd>
  <trd lng="nl">bloemkroon</trd>
  <trd lng="pl">korona kwiatu</trd>
  <trd lng="pt">corola</trd>
  <trd lng="ro">corol&abreve;</trd>
  <trd lng="ru">&c_v;&c_je;&c_n;&c_ch;&c_i;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#x82B1;&#x51A0; <pr>hu&#x101;gu&#x101;n</pr></trd>,
    <trd>&#x8469; <pr>p&#x101;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="korol.0afolio">
  <kap>
    <tld/>a folio
  </kap>
  <snc mrk="korol.0afolio.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      Unu folio de la <ref tip="malprt" cel="korol.0o"><tld/>o</ref>.
    </dif>
    <ref tip="sin" cel="petal.0o">petalo</ref>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_l;&c_jo;&c_s;&c_t;&c_a;&c_k; &c_v;&c_ja;&c_n;&c_o;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Kronblatt</trd>,
    <trd>Bl&uuml;tenblatt</trd>
  </trdgrp>
   <trd lng="en">petal</trd>
   <trd lng="fr">p&eacute;tale</trd>
  <trd lng="hu">p&aacute;rtalev&eacute;l</trd>
  <trd lng="nl">kroonblad</trd>
  <trd lng="pl">p&lstroke;atek korony</trd>
  <trd lng="ro">coroana petal&abreve;</trd>
  <trdgrp lng="zh">
    <trd>&#x82B1;&#x74E3; <pr>hu&#x101;b&#xE0;n</pr></trd>,
    <trd>&#x74E3; <pr>b&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
