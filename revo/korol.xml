<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>korol</rad>/o
</kap>
<drv mrk="korol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      En <ref tip="malprt" cel="flor.0o.BOT">floro</ref> la
      interna cirklo de florfolioj, ofte delikate maldikaj kaj kun
      okulfrapaj koloroj.
    </dif>
  </snc>
  <trd lng="de">Krone</trd>
</drv>
<drv mrk="korol.0afolio">
  <kap><tld/>a folio</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Unu folio de la <ref tip="malprt" cel="korol.0o"><tld/>o</ref>.
    </dif>
    <ref tip="sin" cel="petal.0o">petalo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Kronblatt</trd>,
    <trd>Bl&uuml;tenblatt</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
