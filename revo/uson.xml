<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
     <ofc>9</ofc><rad>Uson</rad>/o
</kap>
<drv mrk="uson.U0o">
     <kap><ofc>9</ofc><tld/>o</kap>
     <uzo tip="fak">GEOG</uzo>
     <uzo tip="fak">POL</uzo>
     <fnt><bib>ZR</bib></fnt>
     <snc>
       <dif>
         Granda <ref tip="lst" cel="sxtat.0o"
         lst="voko:&scirc;tatoj">&scirc;tato</ref>
         <ref tip="malprt" cel="amerik.Nord0o">Nordamerika</ref>,
         federanta 50 <ref cel="sxtat.sub0o">sub&scirc;tatojn</ref>; la
         federa &ccirc;efurbo estas Va&scirc;in(g)tono:
         <ekz>
           saluton al vi, <tld/>o, plej potenca reprezentanto
           de la nova mondo
           <fnt>
           <aut>&amp;Z</aut>: <vrk>La parolado anta&ubreve; la Sesa
           Kongreso
           Esperantista, Washington, 1910-08-15.</vrk></fnt>.
         </ekz>
       </dif>
       <rim>
         Malgra&ubreve; la impreso kiun oni povas ricevi el PIV, en
         Esperanto la vorto <tld/>o ne estas mallongigo de
         la Esperanta vortgrupo
         <ctl>Unui&gcirc;intaj &Scirc;tatoj de Nord-Ameriko</ctl>, sed
         plenvalora
         vorto, proponita ekster Esperantujo de usonanoj Frank Lloyd
         Wright kaj James Duff Law kaj aliaj la&ubreve; la angla vortgrupo
         <ctl>United States of North America</ctl>
         (en la formo <ctl>Usonia</ctl>), &ccirc;e la limo inter
         19/20 jc. &Gcirc;i tamen plej firme enradiki&gcirc;is en Esperanto.
       </rim>
     </snc>
     <trdgrp lng="be">
       <trd>&c_Z;&c_Sh;&c_A;</trd>,
       <trd>&c_Z;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_o;&c_ch;&c_a;&c_n;&c_y;&c_ja;
       &c_Sh;&c_t;&c_a;&c_t;&c_y;
       &c_A;&c_m;&c_e;&c_r;&c_y;&c_k;&c_ib;</trd>,
       <trd>&c_Z;&c_l;&c_u;&c_ch;&c_a;&c_n;&c_y;&c_ja;
       &c_Sh;&c_t;&c_a;&c_t;&c_y;
       &c_A;&c_m;&c_e;&c_r;&c_y;&c_k;&c_ib;</trd>
     </trdgrp>
     <trdgrp lng="bg">
       <trd>&c_S;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_je;&c_n;&c_i; &c_a;&c_m;&c_je;&c_r;&c_i;&c_k;&c_a;&c_n;&c_s;&c_k;&c_i; &c_shch;&c_a;&c_t;&c_i;</trd>,
       <trd>&c_S;&c_A;&c_Shch;</trd>
     </trdgrp>
     <trdgrp lng="de">
       <trd>Vereinigte Staaten von <ind>Amerika</ind></trd>,
       <trd>USA</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>United States of America</trd>,
       <trd>USA</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>Estados Unidos de Am&eacute;rica</trd>,
       <trd>EE.UU.</trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>Yhdysvallat</trd>,
       <trd>USA</trd>
     </trdgrp>
     <trdgrp lng="fr">
       <trd>&Eacute;tats-unis</trd>,
       <trd>E.-U.</trd>,
       <trd>USA</trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>Amerikai Egyes&uuml;lt &Aacute;llamok</trd>,
       <trd>USA</trd>
     </trdgrp>
     <trdgrp lng="it">
       <trd>Stati Uniti d'America</trd>,
       <trd>USA</trd>
     </trdgrp>
     <trdgrp lng="nl">
       <trd>Verenigde Staten van <ind>Amerika</ind></trd>,
       <trd>V.S.</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>Stany Zjednoczone Ameryki P&oacute;&lstroke;nocnej<klr> 
       (oficialnie)</klr></trd>,
       <trd>USA</trd>,
       <trd>Stany</trd>,
       <trd>Stany Zjednoczone</trd>,
       <trd>Stany Zjednoczone Ameryki</trd>
     </trdgrp>
     <trdgrp lng="pt">
       <trd>Estados Unidos da Am&eacute;rica</trd>,
       <trd>EUA</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_S;&c_o;&c_je;&c_d;&c_i;&c_n;&c_jo;&c_n;&c_n;&c_y;&c_je;
       &c_sh;&c_t;&c_a;&c_t;&c_y;
       &c_A;&c_m;&c_je;&c_r;&c_i;&c_k;&c_i;</trd>,
       <trd>&c_S;&c_Sh;&c_A;</trd>
     </trdgrp>
     <trdgrp lng="sk">
       <trd>Spojen&eacute; &scaron;t&aacute;ty americk&eacute;</trd>,
       <trd>USA</trd>
     </trdgrp>
     <trdgrp lng="zh">
       <trd>&#32654;&#22269;</trd>
     </trdgrp>
     <trdgrp lng="sv">
       <trd>Amerikas f&ouml;renta stater</trd>,
       <trd>USA</trd>
     </trdgrp>
</drv>

<drv mrk="uson.0a">
     <kap><tld lit="u"/>a</kap>
     <snc>
       <dif>
         Rilata al <tld/>o:
         <ekz>
           la <tld lit="u"/>a formo de la angla lingvo;
         </ekz>
         <ekz>
           li pagis 15 <tld lit="u"/>ajn dolarojn;
         </ekz>
         <ekz>
           <ind>la <tld lit="u"/>a revo</ind>
           <fnt>
           <aut>V. Rybakov, tr. J. Finkel</aut>:
           <vrk>Gravitavio &leftquot;Carido&rightquot;</vrk>.
           <lok><url
           ref="http://www.esperanto.mv.ru/Gravitavio/08.html">Alvico</url></lok>.</fnt>.
           <trd lng="en">the <ind>American dream</ind></trd>
           <trd lng="hu">az amerikai <ind>&aacute;lom</ind></trd>
           <trd lng="pl">ameryka&nacute;ski sen</trd>
           <trd
           lng="ru">&c_A;&c_m;&c_je;&c_r;&c_i;&c_k;&c_a;&c_n;&c_s;&c_k;&c_a;&c_ja;
           &c_m;&c_je;&c_ch;&c_t;&c_a;</trd>
         </ekz>
       </dif>
     </snc>
     <trd
     lng="be">&c_a;&c_m;&c_e;&c_r;&c_y;&c_k;&c_a;&c_n;&c_s;&c_k;&c_ib;
     <klr>(&c_d;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y; &c_Z;&c_Sh;&c_A;,
     &c_n;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_y; &c_d;&c_a;
     &c_Z;&c_Sh;&c_A;)</klr></trd>
     <trdgrp lng="de">
       <trd>US-amerikanisch</trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>estadounidense</trd>,
       <trd>norteamericano</trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>yhdysvaltalainen <klr tip="ind"> (adj.)</klr></trd>,
       <trd>Yhdysvaltojen</trd>,
       <trd>USA:n</trd>
     </trdgrp>
     <trdgrp lng="fr">
       <trd>am&eacute;ricain<klr> (&eacute;tasunien)</klr></trd>
     </trdgrp>
     <trdgrp lng="hu">
       <trd>USA-beli</trd>,
       <trd>USA-</trd>,
       <trd>amerikai</trd>
     </trdgrp>
     <trd lng="it">statunitense<klr> (agg.)</klr></trd>
     <trd lng="nl">Amerikaans<klr tip="ind"> (betr. de V.S.)</klr></trd>
     <trdgrp lng="pl">
       <trd>ameryka&nacute;ski</trd>,
       <trd>p&oacute;&lstroke;nocnoameryka&nacute;ski</trd>
     </trdgrp>
     <trd lng="pt">norte-americano</trd>
     <trdgrp lng="ru">
       <trd>&c_a;&c_m;&c_je;&c_r;&c_i;&c_k;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;
       <klr tip="amb">(&c_o; &c_S;&c_Sh;&c_A;)</klr></trd>,
       <trd>&c_sh;&c_t;&c_a;&c_t;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j; <klr
       tip="amb">(&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_r;&c_je;&c_ch;&c_n;.)</klr></trd>
     </trdgrp>
     <trd lng="sk">americk&yacute;</trd>
     <trd lng="sv">amerikansk<klr> (g&auml;llandes USA)</klr></trd>
</drv>

<drv mrk="uson.0ano">
     <kap><tld lit="u"/>ano</kap>
     <snc>
       <dif>
         Civitano de <tld/>o:
         <ekz>
           ni venis al vi, <tld lit="u"/>anoj, precipe por pasigi en
           via mezo kaj anta&ubreve; viaj okuloj unu semajnon de nia
           esperantista vivo
           <fnt>
           <aut>&amp;Z</aut>: <vrk>La parolado anta&ubreve; la Sesa
           Kongreso
           Esperantista, Washington, 1910-08-15.</vrk></fnt>.
         </ekz>
       </dif>
       <ref tip="super" cel="amerik.0ano">amerikano</ref>
     </snc>
     <trd lng="be">&c_a;&c_m;&c_e;&c_r;&c_y;&c_k;&c_a;&c_n;&c_je;&c_c;
     <klr>(&c_zh;&c_y;&c_h;&c_a;&c_r;,
     &c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_ib;&c_n;
     &c_Z;&c_Sh;&c_A;)</klr></trd>
     <trdgrp lng="de">
       <trd>US-Amerikaner</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>US citizen</trd>,
       <trd>American</trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>yhdysvaltalainen <klr tip="ind"> (subst.)</klr></trd>
       <trd>amerikkalainen <klr tip="amb"> (Yhdysvaltain
       kansalainen)</klr></trd>
     </trdgrp>
     <trdgrp lng="fr">
       <trd>am&eacute;ricain <klr> (citoyen des
       &Eacute;tats-unis)</klr></trd>
     </trdgrp>
     <trd lng="hu">amerikai<klr> (USA-beli)</klr></trd>
     <trd lng="it">statunitense<klr> (sost.)</klr></trd>
     <trdgrp lng="nl">
       <trd>Amerikaan</trd>,
       <trd>Amerikaans staatsburger</trd>
     </trdgrp>
     <trdgrp lng="pl">
       <trd>Amerykanin</trd>,
       <trd>obywatel <ind>USA</ind></trd>,
       <trd>obywatel <ind>Stan&oacute;w Zjednoczonych Ameryki 
       P&oacute;&lstroke;nocnej</ind><klr> (oficialnie)</klr></trd>
     </trdgrp>
     <trdgrp lng="pt">
       <trd>norte-americano</trd>,
       <trd>estadunidense</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_g;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_n;&c_i;&c_n;
       &c_S;&c_Sh;&c_A;</trd>,
       <trd>&c_a;&c_m;&c_je;&c_r;&c_i;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
     </trdgrp>
     <trd lng="sk">Ameri&ccaron;an</trd>
     <trdgrp lng="zh">
       <trd>&#32654;&#22269;&#20154;</trd>
     </trdgrp>
     <trdgrp lng="sv">
       <trd>amerikan<klr> (USA-medborgare)</klr></trd>,
       <trd>amerikansk medborgare<klr> (USA-medborgare)</klr></trd>
     </trdgrp>
</drv>
