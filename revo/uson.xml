   </trdgrp>
   <trd lng="pt">norte-americano</trd>
   <trdgrp lng="ru">
     <trd>&c_a;&c_m;&c_je;&c_r;&c_i;&c_k;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;
     <klr tip="amb">(&c_o; &c_S;&c_Sh;&c_A;)</klr></trd>,
     <trd>&c_sh;&c_t;&c_a;&c_t;&c_o;&c_v;&c_s;&c_k;&c_i;&c_j; <klr tip="amb">(&c_p;&c_r;&c_o;&c_s;&c_t;&c_o;&c_r;&c_je;&c_ch;&c_n;.)</klr></trd>
   </trdgrp>
   <trd lng="sk">americk&yacute;</trd>
