<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>decembr</rad>/o
</kap>
<drv mrk="decemb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Dek-dua <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj"
      val="12">monato</ref> de la <ref tip="malprt"
      cel="jar.0o.KAL">jaro</ref> la&ubreve; la Gregoria kalendaro, inter
      novembro kaj januaro.
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">januaro, Rim.</ref>
  <trd lng="af">Desember</trd>
  <trd lng="be">&c_s;&c_mol;&c_n;&c_je;&c_zh;&c_a;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_je;&c_k;&c_je;&c_m;&c_v;&c_r;&c_i;</trd>
  <trd lng="br">Kerzu</trd>
  <trd lng="cs">prosinec</trd>
  <trd lng="de">Dezember</trd>
  <trd lng="en">December</trd>
  <trd lng="es">diciembre</trd>
  <trd lng="fr">d&eacute;cembre</trd>
  <trd lng="fy">desimber</trd>
  <trd lng="he">&dalet;&tsadi;&mem;&bet;&resh;</trd>
  <trd lng="hu">december</trd>
  <trd lng="id">Desember</trd>
  <trd lng="io">decembro</trd>
  <trd lng="ja">&#21313;&#20108;&#26376; [<ind>&#12376;&#12421;&#12358;&#12395;&#12364;&#12388;</ind>]</trd>
  <trdgrp lng="jbo">
    <trd>pavrelma'i</trd>,
    <trd>celma'i</trd>
  </trdgrp>
  <trd lng="nl">december</trd>
  <trd lng="pl">grudzie&nacute;</trd>
  <trd lng="pt">dezembro</trd>
  <trd lng="ro">decembrie</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_a;&c_b;&c_r;&c_mol;</trd>
  <trd lng="sk">december</trd>
  <trd lng="sv">december</trd>
  <trd lng="tp">tenpo mun nanpa luka luka tu</trd>
  <trd lng="tr">aral&inodot;k</trd>
  <trd lng="vo">dekul</trd>
</drv>

<drv mrk="decemb.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o.
    </dif>
  </snc>
  <trd lng="io">decembrala</trd>
  <trd lng="vo">dekulik</trd>
</drv>
</art>
