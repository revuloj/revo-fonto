<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Altaj</rad>/o
    </kap>
    <drv mrk="altaj.A0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Montaro inter la Centra <ref tip="malprt" cel="azi.A0o">Azio</ref>
          kaj <ref cel="siberi.0o">Siberio</ref>, sur la teritorio de
          la <ref cel="rus.R0aFederacio">Ruslanda Feferacio</ref>,
          Kaza&hcirc;ujo kaj Mongolujo, formanta akvodislimojn inter la
          basenoj de <ref cel="ob.O0o">Obo</ref>,
          <ref cel="irtisx.I0o">Irti&scirc;o</ref>,
          <ref cel="jenisej.J0o">Jenisejo</ref> kaj la sendefluaj riveroj de
          la Centra Azio.  Pli ol 2000km longa.
        </dif>
      </snc>
      <trd lng="en">Altai</trd>
      <trd lng="hu">Altaj</trd>
      <trd lng="ru">&c_A;&c_l;&c_t;&c_a;&c_j;</trd>
    </drv>
    <drv mrk="altaj.0a.lingvofamilio">
      <kap><tld/>a lingvofamilio</kap>
      <snc>
        <uzo tip="fak">LIN</uzo>
        <dif>
          Hipoteze rekonstruata superfamilio konsistanta el la
          tjurka, mongola kaj tungusa-man&ccirc;ura lingvofamilioj.
        </dif>
      </snc>
      <trd lng="hu">altaji nyelvcsal&aacute;d</trd>
      <trd lng="ru">&c_a;&c_l;&c_t;&c_a;&c_j;&c_s;&c_k;&c_a;&c_ja;
&c_s;&c_je;&c_m;&c_mol;&c_ja; &c_ja;&c_z;&c_y;&c_k;&c_o;&c_v;</trd>
    </drv>
    <drv mrk="altaj.0a.lingvo">
      <kap><tld lit="a"/>a lingvo</kap>
      <snc mrk="altaj.0a.lingvofamilia">
        <uzo tip="fak">LIN</uzo>
        <dif>
          Lingvo de la
          <ref tip="malprt" cel="altaj.0a.lingvofamilio"><tld/>a
lingvofamilio</ref>.
        </dif>
      </snc>
      <trd lng="hu">altaj nyelv</trd>
      <snc mrk="altaj.0a.lingvoloka">
        <uzo tip="fak">LIN</uzo>
        <dif>
          &Ccirc;iu el la indi&gcirc;enaj lingvoj de <tld/>o.
        </dif>
      </snc>
      <trd lng="hu">altaj nyelv</trd>
      <snc mrk="altaj.0a.lingvoojrota">
        <uzo tip="fak">LIN</uzo>
        <dif>
          La &ccirc;efa indi&gcirc;ena lingvo de <tld/>a Respubliko en
Ruslando
          (ekse <ctl>la ojrota</ctl>, el la tjurka lingvofamilio).
        </dif>
      </snc>
      <trdgrp lng="hu">
        <trd>altaj nyelv</trd>,
        <trd>ojrot</trd></trdgrp>
      <trd lng="ru">&c_a;&c_l;&c_t;&c_a;&c_j;&c_s;&c_k;&c_i;&c_j;
&c_ja;&c_z;&c_y;&c_k;</trd>
    </drv>
  </art>

