<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>merkred</rad>/o
</kap>

<drv mrk="merkre.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc>
    <dif>
      Kvara <ref tip="super" cel="tag.0o.diurn">tago</ref> de la
      semajno, inter mardo kaj &jcirc;a&ubreve;do.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_r;&c_a;&c_d;&c_a;</trd>
  <trd lng="bg">&c_s;&c_r;&c_ja;&c_d;&c_a;</trd>
  <trdgrp lng="br">
    <trd>Merc'her</trd>,
    <trd>dimerc'her</trd>
  </trdgrp>
  <trd lng="ca">dimecres</trd>
  <trd lng="cs">st&rcaron;eda</trd>
  <trd lng="de">Mittwoch</trd>
  <trd lng="en">Wednesday</trd>
  <trd lng="es">mi&eacute;rcoles</trd>
  <trd lng="fa">&f_ch;&a_h;&a_A;&a_r;&a_sh;&a_n;&a_b;&a_h;</trd>
  <trd lng="fr">mercredi</trd>
  <trd lng="he">&yod;&vav;&fmem; &resh;&bet;&yod;&ayin;&yod;</trd>
  <trd lng="hu">szerda</trd>
  <trd lng="id">Rabu</trd>
  <trd lng="it">mercoled&igrave;</trd>
  <trd lng="ja">&#27700;&#26332;&#26085; [<ind>&#12377;&#12356;&#12424;&#12358;&#12403;</ind>]</trd>
  <trd lng="nl">woensdag</trd>
  <trd lng="pl">&sacute;roda</trd>
  <trd lng="pt">quarta-feira</trd>
  <trd lng="ro">miercuri</trd>
  <trd lng="ru">&c_s;&c_r;&c_je;&c_d;&c_a;</trd>
  <trd lng="sv">onsdag</trd>
  <trd lng="vo">vesdel</trd>
</drv>

<drv mrk="merkre.cindra0o">
  <kap>Cindra <tld lit="M"/>o, <var><kap>Cindro<tld/>o</kap></var></kap>
  <snc>
    <dif>
      En la okcidenta kristanismo, la <tld/>o je 46 tagoj
      anta&ubreve;anta Paskon, la unua tago de la
      <ref cel="fast.granda0o.KAT">Granda Fasto</ref> okcidenta.
    </dif>
    <rim>
      La nomo venas de la romkatolika kutimo, enkondukita sub papo
      Gregorio la Granda, meti tiutage cindron sur la frunton de
      la pentofarantoj.
    </rim>
  </snc>
  <trd lng="ca">dimecres de Cendra</trd>
  <trd lng="de">Aschermittwoch</trd>
  <trd lng="en">Ash Wednesday</trd>
  <trd lng="es">Mi&eacute;rcoles de Ceniza</trd>
  <trd lng="fa">&f_ch;&a_h;&a_A;&a_r;&a_sh;&a_n;&a_b;&a_h;&a_hamza_sure; &a_kh;&a_A;&f_k;&a_s;&a_t;&a_r;</trd>
  <trd lng="fr">mercredi des Cendres</trd>
  <trd lng="hu">hamvaz&oacute;szerda</trd>
  <trdgrp lng="it">
    <trd>mercoled&igrave; delle <ind>Ceneri</ind></trd>,
    <trd>le <ind>Ceneri</ind></trd>
  </trdgrp>
  <trd lng="ja">&#28784;&#12398;&#27700;&#26332;&#26085;
  [<ind>&#12399;&#12356;&#12398;&#12377;&#12356;&#12424;&#12358;&#12403;</ind>]</trd>
  <trd lng="nl">aswoensdag</trd>
  <trd lng="pl">&Sacute;roda Popielcowa</trd>
  <trd lng="pt">Quarta-feira de Cinzas</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_v;&c_a;&c_ja; &c_s;&c_r;&c_je;&c_d;&c_a;
  &c_v;&c_je;&c_l;&c_i;&c_k;&c_o;&c_g;&c_o; &c_p;&c_o;&c_s;&c_t;&c_a;</trd>
</drv>
</art>
