<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mod</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mod.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mod.0o.KOMUNE">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Neda&ubreve;ra, kaprica kutimo, moro a&ubreve; maniero, koncernanta
      precipe vestadon:
      <ekz>
        vestita la&ubreve; la plej nova pariza <tld/>o;
      </ekz>
      <ekz>
        vestitaj la&ubreve; la tiuepoka <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        vestita la&ubreve; la lasta <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas la lasta <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mallongaj roboj estas nun en <tld/>o;
      </ekz>
      <ekz>
        &ccirc;apelo nigra, &ccirc;ifita kaj de anta&ubreve;longe pasinta <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        homoj, kiuj okupas sin je &ccirc;ia plej sencela kaj senutila
        sensenca&jcirc;o, 
        se &gcirc;i nur estas en <tld/>o kaj konforma al la rutinaj ideoj de
        la amaso
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        estas nun <tld/>o interesi&gcirc;i pri sporto;
      </ekz>
      <ekz>
        mezuri &ccirc;ion per mezurilo de la <tld/>o
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        en &ccirc;iu urbo naski&gcirc;is <tld/>o trakuri la stratojn kun tor&ccirc;oj, 
        muziko, kaj anta&ubreve; &ccirc;io kun plenaj kru&ccirc;oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri tiuj &ccirc;i ideoj estas <tld/>o paroli ne alie, ol kun ironia
        kaj malestima rideto
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="furor.0o">furoro</ref>,
      <ref cel="sensac1.0o">sensacio</ref>
    </refgrp>
    <trd lng="be">&c_m;&c_o;&c_d;&c_a;</trd>
    <trd lng="br">giz</trd>
    <trd lng="en">fashion</trd>
    <trd lng="es">moda</trd>
    <trd lng="fr">mode<klr> (vest. etc.)</klr></trd>
    <trd lng="io">modo</trd>
    <trdgrp lng="ja">
      <trd>&#12501;&#12449;&#12483;&#12471;&#12519;&#12531; [<ind>&#12405;&#12353;&#12387;&#12375;&#12423;&#12435;</ind>]</trd>,
      <trd>&#27969;&#34892; [<ind>&#12426;&#12421;&#12358;&#12371;&#12358;</ind>]</trd>,
      <trd>&#12399;&#12420;&#12426;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>mode</trd>,
      <trd>fashion</trd>
    </trdgrp>
    <trd lng="pl">moda</trd>
    <trd lng="ru">&c_m;&c_o;&c_d;&c_a;</trd>
  </snc>
  <rim>
     Vd <ref tip="hom" cel="mod1.0o">modo</ref> <klr>(gramatiko)</klr>.
  </rim>
  <trdgrp lng="cs">
    <trd>modus</trd>,
    <trd>nej&ccaron;ast&ecaron;j&scaron;&iacute; hodnota statistick&eacute;ho souboru</trd>
  </trdgrp>
  <trd lng="de">Mode</trd>
  <trdgrp lng="sk">
    <trd>modus</trd>,
    <trd>naj&ccaron;astej&scaron;ia hodnota &scaron;tatistick&eacute;ho s&uacute;boru</trd>
  </trdgrp>
</drv>

<drv mrk="mod.0a">
  <kap><tld/>a</kap>
  <snc mrk="mod.0a.MOR">
    <dif>
      Rilata al la <ref tip="vid" cel="mod.0o.KOMUNE"><tld/>o
      <sncref/></ref>.
    </dif>
    <subsnc>
      <dif>
        Prezentanta, priparolanta la <tld/>on:
        <ekz>
          (pri)<tld/>a gazeto.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="mod.0a.lauxmoda">
      <dif>
        <ref tip="dif" cel="mod.laux0a.MOR">La&ubreve;<tld/>a</ref>:
        <ekz>
          en la alta societo plej <tld/>aj estis la interparoladoj pri
          la &gcirc;enerala mal&gcirc;ojo, kiu ekregis e&ccirc; la naturon
          <fnt>
            <bib>Far3</bib>
            <lok>&Ccirc;apitro V</lok>
          </fnt>;
        </ekz>
          <ekz>
            <klr>[li]</klr> &ccirc;iam ankora&ubreve; portis la malnova<tld/>ajn
            &gcirc;isgenuajn pantalonojn, 
            surtuton kun grandaj latunaj butonoj kaj perukon
            <fnt>
              <bib>Fab2</bib>
              <lok>Malnova domo</lok>
            </fnt>;
          </ekz>
          <ekz>
            malnov<tld/>a kale&scirc;o
            <fnt>
              <bib>Fab2</bib>
              <lok>Ru&gcirc;aj &scirc;uoj</lok>
            </fnt>,
            &scirc;ranko
            <fnt>
              <bib>Fab2</bib>
              <lok>Pa&scirc;tistino kaj kamentubisto</lok>
            </fnt>,
            florpotoj
            <fnt>
              <bib>Fab2</bib>
              <lok>Malnova domo</lok>
            </fnt>,
            Franca &gcirc;ardeno
            <fnt>
              <bib>Fab3</bib>
              <lok>En la lasta tago</lok>
            </fnt>,
            poet'
            <fnt>
              <aut>Raymond Schwartz</aut>
              <vrk>La Stranga Butiko</vrk>
              <lok>Tro Homa Sorto De Iu Ne&gcirc;homo</lok>
            </fnt>;
          </ekz>
          <ekz>
            anstata&ubreve; ekpensi pri tio, kion ili parolas, <klr>[ili]</klr>
            preferas blinde &jcirc;etadi dekstren kaj maldekstren la&ubreve;tajn kaj
            <tld/>ajn sed sensencajn frazojn
            <fnt>
              <bib>EE</bib>
            </fnt>;
          </ekz>
          <ekz>
            &gcirc;i estas nova kaj ankora&ubreve; ne <tld/>a
            <fnt>
              <bib>EE</bib>
            </fnt>;
          </ekz>
        <ekz>
          <tld/>aj doktrinoj
          <fnt><bib>Homaranismo</bib></fnt>.
        </ekz>
      </dif>
      <trd lng="br">diouzh ar c'hiz</trd>
      <trd lng="pl">modny</trd>
      <trdgrp lng="de">
        <trd>modisch</trd>,
        <trd>zeitgem&auml;&szlig;</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>&agrave; la mode</trd>,
        <trd>en vogue</trd>
      </trdgrp>
      <trd lng="nl">modieus</trd>
      <trd lng="ru">&c_m;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
    </subsnc>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_d;&c_n;&c_y;</trd>
  <trd lng="cs">m&oacute;dn&iacute;</trd>
  <trd lng="sk">m&oacute;dny</trd>
</drv>

<drv mrk="mod.0istino">
  <kap><tld/>istino</kap>
  <snc mrk="mod.0istino.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Virino, kiu faras, rekomendas a&ubreve; vendas <tld/>a&jcirc;ojn
      por virinoj:
      <ekz>
        li kolizias kun dikaj urbaninoj kaj ridemaj <tld/>istinoj
        <fnt>
          <aut>F. Ko&zcaron;&iacute;k, trad. J. Karen kaj J. Vondrou&scaron;ek</aut>,
          <vrk>La plej eminenta inter Pierotoj</vrk>,
          <lok>2001</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter ni homoj ekzistas kategorio da personoj, kiujn oni nomas 
        emancipitaj virinoj, al tiu kategorio apartenas pensionestrinoj,
        aku&scirc;istinoj, dancistinoj, <klr>[&#8230;]</klr> <tld/>istinoj kaj
        vartistinoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Du pavimbatiloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu volas posedi kiel eble plej multe da &ccirc;ambristinoj, 
        <tld/>istinoj, frizistoj, sklavoj por la portiloj,
        sklavoj por la &ccirc;ambroj, &ccirc;evaloj <klr>[&#8230;]</klr>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estis disportanto &ccirc;e <tld/>istino, kaj en tiu &ccirc;i 
        ofico mi en vakstolaj paketoj alportadis al la homoj nenion
        ekster trompa&jcirc;o
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estas tre bona, ora koro <klr>[sed]</klr> &scirc;i ne 
        amas kudradon, tamen &scirc;i nepre devas ellerni 
        metion <klr>[do la&ubreve; nia decido]</klr> &scirc;i estu <tld/>istino
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_d;&c_y;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="br">kabellerez</trd>
  <trd lng="cs">modistka</trd>
  <trd lng="de">Modistin</trd>
  <trd lng="es">modista</trd>
  <trd lng="fr">modiste</trd>
  <trd lng="nl">modiste</trd>
  <trd lng="pl">modystka</trd>
  <trd lng="ru">&c_m;&c_o;&c_d;&c_i;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="sk">modistka</trd>
</drv>

<drv mrk="mod.eks0a">
  <kap>eks<tld/>a</kap>
  <snc mrk="mod.eks0a.MOR">
    <dif>
      Kiu ne plu estas la&ubreve;<tld/>a, iam estis &gcirc;enerale
      &scirc;atata, sed jam ne:
      <ekz>
        se fotojn oni vidas pri Lidia Zamenhof, &gcirc;ene povas malhelpi
        nin bone rigardi la tiama <tld/>o: virinon ni vidas en ege
        eks<tld/>aj 
        vesta&jcirc;oj &#8208; kiuj ja supozeble estis tre la&ubreve;<tld/>aj tiutempe
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2010/009856.php">Respektinda virino.
          Granda animo &ccirc;u iom naiva?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi trovas &gcirc;in iom eks<tld/>a, almena&ubreve; por rokemaj kaj popemaj
        oreloj
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007002.html">Muziko: 
           Kanzonoj por interna konsumo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Germanio reformas sian impost-le&gcirc;aron, kiu jam de multaj
        jaroj estis eks<tld/>a, malekvilibra, &hcirc;aosa, tro komplika kaj
        precipe nejusta
        <fnt>mod
          <bib>Monato</bib>
          <aut>Maul Stefan</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007124.html">Majstroverko,
          
           sed nur taktika</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kasedoj ja i&gcirc;as pli kaj pli eks<tld/>aj
        <fnt>
          <bib>Monato</bib>
          <aut>pp/cs</aut>
          <vrk><url ref="&Monato;/2005/008544.php">Multe por malmulte da
          mono</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la tria generacio de komputiloj fari&gcirc;is eks<tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="https://www.monato.be/2004/008110.html">Longe
          atendata &ccirc;efverko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">e-maez a c'hiz</trd>
  <trdgrp lng="cs">
    <trd>starom&oacute;dn&iacute;</trd>,
    <trd>vy&scaron;l&yacute; z m&oacute;dy</trd>,
    <trd>zastaral&yacute;</trd>
  </trdgrp>
  <trd lng="en">out of fashion</trd>
  <trdgrp lng="de">
    <trd>altmodisch</trd>,
    <trd>nicht mehr in <ind>Mode</ind></trd>
  </trdgrp>
  <trd lng="fr">d&eacute;mod&eacute;</trd>
  <trd lng="io">ekmoda</trd>
  <trd lng="pl">niemodny</trd>
  <trd lng="ru">&c_v;&c_y;&c_sh;&c_je;&c_d;&c_sh;&c_i;&c_j; &c_i;&c_z; &c_m;&c_o;&c_d;&c_y;</trd>
  <trdgrp lng="sk">
    <trd>vy&scaron;l&yacute; z&nbsp;m&oacute;dy</trd>,
    <trd>zastaran&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="mod.el0igxi">
  <kap>el<tld/>i&gcirc;i, <var><kap>eks<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="mod.el0igxi.MOR">
    <dif>
      I&gcirc;i eks<tld/>a:
      <ekz>
        kelkaj vortoj el<tld/>i&gcirc;is, forpelitaj de pli ta&ubreve;gaj formoj
        <fnt><aut>G. Waringhien</aut>, <bib>PIV1</bib>, <lok>&gcirc;enerala
        anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        kompletoj funebra&jcirc;e same eks<tld/>i&gcirc;is kiel surrele
        vaporlokomotivoj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2011/010399.php">Komplete
          konfuzita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        eks<tld/>i&gcirc;inta a&ubreve; tute forgesita ludo el la propra infana&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010612.php">Ludema
          Italio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_z; <ind>&c_m;&c_o;&c_d;&c_y;</ind></trd>
  <trd lng="br">mont e-maez a c'hiz</trd>
  <trd lng="de">aus der <ind>Mode</ind> kommen</trd>
  <trd lng="en">go out of fashion</trd>
  <trd lng="es">pasar de <ind>moda</ind></trd>
  <trd lng="fr">se <ind>d&eacute;moder</ind></trd>
  <trd lng="io">ekmodeskar</trd>
  <trd lng="nl">uit de <ind>mode</ind> raken</trd>
  <trd lng="pl">wychodzi&cacute; z <ind>mody</ind></trd>
  <trd lng="ru">&c_v;&c_y;&c_j;&c_t;&c_i; &c_i;&c_z; &c_m;&c_o;&c_d;&c_y;</trd>
</drv>

<drv mrk="mod.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="mod.en0igxi.MOR">
    <dif>
      Esti ek&scirc;atata de la publiko, 
      <ref tip="vid" cel="popula.0igxi">populari&gcirc;i</ref>:
      <ekz>
        longe anta&ubreve; ol la vintraj sportoj en<tld/>i&gcirc;is <klr>...</klr>
        <fnt>
          <aut>R. Frison-Roche, trad. R. Bernard</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr34/negx
          ou.html">
          La ne&gcirc;o estas &gcirc;ojo!</url></vrk>,
          <lok>Nica Literatura Revuo, 1958-03 &gcirc;is 04 (3-4), p.
          140a-142a</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la angla en<tld/>i&gcirc;is la esprimo &leftquot;big data&#8221; por priskribi 
        datumarojn tiel grandajn kaj kompleksajn, ke ne eblas pritrakti
        ilin per la tradiciaj rimedoj
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2014/011208.php">La Granda
          Datumaro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>become <ind>fashionable</ind></trd>,
    <trd>get <ind>popular</ind></trd>
  </trdgrp>
  <trd lng="de">in <ind>Mode</ind> kommen</trd>
  <trd lng="fr">devenir <ind>&agrave; la mode</ind></trd>
  <trd lng="nl">populair worden</trd>
</drv>

<drv mrk="mod.laux0a">
  <kap>la&ubreve;<tld/>a</kap>
  <snc mrk="mod.laux0a.MOR">
    <dif>
      Konforma al la <ref tip="vid" cel="mod.0o.KOMUNE"><tld/>o
      <sncref/></ref>:
      <ekz>
        la&ubreve;<tld/>a fasono;
      </ekz>
      <ekz>
        la&ubreve;<tld/>e vestita;
      </ekz>
      <ekz>
        esti la&ubreve;<tld/>a kaj por kapti la atenton de la junularo
        <fnt>
          <bib>Monato</bib>
          <aut>Marteno Ecott</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007201.html">Invadas
          la angla</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        magazeno de la&ubreve;<tld/>aj vesta&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2005/008487.php">Fina
          fakturo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pli kaj pli la&ubreve;<tld/>as luksaj a&ubreve;toj
        <fnt>
          <bib>Monato</bib>
          <aut>Henriel Fidilalao</aut>
          <vrk><url ref="&Monato;/2013/010874.php">Vojoj al bonaj
          profitoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;<tld/>a ideologio de &leftquot;postmodernismo&#8221; &gcirc;enerale egaligas &ccirc;iujn
        
        mondperceptojn, &ccirc;u religiajn, &ccirc;u sciencajn, &ccirc;u
        
        eksterraciajn, 
        neante la eblon rilatigi iujn el ili al la objektiva vero
        <fnt>
          <bib>Monato</bib>
          <aut>Nikolao Gudskov</aut>
          <vrk><url ref="&Monato;/2011/010211.php">Ateismo
          eksterscience</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dand.0o">dando</ref>
    </refgrp>
  </snc>
  <trd lng="br">diouzh ar c'hiz</trd>
  <trdgrp lng="en">
    <trd>fashionable</trd>,
    <trd>in fashion</trd>,
    <trd>trendy</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>modisch</trd>,
    <trd>zeitgem&auml;&szlig;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&agrave; la mode</trd>,
    <trd>en vogue</trd>
  </trdgrp>
  <trd lng="io">segun la modo</trd>
  <trdgrp lng="nl">
    <trd>modieus</trd>,
    <trd>in de mode</trd>,
    <trd>trendy</trd>
  </trdgrp>
  <trd lng="pl">modny</trd>
  <trd lng="ru">&c_m;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
