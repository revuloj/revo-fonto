<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mod</rad>/o
</kap>
<drv mrk="mod.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc mrk="mod.0o.KOMUNE">
    <dif>
      Neda&ubreve;ra, kaprica kutimo, moro a&ubreve; maniero, koncernanta
      precipe vestadon:
      <ekz>
        vestita la&ubreve; la plej nova pariza <tld/>o;
      </ekz>
      <ekz>
        mallongaj roboj estas nun en <tld/>o;
      </ekz>
      <ekz>
        estas nun <tld/>o interesi&gcirc;i pri sporto;
      </ekz>
      <ekz>
        esti en <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mezuri &ccirc;ion per mezurilo de la <tld/>o<fnt>Z</fnt>,
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="furor.0o">furoro</ref>,
      <ref cel="sensac1.0o">sensacio</ref>
    </refgrp>                
    <trd lng="be">&c_m;&c_o;&c_d;&c_a;</trd>
    <trd lng="br">giz</trd>
    <trd lng="es">moda</trd>
    <trd lng="fr">mode<klr> (vest. etc.)</klr></trd>
    <trd lng="nl">mode</trd>
    <trd lng="ru">&c_m;&c_o;&c_d;&c_a;</trd>
  </snc>
  <snc mrk="mod.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <ref tip="dif" cel="modal.verba0o">verba modalo</ref>
    <trd lng="es">modo</trd>
    <trd lng="nl">wijs<klr tip="ind"> (v.werkwoord)</klr></trd>
    <trd lng="ru">&c_n;&c_a;&c_k;&c_l;&c_o;&c_n;&c_je;&c_n;&c_i;&c_je; <klr>(&c_g;&c_l;&c_a;&c_g;&c_o;&c_l;&c_a;)</klr></trd>
    <rim>
      PV konfuzas modalon kaj moduson; PAG donas al <tld/>o la
      sencon modalan.
    </rim>           
    <trd lng="br">doare<klr tip="ind"> (yezhadur)</klr></trd>
    <trd lng="es">modo</trd>
    <trd lng="fr">mode<klr> (grammatical)</klr></trd>
    <trd lng="nl">wijs<klr tip="ind"> (v.werkwoord)</klr></trd>
  </snc>
  <snc mrk="mod.0o.GRA_PV">
    <uzo tip="fak">GRA</uzo>
    <ref tip="dif" cel="modus.0o.GRA">moduso<sncref/></ref>
  </snc>
</drv>

<drv mrk="mod.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>o, konforma al la
      <tld/>o <sncref ref="mod.0o.KOMUNE"/>:
      <ekz>
        <tld/>a gazeto;
      </ekz>
      <ekz>
        la&ubreve;<tld/>a fasono;
      </ekz>
      <ekz>
        la&ubreve;<tld/>e vestita
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dand.0o">dando</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_o;&c_d;&c_n;&c_y;</trd>   
  <trd lng="br">diouzh ar c'hiz</trd>
  <trd lng="fr">&agrave; la mode</trd>
  <trd lng="nl">modieus</trd>
</drv>

<drv mrk="mod.0istino">
  <kap><tld/>istino</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Virino, kiu faras a&ubreve; vendas <tld/>a&jcirc;ojn
      por virinoj.
    </dif>
  </snc>                          
  <trd lng="be">&c_m;&c_a;&c_d;&c_y;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="br">kabellerez</trd>
  <trd lng="es">modista</trd>
  <trd lng="fr">modiste</trd>
  <trd lng="nl">modiste</trd>
  <trd lng="ru">&c_m;&c_o;&c_d;&c_i;&c_s;&c_t;&c_k;&c_a;</trd>
</drv>

<drv mrk="mod.eks0a">
  <kap>eks<tld/>a</kap>
  <snc>
    <dif>
      kiu ne plu estas la&ubreve;<tld/>a, iam estis &gcirc;enerale &scirc;atata, sed jam ne.
    </dif>
  </snc>       
  <trd lng="br">e-maez a c'hiz</trd>
  <trd lng="fr">d&eacute;mod&eacute;</trd>
</drv>

<drv mrk="mod.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      I&gcirc;i eks<tld/>a:
      <ekz>kelkaj vortoj el<tld/>i&gcirc;is, forpelitaj de pli ta&ubreve;gaj formoj
      <fnt><aut>Gaston Waringhien</aut>, <bib>PIV1</bib>, <lok>&gcirc;enerala
        anta&ubreve;parolo</lok></fnt></ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_z; <ind>&c_m;&c_o;&c_d;&c_y;</ind></trd>   
  <trd lng="br">mont e-maez a c'hiz</trd>
  <trd lng="es">pasar de <ind>moda</ind></trd>
  <trd lng="fr">se <ind>d&eacute;moder</ind></trd>
  <trd lng="nl">uit de <ind>mode</ind> raken</trd>
  <trd lng="ru">&c_v;&c_y;&c_j;&c_t;&c_i; &c_i;&c_z; &c_m;&c_o;&c_d;&c_y;</trd>
</drv>
</art>
