<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>mandarin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mandar1.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="mandar1.0o.KUI">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref>, speco de
      oran&gcirc;eto:
      <ekz>
        Teksa ordonis, ke oni metu <klr>...</klr> <tld/>ojn, oran&gcirc;ojn
        kaj
        olivojn en du pladojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 50a, p. 136a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_y;&c_n; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="ca">mandarina</trd>
  <trd lng="de">Mandarine</trd>
  <trd lng="en">mandarin</trd>
  <trd lng="es">mandarina</trd>
  <trd lng="fr">mandarine</trd>
  <trd lng="hu">mandarin</trd>
  <trd lng="nl">mandarijn</trd>
  <trd lng="pl">mandarynka</trd>
  <trdgrp lng="pt">
    <trd>tangerina</trd>,
    <trd>bergamota</trd>,
    <trd>laranja-cravo</trd>,        
    <trd>laranja-mimosa</trd>,
    <trd>mandarina</trd>,
    <trd>mexerica</trd>,    
    <trd>mimosa</trd>,
    <trd>tangerina-cravo</trd>,    
    <trd>tangerina-do-rio</trd>,
    <trd>vergamota</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_i;&c_n; <klr tip="amb">(&c_f;&c_r;&c_u;&c_k;&c_t;)</klr></trd>
  <trd lng="sv">mandarin</trd>
</drv>
</art>
