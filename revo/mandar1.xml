<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mandarin</rad>/o
</kap>
<drv mrk="mandar1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT">Frukto</ref>, speco de
      oran&gcirc;eto:
      <ekz>
        Teksa ordonis, ke oni metu <klr>...</klr> <tld/>ojn, oran&gcirc;ojn kaj
        olivojn en du pladojn
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 50a, p. 136a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_y;&c_n; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="ca">mandarina</trd>
  <trd lng="de">Mandarine</trd>
  <trd lng="en">mandarin</trd>
  <trd lng="es">mandarina</trd>
  <trdgrp lng="fr">
    <trd>mandarine</trd>
  </trdgrp>
  <trd lng="hu">mandarin</trd>
  <trd lng="nl">mandarijn</trd>
  <trd lng="pl">mandarynka</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_i;&c_n; <klr
  tip="amb">(&c_f;&c_r;&c_u;&c_k;&c_t;)</klr></trd>
  <trd lng="sv">mandarin</trd>
</drv>
</art>
