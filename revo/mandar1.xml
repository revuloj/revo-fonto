<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mandar1.xml,v 1.30 2024/01/25 20:10:34 revo Exp $">
<kap>
  <ofc>9</ofc><rad>mandarin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mandar1.0o">
  <kap><ofc>9</ofc><tld/>o
  <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="&AdE;/decidoj/9oa.html">
9a Oficiala Aldono</url></vrk>, <lok>mandarin/o</lok></fnt></kap>
  <snc mrk="mandar1.0o.KUI">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.KUI">Frukto</ref> de
      <tld/>arbo, ia suka oran&gcirc;eto:
      <ekz>
        sur niaj kristnaskaj teleroj ku&scirc;is keksoj, nuksoj, <tld/>oj, bombonoj kaj
        &ccirc;okoladaj figuroj
        <fnt>
          <bib>FSI</bib>
          <lok>IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Teksa</nom> ordonis, ke oni metu <klr>[&#x2026;]</klr> <tld/>ojn, oran&gcirc;ojn
        kaj olivojn en du pladojn
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 50a, p. 136a</lok></fnt>.
      </ekz>
    </dif>
<ref tip="vid" cel="klementin.0o">klementino</ref>
    <bld lok="&WCU;/thumb/7/77/Mandarinenb&auml;umchen.JPG/360px-Mandarinenb&auml;umchen.JPG" prm="CC BY-SA 3.0">bran&ccirc;o kun <tld/>oj (Citrus reticulata), en Badeno-Virtembergo, Germanujo
        <fnt>
          <aut>4028mdk09, propra verko</aut>
          <lok><url ref="&WCW;/File:Mandarinenb%C3%A4umchen.JPG">&WCW;/File:Mandarinenb&auml;umchen.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="hom" cel="mandar.0o">mandareno, mandarino <klr>(&ccirc;ina oficisto)</klr></ref>
  <trd lng="be">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_y;&c_n; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_i;&c_n;&c_a;</trd>
  <trd lng="ca">mandarina</trd>
  <trd lng="de">Mandarine</trd>
  <trd lng="en">mandarin</trd>
  <trd lng="es">mandarina</trd>
  <trd lng="fr">mandarine</trd>
  <trd lng="hu">mandarin</trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30F3;&#x30C0;&#x30EA;&#x30F3;</trd>,
    <trd>&#x30DF;&#x30AB;&#x30F3;  <pr>&#x307F;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">mandarijn</trd>
  <trd lng="pl">mandarynka</trd>
  <trdgrp lng="pt">
    <trd>tangerina</trd>,
    <trd>bergamota</trd>,
    <trd>laranja-cravo</trd>,        
    <trd>laranja-mimosa</trd>,
    <trd>mandarina</trd>,
    <trd>mexerica</trd>,    
    <trd>mimosa</trd>,
    <trd>tangerina-cravo</trd>,    
    <trd>tangerina-do-rio</trd>,
    <trd>vergamota</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_i;&c_n; <klr tip="amb">(&c_f;&c_r;&c_u;&c_k;&c_t;)</klr></trd>
  <trd lng="sv">mandarin</trd>
  <trd lng="uk">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_i;&c_n; (&c_p;&c_l;&c_ib;&c_d;)</trd>
  <trdgrp lng="zh">
    <trd>&#x6A58; <pr>j&#xFA;</pr></trd>,
    <trd>&#x6854; <pr>j&#xFA;</pr></trd>,
    <trd>&#x6854;&#x5B50; <pr>j&#xFA;zi</pr></trd>,
    <trd>&#x6A58;&#x5B50; <pr>j&#xFA;zi</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mandar1.0arbo">
  <kap><ofc>9</ofc><tld/>arbo,
    <var><kap><ofc>9</ofc><tld/>ujo <fnt><bib>PV</bib></fnt></kap></var>
  </kap>
  <snc mrk="mandar1.0arbo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="citrus.0o">Citruso</ref>
      el Orientazio, kultivata pro la fruktoj
      (<trd lng="la">Citrus reticulata</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_y;&c_n;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trd lng="cs">mandarinka <klr>(strom)</klr></trd>
  <trd lng="fr">mandarinier</trd>
  <trd lng="ja">&#x30DE;&#x30F3;&#x30C0;&#x30EA;&#x30F3;</trd>
  <trd lng="sk">mandar&iacute;nka <klr>(strom)</klr></trd>
  <trd lng="uk">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_i;&c_n; (&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</trd>
  <trdgrp lng="zh">
    <trd>&#x67D1;&#x6854; <pr>g&#x101;nj&#xFA;</pr></trd>,
    <trd>&#x6A58; <pr>j&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>
