<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stan.xml,v 1.33 2024/03/25 07:28:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>stan</rad>/o
</kap>

<drv mrk="stan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg kod="IUPAC">Sn</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
  &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="50">elemento</ref> kun
  atomnumero 50, atompezo 118,69;
  griza, facile fandebla posttransira
  <ref tip="super" cel="metal.0o">metalo</ref>:
      <ekz>
        telero, kru&ccirc;o, kulero el <tld/>o;
      </ekz>
      <ekz>
        <tld/>a soldato
        <klr>(kiel infana ludilo)</klr>
        <fnt><bib>Fab1</bib>, <lok>Persista stana soldato</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_l;&c_a;&c_v;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_a;&c_j;</trd>
  <trd lng="br">staen</trd>
  <trdgrp lng="ca">
    <trd>estany</trd>
    <trd>j&uacute;piter<klr> (metal)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>Sn</trd>,
    <trd>c&iacute;n</trd>,
    <trd>stanum</trd>
  </trdgrp>
  <trd lng="de">Zinn</trd>
  <trd lng="en">tin</trd>
  <trd lng="es">esta&ntilde;o</trd>
  <trd lng="fa">&a_q;&a_l;&a_ayn;</trd>
  <trd lng="fr">&eacute;tain</trd>
  <trd lng="hu">&oacute;n</trd>
  <trd lng="id">timah</trd>
  <trd lng="it">stagno</trd>
  <trd lng="ja">&#x30B9;&#x30BA;</trd>
  <trd lng="la">Stannum</trd>
  <trd lng="nl">tin</trd>
  <trd lng="pl">cyna</trd>
  <trd lng="pt">estanho</trd>
  <trd lng="ru">&c_o;&c_l;&c_o;&c_v;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>Sn</trd>,
    <trd>c&iacute;n</trd>
  </trdgrp>
  <trd lng="sv">tenn</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&#x43D;&c_u;&#x43C; (&c_h;&c_ib;&#x43C;&c_ib;&c_ch;&#x43D;&c_i;&c_j; &c_je;&#x43B;&c_je;&#x43C;&c_je;&#x43D;&c_t;)</trd>,
    <trd>&#x43E;&#x43B;&#x43E;&c_v;&#x43E; (&#x43F;&c_r;&#x43E;&c_s;&c_t;&c_a; &c_r;&c_je;&c_ch;&#x43E;&c_v;&c_i;&#x43D;&c_a;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x932B; <pr>x&#x12B;</pr></trd>,
    <trd>&#x9521; <pr>x&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="stan.0i">
  <kap><ofc>*</ofc><tld/>i<fnt>Z</fnt><fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url ref="&AdE;/fundamento/universala_vortaro.html#av-5207">stan'i</url></lok></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
  <dif>
  Tegi per maldika tavolo el <tld/>o:
  <ekz>
    <tld/>ita kaserolo.
  </ekz>
  </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_k;&c_a;&c_l;&c_a;&c_j;&c_d;&c_i;&c_s;&c_v;&c_a;&c_m;</trd>
  <trd lng="br">staena&ntilde;</trd>
  <trd lng="ca">estanyar</trd>
  <trdgrp lng="cs">
    <trd>c&iacute;novat</trd>,
    <trd>p&uring;sobit c&iacute;nem</trd>
  </trdgrp>
  <trd lng="de">verzinnen</trd>
  <trdgrp lng="en">
    <trd>tin</trd>,
    <trd>tin-plate</trd>
  </trdgrp>
  <trd lng="es">esta&ntilde;ar</trd>
  <trd lng="fa">&a_q;&a_l;&a_ayn; &f_g;&a_r;&a_f;&a_t;&a_n;</trd>
  <trd lng="fr">&eacute;tamer</trd>
  <trd lng="hu">&oacute;noz</trd>
  <trd lng="ja">&#x30B9;&#x30BA;&#x30E1;&#x30C3;&#x30AD;&#x3059;&#x308B;</trd>
  <trd lng="pl">cynowa&cacute;</trd>
  <trd lng="ru">&c_l;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>c&iacute;nova&tcaron;</trd>,
    <trd>poc&iacute;nova&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rtenna</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_u;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43E;&#x43A;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i; &#x43E;&#x43B;&#x43E;&c_v;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="stan.0isto">
<kap><tld/>isto</kap>
  <snc>
    <dif>
      Metiisto kiu faras a&jcirc;ojn el <tld/>o.
        <ekz>
          Mi reiris al la stanisto, kaj li denove faris por mi kruron el

          stano.
          <fnt><bib>DOz</bib><lok>&Ccirc;apitro 5</lok></fnt>
        </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_d;&c_z;&c_ib;&c_l;&c_mol;&c_sh;&c_ch;&c_y;&c_k;</trd>
  <trd lng="en">tinsmith</trd>
  <trd lng="fr">&eacute;tameur</trd>
  <trd lng="pl">blacharz</trd>
  <trd lng="uk">&#x43B;&c_u;&c_d;&c_i;&#x43B;&#x44C;&#x43D;&c_i;&#x43A;</trd>
</drv>
</art>

