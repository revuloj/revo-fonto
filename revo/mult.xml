<drv mrk="mult.0epeza">
  <kap><tld/>epeza</kap>
  <snc>
    <dif>
      Tre peza:
      <ekz>
        &scirc;i fordonus kun plezuro &ccirc;iun &ccirc;i belega&jcirc;on kaj de&jcirc;etus la
        <tld/>epezan kronon
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz><ekz>
        la vagonaro <klr>[&#8230;]</klr> estis longa, <tld/>epeza kaj
        konsistis preska&ubreve; nur el personvagonoj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_ja;&c_zh;&c_k;&c_ib;</trd>,
    <trd>&c_c;&c_ja;&c_zh;&c_k;&c_a;&c_v;&c_a;&c_g;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_c;&c_ja;&c_zh;&c_k;&c_a;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>schwer</trd>,
    <trd>gewichtig</trd>
  </trdgrp>
  <trd lng="en">weighty</trd>
  <trd lng="hu">nagyon <ind>s&uacute;lyos</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_ja;&c_zh;&c_jo;&c_l;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_ja;&c_zh;&c_je;&c_l;&c_o;&c_v;&c_je;&c_s;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="mult.0especa">
  <kap><tld/>especa,
      <var><kap><tld/>speca</kap></var>
  </kap>
  <snc>
    <dif>
      Prezentanta <tld/>ajn specojn:
      <ekz>
        per longa kaj <tld/>especa uzado e&ccirc; la plej sova&gcirc;a idiomo de la
        plej barbara gento iom post iom fari&gcirc;as ri&ccirc;a
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen
          en la 21a de a&ubreve;gusto 1911</vrk>
        </fnt>;
      </ekz><ekz>
        oleo kaj kemia&jcirc;oj en <tld/>specaj konteneroj kaj ujoj poluados
        marbordojn
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url
          ref="&Monato;/2012/010800.php">Alflosa&jcirc;oj trans
          la oceano</url></vrk>
        </fnt>;
      </ekz><ekz>
        <tld/>specaj problemoj, precipe la manko de registara helpo
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011214.php">2013:
          dek donacoj al la posteuloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_m;&c_a;&c_t;&c_b;&c_a;&c_k;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_n;&c_a;&c_s;&c_t;&c_a;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">r&uring;zn&yacute;</trd>
  <trd lng="de">vielf&auml;ltig</trd>
  <trd lng="en">manifold</trd>
  <trdgrp lng="hu">
    <trd>sokf&eacute;le</trd>,
    <trd>sokfajta</trd>
  </trdgrp>
  <trd lng="nl">veelvoudig</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_n;&c_y;&c_h; &c_v;&c_i;&c_d;&c_o;&c_v;</trd>
  <trd lng="sk">r&ocirc;zny</trd>
</drv>
