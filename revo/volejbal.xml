<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: volejbal.xml,v 1.9 2019/09/22 16:10:22 revo Exp $">
<kap>
  <rad>volejbal</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>

<drv mrk="volejbal.0o">
  <kap><tld/>o</kap>
  <snc mrk="volejbal.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="dif" cel="pilk.flug0ado">Flugpilkado</ref>:
      <ekz>
        posttagmeze ni ludas sur la ludejo <tld/>on
        <fnt>
          <aut>Theodor Kilian</aut>,
          <vrk><url
          ref="http://www.fi.muni.cz/~abrs/malovec/kilian.txt">Ucebnice
          esperanta</url>, 1995</vrk>,
          <lok>18a leciono, letero al sveda amiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi kun grupo de miaj amikoj ofte ludis pilkon, preferante ne kutime piedpilkan
        ludon, sed <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="https://www.monato.be/2007/009112.php"
              >Busvoja&gcirc;o</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>voleibol</trd>,
    <trd>v&ograve;lei</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odb&iacute;jen&aacute;</trd>,
    <trd>volejbal</trd>
  </trdgrp>
  <trd lng="de">Volleyball</trd>
  <trd lng="fr">volleyball</trd>
  <trd lng="hu">r&ouml;plabda</trd>
  <trdgrp lng="pl">
    <trd>pi&lstroke;ka siatkowa<klr> (dyscyplna sportu)</klr></trd>,
    <trd>siatk&oacute;wka<klr> (dyscyplna sportu)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>voleibol</trd>,
    <trd>v&ocirc;lei</trd>
  </trdgrp>
  <trd lng="sk">volejbal</trd>
</drv>

<drv mrk="volejbal.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="volejbal.0isto.SPO">
    <dif>
      Profesia a&ubreve; kutima ludanto de <tld/>o:
      <ekz>
        usona <tld/>isto, olimpika &ccirc;ampiono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=312180"
              >Bob Ctvrtlik</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">jugador de <ind>voleibol</ind></trd>
  <trd lng="cs">volejbalista</trd>
  <trd lng="de">Volleyballspieler</trd>
  <trdgrp lng="hu">
    <trd>r&ouml;plabd&aacute;s</trd>,
    <trd>r&ouml;plabd&aacute;z&oacute;</trd>
  </trdgrp>
  <trd lng="pl">siatkarz</trd>
  <trdgrp lng="pt">
    <trd>jogador de voleibol</trd>
  </trdgrp>
  <trd lng="sk">volejbalista</trd>
</drv>
</art>

