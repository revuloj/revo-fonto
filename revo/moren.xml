<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: moren.xml,v 1.9 2023/09/02 06:04:14 revo Exp $">
<kap>
  <rad>moren</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="moren.0o">
  <kap><tld/>o</kap>
  <snc mrk="moren.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Mikso de rokoj kaj &scirc;tonpecoj kuntirita kaj postlasita de 
      glacirivero:
      <ekz>
        la grandegaj amasoj da sablo kaj &scirc;tonoj aperantaj en <tld/>oj 
        estas plej ver&scirc;ajne postsignoj de glaciego a&ubreve; pli &gcirc;uste 
        glaciegoj
        <fnt>
          <aut>L. Tell</aut>,
          <vrk>Esti&gcirc;o de la tero kaj de la homo</vrk>,
          <lok>J. R&eacute;gulo, 1969</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_e;&c_n;&c_a; <klr>(&c_g;&c_je;&c_a;&c_l;.)</klr></trd>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_je;&c_n;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>morena</trd>,
    <trd>morrena</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mor&eacute;na</trd>,
    <trd>n&aacute;nos ledovce</trd>
  </trdgrp>
  <trd lng="de">Mor&auml;ne</trd>
  <trd lng="en">moraine</trd>
  <trd lng="fr">moraine</trd>
  <trdgrp lng="ja">
    <trd>&#x6C37;&#x5806;&#x77F3;</trd>,
    <trd>&#x30E2;&#x30EC;&#x30FC;&#x30F3;</trd>
  </trdgrp>
  <trd lng="pl">morena</trd>     
  <trd lng="ro">moren&abreve;</trd>
  <trdgrp lng="sk">
    <trd>mor&eacute;na</trd>,
    <trd>&lcaron;adovcov&yacute; n&aacute;nos</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&#x440;&#x435;&#x43D;&#x430;</trd>,
    <trd>&#x43A;&#x456;&#x43D;&#x446;&#x435;&#x432;&#x430;</trd>,
    <trd>&#x431;&#x43E;&#x43A;&#x43E;&#x432;&#x430;</trd>,
    <trd>&#x43E;&#x441;&#x43D;&#x43E;&#x432;&#x43D;&#x430;</trd>
  </trdgrp>
</drv>
</art>

