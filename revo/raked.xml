<art mrk="$Id: raked.xml,v 1.20 2024/04/13 20:15:51 revo Exp $">
<kap>
  <ofc>9</ofc><rad>raked</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="raked.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="raked.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Pilko<ref tip="super" cel="bat.0ilo.SPO">batilo</ref>
      konsistanta el tenilo kaj ordinare plata, solida
      a&ubreve; elasta batsurfaco:
      <ekz>
        imagu tablotenismat&ccirc;on, en kiu unu el la ludantoj
        rajtas uzi la propran <tld/>on, dum la kontra&ubreve;ulo
        ricevas <tld/>on misekvilibran
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://claudepiron.free.fr/articlesenesperanto/fakto.htm">
          Esperanto - la fakto</url></vrk>,
          <lok>Norvega Esperantisto, 1987:3, p. 5a-8a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">raquette<klr> (sports de balle)</klr></trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_k;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="ca">raqueta</trd>
  <trd lng="de">Schl&auml;ger</trd>
  <trd lng="en">racket</trd>
  <trd lng="es">raqueta <klr> (de tenis, ping-pong)</klr></trd>
  <trd lng="hu">tenisz&uuml;t&odblac;</trd> 
  <trd lng="it">racchetta</trd>  
  <trd lng="ja">&#x30E9;&#x30B1;&#x30C3;&#x30C8;</trd>
  <trd lng="nl">racket</trd>
  <trd lng="pl">rakieta  <klr>(sport)</klr></trd>
  <trd lng="pt">raquete</trd>
  <trd lng="ro">racheta</trd>
  <trd lng="ru">&c_r;&c_a;&c_k;&c_je;&c_t;&c_k;&c_a;</trd>
  <trd lng="uk">&c_r;&c_a;&#x43A;&c_je;&c_t;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x7403;&#x62CD; <pr>qi&#xFA;p&#x101;i</pr></trd>,
    <trd>&#x62CD;&#x5B50; <pr>p&#x101;izi</pr></trd>
  </trdgrp>
</drv>
</art>
