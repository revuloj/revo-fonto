<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: katakana.xml,v 1.8 2021/07/12 09:32:59 revo Exp $">
<kap>
  <rad>katakana</rad>/o <fnt><bib>NePiv</bib></fnt>,
  <var><kap><rad var="n">katakan</rad>/o</kap></var>
</kap>
<drv mrk="katakana.0o">
  <kap><tld/>o, <var><kap><tld var="n"/>o</kap></var></kap>
  <snc mrk="katakana.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="kana.streka0o">Streka kanao</ref>:
      <ekz>
        en la franca eldono la legantoj konantaj <tld lit="K"/>on povas 
        legi la originalan sonon
        <fnt>
          <aut>E. D&uuml;n&szlig;er</aut>,
          <vrk><url ref="http://www.duensser.com/ku_conan1_eo.htm">
          Detektivo Konan</url></vrk>,
          <lok>2005-2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        hiragano kaj <tld var="n"/>o estas amba&ubreve; kano-sistemoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=206964">Rondaj kanaoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_t;&c_a;&c_k;&c_a;&c_n;&c_a;</trd>
    <ref tip="vid" cel="hiragana.0o.TIP">hiraganao</ref>
    <trd lng="de">Katakana</trd>
    <trd lng="fa">&f_k;&a_A;&a_t;&a_A;&f_k;&a_A;&a_n;&a_A;</trd>
    <trd lng="fr">katakana</trd>
    <trd lng="it">katakana</trd>
    <trd lng="pl">katakana</trd>
  </snc>
</drv>
</art>

