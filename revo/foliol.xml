<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>foliol</rad>/o
</kap>
<drv mrk="foliol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc num="1">
    <dif>
      Involukra folio. 
    </dif>
    <adm>
    Sciante, ke "involukro" estas "aro da folioloj", estas
    malfacile kompreni "involukra folio". <aut>MB</aut>
    </adm>
  </snc>
  <snc num="2">
    <dif>
      &Ccirc;iu folieto (foliero) sur komuna pedunklo,
      el kiuj konsistas kunmetita folio:
      <ekz>la folioj de akacio konsistas el <tld/>oj.</ekz>
    </dif>
    <trd lng="fr">foliole</trd>
  </snc>
  <trd lng="de">H&uuml;llblatt</trd>
  <trd lng="ru">&c_l;&c_i;&c_s;&c_t;&c_o;&c_ch;&c_je;&c_k; <klr>(&c_s;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_g;&c_o; &c_l;&c_i;&c_s;&c_t;&c_a;)</klr></trd>
</drv>
</art>
