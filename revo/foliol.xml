<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: foliol.xml,v 1.12 2019/04/06 13:10:15 revo Exp $">
<kap>
  <rad>foliol</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="foliol.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="foliol.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="foli.0ero.BOT">Foliero</ref>:
      <ekz>
        la masklaj floroj entenas multajn &dash; helice situantajn &dash; stamenojn super
        skvamformaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18549"
              >Pinacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- ne traduku evitindajn...
  <trdgrp lng="cs">
    <trd>l&iacute;ste&ccaron;ek slo&zcaron;en&eacute;ho listu</trd>,
    <trd>&scaron;upina obalov&aacute;</trd>
  </trdgrp>
  <trd lng="sk">l&iacute;sto&ccaron;ek <klr>(zlo&zcaron;it&yacute;ch kvetov)</klr></trd>
-->
</drv>
</art>
