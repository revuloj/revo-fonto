<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kate&hcirc;ism</rad>/o, katekismo
</kap>
<drv mrk="katehx.0o">
  <kap><tld/>o</kap>
  <snc mrk="katehx.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Baza lernolibro por <ref tip="vid" cel="katehx2.0i">kate&hcirc;izi</ref>,
      prezentanta la &ccirc;efajn punktojn de religio, tradicie per demandoj kaj
      respondoj.
    </dif>
  </snc>
  <snc mrk="katehx.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Elementa ia ajn lernolibro, konsistanta el demandoj kaj respondoj:
      <ekz>
        <tld/>o de politika ekonomio.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_e;&c_h;&c_ib;&c_z;&c_m;</trd>
  <trd lng="de">Katechismus</trd>
  <trd lng="en">catechism</trd>
  <trd lng="fr">cat&eacute;chisme<klr> (livre)</klr></trd>
  <trd lng="nl">catechismus</trd>
  <trdgrp lng="hu">
    <trd>katekizmus</trd>,
    <trd>hittank&ouml;nyv</trd>,
    <trd>k&aacute;t&eacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_je;&c_h;&c_i;&c_z;&c_i;&c_s;</trd>
</drv>
</art>
