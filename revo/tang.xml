<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tang</rad>/i
</kap>
<drv mrk="tang.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tang.0i.MAR">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="klr">(pri &scirc;ipo)</uzo>
    <dif>
      Oscile movigi la&ubreve;longe, tio estas
      de anta&ubreve;e kaj de malanta&ubreve;e.
    </dif>
    <trd lng="de">schlingern</trd>
    <trdgrp lng="fr">
      <trd>tanguer</trd>
    </trdgrp>
    <trd lng="hu">bukd&aacute;csol<klr tip="ind"> (haj&oacute;)</klr></trd>    
    <trd lng="nl">stampen<klr tip="ind"> (v.schip)</klr></trd>
    <trd lng="ru">&c_i;&c_s;&c_p;&c_y;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;
<ind>&c_k;&c_i;&c_l;&c_je;&c_v;&c_u;&c_ju;
&c_k;&c_a;&c_ch;&c_k;&c_u;</ind></trd>
  </snc>
  <snc mrk="tang.0i.MUZ">
    <dif>
      Danci <tld/>on.
    </dif>
    <trd lng="de">Tango tanzen</trd>
    <trdgrp lng="fr">
      <trd>danser le <ind>tango</ind></trd>
    </trdgrp>
    <trd lng="hu">tang&oacute;zik</trd>    
    <trd lng="nl">tango dansen</trd>
    <trd lng="ru">&c_t;&c_a;&c_n;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;
<ind>&c_t;&c_a;&c_n;&c_g;&c_o;</ind></trd>
    <trd lng="sv">dansa <ind>tango</ind></trd>
  </snc>
</drv>

<drv mrk="tang.0o">
  <kap><tld/>o</kap>
  <snc mrk="tang.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Ago <tld/>i.
    </dif>
    <trd lng="de">Schlingern</trd>
    <trdgrp lng="fr">
      <trd>tangage</trd>
    </trdgrp>
    <trd lng="hu">bukd&aacute;csol&aacute;s<klr tip="ind"> (haj&oacute;&eacute;)</klr></trd>
    <trd lng="nl">het <ind>stampen</ind><klr tip="ind">
    (v.schip)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_i;&c_l;&c_je;&c_v;&c_a;&c_ja;
&c_k;&c_a;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_t;&c_a;&c_n;&c_g;&c_a;&c_zh;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tang.0o.danco">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Para <ref tip="super" cel="danc.0o">danco</ref> Argentin-devena,
      du-takta, kun varia rapido,
      monde populara ekde la 20a jarcentoj.
    </dif>
    <trdgrp lng="fr">
      <trd>tango</trd>
    </trdgrp>
    <trd lng="hu">tang&oacute;</trd>
    <trd lng="ru">&c_t;&c_a;&c_n;&c_g;&c_o;</trd>
  </snc>
  <snc mrk="tang.0o.muzikajxo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>o-danco
      ofte kanzona kun sopiraj kaj kordoloraj temoj.
    </dif>
    <trdgrp lng="fr">
      <trd>tango</trd>
    </trdgrp>
    <trd lng="hu">tang&oacute;</trd>
    <trd lng="ru">&c_t;&c_a;&c_n;&c_g;&c_o;</trd>
  </snc>
  <trd lng="de">Tango</trd>
  <trd lng="nl">tango</trd>
  <trd lng="sv">tango</trd>
</drv>
</art>
