<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tang.xml,v 1.24 2023/10/21 13:02:26 revo Exp $">
<kap>
  <rad>tang</rad>/i
</kap>
<drv mrk="tang.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tang.0i.MAR">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="turn.0igxi.ruligxi">Turni&gcirc;i</ref> &ccirc;irka&ubreve; la transversa akso de la
      veturilo; 
      &ccirc;e &scirc;ipo t.e. pro ondoj levantaj la pruon a&ubreve; pobon:
      <ekz>
        la tria ondego <tld/>igis la Imperiestrinon, kiu krute &jcirc;eti&gcirc;is supren
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jor.0i">jori</ref>,
      <ref cel="gir.0i">giri</ref>,
      <ref cel="rul.0igxi.MAR">ruli&gcirc;i<sncref/></ref>
    </refgrp>
    <bld lok="&WCU;/thumb/d/d0/Achsen_Schiffsbewegung.svg/640px-Achsen_Schiffsbewegung.svg.png" prm="CC BY-SA 3.0" lrg="640">rotaciaj movoj de &scirc;ipo: (x) ruli&gcirc;i, (y) tangi, (z) giri, jori
        <fnt>
          <aut>ThoKay, propra verko</aut>
          <lok><url ref="&WCW;/File:Achsen_Schiffsbewegung.svg">&WCW;/File:Achsen_Schiffsbewegung.svg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_z;&c_a;&c_z;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_k;&c_ib;&c_l;&c_ja;&c_v;&c_u;&c_ju; <ind>&c_g;&c_a;&c_j;&c_d;&c_a;&c_n;&c_k;&c_u;</ind></trd>
    <trdgrp lng="de">
      <trd>schlingern</trd>,
      <trd>nicken <klr>(Schiff)</klr></trd>
    </trdgrp>
    <trd lng="en">pitch (motion of ship etc.)</trd>
    <trdgrp lng="fr">
      <trd>tanguer</trd>
    </trdgrp>
    <trd lng="hu">bukd&aacute;csol<klr tip="ind">
    (haj&oacute;)</klr></trd>    
    <trd lng="nl">stampen<klr tip="ind"> (v.schip)</klr></trd>
    <trd lng="pl">kiwa&cacute;<klr> si&eogonek; (o statku)</klr></trd>
    <trd lng="ru">&c_i;&c_s;&c_p;&c_y;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_k;&c_i;&c_l;&c_je;&c_v;&c_u;&c_ju;
    &c_k;&c_a;&c_ch;&c_k;&c_u;</ind></trd>
  </snc>
  <snc mrk="tang.0i.MUZ">
    <dif>
      Danci <tld/>on:
      <ekz>
        mi tangis kun mia amikino en ties &ccirc;ambro &ccirc;e radio-muziko
        <fnt>
          <bib>KrB</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_a;&c_n;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_t;&c_a;&c_n;&c_g;&c_a;</ind></trd>
    <trd lng="bg">&c_t;&c_a;&c_n;&c_c;&c_u;&c_v;&c_a;&c_m;
    <ind>&c_t;&c_a;&c_n;&c_g;&c_o;</ind></trd>
    <trd lng="de">Tango tanzen</trd>
    <trd lng="en">tango</trd>
    <trdgrp lng="fr">
      <trd>danser le <ind>tango</ind></trd>
    </trdgrp>
    <trd lng="hu">tang&oacute;zik</trd>    
    <trd lng="nl">tango dansen</trd>
    <trd lng="pl">ta&nacute;czy&cacute; <ind>tango</ind></trd>
    <trd lng="ru">&c_t;&c_a;&c_n;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_t;&c_a;&c_n;&c_g;&c_o;</ind></trd>
    <trd lng="sv">dansa <ind>tango</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kym&aacute;cet se <klr>(lo&dcaron;)</klr></trd>,
    <trd>tan&ccaron;it tango</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7E26;&#x63FA;&#x308C;&#x3059;&#x308B;  <pr>&#x305F;&#x3066;&#x3086;&#x308C;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x30BF;&#x30F3;&#x30B4;&#x3092;&#x8E0A;&#x308B;  <pr>&#x305F;&#x3093;&#x3054;&#x3092;&#x304A;&#x3069;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kol&iacute;sa&tcaron; sa <klr>(lo&dcaron;)</klr></trd>,
    <trd>tancova&tcaron; tango</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_z;&c_n;&c_a;&c_v;&c_a;&c_t;&c_i; &c_k;&c_ib;&c_l;&c_mol;&c_o;&c_v;&c_o;&c_ji; &c_k;&c_a;&c_ch;&c_k;&c_i;</trd>,
    <trd>&c_t;&c_a;&c_n;&c_c;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_t;&c_a;&c_n;&c_gu;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="tang.0o">
  <kap><tld/>o</kap>
  <snc mrk="tang.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Ago <tld/>i.
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_ib;&c_l;&c_ja;&c_v;&c_a;&c_ja; <ind>&c_g;&c_a;&c_j;&c_d;&c_a;&c_n;&c_k;&c_a;</ind></trd>,
      <trd>&c_t;&c_a;&c_n;&c_g;&c_a;&c_zh;</trd>
    </trdgrp>
    <trd lng="de">Schlingern</trd>
    <trdgrp lng="fr">
      <trd>tangage</trd>
    </trdgrp>
    <trd lng="hu">bukd&aacute;csol&aacute;s<klr tip="ind">
    (haj&oacute;&eacute;)</klr></trd>
    <trd lng="nl">het <ind>stampen</ind><klr tip="ind">
    (v.schip)</klr></trd>
    <trd lng="pl">kiwanie<klr> (statku)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_i;&c_l;&c_je;&c_v;&c_a;&c_ja;
      &c_k;&c_a;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_t;&c_a;&c_n;&c_g;&c_a;&c_zh;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tang.0o.danco">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Para <ref tip="lst" lst="voko:dancoj" cel="danc.0o">danco</ref> 
      argentin-devena, du-takta, kun varia rapido, monde populara ekde la 20a jarcento:
      <ekz>
        je la unua, post la <tld/>oj, preska&ubreve; ni &ccirc;iuj estis torsonudaj
        <fnt>
          <aut>Higinio Garc&iacute;a</aut>
          <vrk><url ref="&Monato;/2001/007306.html">El la atesto de Mustafao
          Vanesavir&#x2019;</url></vrk>
           <bib>Monato</bib>
          <lok>2001/03, p. 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nuna <tld/>o naski&gcirc;is en la bordeloj de Montevideo 
        kaj Bonaero, en la du lastaj jardekoj de la 19-a jarcento
        <fnt>
          <bib>Viki</bib>,
          <vrk><url ref="&Viki;/Tango">Tango</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_a;&c_n;&c_g;&c_a; <klr>(&c_t;&c_a;&c_n;&c_je;&c_c;)</klr></trd>
    <trd lng="bg">&c_t;&c_a;&c_n;&c_g;&c_o;</trd>
    <trd lng="de">Tango</trd>
    <trdgrp lng="fr">
      <trd>tango</trd>
    </trdgrp>
    <trd lng="hu">tang&oacute;</trd>
    <trd lng="pl">tango</trd>
    <trd lng="ru">&c_t;&c_a;&c_n;&c_g;&c_o;</trd>
  </snc>
  <snc mrk="tang.0o.muzikajxo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="muzik.0ajxo">Muzika&jcirc;o</ref> ta&ubreve;ga por <tld/>o-danco
      ofte kanzona kun sopiraj kaj kordoloraj temoj:
      <ekz>
        mi e&ccirc; ekkantis la la&ubreve;modan <tld/>omelodion: &leftquot;sub varm&ccirc;iel&#x2019; en Argentino, en
        lando de la belvirino...&rightquot;
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_a;&c_n;&c_g;&c_a; <klr>(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; &c_t;&c_v;&c_o;&c_r;)</klr></trd>
    <trd lng="bg">&c_t;&c_a;&c_n;&c_g;&c_o;</trd>
    <trd lng="de">Tango</trd>
    <trdgrp lng="fr">
      <trd>tango</trd>
    </trdgrp>
    <trd lng="hu">tang&oacute;</trd>
    <trd lng="pl">tango</trd>
    <trd lng="ru">&c_t;&c_a;&c_n;&c_g;&c_o;</trd>
  </snc>
  <trd lng="cs">kym&aacute;cen&iacute; lod&ecaron;</trd>
  
  <trdgrp lng="en">
    <trd>pitch (motion of ship etc.)</trd>,
    <trd>tango</trd>
  </trdgrp>
  <trd lng="ja">&#x30BF;&#x30F3;&#x30B4;</trd>
  <trd lng="nl">tango</trd>
  <trd lng="sk">kym&aacute;canie sa lode</trd>
  <trd lng="sv">tango</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_ib;&c_l;&c_mol;&c_o;&c_v;&c_a; &c_k;&c_a;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_n;&c_gu;&c_o; (&c_t;&c_a;&c_n;&c_je;&c_c;&c_mol;)</trd>
  </trdgrp>
</drv>
</art>
