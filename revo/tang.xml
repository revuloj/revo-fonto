<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tang</rad>/i
</kap>
<drv mrk="tang.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tang.0i.MAR">
    <uzo tip="fak">MAR</uzo>
    <uzo tip="klr">(pri &scirc;ipo)</uzo>
    <dif>
      Oscile movigi la&ubreve;longe, tio estas
      de anta&ubreve;e kaj de malanta&ubreve;e.
    </dif>
    <trd lng="de">schlingern</trd>
    <trdgrp lng="fr">
      <trd>tanguer</trd>
    </trdgrp>
    <trd lng="hu">bukd&aacute;csol<klr tip="ind">
    (haj&oacute;)</klr></trd>    
    <trd lng="nl">stampen<klr tip="ind"> (v.schip)</klr></trd>
    <trd lng="pl">kiwa&cacute;<klr> si&eogonek; (o statku)</klr></trd>
    <trd lng="ru">&c_i;&c_s;&c_p;&c_y;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_k;&c_i;&c_l;&c_je;&c_v;&c_u;&c_ju;
    &c_k;&c_a;&c_ch;&c_k;&c_u;</ind></trd>
  </snc>
  <snc mrk="tang.0i.MUZ">
    <dif>
      Danci <tld/>on.
    </dif>
    <trd lng="bg">&c_t;&c_a;&c_n;&c_c;&c_u;&c_v;&c_a;&c_m; <ind>&c_t;&c_a;&c_n;&c_g;&c_o;</ind></trd>
    <trd lng="de">Tango tanzen</trd>
    <trdgrp lng="fr">
      <trd>danser le <ind>tango</ind></trd>
    </trdgrp>
    <trd lng="hu">tang&oacute;zik</trd>    
    <trd lng="nl">tango dansen</trd>
    <trd lng="pl">ta&nacute;czy&cacute; <ind>tango</ind></trd>
    <trd lng="ru">&c_t;&c_a;&c_n;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_t;&c_a;&c_n;&c_g;&c_o;</ind></trd>
    <trd lng="sv">dansa <ind>tango</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>kym&aacute;cet se <klr>(lo&dcaron;)</klr></trd>,
    <trd>tan&ccaron;it tango</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kol&iacute;sa&tcaron; sa <klr>(lo&dcaron;)</klr></trd>,
    <trd>tancova&tcaron; tango</trd>
  </trdgrp>
</drv>

<drv mrk="tang.0o">
  <kap><tld/>o</kap>
  <snc mrk="tang.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Ago <tld/>i.
    </dif>
    <trd lng="de">Schlingern</trd>
    <trdgrp lng="fr">
      <trd>tangage</trd>
    </trdgrp>
    <trd lng="hu">bukd&aacute;csol&aacute;s<klr tip="ind">
    (haj&oacute;&eacute;)</klr></trd>
    <trd lng="nl">het <ind>stampen</ind><klr tip="ind">
    (v.schip)</klr></trd>
    <trd lng="pl">kiwanie<klr> (statku)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_i;&c_l;&c_je;&c_v;&c_a;&c_ja;
      &c_k;&c_a;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_t;&c_a;&c_n;&c_g;&c_a;&c_zh;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tang.0o.danco">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Para <ref tip="lst" lst="voko:dancoj" cel="danc.0o">danco</ref> 
      argentin-devena,
      du-takta, kun varia rapido,
      monde populara ekde la 20a jarcento:
      <ekz>
        la nuna <tld/>o naski&gcirc;is en la bordeloj de Montevideo 
        kaj Bonaero, en la du lastaj jardekoj de la 19-a jarcento
        <fnt>
          <bib>Viki</bib>,
          <vrk><url
          ref="https://eo.wikipedia.org/wiki/Tango">Tango</url></vrk>.
        </fnt>
      </ekz>
    </dif>
    <trd lng="bg">&c_t;&c_a;&c_n;&c_g;&c_o;</trd>
    <trdgrp lng="fr">
      <trd>tango</trd>
    </trdgrp>
    <trd lng="hu">tang&oacute;</trd>
    <trd lng="pl">tango</trd>
    <trd lng="ru">&c_t;&c_a;&c_n;&c_g;&c_o;</trd>
  </snc>
  <snc mrk="tang.0o.muzikajxo">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika&jcirc;o ta&ubreve;ga por <tld/>o-danco
      ofte kanzona kun sopiraj kaj kordoloraj temoj.
    </dif>
    <trd lng="bg">&c_t;&c_a;&c_n;&c_g;&c_o;</trd>
    <trdgrp lng="fr">
      <trd>tango</trd>
    </trdgrp>
    <trd lng="hu">tang&oacute;</trd>
    <trd lng="pl">tango</trd>
    <trd lng="ru">&c_t;&c_a;&c_n;&c_g;&c_o;</trd>
  </snc>
  <trd lng="cs">kym&aacute;cen&iacute; lod&ecaron;</trd>
  <trd lng="de">Tango</trd>
  <trd lng="nl">tango</trd>
  <trd lng="sk">kym&aacute;canie sa lode</trd>
  <trd lng="sv">tango</trd>
</drv>
</art>
