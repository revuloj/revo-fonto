<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fantom.xml,v 1.28 2023/11/09 19:13:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fantom</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="fantom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fantom.0o.MAG">
    <dif>
      Apero de mortinto sub videbla, ne palpebla figuro;
      <ref tip="sin" cel="ombr.0o.reaperanto">ombro<sncref/></ref>,
      <ref tip="sin" cel="aper.re0anto">reaperanto</ref>,
      <ref tip="sin" cel="spirit.0o.reaperanto">spirito<sncref/></ref>:
      <ekz>
        la <tld/>o de la patro en Hamleto;
      </ekz>
      <ekz>
        lian palan viza&gcirc;on la luno <tld/>e prilumis
        <fnt>
          <aut>H. Heine, trad. L. L. Zamenhof</aut>,
          <vrk>La Rabeno de Ba&hcirc;ara&hcirc;</vrk>,
          <lok>1929</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gespenst</trd>,
      <trd>Geist</trd>,
      <trd>Spukgestalt</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&iacute;s&eacute;rtet</trd>,
      <trd>fantom</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>geest</trd>,
      <trd>spook</trd>
    </trdgrp>
  </snc>
  <snc mrk="fantom.0o.iluzio">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Nura &scirc;ajno sen realeco;
      <ref tip="vid" cel="iluzi.0o">iluzio</ref>,
      <ref tip="vid" cel="hximer.0o">&hcirc;imero</ref>:
      <ekz>
        li estas senpova <tld/>o de re&gcirc;o;
      </ekz>
      <ekz>
        post tiu longa malsano li estis nur la <tld/>o de si mem;
      </ekz>
      <ekz>
        postkuri la <tld/>on de la gloro.
      </ekz>
    </dif>
    <trd lng="hu">fantom</trd>
    <trd lng="nl">waanbeeld</trd>
  </snc>
  <adm>
    Vd adm sub spektro. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_n;&c_t;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_ib;&c_d;</trd>
  </trdgrp>
  <trd lng="bo">&t_ba;&t_da;&t_shabchu;&t_da;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>Phantom</trd>,
    <trd>Scheinbild</trd>,
    <trd>Trugbild</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>phantom</trd>,
    <trd>ghost</trd>
  </trdgrp>
  <trd lng="es">fantasma</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_b;&a_H;</trd>,
    <trd>&a_r;&a_w;&a_H;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fant&ocirc;me</trd>,
    <trd>spectre<klr tip="ind"> (fant&ocirc;me)</klr></trd>,
    <trd>ombre<klr tip="ind"> (fant&ocirc;me)</klr></trd>,
    <trd>semblant</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5E7D;&#x970A;  <pr>&#x3086;&#x3046;&#x308C;&#x3044;</pr></trd>,
    <trd>&#x4EA1;&#x970A;  <pr>&#x307C;&#x3046;&#x308C;&#x3044;</pr></trd>,
    <trd>&#x307E;&#x307C;&#x308D;&#x3057;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fantasma</trd>,
    <trd>apari&ccedil;&atilde;o</trd>,
    <trd>alma do outro mundo</trd>,
    <trd>abantesma</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_z;&c_r;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_a;&c_n;&c_t;&c_o;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>m&aacute;toha</trd>,
    <trd>prelud</trd>,
    <trd>pr&iacute;zrak</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>duch</trd>,
    <trd>fantom</trd>,
    <trd>m&aacute;toha</trd>,
    <trd>p&rcaron;&iacute;&scaron;era</trd>,
    <trd>stra&scaron;idlo</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>phantom<klr> (n)</klr></trd>,
    <trd>ghost<klr> (n)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_m;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_i;&c_d;</trd>,
    <trd>&c_d;&c_u;&c_h;</trd>,
    <trd>&c_f;&c_a;&c_n;&c_t;&c_o;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9B3C;&#x602A; <pr>gu&#x1D0;gu&#xE0;i</pr></trd>,
    <trd>&#x5E7D;&#x7075; <pr>y&#x14D;ul&#xED;ng</pr></trd>,
    <trd>&#x5147;&#x795E;&#x60E1;&#x715E; <pr>xi&#x14D;ngsh&#xE9;n&#xE8;sh&#x101;</pr></trd>,
    <trd>&#x9B3C;&#x9B42; <pr>gu&#x1D0;h&#xFA;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fantom.0a">
  <kap><tld/>a</kap>
  <snc mrk="fantom.0a.MAG">
    <dif>
      Simila al <tld/>o, nebula, malklara,
      eble iluzia, maltrankviliga:
      <ekz>
        <tld/>a apera&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>phantom<klr> (adj)</klr></trd>,
    <trd>ghostly<klr> (adj)</klr></trd>
  </trdgrp>
  <trd lng="fr">fantomatique</trd>
  <trdgrp lng="hu">
    <trd>k&iacute;s&eacute;rtet-</trd>,
    <trd>k&iacute;s&eacute;rteties</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5E7D;&#x970A;&#x306E;  <pr>&#x3086;&#x3046;&#x308C;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x307E;&#x307C;&#x308D;&#x3057;&#x306E;&#x3088;&#x3046;&#x306A;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_m;&c_a;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_f;&c_a;&c_n;&c_t;&c_o;&c_m;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="fantom.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fantom.0i.MAG">
    <dif>
      <ref tip="super" cel="aper.0i">Aperi</ref> kiel a&ubreve; kvaza&ubreve;
      <tld/>o;
      <ref tip="sin" cel="hant.0i.MAG">hanti</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a; &c_ja;&c_k; &c_p;&c_r;&c_y;&c_v;&c_ib;&c_d;</trd>
  <trd lng="cs">stra&scaron;it <klr>(stra&scaron;idlo)</klr></trd>
  <trdgrp lng="de">
    <trd>spuken</trd>,
    <trd>geistern</trd>
  </trdgrp>
  <trd lng="en">haunt<klr> (vi)</klr></trd>
  <trdgrp lng="fr">
    <trd>hanter</trd>
  </trdgrp>
  <trd lng="hu">k&iacute;s&eacute;rt</trd>
  <trdgrp lng="ja">
    <trd>&#x5E7D;&#x970A;&#x306B;&#x51FA;&#x308B;  <pr>&#x3086;&#x3046;&#x308C;&#x3044;&#x306B;&#x3067;&#x308B;</pr></trd>,
    <trd>&#x5316;&#x3051;&#x3066;&#x51FA;&#x308B;  <pr>&#x3070;&#x3051;&#x3066;&#x3067;&#x308B;</pr></trd>,
    <trd>&#x5E7D;&#x970A;&#x306E;&#x3088;&#x3046;&#x306B;&#x73FE;&#x308C;&#x308B;  <pr>&#x3086;&#x3046;&#x308C;&#x3044;&#x306E;&#x3088;&#x3046;&#x306B;&#x3042;&#x3089;&#x308F;&#x308C;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">spoken</trd>
  <trd lng="ru">&c_p;&c_o;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja; &c_k;&c_a;&c_k; &c_p;&c_r;&c_i;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>m&aacute;ta&tcaron;</trd>,
    <trd>m&aacute;to&zcaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&#x2019;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_l;&c_u;&c_k;&c_a;&c_t;&c_i; &c_ja;&c_k; &c_p;&c_r;&c_i;&c_m;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_ib;&c_d;&c_s;&c_t;&c_u;&c_p;&c_n;&c_o;</trd>,
    <trd>&c_n;&c_a;&c_v;&#x2019;&c_ja;&c_z;&c_l;&c_i;&c_v;&c_o; &c_p;&c_je;&c_r;&c_je;&c_s;&c_l;&c_ib;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x4F5C;&#x795F; <pr>zu&#xF2;su&#xEC;</pr></trd>
</drv>
</art>
