<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fantom.xml,v 1.24 2022/03/18 13:01:59 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fantom</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="fantom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fantom.0o.MAG">
    <dif>
      Apero de mortinto sub videbla, ne palpebla figuro;
      <ref tip="sin" cel="ombr.0o.reaperanto">ombro<sncref/></ref>,
      <ref tip="sin" cel="aper.re0anto">reaperanto</ref>,
      <ref tip="sin" cel="spirit.0o.reaperanto">spirito<sncref/></ref>:
      <ekz>
        la <tld/>o de la patro en Hamleto;
      </ekz>
      <ekz>
        lian palan viza&gcirc;on la luno <tld/>e prilumis
        <fnt>
          <aut>H. Heine, trad. L. L. Zamenhof</aut>,
          <vrk>La Rabeno de Ba&hcirc;ara&hcirc;</vrk>,
          <lok>1929</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gespenst</trd>,
      <trd>Geist</trd>,
      <trd>Spukgestalt</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&iacute;s&eacute;rtet</trd>,
      <trd>fantom</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>geest</trd>,
      <trd>spook</trd>
    </trdgrp>
  </snc>
  <snc mrk="fantom.0o.iluzio">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Nura &scirc;ajno sen realeco;
      <ref tip="vid" cel="iluzi.0o">iluzio</ref>,
      <ref tip="vid" cel="hximer.0o">&hcirc;imero</ref>:
      <ekz>
        li estas senpova <tld/>o de re&gcirc;o;
      </ekz>
      <ekz>
        post tiu longa malsano li estis nur la <tld/>o de si mem;
      </ekz>
      <ekz>
        postkuri la <tld/>on de la gloro.
      </ekz>
    </dif>
    <trd lng="hu">fantom</trd>
    <trd lng="nl">waanbeeld</trd>
  </snc>
  <adm>
    Vd adm sub spektro. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_n;&c_t;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_ib;&c_d;</trd>
  </trdgrp>
  <trd lng="bo">&#xF56;&#xF51;&#xF74;&#xF51;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>Phantom</trd>,
    <trd>Scheinbild</trd>,
    <trd>Trugbild</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>phantom</trd>,
    <trd>ghost</trd>
  </trdgrp>
  <trd lng="es">fantasma</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_b;&a_H;</trd>,
    <trd>&a_r;&a_w;&a_H;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fant&ocirc;me</trd>,
    <trd>spectre<klr tip="ind"> (fant&ocirc;me)</klr></trd>,
    <trd>ombre<klr tip="ind"> (fant&ocirc;me)</klr></trd>,
    <trd>semblant</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fantasma</trd>,
    <trd>apari&ccedil;&atilde;o</trd>,
    <trd>alma do outro mundo</trd>,
    <trd>abantesma</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_z;&c_r;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_a;&c_n;&c_t;&c_o;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>m&aacute;toha</trd>,
    <trd>prelud</trd>,
    <trd>pr&iacute;zrak</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>duch</trd>,
    <trd>fantom</trd>,
    <trd>m&aacute;toha</trd>,
    <trd>p&rcaron;&iacute;&scaron;era</trd>,
    <trd>stra&scaron;idlo</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x43C;&#x430;&#x440;&#x430;</trd>,
    <trd>&#x43C;&#x430;&#x43D;&#x430;</trd>,
    <trd>&#x43C;&#x430;&#x440;&#x430;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x432;&#x438;&#x434;</trd>,
    <trd>&#x434;&#x443;&#x445;</trd>,
    <trd>&#x444;&#x430;&#x43D;&#x442;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="fantom.0a">
  <kap><tld/>a</kap>
  <snc mrk="fantom.0a.MAG">
    <dif>
      Simila al <tld/>o, nebula, malklara,
      eble iluzia, maltrankviliga:
      <ekz>
        <tld/>a apera&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">fantomatique</trd>
  <trdgrp lng="hu">
    <trd>k&iacute;s&eacute;rtet-</trd>,
    <trd>k&iacute;s&eacute;rteties</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x438;&#x43C;&#x430;&#x440;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x444;&#x430;&#x43D;&#x442;&#x43E;&#x43C;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="fantom.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fantom.0i.MAG">
    <dif>
      <ref tip="super" cel="aper.0i">Aperi</ref> kiel a&ubreve; kvaza&ubreve;
      <tld/>o;
      <ref tip="sin" cel="hant.0i.MAG">hanti</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a; &c_ja;&c_k; &c_p;&c_r;&c_y;&c_v;&c_ib;&c_d;</trd>
  <trd lng="cs">stra&scaron;it <klr>(stra&scaron;idlo)</klr></trd>
  <trdgrp lng="de">
    <trd>spuken</trd>,
    <trd>geistern</trd>
  </trdgrp>
  <trd lng="en">haunt</trd>
  <trdgrp lng="fr">
    <trd>hanter</trd>
  </trdgrp>
  <trd lng="hu">k&iacute;s&eacute;rt</trd>
  <trd lng="nl">spoken</trd>
  <trd lng="ru">&c_p;&c_o;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja; &c_k;&c_a;&c_k; &c_p;&c_r;&c_i;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>m&aacute;ta&tcaron;</trd>,
    <trd>m&aacute;to&zcaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x2019;&#x44F;&#x432;&#x43B;&#x44F;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x431;&#x43B;&#x443;&#x43A;&#x430;&#x442;&#x438; &#x44F;&#x43A; &#x43F;&#x440;&#x438;&#x43C;&#x430;&#x440;&#x430;</trd>,
    <trd>&#x43D;&#x435;&#x432;&#x456;&#x434;&#x441;&#x442;&#x443;&#x43F;&#x43D;&#x43E;</trd>,
    <trd>&#x43D;&#x430;&#x432;&#x2019;&#x44F;&#x437;&#x43B;&#x438;&#x432;&#x43E; &#x43F;&#x435;&#x440;&#x435;&#x441;&#x43B;&#x456;&#x434;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>
</art>
