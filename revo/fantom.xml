<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fantom</rad>/o
</kap>

<drv mrk="fantom.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Apero de mortinto sub videbla sed ne palpebla figuro:
      <ekz>
        la <tld/>o de la patro en Hamleto;
      </ekz>
      <ekz>
        <ind><tld/>a</ind> apera&jcirc;o;
        <trd lng="fr">fantomatique</trd>
        <trdgrp lng="hu">
          <trd>k&iacute;s&eacute;rtet-</trd>,
          <trd>k&iacute;s&eacute;rteties</trd>
        </trdgrp>
      </ekz>
      <ekz>
        lian palan viza&gcirc;on la luno <tld/>e prilumis.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ombr.0o.reaperanto">ombro</ref>,
      <ref cel="aper.re0anto">reaperanto</ref>,
      <ref cel="spirit.0o.reaperanto">spirito</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="vizi.0o">vizio</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Gespenst</trd>,
      <trd>Geist</trd>,
      <trd>Spukgestalt</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&iacute;s&eacute;rtet</trd>,
      <trd>fantom</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>geest</trd>,
      <trd>spook</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Nura &scirc;ajno, sen realeco:
      <ekz>
        li estas senpova <tld/>o de re&gcirc;o;
      </ekz>
      <ekz>
        post tiu longa malsano li estis nur la <tld/>o de si mem;
      </ekz>
      <ekz>
        postkuri la <tld/>on de la gloro.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="iluzi.0o">iluzio</ref>,
      <ref cel="hximer.0o">&hcirc;imero</ref>.
    </refgrp>
    <trd lng="hu">fantom</trd>
    <trd lng="nl">waanbeeld</trd>
  </snc>
  <adm>
    Vd adm sub spektro. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_n;&c_t;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_v;&c_ib;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Phantom</trd>,
    <trd>Scheinbild</trd>,
    <trd>Trugbild</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>phantom</trd>,
    <trd>ghost</trd>
  </trdgrp>
  <trd lng="es">fantasma</trd>
  <trdgrp lng="fr">
    <trd>fant&ocirc;me</trd>,
    <trd>spectre<klr tip="ind"> (fant&ocirc;me)</klr></trd>,
    <trd>ombre<klr tip="ind"> (fant&ocirc;me)</klr></trd>,
    <trd>semblant</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fantasma</trd>,
    <trd>apari&ccedil;&atilde;o</trd>,
    <trd>alma do outro mundo</trd>,
    <trd>abantesma</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_z;&c_r;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_a;&c_n;&c_t;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="fantom.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="aper.0i">Aperi</ref> kiel a&ubreve; kvaza&ubreve;
      <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a; &c_ja;&c_k; &c_p;&c_r;&c_y;&c_v;&c_ib;&c_d;</trd>
  <trdgrp lng="de">
    <trd>spuken</trd>,
    <trd>geistern</trd>
  </trdgrp>
  <trd lng="en">haunt</trd>
  <trdgrp lng="fr">
    <trd>hanter</trd>
  </trdgrp>
  <trd lng="hu">k&iacute;s&eacute;rt</trd>
  <trd lng="nl">spoken</trd>
  <trd lng="ru">&c_p;&c_o;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja; &c_k;&c_a;&c_k; &c_p;&c_r;&c_i;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
