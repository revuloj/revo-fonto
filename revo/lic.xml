<art mrk="$Id$">
<kap>
  <rad>lic</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lic.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Esti permesata:
      <ekz>nupto ne <tld/>as okaze de la imperia funebro
      <fnt><bib>RugxDom</bib> <lok>capitro 68a, p. 453a</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_y;&c_c;&c_mol; <ind>&c_d;&c_a;&c_z;&c_v;&c_o;&c_l;&c_je;&c_n;&c_y;&c_m;</ind></trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>l&eacute;gal</ind></trd>,
    <trd>&ecirc;tre <ind>permis</ind></trd>
  </trdgrp>
</drv>
</art>

