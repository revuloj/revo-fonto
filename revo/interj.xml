<kap>
  <ofc>*</ofc>
  <rad>interjekci</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="interj.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc mrk="interj.0o.GRA">
    <dif>
      <ref tip="dif" cel="kri.ek0o.GRA">Ekkrio</ref>:
      <ekz>
        jen listo de diversfonte kolektitaj <tld/>oj kaj onomatopea&jcirc;oj &ndash; alivorte
        krivortoj kaj sonvortoj
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2011/010411.php">La gramatikiloj de Esperanto la&ubreve; ABC</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        insultaj esprimoj, <tld/>oj, sakroj, reciproke &jcirc;etataj inter fi&scirc;vendistino kaj
        du geklientoj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2013/010593.php">&Ccirc;e l' koro de E&ubreve;ropo ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sign.kri0o">krisigno</ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>ekkrio</ke>.
  </rim>

  <trd lng="be">&c_v;&c_y;&c_k;&c_l;&c_ib;&c_ch;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>citoslovce</trd>,
    <trd>interjekce</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Empfindungswort</trd>,
     <trd>Ausrufewort</trd>,
     <trd>Interjektion</trd>
  </trdgrp>
  <trd lng="en">interjection</trd>
  <trd lng="es">interjecci&oacute;n</trd>
  <trd lng="fr">interjection</trd>
  <trd lng="hu">indulatsz&oacute;</trd>
  <trd lng="ja">&#38291;&#25237;&#35422;  <pr>&#12363;&#12435;&#12392;&#12358;&#12375;</pr></trd>
  <trd lng="nl">tussenwerpsel</trd>
  <trd lng="pl">wykrzyknik<klr tip="amb"> (cz&eogonek;&sacute;&cacute; mowy)</klr></trd>
  <trd lng="pt">interjei&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_m;&c_je;&c_zh;&c_d;&c_o;&c_m;&c_je;&c_t;&c_i;&c_je;</trd>
  <trd lng="sk">citoslovce</trd>
  <trd lng="uk">&c_v;&c_i;&c_g;&c_u;&#1082;</trd>
  <trdgrp lng="zh">
    <trd>&#27449; <pr>y&#299;</pr></trd>,
    <trd>&#21497;&#35789; <pr>t&#224;nc&#237;</pr></trd>
  </trdgrp>
</drv>
