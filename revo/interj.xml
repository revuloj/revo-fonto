<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>interjekci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="interj.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc mrk="interj.0o.GRA">
