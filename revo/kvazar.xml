<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kvazar</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kvazar.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvazar.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="super" cel="astr.0o.1">Astro</ref> tre malproksima, kiu
      intense elsendas radiondojn:
      <ekz>
        la unua <tld/>o estis malkovrita en 1963
        <fnt>
        <aut>Amri Wandel</aut>,
        <vrk>La moderna astrofiziko</vrk>,
        <lok><url ref="http://esperanto.org/AEK/biblioteko.html">Virtuala
        Biblioteko de AEKo</url>, [vidita je 2006-01-24]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_z;&c_a;&c_r;</trd>
  <trd lng="bg">&c_k;&c_v;&c_a;&c_z;&c_a;&c_r;</trd>
  <trd lng="ca">qu&agrave;sar</trd>
  <trd lng="de">Quasar</trd>
  <trd lng="en">quasar</trd>
  <trd lng="es">qu&aacute;sar</trd>
  <trd lng="fr">quasar</trd>
  <trd lng="it">quasar</trd>
  <trdgrp lng="ja">
    <trd>&#12463;&#12456;&#12540;&#12469;&#12540; [<ind>&#12367;&#12360;&#12360;&#12373;&#12354;</ind>]</trd>,
    <trd>&#28310;&#26143; [<ind>&#12376;&#12421;&#12435;&#12379;&#12356;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">quasar</trd>
  <trd lng="pt">quasar</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_z;&c_a;&c_r;</trd>
  <trd lng="uk">&c_k;&c_v;&c_a;&c_z;&c_a;&c_r;</trd>
</drv>
</art>

