<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>larv</rad>/o
</kap>
<drv mrk="larv.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Unua senflugila, ordinare ra&ubreve;psimila, formo de insekto post &gcirc;ia
      elovi&gcirc;o kaj anta&ubreve; &gcirc;ia naskopova formo:
      <ekz>
        ra&ubreve;po estas <tld/>o de papilio.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kokon.0o">kokono</ref>,
      <ref cel="pup.0o">pupo</ref>,
      <ref cel="krizal.0">krizalido</ref>.
    </refgrp>
  </snc>
  <trd lng="en">larva</trd>
  <trd lng="es">larva</trd>
  <trd lng="nl">larve</trd>
  <trd lng="ru">&c_l;&c_i;&c_ch;&c_i;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
