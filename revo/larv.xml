<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>larv</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="larv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="larv.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Unua senflugila, ordinare ra&ubreve;psimila
      formo de insekto post &gcirc;ia elovi&gcirc;o
      kaj anta&ubreve; &gcirc;ia naskopova formo:
      <ekz>
        ra&ubreve;po estas <tld/>o de papilio.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kokon.0o">kokono</ref>,
      <ref cel="krizal.0">krizalido</ref>,
      <ref cel="pup.0o.ZOO">pupo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_ch;&c_y;&c_n;&c_k;&c_a;</trd>
  <trd lng="br">larvenn</trd>
  <trdgrp lng="de">
     <trd>Larve</trd>,
     <trd>Nymphe</trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>&kappa;&alfa_ton;&my;&pi;&jota;&alfa;</trd>,
    <trd>&ny;&ypsilon_ton;&my;&phi;&eta;</trd>
  </trdgrp>
  <trd lng="en">larva</trd>
  <trd lng="es">larva</trd>
  <trd lng="fr">larve</trd>
  <trd lng="hu">l&aacute;rva</trd>
  <trd lng="nl">larve</trd>
  <trd lng="pt">larva</trd>
  <trd lng="ru">&c_l;&c_i;&c_ch;&c_i;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
