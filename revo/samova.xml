<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: samova.xml,v 1.22 2022/05/05 05:09:55 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>samovar</rad>/o
</kap>
<drv mrk="samova.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="masxin.te0o">Tema&scirc;ino</ref> uzata de Rusujo &gcirc;is Okcidentazio,
      kiu kutime krom la akvobolujo anka&ubreve; havas rezervujon por tekoncentra&jcirc;o:
      <ekz>
        sur tablo, inter grandaj siringaj bukedoj, jam bolis la <tld/>o
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 5a, p.
        21a</lok></fnt>;
      </ekz>
      <ekz>
        mi preparos la <tld/>on kaj pretigos teon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        nun la <tld/>o devis kanti
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        tial ke vi eltrinkas dekses <tld/>ojn en tago, tiel vi levas la nazon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bol.0krucxo">bolkru&ccirc;o</ref>,
      <ref cel="primus.0o">primuso</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/2/2d/Samowar-auf-Tisch-2.jpg/319px-Samowar-auf-Tisch-2.jpg"
      prm="CC BY 3.0"
      ><tld/>o trinkilaro
        <fnt>
          <aut>Schlurcher, propra verko&nbsp;</aut>
          <lok><url ref="&WCW;/File:Samowar-auf-Tisch-2.jpg"
              >&WCW;/File:Samowar-auf-Tisch-2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_v;&c_a;&c_r;</trd>
  <trd lng="bg">&c_s;&c_a;&c_m;&c_o;&c_v;&c_a;&c_r;</trd>
  <trd lng="ca">samovar</trd>
  <trd lng="cs">samovar</trd>
  <trd lng="de">Samowar</trd>
  <trd lng="en">samovar</trd>
  <trd lng="es">samovar</trd>
  <trdgrp lng="fr">
    <trd>samovar</trd>
  </trdgrp>
  <trd lng="hu">szamov&aacute;r</trd>
  <trd lng="it">samovar</trd>
  <trd lng="nl">samovar</trd>
  <trd lng="pl">samowar</trd>
  <trdgrp lng="pt">
   <trd>samovar</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_v;&c_a;&c_r;</trd>
  <trd lng="sk">samovar</trd>
  <trd lng="uk">&c_s;&c_a;&c_m;&c_o;&c_v;&c_a;&c_r;</trd>
</drv>
</art>
