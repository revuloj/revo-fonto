<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>sciur</rad>/o
</kap>
<drv mrk="sciur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="sciure.0oj"><tld/>edoj</ref>
      (<trd lng="la">Sciurus</trd>), vivanta sur arboj, kun granda vosto kaj
      hartufoj &ccirc;e la oreloj:
      <ekz>
        peni tiel vane kiel <tld/>o turnanta sian ka&gcirc;on.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/sciur.jpg"><tld/>o</bld>
  <trd lng="de">Eichh&ouml;rnchen</trd>
  <trd lng="fr">&eacute;cureuil</trd>
  <trd lng="nl">eekhoorn</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_k;&c_a;</trd>
</drv>
</art>
