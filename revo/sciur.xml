<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>sciur</rad>/o
</kap>
<drv mrk="sciur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="sciur.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Sciurus</trd>), vivanta sur arboj:
      <ekz>
        peni tiel vane kiel <tld/>o turnanta sian ka&gcirc;on.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Eichh&ouml;rnchen</trd>
  <trd lng="fr">&eacute;cureuil</trd>
</drv>
<drv mrk="sciur.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="rod.0uloj">roduloj</ref>
      (<trd lng="la">Sciuridae</trd>), kun grandaj okuloj kaj longa
      longhara vosto.
    </dif>
  </snc>
  <trd lng="de">H&ouml;rnchen</trd>
</drv>
</art>
