<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sciur</rad>/o
</kap>

<drv mrk="sciur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sciur.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="sciure.0oj"><tld/>edoj</ref>
      (<trd lng="la">Sciurus</trd>), vivanta sur arboj, kun granda vosto
      kaj
      hartufoj &ccirc;e la oreloj:
      <ekz>
        peni tiel vane kiel <tld/>o turnanta sian ka&gcirc;on;
      </ekz>
      <ekz>
        &scirc;i suprenrampis tiel lerte kiel <tld/>eto inter la bran&ccirc;ojn
        <fnt><bib>ElektFab</bib>, <lok>Cindrulino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/sciur.jpg">
    <tld/>o
  </bld>
  <trd lng="be">&c_v;&c_a;&c_v;&c_jo;&c_r;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_t;&c_je;&c_r;&c_i;&c_ch;&c_k;&c_a;</trd>
  <trd lng="ca">esquirol</trd>
  <trd lng="de">Eichh&ouml;rnchen</trd>
  <trd lng="fr">&eacute;cureuil</trd>
  <trd lng="he">&samekh;&nun;&alef;&yod;</trd>
  <trd lng="hu">m&oacute;kus</trd>
  <trd lng="nl">eekhoorn</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_k;&c_a;</trd>
</drv>

<drv mrk="sciur.suker0o">
  <kap>suker<tld/>o</kap>
  <snc mrk="sciur.suker0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Flugipova <ref tip="super" cel="marsup.0oj">marsupialo</ref>
      malgranda (korpe 16 &gcirc;is 20 centimetrojn longa)
      vivanta en norda kaj orienta A&ubreve;stalio, Novgvineo, Tasmanio
      (<trd lng="la">Petaurus breviceps</trd>):
      <ekz>
        mi havas hejmbestojn: fi&scirc;ojn, kuniklon, kaj suker-<tld/>on
        <fnt>
        <aut>Teo Sankaro</aut>,
        <vrk><url
        ref="http://teosankaro.home.comcast.net/">hejmpa&gcirc;o</url></vrk>,
        <lok>2006-01-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kurzkopfgleitbeutler</trd>
  <trd lng="en">sugar glider</trd>
  <trd lng="fr">sugar glider</trd>
  <trd lng="nl">suikereekhoorn</trd>
  <trd lng="pl">lotopa&lstroke;anka kar&lstroke;owata</trd>
</drv>
</art>
