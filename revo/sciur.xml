<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sciur.xml,v 1.41 2024/05/23 19:24:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sciur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sciur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sciur.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="rongx.0ulo" lst="voko:ron&gcirc;uloj">Ron&gcirc;ulo</ref>
       vivanta sur arboj, kun granda longhara vosto
      kaj hartufoj &ccirc;e la oreloj; <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="sciure.0oj"><tld/>edoj</ref>
      (<trd lng="la">Sciurus</trd>):
      <ekz>
        peni tiel vane kiel <tld/>o turnanta sian ka&gcirc;on;
      </ekz>
      <ekz>
        &scirc;i suprenrampis tiel lerte kiel <tld/>eto inter la
        bran&ccirc;ojn
        <fnt><bib>ElektFab</bib>, <lok>Cindrulino</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/sciur.jpg">
    E&ubreve;razia rufa <tld/>o
  </bld>
  <trd lng="be">&c_v;&c_a;&c_v;&c_jo;&c_r;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_t;&c_je;&c_r;&c_i;&c_ch;&c_k;&c_a;</trd>
  <trd lng="ca">esquirol</trd>
  <trd lng="de">Eichh&ouml;rnchen</trd>
  <trd lng="en">squirrel</trd>
  <trd lng="es">ardilla</trd>
  <trd lng="fr">&eacute;cureuil</trd>
  <trd lng="he">&samekh;&nun;&alef;&yod;</trd>
  <trd lng="hu">m&oacute;kus</trd>
  <trd lng="id">tupai</trd>
  <trd lng="it">scoiattolo</trd>
  <trd lng="ja">&#x30EA;&#x30B9;</trd>
  <trd lng="nl">eekhoorn</trd>
  <trd lng="pl">wiewi&oacute;rka</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_k;&c_a;</trd>
  <trd lng="sv">ekorre</trd>
  <trd lng="sk">veverica</trd>
  <trd lng="cs">veverka</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_ib;&#x43B;&#x43A;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_v;&c_ib;&c_r;&#x43A;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x677E;&#x9F20; <pr>s&#x14D;ngsh&#x1D4;</pr></trd>
</drv>

<drv mrk="sciur.flug0o">
  <kap>flug<tld/>o,
  <var><kap>fluganta <tld/>o</kap></var></kap>
  <snc mrk="sciur.flug0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
    Komuna nomo por indiki flugipovan
    <ref tip="vid" cel="sciur.0o.ZOO">sciuron</ref> kaj do
    (la&ubreve; malnova klasifikado)
    subfamilion <trd lng="la">Pteromyinae</trd> a&ubreve;
    (la&ubreve; nova klasifikado) tribon
    <trd lng="la">Pteromyini</trd> (de subfamilio Sciurinae):
    <ekz>
      la membroj kaj e&ccirc; la bazo de la vosto de
      flugantaj <tld/>oj estas ligitaj per lar&gcirc;a
      areo de ha&ubreve;to, kiu funkcias kiel para&scirc;uto
      <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro
      6a</lok></fnt>.
    </ekz>
    </dif>
    <trd lng="de">Gleith&ouml;rnchen</trd>
    <trd lng="fr">&eacute;cureuil volant</trd>
    <trd lng="id"><ind>tupai</ind> terbang</trd>
    <trd lng="it">scoiattolo volante</trd>
    <trd lng="pl">polatucha</trd>
    <trdgrp lng="zh">
    <trd>&#x5BD2;&#x53F7;&#x9E1F; <pr>h&#xE1;nh&#xE0;oni&#x1CE;o</pr></trd>,
    <trd>&#x5BD2;&#x865F;&#x9CE5; <pr>h&#xE1;nh&#xE0;oni&#x1CE;o</pr></trd>,
    <trd>&#x98DB;&#x9F20; <pr>f&emacron;ish&#x1D4;</pr></trd>,
    <trd>&#x98DE;&#x9F20; <pr>f&emacron;ish&#x1D4;</pr></trd>,
    <trd>&#x9F2F; <pr>w&#xFA;</pr></trd>,
    <trd>&#x9E13; <pr>l&#x11B;i</pr></trd>,
    <trd>&#x9F2F;&#x9F20; <pr>w&#xFA;sh&#x1D4;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_t;&c_u;&c_h;&c_a;</trd>
  <trdgrp lng="es">
    <trd>pteromyini</trd>,
    <trd>ardilla voladora</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30E0;&#x30B5;&#x30B5;&#x30D3;  <pr>&#12416;&#12373;&#12373;&#12403;</pr></trd>,
    <trd>&#x30E2;&#x30E2;&#x30F3;&#x30AC;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_je;&c_t;&#x44F;&c_g;&c_a;</trd>,
    <trd>&#x43B;&c_ib;&c_t;&c_a;&#x44E;&c_ch;&c_a; &c_b;&c_ib;&#x43B;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="sciur.suker0o">
  <kap>suker<tld/>o</kap>
  <snc mrk="sciur.suker0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Flugopova <ref tip="lst" cel="marsup.0o" lst="voko:marsupiuloj">marsupiulo</ref>
      malgranda (korpe 16 &gcirc;is 20 centimetrojn longa)
      vivanta en norda kaj orienta A&ubreve;stralio, Novgvineo,
      Tasmanio;
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">
      specio</ref>
      <trd lng="la">Petaurus breviceps</trd>:
      <ekz>
        mi havas hejmbestojn: fi&scirc;ojn, kuniklon, kaj suker-<tld/>on
        <fnt>
        <aut>T. Sankaro</aut>,
        <vrk><url ref="http://teosankaro.home.comcast.net/">
        hejmpa&gcirc;o</url></vrk>,
        <lok>2006-01-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_u;&c_k;&c_r;&c_o;&c_v;&c_a;&c_ja; <ind>&c_p;&c_a;&c_l;&c_ja;&c_t;&c_u;&c_h;&c_a;</ind></trd>
  <trd lng="de">Kurzkopfgleitbeutler</trd>
  <trd lng="en">sugar glider</trd>
  <trdgrp lng="es">
    <trd>falangero del az&uacute;car</trd>,
    <trd>petauro del az&uacute;car</trd>
  </trdgrp>
  <trd lng="fr">sugar glider</trd>
  <trd lng="it">petauro dello zucchero</trd>
  <trd lng="nl">suikereekhoorn</trd>
  <trd lng="pl">lotopa&lstroke;anka kar&lstroke;owata</trd>
  <trd lng="zh">&#x871C;&#x888B;&#x9F2F; <pr>m&#xEC;d&#xE0;iw&#xFA;</pr></trd>
</drv>
</art>
