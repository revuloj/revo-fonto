<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>tajg</rad>/o
    </kap>

    <drv mrk="tajg.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
	<dif>
	  Subarkta praarbarego konifera, formanta zonon &ccirc;irka&ubreve; la
	  norda duonglobo, plejparte inter
	  <ref cel="tundr.0o">tundro</ref> kaj
	  <ref cel="step.0o">stepo</ref>, de
	  <ref cel="siberi.S0o">Siberio</ref> &gcirc;is Skandinavio
	  E&ubreve;razie, kaj en Alasko kaj Kanado Nordamerike.
	</dif>
      </snc>
      <trdgrp lng="en">
	<trd>taiga</trd>,
	<trd>boreal forest</trd>
      </trdgrp>
      <trd lng="ru">&c_t;&c_a;&c_j;&c_g;&c_a;</trd>
    </drv>

  </art>

