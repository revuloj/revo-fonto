<art mrk="$Id: tajg.xml,v 1.17 2022/05/05 17:08:11 revo Exp $">
<kap>
   <rad>tajg</rad>/o
</kap>

<drv mrk="tajg.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">GEOG</uzo>
   <snc>
     <dif>
       Subarkta pra<ref tip="super" cel="arb.0aro">arbar</ref>ego
       konifera, formanta zonon &ccirc;irka&ubreve;
       la norda duonglobo, precipe inter
       <ref cel="tundr.0o.norda">tundro</ref> kaj
       <ref cel="step.0o.ebena">stepo</ref>, de
       <ref cel="siberi.S0o">Siberio</ref> &gcirc;is Skandinavio
       E&ubreve;razie, kaj en Alasko kaj Kanado Nordamerike:
      <ekz>
        <klr>[ili]</klr> iris piede tra intermontejoj kaj &scirc;tonaj altebena&jcirc;oj, kovritaj per virga <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="&Monato;/2004/008350.html">Tiu tago</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        gulo <klr>[&#x2026;]</klr> vivas en tundroj kaj <tld/>oj (pinglofoliaj arbaroj)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9495">Gulo</url></lok>
        </fnt>.
      </ekz>
     </dif>
    <bld lok="&WCU;/thumb/f/f5/Picea_mariana_taiga_clipped.jpg/700px-Picea_mariana_taiga_clipped.jpg" prm="PD">piceoj de nordamerika <tld/>o (Picea mariana taiga), rivero Copper, Alasko
    
        <fnt>
          <aut>Picea_mariana_taiga.jpg: User:MPFderivative work: Minnecologies (talk), Picea_mariana_taiga.jpg</aut>
          <lok><url ref="&WCW;/File:Picea_mariana_taiga_clipped.jpg">&WCW;/File:Picea_mariana_taiga_clipped.jpg</url></lok>
        </fnt>
    </bld>
   </snc>
  <trd lng="be">&c_t;&c_a;&c_j;&c_g;&c_a;</trd>
  <trd lng="ca">taig&agrave;</trd>
  <trd lng="cs">tajga</trd>
  <trd lng="de">Taiga</trd>
  <trdgrp lng="en">
     <trd>taiga</trd>,
     <trd>boreal forest</trd>
   </trdgrp>
  <trd lng="fa">&a_t;&a_A;&f_y;&f_g;&a_A;</trd>
  <trd lng="fr">ta&iuml;ga</trd>
  <trd lng="hu">tajga</trd>
  <trd lng="nl">taiga</trd>
  <trd lng="pl">tajga</trd>
  <trdgrp lng="pt">
   <trd>taiga</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_j;&c_g;&c_a;</trd>
  <trd lng="sk">tajga</trd>
  <trd lng="uk">&#x442;&#x430;&#x439;&#x433;&#x430;</trd>
</drv>

</art>

