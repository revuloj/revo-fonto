    <kap>
      <rad>Rejkjavik</rad>/o
    </kap>

<drv mrk="rejkjavik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="island.I0o">Islando</ref>:
      <ekz>
        en <tld/>o ni renkontis norvegan esperantiston, kiu plendis, ke islandanoj
        konstante respondas angle, kiam li alparolas ilin en la nova norvega, kiu
        estas speco de simpligita islanda
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2015/011315.php">La islanda: revo de ne plu juna lernemulo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        la 98-a UK en <tld/>o
        <fnt>
          <vrk><url ref="https://www.youtube.com/watch?v=tm-JLxHZP6Y">98-a UK en Rejkjaviko: la nova Estraro sin prezentas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj domoj en <tld/>o estas varmigataj per elsubtera varma akvo anka&ubreve; en la
        somero
        <fnt>
          <vrk><url ref="&CRI;98uk/rejkjaviko.pdf">98-a Universala Kongreso de Esperanto</url></vrk>
          <lok>dua bulteno</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Reykjav&iacute;k</trd>
    <trd lng="en">Reykjav&iacute;k</trd>
    <trd lng="fr">Reykjavik</trd>
  </snc>
  <trd lng="uk">&#x420;&#x435;&#x439;&#x43A;&#x2019;&#x44F;&#x432;&#x456;&#x43A; (&#x441;&#x442;&#x43E;&#x43B;&#x438;&#x446;&#x44F;)</trd>
</drv>

