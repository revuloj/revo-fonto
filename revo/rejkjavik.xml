<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: rejkjavik.xml,v 1.6 2023/09/03 07:53:10 afido Exp $">
    <kap>
      <rad>Rejkjavik</rad>/o
    </kap>

<drv mrk="rejkjavik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="island.I0o">Islando</ref>:
      <ekz>
        en <tld/>o ni renkontis norvegan esperantiston, kiu plendis, ke islandanoj
        konstante respondas angle, kiam li alparolas ilin en la nova norvega, kiu
        estas speco de simpligita islanda
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2015/011315.php">La islanda: revo de ne plu juna
          lernemulo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        la 98-a UK en <tld/>o
        <fnt>
          <vrk><url ref="https://www.youtube.com/watch?v=tm-JLxHZP6Y">98-a UK en Rejkjaviko: la nova
          Estraro sin prezentas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj domoj en <tld/>o estas varmigataj per elsubtera varma akvo anka&ubreve; en la
        somero
        <fnt>
          <vrk><url ref="&CRI;98uk/rejkjaviko.pdf">98-a Universala Kongreso de Esperanto</url></vrk>
          <lok>dua bulteno</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_R;&c_e;&c_j;&c_k;&#x2019;&c_ja;&c_v;&c_ib;&c_k;</trd>
    <trd lng="de">Reykjav&iacute;k</trd>
    <trd lng="en">Reykjav&iacute;k</trd>
    <trd lng="fr">Reykjavik</trd>
    <trd lng="pl">Reykjavik</trd>
    <trd lng="ro">Reykjav&iacute;k</trd>
    <trd lng="uk">&c_R;&c_je;&c_j;&#x43A;&#x2019;&#x44F;&c_v;&c_ib;&#x43A; (&c_s;&c_t;&#x43E;&#x43B;&c_i;&c_c;&#x44F;)</trd>
  </snc>
  <trd lng="ja">&#x30EC;&#x30A4;&#x30AD;&#x30E3;&#x30D3;&#x30AF;</trd>
</drv>

  </art>

