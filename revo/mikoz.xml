<art mrk="$Id: mikoz.xml,v 1.20 2023/09/02 06:04:12 revo Exp $">
<kap>
  <rad>mikoz</rad>/o
</kap>
<drv mrk="mikoz.0o">
  <kap><tld/>o</kap>
  <snc>
   <uzo tip="fak">MED</uzo>
    <dif>
      Dan&gcirc;era ha&ubreve;t<ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref> inter
      homoj kaj bestoj, ka&ubreve;zata de parazitaj fungoj:
      <ekz>
        lepro estas infekta ha&ubreve;ta malsano <klr>[&#x2026;]</klr> karakterizita jen per ha&ubreve;taj a&ubreve;
        <tld/>aj nodetoj, jen per makula erupcio, jen per diversaj nervaj perturboj
        kun atrofio kaj gangreno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=32138">Lepro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <rim>
      Ne konfuzu kun <ref tip="hom" cel="mukoz.0o">mukozo</ref>.
    </rim>
  <trd lng="be">&c_m;&c_ib;&c_k;&c_o;&c_z;</trd>
  <trd lng="ca">micosi</trd>
  <trd lng="cs">myk&oacute;za</trd>
  <trd lng="de">Mykose</trd>
  <trd lng="en">mycosis</trd>
  <trd lng="es">micosis</trd>
  <trd lng="fr">mycose</trd>
  <trdgrp lng="hu">
    <trd>gomb&aacute;s megbeteged&eacute;s</trd>,
    <trd>mik&oacute;zis</trd>
  </trdgrp>
  <trd lng="id">mikosis</trd>
  <trdgrp lng="ja">
    <trd>&#x771F;&#x83CC;&#x75C7;  [<ind>&#x3057;&#x3093;&#x304D;&#x3093;&#x3057;&#x3087;&#x3046;</ind>]</trd>,
    <trd>&#x7CF8;&#x72B6;&#x83CC;&#x75C7;  [<ind>&#x7CF8;&#x72B6;&#x83CC;&#x3057;&#x3087;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">mycose</trd>
  <trd lng="pl">grzybica</trd>
  <trd lng="pt">micose</trd>
  <trd lng="ro">micoz&abreve;</trd>
  <trd lng="ru">&c_m;&c_i;&c_k;&c_o;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>myk&oacute;za</trd>,
    <trd>plese&ncaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x456;&#x43A;&#x43E;&#x437;</trd>,
    <trd>&#x433;&#x440;&#x438;&#x431;&#x43A;&#x43E;&#x432;&#x435; &#x437;&#x430;&#x445;&#x432;&#x43E;&#x440;&#x44E;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
