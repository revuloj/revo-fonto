<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vip.xml,v 1.31 2024/03/22 19:19:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vip</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="vip.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vip.0o.ARM">
    <dif>
      <ref tip="super" cel="bat.0ilo">Batilo</ref> konsistanta el &scirc;nuro a&ubreve; rimeno alligita al
      ligna tenilo:
      <ekz>
        oni a&ubreve;das la sonadon de <tld/>oj, la sonadon de bruantaj radoj
        <fnt>
          <bib>MT</bib>
          <lok>Na&hcirc;um 3:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        li denove krakis per la <tld/>o kaj kriis: hop, &ccirc;iuj miaj &ccirc;evaloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/>o estas por &ccirc;evalo, brido por azeno
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 26:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fiakristo <klr>[&#8230;]</klr> pli pro kutimo, ol pro bezono svingas <tld/>on
        <fnt>
          <bib>KFl</bib>
          <lok>VII. Gimnastiko por artaj sistemoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        anta&ubreve; krepusko ili prezentu sin hejme, por ricevi la <tld/>on
        <klr>(<tld/>obatojn)</klr>
        <fnt>
          <bib>SatirRak</bib>
          <lok>Logos</lok>
        </fnt>;
      </ekz>
      <ekz>
	li e&ccirc; el sablo <tld/>ojn tordas
	<klr>(estas elturni&gcirc;ema)</klr>
	<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="knut.0o">knuto</ref>,
      <ref cel="skurgx.0o">skur&gcirc;o</ref>,
    </refgrp>
    <ref tip="vid" cel="verg.0o.punrimedo">vergo<sncref/></ref>
    <trd lng="pt">chicote</trd>
  </snc>
  <snc mrk="vip.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Premanta a&ubreve; punanta superpotenco:
      <ekz>
	&gcirc;emi sub la <tld/>o;
      </ekz>
      <ekz>
        la infanoj, tiu <tld/>o de l&#8217; virino, dum miljaroj metanta la virinon en
        malegalan situon, kompare al la viro
        <fnt>
          <bib>Metrop</bib>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <snc mrk="vip.0o.EVI">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <tld lit="V"/>obato:
      <ekz>
        ricevi kiel instigon nur <tld/>ojn kaj piedbatojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>La vivolongo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_z;&c_u;&c_n;</trd>,
    <trd>&c_p;&c_u;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_tata;&t_tsheg;&t_la;&t_cata;&t_ga;&t_tsheg;</trd>
  <trd lng="ca">fuet</trd>
  <trd lng="cs">bi&ccaron;</trd>
  <trd lng="de">Peitsche</trd>
  <trd lng="en">whip</trd>
  <trdgrp lng="es">
    <trd>l&aacute;tigo</trd>
  </trdgrp>
  <trd lng="fr">fouet</trd>
  <trd lng="hu">ostor</trd>
  <trdgrp lng="id">
    <trd>cambuk</trd>,
    <trd>cemeti</trd>,
    <trd>pecut</trd>
  </trdgrp>
  <trd lng="ja">&#12416;&#12385;</trd>
  <trd lng="nl">zweep</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_i;&c_ch;</trd>,
    <trd>&c_h;&c_l;&c_y;&c_s;&c_t;</trd>
  </trdgrp>
  <trd lng="sk">bi&ccaron;</trd>
  <trd lng="sv">piska</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_i;&c_ch;</trd>,
    <trd>&c_h;&c_l;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_b;&c_a;&c_t;&c_ib;&c_g;</trd>
  </trdgrp>
</drv>

<drv mrk="vip.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vip.0i.ARM">
    <dif>
      Frapi per <tld/>o:
      <ekz>
        li komencis <tld/>i la &ccirc;evalon
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ordonas <tld/>i la kulpajn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        idiotino, mi devus <tld/>i vin kiel hundon
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>!
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la leono per la vosto <tld/>is la sablon, en kiu &gcirc;i ku&scirc;is
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Graograman', la Bunta Morto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skurgx.0i">skur&gcirc;i</ref>
    </refgrp>
    <trd lng="de">auspeitschen</trd>
    <trd lng="pt">chicotear</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_i;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_h;&c_l;&c_je;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_r;&c_o;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vip.0i.kirli">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forte batadi; kirli:
      <ekz>
        hajlo <tld/>is lian viza&gcirc;on
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj tiuj bona&jcirc;oj <tld/>is lian sangon kaj igis por li la vivon &scirc;atinda
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        li rememoris la akvan muelejon apud <nom>Kj&ouml;ge</nom>, kie la akvo bruis kaj estis <tld/>ata
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        knabinoj <klr>[&#8230;]</klr> <tld/>is tapi&scirc;ojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sablo <tld/>is la fenestrajn vitrojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ipo batata de la ondoj kaj <tld/>ata de ventego
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_h;&c_l;&c_je;&c_s;&c_t;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="vip.0i.mallauxdi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senindulge malla&ubreve;di,
      <ref tip="vid" cel="kritik.0i.KOMUNE">kritiki<sncref/></ref>:
      <ekz>
	lia sprita moko <tld/>as &ccirc;ion, kion li trovas kritikinda;
      </ekz>
      <ekz>
	<tld/>i la indiferentecon de la publiko;
      </ekz>
      <ekz>
        strange, li ne <tld/>is la enormajn &scirc;uldojn de Grekio, nek ties senefikan
        ekonomian sistemon
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2011/010545.php"
              >Viziuloj: iru kuracisten</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">gei&szlig;eln</trd>
    <trd lng="ru">&c_b;&c_i;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_c;&c_mol;
  &c_b;&c_ib;&c_z;&c_u;&c_n;&c_o;&c_m;</trd>
  <trd lng="ca">assotar</trd>
  <trd lng="cs">mrskat bi&ccaron;em</trd>
  <trd lng="de">peitschen</trd>
  <trdgrp lng="en">
    <trd>flog</trd>,
    <trd>lash</trd>,
    <trd>whip</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>fustigar</trd>,
    <trd>azotar<klr> (con l&aacute;tigo)</klr></trd>
  </trdgrp>
  <trd lng="fr">fouetter</trd>
  <trd lng="hu">(meg)<ind>ostoroz</ind></trd>
  <trd lng="id">men<ind>cambuk</ind></trd>
  <trdgrp lng="ja">
    <trd>&#12416;&#12385;&#25171;&#12388;  <pr>&#12416;&#12385;&#12358;&#12388;</pr></trd>,
    <trd>&#28608;&#12375;&#12367;&#38750;&#38627;&#12377;&#12427;  <pr>&#12399;&#12370;&#12375;&#12367;&#12402;&#12394;&#12435;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">geselen</trd>
  <trdgrp lng="sk">
    <trd>bi&ccaron;ova&tcaron;</trd>,
    <trd>o&scaron;&lcaron;aha&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">piska</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_i;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_i;&c_t;&c_i; &c_h;&c_l;&c_i;&c_s;&c_t;&c_o;&c_m;</trd>,
    <trd>&c_b;&c_a;&c_t;&c_o;&c_g;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="vip.rajdo0o">
  <kap>rajdo<tld/>o,
  <var><kap>rajd<tld/>o</kap></var></kap>
  <snc mrk="vip.rajdo0o.RAJ">
    <dif>
      <tld lit="V"/>o, ordinare konsistanta el fleksebla
      vergo, per kiu rajdanto instigas a&ubreve; malinstigas
      la rajdobeston:
      <ekz>
        nobla virino malofte forgesis sian stileton
        kaj neniam sian rajd<tld/>on
        <fnt>
          <aut>E. de Zilah</aut>
          <vrk>La Princo &ccirc;e la Hunoj</vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Reitpeitsche</trd>,
    <trd>Gerte</trd>
  </trdgrp>
  <trd lng="en">riding whip</trd>
  <trd lng="es">fusta</trd>
  <trd lng="fr">cravache</trd>
  <trd lng="ja">&#20055;&#39340;&#29992;&#12416;&#12385;  <pr>&#12376;&#12423;&#12358;&#12400;&#12424;&#12358;&#12416;&#12385;</pr></trd>
</drv>
</art>
