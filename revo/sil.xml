<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sil.xml,v 1.22 2023/10/21 07:46:47 revo Exp $">
<kap>
  <rad>sil</rad>/o
</kap>
<drv mrk="sil.0o">
  <kap><tld/>o</kap>
  <snc mrk="sil.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      &Scirc;aktforma <ref tip="super" cel="stok.0ejo">stokejo</ref> por meti
      en
      &gcirc;in grenon, legomojn kaj tiel plu:
      <ekz>
        mia mastro petis de mi ellaboradon de teknik-komerca
        prezo por konstruo de deponejoj kaj <tld/>oj por &scirc;tata
        instanco
        <fnt><aut>Gilbert R. Ledon</aut>: <vrk>Esenco de
        korupto</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 1a, p.
        8a</lok></fnt>;
      </ekz>       
      <ekz>
        <tld/>o, destinita por &leftquot;tradiciaj&rightquot; cerealoj, estas konstruata
        <fnt>
          <bib>MD05</bib>
          <vrk>Genetike Modifitaj Organismoj kontra&ubreve; la Kamparanoj</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a&jcirc;o</ind> <klr>(herboj fermentintaj en <tld/>o por nutri brutojn)</klr>;
        <trd lng="de">Silage</trd>
      </ekz>
      <ekz><ind>en<tld/>igi</ind> <klr>(stoki en <tld/>o)</klr>;
        <trd lng="be">&c_s;&c_ib;&c_l;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
        <trd lng="ca">ensitjar</trd>
        <trd lng="cs">d&aacute;t do sila</trd>
        <trd lng="de"><ind>einlagern</ind> in einem Silo</trd>
        <trd lng="es">ensilar</trd>
        <trdgrp lng="fr">
          <trd>ensiler</trd>
        </trdgrp>
        <trd lng="hu">sil&oacute;z</trd>
        <trd lng="pl">sk&lstroke;adowa&cacute; w <ind>silosie</ind></trd>
        <trd lng="ru">&c_s;&c_i;&c_l;&c_o;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
        <trd lng="sk">sil&aacute;&zcaron;ova&tcaron;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        retiri&gcirc;inta en <tld/>ojn de reciproka nekompreno
        <klr>(kp <ref cel="tur.ebura0o">ebura turo</ref>)</klr>
        <fnt>
          <bib>MD14</bib>
          <vrk>Mankas vorto por tio</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sil.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      &Scirc;aktforma deponejo por lan&ccirc;preta armita raketo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ib;&c_l;&c_a;&c_s;&c_n;&c_a;&c_ja; &c_ja;&c_m;&c_a;</trd>,
    <trd>&c_s;&c_ib;&c_l;&c_a;&c_s;&c_n;&c_a;&c_ja; &c_v;&c_je;&c_zh;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_i;&c_l;&c_o;&c_z;</trd>
  <trd lng="ca">sitja</trd>
  <trdgrp lng="cs">
    <trd>silo</trd>,
    <trd>z&aacute;sobn&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Silo</trd>
  <trd lng="en">silo</trd>
  <trd lng="es">silo</trd>
  <trd lng="fa">&a_s;&f_y;&a_l;&a_w;</trd>
  <trdgrp lng="fr">
    <trd>silo</trd>
  </trdgrp>
  <trd lng="hu">sil&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30A4;&#x30ED;</trd>,
    <trd>&#x8CAF;&#x8535;&#x5EAB;  <pr>&#x3061;&#x3087;&#x305E;&#x3046;&#x3053;</pr></trd>
  </trdgrp>
  <trd lng="nl">silo</trd>
  <trd lng="pl">silos</trd>
  <trd lng="pt">silo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_l;&c_o;&c_s;&c_n;&c_a;&c_ja; &c_ja;&c_m;&c_a;</trd>,
    <trd>&c_s;&c_i;&c_l;&c_o;&c_s;&c_n;&c_a;&c_ja; &c_b;&c_a;&c_sh;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>silo</trd>,
    <trd>z&aacute;sobn&iacute;k</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x438;&#x43B;&#x43E;&#x441;&#x43D;&#x430; &#x44F;&#x43C;&#x430;/&#x432;&#x435;&#x436;&#x430;</trd>
</drv>

</art>
