<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sil</rad>/o
</kap>
<drv mrk="sil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      &Scirc;aktforma provizejo por meti en &gcirc;in grenon, legomojn ktp.
    </dif>
  </snc>
  <trd lng="de">Silo</trd>
  <trd lng="nl">silo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_l;&c_o;&c_s;&c_n;&c_a;&c_ja; &c_ja;&c_m;&c_a;</trd>,
    <trd>&c_s;&c_i;&c_l;&c_o;&c_s;&c_n;&c_a;&c_ja; &c_b;&c_a;&c_sh;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="sil.en0igi">
  <kap>en<tld/>igi</kap>
  <snc>
    <dif>
      Meti en <tld/>on.
    </dif>
  </snc>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_o;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
