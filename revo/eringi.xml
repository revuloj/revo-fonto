<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: eringi.xml,v 1.13 2019/02/25 16:10:18 revo Exp $">
<kap>
  <rad>eringi</rad>/o
</kap>
<drv mrk="eringi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio de <ref
      tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Eryngium</trd>), kreskanta sur vojbordoj:
      <ekz>
        temas pri kardosimila planto kun malmolaj dornaj folioj <klr>[&#8230;]</klr> kelkaj
        specioj de <tld/>oj atingas alton de pluraj metro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=600462"
              >Eringio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/0/05/Eryngium_July_2011-1.jpg/640px-Eryngium_July_2011-1.jpg"
      prm="CC BY-SA 3.0"
      >Ametista <tld/>o (Eryngium amethystinum), &Gcirc;ardeno de Plantoj, Parizo
        <fnt>
          <aut>Alvesgaspar, propra verko</aut>
          <lok><url ref="&WCW;/File:Eryngium_July_2011-1.jpg"
              >https://commons.wikimedia.org/wiki/File:Eryngium_July_2011-1.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Mannstreu</trd>
    <trd lng="en">eryngo</trd>
    <trd lng="fr">panicaut</trd>
    <trdgrp lng="hu">
      <trd>iring&oacute;</trd>,
      <trd>&ouml;rd&ouml;gszek&eacute;r</trd>
    </trdgrp>
    <trd lng="nl">kruisdistel</trd>
    <trd lng="pl">miko&lstroke;ajek</trd>
    <trd lng="pt">er&iacute;ngio</trd>
    <trd
    lng="ru">&c_s;&c_i;&c_n;&c_je;&c_g;&c_o;&c_l;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
  </snc>
</drv>
</art>
