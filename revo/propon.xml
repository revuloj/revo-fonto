<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: propon.xml,v 1.30 2024/02/17 07:52:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>propon</rad>/i
</kap>

<drv mrk="propon.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="propon.0i.por_jugho">
    <dif>
      Prezenti al ies ju&gcirc;o; anta&ubreve;meti diskutota&jcirc;on:
      <ekz>
        ekzemplo <tld/>as sed ne ordonas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i ion al la publika kritiko;
      </ekz>
      <ekz>
        projektoj <klr>[&#x2026;]</klr> <tld/>itaj al la mondo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili pensas, ke se ni faros tiujn plibonigojn, kiujn &ccirc;iu el ili <tld/>as, nia
        afero iros multe pli bone
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        se la Komitato malakceptos vian <tld/>on, submetu vin discipline al &gcirc;ia decido
        <fnt>
          <bib>LR</bib>
          <lok>1. Alfabeto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as <klr>[&#x2026;]</klr>, ke ni honoru ilian memoron per levi&gcirc;o de niaj
        se&gcirc;oj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">vorschlagen</trd>
    <trd lng="hu">javasol</trd>    
    <trd lng="nl">voorstellen</trd>
    <trd lng="sv">f&ouml;resl&aring;</trd>
  </snc>
  <snc mrk="propon.0i.por_dispono">
    <dif>
      Prezenti al iu ion ne petitan, por ke li &gcirc;in disponu:
      <ekz>
        dronanto domon <tld/>as, savito e&ccirc; brikon ne donas
        <fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        homo <tld/>as, Dio disponas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i al iu siajn servojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is al ili monon
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 8:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[pri]</klr> la instruado la <tld/>ado de laboro multe superas &gcirc;ian
        postuladon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is sin kiel servistinon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="ofert.0i">oferti</ref>
    <ref tip="vid" cel="ofer.0i.dedicxi">oferi<sncref/></ref>
    <trdgrp lng="de">
      <trd>anbieten</trd>,
      <trd>antragen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>aj&aacute;nl</trd>,
      <trd>k&iacute;n&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">aanbieden</trd>
    <trd lng="sv">erbjuda</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_a;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_l;&c_a;&c_g;&c_a;&c_m;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chystat</trd>,
    <trd>nab&iacute;dnout</trd>,
    <trd>navrhnout</trd>,
    <trd>pl&aacute;novat</trd>,
    <trd>proponovat</trd>
  </trdgrp>  
  <trd lng="en">propose</trd>
  <trdgrp lng="es">
    <trd>proponer</trd>,
    <trd>sugerir</trd>
  </trdgrp>
  <trd lng="fr">proposer</trd>
  <trdgrp lng="ja">
    <trd>&#x63D0;&#x6848;&#x3059;&#x308B;  <pr>&#x3066;&#x3044;&#x3042;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x7533;&#x3057;&#x51FA;&#x308B;  <pr>&#x3082;&#x3046;&#x3057;&#x3067;&#x308B;</pr></trd>,
    <trd>&#x63D0;&#x4F9B;&#x3059;&#x308B;  <pr>&#x3066;&#x3044;&#x304D;&#x3087;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5DEE;&#x3057;&#x51FA;&#x3059;  <pr>&#x3055;&#x3057;&#x3060;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>proponowa&cacute;</trd>,
    <trd>stawia&cacute;  <ind>wniosek</ind></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>propune</trd>,
    <trd>face o cerere</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>navrhova&tcaron;</trd>,
    <trd>n&uacute;ka&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_r;&#x43E;&#x43F;&#x43E;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x63D0;&#x8B70; <pr>t&#xED;y&#xEC;</pr></trd>,
    <trd>&#x63D0;&#x8BAE; <pr>t&#xED;y&#xEC;</pr></trd>,
    <trd>&#x63D0;&#x6848; <pr>t&#xED;&#xE0;n</pr></trd>,
    <trd>&#x5021;&#x8B70; <pr>ch&#xE0;ngy&#xEC;</pr></trd>,
    <trd>&#x5021;&#x8BAE; <pr>ch&#xE0;ngy&#xEC;</pr></trd>,
    <trd>&#x63D0;&#x8ACB; <pr>t&#xED;q&#x1D0;ng</pr></trd>,
    <trd>&#x63D0;&#x8BF7; <pr>t&#xED;q&#x1D0;ng</pr></trd>,
    <trd>&#x521B;&#x8BAE; <pr>chu&#xE0;ngy&#xEC;</pr></trd>,
    <trd>&#x5275;&#x8B70; <pr>chu&#xE0;ngy&#xEC;</pr></trd>,
    <trd>&#x5EFA;&#x8B70; <pr>ji&#xE0;ny&#xEC;</pr></trd>,
    <trd>&#x5EFA;&#x8BAE; <pr>ji&#xE0;ny&#xEC;</pr></trd>,
    <trd>&#x63D0;&#x51FA; <pr>t&#xED;ch&#x16B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="propon.0o">
  <kap><tld/>o</kap>
  <snc mrk="propon.0o.io">
    <dif>
      Io <tld/>ata:
      <ekz>
        akcepti, diskuti, rifuzi ies <tld/>ojn;
      </ekz>
      <ekz>
        liaj fratoj akceptis lian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 37:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">BIB</uzo>
        ilia ofico estis <klr>[&#x2026;]</klr> zorgi pri la <ind>pano de <tld/>o</ind>
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 23:29</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Schaubrot</trd>,
          <trd>Pr&auml;senzbrot</trd>
        </trdgrp>
        <trd lng="en">schowbread</trd>
        <trd lng="he">&lamed;&het;&fmem; &he;&pe;&nun;&yod;&fmem;</trd>
      </ekz>
      <ekz>
        ili sendis al mi la saman <tld/>on kvar fojojn, kaj mi respondis tion saman
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 6:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni legas <tld/>ojn pri edzi&gcirc;o
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ove Ramel</nom> volis permesi al sinjoro <nom>Doe</nom>, ke li restu dum sia tuta vivo
        en la
        kastelo, sed li ne ricevis dankon pro sia <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        dan&gcirc;eraj estas iliaj <tld/>oj en la nuna tempo, kiam ni anta&ubreve; &ccirc;io bezonas plej
        severan unuecon
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Lingva Komitato estu devigata esplori tiun <tld/>on
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        labor<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2003/008043.html">Seksa sklaveco: &ccirc;u defio al la 21a
          jarcento?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        amend<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Istvan Ertl</aut>
          <vrk><url ref="&Monato;/2003/008103.html">Dio en la konstitucio, diablo en la
          detaloj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Proprasence oni <ref cel="prezen1.0i.proponi">prezentas</ref>
      ion al iu, supozante, ke li &gcirc;in akceptos; 
      oni <em>proponas</em> ion al iu, petante ke li &gcirc;in
      ekzamenu, a&ubreve; esperante, ke li &gcirc;in uzos; oni
      <ref cel="ofer.0i.dedicxi">oferas</ref> al iu
      ion, kion oni multe &scirc;atas, kredante, ke li
      senkoleri&gcirc;os a&ubreve; plenumos onian deziron.
    </rim>
    <ref tip="lst" lst="voko:komunikoj" cel="komuni1.0o">komuniko</ref>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_a;&c_n;&c_o;&c_v;&c_a;</trd>
  <trd lng="bg">&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>nab&iacute;dka</trd>,
    <trd>n&aacute;vrh</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vorschlag</trd>,
    <trd>Angebot</trd>,
    <trd>Antrag</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>sugerencia</trd>,
    <trd>propuesta</trd>
  </trdgrp>
  <trd lng="fr">proposition</trd>
  <trdgrp lng="hu">
    <trd>javaslat</trd>,
    <trd>aj&aacute;nlat</trd>,
    <trd>k&iacute;n&aacute;lat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x63D0;&#x6848;  <pr>&#x3066;&#x3044;&#x3042;&#x3093;</pr></trd>,
    <trd>&#x52D5;&#x8B70;  <pr>&#x3069;&#x3046;&#x304E;</pr></trd>,
    <trd>&#x7533;&#x3057;&#x51FA;  <pr>&#x3082;&#x3046;&#x3057;&#x3067;</pr></trd>,
    <trd>&#x63D0;&#x4F9B;  <pr>&#x3066;&#x3044;&#x304D;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aanbod</trd>,
    <trd>voorstel</trd>
  </trdgrp>
  <trd lng="pl">propozycja</trd>
  <trd lng="ro">propunere</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>n&aacute;vrh</trd>,
    <trd>ponuka</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_r;&#x43E;&#x43F;&#x43E;&c_z;&c_i;&c_c;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x8B70;&#x6848; <pr>y&#xEC;&#xE0;n</pr></trd>,
    <trd>&#x8BAE;&#x6848; <pr>y&#xEC;&#xE0;n</pr></trd>,
    <trd>&#x63D0;&#x8B70; <pr>t&#xED;y&#xEC;</pr></trd>,
    <trd>&#x63D0;&#x8BAE; <pr>t&#xED;y&#xEC;</pr></trd>,
    <trd>&#x5EFA;&#x8B70; <pr>ji&#xE0;ny&#xEC;</pr></trd>,
    <trd>&#x5EFA;&#x8BAE; <pr>ji&#xE0;ny&#xEC;</pr></trd>,
    <trd>&#x63D0;&#x6848; <pr>t&#xED;&#xE0;n</pr></trd>,
    <trd>&#x63D0;&#x540D; <pr>t&#xED;m&#xED;ng</pr></trd>
  </trdgrp>
</drv>
