<drv mrk="fripon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fripon.0o.KOMUNE">
    <dif>
      Ruza malhonestulo:
      <ekz>
        viza&gcirc;o de Katono, sed virto de
        <tld/>o<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la <tld/>oj &scirc;telis de vi anka&ubreve; la duan braceleton
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la karaj
        braceletoj</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kanajl.0o">kanajlo</ref>,
      <ref cel="tromp.0isto">trompisto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_h;&c_l;&c_ja;&c_r;</trd>,
