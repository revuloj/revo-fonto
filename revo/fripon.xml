  <trd lng="ja">&#x3060;&#x307E;&#x3057;&#x53D6;&#x308B;  <pr>&#x3060;&#x307E;&#x3057;&#x3068;&#x308B;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_u;&c_t;&c_mol;<klr tip="amb">
    (&c_o;&c_b;&c_m;&c_a;&c_n;&c_u;&c_t;&c_mol;)</klr></trd>,
    <trd>&c_o;&c_b;&c_zh;&c_u;&c_l;&c_mol;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>oklama&tcaron;</trd>,
    <trd>podvies&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_b;&c_sh;&c_a;&c_h;&c_r;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43E;&c_sh;&c_u;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
