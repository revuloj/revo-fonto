<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fripon</rad>/o
</kap>
<drv mrk="fripon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Ruza malhonestulo:
      <ekz>
        viza&gcirc;o de Katono, sed virto de <tld/>o
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Gauner</trd>,
    <trd>Lump</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_sh;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_u;&c_t;</trd>
  </trdgrp>
</drv>
<drv mrk="fripon.0i">
  <kap><tld/>i</kap>
  <snc num="1">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Agi <tld/>e. 
    </dif>
  </snc>
  <snc num="2">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Proprigi al si per malhonesta&jcirc;oj:
      <ekz>
        <tld/>i monon de (al) iu.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kanajl.0o">kanajlo</ref>,
      <ref cel="tromp">trompisto</ref>.
    </refgrp>
    <trd lng="de">ergaunern</trd>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_sh;&c_je;&c_n;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_u;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
