<drv mrk="fripon.0i">
  <kap><tld/>i</kap>
  <snc mrk="fripon.0i.trompi">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Agi <tld/>e:
      <ekz>
        kiu helpis vin <tld/>i, kiam vi konstruis ponton kaj kalkule
        enskribis lignon por dudek mil rubloj, dum en efektiveco vi ne
        donis e&ccirc; por cent rubloj
        <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno 2a</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <snc mrk="fripon.0i.sxteli">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Proprigi al si per malhonesta&jcirc;oj:
      <ekz>
        <tld/>i monon de (al) iu;
      </ekz>
      <ekz>
        al neniu ni <tld/>is
        <fnt><bib>NT</bib>, <lok>&Ko2; 7:2</lok></fnt>.
      </ekz>
    </dif>
    <adm>
      &Ccirc;i tiun transitivan sencon PIV1 ne konas, sed &gcirc;i
      konas pri~i, kies objekto montras la pri&scirc;teliton.
      Ekzemploj utilus por scii, &ccirc;u vere ~i povas esti
      transitiva. <aut>MB</aut>
    </adm>
    <trd lng="de">ergaunern</trd>
    <trd lng="es">timar</trd>
    <trdgrp lng="fr">
      <trd>arnaquer</trd>,
      <trd>escroquer</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_h;&c_l;&c_ja;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_sh;&c_u;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_m;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_m;&c_a;&c_m;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_o;&c_s;&c_t;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dareba&ccaron;it</trd>,
    <trd>lumpa&ccaron;it</trd>,
    <trd>podv&aacute;d&ecaron;t</trd>,
    <trd>tropit neplechy</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cheat</trd>,
    <trd>swindle</trd>,
    <trd>con</trd>,
    <trd>scam</trd>
  </trdgrp>
  <trd lng="ja">&#x3044;&#x304B;&#x3055;&#x307E;&#x3092;&#x50CD;&#x304F; 
  <pr>&#x3044;&#x304B;&#x3055;&#x307E;&#x3092;&#x306F;&#x305F;&#x3089;&#x304F;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_sh;&c_je;&c_n;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_u;&c_t;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_zh;&c_u;&c_l;&c_mol;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>be&tcaron;&aacute;r&ccaron;i&tcaron;</trd>,
    <trd>dareb&aacute;&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_a;&c_h;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_a;&c_h;&c_r;&c_a;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43E;&c_sh;&c_u;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43C;&c_a;&c_h;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="fripon.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="fripon.0ajxo.KOMUNE">
    <dif>
      Lerta trompo:
      <ekz>
        &ccirc;u vi memoras, kiel ni kune suferis mizeron,
        tagman&gcirc;adis per
        <tld/>a&jcirc;oj
        <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno 8a</lok></fnt>?
      </ekz>
      <ekz>
        ekzemple <ctl><tld lit="F"/>a&jcirc;oj de Skapeno</ctl>
        <fnt><bib>Plumamikoj</bib>, <lok>tradukoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_h;&c_l;&c_ja;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_a;&c_sh;&c_u;&c_k;&c_a;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_t;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_m;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_i;&c_z;&c_m;&c_a;&c_m;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_o;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gaunerei</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>con</trd>,
    <trd>scam</trd>
  </trdgrp>
  <trd lng="es">timo</trd>
  <trdgrp lng="fr">
    <trd>embrouille</trd>,
    <trd>fourberie</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30DA;&#x30C6;&#x30F3;</trd>,
    <trd>&#x3044;&#x304B;&#x3055;&#x307E;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_sh;&c_je;&c_n;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_zh;&c_u;&c_l;&c_mol;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_a;&c_h;&c_r;&c_a;&c_j;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd>&#x43A;&c_r;&c_u;&c_t;&c_ib;&c_j;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd>&#x43E;&c_sh;&c_u;&#x43A;&c_a;&#x43D;&c_s;&c_t;&c_v;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E0D;&#x6B63; <pr>b&#xF9;zh&#xE8;ng</pr></trd>,
    <trd>&#x574F;&#x4E8B; <pr>hu&#xE0;ish&#xEC;</pr></trd>,
    <trd>&#x620F;&#x5F04; <pr>x&#xEC;n&#xF2;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fripon.0emo">
  <kap><tld/>emo</kap>
  <snc mrk="fripon.0emo.KOMUNE">
    <dif>
      Deziro priruzi, trompi aliajn homojn:
      <ekz>
        momenton <tld/>emo denove ekbrilis en &scirc;iaj okuloj
        <fnt>
          <aut>M. Escherich, trad. A. Bader</aut>,
          <vrk>Maru&scirc;ka</vrk>,
          <lok>en: Du Amosentaj Noveloj, 1947</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol; &c_d;&c_a; <ind>&c_m;&c_a;&c_h;&c_l;&c_ja;&c_r;&c_s;&c_t;&c_v;&c_a;</ind></trd>,
    <trd>&c_p;&c_a;&c_d;&c_s;&c_t;&c_u;&c_p;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>espi&egrave;glerie</trd>,
    <trd>fourberie</trd>
  </trdgrp>
</drv>

<drv mrk="fripon.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="fripon.0eto.KOMUNE">
    <dif>
      Faranto de etaj malhonesta&jcirc;oj, malica
      <ref tip="vid" cel="bub.0o">bubo</ref>:
      <ekz>
        <ctl>la <tld/>eto de Tormes</ctl>
        <fnt>
          <aut>anonima, trad. E. de Bonaero</aut>,
          <vrk>La friponeto de Tormes</vrk>,
          <lok>1973</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi sidis rigida <klr>...</klr>, atendante nur tion, ke la
        <tld/>eto aperu el sia pordo por bati min
        <fnt><bib>Kiso</bib>, <lok>la punkto de Arkimedo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_sh;&c_u;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_c;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_h;&c_ib;&c_t;&c_r;&c_u;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kv&iacute;tko</trd>,
    <trd>neposeda</trd>,
    <trd>nezbeda</trd>,
    <trd>pod&scaron;&iacute;vka</trd>,
    <trd>&scaron;ibal</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Spitzbube</trd>,
    <trd>Schelm</trd>
  </trdgrp>
  <trd lng="en">little rascal</trd>
  <trd lng="es">p&iacute;caro</trd>
  <trdgrp lng="fr">
    <trd>coquin</trd>,
    <trd>filou</trd>,
    <trd>fripon</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_u;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>nezbedn&iacute;k</trd>,
    <trd>&scaron;ibal</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x611B;&#x958B;&#x73A9;&#x7B11;&#x8005; <pr>&#xE0;ik&#x101;iw&#xE1;nxi&#xE0;ozh&#x11B;</pr></trd>,
    <trd>&#x6DD8;&#x6C14;&#x9B3C; <pr>t&#xE1;oq&#xEC;gu&#x1D0;</pr></trd>,
    <trd>&#x65E0;&#x8D56; <pr>w&#xFA;l&#xE0;i</pr></trd>,
    <trd>&#x6D41;&#x6D6A;&#x6C49; <pr>li&#xFA;l&#xE0;ngh&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fripon.kun0o">
  <kap>kun<tld/>o</kap>
  <snc mrk="fripon.kun0o.KOMUNE">
    <dif>
      Kunfaranto de <tld/>a&jcirc;o:
      <ekz>
        Uzibitum kaj liaj kun<tld/>oj dependas tro multe en siaj
        intrigoj
        de la irado de la steloj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 14a, ruzo
        kontra&ubreve;
        ruzo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kompli2.0o.JUR">komplico</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_w;&c_r;&c_u;&c_s;&c_mol;&c_n;&c_ib;&c_k; &c_u; &c_m;&c_a;&c_h;&c_l;&c_ja;&c_r;&c_s;&c_t;&c_v;&c_je;</trd>
  <trdgrp lng="bg">
    <trd>&c_s;&c_malmol;&c_u;&c_ch;&c_a;&c_s;&c_t;&c_n;&c_i;&c_k; (&c_v; &c_m;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_i;&c_z;&c_m;&c_a;&c_m;&c_a;)</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>accomplice</trd>,
    <trd>shill</trd>
  </trdgrp>
  <trd lng="fr">complice</trd>
  <trd lng="ru">&c_s;&c_o;&c_o;&c_b;&c_shch;&c_n;&c_i;&c_k;<klr tip="amb"> (&c_p;&c_o;
  &c_m;&c_o;&c_sh;&c_je;&c_n;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_u;)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x540C;&#x72AF; <pr>t&#xF3;ngf&#xE0;n</pr></trd>,
    <trd>&#x4ECE;&#x72AF; <pr>c&#xF3;ngf&#xE0;n</pr></trd>,
    <trd>&#x5171;&#x72AF; <pr>g&#xF2;ngf&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fripon.pri0i">
