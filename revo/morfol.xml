<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: morfol.xml,v 1.20 2023/09/02 06:04:14 revo Exp $">
<kap>
  <rad>morfologi</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="morfol.0o">
  <kap><tld/>o</kap>
  <snc mrk="morfol.0o.KOMUNE">
    <dif>
      Formoscienco, sistema priskribo de formoj.
    </dif>
    <subsnc mrk="morfol.0o.GRA">
      <uzo tip="fak">GRA</uzo>
      <dif>
        Scienco pri la formo de vortoj kaj &gcirc;iaj &scirc;an&gcirc;i&gcirc;oj:
        <ekz>
          la verkintoj konfuzas semantikon (signifo de vorteroj),
          <tld/>on
          (konstruformo de vortoj), kaj kategoriojn de
          verboj
          <fnt><aut>D. Broadribb</aut>, <vrk>Vitraj &scirc;uoj de
          gramatiko</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 3a,
          p.
          31a</lok></fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="morfol.0o.BIO">
      <uzo tip="fak">BIO</uzo>
      <dif>
        Korpa aspekto kaj konstruo de vivuloj, kiel
        studas &gcirc;in la rilata parto de
        <ref tip="malprt" cel="biolog1.0o.SCI">biologio</ref>:
        <ekz>
          &ccirc;efaj faktoj de <tld lit="M"/>o fari&gcirc;as kompreneblaj &ndash; kiam 
          oni rimarkas ke homologaj organoj &ccirc;e malsamaj specioj de iu 
          klaso montras la saman planon
          <fnt>
            <bib>OriginoDeSpecioj</bib>,
            <lok>&ccirc;apitro 13a</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="anatom.0o.SCI">anatomio</ref>
      </refgrp>
    </subsnc>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_f;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="cs">nauka o tvarov&yacute;ch vlastnostech &zcaron;ivo&ccaron;ich&uring; a &ccaron;lov&ecaron;ka</trd>
  <trdgrp lng="de">
    <trd>Morphologie</trd>,
    <trd>Morfologie</trd>,
    <trd>Formenlehre</trd>
  </trdgrp>
  <trd lng="en">morphology</trd>
  <trd lng="es">morfolog&iacute;a</trd>
  <trd lng="fr">morphologie</trd>
  <trd lng="hu">alaktan</trd>
  <trdgrp lng="ja">
    <trd>&#x5F62;&#x614B;&#x8AD6;  [<ind>&#x3051;&#x3044;&#x305F;&#x3044;&#x308D;&#x3093;</ind>]</trd>,
    <trd>&#x5F62;&#x614B;&#x5B66;  [<ind>&#x3051;&#x3044;&#x305F;&#x3044;&#x304C;&#x304F;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>morfologie</trd>,
    <trd>vormleer</trd>
  </trdgrp>
  <trd lng="pl">morfologia</trd>
  <trd lng="pt">morfologia</trd>
  <trd lng="ro">morfologie</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_f;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">n&aacute;uka o tvarov&yacute;ch vlastnostiach &zcaron;ivo&ccaron;&iacute;chov a
  &ccaron;loveka</trd>
  <trd lng="uk">&#x43C;&#x43E;&#x440;&#x444;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>
</drv>

<drv mrk="morfol.0a">
  <kap><tld/>a</kap>
  <snc mrk="morfol.0a.GRA">
    <dif>
      Rilata al vortformoj, al priaj reguloj kaj teorioj:
      <ekz>
        Kaj kiel paroli pri la lipo<em>j</em>, se mankas <tld/>a
        montrilo de pluralo
        <fnt>
          <aut>B. Kolker</aut>,
          <vrk>Eto? Ego!</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 9a, p. 29a</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_f;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>morfologick&yacute;</trd>,
    <trd>tvaroslovn&yacute;</trd>
  </trdgrp>
  <trd lng="de">morphologisch</trd>
  <trd lng="en">morphological</trd>
  <trd lng="fr">morphologique</trd>
  <trd lng="pl">morfologiczny</trd>     
  <trd lng="ro">morfologic</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_f;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>morfologick&yacute;</trd>,
    <trd>tvaroslovn&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x43E;&#x440;&#x444;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x447;&#x43D;&#x438;&#x439;</trd>
</drv>
</art>
