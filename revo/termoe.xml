  <art mrk="$Id$">
    <kap>
      <rad>termoelektr</rad>/a
    </kap>
  
    <drv mrk="termoe.0a">
      <kap><tld/>a</kap>
      <uzo tip="fak">ELE</uzo>
      <snc>
	<dif>
  	  Rilata al la transformi&gcirc;o de varmo en elektron.
  	</dif>
      </snc>
      <trd lng="be">&c_t;&c_e;&c_r;&c_m;&c_a;&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
      <trd lng="de">thermoelektrisch</trd>
      <trdgrp lng="hu">
        <trd>h&odblac;elektromos</trd>,
        <trd>termoelektromos</trd></trdgrp>
      <trd lng="nl">thermo-elektrisch</trd>
     <trdgrp lng="pt">
      <trd>termel&eacute;trico</trd>
     </trdgrp>
      <trd lng="ru">&c_t;&c_je;&c_r;&c_m;&c_o;&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>
  </art>
