<art mrk="$Id: pean.xml,v 1.19 2023/10/20 14:25:20 revo Exp $">
<kap>
  <rad>pean</rad>/o
  <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="pean.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">POE</uzo>
    <dif>
      Himno gloranta Apolonon, kantata de antikvaj grekoj kiel
      venkokanto:
      <ekz>
        da&ubreve;re la tago l'A&hcirc;ajoj per kantoj pacigis la dion, belan
        <tld/>on kantadis la maljunuloj A&hcirc;ajoj, glorigadante Apolon, kaj &gcirc;ojis li en sia koro
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_a;&c_n;</trd>
  <trd lng="cs">v&iacute;cehlas&aacute; p&iacute;se&ncaron; na oslavu boha Apolona nebo v&iacute;t&ecaron;zstv&iacute;</trd>
  <trd lng="de">P&auml;an</trd>
  <trd lng="en">paean</trd>
  <trd lng="fr">p&eacute;an</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;la&eacute;nek <klr tip="amb">(Apoll&oacute;hoz)</klr></trd>,
    <trd>gy&odblac;zelmi &eacute;nek</trd>,
    <trd>pain</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30DD;&#x30ED;&#x30F3;&#x8CDB;&#x6B4C;  <pr>&#x30A2;&#x30DD;&#x30ED;&#x30F3;&#x3055;&#x3093;&#x304B;</pr></trd>,
    <trd>&#x52DD;&#x5229;&#x306E;&#x6B4C;  <pr>&#x3057;&#x3087;&#x3046;&#x308A;&#x306E;&#x3046;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="nl">paean</trd>
  <trdgrp lng="pl">
    <trd>pean</trd>,
    <trd>pie&sacute;&nacute; pochwalna</trd>
  </trdgrp>
  <trd lng="pt">pe&atilde;</trd>
  <trdgrp lng="ro">
    <trd>pean</trd>,
    <trd>c&acirc;ntecul de laud&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_a;&c_n;</trd>
  <trd lng="sk">piese&ncaron; na oslavu boha Apol&oacute;na</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x435;&#x430;&#x43D;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x43C;&#x43E;&#x436;&#x43D;&#x430; &#x43F;&#x456;&#x441;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
