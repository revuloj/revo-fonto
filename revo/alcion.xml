<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>alcion</rad>/o
</kap>
<drv mrk="alcion.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Genro el familio <tld/>edoj
       (<trd lng="la">Alcedo</trd>),
       birdo kun lazura dorso kaj rustkolora ventro,
       vivanta &ccirc;e fi&scirc;ori&ccirc;aj akvejoj.
     </dif>
   </snc>
   <trd lng="be">&c_z;&c_ib;&c_m;&c_a;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
   <trd lng="br">diredig</trd>
   <trd lng="cs">led&ncaron;&aacute;&ccaron;ek</trd>
   <trd lng="de">Eisvogel</trd>
   <trd lng="en">kingfisher</trd>
   <trd lng="fr">martin-p&ecirc;cheur</trd>
   <trd lng="he">&shin;&lamed;&dalet;&gimel;</trd>
   <trd lng="hu">j&eacute;gmad&aacute;r</trd>
   <trd lng="nl">ijsvogel</trd>
   <trd lng="pl">zimorodek</trd>
   <trdgrp lng="pt">
     <trd>alc&iacute;one</trd>,
     <trd>alcedo</trd>
   </trdgrp>
   <trd lng="ru">&c_z;&c_i;&c_m;&c_o;&c_r;&c_o;&c_d;&c_o;&c_k;</trd>
</drv>
<drv mrk="alcion.0edoj">
   <kap><tld/>edoj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Familio de birdoj (<trd lng="la">Alcedinidae</trd>),
       al kiu apartenas la <tld/>o.
     </dif>
   </snc>
   <trd lng="be">&c_z;&c_ib;&c_m;&c_a;&c_r;&c_o;&c_d;&c_k;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
   <trd lng="de">Eisv&ouml;gel</trd>
   <trd lng="fr">alc&eacute;dinid&eacute;s</trd>
   <trd lng="hu">j&eacute;gmad&aacute;rf&eacute;l&eacute;k</trd>
   <trd lng="ru">&c_z;&c_i;&c_m;&c_o;&c_r;&c_o;&c_d;&c_k;&c_i;<klr tip="amb"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
