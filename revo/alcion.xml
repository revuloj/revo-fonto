<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alcion.xml,v 1.27 2023/10/28 08:33:04 revo Exp $">
<kap>
  <ofc>*</ofc><rad>alcion</rad>/o,
  <var><kap><rad var="ed">alced</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="alcion.0o">
   <kap><ofc>*</ofc><tld/>o, <var><kap><tld var="ed"/>o</kap></var></kap>
   <uzo tip="fak">ZOO</uzo>
   <snc mrk="alcion.0o.birdo">
     <dif>
       <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
       el familio <ref tip="malprt" cel="alcion1.0oj"><tld/>edoj</ref>
       (<trd lng="la">Alcedo</trd>),
       <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref>
       kun lazura dorso kaj rustkolora ventro, vivanta &ccirc;e fi&scirc;ori&ccirc;aj akvejoj:
      <ekz>
        venadis la <tld/>oj, kaj la cikonio kaj la hirundo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        por eviti la efikon de tunelknalo en Japanio, oni konstruis la fronton de la
        &scirc;inkanseno kiel la bekon de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=320070">Efiko de tunelknalo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        oni levis el polvo la kapan ornama&jcirc;on, faritan el <tld var="ed"/>aj plumoj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 78a, p. 653-654a</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/cc/Common_Kingfisher_Alcedo_atthis.jpg/480px-Common_Kingfisher_Alcedo_atthis.jpg" prm="CC BY-SA 2.5"><tld/>o, Marburgo, Germanujo
        <fnt>
          <aut>Andreas Trepte, propra verko</aut>
          <lok><url ref="&WCW;/File:Common_Kingfisher_Alcedo_atthis.jpg">&WCW;/File:Common_Kingfisher_Alcedo_atthis.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="zh">
    <trd>&#x666E;&#x901A;&#x7FE0;&#x9CE5; <pr>p&#x1D4;t&#x14D;ngcu&#xEC;ni&#x1CE;o</pr></trd>,
    <trd>&#x666E;&#x901A;&#x7FE0;&#x9E1F; <pr>p&#x1D4;t&#x14D;ngcu&#xEC;ni&#x1CE;o</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_z;&c_ib;&c_m;&c_a;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
  <trd lng="bg">&c_z;&c_je;&c_m;&c_je;&c_r;&c_o;&c_d;&c_n;&c_o; &c_r;&c_i;&c_b;&c_a;&c_r;&c_ch;&c_je;</trd>
  <trd lng="br">diredig</trd>
  <trdgrp lng="ca">
     <trd>blauet</trd>,
     <trd>botiguer</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>korek mo&rcaron;sk&yacute;</trd>,
    <trd>led&ncaron;&aacute;&ccaron;ek</trd>,
    <trd>led&ncaron;&aacute;&ccaron;ek obecn&yacute;</trd>,
    <trd>led&ncaron;&aacute;&ccaron;ek &rcaron;&iacute;&ccaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Eisvogel</trd>
  <trd lng="en">kingfisher</trd>
  <trd lng="es">mart&iacute;n pescador</trd>
  <trd lng="fa">&a_m;&a_A;&a_h;&f_y;&zwnj;&a_kh;&a_w;&a_r;&f_k;</trd>
  <trd lng="fr">martin-p&ecirc;cheur</trd>
  <trd lng="he">&shin;&lamed;&dalet;&gimel;</trd>
  <trd lng="hu">j&eacute;gmad&aacute;r</trd>
  <trd lng="ja">&#x30AB;&#x30EF;&#x30BB;&#x30DF;</trd>
  <trd lng="mk">&c_r;&c_i;&c_b;&c_a;&c_r;&c_ch;&c_je;</trd>
  <trd lng="nl">ijsvogel</trd>
  <trd lng="pl">zimorodek</trd>
  <trdgrp lng="pt">
     <trd>alc&iacute;one</trd>,
     <trd>alcedo</trd>
   </trdgrp>
  <trd lng="ru">&c_z;&c_i;&c_m;&c_o;&c_r;&c_o;&c_d;&c_o;&c_k;</trd>
  <trd lng="sk">ryb&aacute;rik modrosiv&yacute;</trd>
  <trd lng="uk">&#x440;&#x438;&#x431;&#x430;&#x43B;&#x43E;&#x447;&#x43A;&#x430;</trd>
</drv>
</art>
