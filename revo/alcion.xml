<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>alcion</rad>/o
</kap>
<drv mrk="alcion.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <tld/>edoj
      (<trd lng="la">Alcedo</trd>),
      birdo kun lazura dorso kaj rustkolora ventro,
      vivanta &ccirc;e fi&scirc;ori&ccirc;aj akvejoj.
    </dif>
  </snc>
  <trd lng="cs">led&ncaron;&aacute;&ccaron;ek</trd>
  <trd lng="de">Eisvogel</trd>
  <trd lng="en">kingfisher</trd>
  <trd lng="hu">j&eacute;gmad&aacute;r</trd>
  <trd lng="pl">zimorodek</trd>
  <trd lng="ru">&c_z;&c_i;&c_m;&c_o;&c_r;&c_o;&c_d;&c_o;&c_k;</trd>
</drv>
<drv mrk="alcion.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio de birdoj (<trd lng="la">Alcedinidae</trd>),
      al kiu apartenas la <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Eisv&ouml;gel</trd>
</drv>
</art>
