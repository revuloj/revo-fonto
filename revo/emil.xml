<art mrk="$Id: emil.xml,v 1.12 2023/08/31 11:49:28 revo Exp $">
<kap>
  <rad>Emil</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="emil.0o">
  <kap><tld/>o</kap>
  <snc mrk="emil.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>:
      <ekz>
        poste ni iris al Soro kaj trovis tie <tld/>on
        <fnt><bib>Fab4</bib>, <lok>peco da perlovico, 2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_E;&c_m;&c_ib;&c_l;&c_mol;</trd>
  <trd lng="ca">Emili</trd>
  <trd lng="de">Emil</trd>
  <trd lng="fr">&Eacute;mile</trd>
  <trd lng="hu">Emil</trd>
  <trd lng="it">Emilio</trd>
  <trd lng="ja">&#x30A8;&#x30DF;&#x30FC;&#x30EB;</trd>
  <trd lng="pl">Emil</trd>
  <trd lng="pt">Em&iacute;lio</trd>
  <trd lng="ru">&c_E;&c_m;&c_i;&c_l;&c_mol;</trd>
  <trd lng="uk">&#x415;&#x43C;&#x456;&#x43B;</trd>
</drv>
</art>

