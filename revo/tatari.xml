    <trd>&c_T;&c_a;&c_t;&c_a;&c_r;&c_ib;&#x44F;</trd>,
    <trd>&c_T;&c_a;&c_t;&c_a;&c_r;&c_s;&c_t;&c_a;&#x43D; (&c_a;&c_v;&c_t;&#x43E;&#x43D;&#x43E;&#x43C;&#x43D;&c_a;
