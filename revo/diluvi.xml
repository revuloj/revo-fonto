<art mrk="$Id: diluvi.xml,v 1.17 2022/03/15 15:50:55 revo Exp $">
<kap>
  <rad>diluvi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="diluvi.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="diluvi.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="dif" cel="plejst.0o.GEOL">Plejstoceno</ref>:
      <ekz>
        Groto de <nom>Harmanec</nom>: <klr>[&#x2026;]</klr>
        subteraj areoj estis krei&gcirc;intaj en <tld/>aj kalko&scirc;tonoj en la meza triaso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=121839">Harmaneck&aacute; jasky&ncaron;a</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_l;&c_ju;&c_v;&c_ib;&c_j;</trd>
  <trd lng="cs">diluvium</trd>
  <trdgrp lng="de">
     <trd>Diluvium</trd>,
     <trd>Pleistoz&auml;n</trd>
  </trdgrp>
  <trd lng="en">Diluvium</trd>
  <trd lng="fr">diluvium</trd>
  <trd lng="he">&#x5DE;&#x5B7;&#x5D1;&#x5BC;&#x5D5;&#x5BC;&#x5DC;</trd>
  <trd lng="pt">diluvi&atilde;o</trd>
  <trd lng="ru">&c_d;&c_i;&c_l;&c_ju;&c_v;&c_i;&c_j;</trd>
  <trd lng="sk">dil&uacute;vium</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x438;&#x43B;&#x44E;&#x432;&#x456;&#x439;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x43A;&#x43B;&#x430;&#x434;&#x435;&#x43D;&#x43D;&#x44F; &#x435;&#x43F;&#x43E;&#x445;&#x438; &#x43F;&#x43B;&#x435;&#x439;&#x441;&#x442;&#x43E;&#x446;&#x435;&#x43D;&#x443;</trd>
  </trdgrp>
</drv>
</art>
