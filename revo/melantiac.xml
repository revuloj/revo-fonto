<vortaro>
  <art mrk="$Id: melantiac.xml,v 1.4 2020/08/08 19:10:39 revo Exp $">
    <kap>
      <rad>melantiac</rad>/oj
    </kap>

<drv mrk="melantiac.0oj">
  <kap><tld/>oj</kap>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_familioj"
      cel="famili.0o.BIO">Familio</ref>
      de <ref tip="malprt" cel="kotile.unu0aj">unukotiledonaj</ref>
      plantoj (Melanthiaceae),
      al kiu apartenas <ref tip="prt" cel="veratr.0o">veratro</ref>
      kaj <ref tip="prt" cel="amiant.0o">amiantio</ref>:
      <ekz>
        blanka heleboro a&ubreve; blanka veratro estas tre venena planto kiu
        apartenas al la
        <tld lit="M"/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=476758"
              >Blanka heleboro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Germergew&auml;chse</trd>
    <trd lng="en">bunchflower family</trd>
    <trd lng="fr">M&eacute;lanthiac&eacute;es</trd>
    <trd lng="pl">melantkowate</trd>
    <trd lng="ro">melanthiaceae</trd>
  </snc>
</drv>

  </art>

