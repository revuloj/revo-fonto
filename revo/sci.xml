<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sci</rad>/i
</kap>
<drv mrk="sci.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Koni certe kaj precize:
      <ekz>
        mi <tld/>as, ke vi estas honesta;
      </ekz>
      <ekz>
        &scirc;i <tld/>is, ke &scirc;i mem kantus pli bele<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi ne <tld/>is, kion fari<fnt>Z</fnt>,
      </ekz>
      <ekz>
        kiel min savi, kiun el la amba&ubreve; elekti<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i ne <tld/>as, kien &scirc;i iras<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi ne <tld/>as, &ccirc;u ne estos malfeli&ccirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ili ne <tld/>as alian lingvon krom sia propra<fnt>Z</fnt>;
      </ekz>
      <ekz>
        bone <tld/>i sian lecionon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        neniu <tld/>as &ccirc;ion<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne <tld/>i la detalojn de la regularo;
      </ekz>
      <ekz>
        neniu <tld/>as lian tombon<fnt>B</fnt>
        <klr>(kie estas lia tombo)</klr>;
      </ekz>
      <ekz>
        la ven&gcirc;o ne devas <tld/>i (havi) limojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiom mi <tld/>as<fnt>Z</fnt>
        <klr>(la&ubreve; tio, kion mi <tld/>as)</klr>;
      </ekz>
      <ekz>
        li estas multescia<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="nl">weten</trd>
  </snc>
  <snc>
    <dif>
      Esti sperta kaj lerta pri io:
      <ekz>
        <tld/>i na&gcirc;i<fnt>B</fnt>,
      </ekz>
      <ekz>
        pri na&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i ludi &scirc;akojn;
      </ekz>
      <ekz>
        <tld/>u (estu kapabla) elokventi, <tld/>u, anka&ubreve;
        silenti
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vi <tld/>is bone ekspluati mian situacion<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="nl">kunnen</trd>
    <trd lng="ru">&c_u;&c_m;&c_je;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>v&ecaron;d&ecaron;t</trd>,
    <trd>um&ecaron;t</trd>
  </trdgrp>
  <trd lng="de">wissen</trd>
  <trd lng="en">know</trd>
  <trd lng="es">saber</trd>
  <trd lng="ru">&c_z;&c_n;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sci.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Stato de tiu, kiu <tld/>as:
      <ekz>
        tio fari&gcirc;is sen mia <tld/>o;
      </ekz>
      <ekz>
        la&ubreve; mia <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kon">kono</ref>,
      <ref cel="scienc.0o">scienco</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Tio, kion oni <tld/>as:
      <ekz>
        akiri multajn <tld/>ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        plimultigi sian <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        via bu&scirc;o tenu <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Wissen</trd>
  <trd lng="ru">&c_z;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="sci.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      = <tld/>o 1:
      <ekz>
        <tld/>ado pri higieno estas &ccirc;iam utila;
      </ekz>
      <ekz>
        ne<tld/>ado de le&gcirc;o neniun pravigas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_z;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="sci.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      = <tld/>o 2.
    </dif>
  </snc>
  <trd lng="ru">&c_z;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="sci.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi iun <tld/>anta pri io:
      <ekz>
        <tld/>igi iun pri nova&jcirc;o<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_v;&c_je;&c_d;&c_o;&c_m;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_i;&c_n;&c_f;&c_o;&c_r;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Igi ion <tld/>ata de iu:
      <ekz>
        <tld/>igi nova&jcirc;on al iu<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_s;&c_o;&c_o;&c_b;&c_shch;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="de">mitteilen</trd>
</drv>

<drv mrk="sci.0igo">
  <kap><tld/>igo</kap>
  <snc>
    <dif>
      Tio, kion oni <tld/>igas:
      <ekz>
        hodia&ubreve; mi ricevis bonan <tld/>igon;
      </ekz>
      <ekz>
        la <tld/>igo pri mobilizado disporti&gcirc;is kiel fulmotondro.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="inform.0o">informo</ref>,
      <ref cel="aviz.0o">avizo</ref>,
      <ref cel="nov.0ajxo">nova&jcirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Mitteilung</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_v;&c_je;&c_d;&c_o;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="sci.0ema0ama0vola0avida">
  <kap><tld/>ema, <tld/>ama, <tld/>vola, <tld/>avida</kap>
  <snc>
    <dif>
      Tre deziranta <tld/>i:
      <ekz>
        mi <tld/>volas, kiu li estas.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">neugierig</trd>
  <trd lng="nl">nieuwsgierig</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_ju;&c_b;&c_o;&c_z;&c_n;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_o;&c_p;&c_y;&c_t;&c_n;&c_y;&c_j; <klr tip="amb">(&c_o; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="ru"></trd>
</drv>

<drv mrk="sci.mal0o">
  <kap>mal<tld/>o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Plena sen<tld/>o.
    </dif>
  </snc>
  <trd lng="de">Unwissen</trd>
  <trd lng="ru">&c_n;&c_je;&c_z;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="sci.povo0i0povi">
  <kap>povo<tld/>i, <tld/>povi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Havi teorian konon kaj anka&ubreve; praktikan sperton pri io:
      <ekz>
        <tld/>povi
        la hispanan lingvon;
      </ekz>
      <ekz>
        povo<tld/>i legi<fnt>Z</fnt>,
      </ekz>
      <ekz>
        labori<fnt>Z</fnt>,
      </ekz>
      <ekz>
        paroli jude<fnt>Z</fnt>,
      </ekz>
      <ekz>
        ludi harpon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fari bonon ili ne povo<tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ili ne hontas, e&ccirc; ne povo<tld/>as
        ru&gcirc;i&gcirc;i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        povo<tld/>i pretigi vinberan sa&ubreve;con<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni devas povo<tld/>i porti kaj gardi sian personan
        dignon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ili <tld/>povis konservi sufi&ccirc;e liberan komunuman
        aran&gcirc;on<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">k&ouml;nnen</trd>
  <trd lng="ru">&c_u;&c_m;&c_je;&c_t;&c_mol;</trd>
</drv>
</art>
