<drv mrk="sci.povo0i0povi">
  <kap>povo<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <ref tip="dif" cel="sci.0povi"><tld/>povi</ref>
    <ekz>
      li povo<tld/>is na&gcirc;i
      <fnt><bib>Fab4</bib>, <lok>historio el la dunoj</lok></fnt>;
    </ekz>
    <ekz>
      povo<tld/>i paroli jude<fnt>Z</fnt>,
    </ekz>
    <ekz>
      povo<tld/>i ludi harpon<fnt>Z</fnt>;
    </ekz>
    <ekz>
      fari bonon ili ne povo<tld/>as<fnt>Z</fnt>;
    </ekz>
    <ekz>
      ili ne hontas, e&ccirc; ne povo<tld/>as
      ru&gcirc;i&gcirc;i<fnt>Z</fnt>;
    </ekz>
    <ekz>
      povo<tld/>i pretigi vinberan sa&ubreve;con<fnt>Z</fnt>;
    </ekz>
    <ekz>
      oni devas povo<tld/>i porti kaj gardi sian personan
      dignon<fnt>Z</fnt>.
    </ekz>
  </snc>
  <rim>
    En la moderna lingvo la formo &leftquot;<tld/>povi&rightquot;
    estas preferata.
  </rim>
  <trd lng="be">&c_u;&c_m;&c_je;&c_c;&c_mol;</trd>
  <trd lng="de">k&ouml;nnen</trd>
  <trdgrp lng="en">
    <trd>know how</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>saber algo</trd>,
    <trd>conocer como</trd>
  </trdgrp>
  <trd lng="hu">tud <klr tip="ind">(k&eacute;pes)</klr></trd>
  <trd lng="ru">&c_u;&c_m;&c_je;&c_t;&c_mol;</trd>
  <trd lng="vo">kan&ouml;n</trd>
</drv>
