<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ego</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ego.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="ego.0o.PSI">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">PSI</uzo>
    <dif>
      La konscia subjekto, kontraste al la ne<tld/>o a&ubreve; objekto.
    </dif>
    <trdgrp lng="be">
      <trd>&c_e;&c_g;&c_a;</trd>,
      <trd>&c_Ja;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>ego</trd>,
      <trd>jo</trd>
    </trdgrp>
    <trd lng="en">ego</trd>
    <trd lng="es">ego</trd>
    <trdgrp lng="fr">
      <trd>ego</trd>,
      <trd>moi<klr tip="ind"> (subst., philo., psycho.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&Eacute;n</trd>,
      <trd>ego</trd>
    </trdgrp>
    <trd lng="nl">ego</trd>
    <trdgrp lng="de">
      <trd>Ich</trd>,
      <trd>Selbst</trd>,
      <trd>Ego</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ego</trd>,
      <trd><klr>(wewn&eogonek;trzne) </klr>ja</trd>
    </trdgrp>
    <trd lng="pt">ego</trd>
    <trdgrp lng="ru">
      <trd>&c_e;&c_g;&c_o;</trd>,
      <trd>&c_Ja;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>ego</trd>,
    <trd>subjektivn&iacute; j&aacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ego</trd>,
    <trd>subjekt&iacute;vne ja</trd>
  </trdgrp>
</drv>

<drv mrk="ego.0ismo">
  <kap><tld/>ismo</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="ego.0ismo.PSI">
    <dif>
      Ekskluziva memamo, kiu igas personon ser&ccirc;i en la vivo nur la
      propran plezuron a&ubreve; profiton:
    </dif>
    <ref tip="vid" cel="egoism.0o">egoismo</ref>
    <refgrp tip="ant">
      <ref cel="altrui.0o">altruismo</ref>,
      <ref cel="ego.mal0ismo.PSI">mal<tld/>ismo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_e;&c_g;&c_a;&c_ib;&c_z;&c_m;</trd>,
      <trd>&c_s;&c_a;&c_m;&c_a;&c_l;&c_ju;&c_b;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_s;&c_ja;&c_b;&c_je;&c_l;&c_ju;&c_b;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_s;&c_ja;&c_b;&c_je;&c_l;&c_ju;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="bg">&c_je;&c_g;&c_o;&c_i;&c_z;&c_malmol;&c_m;</trd>
    <trd lng="ca">egoisme</trd>
    <trdgrp lng="de">
      <trd>Ichsucht</trd>,
      <trd>Eigensucht</trd>,
      <trd>Selbstsucht</trd>,
      <trd>Egoismus</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>egoism</trd>,
      <trd>egotism</trd>
    </trdgrp>
    <trd lng="es">ego&iacute;smo</trd>
    <trd lng="fr">&eacute;go&iuml;sme</trd>
    <trdgrp lng="hu">
      <trd>&ouml;nz&eacute;s</trd>,
      <trd>egoizmus</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>ego&iuml;sme</trd>,
      <trd>eigenliefde</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>egoizm</trd>,
      <trd>samolubstwo</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_e;&c_g;&c_o;&c_i;&c_z;&c_m;</trd>,
      <trd>&c_s;&c_je;&c_b;&c_ja;&c_l;&c_ju;&c_b;&c_i;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>egoism</trd>,
      <trd>sj&auml;lviskhet</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>egoizmus</trd>,
    <trd>j&aacute;stv&iacute;</trd>,
    <trd>sobectv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>egoizmus</trd>,
    <trd>sebectvo</trd>
  </trdgrp>
</drv>

<drv mrk="ego.0isma">
  <kap><tld/>isma</kap>
  <snc mrk="ego.0isma.PSI">
    <dif>
      Ser&ccirc;anta en la vivo nur la propran plezuron a&ubreve;
      profiton; rilata al tia persono:
      <ekz>
        <tld/>ismaj pensoj, celoj, agoj.
      </ekz>
    </dif>
    <ref tip="ant" cel="altrui.0a">altruisma</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_g;&c_a;&c_ib;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_ja;&c_b;&c_je;&c_l;&c_ju;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_l;&c_ju;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">egoista<klr> (adj.)</klr></trd>
  <trd lng="cs">sobeck&yacute;</trd>
  <trdgrp lng="de">
    <trd>ichs&uuml;chtig</trd>,
    <trd>eigens&uuml;chtig</trd>,
    <trd>selbsts&uuml;chtig</trd>,
    <trd>egoistisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>egotistical</trd>,
    <trd>egoistical</trd>
    <trd>selfish</trd>
  </trdgrp>
  <trd lng="fr">&eacute;go&iuml;ste<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>&ouml;nz&odblac;</trd>,
    <trd>egosita</trd>
  </trdgrp>
  <trd lng="nl">ego&iuml;stisch</trd>
  <trdgrp lng="pl">
    <trd>egoistyczny</trd>,
    <trd>samolubny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_g;&c_o;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_e;&c_g;&c_o;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_je;&c_b;&c_ja;&c_l;&c_ju;&c_b;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>samo&lcaron;&uacute;by</trd>,
    <trd>sebeck&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="ego.0ismulo">
  <kap><tld/>ismulo</kap>
  <snc mrk="ego.0ismulo.PSI">
    <ref tip="dif" cel="ego.0isto"><tld/>isto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_g;&c_a;&c_ib;&c_s;&c_t;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_l;&c_ju;&c_b;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_l;&c_ju;&c_b;&c_je;&c_c;</trd>,
    <trd>&c_s;&c_ja;&c_b;&c_je;&c_l;&c_ju;&c_b;</trd>,
    <trd>&c_s;&c_a;&c_b;&c_je;&c_t;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">egoista<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>egoista</trd>,
    <trd>sobec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Egoist</trd>,
    <trd>selbsts&uuml;chtige Person</trd>,
    <trd>Selbsts&uuml;chtiger</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>egoist</trd>,
    <trd>egotist</trd>
  </trdgrp>
  <trd lng="es">ego&iacute;sta</trd>
  <trd lng="fr">&eacute;go&iuml;ste<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>&ouml;nz&odblac; ember</trd>,
    <trd>egoista</trd>
  </trdgrp>
  <trd lng="nl">ego&iuml;st</trd>
  <trdgrp lng="pl">
    <trd>egoista</trd>,
    <trd>samolub</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_g;&c_o;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_s;&c_je;&c_b;&c_ja;&c_l;&c_ju;&c_b;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sk">sebec</trd>
</drv>

<drv mrk="ego.0isto">
  <kap><tld/>isto</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="ego.0isto.PSI">
    <dif>
      Persono, kiu ser&ccirc;as en la vivo nur la propran plezuron
      a&ubreve; profiton.
    </dif>
    <ref tip="vid" cel="egoist.0o">egoisto</ref>
    <ref tip="ant" cel="altruist.0o">altruisto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_g;&c_a;&c_ib;&c_s;&c_t;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_l;&c_ju;&c_b;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_l;&c_ju;&c_b;&c_je;&c_c;</trd>,
    <trd>&c_s;&c_ja;&c_b;&c_je;&c_l;&c_ju;&c_b;</trd>,
    <trd>&c_s;&c_a;&c_b;&c_je;&c_t;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_g;&c_o;&c_i;&c_s;&c_t;</trd>
  <trd lng="ca">egoista<klr> (subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>egoista</trd>,
    <trd>sobec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ichs&uuml;chtiger Mensch</trd>,
    <trd>eigens&uuml;chtiger Mensch</trd>,
    <trd>selbsts&uuml;chtiger Mensch</trd>,
    <trd>Egoist</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>egoist</trd>,
    <trd>egotist</trd>
  </trdgrp>
  <trd lng="es">ego&iacute;sta</trd>
  <trd lng="fr">&eacute;go&iuml;ste<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>&ouml;nz&odblac; ember</trd>,
    <trd>egoista</trd>
  </trdgrp>
  <trd lng="nl">ego&iuml;st</trd>
  <trdgrp lng="pl">
    <trd>egoista</trd>,
    <trd>samolub</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_g;&c_o;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_s;&c_je;&c_b;&c_ja;&c_l;&c_ju;&c_b;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sk">sebec</trd>
  <trd lng="sv">egoist</trd>
</drv>

<drv mrk="ego.0ista">
  <kap><tld/>ista</kap>
  <snc mrk="ego.0ista.PSI">
    <ref tip="dif" cel="ego.0isma"><tld/>isma</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_g;&c_a;&c_ib;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_ja;&c_b;&c_je;&c_l;&c_ju;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_l;&c_ju;&c_b;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">egoista<klr> (adj.)</klr></trd>
  <trd lng="cs">sobeck&yacute;</trd>
  <trdgrp lng="de">
    <trd>ichs&uuml;chtig</trd>,
    <trd>eigens&uuml;chtig</trd>,
    <trd>selbsts&uuml;chtig</trd>,
    <trd>egoistisch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>egotistical</trd>,
    <trd>egoistical</trd>
  </trdgrp>
  <trd lng="es">ego&iacute;sta</trd>
  <trd lng="fr">&eacute;go&iuml;ste<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="he">&alef;&gimel;&vav;&alef;&yod;&samekh;&tet;&yod;</trd>
  <trdgrp lng="hu">
    <trd>&ouml;nz&odblac;</trd>,
    <trd>egosita</trd>
  </trdgrp>
  <trd lng="nl">ego&iuml;stisch</trd>
  <trdgrp lng="pl">
    <trd>egoistyczny</trd>,
    <trd>samolubny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_g;&c_o;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_e;&c_g;&c_o;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_je;&c_b;&c_ja;&c_l;&c_ju;&c_b;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
