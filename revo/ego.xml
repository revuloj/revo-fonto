<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ego.xml,v 1.38 2023/11/07 19:22:22 revo Exp $">
<!-- la varia&jcirc;oj egoism/o kaj egoist/o estas intence malsupre &ccirc;i, por
aldoni la oficialajn
     radikojn egoism/ kaj egoist/ la&ubreve; 1a OA, kaj samtempe montri ke ni
     konsideras &gcirc;in
     hodia&ubreve; deriva&jcirc;o de ego/. Bv. ne forigi a&ubreve; &scirc;an&gcirc;i! -->
<kap>
  <rad>ego</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><ofc>1</ofc> <rad>egoism</rad>/o</kap></var>,
  <var><kap><ofc>1</ofc> <rad>egoist</rad>/o</kap></var>
</kap>
<drv mrk="ego.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="ego.0o.PSI">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">PSI</uzo>
    <dif>
      La konscia subjekto, kontraste al la ne<tld/>o a&ubreve; objekto.
    </dif>
    <trdgrp lng="be">
      <trd>&c_e;&c_g;&c_a;</trd>,
      <trd>&c_Ja;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>ego</trd>,
      <trd>jo</trd>
    </trdgrp>
    <trd lng="en">ego</trd>
    <trd lng="es">ego</trd>
    <trdgrp lng="fr">
      <trd>ego</trd>,
      <trd>moi<klr tip="ind"> (subst., philo., psycho.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&Eacute;n</trd>,
      <trd>ego</trd>
    </trdgrp>
    <trd lng="nl">ego</trd>
    <trdgrp lng="de">
      <trd>Ich</trd>,
      <trd>Selbst</trd>,
    </trdgrp>
    <trdgrp lng="pl">
      <trd>ego</trd>,
      <trd><klr>(wewn&eogonek;trzne) </klr>ja</trd>
    </trdgrp>
    <trd lng="pt">ego</trd>
    <trdgrp lng="ru">
      <trd>&c_e;&c_g;&c_o;</trd>,
      <trd>&c_Ja;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ego.0o.KOMUNE">
    <dif>
      Propra personeco, konscianta pri siaj bezonoj prioritate, kaj
      ignoranta la bezonojn
      de aliaj:
      <ekz>
        la pli multaj Novepok-aj filozofioj prezentas nur metodojn por
        senti sin pli
        sane, por senti sin pli komforte en la revoj de sia iluzia
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Declerck</aut>
          <vrk><url ref="&Monato;/2009/009048.php">Spirituala
          materialismo</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        klanestroj: rabemaj, oportunismaj fe&ubreve;daj baronoj, kiuj
        prostituas siajn plej
        proksimajn keltajn parencojn por &scirc;veligi siajn <tld/>ojn kaj
        bank-kontojn
        <fnt>
          <aut>W. H. Simcock</aut>
            <vrk><url ref="&Monato;/2000/007111.php">Kla(&ubreve;)nado inter la montoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 13</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="am.mem0o">memamo</ref>,
       <ref cel="estim.mem0o">memestimo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>ego</trd>,
    <trd>subjektivn&iacute; j&aacute;</trd>
  </trdgrp>
  <trd lng="de">Ego</trd>
  <trd lng="he">&#x5D0;&#x5B8;&#x5E0;&#x5D5;&#x5B9;&#x5DB;&#x5B4;&#x5D9;</trd>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30B4;&#x30A4;&#x30B9;&#x30C8;</trd>,
    <trd>&#x5229;&#x5DF1;&#x4E3B;&#x7FA9;&#x8005;  <pr>&#x308A;&#x3053;&#x3057;&#x3085;&#x304E;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ego</trd>,
    <trd>subjekt&iacute;vne ja</trd>
  </trdgrp>
  <trd lng="uk">&c_je;&c_g;&#x43E;&c_ji;&c_s;&c_t;</trd>
</drv>

<drv mrk="ego.0ismo">
  <kap><ofc>1</ofc> <tld/>ismo</kap>
  <snc mrk="ego.0ismo.PSI">
    <dif>
      Ekskluziva memamo, kiu igas personon ser&ccirc;i en la vivo nur la
      propran plezuron a&ubreve; profiton:
      <ekz>
        obeante la vo&ccirc;on de la indiferenta <tld/>ismo
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        li rimarkis, ke <klr>[&#x2026;]</klr> sur la fundo de &ccirc;iu koro ku&scirc;as
        <tld/>ismo
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iajn idealojn kaj celadon gente-naciajn mi rigardas nur kiel
        grupan <tld/>ismon
        kaj hommalamon
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="altrui.0o">altruismo</ref>,
      <ref cel="ego.mal0ismo.PSI">mal<tld/>ismo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_e;&c_g;&c_a;&c_ib;&c_z;&c_m;</trd>,
      <trd>&c_s;&c_a;&c_m;&c_a;&c_l;&c_ju;&c_b;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_s;&c_ja;&c_b;&c_je;&c_l;&c_ju;&c_b;&c_s;&c_t;&c_v;&c_a;</trd>,
      <trd>&c_s;&c_ja;&c_b;&c_je;&c_l;&c_ju;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="bg">&c_je;&c_g;&c_o;&c_i;&c_z;&c_malmol;&c_m;</trd>
    <trd lng="ca">egoisme</trd>
    <trdgrp lng="de">
      <trd>Ichsucht</trd>,
      <trd>Eigensucht</trd>,
      <trd>Selbstsucht</trd>,
      <trd>Egoismus</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>egoism</trd>,
      <trd>egotism</trd>
    </trdgrp>
    <trd lng="es">ego&iacute;smo</trd>
    <trd lng="fr">&eacute;go&iuml;sme</trd>
    <trdgrp lng="hu">
      <trd>&ouml;nz&eacute;s</trd>,
      <trd>egoizmus</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>ego&iuml;sme</trd>,
      <trd>eigenliefde</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>egoizm</trd>,
      <trd>samolubstwo</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_e;&c_g;&c_o;&c_i;&c_z;&c_m;</trd>,
      <trd>&c_s;&c_je;&c_b;&c_ja;&c_l;&c_ju;&c_b;&c_i;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>egoism</trd>,
      <trd>sj&auml;lviskhet</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x81EA;&#x79C1;&#x81EA;&#x5229; <pr>z&#xEC;s&#x12B;z&#xEC;l&#xEC;</pr></trd>,
    <trd>&#x5229;&#x5DF1;&#x4E3B;&#x4E49; <pr>l&#xEC;j&#x1D0;zh&#x1D4;y&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>egoizmus</trd>,
    <trd>j&aacute;stv&iacute;</trd>,
    <trd>sobectv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>egoizmus</trd>,
    <trd>sebectvo</trd>
  </trdgrp>
</drv>

<drv mrk="ego.0isma">
