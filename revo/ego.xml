<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
   <kap>
    <rad>ego</rad>/o
   </kap>
   <uzo tip="fak">PSI</uzo>
   <fnt><bib>PIV1</bib></fnt>
   <drv mrk="ego.0o">
    <kap><tld/>o</kap>
    <snc>
       <dif>
          La konscia subjekto, kontraste al la neegoo a&ubreve; objekto.
       </dif>
        <trdgrp lng="de">
          <trd>Ich</trd>,
          <trd>Selbst</trd>,
          <trd>Ego</trd>
       </trdgrp>
    </snc>
  </drv>

<drv mrk="ego.0isma">
  <kap><tld/>isma</kap>
  <snc>
       <dif>
          Ser&ccirc;anta en la vivo nur la propran plezuron a&ubreve;
profiton. Kp. <ref tip="vid" cel="altrui.0a">altruisma</ref>.
       </dif>
        <trdgrp lng="de">
          <trd>ichs&uuml;chtig</trd>,
          <trd>eigens&uuml;chtig</trd>,
          <trd>selbsts&uuml;chtig</trd>,
          <trd>egoistisch</trd>
       </trdgrp>
  </snc>
</drv>

<drv mrk="ego.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
       <dif>
          Ekskluziva sinamo, kiu igas personon ser&ccirc;i en la vivo nur la
propran plezuron a&ubreve; profiton. Kp. <ref tip="vid"
cel="altrui.0o">altruismo</ref>.
      <ekz>
        <ind><tld/>a</ind>j pensoj, celoj, agoj;
      </ekz>
      <ekz>
        neniu alia malsano <klr>[ol aidoso (...)]</klr> postulis
        pli da kompato, prudento, <ind>mal<tld/>ismo</ind> kaj honesteco &ccirc;e
        sanservaj profesiuloj
        <fnt><bib>UrdHadda</bib>, <lok>p. 25</lok></fnt>.
      </ekz>
       </dif>
       <trdgrp lng="be">
         <trd>&c_e;&c_g;&c_a;&c_i;&c_z;&c_m;</trd>, 
         <trd>&c_s;&c_a;&c_m;&c_a;&c_l;&c_ju;&c_b;&c_s;&c_t;&c_v;&c_a;</trd>
       </trdgrp>
        <trdgrp lng="de">
          <trd>Ichsucht</trd>,
          <trd>Eigensucht</trd>,
          <trd>Selbstsucht</trd>,
          <trd>Egoismus</trd>
       </trdgrp>
       <trd lng="es">egoismo</trd>
       <trd lng="fr">&eacute;go&iuml;sme</trd>
       <trdgrp lng="ru">
          <trd>&c_e;&c_g;&c_o;&c_i;&c_z;&c_m;</trd>, 
          <trd>&c_s;&c_je;&c_b;&c_ja;&c_l;&c_ju;&c_b;&c_i;&c_je;</trd>
       </trdgrp>
       <trdgrp lng="sv">
         <trd>egoism</trd>,
         <trd>sj&auml;lviskhet</trd>
       </trdgrp>
  </snc>
</drv>

<drv mrk="ego.0ista">
  <kap><tld/>ista</kap>
  <snc>
       <dif>
          Egoisma. Kp. <ref tip="vid" cel="altrui.0a">altruisma</ref>.
       </dif>
        <trdgrp lng="de">
          <trd>ichs&uuml;chtig</trd>,
          <trd>eigens&uuml;chtig</trd>,
          <trd>selbsts&uuml;chtig</trd>,
          <trd>egoistisch</trd>
       </trdgrp>
       <trd lng="fr">&eacute;go&iuml;ste<klr tip="ind"> (adj.)</klr></trd>
  </snc>
</drv>

<drv mrk="ego.0isto">
  <kap><tld/>isto</kap>
  <snc>
       <dif>
          Persono, kiu ser&ccirc;as en la vivo nur la propran plezuron
a&ubreve; profiton. Kp. <ref tip="vid" cel="altruist.0o">altruisto</ref>.
       </dif>
       <trdgrp lng="be">
         <trd>&c_e;&c_g;&c_a;&c_ib;&c_s;&c_t;</trd>, 
         <trd>&c_s;&c_a;&c_m;&c_a;&c_l;&c_ju;&c_b;&c_je;&c_c;</trd>
       </trdgrp>
       <trdgrp lng="de">
          <trd>ichs&uuml;chtiger Mensch</trd>,
          <trd>eigens&uuml;chtiger Mensch</trd>,
          <trd>selbsts&uuml;chtiger Mensch</trd>,
          <trd>Egoist</trd>
       </trdgrp>
       <trd lng="es">egoista</trd>
       <trd lng="fr">&eacute;go&iuml;ste<klr tip="ind"> (subst.)</klr></trd>
       <trd lng="hu">egoista</trd>
       <trd lng="nl">ego&iuml;st</trd>
       <trd lng="ru">&c_e;&c_g;&c_o;&c_i;&c_s;&c_t;</trd>
       <trd lng="sv">egoist</trd>
  </snc>
</drv>

<drv mrk="ego.0centra">
  <kap><tld/>centra</kap>
  <snc>
       <dif>
          Rilatiganta &ccirc;ion al sia egoo.
       </dif>
        <trdgrp lng="de">
          <trd>ichbezogen</trd>,
          <trd>egozentrisch</trd>
       </trdgrp>
  </snc>
</drv>

<drv mrk="ego.0centrismo">
  <kap><tld/>centrismo</kap>
  <snc>
       <dif>
          La ago a&ubreve; karaktera trajto rilatigi &ccirc;ion al sia egoo.
       </dif>
        <trdgrp lng="de">
          <trd>Ichbezogenheit</trd>,
          <trd>Egozentrismus</trd>,
          <trd>Egozentrik</trd>
       </trdgrp>
  </snc>
</drv>

</art>

