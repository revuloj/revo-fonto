<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fonolog.xml,v 1.6 2023/05/08 21:24:46 revo Exp $">
<kap>
  <rad>fonolog</rad>/o
</kap>
<drv mrk="fonolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="fonolog.0o.SCI">
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="dif" cel="fonologi.0isto.SCI">Fonologiisto</ref>:
      <ekz>
        la teoria aliro de <tld/>oj al la nocio silabo
        estas tre abstrakta
        <fnt>
          <aut>M. Bavant</aut>,
          <vrk><url
          ref="http://lingvakritiko.com/2006/12/20/silabo-kaj-silabado/">
          Silabo kaj silabado</url></vrk>,
          <lok>Lingva Kritiko, 2006-12-20</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia famo, interalie kiel <tld/>o, disvasti&gcirc;is mondskale
        <fnt>
          <vrk><url ref="http://dvd.ikso.net/faka/esperanto/Martinet/Interlingvistiko_eo.pdf"
              >Pri kelkaj problemoj de interlingvistiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        elstara <tld/>o kaj faka specialisto pri la dekstraj vo&ccirc;kordoj de plena&gcirc;aj
        nekultivistaj viraj lo&gcirc;antoj de la angla vila&gcirc;o <nom>Shutdown</nom>
        <fnt>
          <vrk><url ref="http://esperanto.net/literaturo/tekstoj/neves/fakist.html"
              >Fakistoj - Esperanto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">fon&ograve;leg</trd>
  <trd lng="cs">fonolog</trd>
  <trd lng="de">Phonologe</trd>
  <trd lng="en">phonologist</trd>
  <trd lng="fa">&a_w;&a_A;&a_j;&#8204;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">phonologiste</trd>
  <trd lng="pl">fonolog</trd>
  <trdgrp lng="pt">
    <trd>fon&oacute;logo</trd>,
    <trd>fonologista</trd>
  </trdgrp>
  <trd lng="sk">gonol&oacute;g</trd>
</drv>
</art>

