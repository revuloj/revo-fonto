<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kapot</rad>/o
</kap>
<drv mrk="kapot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kapot.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Granda dika kapu&ccirc;hava <ref tip="super" cel="mantel.0o">mantelo</ref>:
      <ekz>
        <tld/>o el fajna drapo verda fran&gcirc;ita per ru&gcirc;a veluro
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 16a, p.
        476a</lok></fnt>
      </ekz>
    </dif>
    <trd lng="de">Kapuzenmantel</trd>
    <trd lng="fr">capote<klr tip="ind"> (v&ecirc;tement)</klr></trd>
    <trd lng="hu">kapucnis <ind>kab&aacute;t</ind></trd>
    <trd lng="nl">kapotjas</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_shch; &c_s; &c_k;&c_a;&c_p;&c_ju;&c_sh;&c_o;&c_n;&c_o;&c_m;</trd>,
      <trd>&c_b;&c_a;&c_l;&c_a;&c_h;&c_o;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kapot.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Metala kovrilo de <ref tip="malprt" cel="auxt.0o">a&ubreve;ta</ref> motoro.
    </dif>
    <trdgrp lng="de">
      <trd>Motorhaube</trd>,
      <trd>K&uuml;hlerhaube</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>cap&oacute;</trd>,
      <trd>cofre</trd>
    </trdgrp>
    <trd lng="fr">capot<klr tip="ind"> (de voiture)</klr></trd>
    <trd lng="hu">motorh&aacute;ztet&odblac;</trd>
    <trd lng="nl">motorkap</trd>
    <trd lng="ru">&c_k;&c_a;&c_p;&c_o;&c_t;</trd>
  </snc>
</drv>
</art>
