<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kapot.xml,v 1.24 2025/04/09 18:38:10 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kapot</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="kapot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kapot.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Granda dika kapu&ccirc;hava <ref tip="super" cel="mantel.0o">
      mantelo</ref>:
      <ekz>
        <tld/>o el fajna drapo verda fran&gcirc;ita per ru&gcirc;a
        veluro
        <fnt><bib>DonKihxoto</bib>,
        <lok>dua parto, &ccirc;apitro 16a, p. 476a</lok></fnt>;
      </ekz>
      <ekz>
        virina figuro de apena&ubreve; meza kresko, vestita per longa <tld/>o, kies kapu&ccirc;o
        preska&ubreve; tute ka&scirc;is &scirc;ian viza&gcirc;on
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        taratato a&ubreve; taratatko, mallonga pola <tld/>o kun la&ccirc;oj kaj fran&gcirc;oj
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IV.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="anorak.0o">anorako</ref>,
      <ref cel="mantel.kapucx0o">kapu&ccirc;mantelo</ref>,
      <ref cel="frok.0o">froko</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_b;&c_a;&c_l;&c_a;&c_h;&c_o;&c_n;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_o;&c_w;</trd>,
      <trd>&c_h;&c_l;&c_a;&c_m;&c_ib;&c_d;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kapuzenmantel</trd>,
      <trd>Parka</trd>.
      <trd><ofc>*</ofc>Capot</trd>
    </trdgrp>
    <trd lng="en">capote</trd>
    <trd lng="fr"><ofc>*</ofc>capote<klr tip="ind"> (v&ecirc;tement)</klr></trd>
    <trd lng="hu">kapucnis <ind>kab&aacute;t</ind></trd>
    <trdgrp lng="it">
      <trd>mantello<klr> (con cappuccio)</klr></trd>,
      <trd>pastrano<klr> (con cappuccio)</klr></trd>
    </trdgrp>
    <trd lng="nl">kapotjas</trd>
    <trd lng="pl"><ofc>*</ofc>kapota</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_shch; &c_s;
      &c_k;&c_a;&c_p;&c_ju;&c_sh;&c_o;&c_n;&c_o;&c_m;</trd>,
      <trd>&c_b;&c_a;&c_l;&c_a;&c_h;&c_o;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kapot.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Metala kovrilo de <ref tip="malprt" cel="auxt.0o">a&ubreve;ta</ref> motoro:
      <ekz>
        <nom>Martin</nom> sidi&gcirc;is sur la <tld/>o de la a&ubreve;to
        <fnt><aut>I. Nemere</aut>, <vrk>Sur Kampo Granita</vrk>,
        <lok>1983</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_p;&c_o;&c_t;</trd>
    <trdgrp lng="de">
      <trd>Motorhaube</trd>,
      <trd>K&uuml;hlerhaube</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>hood<klr tip="amb"> (car engine cover - US)</klr></trd>,
      <trd>bonnet<klr tip="amb"> (car engine cover -
      British)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>cap&oacute;</trd>,
      <trd>cofre</trd>
    </trdgrp>
    <trd lng="fr">capot<klr tip="ind"> (de voiture)</klr></trd>
    <trd lng="hu">motorh&aacute;ztet&odblac;</trd>
    <trdgrp lng="it">
      <trd>cofano<klr> (di auto)</klr></trd>
    </trdgrp>
    <trd lng="nl">motorkap</trd>
    <trd lng="pl">maska<klr> silnika</klr></trd>
    <trd lng="ru">&c_k;&c_a;&c_p;&c_o;&c_t;</trd>
  </snc>
  <trd lng="bg">&c_k;&c_a;&c_ch;&c_u;&c_l;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>kapota</trd>,
    <trd>pl&aacute;&scaron;&tcaron; s kapuc&iacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12501;&#12540;&#12489;&#20184;&#12365;&#12398;&#38263;&#12356;&#22806;&#22871;  <pr>&#12405;&#12540;&#12393;&#12388;&#12365;&#12398;&#12394;&#12364;&#12356;&#12364;&#12356;&#12392;&#12358;</pr></trd>,
    <trd>&#12508;&#12531;&#12493;&#12483;&#12488;</trd>,
    <trd>&#12456;&#12531;&#12472;&#12531;&#12459;&#12496;&#12540;</trd>
  </trdgrp>
  <trd lng="sk">pl&aacute;&scaron;&tcaron; s kapuc&ncaron;ou</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_l;&c_a;&c_shch; (&c_z; &c_k;&c_a;&c_p;&c_t;&c_u;&c_r;&c_o;&c_m;)</trd>,
    <trd>&c_k;&c_a;&c_p;&c_o;&c_t;</trd>,
    <trd>&c_k;&c_o;&c_zh;&c_u;&c_h;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_i;&c_sh;&c_k;&c_a; (&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_o;&c_g;&c_o; &c_m;&c_o;&c_t;&c_o;&c_r;&c_a;)</trd>
  </trdgrp>
</drv>
</art>
