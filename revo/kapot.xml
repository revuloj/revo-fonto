<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kapot</rad>/o
</kap>
<drv mrk="kapot.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Granda dika kapu&ccirc;hava mantelo.
    </dif>
    <trd lng="de">Kapuzenmantel</trd>
    <trd lng="hu">kapucnis <ind>kab&aacute;t</ind></trd>
    <trd lng="nl">kapotjas</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_shch; &c_s; &c_k;&c_a;&c_p;&c_ju;&c_sh;&c_o;&c_n;&c_o;&c_m;</trd>,
      <trd>&c_b;&c_a;&c_l;&c_a;&c_h;&c_o;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">AUT</uzo>
    <dif>
      Metala kovrilo de a&ubreve;tomobila motoro.
    </dif>
    <trdgrp lng="de">
      <trd>Motorhaube</trd>,
      <trd>K&uuml;hlerhaube</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>cap&oacute;</trd>,
      <trd>cofre</trd>
    </trdgrp>
    <trd lng="hu">motorh&aacute;ztet&odblac;</trd>
    <trd lng="nl">motorkap</trd>
    <trd lng="ru">&c_k;&c_a;&c_p;&c_o;&c_t;</trd>
  </snc>
</drv>
</art>
