<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kapot.xml,v 1.22 2023/10/18 14:48:10 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kapot</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kapot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kapot.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Granda dika kapu&ccirc;hava <ref tip="super" cel="mantel.0o">
      mantelo</ref>:
      <ekz>
        <tld/>o el fajna drapo verda fran&gcirc;ita per ru&gcirc;a
        veluro
        <fnt><bib>DonKihxoto</bib>,
        <lok>dua parto, &ccirc;apitro 16a, p. 476a</lok></fnt>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_a;&c_l;&c_a;&c_h;&c_o;&c_n;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_o;&c_w;</trd>,
      <trd>&c_h;&c_l;&c_a;&c_m;&c_ib;&c_d;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Kapuzenmantel</trd>
    <trd lng="en">capote</trd>
    <trd lng="fr">capote<klr tip="ind"> (v&ecirc;tement)</klr></trd>
    <trd lng="hu">kapucnis <ind>kab&aacute;t</ind></trd>
    <trdgrp lng="it">
      <trd>mantello<klr> (con cappuccio)</klr></trd>,
      <trd>pastrano<klr> (con cappuccio)</klr></trd>
    </trdgrp>
    <trd lng="nl">kapotjas</trd>
    <trd lng="pl">kapota</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_shch; &c_s;
      &c_k;&c_a;&c_p;&c_ju;&c_sh;&c_o;&c_n;&c_o;&c_m;</trd>,
      <trd>&c_b;&c_a;&c_l;&c_a;&c_h;&c_o;&c_n;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kapot.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Metala kovrilo de <ref tip="malprt" cel="auxt.0o">a&ubreve;ta</ref> motoro:
      <ekz>
        Martin sidi&gcirc;is sur la <tld/>o de la a&ubreve;to
        <fnt><aut>I. Nemere</aut>, <vrk>Sur Kampo Granita</vrk>, 
        <lok>1983</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_p;&c_o;&c_t;</trd>
    <trdgrp lng="de">
      <trd>Motorhaube</trd>,
      <trd>K&uuml;hlerhaube</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>hood<klr tip="amb"> (car engine cover - US)</klr></trd>,
      <trd>bonnet<klr tip="amb"> (car engine cover -
      British)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>cap&oacute;</trd>,
      <trd>cofre</trd>
    </trdgrp>
    <trd lng="fr">capot<klr tip="ind"> (de voiture)</klr></trd>
    <trd lng="hu">motorh&aacute;ztet&odblac;</trd>
    <trdgrp lng="it">
      <trd>cofano<klr> (di auto)</klr></trd>
    </trdgrp>
    <trd lng="nl">motorkap</trd>
    <trd lng="pl">maska<klr> silnika</klr></trd>
    <trd lng="ru">&c_k;&c_a;&c_p;&c_o;&c_t;</trd>
  </snc>
  <trd lng="bg">&c_k;&c_a;&c_ch;&c_u;&c_l;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>kapota</trd>,
    <trd>pl&aacute;&scaron;&tcaron; s kapuc&iacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30FC;&#x30C9;&#x4ED8;&#x304D;&#x306E;&#x9577;&#x3044;&#x5916;&#x5957;  <pr>&#x3075;&#x30FC;&#x3069;&#x3064;&#x304D;&#x306E;&#x306A;&#x304C;&#x3044;&#x304C;&#x3044;&#x3068;&#x3046;</pr></trd>,
    <trd>&#x30DC;&#x30F3;&#x30CD;&#x30C3;&#x30C8;</trd>,
    <trd>&#x30A8;&#x30F3;&#x30B8;&#x30F3;&#x30AB;&#x30D0;&#x30FC;</trd>
  </trdgrp>
  <trd lng="sk">pl&aacute;&scaron;&tcaron; s kapuc&ncaron;ou</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&#x430;&#x449; (&#x437; &#x43A;&#x430;&#x43F;&#x442;&#x443;&#x440;&#x43E;&#x43C;)</trd>,
    <trd>&#x43A;&#x430;&#x43F;&#x43E;&#x442;</trd>,
    <trd>&#x43A;&#x43E;&#x436;&#x443;&#x445;</trd>,
    <trd>&#x43F;&#x43E;&#x43A;&#x440;&#x438;&#x448;&#x43A;&#x430; (&#x430;&#x432;&#x442;&#x43E;&#x43C;&#x43E;&#x431;&#x456;&#x43B;&#x44C;&#x43D;&#x43E;&#x433;&#x43E; &#x43C;&#x43E;&#x442;&#x43E;&#x440;&#x430;)</trd>
  </trdgrp>
</drv>
</art>
