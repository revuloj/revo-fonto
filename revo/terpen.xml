<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: terpen.xml,v 1.11 2025/12/31 22:33:56 revo Exp $">
<kap>
  <rad>terpen</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="terpen.0o">
  <kap><tld/>o</kap>
  <snc mrk="terpen.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="hidrok.0o.KEM">Hidrokarbido</ref>
      kiun enhavas rezinoj de vegeta&jcirc;oj;
      simbole <frm>(C<sub>5</sub>H<sub>8</sub>)<sub>n</sub></frm>:
      <ekz>
        en la inflamreduktado partoprenas precipe beta-sisterolo,
        <tld/>oj, malsaturitaj gras-acidoj
        <fnt>
          <aut>Bogn&aacute;r &Aacute;.</aut>,
          <vrk><url ref="&STEB;dommastrumado/noni.htm">
          La kuracplanto Nonio</url></vrk>,
          <lok>[2010?]</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">terpene</trd>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_r;&c_p;&c_e;&c_n;</trd>
  <trd lng="ca">terp&eacute;</trd>
  <trd lng="cs">terpen</trd>
  <trd lng="de">Terpen</trd>
  <trd lng="fr">terp&egrave;ne</trd>
  <trd lng="pl">terpen</trd>
  <trd lng="sk">terp&eacute;n</trd>
  <trd lng="uk">&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#x841C; <pr>ti&emacron;</pr></trd>,
    <trd>&#x841C;&#x70EF; <pr>ti&emacron;x&imacron;</pr></trd>,
    <trd>&#x841C;&#x7C7B; <pr>ti&emacron;l&egrave;i</pr></trd>,
    <trd>&#x841C;&#x985E; <pr>ti&emacron;l&egrave;i</pr></trd>
  </trdgrp>
</drv>
</art>

