  <trd lng="nl">pong&eacute;zijde</trd>
  <trd lng="pl">pong&eacute;  <klr>(tkanina jap.)</klr></trd>
  <trd lng="pt">ponj&ecirc;</trd>
  <trd lng="ro">pong&eacute;</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43D;&c_d;&c_zh;&c_je; (&#x43B;&c_je;&c_g;&#x43A;&c_a; &c_t;&#x43A;&c_a;&#x43D;&c_i;&#x43D;&c_a; &c_z;
  &c_g;&c_r;&c_u;&c_b;&#x43E;&c_g;&#x43E; &c_sh;&#x43E;&c_v;&#x43A;&c_u;)</trd>
</drv>
