<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: logogr.xml,v 1.16 2020/02/07 01:11:31 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>logogrif</rad>/o
</kap>
<drv mrk="logogr.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="logogr.0o.enigmo">
    <dif>
      <ref tip="super" cel="enigm.0o.SPO">Enigmo</ref>, en kiu
      intermiksitaj
      literoj a&ubreve; silaboj de iu vorto formas aliajn vortojn, kiujn oni
      devas diveni:
      <ekz>
        <nom>Novejarque</nom> estis hispana esperantisto, verkisto de
        <tld/>oj kaj &scirc;akoproblemoj
        por diversaj Esperanto-gazetoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26545"
              >Angelhispano Novejarque</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas enigmo, &ccirc;e kiu oni devas la&ubreve; proponitaj difinoj
        diveni plurajn
        vortojn prezentantaj samajn literojn en diversaj aran&gcirc;oj
        (ekzemple kelo,
        kleo, leko)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27444"
              >Logogrifo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="anagra.0o">anagramo</ref>
    <trdgrp lng="hu">
      <trd>sz&oacute;rejtv&eacute;ny</trd>,
      <trd>bet&udblac;rejtv&eacute;ny</trd>,
      <trd>logogrif</trd>
    </trdgrp>    
  </snc>
  <snc mrk="logogr.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Nekomprenebla frazo.
    </dif>
    <trd lng="hu">tal&aacute;nyos mondat</trd>
    <trd lng="pl">niezrozumia&lstroke;e wyra&zdot;enie</trd>    
  </snc>
  <trd lng="ca">logogrif</trd>
  <trdgrp lng="de">
     <trd>Wortr&auml;tsel</trd>,
     <trd>Buchstabenr&auml;tsel</trd>,
     <trd>Logogriph</trd>
  </trdgrp>
  <trd lng="be">&c_l;&c_ja;&c_g;&c_a;&c_g;&c_r;&c_y;&c_f;</trd>
  <trd lng="en">logogriph</trd>
  <trd lng="es">logogrifo</trd>
  <trd lng="nl">letterraadsel</trd>
  <trd lng="pl">logogryf</trd> 
  <trd lng="pt">logogrifo</trd>
  <trd lng="ro">logogrif</trd>
  <trd lng="ru">&c_l;&c_o;&c_g;&c_o;&c_g;&c_r;&c_i;&c_f; <klr>(&c_r;&c_o;&c_d; &c_z;&c_a;&c_g;&c_a;&c_d;&c_k;&c_i;)</klr></trd>
</drv>
</art>
