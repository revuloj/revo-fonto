    <trd>aureool</trd>,
    <trd>stralenkrans</trd>
  </trdgrp>
  <trd lng="pl">nimb</trd>
  <trdgrp lng="pt">
    <trd>aur&eacute;ola</trd>,
    <trd>aura</trd>,
    <trd>nimbo</trd>    
