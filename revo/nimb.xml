    <trd>aur&egrave;ola</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>aureola</trd>,
    <trd>svatoz&aacute;&rcaron;</trd>
  </trdgrp>
  <trd lng="de">Nimbus</trd>
  <trdgrp lng="en">
    <trd>halo</trd>,
    <trd>nimbus</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>nimbe</trd>,
    <trd>aur&eacute;ole<klr tip="ind"> (lumi&egrave;re, prestige)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nimbusz</trd>,
    <trd>dicsf&eacute;ny</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F8C;&#x5149;  [<ind>&#x3054;&#x3053;&#x3046;</ind>]</trd>,
    <trd>&#x5149;&#x8F2A;  [<ind>&#x3053;&#x3046;&#x308A;&#x3093;</ind>]</trd>,
    <trd>&#x5149;&#x306E;&#x8F2A;  [<ind>&#x3072;&#x304B;&#x308A;&#x306E;&#x308A;&#x3093;</ind>]</trd>
  </trdgrp>
