<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>nimb</rad>/o
</kap>
<drv mrk="nimb.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="auxreo.0o">A&ubreve;reolo</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        doni al la afero <tld/>on <klr>(presti&gcirc;on)</klr> de oficialeco.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ib;&c_m;&c_b;</trd>,
    <trd>&c_a;&c_r;&c_e;&c_o;&c_l;</trd>
  </trdgrp>
  <trd lng="de">Nimbus</trd>
  <trdgrp lng="en">
    <trd>halo</trd>,
    <trd>nimbus</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>nimbe</trd>,
    <trd>aur&eacute;ole<klr tip="ind"> (lumi&egrave;re, prestige)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nimbusz</trd>,
    <trd>dicsf&eacute;ny</trd>
  </trdgrp>
   <trdgrp lng="nl">
    <trd>nimbus</trd>,
    <trd>aureool</trd>,
    <trd>stralenkrans</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>aur&eacute;ola</trd>,
    <trd>aura</trd>,
    <trd>nimbo</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_m;&c_b;</trd>,
    <trd>&c_o;&c_r;&c_je;&c_o;&c_l;</trd>
  </trdgrp>
</drv>
</art>
