<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fring.xml,v 1.23 2023/08/31 13:11:31 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>fring</rad>/o
</kap>
<drv mrk="fring.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="fringe1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Fringilla</trd>):
      <ekz>
        estas ja io bela voja&gcirc;i tiel familie, ne kiel la <tld/>oj kaj batalkokoj, &ccirc;e
        kiuj la viroj flugas aparte kaj la virinoj aparte
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>   
      <ekz>
        la vira <tld/>o havas nigrecan fruntopunkton, bluecgrizajn kronon kaj flankan
        duonkolumon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17683">Fringo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/7c/Fringilla_coelebs_chaffinch_male_edit2.jpg/425px-Fringilla_coelebs_chaffinch_male_edit2.jpg" prm="CC BY-SA 2.5"><tld/>o (Fringilla coelebs)
        <fnt>
          <aut>foto de MichaelMaggs, prilaborita de Arad, propra verko</aut>
          <lok><url ref="&WCW;/File:Fringilla_coelebs_chaffinch_male_edit2.jpg">&WCW;/File:Fringilla_coelebs_chaffinch_male_edit2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="bg">&c_o;&c_b;&c_i;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_a; &c_ch;&c_i;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>p&ecaron;&ncaron;kava</trd>,
    <trd>p&ecaron;nkava obecn&aacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Buchfink</trd>,
    <trd>Fink</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chaffinch</trd>,
    <trd>finch</trd>
  </trdgrp>
  <trd lng="es">pinz&oacute;n</trd>
  <trd lng="fr">pinson</trd>
  <trd lng="hu">pinty</trd>
  <trd lng="ja">&#x30A2;&#x30C8;&#x30EA;</trd>
  <trd lng="nl">vink</trd>
  <trd lng="pl">zi&eogonek;ba</trd>
  <trdgrp lng="pt">
    <trd>tentilh&atilde;o</trd>,
    <trd>chupim</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_ja;&c_b;&c_l;&c_i;&c_k;</trd>
  <trd lng="sk">pinka</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x44F;&#x431;&#x43B;&#x438;&#x43A;</trd>,
    <trd>&#x432;&#x2019;&#x44E;&#x440;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>

</art>
