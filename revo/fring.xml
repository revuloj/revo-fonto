<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fring</rad>/o
</kap>
<drv mrk="fring.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fring.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Fringilla</trd>).    
    </dif>
  </snc>
  <trd lng="cs">pa&ncaron;kava</trd>
  <trdgrp lng="de">
    <trd>Buchfink</trd>,
    <trd>Fink</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chaffinch</trd>,
    <trd>finch</trd>
  </trdgrp>
  <trd lng="hu">pinty</trd>
  <trd lng="pl">zi&eogonek;ba</trd>
  <trd lng="ru">&c_z;&c_ja;&c_b;&c_l;&c_i;&c_k;</trd>
</drv>
<drv mrk="fring.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Fringillidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Finken</trd>
</drv>
</art>
