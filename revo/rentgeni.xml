<art mrk="$Id: rentgeni.xml,v 1.6 2024/04/13 20:15:53 revo Exp $">
<kap>
  <rad>rentgeni</rad>/o
</kap>
<drv mrk="rentgeni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Rg</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="111">elemento</ref> kun
      atomnumero 111, atommaso 272 u;
      radioaktiva, rapide disfalanta transirmetalo:
      <ekz>
        la transirmetaloj estas: skandio, titano, vanado, kromo, <klr>[&#x2026;]</klr>, mejtnerio, darm&scirc;tatio, <tld/>o, kaj kopernicio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4279">Transirmetalo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estis unue farita en la jaro 1994
        en Darm&scirc;tato per kunfandi&gcirc;o de bismuto kaj nikelo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34004">Rentgenio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_n;&c_t;&c_g;&c_je;&c_n;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_r;&c_mol;&c_o;&c_n;&c_t;&c_g;&c_je;&c_n;&c_i;&c_j;</trd>
  <trd lng="br">roentgeniom</trd>
  <trd lng="ca">roentgeni</trd>
  <trdgrp lng="de">
    <trd>Roentgenium</trd>,
    <trd>R&ouml;ntgenium</trd>
  </trdgrp>
  <trd lng="en">roentgenium</trd>
  <trd lng="es">roentgenio</trd>
  <trd lng="fr">roentgenium</trd>
  <trd lng="hr">roentgenij</trd>
  <trd lng="hu">r&ouml;ntgenium</trd>
  <trd lng="id">roentgenium</trd>
  <trd lng="it">roentgenio</trd>
  <trd lng="la">roentgenium</trd>
  <trd lng="nl">r&ouml;ntgenium</trd>
  <trd lng="pl">roentgen</trd>
  <trd lng="pt">roentg&ecirc;nio</trd>
  <trd lng="ru">&c_r;&c_je;&c_n;&c_t;&c_g;&c_je;&c_n;&c_i;&c_j;</trd>
  <trd lng="sk">roentgenium</trd>
  <trd lng="sr">&c_r;&c_je;&c_n;&c_d;&c_g;&c_je;&c_n;&c_i;&#x458;&c_u;&c_m;</trd>
  <trd lng="sv">r&ouml;ntgenium</trd>
  <trd lng="uk">&#x440;&#x435;&#x43D;&#x442;&#x491;&#x435;&#x43D;&#x456;&#x439;</trd>
  <trd lng="zh">&#x9300; <pr>l&#xFA;n</pr></trd>
</drv>
</art>

