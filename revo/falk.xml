<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>falk</rad>/o
</kap>
<drv mrk="falk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="falk.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Falco</trd>), taga rabobirdo, dresata
      por &ccirc;asi malgrandajn bestojn.
    </dif>
  </snc>
  <trd lng="de">Falke</trd>
  <trd lng="en">falcon</trd>
</drv>
<drv mrk="falk.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="bird.rabo0oj">rabobirdoj</ref>
      (<trd lng="la">Falconidae</trd>).
    </dif>
  </snc>
</drv>
</art>

<!--
$Log$
