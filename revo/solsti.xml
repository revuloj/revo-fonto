<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>solstic</rad>/o
</kap>
<drv mrk="solsti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Tempo, kiam la suno estas plej malproksime de la ekvatoro:
      <ekz>
        somera, vintra <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sonnenwende</trd>
  <trdgrp lng="nl">
    <trd>solstitium</trd>,
    <trd>dag-en-nachtevening</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_c;&c_je;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="solsti.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj:
      <ekz>
        <tld/>aj punktoj.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
