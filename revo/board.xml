<kap>
  <rad>board</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="board.0i">
  <kap><tld/>i</kap>
  <snc mrk="board.0i.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="vid" cel="zigzag.0i">Zigzage </ref>navigi
      pro la kontra&ubreve;a vento:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        servistoj <klr>...</klr> glitadis inter movi&gcirc;ado de gastoj,
        <tld/>ante kaj transkurante kun lerteco de dancistoj
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/ora.html">
          La ora &ccirc;eno</url></vrk>,
          <lok>2009</lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_v;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>k&#x159;i&#x17E;ovat</trd>,
    <trd>plachtit proti v&#x11B;tru</trd>
  </trdgrp>
  <trd lng="de">kreuzen</trd>
  <trdgrp lng="es">
    <trd>bordear</trd>,
    <trd>dar bordadas</trd>
  </trdgrp>
  <trd lng="fr">louvoyer</trd>
  <trd lng="he">&#x5DC;&#x5EA;&#x5DE;&#x5E7;&#x5DF; &#x5D1;&#x5DE;&#x5E4;&#x5E8;&#x5E9;</trd>
  <trdgrp lng="hu">
    <trd>lav&iacute;roz</trd>,
    <trd>ferd&eacute;z</trd>
  </trdgrp>
  <trd lng="pl">halsowa&cacute;</trd>
  <trd lng="ru">&c_l;&c_a;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kri&#x17E;ova&#x165;</trd>,
    <trd>plachti&#x165; proti vetru</trd>
  </trdgrp>
  <trd lng="uk">&#x43B;&#x430;&#x432;&#x456;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>
