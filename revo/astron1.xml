<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>astronomi</rad>/o
</kap>
<drv mrk="astron1.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      Scienco pri la astroj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Astronomie</trd>,
    <trd>Sternkunde</trd>,
    <trd>Himmelskunde</trd>
  </trdgrp>
  <trd lng="es">astronom&iacute;a</trd>
  <trd lng="fr">astronomie</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ja;</trd>
  <trd lng="sv">astronomi</trd>
</drv>

<drv mrk="astron1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      Specialisto pri <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="astron.0o">astronomo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Astronom</trd>,
    <trd>Sternkundler</trd>,
    <trd>Himmelskundler</trd>
  </trdgrp>
  <trd lng="es">astr&oacute;nomo</trd>
  <trd lng="fr">astronome</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_n;&c_o;&c_m;</trd>
  <trd lng="sv">astronom</trd>
</drv>
</art>
