<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hirund.xml,v 1.38 2025/10/17 10:35:13 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hirund</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="hirund.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hirund.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el la familio <ref tip="malprt" cel="hirund1.0oj"><tld/>edoj</ref>,
      birdo kun pintaj flugiloj kaj longa 12-pluma ofte fendita vosto:
      <ekz>
        turto, <tld/>o, kaj gruo observas la tempon, kiam ili devas veni
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 8:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel <tld/>o forflugas, tiel senka&ubreve;za malbeno ne efektivi&gcirc;as
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 26:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        en a&ubreve;tuno &ccirc;iuj <tld/>oj flugas al la varmaj landoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni klare vidis la frontonon kun la nesto de <tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>unu <tld/>o printempon ne alportas</ind>
        <klr>(oni povas nenion konkludi el ununura ekzemplo)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="pl">jedna jask&oacute;&lstroke;ka wiosny nie
        czyni</trd>
      </ekz>
    </dif>
    <trd lng="bg">&c_l;&c_ja;&c_s;&c_t;&c_o;&c_v;&c_i;&c_c;&c_a;</trd>
    <trd lng="br">gwennili</trd>
    <trdgrp lng="ca">
      <trd>oreneta</trd>,
      <trd>oroneta</trd>,
      <trd>orenola</trd>,
      <trd>oronell</trd>,
      <trd>oronella</trd>
    </trdgrp>
    <trd lng="cs">vla&scaron;tovka</trd>
    <trd lng="de">Schwalbe</trd>
    <trd lng="el">&chi;&epsilon;&lambda;&jota;&delta;&omikron_ton;&ny;&jota;</trd>
    <trd lng="en">swallow</trd>
    <trd lng="es">golondrina</trd>
    <trd lng="fr">hirondelle</trd>
    <trdgrp lng="id">
      <trd>burung layang-layang</trd>,
      <trd>walet</trd>
    </trdgrp>
    <trd lng="nl">zwaluw</trd>
    <trd lng="pl">jask&oacute;&lstroke;ka</trd>
    <trd lng="pt">andorinha</trd>
    <trd lng="ru">&c_l;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>
    <trdgrp lng="zh">
    <trd>&#29141;&#23376; <pr>y&agrave;nzi</pr></trd>,
    <trd>&#40166; <pr>y&#464;</pr></trd>,
    <trd>&#40446; <pr>y&igrave;</pr></trd>,
    <trd>&#29141; <pr>y&agrave;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="hirund.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt" cel="hirund1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Hirundo</trd>).
    </dif>
    <bld
    lok="&WCU;/thumb/9/97/Barn_Swallow_(Hirundo_rustica)_(10).jpg/640px-Barn_Swallow_(Hirundo_rustica)_(10).jpg"
    prm="CC BY-SA 3.0"><tld/>oj (Hirundo rustica), County Cork, Irlando
        <fnt>
          <aut>Ken Billington, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Barn_Swallow_(Hirundo_rustica)_(10).jpg">&WCW;/File:Barn_Swallow_(Hirundo_rustica)_(10).jpg</url></lok>
        </fnt>
    </bld>
  <trd lng="id">Hirundo<klr> (genus)</klr></trd>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_s;&c_t;&c_a;&c_w;&c_k;&c_a;</trd>
  <trd lng="hu">fecske</trd>
  <trd lng="ja">&#12484;&#12496;&#12513;</trd>
  <trd lng="sk">lastovi&ccaron;ka</trd>
  <trd lng="uk">&c_l;&c_a;&c_s;&c_t;&c_ib;&c_v;&c_k;&c_a;</trd>
</drv>

<drv mrk="hirund.nokto0o">
  <kap>nokto<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
    <ref tip="dif" cel="kaprim.0o">kaprimulgo</ref>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_l;&c_ja;&c_k;</trd>
  <trd lng="br">adren</trd>
  <trd lng="cs">lelek</trd>
  <trd lng="de">Nachtschwalbe</trd>
  <trd lng="hu">kecskefej&odblac;</trd>
  <trd lng="id">cabak</trd>
  <trdgrp lng="nl">
    <trd>geitenmelker<klr tip="ind"> (zwaluw)</klr></trd>,
    <trd>nachtzwaluw</trd>
  </trdgrp>
  <trd lng="pl">lelek kozod&oacute;j</trd>
  <trd lng="pt">noitib&oacute;</trd>
  <trd lng="sk">lelek</trd>
  <trd lng="zh">&#22812;&#40441; <pr>y&egrave;y&imacron;ng</pr></trd>
</drv>
</art>
