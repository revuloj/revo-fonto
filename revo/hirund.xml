<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>hirund</rad>/o
</kap>
<drv mrk="hirund.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hirund.0o.KOMUNE">
    <uzo tip="fak">KOMUNE</uzo>
    <dif>
      Iu el la familio <ref tip="malprt" cel="hirund.0edoj"><tld/>edoj</ref>,
      birdo kun longaj flugiloj kaj plej ofte fendita vosto:
      <ekz>
        unu <tld/>o printempon ne alportas<fnt>Z</fnt>
        <klr>(oni povas nenion konkludi el ununura ekzemplo)</klr>.
      </ekz>
    </dif>
    <trd lng="cs">cla&scaron;tovka</trd>
    <trd lng="de">Schwalbe</trd>
    <trd lng="en">swallow</trd>
    <trd lng="hu">fecske</trd>
    <trd lng="pl">jask&oacute;&lstroke;ka</trd>
    <trd lng="ru">&c_l;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="hirund.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super" cel="hirund.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Hirundo</trd>).
    </dif>
  </snc>
</drv>
<drv mrk="hirund.edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="paser.0oformaj">paseroformaj
      birdoj</ref>, kun pintaj flugiloj kaj plata beko
      (<trd lng="la">Hirundinidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Schwalben <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
