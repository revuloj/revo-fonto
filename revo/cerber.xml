<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: cerber.xml,v 1.25 2023/10/31 13:37:03 revo Exp $">
    <kap>
      <rad>Cerber</rad>/o
    </kap>

    <drv mrk="cerber.0o">
      <kap><tld/>o
        <fnt>
          <aut>Vikipedio</aut>
          <vrk><url ref="&Viki;/Cerbero">Cerbero</url></vrk>
        </fnt>
      </kap>
      <snc mrk="cerber.0o.MIT">
        <uzo tip="fak">MIT</uzo>
        <dif>
          Mitologia trikapa <ref tip="super" cel="hund.0o">hundo</ref>,
          gardanta la subteran mondon:
          <ekz>
            li gardas tiun knabinon pli bone ol <tld/>o la inferon.
            <fnt>
              <aut>Henryk Sienkiewicz, trad. Lydia Zamenhof</aut>
              <vrk>Quo Vadis?</vrk>
              <lok>&ccirc;apitro 22a</lok>
            </fnt>
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_C;&c_e;&c_r;&c_b;&c_je;&c_r;</trd>,
          <trd>&c_K;&c_je;&c_r;&c_b;&c_je;&c_r;</trd>
        </trdgrp>
        <trd lng="ca">C&egrave;rber</trd>
        <trdgrp lng="cs">
          <trd>Cerberus</trd>,
          <trd>Kerberos</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Kerberos</trd>,
          <trd>Zerberus</trd>,
          <trd>H&ouml;llenhund</trd>
        </trdgrp>
        <trd lng="el">
      &Kappa;&epsilon_ton;&rho;&beta;&epsilon;&rho;&omikron;&sigma_fina;
        </trd>
        <trd lng="es">Cancerbero</trd>
        <trd lng="fr">Cerb&egrave;re</trd>
        <trdgrp lng="hu">
          <trd>Cerberus</trd>,
          <trd>Kerberosz</trd>
        </trdgrp>
        <trd lng="it">Cerbero</trd>
        <trd lng="pl">Cerber</trd>
        <trd lng="pt">C&eacute;rbero</trd>
        <trdgrp lng="ru">
          <trd>&c_K;&c_je;&c_r;&c_b;&c_je;&c_r;</trd>,
          <trd>&c_C;&c_je;&c_r;&c_b;&c_je;&c_r;</trd>
        </trdgrp>
        <trdgrp lng="sk">
          <trd>Kerberos</trd>,
          <trd>Kerberus</trd>
        </trdgrp>
        <trd lng="zh">&#x72F4; <pr>b&#xEC;</pr></trd>
</snc>
    </drv>

    <drv mrk="cerber.c0o">
      <kap><tld lit="c"/>o</kap>
      <snc mrk="cerber.0o.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Tre severa kaj
          malafabla <ref tip="super" cel="pord.0isto">pordisto</ref>:
          <ekz>
            ekpreninte la kulpilon, la <tld lit="c"/>o saltegis
            mezen de infanoj, kriante:
            &mdash; aro da buboj, al kiu apartenas tiu sageto
            <fnt>
              <aut>Henri Vallienne</aut>:
              <vrk>Kastelo de Prelongo</vrk>.
              <lok>Inko Tyres&ouml;, 2002</lok>.
            </fnt>?
          </ekz>
        </dif>
        <trd lng="be">&c_c;&c_e;&c_r;&c_b;&c_e;&c_r;</trd>
        <trd lng="ca">c&egrave;rber</trd>
        <trd lng="de">Kerberos</trd>
        <trd lng="fr">cerb&egrave;re</trd>
        <trd lng="hu">cerberus</trd>
        <trd lng="it">cerbero</trd>
        <trd lng="pl">cerber</trd>
        <trd lng="pt">c&eacute;rbero</trd>
        <trd lng="ru">&c_c;&c_je;&c_r;&c_b;&c_je;&c_r;</trd>
      </snc>
      <trd lng="uk">&#x426;&#x435;&#x440;&#x431;&#x435;&#x440;</trd>
</drv>
    <adm>
      Nur la unua senco bezonas majusklon. &Ccirc;u necesas enkonduki
      deriva&jcirc;on por la dua senco? <aut>MB</aut>
    </adm>
  </art>
