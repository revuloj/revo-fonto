<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Cerber</rad>/o
    </kap>

    <drv mrk="cerber.0o">
      <kap><tld/>o</kap>
      <snc mrk="cerber.0o.MIT">
        <uzo tip="fak">MIT</uzo>
        <dif>
          Mitologia trikapa hundo, gardanta la subteran mondon.
        </dif>
        <trdgrp lng="de">
          <trd>Kerberos</trd>,
          <trd>Zerberus</trd>,
          <trd>H&ouml;llenhund</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>Cerberus</trd>,
          <trd>Kerberosz</trd>
        </trdgrp>
        <trd lng="el">&Kappa;&epsilon_ton;&rho;&beta;&epsilon;&rho;&omikron;&sigma_fina;</trd>
        <trd lng="pt">c&eacute;rbero</trd>
        <trdgrp lng="ru">
          <trd>&c_K;&c_je;&c_r;&c_b;&c_je;&c_r;</trd>,
          <trd>&c_C;&c_je;&c_r;&c_b;&c_je;&c_r;</trd>
        </trdgrp>
      </snc>

      <snc mrk="cerber.0o.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          <klr>(<tld lit="c"/>o:)</klr> Tre severa kaj
          malafabla pordisto:
          <ekz>
            ekpreninte la kulpilon, la <tld lit="c"/>o saltegis
            mezen de infanoj, kriante:
            &dash; Aro da buboj, al kiu apartenas tiu sageto?
            <fnt>
              <aut>Henri Vallienne</aut>: <vrk>Kastelo de Prelongo</vrk>.
              <lok>Inko Tyres&ouml;, 2002</lok>.
            </fnt>.
          </ekz>
        </dif>
        <trd lng="ru">&c_c;&c_je;&c_r;&c_b;&c_je;&c_r;</trd>
      </snc>
      <trd lng="be">&c_c;&c_e;&c_r;&c_b;&c_e;&c_r;</trd>
      <trd lng="de">Kerberos</trd>
      <trd lng="es">Cancerbero</trd>
      <trd lng="fr">cerb&egrave;re</trd>
      <trd lng="hu">cerberus</trd>
      <trd lng="sk">Kerberos</trd>
    </drv>
    <adm>
      Nur la unua senco bezonas majusklon. &Ccirc;u necesas enkonduki
      deriva&jcirc;on por la dua senco? <aut>MB</aut>
    </adm>
  </art>
