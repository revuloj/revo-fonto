<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: digram.xml,v 1.5 2023/01/31 12:50:23 revo Exp $">
<kap>
  <rad>digram</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="digram.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="digram.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref cel="liter.du0ajxo.TIP">Dulitera&jcirc;o</ref>:
      <ekz>
        la <tld/>oj kaj aliaj grafemoj uzataj de Ralfo
        estas uzataj de na&ubreve; el la 12 plej parolataj lingvoj
        de la mondo
        <fnt>
          <aut>A. Albault</aut>,
          <vrk><url ref="&Sezonoj;2013/05/223Albault/">
          Carlevaro tro facil-anime prognozis</url></vrk>,
          <lok>La Ondo de Esperanto, 2013:4&ndash;5</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="he">&#x5EA;&#x5E8;&#x5E9;&#x5D9;&#x5DD;</trd>
  <trd lng="uk">&c_d;&c_i;&c_g;&c_r;&c_a;&#x43C;&c_a;</trd>
</drv>
</art>

