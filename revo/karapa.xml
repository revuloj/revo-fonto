<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karapa.xml,v 1.16 2022/03/21 17:51:57 revo Exp $">
<kap>
  <rad>karapac</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="karapa.0o">
  <kap><tld/>o</kap>
  <snc mrk="karapa.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Osteca, korneca a&ubreve; kalkeca
      <ref tip="super" cel="sxel.0o">&scirc;elo</ref>,
      kiu &ccirc;irka&ubreve;as la korpon de multaj
      animaloj kiel ekzemple tiun de testudoj a&ubreve; krustacoj;
      <ref tip="sin" cel="kiras.0o.ZOO">kiraso<sncref/></ref>:
      <ekz>
        la <tld/>o de krokodilo estas nedifektebla per kuglo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skvam.0o">skvamo</ref>.
    </refgrp>
    <trd lng="be">&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>
    <trd lng="bg">&c_k;&c_a;&c_r;&c_a;&c_p;&c_a;&c_k;&c_s;</trd>
    <trdgrp lng="ca">
      <trd>closca</trd>,
      <trd>cuirassa</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>R&uuml;ckenschild</trd>,
      <trd>Schild</trd>,
      <trd>R&uuml;ckenpanzer</trd>,
      <trd>Panzer (bei Schildkr&ouml;ten)</trd>,
      <trd>Schale (bei Krebsen)</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>carapace</trd>,
      <trd>upper <ind>shell</ind></trd>
    </trdgrp>
    <trd lng="hu">p&aacute;nc&eacute;l</trd>
    <trd lng="nl">schild<klr tip="ind"> (bijv.v.schildpad)</klr></trd>
    <trdgrp lng="pl">
      <trd>skorupa</trd>,
      <trd>pancerz</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_a;&c_n;&c_c;&c_i;&c_r;&c_mol;</trd>
  </snc>
  <snc mrk="karapa.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rezistemo, morala solido, neinflueblo:
      <ekz>
        ili sendube kredas lin <tld/>e sensenta
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">carapace</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x440;&#x430;&#x43F;&#x430;&#x43A;&#x441;</trd>,
    <trd>&#x43F;&#x430;&#x43D;&#x446;&#x438;&#x440;</trd>,
    <trd>&#x441;&#x43F;&#x438;&#x43D;&#x43D;&#x438;&#x439; &#x449;&#x438;&#x442; (&#x443; &#x447;&#x435;&#x440;&#x435;&#x43F;&#x430;&#x445;)</trd>,
    <trd>&#x448;&#x43A;&#x430;&#x440;&#x430;&#x43B;&#x443;&#x43F;&#x430; (&#x443; &#x440;&#x430;&#x43A;&#x456;&#x432;)</trd>
  </trdgrp>
</drv>
</art>
