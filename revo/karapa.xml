<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karapa.xml,v 1.17 2022/05/28 18:34:47 revo Exp $">
<kap>
  <rad>karapac</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="karapa.0o">
  <kap><tld/>o</kap>
  <snc mrk="karapa.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Osteca, korneca a&ubreve; kalkeca <ref tip="super" cel="sxel.0o">&scirc;elo</ref>,
      kiu &ccirc;irka&ubreve;as la korpon de multaj bestoj kiel ekzemple tiun de testudoj a&ubreve; krustuloj;
      <ref tip="sin" cel="kiras.0o.ZOO">kiraso<sncref/></ref>:
      <ekz>
        la <tld/>o de krokodilo estas nedifektebla per kuglo;
      </ekz>
      <ekz>
        <klr>[&ccirc;e]</klr> Teraj testudoj <klr>[&#8230;]</klr> la brusta <tld/>o havas &ccirc;iam 11 a&ubreve;
        12 &scirc;ildojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4273"
              >Teraj testudoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="krust.0o">krusto</ref>,
      <ref cel="skvam.0o">skvamo</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_a;&c_n;&c_c;&c_y;&c_r;</trd>
    <trd lng="bg">&c_k;&c_a;&c_r;&c_a;&c_p;&c_a;&c_k;&c_s;</trd>
    <trdgrp lng="ca">
      <trd>closca</trd>,
      <trd>cuirassa</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>R&uuml;ckenschild</trd>,
      <trd>Schild</trd>,
      <trd>R&uuml;ckenpanzer</trd>,
      <trd>Panzer (bei Schildkr&ouml;ten)</trd>,
      <trd>Schale (bei Krebsen)</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>carapace</trd>,
      <trd>upper <ind>shell</ind></trd>
    </trdgrp>
    <trd lng="hu">p&aacute;nc&eacute;l</trd>
    <trd lng="nl">schild<klr tip="ind"> (bijv.v.schildpad)</klr></trd>
    <trdgrp lng="pl">
      <trd>skorupa</trd>,
      <trd>pancerz</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_a;&c_n;&c_c;&c_i;&c_r;&c_mol;</trd>
  </snc>
  <snc mrk="karapa.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rezistemo, morala solido, neinflueblo:
      <ekz>
        ili sendube kredas lin <tld/>e sensenta
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">carapace</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_r;&c_a;&c_p;&c_a;&c_k;&c_s;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_c;&c_i;&c_r;</trd>,
    <trd>&c_s;&c_p;&c_i;&c_n;&c_n;&c_i;&c_j; &c_shch;&c_i;&c_t; (&c_u; &c_ch;&c_je;&c_r;&c_je;&c_p;&c_a;&c_h;)</trd>,
    <trd>&c_sh;&c_k;&c_a;&c_r;&c_a;&c_l;&c_u;&c_p;&c_a; (&c_u; &c_r;&c_a;&c_k;&c_ib;&c_v;)</trd>
  </trdgrp>
</drv>
</art>
