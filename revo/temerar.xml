<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>temerar</rad>/a
</kap>
<uzo tip="stl">POE</uzo>
<uzo tip="stl">RAR</uzo>
<drv mrk="temerar.0a">
  <kap><tld/>a</kap>
  <snc>
    <ref tip="dif" cel="tim.mal0a">maltima</ref>
    <ekz>
      vi <klr>...</klr> kondutos brave kaj kura&gcirc;e, ne troa&ubreve;dace kaj
      <tld/>e
      <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 49a, p. 367a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_s;&c_s;&c_t;&c_r;&c_a;&c_sh;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_mol;&c_m;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">temerari <klr>(adj.)</klr></trd>
  <trdgrp lng="cs">
    <trd>nerozvážný</trd>,
    <trd>smělý</trd>,
    <trd>ztřeštěný</trd>
  </trdgrp>
  <trd lng="fr">t&eacute;m&eacute;raire</trd>
  <trdgrp lng="hu">
    <trd>rettenthetetlen</trd>,
    <trd>rend&uuml;letlen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>temer&aacute;rio</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nerozvážny</trd>,
    <trd>pojašený</trd>
  </trdgrp>
</drv>
</art>
