<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: temerar.xml,v 1.13 2022/03/28 12:03:04 revo Exp $">
<kap>
  <rad>temerar</rad>/a
</kap>
<uzo tip="stl">POE</uzo>
<uzo tip="stl">RAR</uzo>
<drv mrk="temerar.0a">
  <kap><tld/>a</kap>
  <snc mrk="temerar.0a.maltima">
    <dif>
      <ref tip="dif" cel="tim.mal0a">Maltima</ref>:
      <ekz>
        vi <klr>[&#x2026;]</klr> kondutos brave kaj kura&gcirc;e, ne troa&ubreve;dace kaj
        <tld/>e
        <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 49a, p. 367a</lok></fnt>;
      </ekz>
      <ekz>
        diri al kurbaj kamparanoj la fajf' difuzi&gcirc;anta <tld/>a: &leftquot;ne estu timaj, fratoj,
        &ccirc;ar kuras mi al fato, triumfu la justico proletara&rightquot;
        <fnt>
          <aut>trad. Pier Luigi Cinquantini</aut>
          <vrk><url ref="http://www.viafabbri43.net/traduzioni.asp?go=La-locomotiva">La
          Lokomotivo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="impuls.0igxema">impulsi&gcirc;ema</ref>,
       <ref cel="drast.0a">drasta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_s;&c_s;&c_t;&c_r;&c_a;&c_sh;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_mol;&c_m;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">temerari <klr>(adj.)</klr></trd>
  <trdgrp lng="en">
    <trd>temerarious</trd>,
    <trd>with temerity</trd>,
    <trd>fearless</trd>,
    <trd>daring</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nerozv&aacute;&zcaron;n&yacute;</trd>,
    <trd>sm&ecaron;l&yacute;</trd>,
    <trd>zt&rcaron;e&scaron;t&ecaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>furchtlos</trd>,
    <trd>r&uuml;cksichtslos</trd>,
    <trd>&uuml;berm&uuml;tig</trd>,
  </trdgrp>
  <trd lng="fr">t&eacute;m&eacute;raire</trd>
  <trdgrp lng="hu">
    <trd>rettenthetetlen</trd>,
    <trd>rend&uuml;letlen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>temer&aacute;rio</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nerozv&aacute;&zcaron;ny</trd>,
    <trd>poja&scaron;en&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43C;&#x456;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x432;&#x430;&#x436;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x445;&#x43E;&#x440;&#x43E;&#x431;&#x440;&#x438;&#x439;</trd>,
    <trd>&#x437;&#x430;&#x432;&#x437;&#x44F;&#x442;&#x438;&#x439;</trd>,
    <trd>&#x440;&#x438;&#x441;&#x43A;&#x43E;&#x432;&#x430;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x431;&#x435;&#x437;&#x440;&#x43E;&#x437;&#x441;&#x443;&#x434;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x440;&#x43E;&#x437;&#x441;&#x443;&#x434;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>
