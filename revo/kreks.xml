<vortaro>
  <art mrk="$Id: kreks.xml,v 1.3 2023/12/03 09:59:00 afido Exp $">
    <kap>
      <rad>kreks</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="kreks.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Unuspecia <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref>
      el familio <ref tip="malprt" cel="raled.0oj">raledoj</ref> (<trd lng="la">Crex crex</trd>),
      kun la&ubreve;ta knara vo&ccirc;o, vivanta en kampoj kaj herbejoj:
      <ekz>
        <tld/>o knaras kaj knaradas fore sur la sekalkampo
        <fnt>
          <aut>August Strindberg, trad. Sten Johansson</aut>
          <vrk><url ref="https://books.google.de/books?id=E8kwIDrjo8cC&amp;lpg=PA50&amp;ots=NNdrhyFuYI&amp;pg=PA50#v=onepage">Insulanoj de Hems&ouml;</url></vrk>
          <lok>p. 50</lok>
        </fnt>;
      </ekz>
      <ekz>
        kukolo estis en la kverkarbaro, kaj la unua <tld/>o ekvo&ccirc;is el la fojnherboj tre
        estreme
        <fnt>
          <aut>Mary Webb, trad. Grace Kirkwood</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0158/0004.php">Veneno Kara</url></vrk>
          <lok>&ccirc;ap. 4: Tor&ccirc;oj kaj rosmarenoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/1b/Crex_crex%2C_Beachy_Head_1.jpg/640px-Crex_crex%2C_Beachy_Head_1.jpg" prm="CC BY 2.0"><tld/>o, Sussex, Britujo
        <fnt>
          <aut>Ron Knight el Seaford, East Sussex, Britujo</aut>
          <lok><url ref="&WCW;/File:Crex_crex,_Beachy_Head_1.jpg">https://commons.wikimedia.org/wiki/File:Crex_crex,_Beachy_Head_1.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Wachtelk&ouml;nig</trd>
    <trd lng="en">corn crake</trd>
    <trd lng="fr">r&acirc;le des gen&ecirc;ts</trd>
    <trdgrp lng="nl">
      <trd>kwartelkoning</trd>,
      <trd>spriet</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_r;&c_o;&c_s;&c_t;&c_je;&c_l;&c_mol;</trd>
  </snc>
  <trdgrp lng="zh">
    <trd>&#x9577;&#x8173;&#x79E7;&#x96DE; <pr>ch&#xE1;ngji&#x1CE;oy&#x101;ngj&#x12B;</pr></trd>,
    <trd>&#x957F;&#x811A;&#x79E7;&#x9E21; <pr>ch&#xE1;ngji&#x1CE;oy&#x101;ngj&#x12B;</pr></trd>
  </trdgrp>
</drv>

  </art>

