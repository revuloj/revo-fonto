<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kadenc</rad>/o
</kap>
<drv mrk="kadenc.0o">
  <kap><tld/>o</kap>
  <snc mrk="kadenc.0o.ofteco">
    <dif>
      Ofteco, la&ubreve; kiu iu fenomeno ripeti&gcirc;as;
      rapideco, la&ubreve; kiu homo a&ubreve; mas&icirc;no devas labori.
    </dif>
    <refgrp tip="vid">
      <ref cel="ritm.0o">ritmo</ref>,
      <ref cel="takt.0o">takto</ref>
    </refgrp>
  </snc>
  <snc mrk="kadenc.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Sinsekvo de akordoj, kiuj kutime signas la finon de frazo.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_d;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Kadenz</trd>,
    <trd>Schlussfall</trd>
  </trdgrp>
  <trd lng="en">cadence</trd>
  <trd lng="es">cadencia</trd>
  <trd lng="fr">cadence</trd>
  <trdgrp lng="hu">
    <trd>kadencia</trd>,
    <trd>z&aacute;rlat</trd>,
    <trd>z&aacute;rad&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">cadans</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
