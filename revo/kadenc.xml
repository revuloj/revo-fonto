<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kadenc</rad>/o
</kap>
<drv mrk="kadenc.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Harmonia kombino de la vortaj sonoj, precipe en prozo:
      <ekz><tld/>igi frazon.</ekz>
    </dif>
    <trd lng="de">Kadenz</trd>
  </snc>
  <uzo tip="fak">MUZ</uzo> 
  <snc num="2">
    <dif>
      Harmonia sinsekvo de taktoj finantaj muzikan frazon. 
    </dif>
    <refgrp tip="sin">
      <ref cel="ritm.0o">ritmo</ref>,
      <ref cel="takt.0o">takto</ref>,
      <ref cel="melodi.0o">melodio</ref>.
    </refgrp>
    <trd lng="de">Schlussfall</trd>
  </snc>
  <trd lng="en">cadence</trd>
  <trd lng="nl">cadans</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
