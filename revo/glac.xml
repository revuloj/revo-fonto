<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>glac</rad>/o</kap>
<drv mrk="glac.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Fandver&scirc;ita travidebla vitro plej &gcirc;enerale polurita sur amba&ubreve;
      surfacoj kaj uzata precipe por speguloj kaj fenestroj.
    </dif>
    <refgrp tip="vid">
      <ref cel="vitr.0o">vitro</ref>,
      <ref cel="vitr.dom0o">domvitro</ref>,
      <ref cel="spegul.0o">spegulo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Spiegelglas</trd>,
    <trd>Schaufensterglas</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>glas<klr tip="ind"> (vensterglas)</klr></trd>,
    <trd>spiegelglas</trd>
  </trdgrp>
  <trd lng="fr">glace<klr tip="ind"> (plaque de verre)</klr></trd>
  <trd lng="ru">&c_z;&c_je;&c_r;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_s;&c_t;&c_je;&c_k;&c_l;&c_o;</trd>
</drv>
</art>
