<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ofer</rad>/i
</kap>
<drv mrk="ofer.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">REL</uzo>
    <dif>
      Fordonaci ion multvaloran sur altaro de dia&jcirc;o, por &gcirc;in honori a&ubreve;
      mildigi:
      <ekz>
        la unuaj homoj <tld/>is nur herbon;
      </ekz>
      <ekz>
        <tld/>i bovon, &scirc;afojn, kolombojn;
      </ekz>
      <ekz>
        Abraham volis <tld/>i sian filon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        al malgrava principo, elmetita pro montro kaj allogo, estas
        <tld/>itaj la principoj plej gravaj<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="konsek.0i">konsekri</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>opfern</trd>,
      <trd>darbringen</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>offeren</trd>,
      <trd>opofferen</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Fordoni multvalora&jcirc;on por servo, uzo, utilo de io a&ubreve; iu; dedi&ccirc;i 1:
      <ekz>
        <tld/>i sian tempon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sian servon, al Esperanto;
      </ekz>
      <ekz>
        <tld/>i iom da pacienco al la afero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i sian vivon al la patrujo;
      </ekz>
      <ekz>
        <tld/>i sumon por la propagando;
      </ekz>
      <ekz>
        multego da laboroj iris <tld/>e <klr>(estis <tld/>itaj)</klr>
        por tiu ideo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        al la afero, kiun mi nun proponas al la publiko, mi <tld/>is miajn
        plej bonajn jarojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">opfern</trd>
    <trdgrp lng="nl">
      <trd>offeren</trd>,
      <trd>opofferen</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Disponigi al iu ion ne petitan; donace prezenti:
      <ekz>
        bonvolu <tld/>i al ni iom da inko<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i monon al amiko;
      </ekz>
      <ekz>
        <tld/>i sian &ccirc;evalon, a&ubreve;tomobilon al invitito;
      </ekz>
      <ekz>
        <tld/>i sian manon al virino, sian brakon al maljunulo;
      </ekz>
      <ekz>
        montru al li dankon, se pli <tld/>i vi al li ne povas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la urbo Bulonjo bonvole <tld/>is gastamon <klr>(gastigon)</klr>
        al nia kongreso<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="liver.0i">liveri</ref>.
    </refgrp>
    <trd lng="nl">aanbieden</trd>
  </snc>
  <trd lng="en">sacrifice</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_r;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_zh;&c_je;&c_r;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_je;&c_s;&c_t;&c_i; &c_v; &c_zh;&c_je;&c_r;&c_t;&c_v;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="ofer.0ado">
  <kap><tld/>(ad)o</kap>
  <dif>
    Ago <tld/>i:
  </dif>
  <snc>
    <ekz>
      Fari <tld/>adon al Baal<fnt>Z</fnt>;
    </ekz>
    <ekz>
      obeado estas pli bona ol <tld/>ado<fnt>Z</fnt>.
    </ekz>
    <trd lng="de">Opfern</trd>
    <trd lng="nl">offer</trd>
  </snc>
  <snc>
    <ekz>
      Multaj suferoj kaj <tld/>oj<fnt>Z</fnt>;
    </ekz>
    <ekz>
      la lernado de iu ajn nacia lingvo postulas grandajn <tld/>ojn
      de tempo kaj mono<fnt>Z</fnt>.
    </ekz>
    <trd lng="nl">opoffering</trd>
  </snc>
  <snc>
    <ekz>
      Mi ne povas akcepti vian <tld/>on, pri kiu tamen mi dankas vin.
    </ekz>
    <trd lng="nl">aanbod</trd>
  </snc>
  <trd lng="ru">&c_zh;&c_je;&c_r;&c_t;&c_v;&c_o;&c_p;&c_r;&c_i;&c_n;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="ofer.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Tio, kion oni <tld/>as.
    </dif>
  </snc>
  <trd lng="de">Opfer</trd>
  <trd lng="nl">offer</trd>
  <trd lng="ru">&c_zh;&c_je;&c_r;&c_t;&c_v;&c_a;</trd>
</drv>

<drv mrk="ofer.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Neprofitema, sinforgesema.
    </dif>
    <trd lng="de">opferbereit</trd>
    <trd lng="nl">opofferingsgezind</trd>
  </snc>
  <snc>
    <dif>
      Sindonema.
    </dif>
    <trd lng="de">opferbereit</trd>
    <trd lng="nl">opofferingsgezind</trd>
  </snc>
  <snc>
    <dif>
      Malavara, donema.
    </dif>
    <trd lng="de">freigiebig</trd>
    <trd lng="nl">vrijgevig</trd>
  </snc>
  <trd lng="ru">&c_g;&c_o;&c_t;&c_o;&c_v;&c_y;&c_j; &c_n;&c_a; &c_zh;&c_je;&c_r;&c_t;&c_v;&c_y;</trd>
</drv>

<drv mrk="ofer.brul0o">
  <kap>brul<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="O"/>o, en kiu la viktimo estis tute bruligata.
    </dif>
  </snc>
  <trd lng="de">Brandopfer</trd>
  <trd lng="nl">brandoffer</trd>
</drv>

<drv mrk="ofer.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Memvole perdi por utilo a&ubreve; bono de iu a&ubreve; io iun a&ubreve; ion tre valoran:
      <ekz>
        for<tld/>i siajn amikojn por sia sendan&gcirc;ereco;
      </ekz>
      <ekz>
        sin for<tld/>i por la publika utilo.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">wegschenken</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_je;&c_r;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="ofer.0bucxi">
  <kap><tld/>bu&ccirc;i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Bu&ccirc;i viktimon por <tld/>o.
    </dif>
  </snc>
  <trd lng="nl">een <ind>offerdier</ind> slachten</trd>
</drv>

<drv mrk="ofer.0doni">
  <kap><tld/>doni</kap>
  <snc>
    <dif>
      <tld lit="O"/>i 3:
      <ekz>
        <tld/>doni bukedon, festenon.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">aanbieden</trd>
</drv>

<drv mrk="ofer.0donaco">
  <kap><tld/>don(ac)o</kap>
  <snc>
    <dif>
      Respekta, honoriga donaco.
    </dif>
    <refgrp tip="sin">
      <ref cel="omagx.0o">oma&gcirc;o</ref>,
      <ref cel="dedicx.0o">dedi&ccirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_je;&c_r;&c_t;&c_v;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

</art>
