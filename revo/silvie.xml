<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: silvie.xml,v 1.15 2023/08/19 21:22:14 revo Exp $">
<kap>
  <rad>silvied</rad>/oj
</kap>
<drv mrk="silvie.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="silvie.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref
      tip="malprt" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Sylviidae</trd>):
      <ekz>
        la Brunflanka &ccirc;etio estas birdospecio dekomence lokita en la grupo de <tld lit="S"/>oj, sed
        lastatempe translokigita kun siaj samgenranoj al nova
        familio de &Ccirc;etiedoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=305560">Brunflanka &ccirc;etio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        fine de la 20a jarcento oni &ccirc;irka&ubreve;kalkulis ke la <tld lit="S"/>oj estas preska&ubreve;
        300 malgrandaj insektovoraj birdospecioj en preska&ubreve; 50 genroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16057">Silviedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_a;&c_s;&c_ja;&c_n;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="cs">p&ecaron;nicovit&iacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Grasm&uuml;cken<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">fauvettes<klr> (famille)</klr></trd>
  <trd lng="hu">posz&aacute;taf&eacute;l&eacute;k</trd>
  <trd lng="it">silvidi</trd>
  <trd lng="nl">zangers van de oude wereld<klr tip="ind">
  (fam.)</klr></trd>
  <trd lng="pl">pokrzewki</trd>
  <trd lng="ru">&c_s;&c_l;&c_a;&c_v;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">penicovit&eacute;</trd>
  <trd lng="sv">s&aring;ngare</trd>
  <trd lng="uk">&c_s;&#x43B;&c_a;&c_v;&#x43A;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>
