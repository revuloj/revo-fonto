<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>per&ccirc;</rad>/o
</kap>
<drv mrk="percx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="perked.0oj">perkedoj</ref>
      (<trd lng="la">Acerina</trd>), malgranda man&gcirc;ebla riverfi&scirc;o
      kun malhelaj makuloj sur la dorso:
      <ekz>la inspektisto <klr>...</klr> veturis en alian lokon ser&ccirc;ante lojtojn, <tld/>ojn kaj kankrojn
      <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 21a, p. 137-138a</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_zh;&c_g;&c_ib;&c_r;</trd>
  <trd lng="de">Kaulbarsch</trd>
  <trdgrp lng="fr">
    <trd>perche goujonni&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">durbincs</trd>
  <trd lng="nl">rivierbaars</trd>
  <trd lng="pt">perca <klr tip="ind">(peixe)</klr></trd>
  <trd lng="ru">&c_jo;&c_r;&c_sh;</trd>
  <trd lng="sv">g&auml;rs</trd>
</drv>
</art>
