<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: percx.xml,v 1.24 2023/09/03 12:45:06 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>per&ccirc;</rad>/o
</kap>

<drv mrk="percx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Malgranda <ref tip="lst" cel="fisx.river0o" lst="voko:fi&scirc;oj">riverfi&scirc;o</ref>
      kun malhelaj makuloj sur la dorso;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="perked.0oj">perkedoj</ref>
      (<trd lng="la">Gymnocephalus</trd>):
      <ekz>la inspektisto <klr>...</klr> veturis en alian lokon
      ser&ccirc;ante lojtojn, <tld/>ojn kaj kankrojn
      <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 21a, p.
      137-138a</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_zh;&c_g;&c_ib;&c_r;</trd>
  <trd lng="bg">&c_o;&c_b;&c_i;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n; &c_b;&c_i;&c_b;&c_a;&c_n;</trd>
  <trd lng="de">Kaulbarsch</trd>
  <trdgrp lng="en">
    <trd>ruff <klr tip="ind">(fish)</klr></trd>,
    <trd>ruffe</trd>
  </trdgrp>
  <trd lng="es">perca</trd>
  <trdgrp lng="fr">
    <trd>perche goujonni&egrave;re</trd>
  </trdgrp>
  <trd lng="hu">durbincs</trd>
  <trd lng="ja">&#x30A2;&#x30BB;&#x30EA;&#x30CA;</trd>
  <trd lng="nl">rivierbaars</trd>
  <trd lng="pl">jazgarz</trd>
  <trd lng="pt">perca <klr tip="ind">(peixe)</klr></trd>
  <trd lng="ro">ghibor&#x21B;</trd>
  <trd lng="ru">&c_jo;&c_r;&c_sh;</trd>
  <trd lng="sv">g&auml;rs</trd>
  <trd lng="uk">&#x439;&#x43E;&#x440;&#x436;</trd>
</drv>
</art>
