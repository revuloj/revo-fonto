<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>klav</rad>/o
</kap>
<drv mrk="klav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tabuleto a&ubreve; butono, kiun oni premas por funkciigi muzikan
      instrumenton a&ubreve; alian aparaton:
      <ekz><tld/>o de piano, de skribma&scirc;ino;</ekz>
      <ekz>ne trafi la &gcirc;ustan <tld/>on <klr>(f. maltrafi)</klr>.</ekz> 
    </dif>
  </snc>
  <trd lng="de">Taste</trd>
  <trd lng="en">key</trd>
  <trd lng="nl">toets</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_v;&c_i;&c_sh;&c_a;</trd>
</drv>
<drv mrk="klav.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Vico da <tld/>oj de instrumento:
      <ekz><tld/>aro de orgeno.</ekz>
    </dif>
  </snc>
  <trd lng="de">Tastatur</trd>
  <trd lng="en">keyboard</trd>
  <trd lng="nl">toetsenbord</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_v;&c_i;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
</drv>
</art>
