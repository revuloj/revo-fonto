<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klav.xml,v 1.30 2022/05/01 08:09:24 revo Exp $">
<kap><ofc>*</ofc>
<rad>klav</rad>/o</kap>
<drv mrk="klav.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Tabuleto a&ubreve; butono, kiun oni premas por funkciigi muzikan
      instrumenton a&ubreve; alian aparaton:
      <ekz>
        <tld/>o de piano, de tajpilo;
      </ekz>
      <ekz>
        <ind>ne trafi la &gcirc;ustan <tld/>on</ind> <klr>(maltrafi)</klr>;
         <trdgrp lng="nl">
          <trd>de <ind>bal</ind> misslaan</trd>,
          <trd>de <ind>plank</ind> misslaan</trd>
        </trdgrp>
      </ekz>
      <ekz>
        muziko ne konsistas en sola frapado de <tld/>oj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni ja bezonas nur ekfrapi unu <tld/>on, kaj estos ricevita
        tono, 
        vi ekfrapos alian <tld/>on kaj vi ricevos alian tonon
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        Marta <klr>[&#x2026;]</klr> ne levante la okulojn nek la manojn de la
        <tld/>oj, komencis ludi la nokturnon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        per <tld/>opremo eblas transigi grandajn monsumojn en sekundero
        de unu al alia mondoparto
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2014/011235.php">Kroata
          ekonomikisto Mladen Stani&ccaron;i&cacute; pri ekonomiaj
          alternativoj</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        sufi&ccirc;us unu a&ubreve; du pliaj <tld/>oj, similaj al tiuj jam
        ekzistantaj &ccirc;e la teleregiloj de dvd-legiloj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010341.php">Kiom da
          homoj, tiom da gustoj</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        re&gcirc;im&scirc;an&gcirc;aj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="&Steloj;monato2/006801.html">Interreto:
          Uniksulo ekspertas Unikodon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_v;&c_ib;&c_sh;&c_a;</trd>
  <trd lng="bg">&c_k;&c_l;&c_a;&c_v;&c_i;&c_sh;</trd>
  <trdgrp lng="cs">
    <trd>kl&aacute;vesa</trd>,
    <trd>tla&ccaron;&iacute;tko</trd>
  </trdgrp>
  <trd lng="de">Taste</trd>
  <trdgrp lng="en">
    <trd>key</trd>,
    <trd>button</trd>
  </trdgrp>
  <trd lng="es">tecla</trd>
  <trd lng="fr">touche<klr tip="ind"> (de clavier)</klr></trd>
  <trd lng="hu">billenty&udblac;</trd>
  <trd lng="nl">toets</trd>
  <trd lng="pl">klawisz</trd>     
  <trd lng="ro">clap&abreve;</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_v;&c_i;&c_sh;&c_a;</trd>
  <trd lng="sk">kl&aacute;ves</trd>
  <trd lng="sv">tangent<klr tip="ind"> (p&aring; klaviatur eller
  tangentbord)</klr></trd>
  <trd lng="tr">tu&scedil;</trd>
  <trd lng="uk">&#x43A;&#x43B;&#x430;&#x432;&#x456;&#x448;&#x430;</trd>
</drv>

<drv mrk="klav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Agi per <tld/>o(j), <ref tip="sin" cel="tajp.0i">tajpi</ref>:
      <ekz>
        tajpu entjeran (dekuman) nombron en la &ccirc;i-supran
        kampon kaj <tld/>u <ctl>Enen</ctl> (&ccirc;aretrevenon).
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_a;&c_c;&c_ib;&c_s;&c_n;&c_u;&c_c;&c_mol; <klr>(&c_k;&c_l;&c_ja;&c_v;&c_ib;&c_sh;&c_u;)</klr></trd>
    <trd lng="bg">&c_n;&c_a;&c_t;&c_i;&c_s;&c_n;&c_a; <ind>&c_k;&c_l;&c_a;&c_v;&c_i;&c_sh;&c_a;</ind></trd>
    <trd lng="de">tippen</trd>
    <trdgrp lng="en">
      <trd>type</trd>,
      <trd>hit <klr>(a key)</klr></trd>
    </trdgrp>
    <trd lng="es">teclear</trd>
    <trd lng="fr">taper<klr tip="ind"> (une touche, un
    num&eacute;ro)</klr></trd>
    <trd lng="hu">bebillenty&udblac;z</trd>    
    <trdgrp lng="nl">
      <trd>intoetsen</trd>,
      <trd>intikken</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_a;&c_zh;&c_a;&c_t;&c_mol;
    <klr>(&c_k;&c_l;&c_a;&c_v;&c_i;&c_sh;&c_u;,
    &c_k;&c_l;&c_a;&c_v;&c_i;&c_sh;&c_i;)</klr></trd>
    <trdgrp lng="sv">
      <trd>trycka<klr> (p&aring; en tangent)</klr></trd>,
      <trd>skriva in<klr> (p&aring; ett tangentbord)</klr></trd>
    </trdgrp>
    <trd lng="tr">tu&scedil;lamak</trd>
  </snc>
  <trd lng="cs">vy&tcaron;uk&aacute;vat <klr>(telefonn&iacute; &ccaron;&iacute;slo)</klr></trd>
  <trdgrp lng="pl">
    <trd>klawiatury u&zdot;ywa&cacute;</trd>,
    <trd>pisa&cacute; na klawiaturze</trd>
  </trdgrp>
  <trd lng="ro">utilizarea tastaturii</trd>
  <trd lng="sk">vy&tcaron;uk&aacute;va&tcaron; <klr>(telef. &ccaron;&iacute;slo)</klr></trd>
</drv>

<drv mrk="klav.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Unu a&ubreve; pluraj vicoj da <tld/>oj de instrumento a&ubreve; aparato:
      <ekz>
        <uzo tip="fak">MUZ</uzo>
        <tld/>aro de orgeno;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> forlasadis tutajn taktojn, haltadis kaj
        ser&ccirc;adis sur la <tld/>aro
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="klav.0aro.KOMP">
      <uzo tip="fak">KOMP</uzo>
      <dif>
        La plej komuna speco de <ref tip="super" cel="en.0igilo">enigilo</ref>
        komputila, kies formo imitas la tajpilan <tld/>aron:
        <ekz>
          esperantigo de komputila <tld/>aro;
        </ekz>
        <ekz>
          interago per <tld/>aro kaj muso
          <fnt>
            <bib>Monato</bib>
            <aut>Franck Arnaud</aut>
            <vrk><url ref="&Monato;/2010/010076.php">Parto
            3: Apple iPad - &ccirc;u revolucio a&ubreve; evolucio?</url></vrk>
            <lok>2010</lok>
          </fnt>;
        </ekz>
        <ekz>
          reagordi la <tld/>aron por uzi britan aran&gcirc;on
          <fnt>
            <bib>Monato</bib>
            <aut>Edmund Grimley Evans</aut>
            <vrk><url ref="&Monato;/2010/010031.php">Knoppix</url></vrk>
            <lok>2010</lok>
          </fnt>;
        </ekz>
        <ekz>
          oni uzas la qwertz-<tld/>aron
          <fnt>
            <bib>Monato</bib>
            <aut>Thierry Salomon</aut>
            <vrk><url ref="&Monato;/2003/008016.html">La
            mondolingvo</url></vrk>
            <lok>2003</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="hu">
        <trd>billenty&udblac;zet</trd>,
        <trd>klaviat&uacute;ra</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_v;&c_ib;&c_ja;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="bg">&c_k;&c_l;&c_a;&c_v;&c_i;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="cs">kl&aacute;vesnice</trd>
  <trd lng="de">Tastatur</trd>
  <trd lng="en">keyboard</trd>
  <trd lng="es">teclado</trd>
  <trd lng="fr">clavier</trd>
  <trdgrp lng="hu">
      <trd>billenty&udblac;zet</trd>,
      <trd>manu&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>klavier</trd>,
    <trd>toetsenbord</trd>
  </trdgrp>
  <trd lng="pl">klawiatura</trd>     
  <trd lng="ro">tastatur&abreve;</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_v;&c_i;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="sk">kl&aacute;vesnica</trd>
  <trdgrp lng="sv">
    <trd>tangentbord</trd>,
    <trd>klaviatur</trd>
  </trdgrp>
  <trd lng="tr">klavye</trd>
  <trd lng="uk">&#x43A;&#x43B;&#x430;&#x432;&#x456;&#x430;&#x442;&#x443;&#x440;&#x430;</trd>
</drv>

<drv mrk="klav.senpasxa0o">
  <kap>senpa&scirc;a <tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      <ref cel="klav.0o"><tld lit="K"/>o</ref> de tajpila a&ubreve; komputila
      klavaro,
      kies premo ne avancas (ne pa&scirc;igas) la &ccirc;areton a&ubreve;
      kursoron,
      kaj kies kutima funkcio estas enigi diakritilon de
      plurelementa signo:
      <ekz>
        <ctl>^</ctl> estas senpa&scirc;a <tld/>o en la enigmetodo
        <ctl>iso-accents-mode</ctl> de Emakso;
      </ekz>
      <ekz>
        uzo de la modifiva <tld/>o Alt Gr anstata&ubreve; la senpa&scirc;a <tld/>o
        maldekstra korno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2402">Klavaro
          Dvorak</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>dead key</trd>,
      <trd>mute key</trd>,
      <trd>accent key</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>touche morte</trd>,
      <trd>touche d'accent</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>z&aacute;rt <ind>billenty&udblac;</ind></trd>,
      <trd>&eacute;kezetbillenty&udblac;</trd>
    </trdgrp>
    <trd lng="nl">dode <ind>toets</ind></trd>
    <trd lng="pl">klawisz &sacute;lepy</trd>     
    <trd lng="ro">orb cheie</trd>
    <trd lng="ru">&c_m;&c_jo;&c_r;&c_t;&c_v;&c_a;&c_ja; &c_k;&c_l;&c_a;&c_v;&c_i;&c_sh;&c_a;</trd>
  </snc>
</drv>
</art>
