<art mrk="$Id: siest.xml,v 1.18 2023/10/21 07:46:47 revo Exp $">
<kap>
  <rad>siest</rad>/o
  <fnt><bib>IK</bib></fnt>
</kap>
<drv mrk="siest.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Posttagman&gcirc;a <ref tip="super" cel="dorm.0o.ripozo">dormo</ref>:
      <ekz>
        en la sekvinta tago, kiam <nom>Wygrycz</nom> en nokta surtuto kaj pantofloj faris sian
        <tld/>on, Franjo estis &ccirc;e la kudrejo kaj Klaro kun la frato sidis &ccirc;e libroj
        kaj kajeroj, iu frapis delikate la pordon
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ib;&c_je;&c_s;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>poledn&iacute; klid</trd>,
    <trd>siesta</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mittagsschlaf</trd>,
    <trd>Siesta</trd>
  </trdgrp>
  <trd lng="en">siesta</trd>
  <trd lng="es">siesta</trd>
  <trd lng="fr">sieste</trd>
  <trd lng="hu">szieszta</trd>
  <trdgrp lng="ja">
    <trd>&#x663C;&#x5BDD;  <pr>&#x3072;&#x308B;&#x306D;</pr></trd>,
    <trd>&#x5348;&#x7761;  <pr>&#x3054;&#x3059;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">si&euml;sta</trd>
  <trdgrp lng="pl">
    <trd>drzemka</trd>,
    <trd>sjesta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_je;&c_s;&c_t;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_l;&c_je;&c_o;&c_b;&c_je;&c_d;&c_je;&c_n;&c_n;&c_y;&c_j; &c_o;&c_t;&c_d;&c_y;&c_h;</trd>
  </trdgrp>
  <trd lng="sk">odpo&ccaron;inok <klr>(odpolud&ncaron;aj&scaron;&iacute;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_ib;&c_jeu;&c_s;&c_t;&c_a;</trd>,
    <trd>&#x43F;&#x43E;&#x43E;&c_b;&c_ib;&c_d;&#x43D;&c_ib;&c_j; &c_s;&#x43E;&#x43D;</trd>
  </trdgrp>
</drv>
</art>
