<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>glim</rad>/o
</kap>
<drv mrk="glim.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Mineralo kun metaleca brilo, kiu facile tavoli&gcirc;as per diafanaj
      lamenetoj:
      <ekz>verda <tld/>o el Siberio, roza <tld/>o el Ameriko.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_sh;&c_ch;&c_a;&c_k;</trd>
  <trd lng="de">Glimmer</trd>
  <trd lng="fr">mica</trd>
  <trdgrp lng="nl">
    <trd>mica</trd>,
    <trd>glimmer</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_ju;&c_d;&c_a;</trd>
</drv>
<drv mrk="glim.0brili">
  <kap><tld/>brili</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Brilegi kiel <tld/>o:
      <ekz>la ne&gcirc;o <tld/>brilas sub la suno.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ib;&c_s;&c_k;&c_a;&c_c;&c_je;&c_c;&c_mol; <klr>(&c_n;&c_ib;&c_b;&c_y; &c_l;&c_u;&c_sh;&c_ch;&c_a;&c_k;)</klr></trd>
  <trd lng="de">schimmern</trd>
  <trdgrp lng="fr">
    <trd>chatoyer</trd>,
    <trd>miroiter</trd></trdgrp> 
  <trdgrp lng="nl">
    <trd>glimmen</trd>,
    <trd>blinken</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_je;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_k;&c_a;&c_k; &c_s;&c_l;&c_ju;&c_d;&c_a;)</klr></trd>
  </trdgrp>
</drv>
</art>
