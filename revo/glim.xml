<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glim.xml,v 1.22 2023/11/11 11:36:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>glim</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="glim.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="glim.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Iu el <ref tip="super" cel="minera.0o">mineralo</ref>j kun metaleca brilo, kiu facile tavoli&gcirc;as per 
      diafanaj lamenetoj:
      <ekz>
        verda <tld/>o el Siberio, roza <tld/>o el Ameriko;
      </ekz>
      <ekz>
        brilantaj sagoj radiis en la vitroanguletoj sub blanka lumo <klr>[&#x2026;]</klr>,
        <tld/>simila glacieto mallevi&gcirc;is sur la subfenestron
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="granit.0o">granito</ref>,
      <ref cel="spat.0o">spato</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_sh;&c_ch;&c_a;&c_k;</trd>
  <trd lng="ca">mica</trd>
  <trd lng="cs">sl&iacute;da</trd>
  <trd lng="de">Glimmer</trd>
  <trd lng="en">mica</trd>
  <trd lng="es">mica</trd>
  <trd lng="fr">mica</trd>
  <trd lng="ja">&#x96F2;&#x6BCD;  <pr>&#x3046;&#x3093;&#x3082;</pr></trd>
  <trdgrp lng="nl">
    <trd>mica</trd>,
    <trd>glimmer</trd>
  </trdgrp>
  <trdgrp lng="pl">
   <trd>mika</trd>, 
   <trd>&lstroke;yszczyk</trd> 
  </trdgrp>
  <trdgrp lng="pt">
   <trd>mica</trd>, 
   <trd>malacacheta</trd> 
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_ju;&c_d;&c_a;</trd>
  <trd lng="sk">s&lcaron;uda</trd>
  <trd lng="uk">&#x441;&#x43B;&#x44E;&#x434;&#x430;</trd>
  <trd lng="zh">&#x4E91;&#x6BCD; <pr>y&#xFA;nm&#x1D4;</pr></trd>
</drv>

<drv mrk="glim.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="glim.0i.LUM">
    <dif>
      <ref tip="super" cel="bril.0i.lumo">Brilegi</ref>
      kiel <tld/>o:
      <ekz>
        suno <tld/>anta tra maldensaj nuboj kvaza&ubreve; el &scirc;pinita silko
        <fnt>
          <bib>Kiso</bib>
        </fnt>;
      </ekz>
      <ekz>
        etaj rubandoj el <tld/>antaj kristalperloj, tio <klr>[&#x2026;]</klr> signifis, ke
        pluvas
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        miloj da <tld/>antaj lumoj de la haveno, de la nubskrapuloj kaj de la hejmecaj
        lo&gcirc;domoj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        liaj malgrandaj kornoj <tld/>is kiel ar&gcirc;ento
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Budhismaj_rakontoj/Re%C4%9Do_Banjan_Cervo">
          Re&gcirc;o Banjan Cervo</url></vrk>,
          <lok>en: Budhismaj rakontoj, 2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun entuziasmete <tld/>aj okuloj <klr>[li]</klr> komentis <klr>[&#x2026;]</klr>
        <fnt>
          <bib>MMa</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_l;&c_ib;&c_s;&c_k;&c_a;&c_c;&c_je;&c_c;&c_mol; 
  <klr>(&c_n;&c_ib;&c_b;&c_y; &c_l;&c_u;&c_sh;&c_ch;&c_a;&c_k;)</klr>
  </trd>
  <trdgrp lng="ca">
    <trd>relluir</trd>,
    <trd>mirallejar</trd>,
    <trd>brillar com la mica</trd>
  </trdgrp>
  <trd lng="cs">t&rcaron;pytit se jako sl&iacute;da</trd>
  <trdgrp lng="de">
    <trd>glitzern</trd>,
    <trd>schimmern</trd>
  </trdgrp>
  <trd lng="en">gleam</trd>
  <trdgrp lng="fr">
    <trd>chatoyer</trd>,
    <trd>miroiter</trd></trdgrp>
  <trd lng="ja">&#x304D;&#x3089;&#x304D;&#x3089;&#x5149;&#x308B;  <pr>&#x304D;&#x3089;&#x304D;&#x3089;&#x3072;&#x304B;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>glimmen</trd>,
    <trd>blinken</trd>
  </trdgrp>
  <trd lng="pl">&lstroke;yska&cacute;</trd>
  <trd lng="pt">brilhar como a mica</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_je;&c_r;&c_k;&c_a;&c_t;&c_mol;</trd>,
    <trd>
    &c_p;&c_je;&c_r;&c_je;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja; 
    <klr>(&c_k;&c_a;&c_k; &c_s;&c_l;&c_ju;&c_d;&c_a;)</klr>
    </trd>
  </trdgrp>
  <trd lng="sk">leskn&uacute;&tcaron; sa <klr>(ako
  s&lcaron;uda)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x432;&#x456;&#x442;&#x438;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x431;&#x43B;&#x438;&#x449;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x43B;&#x438;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x43C;&#x435;&#x440;&#x435;&#x445;&#x442;&#x456;&#x442;&#x438;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6DE1;&#x5149; <pr>d&#xE0;ngu&#x101;ng</pr></trd>,
    <trd>&#x7192;&#x7192; <pr>y&#xED;ngy&#xED;ng</pr></trd>
  </trdgrp>
</drv>
</art>
