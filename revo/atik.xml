<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>atik</rad>/a
</kap>
<drv mrk="atik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta la antikvajn atenanojn:
      <ekz>
        <tld/>a dialekto.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Similanta la puran, elegantan, delikatan stilon a&ubreve; lingvon de la
      atenanoj.
    </dif>
  </snc>
  <snc>
    <dif>
      Similanta la spritan, pikantecan &gcirc;entilecon de la atenanoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="en">Attic</trd>
  <trd lng="fr">attique</trd>
  <trd lng="hu">attikai</trd>
  <trd lng="ru">&c_a;&c_t;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="atik.0eco0ismo">
  <kap><tld/>eco, <tld/>ismo</kap>
  <snc>
    <dif>
      <tld lit="A"/>a modo, dir- a&ubreve; farmaniero.
    </dif>
  </snc>
  <trd lng="en">atticism</trd>
  <trd lng="fr">atticisme</trd>
  <trd lng="hu">atticizmus</trd>
</drv>
</art>
