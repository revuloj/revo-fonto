<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>najl</rad>/o
</kap>
<drv mrk="najl.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Pinta metala bastoneto provizita per pli lar&gcirc;a kapeto kaj 
servanta
       por fiksi objekton, en kiun oni enpu&scirc;as &gcirc;in.
     </dif>
   </snc>                     
   <trd lng="be">&c_c;&c_mol;&c_v;&c_ib;&c_k;</trd>
   <trd lng="ca">clau</trd>
   <trd lng="br">tach</trd>
   <trd lng="de">Nagel</trd>
   <trd lng="el">&kappa;&alfa;&rho;&phi;&jota_ton;</trd>
   <trd lng="es">clavo</trd>
   <trd lng="fr">clou</trd>
   <trd lng="hu">szeg</trd>
   <trd lng="nl">spijker</trd>
   <trd lng="pt">prego</trd>
   <trd lng="ru">&c_g;&c_v;&c_o;&c_z;&c_d;&c_mol;</trd>
   <trd lng="sv">spik</trd>
</drv>

<drv mrk="najl.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Fiksi per <tld/>o:
       <ekz>
         <tld/>i keston;
       </ekz>
       <ekz>
         <tld/>i spegulon &ccirc;e la muro.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_p;&c_r;&c_y;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_c;&c_mol;&c_v;&c_ib;&c_k;&c_o;&c_m;</ind></trd>,
     <trd>&c_p;&c_r;&c_y;&c_c;&c_mol;&c_v;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>                     
   <trd lng="br">tacha&ntilde;</trd>
   <trd lng="ca">clavar</trd>
   <trd lng="de">nageln</trd>
   <trd lng="el">&kappa;&alfa;&rho;&phi;&omega_ton;&ny;&omega;</trd>
   <trd lng="en">nail</trd>      
   <trd lng="es">clavar</trd>
   <trd lng="fr">clouer</trd>
   <trd lng="hu">szegez</trd>
   <trd lng="nl">spijkeren</trd>
   <trd lng="pt">pregar</trd>
   <trd lng="ru">&c_p;&c_r;&c_i;&c_b;&c_i;&c_t;&c_mol; <klr>(&c_g;&c_v;&c_o;&c_z;&c_d;&c_ja;&c_m;&c_i;)</klr></trd>
</drv>

<drv mrk="najl.al0i">
   <kap>al<tld/>i</kap>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Senmovigi kvaza&ubreve; per <tld/>o:
       <ekz>
         surprizo, teruro al<tld/>is lin al la tero;
       </ekz>
       <ekz>
         <ind>al<tld/>ita al la lito</ind> per la malsano;
         <trd lng="br">dalc'het war e wele</trd>
       </ekz>
       <ekz>
         &scirc;i estis kvaza&ubreve; al<tld/>ita al la loko &ccirc;e la vido de la amasigitaj
         juveloj<fnt>Z</fnt>
       </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="forgx">alfor&gcirc;i</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_p;&c_r;&c_y;&c_c;&c_mol;&c_v;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
   <trd lng="de">annageln</trd>
   <trd lng="el">&rho;&jota_ton;&chi;&ny;&omega;</trd>  
   <trd lng="fr">clouer<klr tip="ind"> (fig.)</klr></trd>
   <trd lng="hu">odaszegez</trd>
   <trd lng="nl">vastspijkeren</trd>
   <trd lng="pt">pregar</trd>
   <trd lng="ru">&c_p;&c_r;&c_i;&c_g;&c_v;&c_o;&c_z;&c_d;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="najl.ligno0o">
   <kap>ligno<tld/>o</kap>
   <snc>
     <dif>
       Pinta ligna bastoneto similanta <tld/>on, uzata por &scirc;topi trueton,
       kunigi lignajn pecojn ktp.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_k;&c_a;&c_l;&c_o;&c_k;</trd>,
     <trd>&c_d;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_n;&c_y; <ind>&c_c;&c_mol;&c_v;&c_ib;&c_k;</ind></trd>
   </trdgrp>
   <trd lng="br">ibil</trd>   
   <trd lng="de">Holzstift</trd>  
   <trd lng="fr">cheville</trd>
   <trd lng="hu">faszeg</trd>
   <trd lng="nl">pin</trd>
   <trd lng="pt">pino</trd>
   <trdgrp lng="ru">
     <trd>&c_k;&c_o;&c_l;&c_y;&c_sh;&c_je;&c_k;</trd>,
     <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_ja;&c_n;&c_n;&c_y;&c_j; &c_g;&c_v;&c_o;&c_z;&c_d;&c_mol;</trd>
   </trdgrp>
</drv>
</art>
