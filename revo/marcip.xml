<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>marcipan</rad>/o
</kap>
<drv mrk="marcip.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Kuko farita el pasto kun pistitaj migdaloj kaj sukero:
      <ekz>
        kiu ser&ccirc;as <tld/>on, perdas ofte sian panon<fnt>Z</fnt>
      </ekz>
      <klr>(kiu tro multe deziras, nenion akiras)</klr>.
    </dif>
    <refgrp tip="vid">
      <ref cel="bon.0o">bono</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">massepain</trd>
  <trd lng="nl">marsepein</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_c;&c_i;&c_p;&c_a;&c_n;</trd>
</drv>
</art>
