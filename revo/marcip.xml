  <trd lng="hu">marcip&aacute;n</trd>
  <trd lng="ja">&#x30DE;&#x30B8;&#x30D1;&#x30F3;</trd>
  <trd lng="nl">marsepein</trd>
  <trd lng="pl">marcepan</trd>
  <trd lng="pt">marzip&atilde;</trd>
  <trd lng="ro">mar&#x21B;ipan</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_c;&c_i;&c_p;&c_a;&c_n;</trd>
  <trd lng="uk">&#x43C;&#x430;&#x440;&#x446;&#x438;&#x43F;&#x430;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x674F;&#x4EC1;&#x7CD6; <pr>x&#xEC;ngr&#xE9;nt&#xE1;ng</pr></trd>,
    <trd>&#x674F;&#x4EC1;&#x6CE5; <pr>x&#xEC;ngr&#xE9;nn&#xED;</pr></trd>
  </trdgrp>
</drv>
