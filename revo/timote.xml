<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Timote</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="timote.0o">
      <kap><tld/>o</kap>
      <snc mrk="timote.0o.NOM">
        <uzo tip="fak">NOM</uzo>
        <dif>
          Vira anta&ubreve;nomo grekdevena:
          <ekz>
            tie estis unu dis&ccirc;iplo, nomata <tld/>o, filo de
            kredanta Judino
            kaj de Greka patro
            <fnt><bib>NT</bib>, <lok>&Ago; 16:1</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="de">Timotheus</trd>
      <trd lng="fr">Timoth&eacute;e</trd>
      <trdgrp lng="hu">
        <trd>Tim&oacute;t</trd>,
        <trd>Tim&oacute;teus</trd>
      </trdgrp>
      <trdgrp lng="pt">
      <trd lng="pl">Tymoteusz</trd>
        <trd>Tim&oacute;teo</trd>
      </trdgrp>
      <trd lng="ru">&c_T;&c_i;&c_m;&c_o;&c_f;&c_je;&c_j;</trd>
    </drv>
  </art>

