<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: timote.xml,v 1.13 2024/01/25 09:24:47 revo Exp $">
    <kap>
      <rad>Timote</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="timote.0o">
      <kap><tld/>o</kap>
      <snc mrk="timote.0o.NOM">
        <uzo tip="fak">NOM</uzo>
        <dif>
          Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>
          grekdevena:
          <ekz>
            tie estis unu dis&ccirc;iplo, nomata <tld/>o, filo de
            kredanta Judino
            kaj de Greka patro
            <fnt><bib>NT</bib>, <lok>&Ago; 16:1</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_C;&c_ib;&c_m;&c_o;&c_h;</trd>,
        <trd>&c_C;&c_ib;&c_m;&c_a;&c_f;&c_je;&c_j;</trd>
      </trdgrp>
      <trd lng="ca">Timoteu</trd>
      <trd lng="de">Timotheus</trd>
      <trd lng="fr">Timoth&eacute;e</trd>
      <trdgrp lng="hu">
        <trd>Tim&oacute;t</trd>,
        <trd>Tim&oacute;teus</trd>
      </trdgrp>
      <trd lng="it">Timoteo</trd>
      <trd lng="nl">Timotheus</trd>
      <trd lng="pl">Tymoteusz</trd>
      <trdgrp lng="pt">
        <trd>Tim&oacute;teo</trd>
      </trdgrp>
      <trd lng="ru">&c_T;&c_i;&c_m;&c_o;&c_f;&c_je;&c_j;</trd>
      <trdgrp lng="uk">
    <trd>&c_T;&c_i;&#x43C;&#x43E;&c_f;&c_ib;&c_j;</trd>,
    <trd>&c_T;&c_i;&#x43C;&c_ib;&c_sh;</trd>
  </trdgrp>
</drv>
  </art>

