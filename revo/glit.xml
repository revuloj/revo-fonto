<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>glit</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="glit.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="glit.0i.sursxovigxi">
    <dif>
      Facile sur&scirc;ovi&gcirc;i sur glata a&ubreve; grasa
      supra&jcirc;o:
      <ekz>
        oni <tld/>as kiel sur sapo;
      </ekz>
      <ekz>
        &scirc;tono <tld/>as sur la glacio;
      </ekz>
      <ekz>
        mi lasis min <tld/>i en la kava&jcirc;on;
      </ekz>
      <ekz>
        molekuloj de fluida&jcirc;o facile <tld/>as unu sur alia;
      </ekz>
      <ekz>
        dehakitaj arboj <tld/>is la&ubreve;longe de la deklivo;
      </ekz>
      <ekz>
        fi&scirc;oj <tld/>as en akvo, birdoj en aero;
      </ekz>
      <ekz>
        de<tld/>i de la &ccirc;evalo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>lliscar</trd>,
      <trd>patinar</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>deslizarse</trd>,
      <trd>patinar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cs&uacute;szik</trd>,
      <trd>siklik</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>&sacute;lizga&cacute;</trd>,
      <trd>&sacute;lizga&cacute; si&eogonek;</trd>
    </trdgrp>
  </snc>
  <snc mrk="glit.0i.antauxenigxi">
    <dif>
      Anta&ubreve;eni&gcirc;i sur glata supra&jcirc;o, konservante sian
      ekvilibron:
      <ekz>
        la infanoj <tld/>is gaje sur la nova glacio.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>lliscar</trd>,
      <trd>patinar</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>deslizarse</trd>,
      <trd>patinar</trd>
    </trdgrp>
    <trd lng="hu">cs&uacute;szk&aacute;l</trd>
    <trd lng="pl">&sacute;lizga&cacute; si&eogonek;</trd>
  </snc>
  <snc mrk="glit.0i.fali">
    <dif>
      Perdi la starecon kaj ekvilibron, mar&scirc;ante sur glata
      supra&jcirc;o:
      <ekz>
        mia piedo ek<tld/>is sur la vaksita pargeto, kaj mi falis.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>relliscar</trd>,
      <trd>resquitllar</trd>
    </trdgrp>
    <trd lng="es">resbalar</trd>
    <trd lng="hu">megcs&uacute;szik</trd>
    <trd lng="pl">po&sacute;lizgn&aogonek;&cacute; si&eogonek;</trd>
  </snc>
  <snc mrk="glit.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pasi senbrue, facile, nerimarkite:
      <ekz>
        la servistoj senbrue <tld/>is inter la man&gcirc;antoj;
      </ekz>
      <ekz>
        centoj da bildoj <tld/>is tra liaj pensoj
        <fnt><bib>Fab2</bib>, <lok>Sambuka virineto</lok></fnt>;
      </ekz>
      <ekz>
        senvole <tld/>is miaj okuloj al mia &scirc;ranko;
      </ekz>
      <ekz>
        lian koron tra<tld/>as mallumaj nuboj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <ind>tra<tld/>i</ind>s inter la &ccirc;eestantaro;
        <trdgrp lng="fr">
          <trd>se <ind>glisser</ind><klr tip="ind"> (parmi)</klr></trd>,
          <trd>se <ind>couler</ind><klr tip="ind"> (parmi)</klr></trd>,
          <trd>se <ind>faufiler</ind><klr tip="ind"> (parmi)</klr></trd>
        </trdgrp>
        <trd lng="hu">&aacute;tsiklik</trd>
      </ekz>
      <ekz>
        la pejza&gcirc;o rapide <ind>preter<tld/>i</ind>s anta&ubreve;
        liaj okuloj;
        <trdgrp lng="fr">
          <trd>d&eacute;filer<klr tip="ind"> (se d&eacute;rouler)</klr>
          </trd>,
          <trd>se <ind>d&eacute;rouler</ind><klr tip="ind">
          (d&eacute;filer)</klr></trd>
        </trdgrp>
        <trd lng="hu">tovasiklik</trd>
        <trd lng="nl">voorbijglijden</trd>
      </ekz>
      <ekz>
        ili <tld/>as <klr>(falas)</klr> iom post iom en profundan
        mizeron.
      </ekz>
    </dif>
    <trd lng="hu">siklik</trd>
    <trd lng="pl">prze&sacute;lizgiwa&cacute; si&eogonek;</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="flug.0i">flugi</ref>,
    <ref cel="flu.0i">flui</ref>,
    <ref cel="nagx.0i">na&gcirc;i</ref>,
    <ref cel="port">preterporti&gcirc;i</ref>,
    <ref cel="sxveb.0i">&scirc;vebi</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_g;&c_a;&c_c;&c_je;&c_c;&c_mol;
    </trd>,
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_g;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_w;&c_z;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">gleiten</trd>
  <trd lng="en">glide</trd>
  <trdgrp lng="fr">
    <trd>glisser<klr tip="ind"> (intr., sur qc)</klr></trd>,
    <trd>passer sans bruit</trd>,
    <trd>sombrer (dans)<klr tip="ind"> (s'abandonner)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>glijden</trd>,
    <trd>slippen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>escorregar</trd>,
    <trd>deslizar</trd>,
    <trd>resvalar</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_l;&c_mol;&c_z;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">glida</trd>
  <trdgrp lng="sk">
    <trd>kĺzať<klr>(sa)</klr></trd>,
    <trd>šmýkať sa</trd>
  </trdgrp>
</drv>

<drv mrk="glit.0o">
  <kap><tld/>o</kap>
  <snc mrk="glit.0o.KOMUNE">
    <dif>
      Ago <tld/>i.
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_g;&c_a;&c_n;&c_mol;&c_n;&c_je;
      </trd>,
      <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_g;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_k;&c_o;&c_w;&c_z;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>lliscament</trd>,
      <trd>patinatge</trd>
    </trdgrp>
    <trd lng="es">patinaje</trd>
    <trd lng="fr">glissement</trd>
    <trd lng="hu">cs&uacute;sz&aacute;s</trd>
    <trd lng="nl">het <ind>glijden</ind></trd>
    <trdgrp lng="pl">
      <trd>po&sacute;lizg</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>deslize</trd>,
      <trd>deslizamento</trd>,
      <trd>escorregadela</trd>,
      <trd>escorregadura</trd>,
      <trd>escorregamento</trd>,
      <trd>escorreg&atilde;o</trd>,
      <trd>escorrego</trd>,
      <trd>resvalo</trd>,
      <trd>resvalamento</trd>,
      <trd>resvaladura</trd>
    </trdgrp>
    <trd lng="ru">
    &c_s;&c_k;&c_o;&c_l;&c_mol;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="glit.0o.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      Preska&ubreve; neperceptebla transiro inter du sinsekvaj
      distingeblaj pozicioj de la vo&ccirc;organoj.
    </dif>
    <trd lng="de">Glissando</trd>
    <trd lng="hu">glissando</trd>
  </snc>
  <snc mrk="glit.0o.licenco">
    <uzo tip="fak">POE</uzo>
    <dif>
      Metrika <ref tip="super" cel="licenc1.0o">licenco</ref>, pro kiu
      oni toleras anstata&ubreve;igon de komenca
      <ref cel="jamb.0o">jambo</ref>
      per <ref cel="troke.0o">trokeo</ref>:
      <ekz>
        <ctl>s&iacute;gno de k&oacute;r' sov&aacute;&gcirc;a kaj
        senbr&iacute;da</ctl><fnt>Z</fnt>
        <klr>(en <ref cel="jamb.n0o">kvinjambo</ref>)</klr>;
      </ekz>
      <ekz>
        kelkaj poetoj volis e&ccirc; iri plue: ili provis duoblan
        <tld/>on, t.e. la trokeigon de la du unuaj piedoj
        <fnt><bib>PGl</bib>, <lok>p. 23</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="sk">
    <trd>kĺzanie</trd>,
    <trd>šmyk</trd>,
    <trd>šmýkanie</trd>
  </trdgrp>
</drv>

<drv mrk="glit.0a">
  <kap><tld/>a, <var><kap><tld/>iga</kap></var></kap>
  <snc mrk="glit.0a.glata">
    <dif>
      Tiel glata a&ubreve; grasa, ke oni <tld/>as mar&scirc;ante
      sur &gcirc;i a&ubreve; ekkaptante &gcirc;in:
      <ekz>
        <tld/>a pargeto, angilo;
      </ekz>
      <ekz>
        okazon kaptu &ccirc;e l' kapo, &ccirc;ar la vosto estas
        <tld/>a<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>iga substanco.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>rellisc&oacute;s</trd>,
      <trd>lliscad&iacute;s</trd>
    </trdgrp>
    <trd lng="es">resbaladizo</trd>
    <trd lng="pl">&sacute;liski</trd>
  </snc>
  <snc mrk="glit.0a.evitema">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Evitema, elturni&gcirc;a, malfacile kaptebla:
      <ekz>
        spiritoj <tld/>igaj, kiel serpenta korpo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        angile <tld/>aj argumentoj.
      </ekz>
    </dif>
    <trd lng="pl">&sacute;liski</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_k;&c_ib;</trd>,
    <trd>&c_k;&c_o;&c_w;&c_z;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>glatt</trd>,
    <trd>rutschig</trd>,
    <trd>schmierig</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>glissant</trd>,
    <trd>insaisissable<klr tip="ind"> (fuyant)</klr></trd>,
    <trd>fuyant<klr tip="ind"> (insaisissable)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cs&uacute;sz&oacute;s</trd>,
    <trd>s&iacute;kos</trd>
  </trdgrp>
  <trd lng="nl">glad</trd>
  <trd lng="pt">escorregadio</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_l;&c_mol;&c_z;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>klzký</trd>,
    <trd>šmykľavý</trd>
  </trdgrp>
  <trd lng="sv">hal</trd>
</drv>

<drv mrk="glit.0eglate">
  <kap><tld/>e-glate, <var><kap><tld/>e kaj glate</kap></var></kap>
  <snc mrk="glit.0eglate.KOMUNE">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Tre <ref tip="vid" cel="facil.0e">facile</ref>, sen ajna
      &gcirc;eno:
      <ekz>
        la kontakto kun la gravaj sinjoroj efektivi&gcirc;is <tld/>e kaj
        glate
        <fnt><bib>KielAkvo</bib>, <lok>1:3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>les doigts dans le nez</trd>,
    <trd>sans anicroche</trd>
    <trd>superfacile</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>jak po ma&sacute;le</trd>,
    <trd>g&lstroke;adko</trd>,
    <trd>lekko</trd>
  </trdgrp>
</drv>

<drv mrk="glit.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="glit.0ado.KOMUNE">
    <dif>
      Ago a&ubreve; stato de <tld/>anto:
      <ekz>
        <tld/>ado de sportistoj;
      </ekz>
      <ekz>
        fali pro <tld/>ado.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_g;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_g;&c_a;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_k;&c_o;&c_w;&c_z;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
  <trd lng="fr">glissade</trd>
  <trdgrp lng="hu">
      <trd>cs&uacute;sz&aacute;s</trd>,
      <trd>sikl&aacute;s</trd>,
      <trd>cs&uacute;szk&aacute;l&aacute;s</trd>
    </trdgrp>
  <trd lng="nl">het <ind>glijden</ind></trd>
  <trdgrp lng="pl">
      <trd>&sacute;lizg</trd>,
      <trd>po&sacute;lizg</trd>
    </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_l;&c_mol;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>kĺzanie</trd>,
    <trd>šmyk</trd>,
    <trd>šmýkanie</trd>
  </trdgrp>
</drv>

<drv mrk="glit.0emigi">
  <kap><tld/>emigi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="glit.0emigi.KOMUNE">
    <ref tip="dif" cel="lubrik.0i">lubriki</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_m;&c_a;&c_z;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_m;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_m;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_sh;&c_m;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_sh;&c_m;&c_a;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lubrifier</trd>,
    <trd>graisser</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megken</trd>,
    <trd>beolajoz</trd>
  </trdgrp>
  <trd lng="pl">smarowa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_m;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="glit.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="glit.0ilo.ilo">
    <dif>
      &Ccirc;ia ilo <tld/>anta, servanta por <tld/>i a&ubreve;
      <tld/>igi, kaj pli speciale:
    </dif>
    <subsnc mrk="glit.0ilo.veturilo">
      <dif>
        <tld/>veturilo;
      </dif>
      <trdgrp lng="be">
        <trd>&c_s;&c_a;&c_n;&c_k;&c_ib;</trd>,
        <trd>&c_s;&c_a;&c_n;&c_a;&c_ch;&c_k;&c_ib;</trd>
      </trdgrp>
      <trd lng="de">Schlitten</trd>
      <trdgrp lng="hu">
        <trd>sz&aacute;n</trd>,
        <trd>sz&aacute;nk&oacute;</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>slee</trd>,
        <trd>slede</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>sanie</trd>
      </trdgrp>
      <trd lng="pt">tren&oacute;</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_a;&c_n;&c_i;</trd>,
        <trd>&c_s;&c_a;&c_n;&c_k;&c_i;</trd>,
        <trd>&c_s;&c_a;&c_l;&c_a;&c_z;&c_k;&c_i;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="glit.0ilo.sxuo">
      <dif>
        <ref tip="dif" cel="sxu.glit0o"><tld/>&scirc;uo</ref>:
        <ekz>
          hodia&ubreve; estas bela frosta vetero, tial mi prenos miajn
          <tld/>ilojn kaj iros <tld/>i
          <fnt><bib>F</bib>, <lok>&FE; 32</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_k;&c_a;&c_n;&c_jo;&c_k;</trd>
      <trd lng="ca">pat&iacute;</trd>
      <trd lng="de">Schlittschuh</trd>
      <trd lng="es">pat&iacute;n</trd>
      <trd lng="hu">korcsolya</trd>
      <trdgrp lng="nl">
        <trd>schaats</trd>,
        <trd>ijsschaats</trd>
      </trdgrp>
      <trd lng="pl">&lstroke;y&zdot;wa</trd>
      <trd lng="pt">patim</trd>
      <trd lng="ru">&c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
    </subsnc>
  </snc>
  <adm>
    La unua subsenco estas dubinda, kaj la dua ne estas
    absoluta sinonimo de ~sxuo. <aut>MB</aut>
  </adm>
  <trd lng="fr">patin</trd>
  <trdgrp lng="sk">
    <trd>klzná plocha</trd>,
    <trd>sanica</trd>,
    <trd>sklznica</trd>
  </trdgrp>
</drv>

<drv mrk="glit.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="glit.de0i.KOMUNE">
    <dif>
      Malproksimi&gcirc;i de io <tld/>ante, malsupreniri <tld/>ante:
      <ekz>
        &scirc;i de<tld/>is de la tirkesto rekte sur la plankon
        <fnt><bib>Fab1</bib>,
        <lok>Floroj de la malgranda Ida</lok></fnt>;
      </ekz>
      <ekz>
        miaj fingroj kaptis la randon: mi penis teni min,
        sed miaj fingroj de<tld/>is, kaj mi refalegis
        <fnt><bib>&Scirc;talrato</bib>,
        <lok>&ccirc;apitro 4</lok></fnt>;
      </ekz>
      <ekz>
        kvankam la viro aspektis maljuna, li de<tld/>is de la kamelo
        kun rimarkinda facilmoveco
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 6</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[la suno]</klr> plu de<tld/>is malsupren longigante la
        ombrojn
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 16</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_n;&c_u;&c_c;&c_mol;</trd>
  <trd lng="fr">glisser<klr tip="ind"> (intr., de qc)</klr></trd>
  <trdgrp lng="pl">
    <trd>ze&sacute;lizgiwa&cacute; si&eogonek;</trd>,
    <trd>zje&zdot;d&zdot;a&cacute;</trd>
  </trdgrp>
  <trd lng="sk">pošmyknúť sa</trd>
</drv>

<drv mrk="glit.de0ilo">
  <kap>de<tld/>ilo</kap>
  <snc mrk="glit.de0ilo.tobogano">
    <uzo tip="fak">SPO</uzo>
    <uzo tip="stl">EVI</uzo>
      <ref tip="dif" cel="tobogan.0o.SPO">tobogano</ref>
      <ekz>
      Deglitilo estas infana atrakcio trovi&gcirc;anta plej ofte en 
      infanludejoj, &ccirc;e basenoj, naturaj akvaj areoj destinitaj por
      bani&gcirc;ado, nuntempe anka&ubreve; en infananguloj de
      magazencentrejoj kaj en infan&ccirc;ambroj.
      <fnt>
        <vrk>
          <url ref="http://eo.wikipedia.org/wiki/Deglitilo">Vikipedio
          </url>
        </vrk>,
        <bib>Viki</bib>
     </fnt>
     </ekz>
  </snc>
  <adm>
    Trovebla nur &ccirc;e Vikipedio. &Ccirc;ie alie uzatas deglitejo
    a&ubreve; preferinda tobogano.
    <aut>SC</aut>
  </adm>
  <trd lng="fr">tobogan</trd>
</drv>

<drv mrk="glit.de0ejo">
  <kap>de<tld/>ejo</kap>
  <snc mrk="glit.de0ejo.tobogano">
    <uzo tip="fak">SPO</uzo>
      <ref tip="dif" cel="tobogan.0o.SPO">tobogano</ref>
      <ekz>
      12. tobogano, deglitejo
      <fnt>
        <vrk>
       <url ref="http://eo.lernu.net/lernado/vortoj/bildvortaro/84.php">
          Lernu! - Bildvortaro</url>
        </vrk>,
        <lok>&ccirc;. 84, Urbaj parkoj</lok>
     </fnt>
     </ekz>
  </snc>
  <trd lng="fr">tobogan</trd>
  <trd lng="sk">nájazd <klr>(lyžiarsky)</klr></trd>
</drv>

<drv mrk="glit.el0i">
  <kap>el<tld/>i<fnt>Z</fnt>,
<var><kap>for<tld/>i<fnt>Z</fnt></kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="glit.el0i.glitanta">
    <dif>
      Fori&gcirc;i <tld/>ante:
      <ekz>
        fi&scirc;o el<tld/>as el mano;
      </ekz>
      <ekz>
        <ind>el lia mano &ccirc;iu monero el<tld/>as</ind>
        <klr>(li estas tute mal&scirc;parema)</klr>.
        <trd lng="fr">l'<ind>argent</ind> lui coule entre les
        doigts</trd><fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>wy&sacute;lizgiwa&cacute; si&eogonek;</trd>
    </trdgrp>
    <trd lng="pt">sair deslizando</trd>
    <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_s;&c_k;&c_o;&c_l;&c_mol;&c_z;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_k;&c_a;&c_l;&c_mol;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="glit.el0i.nerimarkite">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fori&gcirc;i nerimarkite, ellasi&gcirc;i:
      <ekz>
        el<tld/>i el domo, el &ccirc;ambro;
      </ekz>
      <ekz>
        tiu vorto senvole el<tld/>is el lia bu&scirc;o;
      </ekz>
      <ekz>
        el<tld/>i&gcirc;is el sub liaj piedoj la fundamento de lia
        materiala ekzistado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la povo for<tld/>is el liaj manoj;
      </ekz>
      <ekz>
        el sub la &scirc;ajno de &gcirc;entileco el<tld/>is
        <klr>(aperis, elvidi&gcirc;is)</klr>
        &ccirc;iumomente ilia fiero kaj mal&scirc;ato
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 6</lok></fnt>;
      </ekz>
      <ekz>
        tiu demando el<tld/>is la registarojn <klr>(la registaroj ne
        atentis
        tiun demandon)</klr>.
      </ekz>
    </dif>
    <adm>
      La ekz-o kun &leftquot;el~igxi&rightquot; meritus klarigon pri la
      sencnuanco disde el~i. La lasta ekz havas strangan konstruon:
      el~i iun = el~i el ies atento. Indus trovi aliajn ekz-ojn kaj krei
      apartan sencon.
      <aut>MB</aut>
    </adm>
    <trdgrp lng="de">
      <trd>entgleiten</trd>,
      <trd>entwischen</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>wegschuiven</trd>,
      <trd>wegglijden</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wy&sacute;lizgiwa&cacute; si&eogonek;</trd>,
      <trd>wymyka&cacute; si&eogonek;</trd>
    </trdgrp>
    <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_k;&c_o;&c_l;&c_mol;&c_z;&c_n;&c_u;&c_t;&c_mol;
    </trd>,
    <trd>&c_u;&c_s;&c_k;&c_o;&c_l;&c_mol;&c_z;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
  <trd>&c_v;&c_y;&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>,
  <trd>&c_v;&c_y;&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'&eacute;chapper<klr tip="ind"> (en se glissant hors de)</klr>
    </trd>,
    <trd>esquiver<klr tip="ind"> (en s'&eacute;chappant)</klr></trd>,
    <trd>glisser<klr tip="ind"> (des mains, etc.)</klr></trd>,
    <trd>tomber<klr tip="ind"> (des mains, etc.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kics&uacute;szik</trd>,
    <trd>kisiklik</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vykĺznuť</trd>,
    <trd>vyšmyknúť sa</trd>
  </trdgrp>
</drv>

<drv mrk="glit.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="glit.en0i.KOMUNE">
    <dif>
      Eniri <tld/>ante:
      <ekz>
        &ccirc;iuj ekiris al la truo kaj singarde en<tld/>is tra
        &gcirc;i
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 7</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la polvo en<tld/>as en keston
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 10</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        diversaj malordoj en<tld/>is en mian kortegon
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 6</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_p;&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_n;&c_u;&c_c;&c_mol;
  </trd>
  <trdgrp lng="fr">
    <trd>se <ind>glisser</ind><klr tip="ind"> (dans)</klr></trd>,
    <trd>se <ind>couler</ind><klr tip="ind"> (dans)</klr></trd>,
    <trd>se <ind>faufiler</ind><klr tip="ind"> (dans)</klr></trd>,
    <trd>s'<ind>insinuer</ind></trd>,
    <trd>s'<ind>introduire</ind></trd>
  </trdgrp>
  <trd lng="hu">becs&uacute;szik</trd>
  <trdgrp lng="pl">
    <trd>w&sacute;lizgiwa&cacute; si&eogonek;</trd>,
    <trd>wkrada&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="pt">entrar deslizando</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_k;&c_o;&c_l;&c_mol;&c_z;&c_n;&c_u;&c_t;&c_mol;
  </trd>
  <trd lng="sk">vkĺznuť</trd>
  <trd lng="sv">halka in i</trd>
</drv>

<drv mrk="glit.0fali">
  <kap><tld/>fali</kap>
  <snc mrk="glit.0fali.KOMUNE">
    <dif>
      <tld lit="G"/>i 3.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>
    &c_p;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_n;&c_u;&c_c;&c_c;&c_a;
    </trd>,
    <trd>&c_p;&c_a;&c_k;&c_a;&c_w;&c_z;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">ausgleiten</trd>
  <trd lng="fr">tomber<klr tip="ind"> (&agrave; la suite d'une
  glissade)</klr></trd>
  <trd lng="hu">elcs&uacute;szik</trd>
  <trd lng="nl">uitglijden</trd>
  <trd lng="pl">po&sacute;lizgn&aogonek;&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_k;&c_o;&c_l;&c_mol;&c_z;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">padnúť pokĺznutím</trd>
</drv>

<drv mrk="glit.0kuri">
  <kap><tld/>kuri<fnt>Z</fnt>,
<var><kap><tld/>umi<fnt>Z</fnt></kap></var></kap>
  <snc mrk="glit.0kuri.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <tld lit="G"/>i 2 por amuzi&gcirc;i, fizike ekzerci&gcirc;i.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_g;&c_ch;&c_y;
    <klr tip="amb">(&c_n;&c_a; &c_k;&c_a;&c_n;&c_mol;&c_k;&c_o;&c_h;)
    </klr></trd>,
    <trd>&c_k;&c_a;&c_t;&c_a;&c_c;&c_c;&c_a;
    <klr tip="amb">(&c_n;&c_a; &c_k;&c_a;&c_n;&c_mol;&c_k;&c_o;&c_h;)
    </klr></trd>,
    <trd>&c_k;&c_o;&c_w;&c_z;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Schlittschuh laufen</trd>
  <trdgrp lng="fr">
    <trd>patiner<klr tip="ind"> (en patins &agrave; glace)</klr></trd>,
    <trd>faire du <ind>patin</ind><klr tip="ind"> (&agrave; glace)</klr>
    </trd>
  </trdgrp>
  <trd lng="hu">korcsoly&aacute;zik</trd>
  <trd lng="nl">schaatsen</trd>
  <trdgrp lng="pl">
    <trd>je&zacute;dzi&cacute; na &lstroke;y&zdot;wach</trd>
  </trdgrp>
  <trd lng="sk">korčuľovať sa</trd>
</drv>

<drv mrk="glit.0punkta.koma">
  <kap><tld/>punkta, <var><kap><tld/>koma</kap></var></kap>
  <snc mrk="glit.0punkta.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Rilata al la maniero prezenti nombron en la formo
      <frm>M&#215;b<sup>E</sup></frm>, kie
      <frm>b</frm> estas la bazo de pozicia nombrosistemo;
      <frm>M</frm> estas la
      <ref tip="prt" cel="mantis.0o.glitpunkta">mantiso</ref>
      (<frm>b</frm>-uma frakcio)
      kaj <frm>E</frm> estas la
      <ref tip="prt" cel="ekspon2.0o.glitpunkta">eksponento</ref>.
      <ekz>
        12.3&#215;10<sup>1</sup>, 1.23&#215;10<sup>2</sup>,
        0.123&#215;10<sup>3</sup> estas egalaj nombroprezentoj
        <tld/>punktaj.
      </ekz>
    </dif>
    <ref tip="malprt" cel="frakci.pozicia0o">poziciaj frakcioj</ref>
    <ref tip="malprt" cel="reel.0o.KOMP">reeloj</ref>
    <ref tip="vid" cel="fiks.0punkta.koma">fikspunkta, fikskoma</ref>
  </snc>
  <trd lng="de">Gleitkomma-</trd>
  <trd lng="en">floating-point</trd>
  <trd lng="fr">&agrave; <ind>virgule</ind> flottante</trd>
  <trd lng="hu">lebeg&odblac;pontos</trd>
  <trd lng="pl">zmiennoprzecinkowy</trd>
  <trd lng="ru">&c_s; <ind>&c_p;&c_l;&c_a;&c_v;&c_a;&c_ju;&c_shch;&c_je;&c_j;</ind> &c_t;&c_o;&c_ch;&c_k;&c_o;&c_j;
  <klr>(&c_i;&c_l;&c_i; &c_z;&c_a;&c_p;&c_ja;&c_t;&c_o;&c_j;)</klr></trd>
  <trd lng="sk">s pohyblivou desatinnou čiarkou</trd>
  <trd lng="sv">flyttals-</trd>
</drv>
