<drv mrk="glit.0a">
  <kap><tld/>a</kap>
  <snc mrk="glit.0a.glata">
    <dif>
      Tia, ke &gcirc;i facile <tld/>as:
      <ekz>
        <tld/>a angilo;
      </ekz>
      <ekz>
        okazon kaptu &ccirc;e l' kapo, &ccirc;ar la vosto estas
        <tld/>a<fnt><bib>PrV</bib></fnt>;
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>rellisc&oacute;s</trd>,
      <trd>lliscad&iacute;s</trd>
    </trdgrp>
    <trd lng="es">resbaladizo</trd>
    <trd lng="pl">&sacute;liski</trd>
  </snc>
  <snc mrk="glit.0a.evitema">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Evitema:
      <ekz>
        angile <tld/>aj argumentoj.
      </ekz>
    </dif>
    <trd lng="pl">&sacute;liski</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_k;&c_ib;</trd>,
    <trd>&c_k;&c_o;&c_w;&c_z;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="cs">kluzk&yacute;</trd>
  <trdgrp lng="de">
    <trd>glatt</trd>,
    <trd>rutschig</trd>,
    <trd>schmierig</trd>
  </trdgrp>
  <trd lng="en">slippery</trd>
  <trdgrp lng="fr">
    <trd>glissant</trd>,
    <trd>insaisissable<klr tip="ind"> (fuyant)</klr></trd>,
    <trd>fuyant<klr tip="ind"> (insaisissable)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cs&uacute;sz&oacute;s</trd>,
    <trd>s&iacute;kos</trd>
  </trdgrp>
  <trd lng="nl">glad</trd>
  <trd lng="pt">escorregadio</trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_l;&c_mol;&c_z;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>klzk&yacute;</trd>,
    <trd>&scaron;myk&lcaron;av&yacute;</trd>
  </trdgrp>
  <trd lng="sv">hal</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_l;&c_i;&c_z;&c_mol;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_v;&c_z;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="glit.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="glit.0iga.glata">
    <dif>
      Tia, ke oni facile <tld/>as pro &gcirc;i:
      <ekz>
        <tld/>a pargeto, &scirc;oseo;
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>rellisc&oacute;s</trd>,
      <trd>lliscad&iacute;s</trd>
    </trdgrp>
    <trd lng="es">resbaladizo</trd>
    <trd lng="pl">&sacute;liski</trd>
  </snc>
  <snc mrk="glit.0iga.fig">
    <uzo tip="stl">FIG</uzo>
    <dif>
     malfacile kaptebla, prezentanta riskojn de mispa&scirc;o:
      <ekz>
        spiritoj <tld/>igaj, kiel serpenta korpo<fnt>Z</fnt>;
      </ekz>
      <ekz>
         glitiga temo, demando.
      </ekz>
    </dif>
    <trd lng="pl">&sacute;liski</trd>
  </snc>
  <trdgrp lng="be">
