<drv mrk="glit.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="glit.en0i.KOMUNE">
    <dif>
      Eniri <tld/>ante:
      <ekz>
        &ccirc;iuj ekiris al la truo kaj singarde en<tld/>is tra
        &gcirc;i
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 7a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la polvo en<tld/>as en keston
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 10a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        diversaj malordoj en<tld/>is en mian kortegon
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 6a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_p;&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_n;&c_u;&c_c;&c_mol;
  </trd>
  <trdgrp lng="cs">
    <trd>lehce vsunout</trd>,
    <trd>vklouznout</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>slide in</trd>,
    <trd>glide in</trd>,
    <trd>slip in</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>glisser</ind><klr tip="ind"> (dans)</klr></trd>,
    <trd>se <ind>couler</ind><klr tip="ind"> (dans)</klr></trd>,
    <trd>se <ind>faufiler</ind><klr tip="ind"> (dans)</klr></trd>,
    <trd>s'<ind>insinuer</ind></trd>,
    <trd>s'<ind>introduire</ind></trd>
  </trdgrp>
  <trd lng="hu">becs&uacute;szik</trd>
  <trdgrp lng="pl">
    <trd>w&sacute;lizgiwa&cacute; si&eogonek;</trd>,
    <trd>wkrada&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="pt">entrar deslizando</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_k;&c_o;&c_l;&c_mol;&c_z;&c_n;&c_u;&c_t;&c_mol;
  </trd>
  <trd lng="sk">vk&lacute;znu&tcaron;</trd>
  <trd lng="sv">halka in i</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_k;&c_o;&c_v;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_l;&c_i;&c_z;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="glit.ter0o">
  <kap>
    ter<tld/>o
  </kap>
  <snc mrk="glit.ter0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Glita movi&gcirc;o de granda teramaso la&ubreve; deklivo:
      <ekz>
        la tertremo anka&ubreve; ka&ubreve;zis ter<tld/>ojn en la dense arbokovrita
        <nom>Pic Macaya</nom>, proksima nacia parko
        <fnt>
          <aut>E. Lezis, trad. D. Lopez-Rueda</aut>
          <vrk><url ref="https://eo.globalvoices.org/2021/09/11892/">Kiel haitia bakejo uzas
          panarbon por kreskigi rezistadon post la tertremo</url></vrk>
          <lok>GlobalVoices, 2021-09-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">landslide</trd>
  <trd lng="fr">glissement de terrain</trd>
</drv>

<drv mrk="glit.0fali">
  <kap><tld/>fali</kap>
  <snc mrk="glit.0fali.KOMUNE">
    <dif>
      <ref tip="dif" cel="glit.0i.fali"><tld/>i<sncref/></ref>
      <ekz>
        la pluvo pli kaj pli grandi&gcirc;is, por ne <tld/>fali, <nom>Wang</nom> demetis
        siajn &scirc;uojn kaj &scirc;trumpojn
        <fnt>
          <aut>Gua Bao</aut>,
          <vrk><url ref="http://www.espero.com.cn/old/epch/z06-06s/06s-2/07-06-2-10.htm">
          Wang Yingyi, kompartiano sin dedi&ccirc;anta al la
          popolo</url></vrk>
          <lok>espero.com.cn, [2007?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>fali en pacan dormon
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk><url
          ref="&DonH;Revuoj/nlr/nlr36/majorkaj.html">Majorkaj
          azule&hcirc;oj</url></vrk>,
          <lok>Nica Literatura Revuo, 3:6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>
    &c_p;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_n;&c_u;&c_c;&c_c;&c_a;
    </trd>,
    <trd>&c_p;&c_a;&c_k;&c_a;&c_w;&c_z;&c_n;&c_u;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>smeknout se a upadnout</trd>,
    <trd>uklouznout s p&aacute;dem</trd>
  </trdgrp>
  <trd lng="de">ausgleiten</trd>
  <trd lng="en">slip and fall</trd>
  <trd lng="fr">tomber<klr tip="ind"> (&agrave; la suite d'une
  glissade)</klr></trd>
  <trd lng="hu">elcs&uacute;szik</trd>
  <trd lng="nl">uitglijden</trd>
  <trd lng="pl">po&sacute;lizgn&aogonek;&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_k;&c_o;&c_l;&c_mol;&c_z;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sk">padn&uacute;&tcaron; pok&lacute;znut&iacute;m</trd>
  <trd lng="uk">&c_u;&c_p;&c_a;&c_s;&c_t;&c_i;&nbsp;(&c_p;&c_o;&c_s;&c_l;&c_i;&c_z;&c_n;&c_u;&c_v;&c_sh;&c_i;&c_s;&c_mol;)</trd>
</drv>

<drv mrk="glit.0kuri">
  <kap><tld/>kuri,
  <var><kap><tld/>umi</kap></var></kap>
  <snc mrk="glit.0kuri.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="glit.0i.antauxenigxi"><tld lit="G"/>i<sncref/></ref>
      por amuzi&gcirc;i, korpe ekzerci&gcirc;i:
      <ekz>
        se mi povus tion fari, mi irus nun malsupren sur la glacion por
        <tld/>kuri
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_g;&c_ch;&c_y;
    <klr tip="amb">(&c_n;&c_a; &c_k;&c_a;&c_n;&c_mol;&c_k;&c_o;&c_h;)
    </klr></trd>,
    <trd>&c_k;&c_a;&c_t;&c_a;&c_c;&c_c;&c_a;
    <klr tip="amb">(&c_n;&c_a; &c_k;&c_a;&c_n;&c_mol;&c_k;&c_o;&c_h;)
    </klr></trd>,
    <trd>&c_k;&c_o;&c_w;&c_z;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bruslit</trd>,
    <trd>klouzat se</trd>
  </trdgrp>
  <trd lng="de">Schlittschuh laufen</trd>
  <trd lng="en">skate</trd>
  <trdgrp lng="fr">
    <trd>patiner<klr tip="ind"> (en patins &agrave; glace)</klr></trd>,
    <trd>faire du <ind>patin</ind><klr tip="ind"> (&agrave; glace)</klr>
    </trd>
  </trdgrp>
  <trd lng="hu">korcsoly&aacute;zik</trd>
  <trd lng="nl">schaatsen</trd>
  <trdgrp lng="pl">
    <trd>je&zacute;dzi&cacute; na &lstroke;y&zdot;wach</trd>
  </trdgrp>
  <trd lng="sk">kor&ccaron;u&lcaron;ova&tcaron; sa</trd>
  <trd lng="uk">&c_k;&c_o;&c_v;&c_z;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
</drv>

<drv mrk="glit.0punkta">
  <kap><tld/>punkta, <var><kap><tld/>koma</kap></var></kap>
  <snc mrk="glit.0punkta.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Rilata al la maniero prezenti nombron en la formo
      <frm>M&#215;b<sup>E</sup></frm>, kie
      <frm>b</frm> estas la bazo de pozicia nombrosistemo;
      <frm>M</frm> estas la
      <ref tip="prt" cel="mantis.0o.glitpunkta">mantiso</ref>
      (<frm>b</frm>-uma frakcio)
      kaj <frm>E</frm> estas la
      <ref tip="prt" cel="ekspon2.0o.glitpunkta">eksponento</ref>:
      <ekz>
        12.3&#215;10<sup>1</sup>, 1.23&#215;10<sup>2</sup>,
        0.123&#215;10<sup>3</sup> estas egalaj nombroprezentoj
        <tld/>punktaj;
      </ekz>
      <ekz>
        la <tld/>punkta kalkulpovo de Celeron do estis jam pli granda ol
        tiu de P5
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Celeron">
          Celeron</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la algoritmo estas realigata per <tld/>koma aritmetiko
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/QR-faktorigo">
          QR-faktorigo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>komaj nombroj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/-0">-0</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="frakci.pozicia0o">poziciaj frakcioj</ref>
    <ref tip="malprt" cel="reel.0o.KOMP">reeloj</ref>
    <ref tip="vid" cel="fiks.0koma">fikspunkta, fikskoma</ref>
  </snc>
  <trd lng="cs">s pohyblivou desetinou &ccaron;&aacute;rkou</trd>
  <trd lng="de">Gleitkomma-</trd>
  <trd lng="en">floating-point</trd>
  <trd lng="fr">&agrave; <ind>virgule</ind> flottante</trd>
  <trd lng="hu">lebeg&odblac;pontos</trd>
  <trd lng="pl">zmiennoprzecinkowy</trd>
  <trd lng="ru">&c_s; <ind>&c_p;&c_l;&c_a;&c_v;&c_a;&c_ju;&c_shch;&c_je;&c_j;</ind>
  &c_t;&c_o;&c_ch;&c_k;&c_o;&c_j;
  <klr>(&c_i;&c_l;&c_i; &c_z;&c_a;&c_p;&c_ja;&c_t;&c_o;&c_j;)</klr></trd>
  <trd lng="sk">s pohyblivou desatinnou &ccaron;iarkou</trd>
  <trd lng="sv">flyttals-</trd>
  <trd lng="uk">&c_z; &c_p;&c_l;&c_a;&c_v;&c_a;&c_ju;&c_ch;&c_o;&c_ju; &c_k;&c_o;&c_m;&c_o;&c_ju;</trd>
</drv>

