<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: maltoz.xml,v 1.7 2023/09/02 06:04:08 revo Exp $">
<kap>
  <rad>maltoz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="maltoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="maltoz.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Maltosukero;
      disakarido aperanta en
      <ref tip="vid" cel="malt.0o">malto</ref>,
      precipe en &gcirc;ermoj de hordeo; 
      &hcirc;emie <frm>C<sub>12</sub>H<sub>22</sub>O<sub>11</sub></frm>:
      <ekz>
        Ovomaltino <klr>...</klr> enhavas la naturajn
        sukerojn, kiujn la organismo ensorbas senpere :
        <tld/>o kaj laktozo 
        <fnt>
          <aut>A. Wander</aut>,
          <vrk><url ref="http://arkivo.esperanto-france.org/divers/ovomalt/ovo-p5.htm">
          La Plej Esenca pri Ovomaltino</url></vrk>,
          <lok>1910</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_mol;&c_t;&c_o;&c_z;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>malt&oacute;za</trd>,
    <trd>sladov&yacute; cukr</trd>
  </trdgrp>
  <trd lng="en">maltose</trd>
  <trd lng="fr">maltose</trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30EB;&#x30C8;&#x30FC;&#x30B9;</trd>,
    <trd>&#x9EA6;&#x82BD;&#x7CD6;  [<ind>&#x3070;&#x304F;&#x304C;&#x3068;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">maltoza</trd>     
  <trd lng="ro">maltoz&abreve;</trd>
  <trdgrp lng="sk">
    <trd>malt&oacute;za</trd>,
    <trd>sladov&yacute; cukor</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&c_a;&#x43B;&#x44C;&c_t;&#x43E;&c_z;&c_a;</trd>
</drv>
</art>

