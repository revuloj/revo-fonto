    <trd lng="en">Gaza city</trd>
    <trd lng="fr">Gaza</trd>
    <trd lng="nl">Gaza</trd>
    <trd lng="ru">&c_G;&c_a;&c_z;&c_a;</trd>
  </snc>
  <trd lng="ja">&#x30AC;&#x30B6;</trd>
  <trdgrp lng="zh">
    <trd>&#x52A0;&#x6C99; <pr>ji&#x101;sh&#x101;</pr></trd>,
    <trd>&#x52A0;&#x6C99;&#x57CE; <pr>ji&#x101;sh&#x101;ch&#xE9;ng</pr></trd>
  </trdgrp>
