<art mrk="$Id: alkalo.xml,v 1.17 2023/10/28 08:33:05 revo Exp $">
<kap>
  <rad>alkaloid</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="alkalo.0o">
  <kap><tld/>o</kap>
  <snc mrk="alkalo.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Organika <ref tip="vid" cel="alkal.0eca">alkaleca</ref> substanco,
      kiaj <ref tip="sub" cel="kafein.0o">kafeino</ref>,
      <ref tip="sub" cel="kokain.0o">kokaino</ref>,
      <ref tip="sub" cel="morfin.0o">morfino</ref>:
      <ekz>
        la <tld/>o de kaktacoj, meskalino, absolute ne rilatas kun brando de agavoj
        <fnt>
          <aut>Fidel Figueroa</aut>
          <vrk><url ref="&Monato;/2000/007033.php">Nepre &gcirc;ia nomo estas Meksiko</url></vrk>
          <bib>Monato</bib>
          <lok>2000/10, p. 14</lok>
          </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x751F;&#x7269;&#x78B1; <pr>sh&#x113;ngw&#xF9;ji&#x1CE;n</pr></trd>,
    <trd>&#x751F;&#x7269;&#x9E7C; <pr>sh&#x113;ngw&#xF9;ji&#x1CE;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_k;&c_a;&c_l;&c_jo;&c_ib;&c_d;</trd>
  <trd lng="bg">&c_a;&c_l;&c_k;&c_a;&c_l;&c_o;&c_i;&c_d;</trd>
  <trd lng="ca">alcaloide</trd>
  <trd lng="cs">alkaloid</trd>
  <trd lng="de">Alkaloid</trd>
  <trd lng="en">alkaloid</trd>
  <trd lng="es">alcaloide</trd>
  <trd lng="fr">alcalo&iuml;de</trd>
  <trd lng="he">&#x5D0;&#x5DC;&#x5E7;&#x5DC;&#x5D5;&#x5D0;&#x5D9;&#x5D3;</trd>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30AB;&#x30ED;&#x30A4;&#x30C9;</trd>
  <trd lng="nl">alcalo&iuml;de</trd>
  <trd lng="pt">alcal&oacute;ide</trd>
  <trd lng="ru">&c_a;&c_l;&c_k;&c_a;&c_l;&c_o;&c_i;&c_d;</trd>
  <trd lng="sk">alkaloid</trd>
  <trd lng="sv">alkaloid</trd>
  <trd lng="uk">&c_a;&#x43B;&#x43A;&c_a;&#x43B;&#x43E;&c_ji;&c_d;</trd>
</drv>
</art>
