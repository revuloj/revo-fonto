<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>glas</rad>/o
</kap>
<drv mrk="glas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Vazo por trinki:
      <ekz><tld/>o de, por biero, vino, mozelvino, &ccirc;ampano;</ekz>
      <ekz><tld/>o da (mezuro, kvanto) vino;</ekz>
      <ekz>
        li esploris iom la fundon de la <tld/>o<fnt>Z</fnt>
        <klr>(estas ebrieta)</klr>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="super">super C 3.</ref>
    </refgrp>
  </snc>
  <trd lng="cs">sklenice</trd>
  <trdgrp lng="de">
    <trd>Glas</trd>,
    <trd>Becher</trd>
  </trdgrp>
  <trd lng="en">glass</trd>
  <trdgrp lng="nl">
    <trd>glas</trd>,
    <trd>beker</trd>
  </trdgrp>
  <trd lng="fr">verre</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_k;&c_a;&c_n;</trd>
  <trd lng="tr">bardak</trd>
</drv>
</art>
