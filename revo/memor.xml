<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>memor</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="memor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Konservi a&ubreve; denove prezenti al si anta&ubreve;e
      ricevitajn perceptojn a&ubreve; ideojn:
      <ekz>
        tiun &ccirc;i tre gravan principon la esperantistoj volu &ccirc;iam bone
        <tld/>i
        <fnt><bib>F</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        neniu plu ilin <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i lecionon, vorton<fnt>Z</fnt>,
      </ekz>
      <ekz>
        ofendon, amikon;
      </ekz>
      <ekz>
        <tld/>i pri io a&ubreve; iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi bone <tld/>as tiun grandiozan vida&jcirc;on;
      </ekz>
      <ekz>
        kiu volas mensogi, devas bone <tld/>i
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gravur.0i">gravuri 2</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_p;&c_a;&c_m;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_g;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>recordar, recordar-se('n)</trd>,
    <trd>memoritzar</trd>
  </trdgrp>
  <trd lng="cs">pamatovat (si)</trd>
  <trd lng="de">sich <ind>erinnern</ind></trd>
  <trd lng="en">remember</trd>
  <trdgrp lng="es">
    <trd>recordar</trd>,
    <trd>acordarse</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>rem&eacute;morer</ind></trd>,
    <trd>se <ind>souvenir</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>eml&eacute;kszik</trd>,
    <trd>eml&eacute;kezik</trd>
  </trdgrp>
  <trd lng="nl">zich <ind>herinneren</ind></trd>
  <trdgrp lng="pt">
    <trd>lembrar-se (de)</trd>,
    <trd>recordar-se (de)</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_m;&c_n;&c_i;&c_t;&c_mol;</trd>;
    <trd>&c_v;&c_s;&c_p;&c_o;&c_m;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pam&auml;tava&tcaron; (sa)</trd>,
    <trd>pamätať si</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>minnas</trd>,
    <trd>komma ih&aring;g</trd>
  </trdgrp>
</drv>

<drv mrk="memor.0o">
  <kap><tld/>o</kap>
  <snc mrk="memor.0o.kapablo">
    <dif>
      Kapablo <tld/>i:
      <ekz>
        e&ccirc; la viza&gcirc;o mem de &scirc;ia voja kolego komencis iel mallumi&gcirc;i
        en &scirc;ia
        <tld/>o
        <fnt><bib>FK</bib>, <lok>Arturo (rakonto de V.
        Devjatnin)</lok></fnt>
      </ekz>
      <ekz>
        havi bonan <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        noti al si en la <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        enskribi en sian <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi uzas la okazon por revoki en vian <tld/>on la
        nomon de<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ca">mem&ograve;ria</trd>
    <trd lng="de">Ged&auml;chtnis</trd>
    <trd lng="es">memoria</trd>
    <trdgrp lng="hu">
      <trd>eml&eacute;kez&odblac;tehets&eacute;g</trd>,
      <trd>mem&oacute;ria</trd>
    </trdgrp>
  </snc>
  <snc mrk="memor.0o.memorado">
    <dif>
      Konservado a&ubreve; denova prezentado al la konscio je ia
      anta&ubreve;a percepto a&ubreve; ideo:
      <ekz>
        havi, teni, gardi la <tld/>on de, pri io, iu;
      </ekz>
      <ekz>
        desegni portreton el la <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        akceptu miajn bon<tld/>ojn.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>record</trd>,
      <trd>mem&ograve;ria</trd>
    </trdgrp>
    <trd lng="de">Erinnerung</trd>
    <trd lng="es">recuerdo</trd>
    <trd lng="fr">souvenir</trd>
    <trdgrp lng="hu">
      <trd>eml&eacute;kezet</trd>,
      <trd>eml&eacute;kez&eacute;s</trd>
    </trdgrp>
  </snc>
  <snc mrk="memor.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Organo de komputilo destinita por ricevo, konservado kaj
      resendo de datumoj.
    </dif>
    <ref tip="malprt" cel="aparat.0aro.KOMP">aparataro</ref>
    <ref tip="prt" cel="datum.0portilo">datumportilo</ref>
    <trd lng="ca">mem&ograve;ria <klr>(RAM, cau, secund&agrave;ria...)</klr></trd>
    <trd lng="de">Speicher</trd>
    <trd lng="en">storage</trd>
    <trd lng="es">memoria</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_m;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="en">memory</trd>
  <trd lng="fr">m&eacute;moire</trd>
  <trdgrp lng="hu">
    <trd>mem&oacute;ria</trd>,
    <trd>t&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">geheugen</trd>
  <trd lng="ru">&c_p;&c_a;&c_m;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>pam&auml;&tcaron;</trd>,
    <trd>schopnosť pamätania</trd>
  </trdgrp>
  <trd lng="sv">minne</trd>
</drv>

<drv mrk="memor.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Parkere.
    </dif>
    <trd lng="be">&c_n;&c_a;&c_p;&c_a;&c_m;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="ca">de mem&ograve;ria</trd>
    <trdgrp lng="hu">
      <trd>eml&eacute;kezetb&odblac;l</trd>,
      <trd>fejb&odblac;l</trd>,
      <trd>k&iacute;v&uuml;lr&odblac;l</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>van <ind>buiten</ind></trd>,
      <trd>uit het <ind>hoofd</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o; <ind>&c_p;&c_a;&c_m;&c_ja;&c_t;&c_i;</ind></trd>,
      <trd>&c_n;&c_a; <ind>&c_p;&c_a;&c_m;&c_ja;&c_t;&c_mol;</ind></trd>,
      <trd>&c_n;&c_a;&c_i;&c_z;&c_u;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">naspam&auml;&tcaron;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>de <ind>m&eacute;moire</ind></trd>,
    <trd>par coeur</trd>
  </trdgrp>
  <trd lng="sk">spamäti</trd>
  <trd lng="sv">utantill</trd>
</drv>

<drv mrk="memor.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <dif>
      Por <tld/>igo pri.
    </dif>
    <trd lng="be">&c_u; <ind>&c_p;&c_a;&c_m;&c_ja;&c_c;&c_mol;</ind> <klr>(&c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;,
    &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
    <trd lng="ca"><ind>en</ind> mem&ograve;ria <ind>de</ind></trd>
    <trd lng="hu">eml&eacute;k&eacute;re</trd>
    <trd lng="nl">ter <ind>nagedachtenis</ind> van</trd>
    <trd lng="ru">&c_v; <ind>&c_p;&c_a;&c_m;&c_ja;&c_t;&c_mol;</ind> <klr>(&c_o; &c_k;&c_o;&c_m;,
    &c_ch;&c_jo;&c_m;-&c_l;.)</klr></trd>
  </snc>
  <trd lng="fr">en m&eacute;moire de</trd>
  <trd lng="sv">till minne av</trd>
</drv>

<drv mrk="memor.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="memor.0ajxo.donaco">
    <dif>
      <tld lit="M"/>iga donaco:
      <ekz>
        <tld/>a&jcirc;o- kaj libro-vendejo
        <fnt>
        <aut>Saliko</aut>,
        <vrk>La hereda&jcirc;o de orientaj ortodoksuloj</vrk>,
        <bib>Monato</bib>,
        <lok>2002:12, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_v;&c_e;&c_n;&c_ib;&c_r;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_a;&c_r;&c_u;&c_n;&c_a;&c_k; <klr tip="amb">(&c_n;&c_a;
      &c_p;&c_a;&c_m;&c_ja;&c_c;&c_mol;)</klr></trd>
    </trdgrp>
    <trd lng="ca">record <klr>(souvenir)</klr></trd>
    <trdgrp lng="hu">
      <trd>eml&eacute;kt&aacute;rgy</trd>,
      <trd>szuven&iacute;r</trd>
    </trdgrp>
    <trd lng="es">recuerdo <klr>(objeto)</klr></trd>
    <trd lng="nl">souvenir</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_d;&c_a;&c_r;&c_o;&c_k; &c_n;&c_a; &c_p;&c_a;&c_m;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_u;&c_v;&c_je;&c_n;&c_i;&c_r;</trd>
    </trdgrp>
    <trd lng="sv">souvenir</trd>
  </snc>
  <snc mrk="memor.0ajxo.memorlibro">
    <uzo tip="klr">(ofte plurnombre)</uzo>
    <ref tip="dif" cel="libr.memor0o"><tld/>libro</ref>
    <ekz>
      Kristoforo Kolumbo en siaj <tld/>a&jcirc;oj lasis detalan sciigon, en
      kia maniero la homoj konati&gcirc;is kun tiu &ccirc;i kreska&jcirc;o
      <fnt><bib>FK</bib>, <lok>bagateloj</lok></fnt>.
    </ekz>
    <trdgrp lng="be">
      <trd>&c_m;&c_e;&c_m;&c_u;&c_a;&c_r;&c_y;</trd>,
      <trd>&c_u;&c_s;&c_p;&c_a;&c_m;&c_ib;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>mem&ograve;ries</trd>,
      <trd>llibre de mem&ograve;ries</trd>
    </trdgrp>
    <trd lng="es">memorias</trd>
    <trd lng="hu">eml&eacute;kirat</trd>
    <trd lng="nl">memoires</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_je;&c_m;&c_u;&c_a;&c_r;&c_y;</trd>,
      <trd>&c_v;&c_o;&c_s;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_n;&c_i;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>memoar</trd>,
      <trd>sj&auml;lvbiografi</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>m&eacute;moires</trd>,
    <trd>souvenir<klr tip="amb"> (objet)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="memor.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Da&ubreve;rigi la <tld/>on pri:
      <ekz>
        &ccirc;iu sono de ilia parolo <tld/>igas nin, ke ili estas fremdaj
        por ni
        <fnt><bib>FK</bib>, <lok>el la unua
        libro de la lingvo esperanto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_a;&c_m;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd><ind>fer</ind> recordar</trd>,
    <trd><ind>portar</ind> a la mem&ograve;ria</trd>
  </trdgrp>
  <trd lng="es">hacer <ind>recordar</ind> <klr>(ej: eso me recuerda
  a&#8230;/que&#8230;)</klr></trd>
  <trd lng="fr">rappeler<klr tip="amb"> (un souvenir)</klr></trd>
  <trd lng="hu">eml&eacute;keztet</trd>
  <trd lng="nl">herinneren</trd>
  <trd lng="ru">&c_n;&c_a;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>udržať</trd>,
    <trd>zachovať v pamäti</trd>
  </trdgrp>
  <trd lng="sv">p&aring;minna</trd>
</drv>

<drv mrk="memor.0igajxo">
  <kap><tld/>iga&jcirc;o</kap>
  <dif>
    &Ccirc;io, kio servas por instigi al <tld/>o pri io a&ubreve; iu
    (monumento, statuo, verko...):
    <ekz>
      terura <tld/>iga&jcirc;o el la mezaj jarcentoj<fnt>Z</fnt>;
    </ekz>
    <ekz>
      tiamaniere vi havos hejme <tld/>iga&jcirc;on pri ni
      <fnt><bib>Fab3</bib>, <lok>supo el kolbasaj
      bastonetoj</lok></fnt>;
    </ekz>
    <ekz>
      &ccirc;i tiuj &scirc;tonoj estos <tld/>iga&jcirc;o por la Izraelidoj eterne
      <fnt><bib>MT</bib>, <lok>&Jos; 4:7</lok></fnt>.
    </ekz>
  </dif>
  <trd lng="be">&c_n;&c_a;&c_p;&c_a;&c_m;&c_ib;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>memorial</trd>,
    <trd>recordatori</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;morial</trd>
  <trdgrp lng="hu">
    <trd>eml&eacute;km&udblac;</trd>,
    <trd>eml&eacute;keztet&odblac; <klr tip="ind">(t&aacute;rgy)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="memor.0igilo">
  <kap><tld/>igilo</kap>
  <snc>
    <dif>
      Kajero, libreto, kie oni enskribas tion, kion oni deziras <tld/>i.
    </dif>
    <trd lng="be">&c_n;&c_a;&c_t;&c_a;&c_t;&c_n;&c_ib;&c_k;</trd>
    <trdgrp lng="hu">
      <trd>jegyzetf&uuml;zet</trd>,
      <trd>eml&eacute;kk&ouml;nyv</trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_a;&c_p;&c_i;&c_s;&c_n;&c_a;&c_ja; &c_k;&c_n;&c_i;&c_zh;&c_k;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Skriba&jcirc;o, noto por <tld/>igi ion al iu; <tld/>noto.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_m;&c_ja;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_n;&c_a;&c_t;&c_a;&c_t;&c_k;&c_a; <klr>(&c_n;&c_a; &c_p;&c_a;&c_m;&c_ja;&c_c;&c_mol;)</klr></trd>,
      <trd>&c_z;&c_a;&c_c;&c_je;&c_m;&c_k;&c_a; <klr>(&c_n;&c_a; &c_p;&c_a;&c_m;&c_ja;&c_c;&c_mol;)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>eml&eacute;keztet&odblac; <klr tip="amb">(&iacute;r&aacute;s)</klr></trd>,
      <trd>memorandum</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_a;&c_m;&c_ja;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_m;&c_je;&c_t;&c_k;&c_a; &c_n;&c_a; &c_p;&c_a;&c_m;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ca">
      <trd>agenda</trd>,
      <trd>memor&agrave;ndum</trd>
    </trdgrp>
  <trdgrp lng="fr">
    <trd>m&eacute;mento</trd>,
    <trd>memorandum</trd>,
    <trd>pense-b&ecirc;te</trd>
  </trdgrp>
  <trd lng="sk">pamäť počítača</trd>
</drv>

<drv mrk="memor.0inda">
  <kap><ofc>*</ofc><tld/>inda</kap>
  <snc>
    <dif>
      Tiom <ref tip="super" cel="grav.0a">grava</ref> a&ubreve; impresa, ke
      oni
      volas &gcirc;in poste <tld/>i:
      <ekz>
        tiu &ccirc;i grava tago restos por mi &ccirc;iam <tld/>inda
        <fnt><bib>F</bib>, <lok>&FE; 41</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>memorable</trd>,
    <trd>inoblidable</trd>
  </trdgrp>
  <trd lng="es">memorable</trd>
  <trd lng="fr">m&eacute;morable</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_a;&c_b;&c_y;&c_v;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">pamätihodný</trd>
  <trd lng="sv">minnesv&auml;rd</trd>
</drv>

<drv mrk="memor.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="memor.en0igi.PSI">
    <dif>
      <ref tip="super" cel="lern.0i.mempenado">Lerni</ref> konscie kaj
      detale, ne nepre kun kompreno:
      <ekz>
        &scirc;i en<tld/>igis la serurkodon de la domo
        <fnt>
        <aut>Last</aut>,
        <vrk>Maldiligenta polico</vrk>,
        <bib>Monato</bib>,
        <lok>2003:12, p. 21a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="parker.0e">parkere</ref>
    </refgrp>
    <trdgrp lng="ca">
    <trd>memoritzar</trd>,
    <trd>"empollar"</trd>
  </trdgrp>
  <trd lng="es">memorizar</trd>
  </snc>
  <snc mrk="memor.en0igi.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Meti en elektronikan memoron:
      <ekz>
        al&gcirc;ustigojn <klr>...</klr> oni povas en<tld/>igi en la
        satelitan
        ricevilon
        <fnt>
        <aut>Pejno Simono</aut>,
        <vrk>Radio/Poloj per pelvo</vrk>,
        <bib>Monato</bib>,
        <lok>1995:4, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">memoritzar <klr>(enregistrar-hi la
  informaci&oacute;)</klr></trd>
    <trd lng="es"><ind>grabar</ind> en memoria</trd>
  </snc>
  <trd lng="fr">m&eacute;moriser</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_o;&c_m;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="memor.mal0i">
  <kap>mal<tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="memor.mal0i.KOMUNE">
    <ref tip="dif" cel="forges.0i.ne_memori">forgesi<sncref/></ref>
    <ekz>
      la infano mire rigardis la interparolantojn,
      kiuj &scirc;ajne mal<tld/>is lian &ccirc;eeston
      <fnt>
        <bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 23a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="es">olvidar</trd>
  <trd lng="fr">oublier</trd>
</drv>

<drv mrk="memor.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc>
    <dif>
      Malfacile konservebla en la <tld/>o:
      <ekz>
        malsimpla ne<tld/>ebla regulo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">megjegyezhetetlen</trd>
    <trdgrp lng="ca">
     <trd>dif&iacute;cil de recordar</trd>,
     <trd>immemorable</trd>
  </trdgrp> 
  </snc>
  <snc>
    <dif>
      Ne plu <tld/>ata de la homoj:
      <ekz>
        de post tempo ne<tld/>ebla
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">immemorial</trd>
    <trd lng="hu">feled&eacute;sbe mer&uuml;lt</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>imm&eacute;morial</trd>,
    <trd>impossible &agrave; retenir</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_z;&c_a;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>
  <trd lng="sk">nezapam&auml;tate&lcaron;n&yacute;</trd>
</drv>

<drv mrk="memor.re0i">
  <kap>re<tld/>i</kap>
  <snc>
    <dif>
      Revenigi, refre&scirc;igi en sia spirito la <tld/>on pri iu a&ubreve; io:
      <ekz>
        oni diras, ke se homo komencas re<tld/>i sian tutan
        travivita&jcirc;on, tio &ccirc;i estas signo de lia proksima morto
        <fnt><bib>FK</bib>, <lok>nokto, (de Eube el
        Odeso)</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="macx.re0i">rema&ccirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_g;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_g;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>rememorar</trd>,
    <trd>evocar</trd>
  </trdgrp>
  <trd lng="es">rememorar</trd>
  <trdgrp lng="fr">
    <trd>se <ind>rappeler</ind></trd>,
    <trd>se <ind>souvenir</ind></trd>
  </trdgrp>
  <trd lng="hu">visszaeml&eacute;kezik</trd>
  <trd lng="nl">zich <ind>herinneren</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_s;&c_p;&c_o;&c_m;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_s;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozpamätať sa</trd>,
    <trd>spomen&uacute;&tcaron; si</trd>,
    <trd>upamätať sa</trd>
  </trdgrp>
</drv>

<drv mrk="memor.re0o">
  <kap>re<tld/>o</kap>
  <snc mrk="memor.re0o.SNC">
    <dif>
      Denova prezentado en la konscio de anta&ubreve;a percepto a&ubreve; ideo:
      <ekz>
        li ne havis ian re<tld/>on pri tio
        <fnt>
          <aut>H. C. Andersen, trad. d-ro L. L. Zamenhof</aut>,
          <vrk><url
          ref="http://www.steloj.de/esperanto/fabeloj4/dunoj.html">Fabeloj
          de Andersen - Historio el la
          dunoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">souvenir</trd>
  </snc>
  <trdgrp lng="sk">
    <trd>memento</trd>,
    <trd>varovanie</trd>,
    <trd>výstraha</trd>
  </trdgrp>
</drv>

<drv mrk="memor.re0igi">
  <kap>re<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Revenigi, refre&scirc;igi en ies spirito la <tld/>on pri:
      <ekz>
        mi re<tld/>igos al vi &ccirc;i tion
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 6a</lok></fnt>!
      </ekz>
      <ekz>
        re<tld/>igi iun pri lia devo;
      </ekz>
      <ekz>
        la &scirc;tonegoj re<tld/>igis per la formo ruinojn
        de citadelo<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_a;&c_m;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>recordar <klr>(quelcom a alg&uacute;)</klr></trd>,
    <trd><ind>fer</ind> recordar</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>recordar</ind> algo a alguien <klr>(refrescar la memoria
    de alguien)</klr></trd>
  </trdgrp>
  <trd lng="fr">rappeler<klr tip="amb"> (quelque chose &agrave;
  quelqu'un)</klr></trd>
  <trdgrp lng="hu">
    <trd>eml&eacute;keztet</trd>,
    <trd>figyelmeztet</trd>,
    <trd>felid&eacute;z</trd>
  </trdgrp>
  <trd lng="nl">herinneren</trd>
  <trd lng="ru">&c_n;&c_a;&c_p;&c_o;&c_m;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">pripomenúť</trd>
</drv>

<drv mrk="memor.0signo">
  <kap><tld/>signo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      <tld lit="M"/>iga&jcirc;o.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>eml&eacute;km&udblac;</trd>,
    <trd>eml&eacute;keztet&odblac; <klr tip="amb">(t&aacute;rgy)</klr></trd>
  </trdgrp>
  <trd lng="nl">gedenkteken</trd>
  <trd lng="ru">&c_n;&c_a;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">značka na pamiatku</trd>
</drv>

<drv mrk="memor.kar0a">
  <kap>kar<tld/>a</kap>
  <snc>
    <dif>
      Kies <tld/>o estas kara.
    </dif>
  </snc>
  <trd lng="ca">de dol&ccedil; record</trd>
  <trdgrp lng="hu">
    <trd>&aacute;ldott eml&eacute;k&udblac;</trd>,
    <trd>megboldogult</trd>
  </trdgrp>
  <trd lng="nl">zaliger</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_z;&c_a;&c_b;&c_y;&c_v;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>blahej</trd>,
    <trd>drahej pamäti</trd>
  </trdgrp>
</drv>
<adm>
  la sencoj de memora&jcirc;o, memorigilo, memoriga&jcirc;o &scirc;ajnas iom arbitre
  diversaj.
  <aut>&jcirc;v</aut>
</adm>
</art>
