    <trd lng="en">duck</trd>
    <trd lng="es">pato</trd>
    <trd lng="fr">canard</trd>
    <trd lng="hu">kacsa</trd>
    <trd lng="id">bebek</trd>
    <trd lng="it">anatra</trd>
    <trd lng="nl">eend</trd>
    <trd lng="no">and</trd>
    <trd lng="pl">kaczka</trd>
    <trd lng="pt">pato</trd>
    <trd lng="ro">ra&#x21B;&abreve;</trd>
    <trd lng="ru">&c_u;&c_t;&c_k;&c_a;</trd>
    <trd lng="sk">ka&ccaron;ica</trd>
    <trdgrp lng="sv">
      <trd>and</trd>,
      <trd>anka</trd>
    </trdgrp>
    <trd lng="tr">&ouml;rdek</trd>
    <trd lng="zh">&#x9E2D; <pr>y&amacron;</pr></trd>
  </snc>

  <snc mrk="anas.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt" cel="anased.0oj"><tld/>edoj</ref>
      (<trd lng="la">Anas</trd>);
      <ref tip="lst" cel="bird.nagxo0o" lst="voko:birdoj">na&gcirc;birdo</ref>,
      kies viro havas plumaron belan kaj tute apartan de la virino:
      <ekz>
        trapasante la facilajn rapidfluejojn de Enco, ni vidis plurfoje
        <tld/>ojn kun belaj oran&gcirc;aj bekoj
        <fnt>
          <aut>W. Schad</aut>,
          <vrk>Tra la Andoj boate</vrk>,
          <bib>Monato</bib>,
          <lok>2001:12, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/b/b0/Anas_platyrhynchos_male_female.jpg/640px-Anas_platyrhynchos_male_female.jpg"
    prm="CC BY-SA 2.5">Platbekaj ge<tld/>oj, M&uuml;hlthal, Stra&szlig;lach-Dingharting, Germanujo
        <fnt>
