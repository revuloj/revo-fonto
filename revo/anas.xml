<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>anas</rad>/o
</kap>
<drv mrk="anas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Na&gcirc;birdo el la familio
      <ref tip="super" cel="anased.0oj">anasedoj</ref>, de
      diversaj genroj.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_ch;&c_k;&c_a;</trd>
    <trd lng="cs">kachna</trd>
    <trd lng="de">Ente</trd>
    <trd lng="es">pato</trd>
    <trd lng="fr">canard</trd>
    <trd lng="nl">eend</trd>
    <trd lng="it">anatra</trd>
    <trd lng="ru">&c_u;&c_t;&c_k;&c_a;</trd>
    <trdgrp lng="sv">
      <trd>and</trd>,
      <trd>anka</trd>
    </trdgrp>
    <trd lng="tr">&ouml;rdek</trd>
  </snc>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super" cel="anased.0oj">anasedoj</ref>
      (<trd lng="la">Anas</trd>).
    </dif>
    <bld lok="../bld/anas.jpg">
      <ind>platbeka <tld/>o</ind> &dash; paro
      <trd lng="cs">kachna divok&aacute;</trd>
      <trd lng="de">Stockente</trd>
      <trd lng="en">mallard</trd>
      <trd lng="fr">canard</trd>
      <trd lng="hu">t&odblac;k&eacute;s r&eacute;ce</trd> 
      <trd lng="la">Anas platyrhynchos</trd>
      <trd lng="pl">krzy&zdot;&oacute;wka</trd>
      <trd lng="ru">&c_k;&c_r;&c_ja;&c_k;&c_v;&c_a;</trd>
    </bld>
  </snc>
</drv>

<drv mrk="anas.tuf0o">
  <kap>tuf<tld/>o <fnt>(<aut>d-ro M&eacute;sz&aacute;ros B&eacute;la</aut>:
  <vrk>Oklingva
  nomaro de e&ubreve;ropaj birdoj</vrk>)</fnt></kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fuligu.0o">Fuligulo</ref>.
    </dif>
  </snc>
  <trd lng="ru">&c_h;&c_o;&c_h;&c_l;&c_a;&c_t;&c_a;&c_ja;
  &c_ch;&c_je;&c_r;&c_n;&c_je;&c_t;&c_mol;</trd>
</drv>
</art>
