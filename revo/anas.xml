<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>anas</rad>/o
</kap>
<drv mrk="anas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Na&gcirc;birdo el la familio
      <ref tip="super" cel="anas.0edoj">anasedoj</ref>, de
      diversaj genroj.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_ch;&c_k;&c_a;</trd>
    <trd lng="cs">kachna</trd>
    <trd lng="de">Ente</trd>
    <trd lng="fr">canard</trd>
    <trd lng="it">anatra</trd>
    <trd lng="ru">&#1091;&#1090;&#1082;&#1072;</trd>
    <trd lng="tr">&ouml;rdek</trd>
  </snc>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super" cel="anas.0edoj">anasedoj</ref>
      (<trd lng="la">Anas</trd>).
    </dif>
    <bld lok="../bld/anas1.jpg">
      <tld/>o <klr>(Anas platyrhynchos)</klr>
    </bld>
    <bld lok="../bld/anas2.jpg">
      <tld/>ino <klr>(Anas platyrhynchos)</klr>
    </bld>
    <trd lng="fr">canard</trd>
  </snc>
</drv>
<drv mrk="anas.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="anser.0oformaj">anseroformaj</ref>
      birdoj (<trd lng="la">Anatidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Enten</trd>
  <trd lng="fr">anatid&eacute;s</trd>
</drv>
<drv mrk="anas.tuf0o">
  <kap>tuf<tld/>o
  <fnt>(<aut>d-ro M&eacute;sz&aacute;ros B&eacute;la</aut>:
  <vrk>Oklingva nomaro de e&ubreve;ropaj birdoj</vrk>)</fnt>
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fuligu.0o">Fuligulo</ref>.
    </dif>
  </snc>
</drv>
</art>
