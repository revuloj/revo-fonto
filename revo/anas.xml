<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>anas</rad>/o <fnt><bib>Fab2</bib></fnt>
</kap>
<drv mrk="anas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="anas.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="lst" cel="bird.nagxo0o"
      lst="voko:birdoj">Na&gcirc;birdo</ref> 
      el la familio
      <ref tip="malprt" cel="anased.0oj"><tld/>edoj</ref>, de
      diversaj genroj:
      <ekz>
        tie ku&scirc;is <tld/>ino sur sia nesto, por elkovi siajn idojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        kokinoj, <tld/>oj kaj meleagroj
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kortbirdaro kiel kokinoj kaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2003/008099.html"
              >Birda gripo, &ccirc;u dan&gcirc;era por la homo?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        rostita <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="https://www.monato.be/2005/008743.php"
              >Konsumado sur trajnoj multekostas</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        tri tutaj rostitaj bovoj, far&ccirc;itaj per anasoj kaj kokinoj, estis elmetitaj meze
        de la bazaro
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_ch;&c_k;&c_a;</trd>
    <trd lng="bg">&c_p;&c_a;&c_t;&c_i;&c_c;&c_a;</trd>
    <trd lng="br">houad</trd>
    <trd lng="cs">kachna</trd>
    <trd lng="de">Ente</trd>
    <trd lng="en">duck</trd>
    <trd lng="es">pato</trd>
    <trd lng="fr">canard</trd>
    <trd lng="hu">kacsa</trd>
    <trd lng="id">bebek</trd>
    <trd lng="it">anatra</trd>
    <trd lng="nl">eend</trd>
    <trd lng="no">and</trd>
    <trd lng="pl">kaczka</trd>
    <trd lng="pt">pato</trd>
    <trd lng="ro">ra&tcedil;&abreve;</trd>
    <trd lng="ru">&c_u;&c_t;&c_k;&c_a;</trd>
    <trd lng="sk">ka&ccaron;ica</trd>
    <trdgrp lng="sv">
      <trd>and</trd>,
      <trd>anka</trd>
    </trdgrp>
    <trd lng="tr">&ouml;rdek</trd>
    <trd lng="zh">&#40493; [<ind>y&#257;</ind>]</trd>
  </snc>

  <snc mrk="anas.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
      cel="anased.0oj"><tld/>edoj</ref>
      (<trd lng="la">Anas</trd>);
      <ref tip="lst" cel="bird.nagxo0o"
      lst="voko:birdoj">na&gcirc;birdo</ref>,
      kies viro havas plumaron belan kaj tute apartan de la virino:
      <ekz>
        trapasante la facilajn rapidfluejojn de Enco, ni vidis plurfoje
        <tld/>ojn kun belaj oran&gcirc;aj bekoj
        <fnt>
          <aut>W. Schad</aut>,
          <vrk>Tra la Andoj boate</vrk>,
          <bib>Monato</bib>,
          <lok>2001:12, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/b/b0/Anas_platyrhynchos_male_female.jpg/640px-Anas_platyrhynchos_male_female.jpg"
      prm="CC BY-SA 2.5"
      >Platbekaj ge<tld/>oj, M&uuml;hlthal, Stra&szlig;lach-Dingharting, Germanujo
        <fnt>
          <aut>Richard Bartz, propra verko</aut>
          <lok><url ref="&WCW;/File:Anas_platyrhynchos_male_female.jpg"
              >https://commons.wikimedia.org/wiki/File:Anas_platyrhynchos_male_female.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="he">&bet;&resh;&vav;&vav;&zayin;</trd>
</drv>

<drv mrk="anas.blankspegula_mar0o">
  <kap>blankspegula mar<tld/>o</kap>
  <snc mrk="anas.blankspegula_mar0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>W. F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:zoologiaj_specioj">Specio</ref>
       de <ref tip="super" cel="anas.mar0o">mar<tld/>o</ref>
      (<trd lng="la">Melanitta fusca</trd>):
      <ekz>
        Blankspegula mar<tld/>o <klr>[&#8230;]</klr> reprodukti&gcirc;as &ccirc;e malproksima nordo de
        E&ubreve;ropo kaj Azio okcidente de la baseno de la rivero Jenisejo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=126311"
              >Blankspegula maranaso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/f/fd/Melanitta_fusca_2.jpg/640px-Melanitta_fusca_2.jpg"
      prm="CC BY 2.0"
      >Blankspegula mar<tld/>o, Falkenberg, Svedujo
        <fnt>
          <aut>&Aring;sa Berndtsson, Sv&auml;rta / Velvet Scoter</aut>
          <lok><url ref="&WCW;/File:Melanitta_fusca_2.jpg"
              >https://commons.wikimedia.org/wiki/File:Melanitta_fusca_2.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="en">
      <trd>white-winged <ind>scoter</ind></trd>,
      <trd>velvet <ind>scoter</ind></trd>
    </trdgrp>
    <trd lng="fr"><ind>macreuse</ind> brune</trd>
    <trd lng="pl">uhla</trd>
    <trd lng="sv">sv&auml;rta</trd>
  </snc>
</drv>

