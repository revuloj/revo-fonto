    </trdgrp>
    <trd lng="de">Pfeifente</trd>
    <trd lng="en"><klr>eurasian </klr>wigeon</trd>
    <trd lng="fr"><ind>canard</ind> siffleur</trd>
    <trd lng="hu">f&uuml;ty&uuml;l&oacute; r&eacute;ce</trd>
    <trd lng="nl">smient</trd>
    <trd lng="pl">&sacute;wistun</trd>
    <trd lng="pt">mareca</trd>
    <trd lng="sv">bl&auml;s<ind>and</ind></trd>
    <trdgrp lng="zh">
    <trd>&#x8D64;&#38968;&#x9D28; <pr>ch&#xEC;j&#x1D0;ngy&amacron;</pr></trd>,
    <trd>&#x8D64;&#39048;&#x9E2D; <pr>ch&#xEC;j&#x1D0;ngy&amacron;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="cs">hv&iacute;zd&aacute;k evroasijsk&yacute;</trd>
