<drv mrk="anas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="anas.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="lst" cel="bird.nagxo0o" lst="voko:birdoj">Na&gcirc;birdo</ref>
      el la familio
      <ref tip="malprt" cel="anased.0oj"><tld/>edoj</ref>, de
      diversaj genroj:
      <ekz>
        tie ku&scirc;is <tld/>ino sur sia nesto, por elkovi siajn idojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        kokinoj, <tld/>oj kaj meleagroj
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kortbirdaro kiel kokinoj kaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2003/008099.html">Birda gripo, &ccirc;u dan&gcirc;era por la homo?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        rostita <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2005/008743.php">Konsumado sur trajnoj multekostas</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        tri tutaj rostitaj bovoj, far&ccirc;itaj per anasoj kaj kokinoj, estis elmetitaj meze
        de la bazaro
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>

    <trd lng="bg">&c_p;&c_a;&c_t;&c_i;&c_c;&c_a;</trd>
    <trd lng="br">houad</trd>
    <trd lng="cs">kachna</trd>
