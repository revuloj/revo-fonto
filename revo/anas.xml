  </trdgrp>
  <trd lng="sk">ka&ccaron;ica ostrochvost&aacute;</trd>
</drv>

<drv mrk="anas.regxa_mol0o">
  <kap>re&gcirc;a mol<tld/>o</kap>
  <snc mrk="anas.regxa_mol0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>W. F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:zoologiaj_specioj">Specio</ref>
       de <ref tip="super" cel="anas.mol0o.ZOO">mol<tld/>o</ref>
      (<trd lng="la">Somateria spectabilis</trd>):
      <ekz>
        Re&gcirc;a mol<tld/>o <klr>[&#8230;]</klr> pasas plej parton de la jaro en marbordaj maraj
        ekosistemoj je altaj latitudoj, kaj migras al tundro de Arkto por
        reprodukti&gcirc;i en junio kaj julio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=258577"
              >Re&gcirc;a molanaso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/9/95/Somateria_spectabilis_(King_Eider_-_Prachteiderente)_-_Weltvogelpark_Walsrode_2012-16.jpg/640px-Somateria_spectabilis_(King_Eider_-_Prachteiderente)_-_Weltvogelpark_Walsrode_2012-16.jpg"
      prm="CC BY-SA 3.0"
      >Re&gcirc;a mol<tld/>o, Birdoparko Walsrode, Germanujo
        <fnt>
          <aut>Olaf Oliviero Riemer, propra verko</aut>
          <lok><url ref="&WCW;/File:Somateria_spectabilis_(King_Eider_-_Prachteiderente)_-_Weltvogelpark_Walsrode_2012-16.jpg"
              >https://commons.wikimedia.org/wiki/File:Somateria_spectabilis_(King_Eider_-_Prachteiderente)_-_Weltvogelpark_Walsrode_2012-16.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="en">king <ind>eider</ind></trd>
    <trd lng="fr"><ind>eider</ind> &agrave; t&ecirc;te grise</trd>
    <trd lng="pl">turkan</trd>
    <trd lng="sv">praktejder</trd>
  </snc>
</drv>

<drv mrk="anas.siberia_mol0o">
  <kap>siberia mol<tld/>o</kap>
  <snc mrk="anas.siberia_mol0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>W. F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:zoologiaj_specioj">Specio</ref>
       de <ref tip="super" cel="anas.mol0o.ZOO">mol<tld/>o</ref>
      (<trd lng="la">Somateria stelleri</trd> a&ubreve;
      <trd lng="la">Polysticta stelleri</trd>):
      <ekz>
        Siberia mol<tld/>o <klr>[&#8230;]</klr> nestas la&ubreve; la marbordoj de Arkta Oceano en
        orienta Siberio kaj Alasko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=258579"
              >Siberia molanaso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/d/db/Polysticta_stelleri.jpg/632px-Polysticta_stelleri.jpg"
      prm="CC BY 2.0"
      >Siberia mol<tld/>o
        <fnt>
          <aut>Ron Knight el Seaford, Britujo</aut>
          <lok><url ref="&WCW;/File:Polysticta_stelleri.jpg"
              >https://commons.wikimedia.org/wiki/File:Polysticta_stelleri.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="en">steller's <ind>eider</ind></trd>
    <trd lng="fr"><ind>eider</ind> de Steller</trd>
    <trd lng="pl">birginiak</trd>
    <trd lng="sv">alf&ouml;rr&auml;dare</trd>
  </snc>
</drv>

<drv mrk="anas.tuf0o">
  <kap>tuf<tld/>o
    <fnt>
      <bib>Bird</bib>
    </fnt>
  </kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="anas.tuf0o.fuligulo">
    <dif>
      <ref tip="dif" cel="fuligu.0o">Fuligulo</ref>:
      <ekz>
        la tuf<tld/>o havas bone videblan longan, blankan flugilmakulon, kiu etendi&gcirc;as
        &gcirc;is la flugilfino
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=127033"
              >Tufanaso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>houad du</trd>,
      <trd>kanig du<klr> [komune]</klr></trd>
    </trdgrp>
    <trd lng="cs">kachna chocholat&aacute;</trd>
    <trd lng="de">Reiherente</trd>
    <trd lng="en">tufted <ind>duck</ind></trd>
    <trd lng="fr">fuligule</trd>
    <trd lng="hu">kontyos r&eacute;ce</trd>
    <trd lng="la">Aythya fuligula</trd>
    <trd lng="nl">kuifeend</trd>
    <trd lng="pl">czernica</trd>
    <trdgrp lng="pt">
      <trd>ful&iacute;gula</trd>,
      <trd>negrina</trd>
    </trdgrp>
    <trd lng="ru">&c_h;&c_o;&c_h;&c_l;&c_a;&c_t;&c_a;&c_ja;
    &c_ch;&c_je;&c_r;&c_n;&c_je;&c_t;&c_mol;</trd>
    <trd lng="sv">vigg</trd>
  </snc>
</drv>
