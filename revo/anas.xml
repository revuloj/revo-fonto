<drv mrk="anas.mol0o">
  <kap><ofc>*</ofc>mol<tld/>o <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5115">mol'anas'</url></lok></fnt></kap>
  <snc mrk="anas.mol0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>W. F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdhvu8.htm">
           Birdonomoj en Esperanto por Hejma Vortaro</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> 
      el la familio <ref tip="malprt"
      cel="anased.0oj"><tld/>edoj</ref>
      (i.a. <trd lng="la">Somateria</trd>):
    </dif>
    <trd lng="de">Eiderente</trd>
    <trd lng="en">eider-<ind>duck</ind></trd>
    <trd lng="fr">eider</trd>
    <trd lng="hu">pehelyr&eacute;ce</trd>
    <trd lng="nl">eidereend</trd>
    <trd lng="sv">ejder</trd>
  </snc>
  <snc mrk="anas.mol0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>W. F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:zoologiaj_specioj">Specio</ref>
       de <ref tip="super"
      cel="anas.mol0o.KOMUNE">mol<tld/>o</ref>
      (<trd lng="la">Somateria mollissima</trd>):
    </dif>
    <ref tip="sin" cel="somateri.0o.ZOO">somaterio</ref>
    <trd lng="en"><klr>common</klr> eider</trd>
    <trd lng="fr"><ind>eider</ind> &agrave; duvet</trd>
    <trd lng="pl">edredon</trd>
    <trd lng="sv">ejder</trd>
  </snc>
  <trd lng="br">eider</trd>
  <trdgrp lng="cs">
    <trd>Kajka obecn&aacute;</trd>,
    <trd>kajka</trd>,
    <trd>kajka mo&rcaron;sk&aacute;</trd>
  </trdgrp>
  <trd lng="nl">eidereend</trd>
  <trd lng="ru">&c_g;&c_a;&c_g;&c_a;</trd>
  <trd lng="sk">kajka</trd>
</drv>

<drv mrk="anas.mar0o.nigra">
  <kap>nigra mar<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>W. F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:zoologiaj_specioj">Specio</ref>
       de <ref tip="super" cel="anas.mar0o">mar<tld/>o</ref>
      (<trd lng="la">Melanitta nigra</trd>):
    </dif>
    <trdgrp lng="en">
      <trd>common <ind>scoter</ind></trd>,
      <trd>black <ind>scoter</ind></trd>
    </trdgrp>
    <trd lng="fr"><ind>macreuse</ind> noire</trd>
    <trd lng="pl">markaczka</trd>
    <trd lng="sv">sj&ouml;orre</trd>
  </snc>
</drv>

<drv mrk="anas.mar0o.okulvitra">
  <kap>okulvitra mar<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>W. F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:zoologiaj_specioj">Specio</ref>
       de <ref tip="super" cel="anas.mar0o">mar<tld/>o</ref>
      (<trd lng="la">Melanitta perspicillata</trd>):
    </dif>
    <trd lng="fr"><ind>macreuse</ind> &agrave; front blanc</trd>
    <trd lng="pl">uhla pstrodzioba</trd>
    <trd lng="sv">vitnackad <ind>sv&auml;rta</ind></trd>
  </snc>
</drv>

<drv mrk="anas.0o.platbeka">
  <kap>platbeka <tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>W. F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:zoologiaj_specioj">Specio</ref>
       de <ref tip="super" cel="anas.0o.ZOO">anaso</ref>
      (<trd lng="la">Anas platyrhynchos</trd>):
      <ekz>
        rostita platbeka <tld/>o, pensis Sapulo, estus proksimume
        la &gcirc;usta afero
        <fnt>
          <aut>O Henry, trad. S. Johansson</aut>,
          <vrk><url ref="http://esperanto-ondo.ru/Tl-ang06.htm">
          Policisto kaj himno</url></vrk>,
          <lok>La Ondo de Esperanto. 2002:11 (97)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Stockente</trd>
    <trd lng="en">mallard</trd>
    <trdgrp lng="fr"><trd>colvert</trd>, <trd>canard
    colvert</trd></trdgrp>
    <trd lng="pl"><klr>kaczka</klr> krzy&zdot;&oacute;wka</trd>
    <trd lng="sv">gr&auml;a<ind>and</ind></trd>
  </snc>
</drv>

<drv mrk="anas.pintvost0o">
  <kap>pintvost<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>W. F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:zoologiaj_specioj">Specio</ref>
       de <ref tip="super" cel="anas.0o.ZOO">anaso</ref>
      (<trd lng="la">Anas acuta</trd>).
    </dif>
    <trd lng="de">Spie&szlig;ente</trd>
    <trd lng="en"><klr>northern</klr> pintail</trd>
    <trd lng="fr"><ind>canard</ind> pilet</trd>
    <trd lng="pl">ro&zdot;eniec</trd>
    <trd lng="sv">stj&auml;rt<ind>and</ind></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>ostralka obecn&aacute;</trd>,
    <trd>ostralka &scaron;t&iacute;hl&aacute;</trd>
  </trdgrp>
  <trd lng="sk">ka&ccaron;ica ostrochvost&aacute;</trd>
</drv>

<drv mrk="anas.mol0o.regxa">
  <kap>re&gcirc;a mol<tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>W. F. Pilger</aut>,
       <vrk>
