<drv mrk="anas.mol0o">
  <kap><ofc>*</ofc>mol<tld/>o
    <fnt><bib>F</bib>,
      <lok>Universala Vortaro,
      <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5115"
      >mol'anas'</url></lok>
    </fnt>
  </kap>
  <snc mrk="anas.mol0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>W. F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdhvu8.htm">
           Birdonomoj en Esperanto por Hejma Vortaro</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> 
      el la familio <ref tip="malprt"
      cel="anased.0oj"><tld/>edoj</ref>
      (i.a. <trd lng="la">Somateria</trd>).
    </dif>
    <trd lng="en">eider-<ind>duck</ind></trd>
    <trd lng="fr">eider</trd>
    <trd lng="hu">pehelyr&eacute;ce</trd>
    <trd lng="nl">eidereend</trd>
    <trd lng="sv">ejder</trd>
  </snc>
  <snc mrk="anas.mol0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>W. F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" 
      lst="voko:zoologiaj_specioj">Specio</ref>
       de <ref tip="super"
      cel="anas.mol0o.KOMUNE">mol<tld/>o</ref>
      (<trd lng="la">Somateria mollissima</trd>):
      <ekz>
        la ju&gcirc;istoj sidis en siaj apogse&gcirc;oj kaj havis malanta&ubreve; la kapo kusenojn el
        lanugo de mol<tld/>oj, &ccirc;ar ili devis multe pensi
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> dorlotas siajn kadukajn membrojn en kusenoj el lanugo de
        mol<tld/>oj, dum lia granda, admirinda filo suferas mizeron
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nesto de la mol<tld/>o estas konstruata &ccirc;e la maro kaj kovrita el la fama
        lanugo, plukita el la ina brusto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8547"
              >Molanaso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/2/27/Somateria_mollissima_Eiderente_2.jpg/640px-Somateria_mollissima_Eiderente_2.jpg"
      prm="CC-BY-SA-3.0"
      >mol<tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Somateria_mollissima_Eiderente_2.jpg"
              >https://commons.wikimedia.org/wiki/File:Somateria_mollissima_Eiderente_2.jpg</url></lok>
        </fnt>
    </bld>
    <ref tip="sin" cel="somateri.0o.ZOO">somaterio</ref>
    <trd lng="en"><klr>common</klr> eider</trd>
    <trd lng="fr"><ind>eider</ind> &agrave; duvet</trd>
    <trd lng="pl">edredon</trd>
    <trd lng="sv">ejder</trd>
  </snc>
  <trd lng="br">eider</trd>
  <trdgrp lng="cs">
    <trd>Kajka obecn&aacute;</trd>,
    <trd>kajka</trd>,
    <trd>kajka mo&rcaron;sk&aacute;</trd>
  </trdgrp>
  <trd lng="de">Eiderente</trd>
  <trd lng="nl">eidereend</trd>
  <trd lng="ru">&c_g;&c_a;&c_g;&c_a;</trd>
  <trd lng="sk">kajka</trd>
