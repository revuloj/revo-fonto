<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>anas</rad>/o
</kap>
<drv mrk="anas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="anas.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Na&gcirc;birdo el la familio
      <ref tip="super" cel="anased.0oj"><tld/>edoj</ref>, de
      diversaj genroj.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_ch;&c_k;&c_a;</trd>
    <trd lng="br">houad</trd>
    <trd lng="cs">kachna</trd>
    <trd lng="de">Ente</trd>
    <trd lng="es">pato</trd>
    <trd lng="fr">canard</trd>
    <trd lng="hu">kacsa</trd>
    <trd lng="nl">eend</trd>
    <trd lng="it">anatra</trd>
    <trd lng="pl">kaczka</trd>
    <trd lng="ru">&c_u;&c_t;&c_k;&c_a;</trd>
    <trd lng="sk">ka&ccaron;ica</trd>
    <trdgrp lng="sv">
      <trd>and</trd>,
      <trd>anka</trd>
    </trdgrp>
    <trd lng="tr">&ouml;rdek</trd>
  </snc>
  <snc mrk="anas.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el la familio <ref tip="super" cel="anased.0oj"><tld/>edoj</ref>
      (<trd lng="la">Anas</trd>):
      <ekz>
        trapasante la facilajn rapidfluejojn de Enco, ni vidis plurfoje
        <tld/>ojn kun belaj oran&gcirc;aj bekoj<fnt><aut>Werner Schad</aut>:
        <vrk>Tra la Andoj boate</vrk>, <bib>Monato</bib><lok>jaro 2001a,
        numero 12a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="../bld/anas.jpg">
      <ind>platbeka <tld/>o</ind> &dash; paro
      <trd lng="br">houad-korz</trd>
      <trd lng="cs">kachna divok&aacute;</trd>
      <trd lng="de">Stockente</trd>
      <trd lng="en">mallard</trd>
      <trd lng="fr">colvert</trd>
      <trd lng="he">&bet;&resh;&kaf;&yod;&yod;&he;</trd>
      <trd lng="hu">t&odblac;k&eacute;s r&eacute;ce</trd>
      <trd lng="la">Anas platyrhynchos</trd>
      <trd lng="pl">krzy&zdot;&oacute;wka</trd>
      <trd lng="ru">&c_k;&c_r;&c_ja;&c_k;&c_v;&c_a;</trd>
    </bld>
  </snc>
  <trd lng="he">&bet;&resh;&vav;&vav;&zayin;</trd>
</drv>

<drv mrk="anas.tuf0o">
  <kap>tuf<tld/>o <fnt><bib>Bird</bib></fnt></kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc>
    <ref tip="dif" cel="fuligu.0o">fuligulo</ref>
  </snc>
  <trdgrp lng="br">
    <trd>houad du</trd>,
    <trd>kanig du<klr> [komune]</klr></trd>
  </trdgrp>
  <trd lng="fr">fuligule</trd>
  <trd lng="hu">kontyos r&eacute;ce</trd>
  <trd lng="pl">czernica</trd>
  <trd lng="ru">&c_h;&c_o;&c_h;&c_l;&c_a;&c_t;&c_a;&c_ja;
  &c_ch;&c_je;&c_r;&c_n;&c_je;&c_t;&c_mol;</trd>
</drv>
</art>
