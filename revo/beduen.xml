<art mrk="$Id: beduen.xml,v 1.15 2021/03/04 20:23:07 revo Exp $">
<kap>
  <rad>beduen</rad>/o
</kap>
<drv mrk="beduen.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Araba <ref tip="super" cel="nomad.0o">nomado</ref>:
      <ekz>
        unu el la rozaj legendoj, kiujn la <tld/>oj kantas apud la &leftquot;Puto de Beno&rightquot; dum la
        plenigado de la kru&ccirc;oj, kiam la suno dormas en la brakoj de la vespero
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_d;&c_u;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_b;&c_je;&c_d;&c_u;&c_i;&c_n;</trd>
  <trd lng="cs">Bedu&iacute;n</trd>
  <trd lng="de">Beduine</trd>
  <trd lng="en">beduin</trd>
  <trd lng="es">beduino</trd>
  <trd lng="fa">&a_b;&a_fatha;&a_d;&a_fatha;&a_w;&f_y;</trd>
  <trd lng="fr">b&eacute;douin</trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B6;&#x5D3;&#x5D5;&#x5BC;&#x5D0;&#x5B4;&#x5D9;</trd>
  <trd lng="hu">beduin</trd>
  <trd lng="nl">bedoe&iuml;en</trd>
  <trd lng="pl">Beduin</trd>
  <trd lng="ru">&c_b;&c_je;&c_d;&c_u;&c_i;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>Bedu&iacute;n</trd>,
    <trd>bedu&iacute;n</trd>
  </trdgrp>
</drv>
</art>

