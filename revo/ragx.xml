<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ragx.xml,v 1.16 2024/03/08 12:38:03 revo Exp $">
<kap>
  <rad>ra&gcirc;</rad>/o  <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="&jcirc;a">ra&jcirc;a</rad>/o <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="ragx.0o">
  <kap><tld/>o, <var><kap><tld var="&jcirc;a"/>o</kap></var></kap>
  <snc>
    <dif>
      <ref tip="super" cel="princ.0o">Princa</ref> titolo
      en <ref tip="malprt" cel="hind.H0ujo">Hindujo</ref>:
      <ekz>
         la&ubreve; la tradicio, lia patro, &Scirc;udodano, estis <tld/>o de la &scirc;tato
         &Scirc;akjujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26152">Budho
          Gotamo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ok elefantoj kondukas lian tendon de oro kaj lino,
        kaj dudeko da <tld var="&jcirc;a"/>oj sekvas lin
        disputante inter si la honoron kunporti lian opalan sagujon
        <fnt>
          <aut>G. A. B&eacute;cquer, trad. A. M. Botella</aut>
          <vrk>La militestro de la ru&gcirc;aj manoj</vrk>
          <lok>en: Hispana Lirika Antologio, 2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_zh;&c_a;</trd>
  <trd lng="bg">&c_r;&c_a;&c_d;&c_zh;&c_a;</trd>
  <trd lng="ca">raja</trd>
  <trd lng="de">Radscha</trd>
  <trdgrp lng="en">
    <trd>raja</trd>,
    <trd>rajah</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>raja</trd>,
    <trd>rajah</trd>
  </trdgrp>
  <trd lng="hu">r&aacute;dzsa</trd>
  <trd lng="ja">&#x30E9;&#x30FC;&#x30B8;&#x30E3;</trd>
  <trd lng="nl">radja</trd>
  <trd lng="pl">rad&zdot;a</trd>
  <trd lng="pt">raj&aacute;</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_zh;&c_a;</trd>
  <trd lng="uk">&c_r;&c_a;&c_d;&c_zh;&c_a; (&c_t;&c_i;&c_t;&c_u;&#x43B;)</trd>
</drv>
</art>
