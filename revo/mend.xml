<drv mrk="mend.antaux0i">
  <kap>
    anta&ubreve;<tld/>i
  </kap>
  <snc>
    <dif>
      <tld/>i ion anta&ubreve; kiam &gcirc;i estas liverebla, 
      eventuale kun <ref cel="rabat.0o">rabato</ref>:
      <ekz>
        al &ccirc;iu familio estis permesite anta&ubreve;<tld/>i maksimume 30&#x202F;000 brikojn &ndash; sufi&ccirc;an
        kvanton por unu domo
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2011/010245.php">Floras enklavo komunista</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        necesas anta&ubreve;<tld/>i la man&gcirc;ojn, bonvolu anonci pri tio anticipe
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 1 (63)</lok>
        </fnt>;
      </ekz>
      <ekz>
        oportuna momento por anta&ubreve;<tld/>i la libron favorpreze
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;5</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="rezerv.0i.poriu">rezervi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; &c_z;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ja;</trd>
  <trd lng="de">vorbestellen</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43F;&c_je;&c_r;&c_je;&c_d;&#x43D;&#x44C;&#x43E; &c_z;&c_a;&#x43C;&#x43E;&c_v;&#x43B;&#x44F;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43F;&#x43E;&#x43F;&c_je;&c_r;&c_je;&c_d;&#x43D;&c_jeu;
    &c_z;&c_a;&#x43C;&#x43E;&c_v;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_d;&#x43F;&#x43B;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9810;&#x8A02; <pr>y&#xF9;d&#xEC;ng</pr></trd>,
    <trd>&#x9884;&#x8BA2; <pr>y&#xF9;d&#xEC;ng</pr></trd>
  </trdgrp>
</drv>






<drv mrk="mend.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="mend.mal0i.KOMUNE">
    <dif>
      Nuligi <tld/>on:
      <ekz>
        per la sama formularo vi povas anka&ubreve; <tld/>i kaj
        mal<tld/>i diversajn servojn;
      </ekz>
      <ekz>
        lastahore mi devis mal<tld/>i pro malsano;
      </ekz>
      <ekz>
        mi decidis mal<tld/>i mian voja&gcirc;on en landon kiu tiom malvolas mian venon
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 7 (57)</lok>
        </fnt>;
      </ekz>
      <ekz>
        per la mal<tld/>o mi perdis pli da mono ol necesis krompagi
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 7 (57)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;
  <ind>&c_z;&c_a;&c_k;&c_a;&c_z;</ind></trd>
  <trd lng="bg">&c_a;&c_n;&c_u;&c_l;&c_i;&c_r;&c_a;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>anulovat objedn&aacute;vku</trd>,
    <trd>zru&scaron;it objedn&aacute;vku</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>abbestellen</trd>,
    <trd>stornieren</trd>
  </trdgrp>
  <trd lng="en">cancel an order</trd>
  <trdgrp lng="es">
    <trd>anular una reserva</trd>,
    <trd>anular un pedido</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_f;&a_A;&a_r;&a_sh;<klr tip="amb">
  (&f_ch;&f_y;&a_z;&f_y; &a_r;&a_A;)</klr>
  &f_p;&a_s;&zwnj;&f_g;&a_r;&a_f;&a_t;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;commander</trd>,
    <trd>annuler une commande</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>visszamond <klr tip="ind">(megrendel&eacute;st)</klr></trd>,
    <trd>lemond <klr tip="ind">(megrendel&eacute;st)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x6CE8;&#x6587;&#x3092;&#x53D6;&#x6D88;&#x3059;  <pr>&#x3061;&#x3085;&#x3046;&#x3082;&#x3093;&#x3092;&#x3068;&#x308A;&#x3051;&#x3059;</pr></trd>
  <trd lng="nl">afbestellen</trd>
  <trdgrp lng="pl">
    <trd>zam&oacute;wienie anulowa&cacute;</trd>
    <trd>odm&oacute;wi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>anuleaz&abreve; comanda</trd>,
    <trd>refuza</trd>,
    <trd>contramanda</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_t;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;
  &c_z;&c_a;&c_k;&c_a;&c_z;</trd>
  <trd lng="sk">odvola&tcaron; alebo zru&scaron;i&tcaron; objedn&aacute;vku</trd>
