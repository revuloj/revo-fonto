<drv mrk="mend.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mend.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="postul.0i.iu">Postuli</ref>, ke iu faru 
      ian poste pagotan servon a&ubreve; liveru
      ian poste pagotan komerca&jcirc;on:
      <ekz>
        la veninto <klr>[&#x2026;]</klr> eltiris du orajn monerojn kaj <tld/>is bonan
        vesperman&gcirc;on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#x2026;]</klr> <tld/>is por li novajn vestojn, &ccirc;ar liaj estis tute eluzitaj
        dum la voja&gcirc;o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is edzi&gcirc;ofestan veston, a&ccirc;etis edzi&gcirc;ofestajn kandelojn
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo sendis kelke da rajdistoj <tld/>i &scirc;ipojn sur la rivero
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        evidente venis jam la tempo <tld/>i &ccirc;erkojn kaj kruci la manojn sur la brusto
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>X</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hotela pordisto demandis al <nom>Brown</nom>: &leftquot;&ccirc;u vi jam mendis la &ccirc;ambrojn, sinjoro?&rightquot;
        <fnt>
          <bib>TAA</bib>
          <lok>&Ccirc;apitro III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        lernolibroj ne ekzistis, kaj <tld/>i ilin el eksterlando ne estis eble, &ccirc;ar la
        limoj estis ankora&ubreve; fermitaj
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        al pentristo <nom>Cranach</nom> oni <tld/>is portreton 
        de kardinalo <nom>Albrecht de Brandenburg</nom>
        <fnt>
          <bib>KFl</bib>
          <lok>V. Pri unueco kaj lukto, kaj anka&ubreve; pri &rightquot;tipeco de l&#x2019; disdivido&#x201D;</lok>
        </fnt>;
      </ekz>
      <ekz>
        la redaktoro <tld/>is al la poeto grandan kontra&ubreve;religian poemon por la sekva
        numero de la revuo
        <fnt>
          <bib>MkM</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u atendu
        <fnt>
          <bib>GDn</bib>
          <lok>AKTO DUA</lok>
        </fnt>
        <klr>(vi senfine atendos, vd <ref cel="kler.0a">klera</ref>)</klr>;
      </ekz>
      <ekz>
        sciencon oni ne <tld/>as, klerecon oni ne vendas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    <ind>&c_z;&c_a;&c_k;&c_a;&c_z;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_r;&c_malmol;&c_ch;&c_v;&c_a;&c_m;</trd>
  <trd lng="bo">&t_ba;&t_ka;&t_naro;&t_da;&t_tsheg;&t_pa;&t_tsheg;&t_ga;&t_ta;&t_naro;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>objednat</trd>,
    <trd>sv&ecaron;&rcaron;it proveden&iacute;</trd>,
    <trd>zadat</trd>
  </trdgrp>
  <trd lng="de">bestellen</trd>
  <trd lng="en">order</trd>
  <trdgrp lng="es">
    <trd>hacer una reserva</trd>,
    <trd>hacer un pedido</trd>,
    <trd>encargar</trd>,
    <trd>pedir</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_f;&a_A;&a_r;&a_sh;
  &a_d;&a_A;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>commander<klr> (passer commande)</klr></trd>,
    <trd>r&eacute;server</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rendel <klr tip="amb">(&aacute;rut)</klr></trd>,
    <trd>megrendel</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6CE8;&#x6587;&#x3059;&#x308B;  <pr>&#x3061;&#x3085;&#x3046;&#x3082;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3042;&#x3064;&#x3089;&#x3048;&#x308B;</trd>,
    <trd>&#x4E88;&#x7D04;&#x3059;&#x308B;  <pr>&#x3088;&#x3084;&#x304F;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">bestellen</trd>
  <trdgrp lng="pl">
    <trd>zamawia&cacute;</trd>,
    <trd>zleca&cacute; <klr>(prac&eogonek;)</klr></trd>,
    <trd>obstalowa&cacute; <klr>(przest.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>encomendar</trd>,
    <trd>pedir <klr tip="amb">(mercadoria)</klr></trd>,
    <trd>mandar fazer</trd>,
    <trd>reservar <klr tip="amb">(quarto, mesa)</klr></trd>    
  </trdgrp>
  <trdgrp lng="ro">
    <trd>cere</trd>,
    <trd>ordona</trd>,
    <trd>&icirc;ntreba</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">objedna&tcaron;</trd>
  <trd lng="uk">&c_z;&c_a;&#x43C;&#x43E;&c_v;&#x43B;&#x44F;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x8A02;&#x8CA8; <pr>d&#xEC;nghu&#xF2;</pr></trd>,
    <trd>&#x8BA2;&#x8D27; <pr>d&#xEC;nghu&#xF2;</pr></trd>,
    <trd>&#x8A02;&#x8CFC; <pr>d&#xEC;ngg&#xF2;u</pr></trd>,
    <trd>&#x8BA2;&#x8D2D; <pr>d&#xEC;ngg&#xF2;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mend.0o">
  <kap><tld/>o</kap>
  <snc mrk="mend.0o.KOMUNE">
    <dif>
      Ago <tld/>i, postulo de varoj a&ubreve; servo:
      <ekz>
        esperante viajn balda&ubreve;ajn <tld/>ojn, mi restas serveme-verseme 
        <nom>Raymond Schwartz</nom>, Butikisto
        <fnt>
          <bib>LSB</bib>
          <lok>La Stranga Butiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la laboro, kiun vi nun faros, estas provo, kiu decidos pri pluaj <tld/>oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la laboro malfrui&gcirc;as, la <tld/>oj ne estas plenumataj &gcirc;ustatempe
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        krei vivipovan lingvon, <klr>[&#x2026;]</klr> oni ne povas tion fari la&ubreve; <tld/>o en la
        da&ubreve;ro de kelke da semajnoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la vetero estis kvaza&ubreve; la&ubreve; <tld/>o <klr>(favora)</klr>
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili akiris rapidan famon, la mendoj alfluis
        <fnt>
          <bib>ChL</bib>
          <lok>Noto De L&#x2019; A&ubreve;toro</lok>
        </fnt>;
      </ekz>
      <ekz>
        se viaj liveroj estos kontentigaj, kiel mi esperas, mi sendos al vi pli grandajn
        <tld/>ojn
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_m;&c_o;&c_v;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_a;&c_z;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_ngata;&t_naro;&t_na;&t_tsheg;&t_na;&t_sa;&t_tsheg;&t_ga;&t_ta;&t_na;&t_tsheg;&t_kha;&t_drengbu;&t_la;&t_tsheg;&t_ba;&t_yata;&t_drengbu;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="de">Bestellung</trd>
  <trdgrp lng="es">
    <trd>comanda</trd>,
    <trd>encargo</trd>,
    <trd>pedido</trd>
  </trdgrp>
  <trd lng="fr">commande</trd>
  <trdgrp lng="ja">
    <trd>&#x6CE8;&#x6587;  <pr>&#x3061;&#x3085;&#x3046;&#x3076;&#x3093;</pr></trd>,
    <trd>&#x767A;&#x6CE8;  <pr>&#x306F;&#x3063;&#x3061;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x4E88;&#x7D04;  <pr>&#x3088;&#x3084;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zlecenie</trd>,
    <trd>anga&zdot;</trd>,
    <trd>rezerwacja</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>comision</trd>,
    <trd>rezervare</trd>,
    <trd>angajament</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_a;&#x43C;&#x43E;&c_v;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x8A02;&#x55AE; <pr>d&#xEC;ngd&#x101;n</pr></trd>,
    <trd>&#x8BA2;&#x5355; <pr>d&#xEC;ngd&#x101;n</pr></trd>,
    <trd>&#x8A02;&#x8CA8; <pr>d&#xEC;nghu&#xF2;</pr></trd>,
    <trd>&#x8BA2;&#x8D27; <pr>d&#xEC;nghu&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mend.0ilo">
