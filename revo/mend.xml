<drv mrk="mend.antaux0i">
  <kap>
    anta&ubreve;<tld/>i
  </kap>
  <snc>
    <dif>
      <tld/>i ion anta&ubreve; kiam &gcirc;i estas liverebla,
      eventuale kun <ref cel="rabat.0o">rabato</ref>:
      <ekz>
        al &ccirc;iu familio estis permesite anta&ubreve;<tld/>i maksimume 30&#8239;000 brikojn &ndash; sufi&ccirc;an
        kvanton por unu domo
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2011/010245.php">Floras enklavo komunista</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        necesas anta&ubreve;<tld/>i la man&gcirc;ojn, bonvolu anonci pri tio anticipe
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 1 (63)</lok>
        </fnt>;
      </ekz>
      <ekz>
        oportuna momento por anta&ubreve;<tld/>i la libron favorpreze
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;5</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="rezerv.0i.poriu">rezervi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_m;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;
  &c_z;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ja;</trd>
  <trd lng="ca">encarregar abans de l&#8217;edici&oacute; o la producci&oacute;</trd>
  <trd lng="de">vorbestellen</trd>
  <trdgrp lng="id">
    <trd><baz>laku</baz>melakukan prapesan</trd>,
    <trd><baz>prapesan</baz>memprapesan</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_d;&c_n;&c_mol;&c_o; &c_z;&c_a;&c_m;&c_o;&c_v;&c_l;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_p;&c_o;&c_p;&c_je;&c_r;&c_je;&c_d;&c_n;&c_jeu;
    &c_z;&c_a;&c_m;&c_o;&c_v;&c_l;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_p;&c_l;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38928;&#35330; <pr>y&ugrave;d&igrave;ng</pr></trd>,
    <trd>&#39044;&#35746; <pr>y&ugrave;d&igrave;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mend.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="mend.mal0i.KOMUNE">
    <dif>
      Nuligi <tld/>on:
      <ekz>
        per la sama formularo vi povas anka&ubreve; <tld/>i kaj
        mal<tld/>i diversajn servojn;
      </ekz>
      <ekz>
        lastahore mi devis mal<tld/>i pro malsano;
      </ekz>
      <ekz>
        mi decidis mal<tld/>i mian voja&gcirc;on en landon kiu tiom malvolas mian venon
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 7 (57)</lok>
        </fnt>;
      </ekz>
      <ekz>
        per la mal<tld/>o mi perdis pli da mono ol necesis krompagi
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 7 (57)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;
  <ind>&c_z;&c_a;&c_k;&c_a;&c_z;</ind></trd>
  <trd lng="bg">&c_a;&c_n;&c_u;&c_l;&c_i;&c_r;&c_a;&c_n;&c_je;</trd>
  <trdgrp lng="ca">
    <trd>anul&middot;lar una comanda</trd>,
    <trd>anul&middot;lar una reserva</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>anulovat objedn&aacute;vku</trd>,
    <trd>zru&scaron;it objedn&aacute;vku</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>abbestellen</trd>,
    <trd>stornieren</trd>
  </trdgrp>
  <trd lng="en">cancel an order</trd>
  <trdgrp lng="es">
    <trd>anular una reserva</trd>,
    <trd>anular un pedido</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_f;&a_A;&a_r;&a_sh;<klr tip="amb">
  (&f_ch;&f_y;&a_z;&f_y; &a_r;&a_A;)</klr>
  &f_p;&a_s;&#8204;&f_g;&a_r;&a_f;&a_t;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;commander</trd>,
    <trd>annuler une commande</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>visszamond <klr tip="ind">(megrendel&eacute;st)</klr></trd>,
    <trd>lemond <klr tip="ind">(megrendel&eacute;st)</klr></trd>
  </trdgrp>
