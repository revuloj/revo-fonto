<drv mrk="mend.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mend.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="postul.0i.iu">Postuli</ref>, ke iu faru
      ian poste pagotan servon a&ubreve; liveru
      ian poste pagotan komerca&jcirc;on:
      <ekz>
        la veninto <klr>[&#8230;]</klr> eltiris du orajn monerojn kaj <tld/>is bonan
        vesperman&gcirc;on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#8230;]</klr> <tld/>is por li novajn vestojn, &ccirc;ar liaj estis tute eluzitaj
        dum la voja&gcirc;o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is edzi&gcirc;ofestan veston, a&ccirc;etis edzi&gcirc;ofestajn kandelojn
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo sendis kelke da rajdistoj <tld/>i &scirc;ipojn sur la rivero
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        evidente venis jam la tempo <tld/>i &ccirc;erkojn kaj kruci la manojn sur la brusto
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>X</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hotela pordisto demandis al <nom>Brown</nom>: &leftquot;&ccirc;u vi jam mendis la &ccirc;ambrojn,
        sinjoro?&rightquot;
        <fnt>
          <bib>TAA</bib>
          <lok>&Ccirc;apitro III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        lernolibroj ne ekzistis, kaj <tld/>i ilin el eksterlando ne estis eble, &ccirc;ar la
        limoj estis ankora&ubreve; fermitaj
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        al pentristo <nom>Cranach</nom> oni <tld/>is portreton
        de kardinalo <nom>Albrecht de Brandenburg</nom>
        <fnt>
          <bib>KFl</bib>
          <lok>V. Pri unueco kaj lukto, kaj anka&ubreve; pri &rightquot;tipeco de l&#8217; disdivido&#8221;</lok>
        </fnt>;
      </ekz>
      <ekz>
        la redaktoro <tld/>is al la poeto grandan kontra&ubreve;religian poemon por la sekva
        numero de la revuo
        <fnt>
          <bib>MkM</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u atendu
        <fnt>
          <bib>GDn</bib>
          <lok>AKTO DUA</lok>
        </fnt>
        <klr>(vi senfine atendos, vd <ref cel="kler.0a">klera</ref>)</klr>;
      </ekz>
      <ekz>
        sciencon oni ne <tld/>as, klerecon oni ne vendas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
    <ind>&c_z;&c_a;&c_k;&c_a;&c_z;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_r;&c_malmol;&c_ch;&c_v;&c_a;&c_m;</trd>
  <trd
  lng="bo">&t_ba;&t_ka;&t_naro;&t_da;&t_tsheg;&t_pa;&t_tsheg;&t_ga;&t_ta;&t_naro;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>encarregar</trd>,
    <trd>encomanar</trd>,
    <trd>demanar</trd>,
    <trd>reservar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>objednat</trd>,
    <trd>sv&ecaron;&rcaron;it proveden&iacute;</trd>,
    <trd>zadat</trd>
  </trdgrp>
  <trd lng="de">bestellen</trd>
  <trd lng="en">order</trd>
  <trdgrp lng="es">
    <trd>hacer una reserva</trd>,
    <trd>hacer un pedido</trd>,
