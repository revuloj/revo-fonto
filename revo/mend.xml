    <trd>pedir</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_f;&a_A;&a_r;&a_sh;
  &a_d;&a_A;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>commander<klr> (passer commande)</klr></trd>,
    <trd>r&eacute;server</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rendel <klr tip="amb">(&aacute;rut)</klr></trd>,
    <trd>megrendel</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>pesan</baz>memesan</trd>,
    <trd><baz>order</baz>mengorder</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#27880;&#25991;&#12377;&#12427;  <pr>&#12385;&#12421;&#12358;&#12418;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#12354;&#12388;&#12425;&#12360;&#12427;</trd>,
    <trd>&#20104;&#32004;&#12377;&#12427;  <pr>&#12424;&#12420;&#12367;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">bestellen</trd>
  <trdgrp lng="pl">
    <trd>zamawia&cacute;</trd>,
    <trd>zleca&cacute; <klr>(prac&eogonek;)</klr></trd>,
    <trd>obstalowa&cacute; <klr>(przest.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>encomendar</trd>,
    <trd>pedir <klr tip="amb">(mercadoria)</klr></trd>,
    <trd>mandar fazer</trd>,
    <trd>reservar <klr tip="amb">(quarto, mesa)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>cere</trd>,
    <trd>ordona</trd>,
    <trd>&icirc;ntreba</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">objedna&tcaron;</trd>
  <trd lng="uk">&c_z;&c_a;&c_m;&c_o;&c_v;&c_l;&c_ja;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#35330;&#36008; <pr>d&igrave;nghu&ograve;</pr></trd>,
    <trd>&#35746;&#36135; <pr>d&igrave;nghu&ograve;</pr></trd>,
    <trd>&#35330;&#36092; <pr>d&igrave;ngg&ograve;u</pr></trd>,
    <trd>&#35746;&#36141; <pr>d&igrave;ngg&ograve;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mend.0o">
  <kap><tld/>o</kap>
  <snc mrk="mend.0o.KOMUNE">
    <dif>
      Ago <tld/>i, postulo de varoj a&ubreve; servo:
      <ekz>
        esperante viajn balda&ubreve;ajn <tld/>ojn, mi restas serveme-verseme
        <nom>Raymond Schwartz</nom>, Butikisto
        <fnt>
          <bib>LSB</bib>
          <lok>La Stranga Butiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la laboro, kiun vi nun faros, estas provo, kiu decidos pri pluaj <tld/>oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la laboro malfrui&gcirc;as, la <tld/>oj ne estas plenumataj &gcirc;ustatempe
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        krei vivipovan lingvon, <klr>[&#8230;]</klr> oni ne povas tion fari la&ubreve; <tld/>o en la
        da&ubreve;ro de kelke da semajnoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la vetero estis kvaza&ubreve; la&ubreve; <tld/>o <klr>(favora)</klr>
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili akiris rapidan famon, la mendoj alfluis
        <fnt>
          <bib>ChL</bib>
          <lok>Noto De L&#8217; A&ubreve;toro</lok>
        </fnt>;
      </ekz>
      <ekz>
        se viaj liveroj estos kontentigaj, kiel mi esperas, mi sendos al vi pli grandajn
        <tld/>ojn
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_m;&c_o;&c_v;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_a;&c_z;</trd>
  </trdgrp>
  <trd
  lng="bo">&t_sa;&t_ngata;&t_naro;&t_na;&t_tsheg;&t_na;&t_sa;&t_tsheg;&t_ga;&t_ta;&t_na;&t_tsheg;&t_kha;&t_drengbu;&t_la;&t_tsheg;&t_ba;&t_yata;&t_drengbu;&t_da;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>comanda</trd>,
    <trd>enc&agrave;rrec</trd>,
    <trd>reserva</trd>
  </trdgrp>
  <trd lng="de">Bestellung</trd>
  <trdgrp lng="es">
    <trd>comanda</trd>,
    <trd>encargo</trd>,
    <trd>pedido</trd>
  </trdgrp>
  <trd lng="fr">commande</trd>
  <trdgrp lng="id">
    <trd>pesanan</trd>,
    <trd>order</trd>,
    <trd>orderan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#27880;&#25991;  <pr>&#12385;&#12421;&#12358;&#12406;&#12435;</pr></trd>,
    <trd>&#30330;&#27880;  <pr>&#12399;&#12387;&#12385;&#12421;&#12358;</pr></trd>,
    <trd>&#20104;&#32004;  <pr>&#12424;&#12420;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zlecenie</trd>,
    <trd>anga&zdot;</trd>,
    <trd>rezerwacja</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>comision</trd>,
    <trd>rezervare</trd>,
    <trd>angajament</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_a;&c_m;&c_o;&c_v;&c_l;&c_je;&c_n;&c_n;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#35330;&#21934; <pr>d&igrave;ngd&amacron;n</pr></trd>,
    <trd>&#35746;&#21333; <pr>d&igrave;ngd&amacron;n</pr></trd>,
    <trd>&#35330;&#36008; <pr>d&igrave;nghu&ograve;</pr></trd>,
    <trd>&#35746;&#36135; <pr>d&igrave;nghu&ograve;</pr></trd>
  </trdgrp>
</drv>

