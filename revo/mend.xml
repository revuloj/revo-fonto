<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>mend</rad>/i
    </kap>

    <drv mrk="mend.0i">
      <kap>
        <ofc>*</ofc><tld/>i
      </kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Postuli, ke iu faru ian poste pagotan servon a&ubreve; liveru ian poste
          pagotan komerca&jcirc;on:
          <ekz>
            <tld/>i veston al tajloro;
          </ekz><ekz>
            <tld/>i karbon, meblojn;
          </ekz><ekz>
            <tld/>i &ccirc;ambron en hotelo;
          </ekz><ekz>
            <tld/>i kotleton en restoracio<fnt>B</fnt>;
          </ekz><ekz>
            fari <tld/>on;
          </ekz><ekz>
            plenumi <tld/>on<fnt>Z</fnt>;
          </ekz><ekz>
            <tld/>u atendu<fnt>Z</fnt>
            <klr>(vi senfine atendos. <ref tip="vid"
                cel="kler.0a">klera</ref>)</klr>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_z;&c_a;&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_z;&c_a;&c_k;&c_a;&c_z;</ind></trd>
      </trdgrp>
      <trd lng="en">order</trd>
      <trdgrp lng="hu">
        <trd>rendel <klr tip="amb">(&aacute;rut)</klr></trd>,
        <trd>megrendel</trd>
      </trdgrp>
      <trd lng="nl">bestellen</trd>
      <trd lng="de">bestellen</trd>
      <trdgrp lng="pt">
        <trd>encomendar</trd>,
        <trd>pedir <klr tip="amb">(mercadoria)</klr></trd>,
        <trd>mandar fazer</trd>,
        <trd>reservar <klr tip="amb">(quarto, mesa)</klr></trd>    
      </trdgrp>
      <trd lng="ru">&c_z;&c_a;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="mend.mal0i">
      <kap>mal<tld/>i</kap>
      <snc>
        <dif>
          Nuligi <tld/>on:
          <ekz>
            per la sama formularo vi povas anka&ubreve; <tld/>i kaj
            mal<tld/>i diversajn servojn;
          </ekz><ekz>
            lastahore mi devis mal<tld/>i pro malsano;
          </ekz><ekz>
            pro neveno de la gasto ili mal<tld/>is la tagman&gcirc;on
            en la restoracio.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_s;&c_k;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;
<ind>&c_z;&c_a;&c_k;&c_a;&c_z;</ind></trd>
      <trd lng="en">cancel an order</trd>
      <trdgrp lng="hu">
        <trd>visszamond <klr tip="ind">(megrendel&eacute;st)</klr></trd>,
        <trd>lemond <klr tip="ind">(megrendel&eacute;st)</klr></trd>
      </trdgrp>
      <trd lng="nl">afbestellen</trd>
      <trdgrp lng="de">
        <trd>abbestellen</trd>,
        <trd>stornieren</trd>
      </trdgrp>
      <trd lng="ru">&c_o;&c_t;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol; &c_z;&c_a;&c_k;&c_a;&c_z;</trd>
    </drv>

    <drv mrk="mend.kontraux0i">
      <kap>kontra&ubreve;<tld/>i</kap>
      <snc>
        <uzo tip="stl">ARK</uzo>
        <uzo tip="stl">EVI</uzo>
        <ref tip="dif" cel="mend.mal0i">Mal<tld/>i</ref>
      </snc>
    </drv>

    <drv mrk="mend.inform0o">
      <kap>inform<tld/>o</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Komando servanta por akiri informon el komputa sistemo
          (el operaciumo, el datumbanko, ofte en dialogo).
        </dif>
      </snc>
      <trdgrp lng="en">
        <trd>enquiry</trd>, <trd>inquiry</trd>, <trd>query</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Anfrage</trd>, <trd>Abfrage</trd>
      </trdgrp>
      <trd lng="pl">kwerenda</trd>
      <trd lng="ru">&c_z;&c_a;&c_p;&c_r;&c_o;&c_s;</trd>
    </drv>

  </art>
