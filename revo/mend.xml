<drv mrk="mend.inform0o">
  <kap>inform<tld/>o</kap>
  <snc mrk="mend.inform0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Komando servanta por akiri informon el komputa sistemo
      (el operaciumo, el datumbanko, ofte en dialogo):
      <ekz>
        &gcirc;iaj pa&gcirc;aroj troveblas <klr>[&#x2026;]</klr> ene de ser&ccirc;ilaj rezultpa&gcirc;oj la&ubreve;
        inform<tld/>oj pri lingvaj demandoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51340">Esperanto novosti</url></lok>
        </fnt>.
      </ekz>
    </dif>
<ref tip="vid" cel="sercx.0o">ser&ccirc;o</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>dotaz</trd>,
    <trd>dotazov&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Anfrage</trd>, <trd>Abfrage</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>enquiry</trd>, <trd>inquiry</trd>, <trd>query</trd>
  </trdgrp>
  <trd lng="es">consulta</trd>
  <trd lng="fa">&a_j;&a_w;&f_y;&a_sh;</trd>
  <trd lng="fr">requ&ecirc;te</trd>
  <trdgrp lng="pl">
    <trd>kwerenda</trd>,
    <trd>zapytanie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>interogare</trd>
    <trd>anchet&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_r;&c_o;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>dopyt</trd>,
    <trd>ot&aacute;zka</trd>
  </trdgrp>
</drv>
</art>
