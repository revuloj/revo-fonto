<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: trent.xml,v 1.5 2023/09/04 19:01:53 revo Exp $">
    <kap>
      <rad>Trent</rad>/o
    </kap>

<drv mrk="trent.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="trent.0o.urbo">
    <dif>
      Nord<ref tip="malprt" cel="ital.I0ujo">itala</ref> <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la valo de la rivero Adi&gcirc;o, 55&#x202F;km sude de Bolzano kaj 100&#x202F;km norde de Verono:
      <ekz>
        la koncilio de <tld/>o (1545-1563) ordonis, ke &ccirc;iujn baptojn, geedzi&gcirc;ojn kaj
        entombigojn oni devas noti en registroj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2006/008970.php">Genealogio</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        meze de la jaro 1475 fama &leftquot;a&ccirc;a&jcirc;o&rightquot; okazis en la urbo <tld/>o, la tuta juda loka
        komunumo <klr>[&#x2026;]</klr> estis akuzata pri malamega infanmortigo
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2007/009198.php">&Ccirc;u vere okazis sang-oferaj paskoj?</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Trient</trd>
    <trd lng="en">Trento</trd>
    <trd lng="fr">Trente</trd>
  </snc>
  <trd lng="be">&c_T;&c_r;&c_e;&c_n;&c_t;&c_a;</trd>
  <trd lng="ja">&#x30C8;&#x30EC;&#x30F3;&#x30C8;</trd>
  <trd lng="uk">&c_T;&c_r;&c_je;&#x43D;&c_t;&#x43E;</trd>
</drv>

  </art>

