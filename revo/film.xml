<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: film.xml,v 1.55 2025/10/16 04:35:06 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>film</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="film.0o">
  <kap><tld/>o</kap>
  <snc mrk="film.0o.KIN">
    <tezrad fak="KIN"/>
    <uzo tip="fak">KIN</uzo>
    <dif>
      Serio de movi&gcirc;aj bildoj, projekciebla sur ekranon:
      <ekz>
        <ind>senvo&ccirc;a <tld/>o</ind>
        <fnt>
          <bib>VaK</bib>
        </fnt>;
        <trd lng="de">Stummfilm</trd>
        <trd lng="id">film bisu</trd>
      </ekz>
      <ekz>
        <ind>son<tld/>o</ind>
        <fnt>
          <bib>Ret</bib>
        </fnt>;
        <trd lng="de">Tonfilm</trd>
        <trd lng="id">film suara</trd>
      </ekz>
      <ekz>
        en la popola domo oni montros gajan <tld/>on
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        2a</lok></fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; jam vidita <tld/>o
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        simile al kino<tld/>o rapidas miaj tagoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>,
      <trd>&c_k;&c_ib;&c_n;&c_a;&c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>,
      <trd>&c_k;&c_ib;&c_n;&c_a;&c_s;&c_t;&c_u;&c_zh;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>movie</trd>,
      <trd>film</trd>,
      <trd>motion picture</trd>
    </trdgrp>
    <trd lng="pl">film</trd>
    <trd lng="ru">&c_f;&c_i;&c_l;&c_mol;&c_m;</trd>
  </snc>
  <snc mrk="film.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref cel="impres.0ebla">Impresebla</ref> membrano el
      celuloido a&ubreve; el simila substanco uzata unuope por
      fotografado a&ubreve; en strio por <tld/>ado:
      <ekz>
        <klr>[en]</klr> la gazeta oficejo transdonu &ccirc;i tiujn <tld/>ojn, por ke ili rivelu la
        fotojn
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la solaj dokumentoj de <nom>NKVD</nom>, kiujn ni disponas <klr>[&#8230;]</klr> estas nun
        konsulteblaj
        sur mikro<tld/>o
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
    <adm>
      Sub "impresi" indus spliti ~igxema disde ~ebla. <aut>MB</aut>
    </adm>
    <trdgrp lng="be">
      <trd>&c_p;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_t;&c_u;&c_zh;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="en">film</trd>
    <trdgrp lng="pl">
      <trd>film</trd>,
      <trd>b&lstroke;ona<klr tip="amb"> fotograficzna</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="film.0o.tavolo">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Maldika tavolo a&ubreve; &scirc;elo, kovranta ion:
      <ekz>
        iriza benzina <tld/>o sur akvo;
      </ekz><ekz>
        celofana <tld/>o;
      </ekz><ekz>
        forcejo, kovrita per <tld/>o.
      </ekz>
    </dif>
    <ref tip="vid" cel="membra.0o">membrano</ref>
    <trd lng="be">&c_p;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>
    <trd lng="en">film</trd>
    <trdgrp lng="pl">
      <trd>film</trd>,
      <trd>b&lstroke;ona</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_l;&c_jo;&c_n;&c_k;&c_a;</trd>
  </snc>
  <trd lng="bo">&t_ga;&t_lata;&t_naro;&t_ga;&t_tsheg;&t_ba;&t_ra;&t_nyata;&t_na;&t_tsheg;</trd>
  <trd lng="cs">film</trd>
  <trd lng="de">Film</trd>
  <trd lng="en">film</trd>
  <trd lng="es">pel&iacute;cula</trd>
  <trdgrp lng="fr">
    <trd>film</trd>,
    <trd>feuille<klr tip="ind"> (film)</klr></trd>,
    <trd>pellicule<klr tip="ind"> (phot., mince couche)</klr></trd>
  </trdgrp>
  <trd lng="hu">film</trd>
  <trd lng="id">film</trd>
  <trdgrp lng="ja">
    <trd>&#12501;&#12451;&#12523;&#12512;  <pr>&#12405;&#12355;&#12427;&#12416;</pr></trd>,
    <trd>&#26144;&#30011;  <pr>&#12360;&#12356;&#12364;</pr></trd>
  </trdgrp>
  <trd lng="nl">film</trd>
  <trdgrp lng="pt">
    <trd>filme</trd>,
    <trd>pel&iacute;cula</trd>
  </trdgrp>
  <trd lng="sk">film</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_l;&c_ib;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_o;&c_p;&c_l;&c_ib;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_o;&c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#36575;&#29255; <pr>ru&#462;npi&agrave;n</pr></trd>,
    <trd>&#24863;&#20809;&#33014;&#29255; <pr>g&#462;ngu&amacron;ngji&amacron;opi&agrave;n</pr></trd>,
    <trd>&#24433;&#29255; <pr>y&#464;ngpi&agrave;n</pr></trd>,
    <trd>&#33014;&#29255; <pr>ji&amacron;opi&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="film.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="film.0i.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      Fari <tld/>on, registri per movbildoj:
      <ekz>
        <tld/>i dramon, komedion, scenon;
      </ekz>
      <ekz>
        <tld/>i eventon per videokamero
        <fnt>
          <bib>KFl</bib>
        </fnt>;
      </ekz>
      <ekz>
        preterpasantoj <klr>[&#8230;]</klr> mult-foje e&ccirc; fotis kaj
        <tld/>is la grupon
        <fnt>
        <aut>C. Dorneles Soares</aut>,
        <vrk>Ankora&ubreve; pri la Monda Socia Forumo</vrk>,
        <lok>soc.culture.esperanto, 2002-02-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_d;&c_y;&c_m;&c_a;&c_c;&c_mol;
  <klr>(&c_f;&c_ib;&c_l;&c_mol;&c_m;)</klr></trd>
  <trdgrp lng="cs">
    <trd>filmovat</trd>,
    <trd>nafilmovat</trd>
  </trdgrp>
  <trd lng="de">filmen</trd>
  <trdgrp lng="en">
    <trd>film</trd>,
    <trd>shoot (a film)</trd>
  </trdgrp>
  <trd lng="es">filmar</trd>
  <trd lng="fr">filmer</trd>
  <trd lng="hu">filmez</trd>
  <trdgrp lng="id">
    <trd><baz>film</baz>memfilmkan</trd>,
    <trd><baz>rekam</baz>merekam</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26144;&#30011;&#12395;&#12392;&#12427;  <pr>&#12360;&#12356;&#12364;&#12395;&#12392;&#12427;</pr></trd>,
    <trd>&#25774;&#24433;&#12377;&#12427;  <pr>&#12373;&#12388;&#12360;&#12356;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>filmowa&cacute;</trd>,
    <trd>kr&eogonek;ci&cacute; <ind>film</ind></trd>,
    <trd>nagrywa&cacute; <ind>film</ind></trd>,
    <trd>rejestrowa&cacute; <ind>film</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_n;&c_a;
  &c_k;&c_i;&c_n;&c_o;&c_p;&c_l;&c_jo;&c_n;&c_k;&c_u;)</klr></trd>
  <trdgrp lng="sk">
    <trd>filmova&tcaron;</trd>,
    <trd>nakr&uacute;ca&tcaron; film</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_n;&c_ib;&c_m;&c_a;&c_t;&c_i; &c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>
  <trdgrp lng="zh">
    <trd>&#25293;&#29255; <pr>p&amacron;ipi&amacron;n</pr></trd>,
    <trd>&#23556;&#20987; <pr>sh&egrave;j&iacute;</pr></trd>,
    <trd>&#24405;&#24433; <pr>l&ugrave;y&#464;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="film.0aro">
  <kap>
    <tld/>aro
  </kap>
  <snc>
    <dif>
     Kolekto da <tld/>oj:
     <ekz>
        La&ubreve;zana Esperanto-Societo proponas al vi <tld/>aron surrete
        <fnt>
          <bib>Kon11</bib>
          <lok>Reagoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="seri.film0o">filmserio</ref>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_l;&c_mol;&c_m;&c_a;&c_t;&c_e;&c_k;&c_a;</trd>
  <trd lng="de">Filmsammlung</trd>
  <trd lng="en">movie collection</trd>
<trd lng="id">koleksi film</trd>
</drv>

<drv mrk="film.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc mrk="film.0ejo.kinejo">
    <dif>
      <tld lit="F"/>spektejo, <ref tip="sin" cel="kin.0ejo">kinejo</ref> a&ubreve; repa&gcirc;o:
      <ekz>
        por ekscii pli multe pri tiuj <tld/>oj, vizitu la Verdan <tld lit="F"/>ejon
        <fnt>
          <bib>Kon11</bib>
          <lok>Esperanta kinarto vivas malgra&ubreve; la esperantistoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        tipa filmejo ne ekzistis, <nom>Chuck Smith</nom> montris kelkajn mojosajn <tld/>ojn en la
        lernejo
        <fnt>
          <bib>Kon11</bib>
          <lok>La IJK estas la homoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_ib;&c_n;&c_a;&c_t;&c_e;&c_a;&c_t;&c_a;&c_r;</trd>,
      <trd>&c_k;&c_ib;&c_n;&c_o;</trd>
    </trdgrp>
    <trd lng="de">Kino</trd>
    <trd lng="en">cinema</trd>
    <trd lng="fr">cin&eacute;ma</trd>
  <trd lng="id">bioskop</trd>
  </snc>
  <snc>
    <dif>
      <tld lit="F"/>farejo; <ref tip="super" cel="studi.0o.KOMUNE">studio</ref>:
      <ekz>
         &scirc;i per fera mano restarigas la ordon en la <tld/>ejo kaj majstre direktas la laboron
         <klr>[&#8230;]</klr> &scirc;i elver&scirc;as sur la <tld/>obendon siajn sentojn &ndash; timon, angoron, elrevi&gcirc;on,
         deziron
        <fnt>
          <bib>Kon11</bib>
          <lok>Filmo-recenzo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu speciala parko estas tre grava por la <tld/>industrio, &ccirc;ar &gcirc;i estas la sola
        <tld/>ejo en &Ccirc;inio
        <fnt>
          <bib>Monato</bib>
          <aut>Chen Yinhua</aut>
          <vrk><url ref="&Monato;/2008/009601.php"
              >Evoluanta filmejo Hengdian</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_ib;&c_n;&c_a;&c_s;&c_t;&c_u;&c_d;&c_y;&c_ja;</trd>
    <trd lng="de">Filmstudio</trd>
    <trd lng="en">film studio</trd>
  <trd lng="id">studio film</trd>
  </snc>
</drv>

<drv mrk="film.0eto">
  <kap>
    <tld/>eto
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="film.vide0o">Videfilmo</ref> tre mallonga, nur
      kelkminuta, kelksekunda:
      <ekz>
        oni povus facile informi milojn da homoj per blogoj, jutubaj <tld/>etoj kaj
        gazetoj
        <fnt>
          <bib>Kon11</bib>
          <lok>Cifereca mono</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej kaptis mian atenton <klr>[&#8230;]</klr> la filmeto de la hispana kantistino
        <nom>Mar&iacute;a Villal&oacute;n</nom>, <klr>[&#8230;]</klr> en kiu &scirc;i kantis tradukitan version de sia
        muzika&jcirc;o &leftquot;<nom>La lluvia</nom>&rightquot; (La pluvo)
        <fnt>
          <bib>Kon11</bib>
          <lok>Varsovia Vento - Facila</lok>
        </fnt>!
      </ekz>
      <ekz>
        sur ekranoj oni povis vidi fotojn kaj <tld/>etojn el iu rifu&gcirc;intejo
        <fnt>
          <bib>Kon11</bib>
          <lok>Aktive, interjunulare (Kirill Novi&ccirc;enko)</lok>
        </fnt>;
      </ekz>
      <ekz>
        kutime, tiaj aktor-agentejoj unue petas fotoalbumon, <tld/>etojn kaj
        vivprotokolon, kaj poste, se necese, organizas provludon
        <fnt>
          <bib>Kon11</bib>
          <lok>La Blaga Blogo</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="film.mallong0o">mallongfilmo</ref>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ib;&c_p;</trd>
  <trd lng="de">Videoclip</trd>
  <trd lng="en">clip vid&eacute;o</trd>
  <trd lng="fr">video clip</trd>
<trd lng="id">klip video</trd>
</drv>

<drv mrk="film.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="film.0ilo.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      <ref tip="super" cel="kamera.0o.FOT">Kamerao</ref> por registri
      <ref tip="vid" cel="film.0o.KIN"><tld/>on</ref>:
      <ekz>
        ne estas fotiloj kaj <tld/>iloj en la loko
        <fnt>
        <aut>Jaena Esperantisto</aut>,
          <vrk>Re: Usonanino estis barbare mortigita en la
          Gaza-strio</vrk>,
          <lok>soc.culture.esperanto, 2003-03-19</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iutage kelkaj bestoj venas tien, <klr>[&#8230;]</klr> indiferentaj pri tiuj homoj
        kiuj, kun fotilo a&ubreve; <tld/>ilo en la manoj, &ccirc;iumatene klopodas akiri la plej
        mirigajn bildojn
        <fnt>
          <bib>Monato</bib>
          <aut>Izabel C. O. Santiago</aut>
          <vrk>Fa&ubreve;no: La &leftquot;drakoj&rightquot; de Komodo: Gigantaj bestoj sur izola insulo</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_a;&c_k;&c_a;&c_m;&c_e;&c_r;&c_a;</trd>
  <trd lng="cs">filmov&aacute; kamera</trd>
  <trd lng="de">Filmkamera</trd>
  <trd lng="en">movie camera</trd>
  <trd lng="fr">cam&eacute;ra</trd>
  <trdgrp lng="id">
    <trd>cine-kamera</trd>,
    <trd>kamera film</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#26144;&#30011;&#29992;&#12459;&#12513;&#12521;  <pr>&#12360;&#12356;&#12364;&#12424;&#12358;&#12363;&#12417;&#12425;</pr></trd>,
    <trd>&#25774;&#24433;&#27231;  <pr>&#12373;&#12388;&#12360;&#12356;&#12365;</pr></trd>
  </trdgrp>
  <trd lng="pl">kamera<klr> filmowa</klr></trd>
  <trd lng="sk">filmov&aacute; kamera</trd>
  <trd lng="zh">&#25668;&#30456;&#26426; <pr>sh&egrave;xi&amacron;ngj&imacron;</pr></trd>
</drv>

<drv mrk="film.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      <tld lit="F"/>faristo; iu el diversaj specialistoj pri <tld/>farado:
      <ekz>
        verkistoj, muzikistoj, <tld/>istoj povus ricevi donacojn
        <fnt>
          <bib>Kon11</bib>
          <lok>Cifereca mono:</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Hollis Frampton</nom> estis usona artisto kaj <tld/>isto/realiganto el eksperimenta
        kino/kinarto dum la sesdekaj jaroj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia ambicio kiel dokumentada <tld/>isto estas nur komuniki je profunda emocia
        nivelo io pri vivi en la mondo hodia&ubreve;
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_n;&c_e;&c_m;&c_a;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_s;&c_t;</trd>,
    <trd>&c_k;&c_ib;&c_n;&c_a;&c_r;&c_e;&c_zh;&c_y;&c_s;&c_e;&c_r;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_n;&c_ib;&c_k; <ind>&c_k;&c_ib;&c_n;&c_a;&c_g;&c_a;&c_l;&c_ib;&c_n;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Filmer</trd>,
    <trd>Filmemacher</trd>
  </trdgrp>
  <trd lng="en">filmmaker</trd>
  <trd lng="fr">cin&eacute;aste</trd>
<trdgrp lng="id">
  <trd>ahli perfilman</trd>,
  <trd><baz>buat</baz>pembuat film</trd>,
  <trd>sineas</trd>
</trdgrp>
</drv>

<drv mrk="film.desegno0o">
  <kap>desegno<tld/>o</kap>
  <snc mrk="film.desegno0o.KIN">
    <uzo tip="fak">KIN</uzo>
    <dif>
      Animacia verko desegnita kaj prezentebla kiel
      <ref tip="super" cel="film.0o.KIN">filmo</ref>:
      <ekz>
        kiam mi estis infano, mi vidadis desegno<tld/>ojn fare de
        <klr>[&#8230;]</klr> <nom>Walt Disney</nom>
        <fnt>
        <aut>Zhong Q.</aut>,
        <vrk>desegnofilmo la totoroo...</vrk>,
        <lok>soc.culture.esperanto, 1994-05-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_l;&c_mol;&c_t;&c_y;&c_p;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y;
    &c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>,
    <trd>&c_m;&c_u;&c_l;&c_mol;&c_t;&c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>,
    <trd>&c_a;&c_n;&c_ib;&c_m;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y; &c_f;&c_ib;&c_l;&c_mol;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zeichentrickfilm</trd>,
    <trd>Trickfilm</trd>
  </trdgrp>
  <trd lng="en">animated cartoon</trd>
  <trd lng="fr">dessin anim&eacute;</trd>
