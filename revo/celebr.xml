<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: celebr.xml,v 1.25 2023/10/31 13:37:03 revo Exp $">
<kap>
  <rad>celebr</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="celebr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="celebr.0i.MOR">
    <dif>
      Plenumi la&ubreve;kutime ian
      <ref cel="ceremo.0o.solena">ceremonion</ref>:
      <ekz>
        kantado estos &ccirc;e vi, kiel en la nokto de <tld/>ado de festo
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 30:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun siaj familianoj <klr>[&#x2026;]</klr> li <tld/>is la vesperan solenon da la Paska
        festo
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ricevis la premion, du botelojn da bonega konjako <klr>[&#x2026;]</klr> por <tld/>i
        sian venkon
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ado de la 15-jara datreveno
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;ar anka&ubreve; mi estas &gcirc;entila homo, mi helpos vian <tld/>on <klr>[&#x2026;]</klr>, sed
        beda&ubreve;rinde &ccirc;e tio mi ne atentis la ventdirekton
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fest.pri0i.KOMUNE">prifesti</ref>,
      <ref cel="solen.0i.MOR">soleni</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x7D00;&#x5E74; <pr>j&#xEC;ni&#xE1;n</pr></trd>,
    <trd>&#x4E3E;&#x529E;&#x5E86;&#x5178; <pr>j&#x1D4;b&#xE0;nq&#xEC;ngdi&#x1CE;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">celebrar</trd>
  <trdgrp lng="cs">
    <trd>celebrovat</trd>,
    <trd>oslavovat</trd>,
    <trd>slavit</trd>
  </trdgrp>
  <trd lng="de">zelebrieren</trd>
  <trd lng="el">&gamma;&jota;&omikron;&rho;&tau;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="en">celebrate</trd>
  <trd lng="es">celebrar</trd>
  <trd lng="fr">c&eacute;l&eacute;brer</trd>
  <trd lng="he">&lamed;&het;&gimel;&vav;&gimel;</trd>
  <trdgrp lng="hu">
    <trd>celebr&aacute;l</trd>,
    <trd>meg&uuml;l</trd>
  </trdgrp>
  <trd lng="it">celebrare</trd>
  <trd lng="ja">&#x3068;&#x308A;&#x884C;&#x3046;  <pr>&#x3068;&#x308A;&#x304A;&#x3053;&#x306A;&#x3046;</pr></trd>
  <trdgrp lng="pl">
    <trd>celebrowa&cacute;</trd>,
    <trd>odprawia&cacute; <ind>ceremoni&eogonek;</ind></trd>
  </trdgrp>
  <trd lng="pt">celebrar</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_s;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_l;&c_ja;&c_t;&c_mol;
    &c_c;&c_je;&c_r;&c_je;&c_m;&c_o;&c_n;&c_i;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>oslavova&tcaron;</trd>,
    <trd>sl&aacute;vi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x443;&#x448;&#x430;&#x43D;&#x43E;&#x432;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x437;&#x43D;&#x430;&#x447;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x434;&#x456;&#x439;&#x441;&#x43D;&#x44E;&#x432;&#x430;&#x442;&#x438; &#x446;&#x435;&#x440;&#x435;&#x43C;&#x43E;&#x43D;&#x456;&#x44E;</trd>
  </trdgrp>
</drv>
</art>
