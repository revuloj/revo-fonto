<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dril</rad>/o
</kap>
<drv mrk="dril.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="bor.0aparato">Boraparato</ref> konsistanta
      el
      stango, en kiu estas fiksita <ref cel="bor.0ilo">borilo</ref> kaj
      kiu
      povas rapide turni&gcirc;i movate de elektra motoro, a&ubreve; de
      homa
      forto, ekz-e per ar&ccirc;o, kies &scirc;nuro
      &ccirc;irka&ubreve;as
      pulieton, a&ubreve; per &scirc;ra&ubreve;be borita stangeto, kiun
      oni
      glitigas la&ubreve;longe de la stango havanta tiam helican
      &scirc;ra&ubreve;ba&jcirc;on.  
    </dif>
    <trdgrp lng="bg">
      <trd>&c_b;&c_o;&c_r;&c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;</trd>,
      <trd>&c_d;&c_r;&c_je;&c_l;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Drillbohrer</trd>,
      <trd>Bohrmaschine</trd>
    </trdgrp>
    <trd lng="fr">perceuse</trd>
    <trdgrp lng="pl">
      <trd>wiertarka</trd>,
      <trd>&sacute;wider</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>broca</trd>,
      <trd>arco de <ind>pua</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_r;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_o;&c_v;&c_o;&c_r;&c_o;&c_t;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
