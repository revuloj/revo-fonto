<art mrk="$Id: pitore.xml,v 1.16 2022/03/26 10:43:37 revo Exp $">
<kap>
  <rad>pitoresk</rad>/a
</kap>
<drv mrk="pitore.0a">
  <kap><tld/>a</kap>
  <snc mrk="pitore.0a.BELA">
  <uzo tip="fak">BELA</uzo>
    <dif>
      Pentrinda; karakterizas objekton ta&ubreve;gan por esti reprezentata per
      pentrado:
      <ekz>
        <tld/>a pejza&gcirc;o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>pittoresk</trd>,
      <trd>malerisch</trd>
    </trdgrp>
    <trd lng="hu">fest&odblac;i</trd>
    <trd lng="nl">schilderachtig</trd>
  </snc>
  <snc mrk="pitore.0a.BELE">
  <uzo tip="fak">BELE</uzo>
    <dif>
      Vive kaj pikante originala en literatura verko:
      <ekz>
        <tld/>a stilo, priskribo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>bildhaft</trd>,
      <trd>anschaulich</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>k&eacute;pszer&udblac;</trd>,
      <trd>szeml&eacute;letes</trd>
    </trdgrp>
    <trd lng="nl">aanschouwelijk</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_a;&c_s;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_y;&c_r;&c_a;&c_z;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="sin">
    <ref cel="pentr.0inda">pentrinda</ref>
  </refgrp>
  <trd lng="be">&c_m;&c_a;&c_l;&c_ja;&c_w;&c_n;&c_ib;&c_ch;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>malebn&yacute;</trd>,
    <trd>pitoreskn&iacute;</trd>
  </trdgrp> 
  <trd lng="en">picturesque</trd>
  <trdgrp lng="fr">
    <trd>pittoresque</trd>
  </trdgrp>
  <trd lng="it">pittoresco</trd>
  <trd lng="pl">malowniczy</trd>
  <trd lng="pt">pitoresco</trd>
  <trd lng="ro">pitoresc</trd>
  <trd lng="ru">&c_zh;&c_i;&c_v;&c_o;&c_p;&c_i;&c_s;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">malebn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x43B;&#x44C;&#x43E;&#x432;&#x43D;&#x438;&#x447;&#x438;&#x439;</trd>,
    <trd>&#x436;&#x438;&#x432;&#x43E;&#x43F;&#x438;&#x441;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>
