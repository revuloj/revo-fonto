<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pitoresk</rad>/a
</kap>
<drv mrk="pitore.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Pentrinda; karakterizas objekton ta&ubreve;gan por esti reprezentata per
      pentrado:
      <ekz>
        <tld/>a pejza&gcirc;o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>pittoresk</trd>,
      <trd>malerisch</trd>
    </trdgrp>
    <trd lng="nl">schilderachtig</trd>
  </snc>
  <snc>
    <dif>
      Vive kaj pikante originala en literatura verko:
      <ekz>
        <tld/>a stilo, priskribo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>bildhaft</trd>,
      <trd>anschaulich</trd>
    </trdgrp>
    <trd lng="nl">aanschouwelijk</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_a;&c_s;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_y;&c_r;&c_a;&c_z;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="ru">&c_zh;&c_i;&c_v;&c_o;&c_p;&c_i;&c_s;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
