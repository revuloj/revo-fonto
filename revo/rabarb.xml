<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>rabarb</rad>/o
</kap>
<drv mrk="rabarb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="poligo1.0oj">poligonacoj</ref>
      (<trd lng="la">Rheum</trd>),
      kun tre ampleksaj folioj, kies trunkoj
      estas uzataj por fari kompoton a&ubreve; konfita&jcirc;on kaj kies
      pulvorigitaj radikoj servas kiel laksigilo.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_a;&c_r;&c_b;&c_a;&c_r;</trd>
  <trd lng="de">Rhabarber</trd>
  <trd lng="fr">rhubarbe</trd>
  <trd lng="hu">rebarbara</trd>
  <trd lng="nl">rabarber</trd>
  <trd lng="pl">rabarbar</trd>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sv">rabarber</trd>
</drv>
</art>
