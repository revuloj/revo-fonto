<art mrk="$Id: rabarb.xml,v 1.26 2024/04/13 20:15:51 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>rabarb</rad>/o
</kap>
<drv mrk="rabarb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="poligo1.0oj">poligonacoj</ref>
      (<trd lng="la">Rheum</trd>),
      kun tre ampleksaj folioj, kies trunkoj
      estas uzataj por fari kompoton a&ubreve; konfita&jcirc;on kaj kies
      pulvorigitaj radikoj servas kiel laksigilo:
      <ekz>
        la deserto estis &ccirc;iam a&ubreve; <tld/>o kun kustardo (ova&jcirc;o), a&ubreve; sekaj prunoj kun
        kustardo
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2012/010361.php">Lisuarte</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/9/97/Hanbury_Hall_Gardens_-_rhubarb_(Rheum_rhabarbarum).jpg/640px-Hanbury_Hall_Gardens_-_rhubarb_(Rheum_rhabarbarum).jpg" prm="CC BY-SA 4.0"><tld/>o (Rheum rhabarbarum) en &Gcirc;ardeno Hanbury Hall
        <fnt>
          <aut>DeFacto, propra verko</aut>
          <lok><url ref="&WCW;/File:Hanbury_Hall_Gardens_-_rhubarb_(Rheum_rhabarbarum).jpg">&WCW;/File:Hanbury_Hall_Gardens_-_rhubarb_(Rheum_rhabarbarum).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_a;&c_r;&c_b;&c_a;&c_r;</trd>
  <trd lng="bg">&c_r;&c_je;&c_v;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>rebarbora</trd>,
    <trd>reve&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Rhabarber</trd>
  <trd lng="en">rhubarb</trd>
  <trd lng="fr">rhubarbe</trd>
  <trd lng="hu">rebarbara</trd>
  <trd lng="id">rhubarb</trd>
  <trdgrp lng="ja">
    <trd>&#x30C0;&#x30A4;&#x30AA;&#x30A6;</trd>,
    <trd>&#x30EB;&#x30D0;&#x30FC;&#x30D6;</trd>,
    <trd>&#x30B7;&#x30E7;&#x30AF;&#x30E8;&#x30A6;&#x30C0;&#x30A4;&#x30AA;&#x30A6;</trd>
  </trdgrp>
  <trd lng="nl">rabarber</trd>
  <trd lng="pl">rabarbar</trd>
  <trdgrp lng="pt">
   <trd>ruibarbo</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">rebarbora</trd>
  <trd lng="sv">rabarber</trd>
  <trd lng="uk">&c_r;&c_je;&c_v;&c_ib;&#x43D;&#x44C;</trd>
  <trdgrp lng="zh">
    <trd>&#x5927;&#x9EC3; <pr>d&#xE0;hu&#xE1;ng</pr></trd>,
    <trd>&#x5927;&#x9EC4; <pr>d&#xE0;hu&#xE1;ng</pr></trd>
  </trdgrp>
</drv>
</art>
