<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>liliput</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="lilipu.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Malgrandega, kiel la lo&gcirc;antoj de la fantazia samnoma lando imagita
      de Swift:
      <ekz>kresko mamuta, sed sa&gcirc;o <tld/>a
<fnt><bib>PrV</bib></fnt>...</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_l;&c_ib;&c_p;&c_u;&c_c;&c_k;&c_ib;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_l;&c_ib;&c_k;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>lilliputanisch</trd>,
    <trd>zwergenhaft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Lilliputian</trd>,
    <trd>diminutive</trd>,
    <trd>dwarf</trd>,
    <trd>tiny</trd>
  </trdgrp>
  <trd lng="es">liliputiense</trd>
  <trd lng="hu">liliputi</trd>
  <trd lng="nl">dwergachtig</trd>
  <trd lng="pt">liliputiano</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_l;&c_i;&c_p;&c_u;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_l;&c_i;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
