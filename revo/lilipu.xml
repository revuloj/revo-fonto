    <trd>diminut</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>lilliputanisch</trd>,
    <trd>zwergenhaft</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Lilliputian</trd>,
    <trd>diminutive</trd>,
    <trd>dwarf</trd>,
    <trd>tiny</trd>
  </trdgrp>
  <trd lng="es">liliputiense</trd>
  <trd lng="fr">lilliputien</trd>
  <trd lng="hu">liliputi</trd>
  <trdgrp lng="ja">
    <trd>&#23567;&#20154;&#12398;  <pr>&#12371;&#12403;&#12392;&#12398;</pr></trd>,
    <trd>&#12385;&#12387;&#12413;&#12369;&#12394;</trd>,
    <trd>&#21329;&#23567;&#12394; <pr>&#12402;&#12375;&#12423;&#12358;&#12394;</pr></trd>
  </trdgrp>
  <trd lng="nl">dwergachtig</trd>
