<vortaro>
<art mrk="$Id$">
<kap>
  <rad>liliput</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="lilipu.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Malgrandega, kiel la lo&gcirc;antoj de la fantazia samnoma lando imagita
      de Swift:
      <ekz>
        kresko mamuta, sed sa&gcirc;o <tld/>a<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>Lilliputian</trd>,
    <trd>diminutive</trd>,
    <trd>dwarf</trd>,
    <trd>tiny</trd>
  </trdgrp>
  <trd lng="nl">dwergachtig</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_l;&c_i;&c_p;&c_u;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_l;&c_i;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
