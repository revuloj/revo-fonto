  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_l;&c_ib;&c_p;&c_u;&c_c;&c_k;&c_ib;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_l;&c_ib;&c_k;&c_a;&c_v;&c_y;</trd>
