<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kariatid.xml,v 1.6 2023/06/23 15:31:13 revo Exp $">
<kap>
  <rad>kariatid</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kariatid.0o">
  <kap><tld/>o</kap>
  <snc mrk="kariatid.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="kolon.0o.ARKI">Kolono</ref> en formo de
      grandega homstatuo:
      <ekz>
        <klr>A&ubreve;guste Rodin</klr> iris al Bruxelles, kie li faris la 
        <tld/>ojn de l' <ctl>Palais de Bourse</ctl>
        <fnt>
         <aut>P. Vars&aacute;nyi</aut>,
         <vrk>Auguste Rodin</vrk>,
         <lok><url ref="http://literaturamondo.org/PDF/1924/LM%201924-01bc.pdf">
         Literatura Mondo, 1924-01 (3:1)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_y;&c_ja;&c_t;&c_y;&c_d;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>b&rcaron;evnono&scaron;</trd>,
    <trd>karyatida</trd>,
    <trd>k&oacute;ra</trd>
  </trdgrp>
  <trd lng="fr">cariatide</trd>
  <trd lng="pl">kariatyda</trd>     
  <trd lng="ro">cariatid&abreve;</trd>
  <trdgrp lng="sk">
    <trd>karyat&iacute;da</trd>,
    <trd>podpern&aacute; socha</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&c_a;&c_r;&c_ib;&c_a;&c_t;&c_i;&c_d;&c_a;</trd>
</drv>
</art>

