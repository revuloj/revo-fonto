<drv mrk="preciz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="preciz.0a.KOMUNE">
    <dif>
      Klare kaj detale determinita:
      <ekz>
        lia multjare ellaborita projekto jam ricevis <tld/>ajn, difinitajn formojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiuj &ccirc;i horlo&gcirc;oj iras tute regule kaj &ccirc;iuj en <tld/>a konsento
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi deziris havi <tld/>an klarigon
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 7:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni metis lin en malliberejon, &gcirc;is oni ricevos <tld/>an decidon
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 24:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        montri&gcirc;os, ke la afero estas <tld/>a vero
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 17:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        ju pli <tld/>aj la kalkuloj, des pli fortika la amikeco
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
   </dif>
