<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>preciz</rad>/a
</kap>
<drv mrk="preciz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Klare kaj detale determinita:
      <ekz>
        <tld/>a ordono, esprimo, difino, indiko, sciigo, horo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a horlo&gcirc;o, verkisto, instrumento;
      </ekz>
      <ekz>
        <tld/>e respondi al demando;
      </ekz>
      <ekz>
        mal<tld/>a
        (<refgrp tip="vid">
          <ref cel="nebul.0a">nebula</ref>,
          <ref cel="vat.0i">vati</ref>
        </refgrp>) ordono, vorto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="akurat.0a">akurata</ref>,
      <ref cel="gxust.0a">&gcirc;usta</ref>,
      <ref cel="net.0a">neta</ref>,
      <ref cel="ekzakt.0a">ekzakta</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>pr&auml;zise</trd>,
    <trd>genau</trd>
  </trdgrp>
  <trd lng="cs">p&rcaron;esn&yacute;</trd>
  <trd lng="en">precise</trd>
  <trdgrp lng="nl">
    <trd>precies</trd>,
    <trd>nauwkeurig</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="preciz.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Klara, detala kaj severa difiniteco:
      <ekz>
        <tld/>eco de desegno, stilo, le&gcirc;o, kalkulo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pr&auml;zision</trd>,
    <trd>Genauigkeit</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>nauwkeurigheid</trd>,
    <trd>precisie</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="preciz.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi ion <tld/>a:
      <ekz>
        <tld/>igi la signifon de vorto, la datojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>pr&auml;zisieren</trd>,
    <trd>genauer <ind>bestimmen</ind></trd>
  </trdgrp>
  <trd lng="nl">preciseren</trd>
  <trd lng="ru">&c_u;&c_t;&c_o;&c_ch;&c_n;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
