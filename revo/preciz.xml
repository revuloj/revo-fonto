<drv mrk="preciz.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="preciz.0igi.KOMUNE">
    <dif>
      Igi <tld/>a:
      <ekz>
        la Bulonja deklaracio <tld/>igis la oficialan, de &ccirc;iuj sendispute akceptitan
        esencon de la Esperantismo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la tre maloftaj okazoj de neklareco ni povas ja <tld/>igi nian parolon,
        dirante <klr>[&#x2026;]</klr>
        <fnt>
          <bib>LR</bib>
          <lok>8. Personaj pronomoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igante la signifon de &leftquot;a&jcirc;&rightquot; nur aktive a&ubreve; nur pasive
        <fnt>
          <bib>LR</bib>
          <lok>1. Vortfarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        povas esti utile, <tld/>igi, &ccirc;u oni parolas pri unu, &ccirc;u pri la alia
        <fnt><aut>Andr&eacute; Cherpillod</aut>,
        <vrk><url ref="&Monato;/2000/006937.php">Sinjorino Le secr&eacute;taire ...</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/07, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        li simpligis kaj <tld/>igis la &ccirc;e&hcirc;an ortografion per enkonduko de supersignoj
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>
            <vrk><url ref="&Monato;/2000/007031.php">Johano Hus &ndash; &ccirc;u ne plu herezulo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 19</lok>
          </fnt>.
      </ekz>
   </dif>
   <refgrp tip="vid">
     <ref cel="determ.0i.KOMUNE">determini<sncref/></ref>,
     <ref cel="difin.0i.fiksi">difini<sncref/></ref>,
     <ref cel="fiks.0i.kutimon">fiksi<sncref/></ref>,
     <ref cel="specif.0i">specifi</ref>
   </refgrp>
 </snc>
  <trdgrp lng="be">
   <trd>&c_u;&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_ib;&c_c;&c_mol;</trd>,
