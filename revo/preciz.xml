<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: preciz.xml,v 1.22 2021/05/29 14:33:07 revo Exp $">
<kap>
 <ofc>*</ofc>
 <rad>preciz</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="preciz.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="preciz.0a.KOMUNE">
    <dif>
      Klare kaj detale determinita:
      <ekz>
        lia multjare ellaborita projekto jam ricevis <tld/>ajn, difinitajn formojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiuj &ccirc;i horlo&gcirc;oj iras tute regule kaj &ccirc;iuj en <tld/>a konsento
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi deziris havi <tld/>an klarigon
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 7:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni metis lin en malliberejon, &gcirc;is oni ricevos <tld/>an decidon
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 24:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        montri&gcirc;os, ke la afero estas <tld/>a vero
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 17:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        ju pli <tld/>aj la kalkuloj, des pli fortika la amikeco
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
   </dif>
   <refgrp tip="vid">
     <ref cel="akurat.0e">akurata</ref>,
     <ref cel="detal.0a.0hava">detala<sncref/></ref>,
     <ref cel="ekzakt.0a">ekzakta</ref>,
     <ref cel="gxust.0a">&gcirc;usta</ref>,
     <ref cel="net.0a">neta</ref>,
     <ref cel="strikt.0a.FIG">strikta<sncref/></ref>
   </refgrp>
 </snc>
  <trdgrp lng="be">
   <trd>&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_y;</trd>,
   <trd>&c_p;&c_e;&c_w;&c_n;&c_y;</trd>
 </trdgrp>
  <trd lng="bo">&#xF5E;&#xF72;&#xF56;&#xF0B;&#xF55;&#xFB2;&#xF0B;</trd>
  <trd lng="ca">prec&iacute;s</trd>
  <trdgrp lng="cs">
    <trd>dochviln&yacute;</trd>,
    <trd>p&rcaron;esn&yacute;</trd>,
    <trd>precizn&iacute;</trd>,
    <trd>p&rcaron;esn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
   <trd>pr&auml;zise</trd>,
   <trd>genau</trd>
 </trdgrp>
  <trd lng="en">precise</trd>
  <trd lng="es">preciso</trd>
  <trd lng="fr">pr&eacute;cis</trd>
  <trdgrp lng="hu">
   <trd>pontos</trd>,
   <trd>prec&iacute;z</trd>
 </trdgrp>
  <trdgrp lng="nl">
   <trd>precies</trd>,
   <trd>nauwkeurig</trd>
 </trdgrp>
  <trdgrp lng="pl">
   <trd>dok&lstroke;adny</trd>,
   <trd>precyzyjny</trd>,
   <trd>&sacute;cis&lstroke;y</trd>
 </trdgrp>
  <trdgrp lng="pt">
    <trd>precizo</trd>,
    <trd>exato</trd>,
    <trd>justo</trd>,
    <trd>certo</trd>,    
    <trd>expresso</trd>        
  </trdgrp>
  <trdgrp lng="ro">
   <trd>exact</trd>,
   <trd>perfect</trd>
 </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>prec&iacute;zny</trd>,
    <trd>presn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="preciz.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      En <tld/>a maniero:
      <ekz>
        mi ekzamenis <tld/>e, &ccirc;u &ccirc;io estas en ordo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi observu <tld/>e kaj plenumu &ccirc;ion, kion instruos al vi la pastroj
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 24:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        tia estas <tld/>e la son&gcirc;o kaj &gcirc;ia &gcirc;usta klarigo
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 2:45</lok>
        </fnt>;
      </ekz>
      <ekz>
        Herodo sekrete venigis la sa&gcirc;ulojn, kaj <tld/>e scii&gcirc;is de ili
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 2:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pasinta&jcirc;on vi konas <tld/>e
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e la&ubreve; la tempo kaj la loko
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi ne memoras tute <tld/>e, en kiu jaro de mia vivo mi perdis la religian kredon
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<trd lng="bo">&#xF62;&#xF9F;&#xF42;&#xF0B;&#xF62;&#xF9F;&#xF42;&#xF0B;</trd>
  <trd lng="en">precisely</trd>
</drv>


<drv mrk="preciz.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="preciz.0eco.KOMUNE">
    <dif>
      Klara, detala kaj severa difiniteco:
      <ekz>
        matematika <tld/>eco
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni povus jam nun kun plena certeco kaj <tld/>eco anta&ubreve;vidi, <klr>[&#x2026;]</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la vorto &leftquot;krom&rightquot; povas iufoje ka&ubreve;zi ne<tld/>econ a&ubreve; malkompreni&gcirc;on
        <fnt>
          <bib>LR</bib>
          <lok>13. Prepozicioj</lok>
        </fnt>.
      </ekz>
   </dif>
 </snc>
  <trdgrp lng="be">
   <trd>&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
   <trd>&c_p;&c_e;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
 </trdgrp>
  <trd lng="ca">precisi&oacute;</trd>
  <trd lng="cs">p&rcaron;esnost</trd>
  <trdgrp lng="de">
   <trd>Pr&auml;zision</trd>,
   <trd>Genauigkeit</trd>
 </trdgrp>
  <trd lng="es">precisi&oacute;n</trd>
  <trd lng="fr">pr&eacute;cision<klr> (qualit&eacute;)</klr></trd>
  <trdgrp lng="hu">
   <trd>pontoss&aacute;g</trd>,
   <trd>precizit&aacute;s</trd>
 </trdgrp>
  <trdgrp lng="nl">
   <trd>nauwkeurigheid</trd>,
   <trd>precisie</trd>
 </trdgrp>
  <trdgrp lng="pl">
   <trd>dok&lstroke;adno&sacute;&cacute;</trd>,
   <trd>precyzyjno&sacute;&cacute;</trd>,
   <trd>&sacute;cis&lstroke;o&sacute;&cacute;</trd>
 </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">presnos&tcaron;</trd>
</drv>

<drv mrk="preciz.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="preciz.0igi.KOMUNE">
    <dif>
      Igi <tld/>a:
      <ekz>
        la Bulonja deklaracio <tld/>igis la oficialan, de &ccirc;iuj sendispute akceptitan
        esencon de la Esperantismo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la tre maloftaj okazoj de neklareco ni povas ja <tld/>igi nian parolon,
        dirante <klr>[&#x2026;]</klr>
        <fnt>
          <bib>LR</bib>
          <lok>8. Personaj pronomoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igante la signifon de &leftquot;a&jcirc;&rightquot; nur aktive a&ubreve; nur pasive
        <fnt>
          <bib>LR</bib>
          <lok>1. Vortfarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        povas esti utile, <tld/>igi, &ccirc;u oni parolas pri unu, &ccirc;u pri la alia
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006937.html">Genroj en la Franca:
          Sinjorino &leftquot;Le secr&eacute;taire&rightquot; (la sekretario)</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li simpligis kaj <tld/>igis la &ccirc;e&hcirc;an ortografion per enkonduko de supersignoj
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007031.html">Katolikismo: Johano Hus &dash;
          &ccirc;u ne plu herezulo?</url></vrk>
        </fnt>.
      </ekz>
   </dif>
   <refgrp tip="vid">
     <ref cel="determ.0i.KOMUNE">determini<sncref/></ref>,
     <ref cel="difin.0i.fiksi">difini<sncref/></ref>,
     <ref cel="fiks.0i.kutimon">fiksi<sncref/></ref>,
     <ref cel="specif.0i">specifi</ref>
   </refgrp>
 </snc>
  <trdgrp lng="be">
   <trd>&c_u;&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_ib;&c_c;&c_mol;</trd>,
   <trd>&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_a; &c_v;&c_y;&c_z;&c_n;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>
 </trdgrp>
  <trd lng="ca">precisar</trd>
  <trdgrp lng="cs">
    <trd>precizovat</trd>,
    <trd>se&rcaron;&iacute;dit</trd>,
    <trd>up&rcaron;esnit</trd>
  </trdgrp>
  <trdgrp lng="de">
   <trd>pr&auml;zisieren</trd>,
   <trd>genauer <ind>bestimmen</ind></trd>
 </trdgrp>
  <trd lng="es">precisar</trd>
  <trd lng="fr">pr&eacute;ciser</trd>
  <trd lng="hu">pontos&iacute;t</trd>
  <trd lng="nl">preciseren</trd>
  <trdgrp lng="pl">
   <trd>sprecyzowa&cacute;</trd>,
   <trd>u&sacute;ci&sacute;li&cacute;</trd>
 </trdgrp>
  <trd lng="ru">&c_u;&c_t;&c_o;&c_ch;&c_n;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>precizova&tcaron;</trd>,
    <trd>spresni&tcaron;</trd>,
    <trd>upresni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="preciz.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="preciz.mal0a.KOMUNE">
    <dif>
      Kiu ne klaras a&ubreve; ne enhavas sufi&ccirc;an detalecon:
      <ekz>
        elparolo malfacila, ortografio mal<tld/>a, sed kia forto kaj rapido en la frazoj!
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        se estas nur mal<tld/>a informo pri dokumento, &gcirc;i apena&ubreve; estas retrovebla inter
        la miloj da dosieroj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="https://www.monato.be/2005/008753.php">Google Desktop Search: ser&ccirc;ilo por
          la propra komputilo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;spirita&rightquot; estas iom mal<tld/>a koncepto
        <fnt>
          <bib>Monato</bib>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="https://www.monato.be/2007/009197.php">Kio estas spiriteco?</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="ambigu.0a">ambigua</ref>,
      <ref cel="nebul.0a.FIG">nebula<sncref/></ref>,
      <ref cel="svag.0a.KOMUNE">svaga</ref>
    </refgrp>    
  </snc>
  <trdgrp lng="cs">
    <trd>nejasn&yacute;</trd>,
    <trd>neur&ccaron;it&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ungenau</trd>,
    <trd>unscharf</trd>,
    <trd>schwammig</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>impreciso</trd>,
    <trd>vago</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>impr&eacute;cis</trd>,
    <trd>vague</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nejasn&yacute;</trd>,
    <trd>neur&ccaron;it&yacute;</trd>
  </trdgrp>
</drv>
</art>
