<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>oktav</rad>/o
</kap>
<drv mrk="oktav.0o">
  <kap><tld/>o</kap>
  <snc mrk="oktav.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <ref tip="dif" cel="okt.0o">okto</ref>
    <trd lng="de">Oktave</trd>
    <trdgrp lng="fr">
      <trd>octave<klr> (intervalle musical)</klr></trd>
    </trdgrp>
    <trd lng="nl">octaaf</trd>
    <trd lng="ru">&c_o;&c_k;&c_t;&c_a;&c_v;&c_a;</trd>
  </snc>
  <snc mrk="oktav.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Libro en kiu &ccirc;iu paperfolio estas trioble faldita kaj formas 16
      pa&gcirc;ojn:
      <ekz>
        la libro gajnus per tio kelkajn <tld/>ajn pa&gcirc;ojn...<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Oktavheft</trd>
    <trdgrp lng="fr">
      <trd>in-octavo</trd>
    </trdgrp>
    <trd lng="ru">&c_i;&c_n;-&c_o;&c_k;&c_t;&c_a;&c_v;&c_o;</trd>
  </snc>
</drv>
</art>
