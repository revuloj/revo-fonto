<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: avanta.xml,v 1.27 2023/10/13 17:08:38 revo Exp $">
    <kap>
      <ofc>8</ofc>
      <rad>avanta&gcirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="avanta.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Tio, kio donas ian superecon, prefera&jcirc;on, superan
          profiton a&ubreve;
          enhavas pli grandan utilon, oportunon:
          <ekz>
            &ccirc;iu formo de regado havas siajn <tld/>ojn;
          </ekz><ekz>
            la amikeco havas tiun <tld/>on super la parenceco, ke...
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a;</trd>, 
        <trd>&c_p;&c_r;&c_y;&c_v;&c_ib;&c_l;&c_je;&c_ja;</trd>
      </trdgrp>  
      <trd lng="de">Vorteil</trd>
      <trd lng="en">advantage</trd>
      <trd lng="es">ventaja</trd>
      <trd lng="fr">avantage</trd>
      <trd lng="he">&#x5D9;&#x5EA;&#x5E8;&#x5D5;&#x5DF;</trd>
  <trd lng="hu">el&odblac;ny</trd>
      <trd lng="it">vantaggio</trd>
      <trdgrp lng="ja">
    <trd>&#x6709;&#x5229;&#x306A;&#x7ACB;&#x5834;  <pr>&#x3086;&#x3046;&#x308A;&#x306A;&#x305F;&#x3061;&#x3070;</pr></trd>,
    <trd>&#x5229;&#x70B9;  <pr>&#x308A;&#x3066;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">voordeel</trd>
      <trdgrp lng="pl">
        <trd>korzy&sacute;&cacute;</trd>, 
        <trd>zaleta</trd>
      </trdgrp>  
      <trdgrp lng="ro">
        <trd>avantaj</trd>,
        <trd>beneficiu</trd>,
        <trd>folos</trd>, 
        <trd>profit</trd>
      </trdgrp>  
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_je;&c_i;&c_m;&c_u;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
        <trd>&c_v;&c_y;&c_g;&c_o;&c_d;&c_a;</trd>,
        <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
      </trdgrp>
      <trd lng="sk">v&yacute;hoda</trd>
      <trdgrp lng="th">
        <trd>&#xE1C;&#xE25;&#xE1B;&#xE23;&#xE30;&#xE42;&#xE22;&#xE0A;&#xE19;&#xE4C;</trd>,
        <trd>&#xE04;&#xE27;&#xE32;&#xE21;&#xE44;&#xE14;&#xE49;&#xE40;&#xE1B;&#xE23;&#xE35;&#xE22;&#xE1A;</trd>
      </trdgrp>
      <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_v;&c_a;&c_g;&c_a;</trd>,
    <trd>&c_d;&#x43E;&c_s;&c_t;&#x43E;&c_ji;&#x43D;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd>&c_d;&#x43E;&c_b;&c_r;&c_a; &c_s;&c_t;&#x43E;&c_r;&#x43E;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>

    <drv mrk="avanta.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Prezentanta <tld/>on:
          <ekz>
            <tld/>aj kondi&ccirc;oj, <tld/>a prezo.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>, 
        <trd>&c_s;&c_p;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>, 
        <trd>&c_ib;&c_l;&c_mol;&c_g;&c_o;&c_t;&c_n;&c_y;</trd>
      </trdgrp>  
      <trd lng="de">vorteilhaft</trd>
      <trd lng="en">advantageous</trd>
      <trd lng="es">ventajoso</trd>
      <trd lng="fr">avantageux</trd>
      <trd lng="he">&#x5DE;&#x5D5;&#x5B9;&#x5E2;&#x5B4;&#x5D9;&#x5DC;</trd>
  <trd lng="hu">el&odblac;ny&ouml;s</trd>
      <trd lng="it">vantaggioso</trd>
      <trdgrp lng="ja">
    <trd>&#x6709;&#x5229;&#x306A;  <pr>&#x3086;&#x3046;&#x308A;&#x306A;</pr></trd>,
    <trd>&#x5F97;&#x306A;  <pr>&#x3068;&#x304F;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="pl">korzystny</trd>
      <trd lng="ro">avantajos</trd>
      <trdgrp lng="ru">
        <trd>&c_v;&c_y;&c_g;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_l;&c_mol;&c_g;&c_o;&c_t;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
      <trd lng="sk">v&yacute;hodn&yacute;</trd>
      <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_g;&c_ib;&c_d;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43F;&c_r;&c_i;&#x44F;&c_t;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_ib;&#x43B;&#x44C;&c_g;&#x43E;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

    <drv mrk="avanta.0e">
      <kap><tld/>e</kap>
      <snc>
        <dif>
          Kun <tld/>o:
          <ekz>
            tiu kuracilo estas <tld/>e anstata&ubreve;igita per la nova.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_s;&c_p;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_a;</trd>
      <trd lng="de">vorteilhaft</trd>
      <trdgrp lng="en">
        <trd>advantageously</trd>, 
        <trd>to one's advantage</trd>
      </trdgrp>  
      <trd lng="fr">avantageusement</trd>
      <trd lng="hu">el&odblac;ny&ouml;sen</trd>
      <trd lng="it">vantaggiosamente</trd>
      <trdgrp lng="ja">
    <trd>&#x6709;&#x5229;&#x306B;  <pr>&#x3086;&#x3046;&#x308A;&#x306B;</pr></trd>,
    <trd>&#x90FD;&#x5408;&#x3088;&#x304F;  <pr>&#x3064;&#x3054;&#x3046;&#x3088;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
        <trd>korzystnie</trd>, 
        <trd>na dobre</trd>
      </trdgrp>
      <trd lng="ro">pentru bine</trd>
      <trd lng="sk">v&yacute;hodne</trd>
    </drv>

    <drv mrk="avanta.mal0o">
      <kap>mal<tld/>o</kap>
      <snc>
        <dif>
          Tio, kio malfavorigas onian situacion, malutilas,
          malfaciligas la agadon, vivon ktp:
          <ekz>
            e&ccirc; nur bezono, skribi kaj legi fremdan alfabeton,
            estas
            iom grava mal<tld/>o
            <fnt>
              <aut>V. Sadler</aut>: <vrk>Azia Strategio</vrk>.
              <lok>UEA, 1963</lok>
            </fnt>;
          </ekz><ekz>
            ni povus klarigi <klr>(...)</klr> en Bruselo la kostojn
            kaj mal<tld/>ojn de la angla-franca lingva monopolo
            <fnt>
              <aut>D. Ferguson</aut>:
              <vrk>Bruselo: la nova Bjalistoko</vrk>. <bib>Monato</bib>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_n;&c_je;&c_d;&c_a;&c_h;&c_o;&c_p;</trd>,
        <trd>&c_b;&c_r;&c_a;&c_k;</trd>,
        <trd>&c_z;&c_a;&c_g;&c_a;&c_n;&c_a;</trd>
      </trdgrp>
      <trd lng="bo">&#xF66;&#xF90;&#xFB1;&#xF7C;&#xF53;&#xF0B;</trd>
  <trd lng="en">disadvantage</trd>
      <trd lng="fr">d&eacute;savantage</trd>
      <trd lng="it">svantaggio</trd>
      <trdgrp lng="ja">
    <trd>&#x4E0D;&#x5229;&#x306A;&#x7ACB;&#x5834;  <pr>&#x3075;&#x308A;&#x306A;&#x305F;&#x3061;&#x3070;</pr></trd>,
    <trd>&#x640D;&#x5931;  <pr>&#x305D;&#x3093;&#x3057;&#x3064;</pr></trd>,
    <trd>&#x6B20;&#x70B9;  <pr>&#x3051;&#x3063;&#x3066;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">wada</trd>
      <trd lng="ro">defect</trd>
      <trdgrp lng="ru">
        <trd>&c_n;&c_je;&c_u;&c_d;&c_o;&c_b;&c_s;&c_t;&c_v;&c_o;</trd>,
        <trd>&c_n;&c_je;&c_b;&c_l;&c_a;&c_g;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_o;&c_je;
        &c_u;&c_s;&c_l;&c_o;&c_v;&c_i;&c_je;</trd>,
        <trd>&c_n;&c_je;&c_d;&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_k;</trd>,
        <trd>&c_v;&c_r;&c_je;&c_d;</trd>,
        <trd>&c_u;&c_shch;&c_je;&c_r;&c_b;</trd>
      </trdgrp>
    </drv>


  </art>
