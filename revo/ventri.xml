<art mrk="$Id: ventri.xml,v 1.26 2023/10/28 09:22:07 revo Exp $">
<kap>
  <rad>ventrikl</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="ventri.0o">
  <kap><tld/>o</kap>
  <snc mrk="ventri.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Kava iom &scirc;vela parto de iuj organoj, i.a. la
      du malsupraj kava&jcirc;oj de koro:
      <ekz>
        li trafas la koron, <klr>[&#8230;]</klr> la&ubreve; elekto, en ajnan atrion a&ubreve; en ajnan el la
        <tld/>oj
        <fnt>
          <bib>MkM</bib>
          <lok>24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Mi tial &scirc;imas en ventriklo &parsl; De sinekzalta histrion'
        <fnt>
          <bib>LSB</bib>
          <lok>Lamentado</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="atri.0o.ANA">atrio<sncref/></ref>,
      <ref cel="vestib.0o.ANA">vestiblo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_l;&c_u;&c_n;&c_a;&c_ch;&c_a;&c_k;</trd>
  <trd lng="ca">ventricle</trd>
  <trdgrp lng="cs">
    <trd>komora</trd>,
    <trd>t&ecaron;ln&iacute; dutina</trd>,
    <trd>ventrikl</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ventrikel</trd>,
    <trd>Kammer</trd>
  </trdgrp>
  <trd lng="en">ventricle</trd>
  <trd lng="es">ventr&iacute;culo</trd>
  <trd lng="fr">ventricule</trd>
  <trd lng="hu">(sz&iacute;v)<ind>kamra</ind></trd>
  <trd lng="it">ventricolo</trd>
  <trd lng="ja">&#23460;  <pr>&#12416;&#12429;</pr></trd>
  <trdgrp lng="nl">
    <trd>ventrikel</trd>,
    <trd>kamer<klr tip="ind"> (v.hart)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>jama <klr>(anatomia)</klr></trd>,
    <trd>komora <klr>(serca)</klr></trd>,
  </trdgrp>
  <trd lng="pt">ventr&iacute;culo</trd>
  <trd lng="ro">ventricul</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_u;&c_d;&c_o;&c_ch;&c_je;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>komora</trd>,
    <trd>telesn&aacute; dutina</trd>
  </trdgrp>
  <trd lng="uk">&c_sh;&c_l;&c_u;&c_n;&c_o;&c_ch;&c_o;&c_k; (&c_s;&c_je;&c_r;&c_c;&c_ja;)</trd>
</drv>
</art>
