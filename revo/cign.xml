<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cign</rad>/o
</kap>
<drv mrk="cign.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cign.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="anased.0oj">anasedoj</ref>
      (<trd lng="la">Cygnus</trd>), granda, ordinare blanka na&gcirc;birdo,
      kun longa fleksebla kolo:
      <ekz>
        el la densejo elna&gcirc;is tri belaj blankaj cignoj
        <fnt><bib>Fab2</bib>, <lok>Malbela anasido</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="../bld/cign.jpg">
       <ind>muta <tld/>o</ind>
       <trd lng="cs">labu&tcaron; velk&aacute;</trd>
       <trd lng="de">H&ouml;ckerschwan</trd>
       <trd lng="en">mute swan</trd>
       <trd lng="hu">b&uuml;tyk&ouml;s hatty&uacute;</trd>
       <trd lng="la">Cygnus olor</trd>
       <trd lng="pl">lab&eogonek;d&zacute; niemy</trd>
       <trd lng="ru">&c_l;&c_je;&c_b;&c_je;&c_d;&c_mol;-&c_sh;&c_i;&c_p;&c_u;&c_n;</trd>
    </bld>
    <trd lng="cs">labu&tcaron;</trd>
    <trd lng="de">Schwan</trd>
    <trd lng="en">swan</trd>
    <trd lng="el">&kappa;&ypsilon_ton;&kappa;&ny;&omikron;&sigma_fina;</trd>
    <trd lng="es">cisne</trd>
    <trd lng="fr">cygne</trd>
    <trd lng="he">&bet;&resh;&bet;&vav;&resh;</trd>
    <trd lng="hu">hatty&uacute;</trd>
    <trd lng="nl">zwaan</trd>
    <trd lng="pl">lab&eogonek;d&zacute;</trd>
    <trd lng="pt">cisne</trd>
    <trd lng="ru">&c_l;&c_je;&c_b;&c_je;&c_d;&c_mol;</trd>
  </snc>
  <snc mrk="cign.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Harmonia poeto, a&ubreve;toro:
      <ekz>
        <ind>kanto de <tld/>o</ind><fnt>Z</fnt>
        <klr>(lasta plej bela verko de a&ubreve;toro)</klr>.
        <trd lng="ru">&c_l;&c_je;&c_b;&c_je;&c_d;&c_i;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_s;&c_n;&c_ja;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="sk">labu&tcaron;</trd>
</drv>
</art>
