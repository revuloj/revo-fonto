<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cign.xml,v 1.48 2025/07/09 06:43:23 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>cign</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="cign.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cign.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda, ordinare blanka
      <ref tip="lst" cel="bird.nagxo0o" lst="voko:birdoj">na&gcirc;birdo</ref>, kun longa fleksebla kolo;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="anased.0oj">anasedoj</ref>
      (<trd lng="la">Cygnus</trd>):
      <ekz>
        el la densejo elna&gcirc;is tri belaj blankaj cignoj
        <fnt><bib>Fab2</bib>, <lok>Malbela anasido</lok></fnt>;
      </ekz>
      <ekz>
        granda karbonigra <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kun maldol&ccirc;a sento de neplenumita espero vi eble demandos: &ccirc;u en sia lasta
        kongresa parolo, en sia <ind>kanto de <tld/>o</ind>
        <klr>(vivlasta kompona&jcirc;o, verko, parolo, kp <ref cel="elegi.0o">elegio</ref>)</klr>,
        li nenion pli havis por diri al ni
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>?
        <trd lng="be">&c_l;&c_je;&c_b;&c_ja;&c_d;&c_z;&c_ib;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_s;&c_mol;&c_n;&c_ja;</trd>
        <trdgrp lng="ca">
          <trd>cant del <ind>cigne</ind></trd>,
          <trd>darrera revifada</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Schwanengesang</trd>,
          <trd>Abgesang</trd>
        </trdgrp>
        <trd lng="fr">chant du <ind>cygne</ind></trd>
        <trd lng="es">cisne</trd>
        <trd lng="hu">hatty&uacute;dal</trd>
        <trd lng="it">canto del <ind>cigno</ind></trd>
        <trd lng="ru">&c_l;&c_je;&c_b;&c_je;&c_d;&c_i;&c_n;&c_a;&c_ja;
        &c_p;&c_je;&c_s;&c_n;&c_ja;</trd>
      </ekz>
      <ekz>
        &scirc;i falis kiel mortanta <tld/>o en la mezon de la arbara lago
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="../bld/cign.jpg">
      <ind>muta <tld/>o</ind>
      <trd lng="be">&c_sh;&c_y;&c_p;&c_u;&c_n; <klr tip="amb">(&c_l;&c_je;&c_b;&c_je;&c_d;&c_z;&c_mol;)</klr></trd>
      <trd lng="ca"><ind>cigne</ind> mut</trd>
      <trd lng="cs">labu&tcaron; velk&aacute;</trd>
      <trd lng="de">H&ouml;ckerschwan</trd>
      <trd lng="en">mute swan</trd>
      <trd lng="hu">b&uuml;tyk&ouml;s hatty&uacute;</trd>
      <trd lng="la">Cygnus olor</trd>
      <trd lng="pl">&lstroke;ab&eogonek;d&zacute; niemy</trd>
      <trd lng="ro">leb&abreve;d&abreve; de var&abreve;</trd>
      <trd lng="ru">&c_l;&c_je;&c_b;&c_je;&c_d;&c_mol;-&c_sh;&c_i;&c_p;&c_u;&c_n;</trd>
    </bld>
    <trd lng="be">&c_l;&c_je;&c_b;&c_je;&c_d;&c_z;&c_mol;</trd>
    <trd lng="bg">&c_l;&c_je;&c_b;&c_je;&c_d;</trd>
    <trd lng="br">alarc'h</trd>
    <trd lng="ca">cigne</trd>
    <trd lng="cs">labu&tcaron;</trd>
    <trd lng="de">Schwan</trd>
    <trd lng="el">&kappa;&ypsilon_ton;&kappa;&ny;&omikron;&sigma_fina;</trd>
    <trd lng="en">swan</trd>
    <trd lng="es">cisne</trd>
    <trd lng="fr">cygne</trd>
    <trd lng="he">&bet;&resh;&bet;&vav;&resh;</trd>
    <trd lng="hu">hatty&uacute;</trd>
    <trd lng="id">angsa</trd>
    <trd lng="it">cigno</trd>
    <trd lng="nl">zwaan</trd>
    <trd lng="pl">&lstroke;ab&eogonek;d&zacute;</trd>
    <trd lng="pt">cisne</trd>
    <trd lng="ro">leb&abreve;d&abreve;</trd>
    <trd lng="ru">&c_l;&c_je;&c_b;&c_je;&c_d;&c_mol;</trd>
    <trd lng="sk">labu&tcaron;</trd>
    <trd lng="zh">&#40288; <pr>g&#468;</pr></trd>
  </snc>
</drv>

<drv mrk="cign.C0o">
  <kap><tld lit="C"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Cyg</mlg>
  <snc mrk="cign.C0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> (<trd lng="la">Cygnus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_L;&c_je;&c_b;&c_je;&c_d;&c_z;&c_mol; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="ca">Cigne</trd>
  <trd lng="cs">labu&tcaron;</trd>
  <trd lng="de">Schwan</trd>
  <trd lng="en">Cignus</trd>
  <trd lng="es">Cisne</trd>
  <trd lng="fr">Cygne<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="he">&bet;&#1468;&#1463;&resh;&bet;&#1468;&vav;&#1468;&resh;</trd>
  <trd lng="it">Cigno</trd>
  <trdgrp lng="ja">
    <trd>&#12495;&#12463;&#12481;&#12519;&#12454;  <pr>&#12399;&#12367;&#12385;&#12423;&#12358;</pr></trd>,
    <trd>&#35433;&#32854;  <pr>&#12375;&#12379;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="pl">&Lstroke;ab&eogonek;d&zacute;  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Leb&abreve;da  <klr>(constela&tcomma;ie)</klr></trd>
  <trd lng="sk">labu&tcaron;</trd>
  <trd lng="uk">&c_l;&c_je;&c_b;&c_ib;&c_d;&c_mol;</trd>
</drv>

</art>
