<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hermetik.xml,v 1.18 2023/10/16 05:12:01 revo Exp $">
<kap>
  <rad>hermetik</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hermetik.0a">
  <kap><tld/>a</kap>
  <snc mrk="hermetik.0a.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Lasanta eniri a&ubreve; eliri neniun fluida&jcirc;on; provizita
      per tia fermosistemo:
      <ekz>
        la kovrilo estis <tld/>e fermebla per krampoj
        <fnt>
          <bib>KrM</bib>
          <lok>3. Ne Gutas Mielo El La &Ccirc;ielo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        peza fera pordoluko <tld/>e fermebla
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 31</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie estas silento, kvaza&ubreve; en <tld/>e &scirc;losita skatolo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> maro <klr>[furiozas kontra&ubreve;]</klr> hermetike &scirc;lositajn vitrojn
        de l&#x2019; iluminiloj, superver&scirc;ante ilin
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fenestroj estis <tld/>e fermitaj per adhera rubando
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2003/008113.html">Forpaso-forumo</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_je;&c_r;&c_m;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
    <trd lng="ca">herm&egrave;tic</trd>
    <trdgrp lng="de">
      <trd>hermetisch</trd>,
      <trd>luft- und <ind>wasserdicht</ind></trd>
    </trdgrp>
    <trd lng="el">&epsilon;&rho;&my;&eta;&tau;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
    <trdgrp lng="en">
      <trd>hermetic</trd>,
      <trd>sealed</trd>
    </trdgrp>
    <trd lng="es">herm&eacute;tico</trd>
    <trd lng="fr">&eacute;tanche</trd>
    <trdgrp lng="hu">
      <trd>hermetikus</trd>,
      <trd>t&ouml;m&iacute;tett</trd>,
      <trd>szigetelt</trd>
    </trdgrp>
    <trd lng="it">ermetico<klr> (a chiusura stagna)</klr></trd>
    <trd lng="nl">hermetisch</trd>
    <trdgrp lng="pl">
      <trd>hermetyczny</trd>,
      <trd>szczelny</trd>
    </trdgrp>
    <trd lng="pt">herm&eacute;tico</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_je;&c_r;&c_m;&c_je;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_g;&c_je;&c_r;&c_m;&c_je;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_p;&c_l;&c_o;&c_t;&c_n;&c_o; &c_z;&c_a;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="hermetik.0a.malklara">
    <uzo tip="stl">RAR</uzo>
    <dif>
      Malfacile enirebla de spirito,
      malfacile komprenebla,
      <ref tip="vid" cel="klar.mal0a">malklara</ref>,
      <ref tip="vid" cel="obskur.0a.malklara">obskura<sncref/></ref>:
      <ekz>
        <tld/>aj dogmoj de arkaikaj obskurantismoj
        <fnt>
          <aut>Gilbert Ledon</aut>
            <vrk><url ref="&Monato;/2000/007018.php">Kulturo de milito</url></vrk>
            <bib>Monato</bib>
            <lok>2000/06, p. 5</lok>
          </fnt>;
      </ekz>
      <ekz>
        <nom>Dao De Jing</nom> de <nom>La&ubreve;zi</nom> <klr>[&#x2026;]</klr>
        de &ccirc;iam estas <tld/>a por ordinaraj legantoj
        <fnt>
          <aut>Wang Chongfang</aut>,
          <vrk>Enkonduko al Dao De Jing</vrk>,
          <lok>[2012?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="neuxtr1.0a">ne&ubreve;trala</ref>,
      <ref cel="obskur.0a.malklara">obskura<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_n;&c_je;&c_d;&c_a;&c_s;&c_t;&c_u;&c_p;&c_n;&c_y; &c_r;&c_a;&c_z;&c_u;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ju;</trd>,
      <trd>&c_n;&c_je;&c_z;&c_r;&c_a;&c_z;&c_u;&c_m;&c_je;&c_l;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>unergr&uuml;ndlich</trd>,
      <trd>undurchdringlich</trd>
    </trdgrp>
    <trd lng="en">hermetic<klr> (occult)</klr></trd>
  </snc>
<ref tip="sin" cel="hermet.0a">hermeta</ref>
  <trdgrp lng="cs">
    <trd>hermetick&yacute;</trd>,
    <trd>neprody&scaron;n&yacute;</trd>,
    <trd>vzduchot&ecaron;sn&yacute;</trd>
  </trdgrp>
  <trd lng="fr">herm&eacute;tique</trd>
  <trdgrp lng="ja">
    <trd>&#x5BC6;&#x5C01;&#x3057;&#x305F;  <pr>&#x307F;&#x3063;&#x3077;&#x3046;&#x3057;&#x305F;</pr></trd>,
    <trd>&#x5BC6;&#x9589;&#x3057;&#x305F;  <pr>&#x307F;&#x3063;&#x307A;&#x3044;&#x3057;&#x305F;</pr></trd>,
    <trd>&#x96E3;&#x89E3;&#x306A;  <pr>&#x306A;&#x3093;&#x304B;&#x3044;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hermetick&yacute;</trd>,
    <trd>vzduchotesn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_je;&c_r;&c_m;&c_je;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_shch;&c_ib;&c_l;&c_mol;&c_n;&c_o; &c_z;&c_a;&c_k;&c_r;&c_i;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="hermetik.0igi">
  <kap>
    <tld/>igi
  </kap>
  <dif>
    Igi ion <tld/>a.
  </dif>
  <snc ref="hermetik.0a.TEK">
     <ekz>
        la akvotunelo <klr>[&#x2026;]</klr> estas <klr>[&#x2026;]</klr> betonumita por <tld/>igi &gcirc;in
        <fnt>
          <aut>Josef Mader-Graz</aut>
          <vrk><url ref="https://archive.org/details/AustriaEsperantisto_1925_n012_jul/page/77/mode/1up">A&ubreve;stria Esperantisto (2a jaro, No. 7, Jul 1925)</url></vrk>
          <lok>La Akvoenergia Elektrejo en Teigitsch-Arnstein</lok>
        </fnt>;
      </ekz>
      <ekz>
        por <tld/>igi la ujon, oni devas turni la remetitan kovrilon
        <fnt>
          <aut>Xie Ruifeng</aut>
          <vrk><url ref="http://esperanto.china.org.cn/2021-04/08/content_77387933.htm">esperanto.china.org.cn (8 Apr 2021)</url></vrk>
          <lok>&Ccirc;u vi vidis &leftquot;premkuirpoton&rightquot; anta&#x1D4; 2 000 jaroj?</lok>
        </fnt>.
      </ekz>
  <trd lng="de">hermetisch <ind>verschlie&szlig;en</ind></trd>
  </snc>
  <snc ref="hermetik.0a.malklara">
     <ekz>
        siajn perceptojn ili <tld/>igis kontra&ubreve; la eblo ke <klr>[&#x2026;]</klr>
        <fnt>
          <aut>Edgar Allan Poe (tr. Edwin Grobe)</aut>
          <vrk><url ref="&Gutenberg;files/18326/18326-h/18326-h.htm">La Murdoj de Kadavrejo-Strato</url></vrk>
        </fnt>;
     </ekz>
     <ekz>
        avino kuiris la karnon <klr>[&#x2026;]</klr>, <tld/>igante la supon
        <fnt>
          <aut>Mark HAMME</aut>
          <vrk><url ref="https://bulteno.esperanto-usa.org/a/2022/01/pdf/bulteno.pdf">Usona Esperantisto (No. 2022:1, Jan-Feb)</url></vrk>
          <lok>Testudosupo (p. 14)</lok>
        </fnt>.
     </ekz>
  <trdgrp lng="de">
    <trd><ind>unergr&uuml;ndlich</ind> machen</trd>,
    <trd>abschotten</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_r;&c_m;&c_e;&c_t;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">seal<klr> (tr)</klr></trd>
</drv>


</art>

