<drv mrk="sol.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="sol.0a.neakompanata">
    <dif>
      Neakompanata, ne estanta kun aliaj, <ref cel="izol.0ita.KOMUNE">izolita</ref>:
      <ekz>
        li &ccirc;esis promeni <tld/>a kaj komencis ser&ccirc;i societon
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili apartenu al vi <tld/>a, sed ne al aliaj kun vi
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 5:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi blasfemas, vi <tld/>a suferos
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 9:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        preteriris <tld/>a paro, brako sub brako
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        forkuros la tuta popolo, kiu estas kun li, tiam mi mortigos la re&gcirc;on <tld/>an
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 17:2</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
       <trd>&c_a;&c_d;&c_z;&c_ib;&c_n;</trd>,
       <trd>&c_s;&c_a;&c_m;&c_o;&c_t;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="hu">egyed&uuml;l</trd>
    <trdgrp lng="ru">
       <trd>&c_o;&c_d;&c_i;&c_n; <klr tip="amb">(&c_v;
       &c_z;&c_n;&c_a;&c_ch;.
       &c_p;&c_r;&c_i;&c_l;&c_a;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_g;&c_o;)</klr></trd>,
       <trd>&c_o;&c_d;&c_i;&c_n;&c_o;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sol.0a.nehelpata">
    <dif>
      Nehelpata:
      <ekz>
        li prenis sur siajn &scirc;ultrojn nian tutan aferon, <klr>[&#x2026;]</klr> li <tld/>a
        subtenadis &gcirc;in tiel longe
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        kion povas fari li <tld/>a kontra&ubreve; cent dudek homoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>?
      </ekz>
    </dif>
    <trdgrp lng="be">
       <trd>&c_a;&c_d;&c_z;&c_ib;&c_n;</trd>,
       <trd>&c_s;&c_a;&c_m;</trd>
    </trdgrp>
    <trdgrp lng="hu">
       <trd>egyed&uuml;l</trd>,
       <trd>egymaga</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_d;&c_i;&c_n; <klr tip="amb">(&c_v;
     &c_z;&c_n;&c_a;&c_ch;.
     &c_p;&c_r;&c_i;&c_l;&c_a;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_g;&c_o;)</klr></trd>
  </snc>
  <snc mrk="sol.0a.neniukroma">
    <dif>
      Unu kaj neniu kroma:
      <ekz>
        en la tuta afero mi <tld/>a estas kulpa
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
         ne diru, ke &ccirc;iuj junuloj filoj de la re&gcirc;o estas
         mortigitaj, &ccirc;ar nur <nom>Amnon</nom> <tld/>a mortis
         <fnt><bib>MT</bib>, <lok>&Sa2; 13:32</lok>.</fnt>;
      </ekz>
      <ekz>
        &scirc;prucu sur ilin nur unu <tld/>an guton de tiu &ccirc;i trinka&jcirc;o, tiam iliaj brakoj kaj
        fingroj disfalos
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="nur.0">nur</ref>
    <trd lng="be">&c_a;&c_d;&c_z;&c_ib;&c_n;&c_y;</trd>
    <trd lng="hu">egyesegyed&uuml;l</trd>
    <trd lng="ru">&c_je;&c_d;&c_i;&c_n;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    <trd lng="ms">keseorangan</trd>
  </snc>
  <snc mrk="sol.0a.ununura">
    <dif>
      <ref tip="dif" cel="nur.unu0a">Ununura</ref> ekzistanta,
      <ref tip="sin" cel="unik.0a.ununura">unika</ref>:
      <ekz>
        mi estis filo dorlotata kaj <tld/>a de mia patrino
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 4:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        funebru kiel pro <tld/>a filo, ploru maldol&ccirc;e
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 6:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la nuna minuto Esperanto, vere, aperas kiel <tld/>a kandidato por lingvo
        internacia
        <fnt>
          <bib>EE</bib>
          <lok>VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estas la <tld/>a vojo, kiu kun plena certeco alkondukos nin al nia celo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>.
      </ekz>
     </dif>
     <trd lng="be">&c_a;&c_d;&c_z;&c_ib;&c_n;&c_y;</trd>
     <trd lng="de">einziger</trd>
     <trdgrp lng="hu">
       <trd>egyed&uuml;li</trd>,
       <trd>egyetlen</trd>
