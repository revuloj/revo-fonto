    <trd>&#x43E;&#x434;&#x438;&#x43D;</trd>,
    <trd>&#x43E;&#x434;&#x438;&#x43D;&#x43E;&#x43A;&#x43E;</trd>,
    <trd>&#x43F;&#x43E;&#x43E;&#x434;&#x438;&#x43D;&#x446;&#x456;</trd>,
    <trd>&#x441;&#x430;&#x43C;&#x456;&#x442;&#x43D;&#x43E;</trd>,
    <trd>&#x441;&#x430;&#x43C;&#x43E;&#x442;&#x43D;&#x44C;&#x43E;</trd>,
    <trd>&#x442;&#x456;&#x43B;&#x44C;&#x43A;&#x438;</trd>,
    <trd>&#x43B;&#x438;&#x448;&#x435;</trd>
  </trdgrp>
</drv>

