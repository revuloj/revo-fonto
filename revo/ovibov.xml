<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ovibov.xml,v 1.9 2024/03/26 10:30:29 revo Exp $">
<kap>
  <rad>ovibov</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ovibov.0o">
  <kap><tld/>o</kap>
  <snc mrk="ovibov.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bovaspekta <ref tip="super" cel="antilo.0o.subfamiliano">antilopo</ref>
      kun dika felo, el malvarma Nordameriko kaj
      Gronlando (<trd lng="la">Ovibos moschatus</trd>):
      <ekz>
        oni taksas, ke estas sepdek &gcirc;is okdek mil <tld/>oj sur la
        insulo
        <fnt>
          <aut>Arono kaj Karlina</aut>,
          <vrk><url ref="http://radioverda.com/programoj/2010/3/14/rv161-20100314.html">
          Radio Verda</url></vrk>,
          <lok>RV161, 2010-03-14</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mosk.0ulo">moskulo</ref>
    <bld
      lok="&WCU;/thumb/e/ec/Musk-Ox_(Male).jpg/640px-Musk-Ox_(Male).jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (masklo), Norda Kebeko
        <fnt>
          <aut>Alan D. Wilson, naturespicsonline</aut>
          <lok><url ref="&WCW;/File:Musk-Ox_(Male).jpg"
              >https://commons.wikimedia.org/wiki/File:Musk-Ox_(Male).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_r;&c_a;&c_n;&c_a;&c_b;&c_y;&c_k;</trd>,
    <trd>&c_a;&c_w;&c_c;&c_a;&c_b;&c_y;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pi&zcaron;mo&ncaron;</trd>,
    <trd>pi&zcaron;mo&ncaron; severn&iacute;</trd>,
    <trd>pi&zcaron;mo&ncaron; v&yacute;chodn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Moschusochse</trd>,
    <trd>Bisamochse</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>buey almizclero</trd>,
    <trd>buey almizclado</trd>
  </trdgrp>
  <trd lng="fr">b&oelig;uf musqu&eacute;</trd>
  <trdgrp lng="pl">
    <trd>pi&zdot;mow&oacute;&lstroke;</trd>,
    <trd>w&oacute;&lstroke; pi&zdot;mowy</trd>
  </trdgrp>
  <trd lng="ro">bou moscat</trd>
  <trd lng="sk">pi&zcaron;mo&ncaron; severn&yacute;</trd>
  <trd lng="uk">&c_v;&c_ib;&c_v;&c_c;&c_je;&c_b;&c_i;&c_k; &c_m;&c_u;&c_s;&c_k;&c_u;&c_s;&c_n;&c_i;&c_j;</trd>
</drv>
</art>

