<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gorgx.xml,v 1.41 2024/03/05 15:22:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gor&gcirc;</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="gorgx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gorgx.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Interna kava parto de la <ref tip="malprt" cel="kol.0o.korpo">kolo</ref>, per kiu oni glutas
      kaj spiras:
      <ekz>
        teni iun &ccirc;e la <tld/>o;
      </ekz>
      <ekz>
        <tld/>premi iun <klr>(<tld/>preme sufoke mortigi)</klr>;
      </ekz>
      <ekz>
        nia veturigisto komencis kriegi el la tuta <tld/>o
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
      <ekz>
        vi metu tran&ccirc;ilon al via <tld/>o se vi estas avidulo
        <fnt><bib>MT</bib>, <lok>&Sen; 23:2</lok></fnt>;
      </ekz>
      <ekz>
        la mieltavolo estas dol&ccirc;a por via <tld/>o
        <fnt><bib>MT</bib>, <lok>&Sen; 24:13</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Kluttig</nom> malfermis la bu&scirc;on, lia <ind><tld/>nodo</ind>
        <klr>(kp <ref cel="pom.gorgxo0o">gor&gcirc;opomo</ref>, <ref cel="pom.gorgxo0o">adampomo</ref>)</klr> levi&gcirc;is
        <fnt>
          <vrk><url ref="https://archive.org/stream/PacoGDREldono_1972/PacoGDREldono_1972_djvu.txt">Sceno el la filmo &leftquot;Nuda inter lupoj&rightquot;</url></vrk>
          <lok>Paco (Germana Eldono) , 1972, p.21</lok>
        </fnt>;
        <trd lng="de">Adamsafel</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la morto jam tenas lin &ccirc;e la <tld/>o;
      </ekz>
      <ekz>
        &gcirc;i jam staras al mi en la <tld/>o
        <klr>(tedegas min)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_r;&c_l;&c_a;</trd>
  <trd lng="bo">&t_ra;&t_naro;&t_nga;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>Gurgel</trd>,
    <trd>Rachen</trd>
  </trdgrp>
  <trd lng="en">throat</trd>
  <trd lng="es">garganta</trd>
  <trd lng="fr">gorge<klr tip="ind"> (anat.)</klr></trd>
  <trd lng="hu">torok</trd>
  <trd lng="id"><ind>tenggorok</ind>an</trd>
  <trdgrp lng="ja">
    <trd>&#12398;&#12393;</trd>,
    <trd>&#12398;&#12393;&#39318;  <pr>&#12398;&#12393;&#12367;&#12403;</pr></trd>,
    <trd>&#21693;&#21897;  <pr>&#12356;&#12435;&#12371;&#12358;</pr></trd>,
    <trd>&#23777;&#35895;  <pr>&#12365;&#12423;&#12358;&#12371;&#12367;</pr></trd>,
    <trd>&#23665;&#38291;&#12398;&#29421;&#12356;&#36947;  <pr>&#12373;&#12435;&#12363;&#12435;&#12398;&#12379;&#12414;&#12356;&#12393;&#12358;</pr></trd>,
    <trd>&#29421;&#12356;&#28286;&#21475;  <pr>&#12379;&#12414;&#12356;&#12431;&#12435;&#12371;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">keel</trd>
  <trd lng="pl">gard&lstroke;o</trd>
  <trdgrp lng="pt">
   <trd>garganta</trd>,
   <trd>goela</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_l;&c_o;</trd>
  <trd lng="tr">bo&gbreve;az<klr>(anatomi)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_o;&c_r;&c_l;&c_o;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_t;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_sh;&c_i;&c_j;&c_k;&c_a;&nbsp;(&c_p;&c_l;&c_ja;&c_sh;&c_k;&c_i;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21549; <pr>h&aacute;ng</pr></trd>,
    <trd>&#21657; <pr>l&oacute;ng</pr></trd>,
    <trd>&#21721; <pr>ku&agrave;i</pr></trd>,
    <trd>&#21897; <pr>h&oacute;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gorgx.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="gorgx.en0igxi.ANA">
    <dif>
      Eni&gcirc;i, resti en la <tld/>o:
      <ekz>
        ankora&ubreve; nun, kiam ni parolas pri &gcirc;i, okazas ke la koro
        bategas, la
        okulo ploretas kaj la vo&ccirc;o en<tld/>i&gcirc;as
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>impresoj de svedoj, voja&gcirc;antaj
        en
        Rusujo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_ja;&c_c;&c_mol; &c_u; <ind>&c_g;&c_o;&c_r;&c_l;&c_je;</ind></trd>
  <trd lng="en">get stuck in the throat</trd>
  <trdgrp lng="fr">
    <trd>rester dans la <ind>gorge</ind></trd>,
    <trd>s'<ind>enrouer</ind><klr tip="amb"> (voix)</klr></trd>
  </trdgrp>
  <trd lng="nl">in de <ind>keel</ind> blijven steken</trd>
  <trd lng="pl">stan&aogonek;&cacute; w gardle</trd>
  <trd lng="tr">bo&gbreve;az&inodot;nda kalmak</trd>
</drv>

<drv mrk="gorgx.plen0e">
  <kap>plen<tld/>e</kap>
  <snc mrk="gorgx.plen0e.plenvocxe">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="vocx.plen0e">Plenvo&ccirc;e</ref>:
      <ekz>
        trupeto da viglaj gejunuloj, kiuj plen<tld/>e kantis
        <fnt>
          <aut>Hans Weinhengst</aut>,
          <vrk>Tur-Strato 4</vrk>,
          <lok><url ref="http://www.fadedpage.com/showbook.php?pid=20131216">
        Fadedpage</url>
          </lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas tre gaja, e&ccirc; petolema, ridegas brue, plen<tld/>e
        <fnt><aut>L. Banmer</aut>, <bib>Lanti</bib>,
        <lok>anta&ubreve;parolo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a; &c_w;&c_s;&c_jo; <ind>&c_g;&c_o;&c_r;&c_l;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>lauthals</trd>
  </trdgrp>
  <trd lng="en">full-throated</trd>
  <trd lng="fr">&agrave; pleine gorge</trd>
  <trd lng="ja">&#22823;&#22768;&#12391;  <pr>&#12362;&#12362;&#12372;&#12360;&#12391;</pr></trd>
  <trd lng="nl">uit volle <ind>borst</ind></trd>
  <trd lng="pl">pe&lstroke;nym g&lstroke;osem</trd>
  <trd lng="ru">&c_v;&c_o; &c_v;&c_s;&c_jo; <ind>&c_g;&c_o;&c_r;&c_l;&c_o;</ind></trd>
  <trd lng="uk">&c_n;&c_a; &c_c;&c_ib;&c_l;&c_je;/&c_p;&c_o;&c_v;&c_n;&c_je; &c_g;&c_o;&c_r;&c_l;&c_o;</trd>
</drv>

<drv mrk="gorgx.rugx0ulo">
  <kap>ru&gcirc;<tld/>ulo
      <fnt><vrk><url ref="&STEB;vortaroj/komunlingvaj-nomoj-de-europaj-birdoj.htm">Komunlingvaj
        nomoj de e&ubreve;ropaj birdoj
        </url></vrk></fnt>
  </kap>
  <snc mrk="gorgx.rugx0ulo.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:birdoj" cel="bird.0o">Birdo</ref>, <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">specio</ref>
      (<trd lng="la">Erithacus rubecula</trd>) el la familio
      mu&scirc;kaptuledoj (<trd lng="la">Muscicapidae</trd>), kun ru&gcirc;kolora kolo:
      <ekz>
        <tld/>oj vigle defendas sian &ccirc;asteritorion kie ili trovas insektojn por sin
        nutri;
      </ekz>
      <ekz>
        la E&ubreve;ropa ru&gcirc;<tld/>ulo estis unu el multaj specioj originale priskribitaj de
        <nom>Linnaeus</nom> en sia verko de la 18a jarcento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18754">Ru&gcirc;gor&gcirc;ulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/f3/Erithacus_rubecula_with_cocked_head.jpg/600px-Erithacus_rubecula_with_cocked_head.jpg" prm="CC BY-SA 3.0">E&ubreve;ropa ru&gcirc;<tld/>ulo (Erithacus rubecula)
        <fnt>
          <aut>Francis C. Franklin, propra verko</aut>
          <lok><url ref="&WCW;/File:Erithacus_rubecula_with_cocked_head.jpg">&WCW;/File:Erithacus_rubecula_with_cocked_head.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Rotkehlchen</trd>
    <trdgrp lng="zh">
    <trd>&#27431;&#27954;&#30693;&#26356;&#40479; <pr>&omacron;uzh&omacron;uzh&imacron;g&egrave;ngni&#462;o</pr></trd>,
    <trd>&#30693;&#26356;&#40165; <pr>zh&imacron;g&emacron;ngni&#462;o</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_a;&c_sh;&c_y;&c_j;&c_k;&c_a;</trd>
  <trd lng="en">(European) robin</trd>
  <trd lng="es">petirrojo</trd>
  <trd lng="fr">rouge-gorge</trd>
  <trdgrp lng="ja">
    <trd>&#12520;&#12540;&#12525;&#12483;&#12497;&#12467;&#12510;&#12489;&#12522;</trd>,
    <trd>&#12525;&#12499;&#12531;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rudzik <klr>zwyczajny</klr></trd>,
    <trd>raszka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_l;&c_mol;&c_sh;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_ib;&c_d;&c_sh;&c_je; &c_m;&c_a;&c_l;&c_i;&c_n;&c_ib;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="gorgx.tralavila0on">
  <kap>tralavi la <tld/>on</kap> <fnt><bib>PrV</bib></fnt>
  <snc mrk="gorgx.tralavila0on.FIG">
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Trinki ion, precipe <ref tip="vid" cel="drink.0i">drinki</ref>:
      <ekz>
        nu, mi ne diru ebrii&gcirc;i, sed tamen mi bone tralavos la <tld/>on
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>tria libro, &ccirc;apitro
        47a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">die <ind>Kehle</ind> durchsp&uuml;len</trd>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_a;&c_ch;&c_y;&c_c;&c_mol; <ind>&c_g;&c_o;&c_r;&c_l;&c_a;</ind></trd>
  <trd lng="en">(fig.) have an (alcoholic) drink</trd>
  <trd lng="fr">se <ind>rincer la dalle</ind></trd>
  <trd lng="nl">je <ind>keel</ind> spoelen<klr tip="ind">
  (drinken)</klr></trd>
  <trd lng="pl">przep&lstroke;uka&cacute; gard&lstroke;o</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_m;&c_o;&c_ch;&c_i;&c_t;&c_mol; &c_g;&c_o;&c_r;&c_l;&c_o;</trd>
</drv>
</art>
