<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sved.xml,v 1.40 2024/03/25 07:28:38 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>sved</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="sved.0o">
  <kap>
    <tld/>o
    <fnt><bib>ZR</bib></fnt>
  </kap>
  <snc mrk="sved.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de <ref tip="malprt" cel="sved.S0ujo"><tld lit="S"/>ujo</ref>.
    </dif>
    <trd lng="de">schwedischer Staatsb&uuml;rger</trd>
    <trd lng="nl">Zweed</trd>
  </snc>
  <snc mrk="sved.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        intertempe, krom <tld/>ojn, vi renkontas rusojn, osetojn kaj
        talibanojn, kvankam tre mar&gcirc;ene
        <fnt><aut>S. Derevjanni&hcirc;</aut>,
        <vrk><url ref="&Monato;/2001/nknu.html">Jen la maltroigo de
        hodia&ubreve;</url></vrk>,
        <bib>Monato</bib>,
        <lok>2001/07, p. 26</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">suec</trd>
    <trd lng="de">Schwede</trd>
    <trd lng="nl">Zweed</trd>
  </snc>
  <trd lng="be">&c_sh;&c_v;&c_e;&c_d;</trd>
  <trd lng="bg">&c_sh;&c_v;&c_je;&c_d;</trd>
  <trd lng="en">Swede</trd>
  <trdgrp lng="es">
    <trd>sueco</trd>,
    <trd>sueca</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_w;&a_y_hamza;&a_d;&f_y;</trd>
  <trdgrp lng="fr"> 
    <trd>Su&eacute;dois</trd>
    <trd>Su&eacute;doise<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="gl">
    <trd>sueco</trd>,
    <trd>sueca</trd>
  </trdgrp>
  <trd lng="hu">sv&eacute;d</trd>
  <trdgrp lng="io">
    <trd>Suediano</trd>,
    <trd>Suedo</trd>
  </trdgrp>
  <trd lng="it">svedese<klr> (sost.)</klr></trd>  
  <trd lng="ja">&#x30B9;&#x30A6;&#x30A7;&#x30FC;&#x30C7;&#x30F3;&#x4EBA;  <pr>&#x30B9;&#x30A6;&#x30A7;&#x30FC;&#x30C7;&#x30F3;&#x3058;&#x3093;</pr></trd>
  <trd lng="no">svenske</trd>
  <trd lng="pl">Szwed</trd>
  <trdgrp lng="pt">
    <trd>sueco</trd>,
    <trd>sueca</trd>
  </trdgrp>
  <trd lng="tok">jan pi ma Wensa</trd>
  <trd lng="ru">&c_sh;&c_v;&c_je;&c_d;</trd>
  <trd lng="sv">svensk</trd>
  <trd lng="uk">&c_sh;&c_v;&c_je;&c_d;</trd>
  <trd lng="vo">Sved&auml;nan</trd>
  <trd lng="zh">&#x745E;&#x5178;&#x4EBA; <pr>ru&#xEC;di&#x1CE;nr&#xE9;n</pr></trd>
</drv>

<drv mrk="sved.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco, lingvo a&ubreve; &scirc;tato:
      <ekz>
        el la <tld/>a tradukis Sabira St&aring;hlberg
        <fnt><aut>G. Mattos</aut>,
        <vrk><url ref="&Monato;/2000/lmal.html">Etoso de
        liberi&gcirc;o kaj de
        liberigo</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/10, p. 30</lok>
        </fnt>;
      </ekz>
      <ekz>
        Hjalmar S&ouml;derberg (1869-1941) estis tre sukcesa
        verkisto <tld/>a
        <fnt><aut>D. Broadribb</aut>,
        <vrk><url ref="&Monato;/2000/pelt.html">Facile
        delogebla</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/04, p. 31</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_v;&c_e;&c_d;&c_z;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_sh;&c_v;&c_je;&c_d;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">suec</trd>
  <trd lng="cs">&scaron;v&eacute;dsk&yacute;</trd>
  <trd lng="de">schwedisch</trd>
  <trd lng="en">Swedish <klr> (adj. and language)</klr></trd>
  <trdgrp lng="es">
    <trd>sueco</trd>,
    <trd>sueca</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_w;&a_y_hamza;&a_d;&f_y;</trd>
   <trdgrp lng="fr"> 
    <trd>su&eacute;dois</trd>
    <trd>su&eacute;doise<klr> (adj.)</klr></trd>
    <trd>su&eacute;dois<klr> (langue)</klr></trd>
   </trdgrp>
  <trd lng="fy">Sweedsk</trd>
  <trdgrp lng="gl">
    <trd>sueco</trd>,
    <trd>sueca</trd>
  </trdgrp>
  <trd lng="hu">sv&eacute;d</trd>
  <trdgrp lng="io">
    <trd>Suediana</trd>,
    <trd>Sueda</trd>
  </trdgrp>
  <trd lng="it">svedese<klr> (agg.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30A6;&#x30A7;&#x30FC;&#x30C7;&#x30F3;&#x306E;  <pr>&#x3059;&#x3046;&#x3047;&#x30FC;&#x3067;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x30B9;&#x30A6;&#x30A7;&#x30FC;&#x30C7;&#x30F3;&#x8A9E;&#x306E;  <pr>&#x3059;&#x3046;&#x3047;&#x30FC;&#x3067;&#x3093;&#x3054;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">Zweeds</trd>
  <trd lng="no">svensk</trd>
  <trd lng="pl">szwedzki</trd>
  <trdgrp lng="pt">
    <trd>sueco</trd>,
    <trd>sueca</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_v;&c_je;&c_d;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">&scaron;v&eacute;dsky</trd>
  <trd lng="sv">svensk</trd>
  <trd lng="tok">pi ma Wensa</trd>
  <trd lng="uk">&c_sh;&c_v;&c_je;&c_d;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
  <trd lng="vo">Sved&auml;nik</trd>
  <trdgrp lng="zh">
    <trd>&#x745E;&#x5178;&#x8A9E; <pr>Ru&#xEC;di&#x1CE;ny&#x1D4;</pr></trd>,
    <trd>&#x745E;&#x5178;&#x8BED; <pr>Ru&#xEC;di&#x1CE;ny&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sved.S0ujo">
  <kap><tld lit="S"/>ujo
   <fnt><bib>ZR</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">SE</mlg>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta
      en norda <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      inter Norvegujo kaj Finnlando.
    </dif>
   <ref tip="sin" cel="svedi.0o">Svedio</ref>
  </snc>
  <trd lng="af">Swede</trd>
  <trd lng="be">&c_Sh;&c_v;&c_e;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_Sh;&c_v;&c_je;&c_c;&c_i;&c_ja;</trd>
  <trd lng="ca">Su&egrave;cia</trd>
  <trd lng="cs">&Scaron;v&eacute;dsko</trd>
  <trd lng="cy">Sweden</trd>
  <trd lng="da">Sverige</trd>
  <trd lng="de">Schweden</trd>
  <trd lng="el">&Sigma;&omikron;&ypsilon;&epsilon;&delta;&jota_acute;&alfa;</trd>
  <trd lng="en">Sweden</trd>
  <trd lng="es">Suecia</trd>
  <trd lng="eu">Suedia</trd>
  <trd lng="fa">&a_s;&a_w;&a_y_hamza;&a_d;</trd>
  <trd lng="fi">Ruotsi</trd>
  <trdgrp lng="fr">
    <trd>Su&egrave;de</trd>,
    <trd>Royaume de Su&egrave;de</trd>
  </trdgrp>
  <trd lng="fy">Sweden</trd>
  <trd lng="gd">an <ind>tSualainn</ind></trd>
  <trd lng="gl">Suecia</trd>
  <trd lng="hr">&Scaron;vedska</trd>
  <trd lng="hu">Sv&eacute;dorsz&aacute;g</trd>
  <trd lng="ia">Svedia</trd>
  <trd lng="id">Swedia</trd>
  <trd lng="io">Suedia</trd>
  <trd lng="is">Sv&iacute;&#xFE;j&oacute;&#xF0;</trd>
  <trd lng="it">Svezia</trd>
  <trd lng="la">Suecia</trd>
  <trd lng="lt">&Scaron;vedija</trd>
  <trd lng="lv">Zviedrija</trd>
  <trd lng="nl">Zweden</trd>
  <trd lng="no">Sverige</trd>
  <trd lng="oc">Su&egrave;cia</trd>
  <trd lng="pl">Szwecja</trd>
  <trd lng="pt">Su&eacute;cia</trd>
  <trd lng="ru">&c_Sh;&c_v;&c_je;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">&Scaron;v&eacute;dsko</trd>
  <trd lng="sv">Sverige</trd>
  <trd lng="sw">Uswidi</trd>
  <trd lng="tok">ma Wensa</trd>
  <trd lng="tr">&Idot;sve&ccedil;</trd>
  <trd lng="vo">Sved&auml;n</trd>
  <trd lng="zh">&#x745E;&#x5178; <pr>Ru&#xEC;di&#x1CE;n</pr></trd>
</drv>
