<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kapucen</rad>/o
</kap>
<drv mrk="kapuce.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Franciskana mona&hcirc;o, primitive almozpetanta, kun bruna robo kaj
      pinta kapu&ccirc;o:
      <ekz>
        kun barbo tiel longa, kiel tiu de <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_u;&c_c;&c_y;&c_n; <klr>(&c_m;&c_a;&c_n;&c_a;&c_h;)</klr></trd>
  <trd lng="de">Kapuziner</trd>
  <trd lng="en">Capuchin monk</trd>
  <trdgrp lng="fr">
    <trd>capucin<klr tip="ind"> (moine)</klr></trd>,
    <trd>moine capucin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kapucinus</trd>,
    <trd>csukly&aacute;s bar&aacute;t</trd>
  </trdgrp>
  <trd lng="nl">Capucijner monnik</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_u;&c_c;&c_i;&c_n; <klr>(&c_m;&c_o;&c_n;&c_a;&c_h;)</klr></trd>
</drv>
</art>
