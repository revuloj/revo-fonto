<art mrk="$Id: kapuce.xml,v 1.21 2024/05/10 12:37:47 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kapucen</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="kapuce.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="kapuce.0o.KAT">
    <uzo tip="fak">KAT</uzo>
    <dif>
      Franciskana <ref tip="super" cel="monahx.0o">mona&hcirc;o</ref>, primitive almozpetanta, kun bruna robo
      kaj pinta <ref tip="super" cel="kapucx.0o.TEKS">kapu&ccirc;o</ref>:
      <ekz>
        li mem en <tld/>a robo jam en&scirc;teli&gcirc;is
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;atataj de turistoj estas anka&ubreve; la katakomboj, precipe tiuj de la <tld/>oj el la
        16a jarcento
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011265.php"
              >En Palermo subtere</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        &Ccirc;u <tld/>aro Ni estas? &parsl; Silentanto, inter nobelaro
        <fnt>
          <bib>SrT</bib>
          <lok>Libro V.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_u;&c_c;&c_y;&c_n;
  <klr>(&c_m;&c_a;&c_n;&c_a;&c_h;)</klr></trd>
  <trd lng="br">kabusin<klr> (lean)</klr></trd>
  <trd lng="ca">caputx&iacute;</trd>
  <trd lng="de">Kapuziner</trd>
  <trd lng="en">Capuchin monk</trd>
  <trdgrp lng="fr">
    <trd>capucin<klr tip="ind"> (moine)</klr></trd>,
    <trd>moine capucin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kapucinus</trd>,
    <trd>csukly&aacute;s bar&aacute;t</trd>
  </trdgrp>
  <trd lng="it">cappuccino<klr> (frate)</klr></trd>
  <trd lng="ja">&#12459;&#12503;&#12481;&#12531;&#20250;&#20462;&#36947;&#22763;  <pr>&#12459;&#12503;&#12481;&#12531;&#12363;&#12356;&#12375;&#12421;&#12358;&#12393;&#12358;&#12375;</pr></trd>
  <trd lng="nl">kapucijn<klr tip="ind"> (monnik)</klr></trd>
  <trd lng="pl">kapucyn</trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_u;&c_c;&c_i;&c_n;
  <klr>(&c_m;&c_o;&c_n;&c_a;&c_h;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_p;&c_u;&c_c;&c_i;&c_n;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_u;&c_c;&c_i;&c_n; (&c_r;&c_ib;&c_d; &c_m;&c_a;&c_v;&c_p;)</trd>
  </trdgrp>
  <trd lng="zh">&#20711;&#24125;&#29492; <pr>s&emacron;ngm&agrave;oh&oacute;u</pr></trd>
</drv>
</art>
