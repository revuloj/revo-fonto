<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glis.xml,v 1.18 2023/11/11 11:36:35 revo Exp $">
<kap>
  <ofc>9</ofc><rad>glis</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="glis.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <snc mrk="glis.0i.AVI">
    <dif>
      Senage <ref tip="super" cel="flug.0i.sin_movi">flugi</ref>, sin
      lasante al la portado de aero:
      <ekz>
        ekfluginte, &gcirc;i &ccirc;iam svingas kelkfoje siajn flugilojn kaj sekve
        <tld/>as anta&ubreve;en
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="http://es.chinabroadcast.cn/1/2005/06/08/1@25515.htm">Malabara
          bunta bucero</url></vrk>,
          <lok>2005-06-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_n;&c_e;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">planar</trd>
  <trd lng="cs">plachtit na v&ecaron;troni</trd>
  <trdgrp lng="de">
    <trd>segeln<klr tip="amb"> (Flugzeug)</klr></trd>,
    <trd>gleiten<klr tip="amb"> (Flugzeug)</klr></trd>
  </trdgrp>
  <trd lng="en">glide</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_b;&f_k; &f_p;&a_r;&f_y;&a_d;&a_n;</trd>,
    <trd>&a_d;&a_r; <ind>&a_h;&a_w;&a_A;</ind> &a_sh;&a_n;&a_A;&a_w;&a_r; &a_b;&a_w;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">planer</trd>
  <trdgrp lng="it">
    <trd>veleggiare<klr> (volare)</klr></trd>,
    <trd><ind>volare</ind> a vela</trd>,
    <trd>planare<klr> (volare)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x6ED1;&#x7A7A;&#x3059;&#x308B;  <pr>&#x304B;&#x3063;&#x304F;&#x3046;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">zweven</trd>
  <trd lng="pt">planar</trd>
  <trd lng="sk">plachti&tcaron; na vetroni</trd>
  <trd lng="tr">s&uuml;z&uuml;lmek<klr>(ku&scedil;, u&ccedil;ak)</klr></trd>
  <trd lng="uk">&#x43F;&#x43B;&c_a;&#x43D;&c_je;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="glis.0ilo">
  <kap><ofc>9</ofc><tld/>ilo <fnt><bib>SPV</bib></fnt></kap>
  <snc mrk="glis.0ilo.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Senmotora <ref tip="super" cel="avi.0o.AVI">avio</ref>, kiun alia
      flugma&scirc;ino a&ubreve; a&ubreve;to tiras supren, kaj per kiu oni poste
      <tld/>as:
      <ekz>
        la unuaj homaj flug-eksperimentoj estis per <tld/>iloj
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Aviadilo&rightquot;,
        2005-11-13</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_n;&c_e;&c_r;</trd>
  <trd lng="ca">planador</trd>
  <trdgrp lng="cs">
    <trd>gliz&eacute;r</trd>,
    <trd>kluz&aacute;k</trd>,
    <trd>v&ecaron;tro&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Segelflugzeug</trd>
  <trd lng="en">glider</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_w;&a_A;&a_s;&a_damma;&a_r;</trd>,
    <trd>&f_g;&a_l;&a_A;&f_y;&a_d;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">planeur</trd>
  <trd lng="it">aliante</trd>
  <trd lng="ja">&#x30B0;&#x30E9;&#x30A4;&#x30C0;&#x30FC;</trd>
  <trd lng="nl">zweefvliegtuig</trd>
  <trd lng="pt">planador</trd>
  <trdgrp lng="sk">
    <trd>klz&aacute;k</trd>,
    <trd>vetro&ncaron;</trd>
  </trdgrp>
  <trd lng="tr">plan&ouml;r</trd>
  <trd lng="uk">&#x43F;&#x43B;&c_a;&#x43D;&c_je;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x6C34;&#x4E0A;&#x98DB;&#x6A5F; <pr>shu&#x1D0;sh&#xE0;ngf&#x113;ij&#x12B;</pr></trd>,
    <trd>&#x6ED1;&#x884C;&#x8005; <pr>hu&#xE1;x&#xED;ngzh&#x11B;</pr></trd>,
    <trd>&#x6ED1;&#x7FD4;&#x673A; <pr>hu&#xE1;xi&#xE1;ngj&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>

