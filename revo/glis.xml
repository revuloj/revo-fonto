<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>glis</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="glis.0i">
  <kap><tld/>i</kap>
  <snc mrk="glis.0i.AVI">
    <dif>
      Senage <ref tip="super" cel="flug.0i.sin_movi">flugi</ref>, sin
      lasante al la portado de aero:
      <ekz>
        ekfluginte, &gcirc;i &ccirc;iam svingas kelkfoje siajn flugilojn kaj sekve
        <tld/>as anta&ubreve;en
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url
          ref="http://es.chinabroadcast.cn/1/2005/06/08/1@25515.htm">Malabara
          bunta bucero</url></vrk>,
          <lok>2005-06-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>segeln<klr tip="amb"> (Flugzeug)</klr></trd>,
    <trd>gleiten<klr tip="amb"> (Flugzeug)</klr></trd>
  </trdgrp>
  <trd lng="en">glide</trd>
  <trd lng="fr">planer</trd>
  <trd lng="nl">zweven</trd>
  <trd lng="pt">planar</trd>
</drv>

<drv mrk="glis.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="glis.0ilo.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Senmotora <ref tip="super" cel="avi.0o.AVI">avio</ref>, kiun alia
      flugma&scirc;ino a&ubreve; a&ubreve;to tiras supren, kaj per kiu oni poste
      <tld/>as:
      <ekz>
        la unuaj homaj flug-eksperimentoj estis per <tld/>iloj
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Aviadilo&rightquot;,
        2005-11-13</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Segelflugzeug</trd>
  <trd lng="en">glider</trd>
  <trd lng="fr">planeur</trd>
  <trd lng="nl">zweefvliegtuig</trd>
  <trd lng="pt">planador</trd>
</drv>
</art>

