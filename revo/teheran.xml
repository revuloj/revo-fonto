<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: teheran.xml,v 1.12 2025/04/01 22:28:26 revo Exp $">
<kap><ofc>10</ofc>
    <rad>Teheran</rad>/o,
    <var><kap>Tehrano</kap></var>
    <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="teheran.0o">
  <kap><tld/>o,
   <var><kap>Tehrano</kap></var>
  </kap>
  <snc mrk="teheran.0o.cxefurbo">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="urb.cxef0o" lst="voko:&ccirc;efurboj">&Ccirc;efurbo</ref>
      de <ref tip="malprt" cel="iran.I0o">Irano</ref>:
      <ekz>
        kun &ccirc;irka&ubreve; 8.3 milionoj da lo&gcirc;antoj <tld/>o estas la plej granda
        urbo
        <klr>[...]</klr> en Okcidenta Azio <klr>[...]</klr> &ccirc;irka&ubreve; 99%
        de la
        lo&gcirc;antaro komprenas kaj parolas la persan
        <fnt><bib>Viki</bib>, <lok>Teherano</lok></fnt>;
      </ekz>
      <ekz>
         pirogoj de niaj armenaj amikoj estis diable bongustaj,
         do ankora&ubreve; multaj islamanoj kaj anka&ubreve; neislamanoj el
         foraj anguloj de Tehrano venis kaj a&ccirc;etis ilin
          <fnt>
              <vrk><url ref="http://becxjo.blogspot.com/2005_09_01_archive.html">Mia sopiro pri
              man&gcirc;a&jcirc;o</url></vrk>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_T;&c_e;&c_g;&c_je;&c_r;&c_a;&c_n;</trd>
  <trd lng="cs">Teher&aacute;n</trd>
  <trd lng="de">Teheran</trd>
  <trdgrp lng="en">
    <trd>Tehran</trd>,
    <trd>Teheran</trd>
  </trdgrp>
  <trd lng="fr">T&eacute;h&eacute;ran</trd>
  <trd lng="ja">&#12486;&#12504;&#12521;&#12531;</trd>
  <trd lng="pl">Teheran</trd>
  <trd lng="sk">Teher&aacute;n</trd>
  <trd lng="uk">&c_T;&c_je;&c_g;&c_je;&c_r;&c_a;&c_n; (&c_s;&c_t;&c_o;&c_l;&c_i;&c_c;&c_ja;)</trd>
  <trdgrp lng="zh">
    <trd>&#20234;&#26391;&#39318;&#37117; <pr>y&imacron;l&#462;ngsh&#466;ud&umacron;</pr></trd>,
    <trd>&#24503;&#40657;&#20848; <pr>d&eacute;h&egrave;l&aacute;n</pr></trd>,
    <trd>&#24503;&#40657;&#34349; <pr>d&eacute;h&egrave;l&aacute;n</pr></trd>
  </trdgrp>
</drv>
</art>

