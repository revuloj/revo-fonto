<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>Teheran</rad>/o, 
    <var><kap>Tehrano</kap></var>
    <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="teheran.0o">
  <kap><tld/>o</kap>
  <snc mrk="teheran.0o.cxefurbo">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="urb.cxef0o" lst="voko:&ccirc;efurboj">&Ccirc;efurbo</ref>
      de <ref tip="malprt" cel="iran.0o">Irano</ref>:
      <ekz>
        kun &ccirc;irka&ubreve; 8.3 milionoj da lo&gcirc;antoj <tld/>o estas la plej granda
        urbo 
        <klr>[...]</klr> en Okcidenta Azio <klr>[...]</klr> &ccirc;irka&ubreve; 99%
        de la 
        lo&gcirc;antaro komprenas kaj parolas la persan
        <fnt><bib>Viki</bib>, <lok>Teherano</lok></fnt>;
      </ekz>
      <ekz>
         pirogoj de niaj armenaj amikoj estis diable bongustaj,
         do ankora&ubreve; multaj islamanoj kaj anka&ubreve; neislamanoj el
         foraj anguloj de Tehrano venis kaj a&ccirc;etis ilin
          <fnt>
              <vrk><url
              ref="http://becxjo.blogspot.com/2005_09_01_archive.html"
                  >Mia sopiro pri man&gcirc;a&jcirc;o</url></vrk>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Teheran</trd>
</drv>
</art>

