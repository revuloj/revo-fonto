<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>piment</rad>/o
</kap>
<drv mrk="piment.0o">
  <kap><tld/>o</kap>
  <snc mrk="piment.0o.spico">
    <uzo tip="fak">KUI</uzo>
    <dif>
       <ref tip="super" cel="spic.0o">Spico</ref> el la akregustaj sekigitaj beroj
       de la <tld/>ujo:
       <ekz>
         <tld/>o fajnigas &ccirc;efe supojn, sa&ubreve;cojn, marina&jcirc;ojn, kukojn kaj likvorojn
         <fnt><bib>Viki</bib>, <lok>pimento</lok></fnt>.
       </ekz>
    </dif>
    <trd lng="de">Piment</trd>
  </snc>
  <snc mrk="piment.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif> 
      Genro el la familio <ref tip="super" cel="mirtac.0oj">mirtacoj</ref>
      (<trd lng="la">Pimenta</trd>), kies beroj estas uzataj kiel spico.
    </dif>
    <trd lng="de">Pimentbaum</trd>
  </snc>
</drv>
</art>

<!--
$Log$
-->
</vortaro>