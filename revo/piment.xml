<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: piment.xml,v 1.15 2024/02/17 06:43:07 revo Exp $">
<kap>
  <rad>piment</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="piment.0o">
  <kap><tld/>o</kap>
  <snc mrk="piment.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="spic.0o" lst="voko:spicoj">Spico</ref> el la
      akregustaj
      sekigitaj beroj de la <tld/>ujo:
      <ekz>
        <tld/>o fajnigas &ccirc;efe supojn, sa&ubreve;cojn,
        marina&jcirc;ojn, kukojn kaj likvorojn
        <fnt><bib>Viki</bib>, <lok>pimento</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_n;&c_g;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_a;&c_je; &c_z;&c_je;&c_l;&c_l;&c_je;</trd>,
      <trd>&c_ja;&c_m;&c_a;&c_j;&c_s;&c_k;&c_ib; &c_p;&c_je;&c_r;&c_a;&c_c;</trd>,
      <trd>&c_p;&c_a;&c_h;&c_u;&c_ch;&c_y; &c_p;&c_je;&c_r;&c_a;&c_c;</trd>,
      <trd>&c_d;&c_u;&c_h;&c_m;&c_ja;&c_n;&c_y; &c_p;&c_je;&c_r;&c_a;&c_c;</trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_ja;&c_m;&c_a;&c_j;&c_k;&c_a; &c_p;&c_i;&c_p;&c_je;&c_r;</trd>,
      <trd>&c_b;&c_a;&c_h;&c_a;&c_r;</trd>
    </trdgrp>
    <trd lng="de">Piment</trd>
    <trd lng="en">allspice</trd>
    <trd lng="hu">szegf&udblac;bors <klr tip="amb">(f&udblac;szer)</klr></trd>
    <trdgrp lng="nl">
      <trd>piment</trd>,
      <trd>jamaicapeper</trd>
    </trdgrp>
    <trd lng="zh">&#x591A;&#x9999;&#x679C; <pr>du&#x14D;xi&#x101;nggu&#x1D2;</pr></trd>
</snc>
  <snc mrk="piment.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el la familio <ref tip="malprt" cel="mirtac.0oj">mirtacoj</ref>
      (<trd lng="la">Pimenta</trd>), kies beroj estas uzataj kiel spico.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_ib;&c_m;&c_e;&c_n;&c_t;</trd>,
      <trd>&c_p;&c_ib;&c_m;&c_e;&c_n;&c_t;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>anglick&eacute; ko&rcaron;en&iacute;</trd>,
      <trd>jamajsk&yacute; pep&rcaron;</trd>,
      <trd>nov&eacute; ko&rcaron;en&iacute;</trd>,
      <trd>piment</trd>
    </trdgrp>
    <trd lng="de">Pimentbaum</trd>
    <trd lng="en">pimenta</trd>
    <trd lng="hu">szegf&udblac;bors <klr tip="amb">(fa)</klr></trd>
    <trd lng="nl">pimentboom</trd>
    <trdgrp lng="pt">
      <trd>piment&atilde;o</trd>,
      <trd>pimento</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">piment de la jama&iuml;que</trd>
  <trdgrp lng="ja">
    <trd>&#x30AA;&#x30FC;&#x30EB;&#x30B9;&#x30D1;&#x30A4;&#x30B9;</trd>,
    <trd>&#x30D4;&#x30E1;&#x30F3;&#x30C8;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>korzennik</trd>,
    <trd>ziele angielskie</trd>
  </trdgrp>
  <trd lng="ro">ienibahar</trd>
  <trdgrp lng="sk">
    <trd>nov&eacute; korenie</trd>,
    <trd>piment</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&c_h;&c_u;&c_ch;&c_i;&c_j; &#x43F;&c_je;&c_r;&c_je;&c_c;&#x44C;</trd>,
    <trd>&#x43F;&c_ib;&#x43C;&c_je;&#x43D;&c_t;</trd>
  </trdgrp>
</drv>
</art>
