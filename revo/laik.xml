<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>3</ofc>
      <rad>laik</rad>/o
    </kap>
    <drv mrk="laik.0o">
      <kap><tld/>o</kap>
      <snc mrk="laik.0o.pasxtato">
        <uzo tip="fak">REL</uzo>        
        <dif>
          <ref tip="super" cel="eklezi.0ano">Ekleziano</ref>
          kiu ne estas pastro a&ubreve; mona&hcirc;o. 
        </dif>
        <refgrp tip="vid">
          <ref cel="profan.0ulo">profanulo</ref>,
          <ref cel="eklezi.0ulo">ekleziulo</ref>,
          <ref cel="religi.0ulo">religiulo</ref>.
        </refgrp>
        <trd lng="ru">&c_m;&c_i;&c_r;&c_ja;&c_n;&c_i;&c_n;</trd>
      </snc>
      <snc mrk="laik.0o.FIG">
        <uzo tip="stl">FIG</uzo><dif>
          Homo, ne havanta apartan kompetentecon; nefakulo, malspertulo:
          <ekz>
            <tld/>o en lingvistiko ofte ne konscias la alilandan
            devenon de esprimo, kiun li uzas.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="dileta.0o">diletanto</ref>.
        </refgrp>
        <trd lng="ru">&c_p;&c_r;&c_o;&c_f;&c_a;&c_n;</trd>
      </snc>
    </drv>
    <drv mrk="laik.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Havanta karakteron de <tld/>o <sncref ref="laik.0o.pasxtato"/>:
          <ekz>
            <tld/>a lernejo, instruisto.
          </ekz>
        </dif>
        <trd lng="ru">&c_s;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;
          <klr tip="ind">(&c_n;&c_je; &c_c;&c_je;&c_r;&c_k;&c_o;&c_v;&c_n;&c_y;&c_j;)</klr></trd>
      </snc>
      <snc>
        <uzo tip="fak">POL</uzo>
        <uzo tip="stl">EVI</uzo>
        <dif>
          Kontra&ubreve;klerikala.  
        </dif>
        <rim>
          Evitinda franca&jcirc;o.  Tiusence devus esti &leftquot;<tld/>isto&rightquot; a&ubreve;
          &leftquot;<tld/>ismano&rightquot; a&ubreve; simile.
          <aut>Sergio Pokrovskij</aut>
        </rim>
      </snc>
    </drv>
  </art>
<!--
$Log$
-->
</vortaro>
