<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bromeli.xml,v 1.11 2023/04/04 07:24:50 revo Exp $">
<kap>
  <rad>bromeliac</rad>/oj
</kap>
<drv mrk="bromeli.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref>  de <ref tip="malprt"
      cel="kotile.unu0aj">unukotiledonaj</ref>
      tropikaj plantoj (<trd lng="la">Bromeliaceae</trd>):
      <ekz>
        la KAM-me&hcirc;anismo estas pruvita &ccirc;e 25 plantaj familioj, <klr>[&#8230;]</klr> krom la
        nomdonanta familio de krasulacoj pliaj familioj estas: kaktacoj, <tld/>oj,
        orkideacoj <klr>[&#8230;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=279549"
              >KAM-fotosintezo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="ananas.0ujo">ananasujo</ref>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_m;&c_e;&c_l;&c_ib;&c_je;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trdgrp lng="de">
    <trd>Ananasgew&auml;chse</trd>,
    <trd>Bromeliazeen</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>anan&aacute;szf&eacute;l&eacute;k</trd>,
    <trd>brom&eacute;liaf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_o;&c_m;&c_je;&c_l;&c_i;&c_je;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_a;&c_n;&c_a;&c_n;&c_a;&c_s;&c_o;&c_v;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="fr">brom&eacute;liac&eacute;es</trd>
</drv>
</art>
