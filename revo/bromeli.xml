<vortaro>
<art mrk="$Id$">
<kap>
  <rad>bromeliac</rad>/oj
</kap>
<drv mrk="bromeli.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.unu0aj">unukotiledonaj</ref>
      tropikaj plantoj (<trd lng="la">Bromeliaceae</trd>).
    </dif>
    <ref tip="sub" cel="ananas.0ujo">ananasujo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Ananasgew&auml;chse</trd>,
    <trd>Bromeliazeen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_o;&c_m;&c_je;&c_l;&c_i;&c_je;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_a;&c_n;&c_a;&c_n;&c_a;&c_s;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="fr">brom&eacute;liac&eacute;es</trd>
</drv>
</art>
