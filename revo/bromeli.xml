<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>bromeli</rad>/acoj
</kap>
<drv mrk="bromeli.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.unu0aj">unukotiledonaj</ref>
      tropikaj plantoj (<trd lng="la">Bromeliaceae</trd>).
    </dif>
    <ref tip="sub" cel="ananas.0ujo">ananasujo</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Ananasgew&auml;chse</trd>,
    <trd>Bromeliazeen</trd>
  </trdgrp>
</drv>
</art>
