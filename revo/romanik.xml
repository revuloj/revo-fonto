<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><rad>romanik</rad>/o <fnt><bib>PIV1</bib></fnt></kap>

    <drv mrk="romanik.0o">
      <kap><tld/>o</kap>
      <snc>
	<dif>
	  Arta stilo ekinta en la 10a, prospera en la 11a kaj 12a jarcentoj
	  precipe en okcidenta E&ubreve;ropo, favoranta simplajn formojn kaj
	  fre&scirc;ajn, popoleskajn ornama&jcirc;ojn, kun multaj regionaj
	  substiloj.
	</dif>
	<subsnc mrk="romanik.0o.ARKI">
	  <uzo tip="fak">ARKI</uzo>
	  <dif>
	    En ar&hcirc;itekturo <tld/>o ekuzis <ref tip="vid"
	      cel="volb.0o">volbojn</ref>,
	    duoncirklaj arkojn, galeriojn, <ref tip="vid"
	      cel="pilast.apog0o">apog-pilastrojn</ref>.
	  </dif>
	</subsnc>
	<subsnc mrk="romanik.0o.BELA.pentrarto">
	  <uzo tip="fak">BELA</uzo>
	  <dif>
	    En pentrarto <tld/>o ornamis monumentojn kaj ilustris librojn
	    per plenkolorajn simbolaj figuroj kaj abstrakta&jcirc;ojn.
	  </dif>
	</subsnc>
      </snc>
      <trd lng="de">Romanik</trd>
      <trd lng="es">rom&aacute;nico</trd>
      <trdgrp lng="fr">
	<trd>style <ind>roman</ind></trd>
      </trdgrp>
      <trd lng="hu">rom&aacute;n st&iacute;lus</trd>
      <trd lng="nl">romaanse stijl</trd>
      <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j; &c_s;&c_t;&c_i;&c_l;&c_mol;</trd>
    </drv>

    <drv mrk="romanik.0a">
      <kap><tld/>a</kap>
      <snc>
	<dif>
	  Konforma al <tld/>o.
	</dif>
      </snc>
      <trd lng="de">romanisch</trd>
      <trd lng="fr">roman<klr> (adj.)</klr></trd>
      <trd lng="hu">rom&aacute;n st&iacute;lus&uacute;</trd>
      <trd lng="nl">romaans<klr tip="ind"> (stijl)</klr></trd>
      <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>
  </art>
