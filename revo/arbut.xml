<art mrk="$Id$">
<kap>
<rad>arbut</rad>/o
</kap>
<drv mrk="arbut.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Frukto de <tld/>ujo, iomete fragsimila. 
    </dif>
