<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arbut.xml,v 1.24 2025/10/25 14:27:03 revo Exp $">
<kap>
  <rad>arbut</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="arbut.0o">
  <kap><tld/>o</kap>
  <snc>
     <fnt><bib>PV</bib></fnt>
     <dif>
       Frukto de <tld/>ujo, iomete fragsimila.
     </dif>
     <trd lng="be">&c_p;&c_l;&c_o;&c_d; &c_s;&c_u;&c_n;&c_ib;&c_ch;&c_n;&c_a;&c_g;&c_a; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  </snc>
  <snc mrk="arbut.0o.arbedo">
    <uzo tip="fak">BOT</uzo>
    <fnt><bib>PIV2</bib></fnt>
    <dif>
     <tld lit="A"/>ujo:
     <ekz>
        oni nomas Madridon <klr>[&#x2026;]</klr> la urbo de la urso kaj la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2728">Madrido</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="malprt" cel="makis.0o">makiso</ref> <klr>[&#x2026;]</klr> kun verdaj kverkoj, korkokverkoj, mirtoj, erikoj,
        okcidentaj <tld/>oj, oleandroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=160907"
              >Makiso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Kanaria <tld/>o estas minacata
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=343084">Kanaria arbuto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_u;&c_n;&c_ib;&c_ch;&c_n;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
    <trdgrp lng="zh">
    <trd>&#x6768;&#x6885;&#x5C5E; <pr>y&aacute;ngm&eacute;ish&#x1D4;</pr></trd>,
    <trd>&#x694A;&#x6885;&#x5C6C; <pr>y&aacute;ngm&eacute;ish&#x1D4;</pr></trd>,
    <trd>&#x85E4;&#x5730;&#x8393; <pr>t&eacute;ngdem&eacute;i</pr></trd>,
    <trd>&#x91CE;&#x8349;&#x8393;&#x6811; <pr>y&ecaron;c&#x1CE;om&eacute;ish&ugrave;</pr></trd>,
    <trd>&#x91CE;&#x8349;&#x8393;&#x6A39; <pr>y&ecaron;c&#x1CE;om&eacute;ish&ugrave;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>medv&ecaron;dice</trd>,
    <trd>planika</trd>
  </trdgrp>
  <trd lng="de">Erdbeerbaumfrucht</trd>
  <trd lng="en">Arbutus berry</trd>
  <trd lng="es">madro&ntilde;o</trd>
  <trd lng="fr">arbouse</trd>
  <trd lng="he">&qof;&tet;&lamed;&bet;</trd>
  <trd lng="hu">szam&oacute;ca</trd>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30D6;&#x30C4;&#x30B9;&#x306E;&#x5B9F;  <pr>&#x30A2;&#x30EB;&#x30D6;&#x30C4;&#x30B9;&#x306E;&#x307F;</pr></trd>
  <trd lng="pl">chru&sacute;cina jagodna</trd>
  <trd lng="sk">medvedica</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_n;&c_i;&c_ch;&c_n;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_s;&c_u;&c_n;&c_i;&c_ch;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="arbut.0ujo">
  <kap><tld/>ujo</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      Genro el familio <ref tip="super" cel="erikac.0oj">erikacoj</ref>
      (<trd lng="la">Arbutus</trd>), arbedo kreskanta &ccirc;e Mediteraneo:
      <ekz>
        la Statuo de la Urso kaj <tld lit="A"/>ujo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=277729">Statuo de la Urso kaj
          Arbutujo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/3/3f/Arbutus_unedo_RF.jpg/640px-Arbutus_unedo_RF.jpg"
      prm="CC BY 4.0"
      >folioj kaj fruktoj de <tld/>ujo (Arbutus unedo)
        <fnt>
          <aut>Robert Flogaus-Faust, propra verko</aut>
          <lok><url ref="&WCW;/File:Arbutus_unedo_RF.jpg"
              >https://commons.wikimedia.org/wiki/File:Arbutus_unedo_RF.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_n;&c_ib;&c_ch;&c_n;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>medv&ecaron;dice</trd>,
    <trd>planika</trd>
  </trdgrp>
  <trd lng="de">Erdbeerbaum</trd>
  <trd lng="el">&kappa;&omikron;&ypsilon;&my;&alfa;&rho;&jota;&alfa_ton;</trd>
  <trd lng="en">arbutus (strawberry tree)</trd>
  <trd lng="es">madro&ntilde;o</trd>
  <trd lng="fr">arbousier</trd>
  <trd lng="hu">szam&oacute;cabokor</trd>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30D6;&#x30C4;&#x30B9;</trd>
  <trd lng="pl">truskawkowe drzewo</trd>
  <trd lng="ru">&c_z;&c_je;&c_m;&c_l;&c_ja;&c_n;&c_i;&c_ch;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sk">medvedica</trd>
</drv>
</art>
