<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>arbut</rad>/o
</kap>
<drv mrk="arbut.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Frukto de <tld/>ujo, iomete fragsimila.
     </dif>
   </snc>
   <trd lng="fr">arbouse</trd>
   <trd lng="es">madro&ntilde;o</trd>
</drv>
<drv mrk="arbut.0ujo">
   <kap><tld/>ujo</kap>
   <snc>
     <dif>
       Genro el familio <ref tip="super" cel="erik.0acoj">erikacoj</ref>
       (<trd lng="la">Arbutus</trd>), arbedo kreskanta &ccirc;e Mediteraneo.
     </dif>
   </snc>
   <trd lng="de">Erdbeerbaum</trd>
   <trd lng="en">strawberry tree</trd>
   <trd lng="fr">arbousier</trd>
   <trd lng="ru">&c_z;&c_je;&c_m;&c_l;&c_ja;&c_n;&c_i;&c_ch;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
   <trd lng="es">madro&ntilde;o</trd>
</drv>
</art>
