<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kitel.xml,v 1.25 2023/12/03 09:58:49 revo Exp $">
    <kap>
      <ofc>*</ofc>
      <rad>kitel</rad>/o
    </kap>
    <drv mrk="kitel.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">TEKS</uzo>
      <snc mrk="kitel.0o.bluzo">
 <dif>
   Vasta manikhava vesto el vulgara &scirc;tofo, kovranta de &scirc;ultro
   &gcirc;is genuoj; longa <ref tip="super" cel="bluz.0o">bluzo</ref>:
   <ekz>
     oni vestis &scirc;in <klr>(la forbruligotan sor&ccirc;istinon)</klr>
     per <tld/>o el maldelikata saktolo
     <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>;
   </ekz><ekz>
     vi fari&gcirc;is eleganta sinjorino, kaj mi devas montri min al
            vi en mia malpura <tld/>o!
     <fnt><bib>Fab3</bib>, <lok>Ib kaj malgranda Kristino</lok></fnt>;
   </ekz><ekz>
     kiam do Simon Petro a&ubreve;dis, ke &gcirc;i estas la Sinjoro, li alzonis al
     si sian <tld/>on (&ccirc;ar li estis nuda) kaj &jcirc;etis sin en la maron
     <fnt><bib>NT</bib>, <lok>&Joh; 21:7</lok></fnt>;
   </ekz><ekz>
     Bartos Antonin, fervojista pensiulo, postulas la kompenson
     por la neniigita mort<tld/>o
     <fnt>
       <aut>Karel &Ccirc;APEK, tr. Josef CHVOSTA</aut>:
       <vrk>Jura kazo</vrk>. <lok>Nica Literatura Revuo, 3:6</lok>
     </fnt>.
   </ekz>
 </dif>
 <trd lng="en">frock</trd>
 <trd lng="fa">&a_r;&a_w;&a_l;&a_b;&a_A;&a_s;&f_y;</trd>
 <trd lng="fr">souquenille </trd>
 <trd lng="ca">guardapols</trd>
 <trdgrp lng="oc">
   <trd>sarguina</trd>,
   <trd>sarguin</trd>
 </trdgrp>
 <trdgrp lng="pl">
   <trd>kitel</trd>,
   <trd>fartuch</trd>
 </trdgrp>
 <trdgrp lng="ru">
   <trd>&c_b;&c_a;&c_l;&c_a;&c_h;&c_o;&c_n;</trd>,
   <trd>&c_b;&c_l;&c_u;&c_z;&c_a;</trd>
 </trdgrp>
      </snc>
      <snc mrk="kitel.0o.hxalato">
 <dif>
   <ref tip="dif" cel="hxalat.0o">&Hcirc;alato</ref>:
   <ekz>
     sufi&ccirc;as vidigi sur televida ekrano aktoron kun blanka
     <tld/>o en falsa laboratorio por ke la publiko eka&ccirc;etu
     la saman markon de lavpulvoro, kiun li tenas mane
     <fnt>
       <aut>Jorge Camacho</aut>:
       <vrk>La liturgio de l' foiro (Elementoj por ekzegezo)</vrk>
     </fnt>.
   </ekz>
 </dif>
 <trd lng="en">smock</trd>
 <trd lng="fa">&a_kh;&a_l;&a_ayn;&a_t;</trd>
 <trdgrp lng="ru">
   <trd>&c_s;&c_p;&c_je;&c_c;&c_o;&c_v;&c_k;&c_a;</trd>,
   <trd>&c_r;&c_a;&c_b;&c_o;&c_ch;&c_i;&c_j; &c_h;&c_a;&c_l;&c_a;&c_t;</trd>
 </trdgrp>
      </snc>
      <rim>
 Nur la senco <tld/>o<sncref ref="kitel.0o.bluzo"/> estas fundamenta.
      </rim>
      <trd lng="de">Kittel</trd>
      <trdgrp lng="hu">
 <trd>k&ouml;peny<klr tip="ind"> (munka-)</klr></trd>,
 <trd>munkak&ouml;peny</trd>
      </trdgrp>
      <trdgrp lng="ja">
    <trd>&#x30B9;&#x30E2;&#x30C3;&#x30AF;</trd>,
    <trd>&#x30AA;&#x30FC;&#x30D0;&#x30FC;&#x30AA;&#x30FC;&#x30EB;  <pr>&#x304A;&#x30FC;&#x3070;&#x30FC;&#x304A;&#x30FC;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
 <trd>kiel<klr tip="ind"> (stofjas)</klr></trd>,
 <trd>stofjas</trd>
      </trdgrp>
      <trdgrp lng="sk">
    <trd>halena</trd>,
    <trd>pracovn&#xFD; pl&#xE1;&#x161;&#x165;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bunda</trd>,
    <trd>halena</trd>,
    <trd>kazajka</trd>,
    <trd>pracovn&#xED; pl&#xE1;&#x161;&#x165;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x431;&#x43E;&#x447;&#x438;&#x439; &#x445;&#x430;&#x43B;&#x430;&#x442;</trd>,
    <trd>&#x441;&#x43F;&#x435;&#x446;&#x456;&#x432;&#x43A;&#x430;</trd>,
    <trd>&#x440;&#x43E;&#x431;&#x43E;&#x447;&#x430; &#x431;&#x43B;&#x443;&#x437;&#x430;</trd>
  </trdgrp>
</drv>
    
    <drv mrk="kitel.trud0o">
  <kap>trud<tld/>o</kap>
  <snc mrk="kitel.trud0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <tld lit="K"/>o kun longaj manikoj, uzata por senmovigi
      psikiatrajn pacientojn a&ubreve; dan&gcirc;erajn personojn:
      <ekz>
        li ku&scirc;is, vestita per trud<tld/>o, firme ligita al la feraj
        rungoj de sia lito
        <fnt><bib>LOdE</bib>, <lok>2006:12</lok></fnt>
      </ekz>
    </dif>
    <trd lng="pl">kaftan bezpiecze&nacute;stwa</trd>
  </snc>
   <snc mrk="kitel.trud0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io kio bremsas la disvolvi&gcirc;on:
      <ekz>
        Flandra &scirc;tato devas reakiri sian suverenecon per retiri&gcirc;o el
        trud<tld/>o de monda kapitalismo 
        <fnt><vrk>Meervoud, maldekstra suverenista
        cerbumkomitato</vrk></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">camisa de for&ccedil;a</trd>
  <trd lng="cs">sv&#x11B;rac&#xED; kazajka</trd>
  <trd lng="de">Zwangsjacke</trd>
  <trd lng="fr">camisole de force</trd>
  <trd lng="hu">k&eacute;nyszerzubbony</trd>
  <trdgrp lng="ja">
    <trd>&#x62D8;&#x675F;&#x8863;  <pr>&#x3053;&#x3046;&#x305D;&#x304F;&#x3044;</pr></trd>,
    <trd>&#x62D8;&#x675F;&#x670D;  <pr>&#x3053;&#x3046;&#x305D;&#x304F;&#x3075;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">dwangbuis</trd>
  <trd lng="oc">camisa de f&ograve;r&ccedil;a</trd>
  <trd lng="sk">zvieracia kazajka</trd>
  <trd lng="uk">&#x433;&#x430;&#x43C;&#x456;&#x432;&#x43D;&#x430; &#x441;&#x43E;&#x440;&#x43E;&#x447;&#x43A;&#x430;</trd>
  <trd lng="zh">&#x62D8;&#x675F;&#x8863; <pr>j&#x16B;sh&#xF9;y&#x12B;</pr></trd>
</drv>
