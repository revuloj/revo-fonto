<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kitel</rad>/o
</kap>
<drv mrk="kitel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Vasta manikhava vira vesto el vulgara &scirc;tofo, kovranta de &scirc;ultro
      &gcirc;is genuoj; longa bluzo.  
    </dif>
  </snc>
  <trd lng="en">smock</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_je;&c_c;&c_o;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_o;&c_ch;&c_i;&c_j; &c_h;&c_a;&c_l;&c_a;&c_t;</trd>,
    <trd>&c_k;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
