<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lambrekin.xml,v 1.2 2019/10/12 11:10:27 revo Exp $">
<kap>
  <rad>lambrekin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lambrekin.0o">
  <kap><tld/>o</kap>
  <snc mrk="lambrekin.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">HER</uzo>
    <dif>
      &Scirc;irma&jcirc;o ordinare <ref tip="vid"
      cel="feston.0o.KOMUNE">festona</ref>,
      kiu orname pendas la&ubreve; la supra rando de tegmento, 
      pordo, kurteno...:
      <ekz>
        kurtenoj, <tld/>oj, pasamentoj kaj &ccirc;iaj kudra&jcirc;oj
        <fnt>
        <bib>Salamandroj</bib>,
        <lok>libro 1a, &ccirc;apitro 3a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="lambrekin.0o.RIM_SIN">
    Alia esperantigo: <em>lambrekeno</em>.
  </rim>
  <trd lng="fr">lambrequin</trd>
  <trdgrp lng="pl">
    <trd>labry {pl} <klr>(heraldyka)</klr></trd>,
    <trd>lambrekin</trd>
  </trdgrp>
  <trd lng="ro">pelmet</trd>
</drv>
</art>

