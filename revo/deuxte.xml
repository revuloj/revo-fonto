<art mrk="$Id: deuxte.xml,v 1.16 2023/11/04 13:16:27 revo Exp $">
<kap>
  <rad>de&ubreve;teri</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="deuxte.0o">
  <kap><tld/>o</kap>
  <snc mrk="deuxte.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Peza <ref tip="vid" cel="hidrog.0o">hidrogeno</ref>, kun
      atompezo 2, kies atomkerno konsistas el
      <ref tip="vid" cel="proton.0o">protono</ref> kaj
      <ref tip="vid" cel="neuxtr2.0o">ne&ubreve;trono</ref>:
      <ekz>
        brunaj nanoj devas havi almena&ubreve; 13,6 MJup (jupiterajn masojn), kiu estas la
        minimuma maso por fuzii <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Oliver Kim</aut>
          <vrk><url ref="&Monato;/2006/008796.php">Unua fotografa&jcirc;o de ekstersunsistema planedo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[oni]</klr> eltrovis per analizo de izotopoj (karbono-13 kaj <tld/>o), ke
        la metano <klr>[&#x2026;]</klr> devenas de la putrado de mar&ccirc;a akvo sur la tera
        supra&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fuss</aut>
          <vrk><url ref="&Monato;/2014/011348.php">Novtipa gasminado: &ccirc;u vere dan&gcirc;era?</url></vrk>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_e;&c_w;&c_t;&c_e;&c_r;&c_y;&c_j;</trd>,
    <trd>&c_d;&c_e;&c_j;&c_t;&c_e;&c_r;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="de">Deuterium</trd>
  <trd lng="fa">&a_d;&a_w;&a_t;&a_r;&f_y;&a_m;</trd>
  <trd lng="fr">deut&eacute;rium</trd>
  <trd lng="he">&#x5D3;&#x5D0;&#x5D5;&#x5D8;&#x5E8;&#x5D9;&#x5D5;&#x5DD;</trd>
  <trdgrp lng="hu">
    <trd>deut&eacute;rium</trd>,
    <trd>neh&eacute;zhidrog&eacute;n</trd>
  </trdgrp>
  <trd lng="nl">deuterium</trd>
  <trd lng="pl">deuter</trd>
  <trd lng="pt">deut&eacute;rio</trd>
  <trd lng="ru">&c_d;&c_je;&c_j;&c_t;&c_je;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">deut&eacute;rium</trd>
  <trd lng="uk">&#x434;&#x435;&#x439;&#x442;&#x435;&#x440;&#x456;&#x439;</trd>
  <trd lng="zh">&#x6C18; <pr>d&#x101;o</pr></trd>
</drv>
</art>
