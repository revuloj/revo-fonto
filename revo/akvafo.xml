<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akvafo.xml,v 1.21 2023/10/12 18:04:56 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>akvafort</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akvafo.0o">
  <kap><tld/>o</kap>
  <snc mrk="akvafo.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Gravura procedo per acido mordanta metalan surfacon.
    </dif>
    <trd lng="es">agua fuerte</trd>
  </snc>
  <snc mrk="akvafo.0o.verko">
    <dif>
      <tld lit="A"/>a&jcirc;o, verko farita per <tld/>a procedo:
      <ekz>
        &ccirc;ion, kion li posedis, li elspezis por <tld/>oj,
        da kiuj li havis plenan paperujon en fundo de sia
        &scirc;ranko
        <fnt>
          <aut>H. Sienkiewicz, trad. Lidja Zamenhof</aut>,
          <vrk><url ref="&EduInf;lern/librejo/0044/index.php">
          Lux in tenebris lucet</url></vrk>,
          <lok>en: Noveloj, 1925</lok>
        </fnt>.
      </ekz>
    </dif>
      <trd lng="es">aguafuerte</trd>
  </snc>
  <trd lng="be">&c_a;&c_f;&c_o;&c_r;&c_t;</trd>
  <trd lng="ca">aiguafort</trd>
  <trd lng="cs">lept</trd>
  <trd lng="fr">eau-forte</trd>
  <trd lng="he">&#x5EA;&#x5B7;&#x5D7;&#x5E8;&#x5B4;&#x5D9;&#x5D8;</trd>
  <trd lng="it">acquaforte</trd>
  <trdgrp lng="ja">
    <trd>&#x9285;&#x7248;  <pr>&#x3069;&#x3046;&#x3070;&#x3093;</pr></trd>,
    <trd>&#x30A8;&#x30C3;&#x30C1;&#x30F3;&#x30B0;</trd>
  </trdgrp>
  <trd lng="pt">&aacute;gua-forte</trd>
  <trd lng="ru">&c_o;&c_f;&c_o;&c_r;&c_t;</trd>
  <trd lng="sk">lept</trd>
  <trd lng="uk">&#x43E;&#x444;&#x43E;&#x440;&#x442; (&#x491;&#x440;&#x430;&#x432;&#x44E;&#x440;&#x430;)</trd>
</drv>
</art>
