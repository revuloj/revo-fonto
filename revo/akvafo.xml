<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>akvafort</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="akvafo.0o">
  <kap><tld/>o</kap>
  <snc mrk="akvafo.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Gravura procedo per acido mordanta metalan surfacon.
    </dif>
  </snc>
  <snc mrk="akvafo.0o.verko">
    <dif>
      <tld lit="A"/>a&jcirc;o, verko farita per <tld/>a procedo.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_f;&c_o;&c_r;&c_t;</trd>
  <trd lng="ca">aiguafort</trd>
  <trd lng="es">agua fuerte</trd>
  <trd lng="fr">eau-forte</trd>
  <trd lng="it">acquaforte</trd>
  <trd lng="pt">&aacute;gua-forte</trd>
  <trd lng="ru">&c_o;&c_f;&c_o;&c_r;&c_t;</trd>
  <trd lng="sk">lept</trd>
</drv>
</art>
