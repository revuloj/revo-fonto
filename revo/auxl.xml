<art mrk="$Id: auxl.xml,v 1.18 2022/03/12 15:33:56 revo Exp $">
<kap>
  <rad>a&ubreve;l</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="auxl.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxl.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">PED</uzo>
    <dif>
      <ref tip="super" cel="solen.0ejo">Solenejo</ref>, festo&ccirc;ambrego &ccirc;e 
      <ref tip="malprt" cel="univer2.0o">universitato</ref>:
      <ekz>
        la gimnazianoj en militserva a&gcirc;o estas kunvokitaj akurate je la
        8a kaj duono, en la granda <tld/>o
        <fnt><bib>KielAkvo</bib>, <lok>2:2</lok></fnt>;
      </ekz>
      <ekz>
        en la Granda <tld lit="A"/>o de la universitato multaj eminentuloj salutis,
        gratulis kaj bondeziris al la komuna iniciato de <nom>ULBS</nom> kaj <nom>AIS</nom>
        <fnt>
          <bib>LOdE</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">AIS fondis departementon kun la universitato de Sibiu-Hermannstadt </url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxdit.0o">a&ubreve;ditorio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_k;&c_t;&c_a;&c_v;&c_a;&c_ja; &c_z;&c_a;&c_l;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_f;&c_e;&c_r;&c_e;&c_n;&c_c;-&c_z;&c_a;&c_l;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>aula magna</trd>,
    <trd>paranimf</trd>,
    <trd>auditori</trd>
  </trdgrp>
  <trd lng="de">Aula</trd>
  <trdgrp lng="en">
    <trd>hall</trd>,
    <trd>auditorium</trd>
  </trdgrp>
  <trd lng="fr">salle de r&eacute;ception</trd>
  <trd lng="he">&alef;&vav;&lamed;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>aula</trd>,
    <trd>d&iacute;szterem</trd>
  </trdgrp>
  <trd lng="it"><ind>aula</ind> magna</trd>
  <trd lng="pl">aula</trd>
  <trd lng="ro">hala de asamblare</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_t;&c_o;&c_v;&c_y;&c_j; &c_z;&c_a;&c_l;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_f;&c_je;&c_r;&c_je;&c_n;&c_c;-&c_z;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">aula</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43A;&#x442;&#x43E;&#x432;&#x438;&#x439; &#x437;&#x430;&#x43B;</trd>,
    <trd>&#x43A;&#x43E;&#x43D;&#x444;&#x435;&#x440;&#x435;&#x43D;&#x446;-&#x437;&#x430;&#x43B;</trd>,
    <trd>&#x430;&#x432;&#x43B;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
