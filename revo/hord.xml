<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hord</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="hord.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nomadaro, nomada popolo.
    </dif>
    <ref tip="vid" cel="trib.0o">tribo</ref>
    <trd lng="nl">horde</trd>
  </snc>
  <snc>
    <dif>
      Nomada banda&ccirc;o:
      <ekz>
        la Eternulo sendis <tld/>ojn da &Hcirc;aldeoj sur Judujon
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>II. Re&gcirc;oj 24:2</lok>)</fnt>.
      </ekz>
    </dif>
    <trd lng="nl">woeste <ind>bende</ind></trd>
  </snc>
  <snc>
    <dif>
      Senorda, sendisciplina kaj malbonfara bando.
    </dif>
    <trd lng="nl">bende</trd>
  </snc>
  <trd lng="de">Horde</trd>
  <trd lng="fr">horde</trd>
  <trd lng="ru">&c_o;&c_r;&c_d;&c_a;</trd>
</drv>
</art>
