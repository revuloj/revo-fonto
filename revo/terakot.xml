<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>terakot</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="terakot.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="vid" cel="cerami.0o">Ceramika</ref> substanco bakita el
      ferenhava, brikkolora argilo:
      <ekz>
        sian fabrikon en Rusio planas konstrui <ctl>Atlas</ctl>, la pola
        gvidanto de gluoj por glazuro kaj <tld/>o<fnt><aut>Stanis&lstroke;aw
        &Sacute;migielski</aut>: <vrk>Laboro tro multekosta</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 8a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>terracotta</trd>,
    <trd>terre cuite<klr> (mat&eacute;riau)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="terakot.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      El <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>en <ind>terre cuite</ind></trd>
  </trdgrp>
</drv>

<drv mrk="terakot.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Objekto el <tld/>o:
      <ekz>
        la purpuraj <tld/>a&jcirc;oj de Yixing, precipe &gcirc;ia tekru&ccirc;o, estas stile
        modestaj kaj elegantaj<fnt><bib>EncCxi</bib> <lok>&ccirc;ap. &leftquot;Ceramiko&rightquot;,
        p. 210a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>terre cuite<klr> (objet)</klr></trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>