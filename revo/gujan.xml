<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gujan</rad>/o
</kap>

<drv mrk="gujan.0o">
  <kap>
    <tld lit="G"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="gujan.0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref>
      en la nordo de <ref tip="malprt"
      cel="amerik.Sud0o">Sudameriko</ref>.
      ISO-3166 kodo: GY.
      &Gcirc;i limas kun
      <ref tip="vid" cel="venezuel.0o">Venezuelo</ref>,
      <ref tip="vid" cel="brazil.0o">Brazilo</ref>,
      <ref tip="vid" cel="surinam.0o">Surinamo</ref> kaj 
      <ref tip="vid" cel="atlant.0o">Atlantiko</ref>.
    </dif>
    <ref tip="sin" cel="gvajan.0o">Gvajano</ref>
    <trd lng="bg">&c_G;&c_v;&c_i;&c_a;&c_n;&c_a;</trd>
    <trd lng="de">Guyana</trd>
    <trdgrp lng="pl">
      <trd>Gujana</trd>,
      <trd>Kooperacyjna Republika <ind>Gujany</ind></trd>
    </trdgrp>
    <trd lng="pt">Guiana</trd>
  </snc>
</drv>

<drv mrk="gujan.franca0o">
  <kap>
    Franca <tld lit="G"/>o
    <fnt>
      <bib>PIV2</bib>
    </fnt>
  </kap>
  <snc mrk="gujan.franca0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="malprt" cel="franc.0a">Franca</ref> dependa teritorio
      en la nordo de <ref tip="malprt"
      cel="amerik.Sud0o">Sudameriko</ref>
      kun &ccirc;efurbo Kajeno.
      ISO-3166 kodo: GF.
      &Gcirc;i limas kun
      <ref tip="vid" cel="surinam.0o">Surinamo</ref>,
      <ref tip="vid" cel="brazil.0o">Brazilo</ref> kaj
      <ref tip="vid" cel="atlant.0o">Atlantiko</ref>.
    </dif>
    <ref tip="sin" cel="gvajan.franca0o">Franca Gviano</ref>
  <trd lng="bg">&c_F;&c_r;&c_je;&c_n;&c_s;&c_k;&c_a;
  &c_G;&c_v;&c_i;&c_a;&c_n;&c_a;</trd>
  <trd lng="de">Franz&ouml;sisch-Guayana</trd>
  <trd lng="fr">R&eacute;gion <ind>Guyane</ind></trd>
  <trd lng="pl">Gujana Francuska</trd>
  <trd lng="pt">Guiana Francesa</trd>
  </snc>
  <rim>"Monato" donas anka&ubreve; nomon Franca Gviano.
  <aut>SC</aut></rim>
</drv>

</art>

<!--
$Log$
-->
</vortaro>