<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: abel1.xml,v 1.34 2023/10/27 12:51:04 revo Exp $">
<kap>
  <rad>Abel</rad>/o
</kap>
<drv mrk="abel1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc mrk="abel1.0o.matematikisto">
    <dif>
      Norveglingve: <trd lng="no">Niels <ind>Abel</ind></trd>,
      (1802&#x202F;-&#x202F;1829); norvega <ref tip="lst" lst="voko:historiaj_personoj" cel="matema.0isto">matematikisto</ref>:
      <ekz>
        la <tld/>-Premio (a&ubreve; <tld/>a Premio) estas jara matematika premio
        <klr>[&#x2026;]</klr>, &ccirc;i tiu premio celas kovri la mankon de Nobel-premio por
        matematiko, kvankam oni konsideris, ke la Medalo <nom>Fields</nom> estis jam ekvivalento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=163839">Abel-Premio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5C3C;&#x5C14;&#x65AF;&#x963F;&#x8D1D;&#x5C14; <pr>n&#xED;&#x11B;rs&#x12B;&#x101;b&#xE8;i&#x11B;r</pr></trd>,
    <trd>&#x5C3C;&#x723E;&#x65AF;&#x963F;&#x8C9D;&#x723E; <pr>n&#xED;&#x11B;rs&#x12B;&#x101;b&#xE8;i&#x11B;r</pr></trd>
  </trdgrp>
</snc>
  <rim>
    <ref tip="hom" cel="abel.0o">Abelo</ref>
    estas anka&ubreve; insekto.
  </rim>
  <trd lng="be">&c_A;&c_b;&c_e;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_A;&c_b;&c_je;&c_l;</trd>
  <trd lng="br">Abel</trd>
  <trd lng="ca">Abel</trd>  
  <trd lng="da">Abel</trd>
  <trd lng="de">Abel</trd>
  <trd lng="en">Abel</trd>
  <trd lng="es">Abel</trd>
  <trd lng="fa">&a_n;&f_y;&a_l;&a_s; &a_h;&a_n;&a_r;&f_y;&f_k; <ind>&a_A_madda;&a_b;&a_l;</ind></trd>
  <trd lng="fr">Abel</trd>
  <trd lng="hu">Abel</trd>
  <trd lng="it">Abel</trd>
  <trd lng="nl">Niels <ind>Abel</ind></trd>
  <trd lng="pl">Abel</trd>
  <trd lng="pt">Abel</trd>
  <trd lng="ru">&c_A;&c_b;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="abel1.a0a">
  <kap><tld lit="a"/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="abel1.a0a.grupo">
    <fnt><bib>MatTerm</bib><lok>p. 14</lok></fnt>
    <uzo tip="klr">(p.p. grupo)</uzo>
    <ref tip="dif" cel="komut.0eca.grupoido">komuteca</ref>
    <ref tip="vid" cel="grup.0o.MAT">grupo</ref>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_e;&c_l;&c_je;&c_v;&c_a; <klr tip="amb">(&c_g;&c_r;&c_u;&c_p;&c_a;)</klr></trd>,
      <trd>&c_k;&c_a;&c_m;&c_u;&c_t;&c_a;&c_t;&c_y;&c_w;&c_n;&c_a;&c_ja; <klr tip="amb">(&c_g;&c_r;&c_u;&c_p;&c_a;)</klr></trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd><klr tip="ind">(group) </klr>abelian</trd>,
      <trd><klr tip="ind">(group) </klr>komutativel</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd><klr tip="ind">(grup) </klr>abeli&agrave;</trd>,
      <trd><klr tip="ind">(grup) </klr>commutatiu</trd>
    </trdgrp>
    <trd lng="da">abelsk</trd>
    <trdgrp lng="de">
      <trd>Abelsch<klr tip="ind"> (-e Gruppe)</klr></trd>,
      <trd>kommutativ<klr tip="ind"> (-e Gruppe)</klr></trd>
    </trdgrp> 
    <trdgrp lng="en">
      <trd>Abelian<klr tip="ind"> (group)</klr></trd>,
      <trd>commutative<klr tip="ind"> (group)</klr></trd>
    </trdgrp> 
    <trdgrp lng="fa">
      <trd>&a_A_madda;&a_b;&a_l;&f_y;</trd>,
      <trd>&a_t;&a_ayn;&a_w;&f_y;&a_D;&zwnj;&f_p;&a_dh;&f_y;&a_r;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd><klr tip="ind">(groupe) </klr>ab&eacute;lien</trd>,
      <trd><klr tip="ind">(groupe) </klr>commutatif</trd>
    </trdgrp> 
    <trdgrp lng="hu">
      <trd>kommutat&iacute;v</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd><klr>(gruppo) </klr>abeliano</trd>,
      <trd><klr>(gruppo) </klr>commutativo</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>Abels<klr tip="ind"> (groep)</klr></trd>,
      <trd>commutatief<klr tip="ind"> (groep)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd><klr tip="amb">(grupa) </klr>abelowa</trd>,
      <trd><klr tip="amb">(grupa) </klr>przemienna</trd>
    </trdgrp> 
    <trd lng="pt">comutativo</trd>
    <trdgrp lng="ru">
      <trd>&c_a;&c_b;&c_je;&c_l;&c_je;&c_v;&c_a;<klr tip="amb"> (&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;)</klr></trd>,
      <trd>&c_k;&c_o;&c_m;&c_m;&c_u;&c_t;&c_a;&c_t;&c_i;&c_v;&c_n;&c_a;&c_ja;<klr tip="amb"> (&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;)</klr></trd>
    </trdgrp> 
    <trd lng="sk">&aacute;belovsk&aacute;</trd>
  </snc>
  <snc mrk="abel1.a0a.diversaj">
    <dif>
      Iel rilata al <tld/>o:
      <ekz>
        <ind><tld lit="a"/>a kriterio</ind> <klr>(pri konver&gcirc;o de
        iuj serioj)</klr>.
        <trdgrp lng="be">
          <trd>&c_k;&c_r;&c_y;&c_t;&c_e;&c_r; &c_A;&c_b;&c_e;&c_l;&c_ja;</trd>,
          <trd>&c_p;&c_r;&c_y;&c_k;&c_m;&c_je;&c_t;&c_a; &c_A;&c_b;&c_e;&c_l;&c_ja;</trd>
        </trdgrp>
        <trdgrp lng="br">
          <trd>dezverk Abel</trd>,
          <trd>kriter Abel</trd>
        </trdgrp>
        <trd lng="de">Abelsches <ind>Kriterium</ind></trd>
        <trd lng="en">Abel['s] <ind>test</ind></trd>
        <trd lng="fa">&a_m;&a_ayn;&f_y;&a_A;&a_r; &a_A_madda;&a_b;&a_l;&f_y;</trd>        
        <trd lng="fr">crit&egrave;re d'Abel</trd>
        <trd lng="pl">kryterium Abela</trd>
        <trdgrp lng="ru">
          <trd>&c_k;&c_r;&c_i;&c_t;&c_je;&c_r;&c_i;&c_j; 
          &c_A;&c_b;&c_je;&c_l;&c_ja;</trd>,
          <trd>&c_p;&c_r;&c_i;&c_z;&c_n;&c_a;&c_k; 
          &c_A;&c_b;&c_je;&c_l;&c_ja;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_A;&c_b;&c_e;&c_l;&c_ja;</trd>,
      <trd>&c_a;&c_b;&c_e;&c_l;&c_je;&c_w;</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>Abel</trd>,
      <trd>abelian</trd>
    </trdgrp>
    <trd lng="ca">abeli&agrave;</trd>
    <trdgrp lng="de">
      <trd>von <ind>Abel</ind></trd>,
      <trd>Abelsch</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Abel['s]</trd>,
      <trd>Abelian</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>de Abel</trd>,
      <trd>abeliano</trd>
    </trdgrp>
    <trd lng="fa">&a_A_madda;&a_b;&a_l;&f_y;</trd>
    <trdgrp lng="fr">
      <trd>d'<ind>Abel</ind></trd>,
      <trd>ab&eacute;lien</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>Abel-f&eacute;le</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>di Abel</trd>,
      <trd>abeliano</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>Abels</trd>,
      <trd>van <ind>Abel</ind></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Abela</trd>,
      <trd>abelowy</trd>
    </trdgrp>
    <trd lng="pt">abeliano</trd>
    <trdgrp lng="ru">
      <trd>&c_A;&c_b;&c_je;&c_l;&c_ja;</trd>,
      <trd>&c_a;&c_b;&c_je;&c_l;&c_je;&c_v;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>Abel&uring;v</trd>,
    <trd>abelovsk&yacute;</trd>
  </trdgrp>
  <trd lng="sk">&Aacute;belov</trd>
</drv>
</art>
