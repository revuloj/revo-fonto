<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Abel</rad>/o
</kap>
<drv mrk="abel1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Norveglingve: Niels <trd lng="no">Abel</trd>, 1802-1829.
      Norvega matematikisto.
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_b;&c_e;&c_l;&c_mol;</trd>
  <trd lng="br">Abel</trd>
  <trd lng="de">Abel</trd>
  <trd lng="en">Abel</trd>
  <trd lng="es">Abel</trd>
  <trd lng="fr">Abel</trd>
  <trd lng="hu">Abel</trd>
  <trd lng="pl">Abel</trd>
  <trd lng="ru">&c_A;&c_b;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="abel1.a0a">
  <kap><tld lit="a"/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="abel1.a0a.grupo">
    <fnt><bib>MatTerm</bib><lok>p. 14</lok></fnt>
    <dif>
      <klr>(p.p. <ref cel="grup.0o.MAT">grupo</ref>)</klr>
    </dif>
    <ref tip="dif" cel="komut.0eca.grupoido">komuteca.</ref>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_e;&c_l;&c_je;&c_v;&c_a; <klr tip="amb">(&c_g;&c_r;&c_u;&c_p;&c_a;)</klr></trd>,
      <trd>&c_k;&c_a;&c_m;&c_u;&c_t;&c_a;&c_t;&c_y;&c_w;&c_n;&c_a;&c_ja; <klr tip="amb">(&c_g;&c_r;&c_u;&c_p;&c_a;)</klr></trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd><klr tip="ind">(group) </klr>abelian</trd>,
      <trd><klr tip="ind">(group) </klr>komutativel</trd></trdgrp>
    <trdgrp lng="de">
      <trd>Abelsch<klr tip="ind"> (-e Gruppe)</klr></trd>,
      <trd>kommutativ<klr tip="ind"> (-e Gruppe)</klr></trd></trdgrp> 
    <trdgrp lng="en">
      <trd>Abelian<klr tip="ind"> (group)</klr></trd>,
      <trd>commutative<klr tip="ind"> (group)</klr></trd></trdgrp> 
    <trdgrp lng="fr">
      <trd><klr tip="ind">(groupe) </klr>ab&eacute;lien</trd>,
      <trd><klr tip="ind">(groupe) </klr>commutatif</trd></trdgrp> 
    <trdgrp lng="hu">
      <trd>kommutat&iacute;v</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd><klr tip="amb">(grupa) </klr>abelowa</trd>,
      <trd><klr tip="amb">(grupa) </klr>przemienna</trd></trdgrp> 
    <trdgrp lng="ru">
      <trd>&c_a;&c_b;&c_je;&c_l;&c_je;&c_v;&c_a;
      <klr tip="amb"> (&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;)
      </klr></trd>,
      <trd>&c_k;&c_o;&c_m;&c_m;&c_u;&c_t;&c_a;&c_t;&c_i;&c_v;&c_n;&c_a;&c_ja;
      <klr tip="amb"> (&c_g;&c_r;&c_u;&c_p;&c_p;&c_a;)
      </klr></trd></trdgrp> 
    <trd lng="sk">&aacute;belovsk&aacute;</trd>
  </snc>
  <snc mrk="abel1.a0a.diversaj">
    <dif>
      Iel rilata al <tld/>o:
      <ekz>
        <ind><tld lit="a"/>a kriterio</ind> <klr>(pri konver&gcirc;o de
        iuj serioj)</klr>.
        <trdgrp lng="be">
          <trd>&c_k;&c_r;&c_y;&c_t;&c_e;&c_r; &c_A;&c_b;&c_e;&c_l;&c_ja;</trd>,
          <trd>&c_p;&c_r;&c_y;&c_k;&c_m;&c_je;&c_t;&c_a; &c_A;&c_b;&c_e;&c_l;&c_ja;</trd>
        </trdgrp>
        <trdgrp lng="br">
          <trd>dezverk Abel</trd>,
          <trd>kriter Abel</trd>
        </trdgrp>
        <trd lng="de">Abelsches <ind>Kriterium</ind></trd>
        <trd lng="en">Abel['s] <ind>test</ind></trd>
        <trd lng="fr">crit&egrave;re d'Abel</trd>
        <trd lng="pl">kryterium Abela</trd>
        <trdgrp lng="ru">
          <trd>&c_k;&c_r;&c_i;&c_t;&c_je;&c_r;&c_i;&c_j; 
          &c_A;&c_b;&c_je;&c_l;&c_ja;</trd>,
          <trd>&c_p;&c_r;&c_i;&c_z;&c_n;&c_a;&c_k; 
          &c_A;&c_b;&c_je;&c_l;&c_ja;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_A;&c_b;&c_e;&c_l;&c_ja;</trd>,
      <trd>&c_a;&c_b;&c_e;&c_l;&c_je;&c_w;</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>Abel</trd>,
      <trd>abelian</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>von <ind>Abel</ind></trd>,
      <trd>Abelsch</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Abel['s]</trd>,
      <trd>Abelian</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>de Abel</trd>,
      <trd>abeliano</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d'<ind>Abel</ind></trd>,
      <trd>ab&eacute;lien</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>Abel-f&eacute;le</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Abela</trd>,
      <trd>abelowy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_A;&c_b;&c_je;&c_l;&c_ja;</trd>,
      <trd>&c_a;&c_b;&c_je;&c_l;&c_je;&c_v;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
