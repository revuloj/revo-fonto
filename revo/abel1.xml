<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Abel</rad>/o
</kap>
<drv mrk="abel1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Norveglingve: Niels <trd lng="no">Abel</trd>, 1802-1829.
      Norvega matematikisto.
    </dif>
  </snc>
  <trd lng="de">Abel</trd>
  <trd lng="en">Abel</trd>
  <trd lng="es">Abel</trd>
  <trd lng="fr">Abel</trd>
  <trd lng="ru">Абель</trd>
</drv>

<drv mrk="abel1.a0a">
  <kap><tld lit="a"/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="abel1.a0a.grupo">
    <fnt><bib>MatTerm</bib><lok>p. 14</lok></fnt>
    <dif>
      <klr>(p.p. <ref cel="grup.0o.MAT">grupo</ref>)</klr>
    </dif>
    <ref tip="dif" cel="komut.0eca.magmo">komuteca.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>von <ind>Abel</ind></trd>,
    <trd>Abelsch</trd></trdgrp> 
  <trdgrp lng="en">
    <trd>Abel['s]</trd>,
    <trd>Abelian</trd></trdgrp> 
  <trdgrp lng="es">
    <trd>de Abel</trd>,
    <trd>abeliano</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>d'<ind>Abel</ind></trd>,
    <trd>abélien</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>Абеля</trd>,
    <trd>абелев</trd></trdgrp> 
</drv>
</art>

