<art mrk="$Id: igvano.xml,v 1.19 2023/11/19 10:22:02 revo Exp $">
<kap>
  <rad>igvanodont</rad>/o
</kap>
<drv mrk="igvano.0o">
  <kap><tld/>o</kap>
  <snc mrk="igvano.0o.ZOO">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Grandega fosilia <ref tip="lst" lst="voko:reptilioj" cel="reptil.0o">reptilio</ref>,
      vivinta en la kretaceo:
      <ekz>
        tre popularaj &ccirc;e la publiko estas la tiranosa&ubreve;ro, originalo de <tld/>o, kaj la
        talismanulo de la muzeo, triceratopo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=205463">Muzeo Senckenberg</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        oni tradicie kalkulas inter la hipsilofodontedoj preska&ubreve; &ccirc;iujn dupiedajn
        ornitiskiojn krom la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=198406">Hipsilofodontedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/5/5a/Iguanodon_bernissartensis_from_above.JPG/640px-Iguanodon_bernissartensis_from_above.JPG" prm="PD">skeleto de <tld/>o (Iguanodon bernissartensis)
        <fnt>
          <aut>LadyofHats fotis en la Nacia Muzeo de Naturhistorio, Parizo</aut>
          <lok><url ref="&WCW;/File:Iguanodon_bernissartensis_from_above.JPG">&WCW;/File:Iguanodon_bernissartensis_from_above.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_ib;&c_g;&c_u;&c_a;&c_n;&c_a;&c_d;&c_o;&c_n;&c_t;</trd>
  <trd lng="de">Iguanodon</trd>
  <trd lng="es">iguanodonte</trd>
  <trd lng="fr">iguanodon</trd>
  <trd lng="hu">Iguanodon</trd>
  <trdgrp lng="ja">
    <trd>&#x30A4;&#x30B0;&#x30A2;&#x30CE;&#x30C9;&#x30F3;</trd>,
    <trd>&#x79BD;&#x7ADC;  <pr>&#x304D;&#x3093;&#x308A;&#x3085;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">iguanodon</trd>
  <trd lng="pl">iguanodont</trd>
  <trd lng="pt">iguanodonte</trd>
  <trd lng="ru">&c_i;&c_g;&c_u;&c_a;&c_n;&c_o;&c_d;&c_o;&c_n;&c_t;</trd>
  <trd lng="uk">&#x456;&#x491;&#x443;&#x430;&#x43D;&#x43E;&#x434;&#x43E;&#x43D;&#x442;</trd>
  <trd lng="zh">&#x79BD;&#x9F8D; <pr>q&#xED;nl&#xF3;ng</pr></trd>
</drv>
