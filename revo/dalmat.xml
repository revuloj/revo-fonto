<kap>
  <rad>dalmatik</rad>/o
</kap>
<drv mrk="dalmat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Diakona <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="vest.0o.TEKS">vesto</ref> por diservo:
      <ekz>
        la statuo de la Nigra Madono vestita per la karakteriza <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=494341">Sankta Hejmo de Loreto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="surpli.0o.TEKS">surpliso</ref>,
      <ref cel="sutan.0o.TEKS">sutano</ref>
    </refgrp>     
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="ca">dalm&agrave;tica</trd>
  <trd lng="cs">dalmatika</trd>
  <trd lng="de">Dalmatika</trd>
  <trd lng="en">dalmatic</trd>
  <trd lng="es">dalm&aacute;tica</trd>
  <trd lng="fr">dalmatique<klr tip="ind"> (v&ecirc;tement
  liturgique)</klr></trd>
  <trd lng="he">&#x5D2;&#x5DC;&#x5D9;&#x5DE;&#x5D4;</trd>
  <trd lng="hu">dalmatika</trd>
  <trd lng="it">dalmatica<klr> (sost.)</klr></trd>
  <trd lng="ja">&#x30C0;&#x30EB;&#x30DE;&#x30C1;&#x30AB;</trd>
  <trd lng="pl">dalmatynka</trd>
  <trd lng="pt">dalm&aacute;tica</trd>
  <trd lng="ru">&c_d;&c_a;&c_l;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="uk">&#x434;&#x430;&#x43B;&#x43C;&#x430;&#x442;&#x438;&#x43A;</trd>
