<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: frekve2.xml,v 1.24 2024/09/26 06:38:46 revo Exp $">
<kap>
  <rad>frekvent</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="frekve2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="frekve2.0i.KOMUNE">
    <dif>
      <ke><ref tip="dif" cel="vizit.0adi">Vizitadi</ref></ke>:
      <ekz>
        &leftquot;mi <tld/>adis la lernejon dum 30 jaroj&rightquot;, aldonas la eksinstruisto kun fiero
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007091.php">Du patroj interparolas</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 26</lok>
          </fnt>;
      </ekz>
      <ekz>
        eniras kaj eliras sen&ccirc;ese gelernantoj kaj a&gcirc;uloj, kiuj <tld/>as la bibliotekon
        <fnt><aut>Bardhyl Selimi</aut>:
          <vrk><url ref="&Monato;/2001/007205.html">Voja&gcirc;impresoj, somero 2000</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/01, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i anka&ubreve; <tld/>as kurson por terkulturistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2003/008043.html">Seksa sklaveco: &ccirc;u defio al la 21a
          jarcento?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        teatrojn, kiujn la muzikisto ofte kaj &gcirc;oje <tld/>is
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008353.html">Plensukcesa oma&gcirc;o al Jacques
          Brel</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        marpla&gcirc;oj de Albanio, &gcirc;is nun malpli <tld/>ataj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/007925.html">Voja&gcirc;impresoj, somero 2002: Kosovo
          anta&ubreve;eniras</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        Mikela <klr>[&#8230;]</klr> intense <tld/>is la socian vivon
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2006/008905.php">Negro i&gcirc;is nigrulo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_v;&c_je;&c_d;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_ch;&c_a;&c_s;&c_t;&c_a;, &c_r;&c_e;&c_g;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>doch&aacute;zet</trd>,
    <trd>nav&scaron;t&ecaron;vovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>oft <ind>besuchen</ind></trd>,
    <trd>verkehren</trd>,
    <trd>Umgang haben</trd>,
    <trd>frequentieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>frequent</trd>,
    <trd>habitually visit</trd>
  </trdgrp>
  <trd lng="es">frecuentar</trd>
  <trdgrp lng="fr">
    <trd>fr&eacute;quenter</trd>,
    <trd>courir<klr tip="ind"> (fr&eacute;quenter)</klr></trd>,
    <trd>peupler<klr tip="ind"> (fr&eacute;quenter)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12424;&#x304F;&#x884C;&#x304F;  <pr>&#12424;&#x304F;&#12356;&#x304F;</pr></trd>,
    <trd>&#x901A;&#12358;  <pr>&#x304B;&#12424;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">(geregeld) <ind>bezoeken</ind></trd>
  <trd lng="pt">freq&uuml;entar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_g;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_o; &c_p;&c_o;&c_s;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_je;&c_d;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>doch&aacute;dza&tcaron;</trd>,
    <trd>nav&scaron;tevova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_v;&c_ib;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&#x43E;&c_d;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="frekve2.0ulo">
  <kap><tld/>ulo, <var><kap><tld/>anto</kap></var></kap>
  <snc mrk="frekve2.0ulo.KOMUNE">
    <dif>
      Ofta, <ke>regula <ref tip="sin" cel="vizit.0anto">vizitanto</ref></ke>:
      <ekz>
        ne atentante rigardojn de <tld/>uloj de la ejo,
        <nom>Tavi</nom> kura&gcirc;e okupi&gcirc;is pri la man&gcirc;a&jcirc;o
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/mondo.html">
          La mondo brilanta</url></vrk>,
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>anto de artaj foiroj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2015/011419.php">10 x 15</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_w;&c_s;&c_jo;&c_d;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_l;&c_y; &c_n;&c_a;&c_v;&c_je;&c_d;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">frekventant</trd>
  <trd lng="de">Besucher</trd>
  <trd lng="fr">habitu&eacute;<klr> (subst.)</klr></trd>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_s;&c_je;&c_g;&c_d;&c_t;&c_a;&c_j;</trd>
  <trd lng="sk">frekventant</trd>
</drv>
</art>
