<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>frekvent</rad>/i
</kap>
<drv mrk="frekve2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ofte venadi, vizitadi, &ccirc;eestadi; kutime interrilati kun:
      <ekz><tld/>i personon, lokon, kurson, muzeon, teatron;</ekz>
      <ekz>tre <tld/>ita promenejo;</ekz>
      <ekz><tld/>i eminentulojn.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>oft besuchen</trd>,
     <trd>verkehren</trd>,
     <trd>Umgang haben</trd>,
     <trd>frequentieren</trd></trdgrp>
  <trd lng="es">frecuentar</trd>
  <trdgrp lng="fr">
    <trd>fr&eacute;quenter</trd>,
    <trd>courir<klr tip="ind"> (fr&eacute;quenter)</klr></trd>,
    <trd>peupler<klr tip="ind"> (fr&eacute;quenter)</klr></trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_g;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_o; &c_p;&c_o;&c_s;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_je;&c_d;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
