<art mrk="$Id: bolivi.xml,v 1.22 2023/08/31 08:48:52 revo Exp $">

  <kap><ofc>8</ofc>
     <rad>Bolivi</rad>/o <fnt><bib>PIV1</bib></fnt>
  </kap>

<drv mrk="bolivi.B0o">
  <kap><ofc>8</ofc><tld/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">BO</mlg>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="malprt" cel="amerik.Sud0o">Sudamerika</ref>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&scirc;tato</ref>,
      limanta kun 
      <ref tip="vid" cel="brazil.B0o">Brazilo</ref>,
      <ref tip="vid" cel="paragvaj.P0o">Paragvajo</ref>, 
      <ref tip="vid" cel="argentin.A0o">Argentino</ref>, 
      <ref tip="vid" cel="cxili.CX0o">&Ccirc;ilio</ref> kaj
      <ref tip="vid" cel="peru.P0o">Peruo</ref>:
      <ekz>
        kvankam mi naski&gcirc;is en <tld/>o, do, la&ubreve; sud-amerika le&gcirc;o, 
        mi estas <tld lit="b"/>ano, miaj gepatroj estis skotoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2009/009797.php">Pli 
          pri la mistera insulo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Venezuelo, <tld/>o kaj Ekvadoro la kontraktoj estas nuntempe retraktataj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010686.php">Mizeriga petrolo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_l;&c_ib;&c_v;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_B;&c_o;&c_l;&c_i;&c_v;&c_i;&c_ja;</trd>
  <trd lng="cs">Bolivie</trd>
  <trd lng="de">Bolivien</trd>
  <trd lng="en">Bolivia</trd>
  <trd lng="fr">Bolivie</trd>
  <trd lng="he">&#x5D1;&#x5D5;&#x5DC;&#x5D9;&#x5D1;&#x5D9;&#x5D4;</trd>
  <trd lng="hu">Bol&iacute;via</trd>
  <trd lng="id">Bolivia</trd>
  <trd lng="ja">&#x30DC;&#x30EA;&#x30D3;&#x30A2;</trd>
  <trd lng="nl">Bolivi&euml;</trd>
  <trd lng="no">Bolivia</trd>
  <trd lng="pl">Boliwia</trd>
  <trd lng="pt">Bol&iacute;via</trd>
  <trd lng="sk">Bol&iacute;via</trd>
  <trd lng="uk">&#x411;&#x43E;&#x43B;&#x456;&#x432;&#x456;&#x44F;</trd>
</drv>
