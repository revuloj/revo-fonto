<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kvart</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kvart1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kvart1.0o.MEZ">
    <dif>
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:longoj">Mezurunuo</ref>
      de flua&jcirc;oj,
      egala proksimume al 0,3 litro,
      kvarono de <ref tip="malprt" 
      cel="stof.0o.MEZ">stofo</ref>:
      <ekz>
        en la brandon li miksis du <tld/>ojn da pulvo, unu 
        <tld/>on da sulfurpulvoro kaj saman kvanton da salo 
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="hom">
    <ref cel="kvart.0o.intervalo">kvarto</ref>
  </refgrp>
  <trd lng="fr">quart<klr> (de liquide)</klr></trd>
  <trd lng="sk">kvarta</trd>
  <trdgrp lng="cs">
    <trd>kvart</trd>,
    <trd>věrtel <klr>(dutá míra)</klr></trd>
  </trdgrp>
</drv>
</art>

