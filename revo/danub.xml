<art mrk="$Id: danub.xml,v 1.23 2024/09/24 06:37:30 revo Exp $">
<kap><ofc>10</ofc>
  <rad>Danub</rad>/o
</kap>
<drv mrk="danub.D0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" cel="river.0o" lst="voko:riveroj">rivero</ref>, fluanta el
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>, tra
      A&ubreve;strujo, Hungarujo kaj Rumanujo en la Nigran Maron:
      <ekz>
        la fama valso <ctl>&Ccirc;e la bela blua
        <tld/>o</ctl><fnt><aut>Evgeni
        Georgiev</aut>: <vrk>Jubileo de valsre&gcirc;o</vrk>,
        <bib>Monato</bib><lok>jaro 1999a, numero 9a, p. 22a</lok></fnt>;
      </ekz>
      <ekz>
        nova ponto sur la rivero <tld/>o estos konstruita inter Bulgario
        kaj Rumanio<fnt><aut>Evgeni Georgiev</aut>: <vrk>Nova ponto sur
        Danubo</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 1a, p.
        11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="he">&#x5D3;&#x5E0;&#x5D5;&#x5D1;&#x5D4;</trd>
  <trd lng="ja">&#x30C9;&#x30CA;&#x30A6;&#x5DDD;  <pr>&#x30C9;&#x30CA;&#x30A6;&#x304B;&#x308F;</pr></trd>
  <trd lng="lat">Danuvius</trd>
  <trd lng="be">&c_D;&c_u;&c_n;&c_a;&c_j;</trd>
  <trd lng="bg">&c_D;&c_u;&c_n;&c_a;&c_v;</trd>
  <trd lng="ca">Danubi</trd>
  <trd lng="cs">Dunaj</trd>
  <trd lng="de">Donau</trd>
  <trd lng="en">Danube River</trd>
  <trd lng="es">Danubio</trd>
  <trd lng="fr">Danube</trd>
  <trd lng="hu">Duna</trd>
  <trd lng="nl">Donau</trd>
  <trd lng="pl">Dunaj</trd>
  <trd lng="pt">Dan&uacute;bio</trd>
  <trd lng="ru">&c_D;&c_u;&c_n;&c_a;&c_j;</trd>
  <trd lng="sk">Dunaj</trd>
  <trd lng="uk">&#x414;&#x443;&#x43D;&#x430;&#x439;</trd>
  <trd lng="zh">&#x591A;&#x7459;&#x6CB3; <pr>du&#x14D;n&#x1CE;oh&#xE9;</pr></trd>
</drv>
</art>
