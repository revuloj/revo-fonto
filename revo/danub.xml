<art mrk="$Id$">
<kap>
<rad>Danub</rad>/o
</kap>
<drv mrk="danub.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="super" cel="river.0o">rivero</ref>, fluanta el
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref>, tra
A&ubreve;strujo, Hungarujo kaj Rumanujo en la Nigran Maron.
    </dif>
  </snc>
  <trd lng="de">Donau</trd>
  <trd lng="nl">Donau</trd>
  <trd lng="lat">Danuvius</trd>
</drv>
</art>

