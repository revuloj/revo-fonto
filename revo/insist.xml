<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>insist</rad>/i
</kap>
<drv mrk="insist.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Persisti &ccirc;e sia opinio a&ubreve; postulo:
      <ekz>
        ne <tld/>u pri via peto, kiun ni ne povas konsenti;
      </ekz>
      <ekz>
        estas necese <tld/>i pri tiu regulo;
      </ekz>
      <ekz>
        oni devas <tld/>i pri la graveco de la unueco en la internacia lingvo;
      </ekz>
      <ekz>
        li <tld/>is &ccirc;e li, ke li prenu
        <fnt><bib>M</bib>, <lok>II. re&gcirc;oj 5:16</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akcent.0i">akcenti</ref>,
      <ref cel="emfaz.0i">emfazi</ref>,
      <ref cel="relief">reliefigi</ref>,
      <ref cel="strek.sub0i">substreki</ref>,
      <ref cel="urgx.0i">ur&gcirc;i</ref>,
      <ref cel="trud.0i">trudi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>beharren</trd>,
    <trd>bestehen <klr tip="ind">(auf etw.)</klr></trd>,
    <trd>insistieren</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_n;&c_a; &c_s;&c_v;&c_o;&c_jo;&c_m;)</klr></trd>
</drv>
<drv mrk="insist.0ado">
  <kap><tld/>(ad)o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        li tedas nin per sia <tld/>ado.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Beharren</trd>,
    <trd>Dr&auml;ngen</trd>,
    <trd>Nachdruck</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_ch;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
<drv mrk="insist.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="I"/>anta:
      <ekz>
        <tld/>a peto;
      </ekz>
      <ekz>
        <tld/>e ripetita reklamo;
      </ekz>
      <ekz>
        sen<tld/>e diri.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">beharrlich</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
</drv>
</art>
