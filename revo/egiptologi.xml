<art mrk="$Id: egiptologi.xml,v 1.11 2022/09/26 14:06:53 revo Exp $">
<kap>
  <rad>egiptologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="egiptologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="egiptologi.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="malprt" cel="histor.0o.SCI">Historia</ref> scienco pri
      la
      antikva egipta kulturo:
      <ekz>
        ne maltrafu la salonon pri <tld/>o
        <fnt>
        <aut>J.-F. Schwaiger</aut>,
        <vrk><url ref="http://colmar1.free.fr/Esperanto/eliroj.html">[Colmar],
        Muzeoj</url></vrk>,
        <lok>1999-12-16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_g;&c_ib;&c_p;&c_t;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_g;&c_i;&c_p;&c_t;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">egiptologia</trd>
  <trd lng="de">&Auml;gyptologie</trd>
  <trd lng="fr">&eacute;gyptologie</trd>
  <trd lng="pl">egiptologia</trd>
  <trd lng="pt">egiptologia</trd>
  <trd lng="ru">&c_je;&c_g;&c_i;&c_p;&c_t;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="uk">&c_jeu;&c_g;&c_i;&#x43F;&c_t;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="egiptologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="egiptologi.0isto.HIS">
    <dif>
      Historiisto studanta la antikvan egiptan kulturon, fakulo pri <ref tip="vid"
      cel="egiptologi.0o.HIS"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_g;&c_ib;&c_p;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_je;&c_g;&c_i;&c_p;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">egipt&ograve;leg</trd>
  <trd lng="de">&Auml;gyptologe</trd>
  <trd lng="fr">&eacute;gyptologue</trd>
  <trd lng="he">&#x5DE;&#x5E6;&#x5E8;&#x5D9;&#x5E1;&#x5D8;&#x5D5;&#x5DC;&#x5D5;&#x5D2;</trd>
  <trd lng="pl">egiptolog</trd>
  <trd lng="pt">egipt&oacute;logo</trd>
  <trd lng="ru">&c_je;&c_g;&c_i;&c_p;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
