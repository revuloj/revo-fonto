<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>8</ofc>
<rad>cirkl</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="cirkl.0o">
<kap><tld/>o</kap>

  <snc mrk="cirkl.0o.linio">
    <uzo tip="fak">MAT</uzo>
    <dif>
      Ebena <ref tip="super" cel="lini.0o.MAT">linio</ref>, konsistanta el
      &ccirc;iuj punktoj, kies distanco al certa punkto <klr>(&gcirc;ia
      centro)</klr> egalas al certa valoro <klr>(&gcirc;ia radio)</klr>.
    </dif>
    <refgrp tip="vid">
      <ref cel="centr.0o">centro</ref>,
      <ref cel="radi.0o">radio</ref>.
    </refgrp>
  </snc>
  
  <snc mrk="cirkl.0o.surfaco">
    <uzo tip="fak">MAT</uzo>
    <dif>
      Ebena <ref tip="super" cel="surfac.0o.MAT">surfaco</ref>, kiun
      limas <tld/>o <sncref ref="cirkl.0o.linio"/>.
    </dif>
  </snc>

<snc mrk="cirkl.0o.KOMUNE">
<dif>
&Ccirc;ia objekto pli-malpli samforma kiel <tld/>o
<sncref ref="cirkl.0o.linio"/> a&ubreve; <sncref ref="cirkl.0o.surfaco"/>:
<ekz>la kanalo k la &scirc;oseo &ccirc;irka&ubreve;is duon<tld/>e la dezerton<fnt>B</fnt>.
</ekz> 
</dif>
</snc>
<refgrp tip="vid">
  <ref cel="rond.0o">rondo</ref>.
</refgrp>
<trd lng="en">circle</trd>
<trd lng="fr">cercle</trd>
<trd lng="nl">cirkel</trd>
<trdgrp lng="ru">
  <trd>&c_o;&c_k;&c_r;&c_u;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
  <trd>&c_k;&c_r;&c_u;&c_g;</trd>
</trdgrp>
</drv>
</art>
