<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kristian.xml,v 1.8 2022/12/15 08:34:09 revo Exp $">
<kap>
  <rad>Kristian</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kristian.0o">
  <kap><tld/>o</kap>
  <snc mrk="kristian.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        la re&gcirc;o <tld/>o la Sepa sidis sur la Dana trono kaj estis tiam
        juna homo
        <fnt><bib>Fab4</bib>, <lok>historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_H;&c_r;&c_y;&c_s;&c_t;&c_y;&c_ja;&c_n;</trd>
  <trd lng="fr">Christian</trd>
  <trd lng="it">Cristiano</trd>
  <trdgrp lng="nl">
    <trd>Kristiaan</trd>,
    <trd>Kris</trd>
  </trdgrp>
  <trd lng="pl">Krystian</trd>
  <trd lng="pt">Cristiano</trd>
  <trd lng="ro">Cristiano</trd>
  <trd lng="uk">&#x41A;&c_r;&c_i;&c_s;&c_t;&c_ib;&c_a;&#x43D;</trd>
</drv>
</art>

