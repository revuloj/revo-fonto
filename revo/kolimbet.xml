<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolimbet.xml,v 1.2 2024/07/31 06:30:07 revo Exp $">
<kap>
  <rad>kolimbet</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="kolimbet.0o">
  <kap><tld/>o</kap>
  <snc mrk="kolimbet.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Akvoskarabo vivanta en mezvarmaj landoj,
      en sensalaj akvejoj, rapide na&gcirc;anta,
      predema (<trd lng="la">Colymbetes</trd>);
      genro el familio disticedoj:
      <ekz>
        <tld lit="K"/>o <klr>...</klr>
        iam flugis sur la Biglon, kiu estis kvardek kvin mejlojn
        for de la plej proksima tero
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_y;&c_v;&c_u;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_b;&c_a;&c_l;&c_a;&c_c;&c_ja;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="fr">colymb&egrave;te</trd>
</drv>
</art>

