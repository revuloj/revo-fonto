<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: karolin.xml,v 1.5 2022/01/26 08:33:35 revo Exp $">
<kap>
  <rad>Karolin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="karolin.norda0o">
  <kap>Norda <tld/>o</kap>
  <snc mrk="karolin.norda0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la orienta parto de 
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        &Ccirc;erokoj <klr>...</klr> tradicie lo&gcirc;is en Tennessee, 
        Norda-<tld/>o kaj Georgio
        <fnt>
          <aut>Myntsy </aut>,
          <vrk>&Ccirc;erokia lingvo</vrk>,
          <lok>en <url ref="http://edukado.net/komunumo/forumo?f=348">
          </url>edukado.net, forumo, 2012-09-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_w;&c_n;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_K;&c_a;&c_r;&c_a;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">Severn&iacute; Karol&iacute;na</trd>
  <trd lng="en">North Carolina</trd>
  <trd lng="fr">Caroline du Nord</trd>
  <trd lng="pl">Karolina P&oacute;&lstroke;nocna</trd>     
  <trd lng="ro">Carolina de Nord </trd>
  <trd lng="sk">Severn&aacute; Karol&iacute;na</trd>
</drv>

<drv mrk="karolin.suda0o">
  <kap>Suda <tld/>o</kap>
  <snc mrk="karolin.suda0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Sub&scirc;tato en la sudorienta parto de 
      <ref tip="malprt" cel="uson.U0o.GEOG">Usono</ref>:
      <ekz>
        en Calvin Lake, Suda <tld/>o, bombokomploto 
        <klr>...</klr> estis malsukcesigita
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url
          ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Januaro_2007.html">
          taglibro</url></vrk>,
          <lok>2007-01-25</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_a;&c_w;&c_d;&c_n;&c_jo;&c_v;&c_a;&c_ja; &c_K;&c_a;&c_r;&c_a;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">Ji&zcaron;n&iacute; Karol&iacute;na</trd>
  <trd lng="en">South Carolina</trd>
  <trd lng="fr">Caroline du Sud</trd>
  <trd lng="pl">Karolina Po&lstroke;udniowa</trd>     
  <trd lng="ro">Carolina de Sud</trd>
  <trd lng="sk">Ju&zcaron;n&aacute; Karol&iacute;na</trd>
</drv>
</art>

