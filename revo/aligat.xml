<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>aligator</rad>/o
</kap>
<drv mrk="aligat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="aligat.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Alligator</trd>).
    </dif>
  </snc>
  <trd lng="de">Alligator</trd>
  <trd lng="en">alligator</trd>
  <trd
  lng="ru">&#1072;&#1083;&#1083;&#1080;&#1075;&#1072;&#1090;&#1086;&#1088;</trd>
</drv>
<drv mrk="aligat.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="krokod.0uloj">krokodiluloj</ref>
      (<trd lng="la">Alligatoridae</trd>), &ccirc;e kiuj la kvara dento estas
      ne videbla &ccirc;e fermita bu&scirc;o.
    </dif>
  </snc>
  <trd lng="de">Alligatoren</trd>
</drv>
</art>
