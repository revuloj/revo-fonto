    <trd>Gerippe</trd>,
    <trd>Ger&uuml;st</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ossements</trd>,
    <trd>squelette</trd>
  </trdgrp>
  <trd lng="it">scheletro</trd>
  <trdgrp lng="ja">
    <trd>&#39592;&#26684;  <pr>&#12371;&#12387;&#12363;&#12367;</pr></trd>,
    <trd>&#39608;&#39592;  <pr>&#12364;&#12356;&#12371;&#12388;</pr></trd>,
    <trd>&#22806;&#39592;&#26684;  <pr>&#12364;&#12356;&#12371;&#12387;&#12363;&#12367;</pr></trd>,
    <trd>&#39592;&#32068;&#12415;  <pr>&#12411;&#12397;&#12368;&#12415;</pr></trd>,
    <trd>&#39592;&#23376;  <pr>&#12373;&#12356;&#12371;&#12429;</pr></trd>,
    <trd>&#27010;&#30053;  <pr>&#12364;&#12356;&#12426;&#12419;&#12367;</pr></trd>,
    <trd>&#12420;&#12379;&#12371;&#12369;&#12383;&#20154;  <pr>&#12420;&#12379;&#12371;&#12369;&#12383;&#12376;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">skelet</trd>
  <trd lng="pl">szkielet</trd>
  <trd lng="sk">kostra</trd>
  <trd lng="sv">skelett</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_je;&c_l;&c_je;&c_t;</trd>,
    <trd>&c_k;&c_ib;&c_s;&c_t;&c_ja;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_k;&c_a;&c_s;</trd>,
    <trd>&c_o;&c_s;&c_t;&c_o;&c_v;</trd>,
    <trd>&c_k;&c_ib;&c_s;&c_t;&c_ja;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#39592;&#39608; <pr>g&#468;h&aacute;i</pr></trd>,
    <trd>&#39607; <pr>k&#363;</pr></trd>,
    <trd>&#39612; <pr>g&eacute;</pr></trd>,
    <trd>&#39592;&#39612;&#31995;&#32113; <pr>g&#468;g&eacute;x&igrave;t&#466;ng</pr></trd>,
    <trd>&#39592;&#39612;&#31995;&#32479; <pr>g&#468;g&eacute;x&igrave;t&#466;ng</pr></trd>,
    <trd>&#39608;&#39592; <pr>h&aacute;ig&#468;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="skelet.0a">
  <kap><tld/>a</kap>
  <snc mrk="skelet.0a.osta">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Rilata al <ref cel="skelet.0o.KOMUNE"><tld/>o<sncref/></ref>:
      <ekz>
        frakturo <klr>[&#8230;]</klr> estas fendi&gcirc;o a&ubreve; rompi&gcirc;o de osto, do de parto el la <tld/>a sistemo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=212812">Frakturo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        striaj muskoloj (<tld/>aj muskoloj) aktivi&gcirc;as vole per la centra nerva sistemo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22095">Muskolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_k;&c_ib;&c_l;&c_je;&c_t;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>Skelett-</trd>,
      <trd>Knochen-</trd>
    </trdgrp>
    <trd lng="pl">szkieletowy</trd>
  </snc>
  <snc mrk="skelet.0a.traba">
    <uzo tip="fak">KON</uzo>
    <dif>
      Rilata al <ref cel="skelet.0o.KON"><tld/>o<sncref/></ref>:
      <ekz>
        saltigilo estas gimnastika kaj refre&scirc;iga aparato konsistanta el unu
        peco de alligita forta &scirc;tofo super &scirc;tala <tld/>a konstruo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=205970">Trampolino</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kradtraba <tld/>a domo kovrita per kano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=374438">Friedrichsruh</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_a;&c_r;&c_k;&c_a;&c_s;&c_n;&c_y;</trd>,
      <trd>&c_sh;&c_k;&c_ib;&c_l;&c_je;&c_t;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="de">Rahmen-</trd>
    <trd lng="pl">szkieletowy</trd>
  </snc>
  <snc mrk="skelet.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Prezentanta nur la &ccirc;efajn liniojn a&ubreve; ideojn, sendetala:
      <ekz>
        &gcirc;i havis nek murojn, nek tegmenton, nur kadron formitan el ok stangoj supre kunligitaj,
        simila al <tld/>a okangula templo
        <fnt>
          <aut>M&aacute;ire NicAoidh</aut>
          <vrk><url ref="&Monato;/2000/006878.php">&Gcirc;ardenumi</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 20</lok>
          </fnt>;
      </ekz>
      <ekz>
        parenteze mia <tld/>a teorio
        <fnt>
          <bib>Monato</bib>
          <aut>Paul GUBBINS</aut>
          <vrk><url ref="&Monato;/2003/008086.html">Taglibro de
          l' (anta&ubreve;)milito ... el la lando de &Scirc;ekspiro (2)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_h;&c_je;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
    <trdgrp lng="ca">
      <trd>succint</trd>,
      <trd>sumari</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>v&aacute;zszer&udblac;</trd>,
      <trd>v&aacute;zlatos</trd>
    </trdgrp>
    <trd lng="it">scarno<klr> (essenziale)</klr></trd>
    <trd lng="pl">szkieletowy</trd>
    <trd lng="ru">&c_s;&c_h;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </snc>
<!-- dubinda
  <snc mrk="skelet.0a.FIG2">
    <dif>
      Malgrasega, senkarna.
    </dif>
    <trd lng="be">&c_h;&c_u;&c_d;&c_a;&c_r;&c_l;&c_ja;&c_v;&c_y;</trd>
    <trd lng="ca">esquel&egrave;tic</trd>
