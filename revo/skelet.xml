<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>skelet</rad>/o
</kap>
<drv mrk="skelet.0o">
  <kap><tld/>o</kap>
  <snc mrk="skelet.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="dif" cel="ost.0aro">Ostaro</ref>.
    </dif>
    <trd lng="en">skeleton</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_je;&c_l;&c_je;&c_t;</trd>,
      <trd>&c_k;&c_o;&c_s;&c_t;&c_ja;&c_k;</trd>,
      <trd>&c_o;&c_s;&c_t;&c_o;&c_v;</trd>
    </trdgrp>
  </snc>
  <snc mrk="skelet.0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Trabaro de konstruo, de &scirc;ipo.
    </dif>
    <refgrp tip="vid">
      <ref cel="armatu.0o">armaturo</ref>,
      <ref cel="cxarpe.0ajxo">&ccirc;arpenta&jcirc;o</ref>,
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_a;&c_r;&c_k;&c_a;&c_s;</trd>,
      <trd>&c_o;&c_s;&c_t;&c_o;&c_v;</trd>
    </trdgrp>
  </snc>
  <snc mrk="skelet.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Skiza plano de verko; precipaj trajtoj de organiza&jcirc;o:
      <ekz>
        la naturo mem de Egipto, postulante grandan, sen&ccirc;esan kaj regulan
        laboron, kreis la <tld/>on de la socia organizo de &ccirc;i tiu
        lando<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="skem.0o">skemo</ref>,
      <ref cel="skiz.0o">skizo</ref>,
      <ref cel="kanvas.0o">kanvaso</ref>,
      <ref cel="sum.re0o">resumo</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_l;&c_a;&c_n;</trd>,
      <trd>&c_s;&c_h;&c_je;&c_m;&c_a;</trd>,
      <trd>&c_o;&c_s;&c_t;&c_o;&c_v;</trd>
    </trdgrp>
  </snc>
  <snc mrk="skelet.0o.FIG2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre malgrasa homo.
    </dif>
    <trd lng="ru">&c_s;&c_k;&c_je;&c_l;&c_je;&c_t;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>Skelett</trd>,
    <trd>Gerippe</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>squelette</trd>
  </trdgrp>
  <trd lng="nl">skelet</trd>
</drv>

<drv mrk="skelet.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Malgrasega, senkarna.
    </dif>
    <trdgrp lng="fr">
      <trd>squelettique</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_l;&c_ja;&c_v;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Prezentanta nur la &ccirc;efajn liniojn a&ubreve; ideojn, sendetala.
    </dif>
    <trd lng="ru">&c_s;&c_h;&c_je;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>
</art>
