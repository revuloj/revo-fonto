<kap>
  <rad>albumin</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="albumi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Blanka kaj viskeca substanco, <ref tip="super" cel="protein.0o">proteinoj</ref>,
      kiuj konsistigas la blankon de ovo kaj plej grandan parton de la animalaj kaj vegeta&jcirc;aj &ccirc;eloj:
      <ekz>
        proteinoj estas gravaj, &ccirc;ar ili konsistigas la korpon: la keratinon de la ha&ubreve;to
        kaj ungoj, la osteinon de la ostoj, la hemoglobinon de la ru&gcirc;aj sango&ccirc;eloj,
        la mioglobinon de la muskoloj, la <tld/>on de la sango, la gelatenecan
        substancon en la &ccirc;eloj, ktp
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/007157.html">Vivo: Riboj</url></vrk>
        </fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_o;&c_k;</trd>
  <trd lng="ca">alb&uacute;mina</trd>
  <trdgrp lng="cs">
    <trd>albumin</trd>,
    <trd>b&iacute;lkovina</trd>
  </trdgrp>
  <trd lng="de">Albumin</trd>
  <trdgrp lng="en">
     <trd>egg white</trd>,
     <trd>albumin</trd>
  </trdgrp>
  <trd lng="es">alb&uacute;mina</trd>
  <trd lng="fr">albumine</trd>
  <trd lng="he">&#x5D0;&#x5DC;&#x5D1;&#x5D5;&#x5DE;&#x5D9;&#x5DF;</trd>
  <trd lng="nl">albumine</trd>
  <trd lng="pt">albumina</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_o;&c_k;</trd>
  <trd lng="sk">bielkovina</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43B;&#x44C;&#x431;&#x443;&#x43C;&#x456;&#x43D;</trd>,
    <trd>&#x431;&#x456;&#x43B;&#x43E;&#x43A;</trd>,
    <trd>&#x431;&#x456;&#x43B;&#x43A;&#x43E;&#x432;&#x438;&#x43D;&#x430;</trd>,
    <trd>&#x431;&#x456;&#x43B;&#x43A;&#x43E;&#x432;&#x430; &#x440;&#x435;&#x447;&#x43E;&#x432;&#x438;&#x43D;&#x430;</trd>
  </trdgrp>
</drv>
