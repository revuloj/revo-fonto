<art mrk="$Id: reni.xml,v 1.19 2024/04/13 20:15:53 revo Exp $">
<kap>
  <rad>reni</rad>/o
</kap>
<drv mrk="reni.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Re</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="75">elemento</ref> kun
      atomnumero 75, atompezo 186,2;
      ar&gcirc;entgriza, mola transir<ref tip="super" cel="metal.0o">metalo</ref>:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> havas la plej altan fandpunkton krom karbono kaj
        volframo, &gcirc;i estas uzata en nefandemaj alojoj kaj kataliziloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24656">Renio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_n;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_r;&c_je;&c_n;&c_i;&c_j;</trd>
  <trd lng="br">reniom</trd>
  <trdgrp lng="cs">
    <trd>Re</trd>,
    <trd>rhenium</trd>
  </trdgrp>
  <trd lng="de">Rhenium</trd>
  <trd lng="en">rhenium</trd>
  <trd lng="es">renio</trd>
  <trd lng="fr">rh&eacute;nium</trd>
  <trd lng="hu">r&eacute;nium</trd>
  <trd lng="id">renium</trd>
  <trd lng="it">renio</trd>
  <trd lng="la">Rhenium</trd>
  <trd lng="nl">rhenium</trd>
  <trd lng="pl">ren</trd>
  <trdgrp lng="pt">
    <trd>r&ecirc;nio (BRA)</trd>,
    <trd>r&eacute;nio (PT)</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>Re</trd>,
    <trd>rh&eacute;nium</trd>
  </trdgrp>
  <trd lng="sv">rhenium</trd>
  <trd lng="uk">&#x440;&#x435;&#x43D;&#x456;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x9338; <pr>l&#xE1;i</pr></trd>,
    <trd>&#x94FC; <pr>l&#xE1;i</pr></trd>
  </trdgrp>
</drv>
</art>
