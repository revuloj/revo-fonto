<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ne&ubreve;rolog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="neuxrolo.0o">
  <kap><tld/>o</kap>
  <snc mrk="neuxrolo.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <ref tip="dif" cel="neuxro.0isto">ne&ubreve;rologiisto</ref>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_w;&c_r;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="de">
    <trd>Neurologe</trd>,
    <trd>Nervenarzt</trd>
  </trdgrp>
  <trd lng="el">&ny;&epsilon;&ypsilon;&rho;&omikron;&lambda;&omikron_ton;&gamma;&omikron;&sigma_fina;</trd>
  <trd lng="en">neurologist</trd>
  <trd lng="es">neur&oacute;logo</trd>
  <trd lng="fr">neurologue</trd>
  <trdgrp lng="hu">
    <trd>ideggy&oacute;gy&aacute;sz</trd>,
    <trd>neurol&oacute;gus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>neuroloog</trd>,
    <trd>zenuwspecialist</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>neurologista</trd>,
    <trd>nevrologista</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_r;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
