<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>potas</rad>/o
</kap>
<drv mrk="potas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      KOH. Senkolora alkala bazo, kalia hidroksido.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pottasche</trd>,
    <trd>Kaliumkarbonat</trd>
  </trdgrp>
  <trd lng="nl">potas</trd>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_a;&c_sh;</trd>
</drv>
</art>
