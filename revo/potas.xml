<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: potas.xml,v 1.19 2019/01/29 20:10:16 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>potas</rad>/o
</kap>
<drv mrk="potas.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">KEM</uzo>
   <snc>
     <dif>
       <frm>K<sub>2</sub>CO<sub>3</sub></frm>, kalia
       karbonato, blanka pulvoro, ekstraktata el
       cindro, iam uzata por sap-, vitrofarado kaj salpetrokuirado:
      <ekz>
        longtempe brun&scirc;tono, pe&ccirc;o kaj kenfulgo, <tld/>o kaj salmiako, <klr>[&#8230;]</klr>
        estis la &ccirc;efaj produktoj de la urbo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=178475"
              >Langewiesen</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="ca">potassi</trd>
  <trdgrp lng="de">
     <trd>Pottasche</trd>,
     <trd>Kaliumkarbonat</trd>
   </trdgrp>
  <trdgrp lng="en">
     <trd>caustic potash</trd>,
     <trd>potassium hydroxide</trd>
   </trdgrp>
  <trd lng="fr">potasse</trd>
  <trdgrp lng="hu">
     <trd>hamuzs&iacute;r</trd>,
     <trd>k&aacute;liumkarbon&aacute;t</trd>
   </trdgrp>
  <trd lng="nl">potas</trd>
  <trdgrp lng="pt">
    <trd>potassa</trd>,
    <trd>carbonato de <ind>pot&aacute;ssio</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_a;&c_sh;</trd>
  <trdgrp lng="sk">
    <trd>pota&scaron;</trd>,
    <trd>salajka</trd>,
    <trd>uhli&ccaron;itan draseln&yacute;</trd>
  </trdgrp>
  <trdgrp lng="sv">
     <trd>pottaska</trd>,
     <trd>kaliumkarbonat</trd>
   </trdgrp>
</drv>

<drv mrk="potas.kauxstika0">
   <kap>ka&ubreve;stika <tld/>o</kap>
   <uzo tip="fak">KEM</uzo>
   <snc>
     <dif>
       <frm>KOH</frm>, senkolora alkala bazo, kalia hidroksido.
     </dif>
   </snc>
   <trd lng="ca">hidr&ograve;xid de <ind>potassi</ind></trd>
   <trdgrp lng="hu">
     <trd>k&aacute;lil&uacute;g</trd>,
     <trd>k&aacute;liumhidroxid</trd>
   </trdgrp>
   <trd lng="fr">potasse caustique</trd>
   <trdgrp lng="nl">
     <trd>kaliumhydroxide</trd>,
     <trd>bijtende <ind>potas</ind></trd>
   </trdgrp>
  <trdgrp lng="pt">
    <trd>hidr&oacute;xido de <ind>pot&aacute;ssio</ind></trd>,
    <trd>potassa c&aacute;ustica</trd>,
    <trd>lix&iacute;via</trd>    
  </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_je;&c_d;&c_k;&c_o;&c_je; &c_k;&c_a;&c_l;&c_i;</trd>,
     <trd>&c_g;&c_i;&c_d;&c_r;&c_o;&c_o;&c_k;&c_i;&c_s;&c_mol; 
&c_k;&c_a;&c_l;&c_i;&c_ja;</trd>
   </trdgrp>
   <trdgrp lng="sv">
     <trd>lut</trd>,
     <trd>kalilut</trd>,
     <trd>kaliumhydroxid</trd>
   </trdgrp>
</drv>
</art>
