<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spik.xml,v 1.24 2024/03/25 07:28:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>spik</rad>/o
</kap>
<drv mrk="spik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref>, kie la floroj
      sidas rekte &ccirc;e la akso:
      <ekz>
        <tld/>o de greno, plantago;
      </ekz>
      <ekz>
        <ind>kunmetita <tld/>o</ind>
        <klr>(&ccirc;e meza akso sidas spiketoj, ekz. &ccirc;e
        tritiko, sekalo, hordeo)</klr>;
        <trdgrp lng="de">
          <trd>zusammengesetzte &Auml;hre</trd>,
          <trd>Schein&auml;hre</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <tld/>o malplena plej alte sin tenas
        <klr>(plej nekapablaj kaj nekompetentaj homoj estas plej fieraj.
        <ref tip="sin" cel="barel.0o">barelo</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_a;&c_s;</trd>
  <trd lng="cs">klas</trd>
  <trd lng="de">&Auml;hre</trd>
  <trdgrp lng="en">
    <trd>ear <klr>(of corn, etc)</klr></trd>,
    <trd>spike <klr>(of corn, etc)</klr></trd>
  </trdgrp>
  <trd lng="fr">&eacute;pi</trd>
  <trdgrp lng="hu">
    <trd>kal&aacute;sz</trd>,
    <trd>f&uuml;z&eacute;r<klr tip="ind"> (vir&aacute;gzat)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7A42;  <pr>&#x307B;</pr></trd>,
    <trd>&#x7A42;&#x72B6;&#x82B1;&#x5E8F;  <pr>&#x3059;&#x3044;&#x3058;&#x3087;&#x3046;&#x304B;&#x3058;&#x3087;</pr></trd>
  </trdgrp>
  <trd lng="nl">aar</trd>
  <trd lng="pl">k&lstroke;os</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_s;</trd>
  <trd lng="sk">klas</trd>
  <trd lng="tr">ba&scedil;ak</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43B;&#x43E;&c_s;&c_s;&#x44F;</trd>
  <trd lng="zh">&#x7A57; <pr>su&#xEC;</pr></trd>
</drv>

<drv mrk="spik.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="rikolt.post0i">postrikolti</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_r;&c_e;&c_sh;&c_t;&c_k;&c_ib; &c_p;&c_a;&c_s;&c_mol;&c_l;&c_ja; &c_zh;&c_n;&c_ib;&c_v;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>pab&#x11B;rkovat</trd>,
    <trd>sb&#xED;rat klasy</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&ouml;ng&eacute;szik</trd>,
    <trd>tall&oacute;zik</trd>
  </trdgrp>
  <trd lng="ja">&#x843D;&#x7A42;&#x3092;&#x62FE;&#x3046;  <pr>&#x304A;&#x3061;&#x307C;&#x3092;&#x3072;&#x308D;&#x3046;</pr></trd>
  <trd lng="nl">lezen<klr tip="ind"> (v.aren)</klr></trd>
  <trdgrp lng="pl">
    <trd>zbiera&cacute;</trd>,
    <trd>zebra&cacute;  <ind>pok&lstroke;osie</ind>  po &zdot;e&nacute;cach</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol; <ind>&c_k;&c_o;&c_l;&c_o;&c_s;&c_k;&c_i;</ind></trd>
  <trd lng="sk">paberkova&tcaron; <klr>(na strnisku)</klr></trd>
  <trd lng="uk">&c_z;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i; &#x43A;&#x43E;&#x43B;&#x43E;&c_s;&#x43A;&c_i;</trd>
</drv>
</art>
