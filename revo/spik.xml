<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>spik</rad>/o
</kap>
<drv mrk="spik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref>, kie la floroj
      sidas rekte &ccirc;e la akso:
      <ekz>
        <tld/>o de greno, plantago;
      </ekz>
      <ekz>
        <ind>kunmetita <tld/>o</ind>
        <klr>(&ccirc;e meza akso sidas spiketoj, ekz. &ccirc;e
        tritiko, sekalo, hordeo)</klr>;
        <trdgrp lng="de">
          <trd>zusammengesetzte &Auml;hre</trd>,
          <trd>Schein&auml;hre</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <tld/>o malplena plej alte sin tenas
        <klr>(plej nekapablaj kaj nekompetentaj homoj estas plej fieraj.
        <ref tip="sin" cel="barel.0o">barelo</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_a;&c_s;</trd>
  <trd lng="de">&Auml;hre</trd>
  <trdgrp lng="hu">
    <trd>kal&aacute;sz</trd>,
    <trd>f&uuml;z&eacute;r<klr tip="ind"> (vir&aacute;gzat)</klr></trd>
  </trdgrp>
  <trd lng="nl">aar</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_s;</trd>
</drv>

<drv mrk="spik.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <ref tip="dif" cel="rikolt.post0i">postrikolti</ref>
  </snc>
  <trdgrp lng="hu">
    <trd>b&ouml;ng&eacute;szik</trd>,
    <trd>tall&oacute;zik</trd>
  </trdgrp>
  <trd lng="nl">lezen<klr tip="ind"> (v.aren)</klr></trd>
</drv>
</art>
