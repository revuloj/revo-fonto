<drv mrk="atom.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <uzo tip="fak">FIL</uzo>
    <dif>
      <ref tip="super" cel="doktri.0o">Doktrino</ref> pri la <tld/>a konsisto de la materio kaj 
      universo, kaj ilia formi&gcirc;o per kuni&gcirc;o de <tld/>oj:
      <ekz>
        <klr>[fondi&gcirc;is la]</klr> <nom>Epikur</nom><klr>[a]</klr> filozofio tute materiisma kaj
        <tld/>isma de la naturo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=308270"
              >Greka filozofio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        budhana <tld/>ismo kaj la Jaina skolo da&ubreve;rigis la <tld/>tradicion
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2914"
              >Materiismo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_a;&c_m;&c_ib;&c_z;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>atomistika</trd>,
    <trd>atomizmus</trd>
  </trdgrp>
  <trd lng="de">Atomismus</trd>
  <trd lng="en">atomism</trd>
  <trd lng="es">atomismo</trd>
  <trd lng="fr">atomisme</trd>
  <trd lng="he">&alef;&tet;&vav;&mem;&yod;&zayin;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>atomizmus</trd>,
    <trd>atomisztika</trd></trdgrp>
  <trd lng="it">atomismo</trd>
  <trd lng="nl">atomisme</trd>
  <trd lng="ru">&c_a;&c_t;&c_o;&c_m;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">atomizmus</trd>
</drv>

