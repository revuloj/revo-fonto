<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>distord</rad>/o
  </kap>
  <drv mrk="distor.0o">
    <kap><tld/>o</kap>
    <snc num="1">
      <uzo tip="fak">FOT</uzo>
      <uzo tip="fak">FIZ</uzo>
      <dif>
        Aberacio konsistanta el tio, ke bildo, kiu estas iom malproksime
        de
        la &ccirc;efa akso de lenso a&ubreve; de ne enfokusigita
        objektivo,
        estas kurbe reproduktata. 
      </dif>
      <trd
      lng="be">&c_s;&c_k;&c_r;&c_y;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
      <trd lng="de">Verzeichnung</trd>
      <trd lng="pl">dystorsja</trd>
      <trd
      lng="ru">&c_i;&c_s;&c_k;&c_r;&c_i;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </snc>
    <snc num="2">
      <uzo tip="fak">ELE</uzo>
      <dif>
        Aliformi&gcirc;o de elektra a&ubreve; magneta kampo sub la
        influo de alia
        kampo a&ubreve; de okaza ka&ubreve;zo. 
      </dif>
      <trd
      lng="be">&c_s;&c_k;&c_a;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
      <trd lng="de">Verzerrung</trd>
      <trd
      lng="ru">&c_i;&c_s;&c_k;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
    </snc>
    <snc num="3">
      <uzo tip="fak">ELE</uzo>
      <dif>
        Ne&gcirc;usta reprodukto de sono &ccirc;e ricevado en
        radiotelefonio.  
      </dif>
      <trd
      lng="be">&c_s;&c_k;&c_a;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
      <trd lng="de">Verzerrung</trd>
      <trd
      lng="ru">&c_i;&c_s;&c_k;&c_a;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
    </snc>
    <trdgrp lng="fa">
      <trd>&a_w;&a_A;&f_p;&f_y;&f_ch;&a_sh;</trd>,
      <trd>&a_A;&a_ayn;&a_w;&a_j;&a_A;&a_j;</trd>
    </trdgrp>
    <trd lng="fr">distorsion</trd>
    <trd lng="hu">torz&iacute;t&aacute;s</trd>
    <trd lng="pt">distor&ccedil;&atilde;o</trd>
  </drv>
</art>
