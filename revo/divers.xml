<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>divers</rad>/a
</kap>
<drv mrk="divers.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="divers.0a.malsamspeca">
    <dif>
      Malsamspeca:
      <ekz>
        la&ubreve; <tld/>a maniero;
      </ekz>
      <ekz>
        &scirc;tonoj de <tld/>a grandeco;
      </ekz>
      <ekz>
        monpaperoj de <tld/>a valoro;
      </ekz>
      <ekz>
        homoj de <tld/>a ekstera&jcirc;o, de <tld/>aj
        specoj<fnt>Z</fnt>, agoj k karakteroj;
      </ekz>
      <ekz>
        tiun &ccirc;i flankon <tld/>aj esp-istoj povas klarigi al si
        en la plej <tld/>a maniero k en la plej <tld/>aj gradoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>gentaj lo&gcirc;antoj
        <fnt><bib>MT</bib>, <lok>&Jer; 50:37</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>konsista<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="ali.0a">alia</ref>,
      <ref cel="difere.0a">diferenca</ref>,
      <ref cel="mozaik">mozaika</ref>,
      <ref cel="kalejd">kalejdoskopa</ref>.
    </refgrp>
  </snc>
  <snc mrk="divers.0a.kelkaj">
    <uzo tip="klr">(Nur plurale)</uzo>
    <dif>
      Kelke da malsamaj:
      <ekz>
        ni a&ccirc;etis <tld/>ajn ilojn;
      </ekz>
      <ekz>
        <tld/>aj detaloj<fnt>Z</fnt>,
        cirkonstancoj<fnt>Z</fnt>, lokoj<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="plur.0aj">pluraj</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_o;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_n;&c_a;&c_s;&c_t;&c_a;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">r&uring;zn&yacute;</trd>
  <trd lng="de">verschieden</trd>
  <trd lng="en">diverse</trd>
  <trd lng="es">diverso</trd>
  <trd lng="fr">divers</trd>
  <trdgrp lng="hu">
    <trd>k&uuml;l&ouml;nb&ouml;z&odblac;</trd>,
    <trd>k&uuml;l&ouml;nf&eacute;le</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="divers.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Malsameco:
      <ekz>
        la <tld/>eco de la klimatoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_n;&c_a;&c_s;&c_t;&c_a;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Verschiedenheit</trd>
  <trd lng="fr">diversit&eacute;</trd>
  <trd lng="es">diversidad</trd>
  <trdgrp lng="hu">
    <trd>k&uuml;l&ouml;nb&ouml;z&odblac;s&eacute;g</trd>,
    <trd>diverzit&aacute;s</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_i;&c_je;</trd>
</drv>
<drv mrk="divers.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a. 
    </dif>
    <refgrp tip="vid">
      <ref cel="vari">variigi</ref>,
      <ref cel="sxangx.0i">&scirc;an&gcirc;i</ref>,
      <ref cel="difere.0igi">diferencigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_n;&c_a;&c_s;&c_t;&c_a;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_r;&c_a;&c_z;&c_n;&c_a;&c_s;&c_t;&c_a;&c_j;&c_n;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="fr">diversifier</trd>
  <trd lng="es">diversificar</trd>
  <trdgrp lng="hu">
    <trd>k&uuml;l&ouml;nb&ouml;z&odblac;v&eacute; tesz</trd>,
    <trd>diverzifik&aacute;l</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
