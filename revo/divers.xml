    <trd>&#22793;&#21270;&#12434;&#12388;&#12369;&#x308B;  <pr>&#12408;&#12435;&#x304B;&#12434;&#12388;&#12369;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>por&oacute;&zdot;nia&cacute;</trd>,
    <trd>dywersyfikowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_n;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>oddeli&tcaron;</trd>,
    <trd>rozli&scaron;ova&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_u;&c_r;&c_ib;&c_z;&c_n;&c_o;&c_m;&c_a;&c_n;&c_ib;&c_t;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="divers.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <snc>
    <dif>
      Fari&gcirc;i <tld/>a, varia:
      <ekz>
        ili poste dividi&gcirc;is kaj <tld/>i&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Mihail Korotkov </aut>
          <vrk><url ref="&Monato;/2012/010634.php">La turka havas familion</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz><ekz>
        <klr>[la]</klr> kolekto sufi&ccirc;e vasti&gcirc;is <klr>[&#8230;]</klr> kaj tre diversi&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Sylvain Lelarge</aut>
          <vrk><url ref="&Monato;/2004/008496.html">La argilaj najbaretoj de Jesuo
          provenca tradicio</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz><ekz>
        multaj japanaj kompanioj fiaskis, kiam ili <tld/>i&gcirc;is, neglektante sian propran
        aferon
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2013/010932.php">Adapti, persisti,
          akcepti</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
