<kap>
   <ofc>9</ofc><rad>saksofon</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="saksof.0o">
   <kap><ofc>9</ofc><tld/>o</kap>
   <uzo tip="fak">MUZ</uzo>
   <snc>
     <dif>
      Kupra <ref tip="lst" cel="instru2.blov0o" lst="voko:blovinstrumentoj">blovinstrumento</ref>
      kun klapoj:
      <ekz>
         li scipovas ludi anka&ubreve; germanan kaj rusan tirharmonikojn, bandonion, koncertinon, <tld/>on,
         flutetojn
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2003/008095.html">La segilo sonas!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la disko Civilizacio enhavas junularan rokmuzikon, en tre klara Esperanto, 
        akompanatan de perkutiloj, gitaro, piano, orgeno, <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>pp/cs</aut>
          <vrk><url ref="&Monato;/2005/008543.php">Tiel ili protestas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiu dika tipo / Kun kolumo el karton' / Estas ido de l' polipo / Kaj de ina <tld/>'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_s;&c_a;&c_k;&c_s;&c_a;&c_f;&c_o;&c_n;</trd>
   <trd lng="bg">&c_s;&c_a;&c_k;&c_s;&c_o;&c_f;&c_o;&c_n;</trd>
   <trdgrp lng="ca">
     <trd>sax&ograve;fon</trd>,
     <trd>saxofon</trd>,
     <trd>saxo</trd>
   </trdgrp>
   <trd lng="de">Saxophon</trd>
   <trd lng="en">saxophone</trd>
   <trdgrp lng="es">
     <trd>saxof&oacute;n</trd>,
     <trd>sax&oacute;fono</trd>,
     <trd>saxo <klr> (abrev.)</klr></trd>
   </trdgrp>
   <trd lng="hu">szakszofon</trd>
   <trd lng="id">saksofon</trd>
   <trdgrp lng="it">
