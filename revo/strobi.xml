<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: strobi.xml,v 1.19 2024/03/25 07:28:37 revo Exp $">
<kap>
  <rad>strobil</rad>/o
</kap>
<drv mrk="strobi.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="strobi.0o.BOT">
    <dif>
      <ref tip="super" cel="inflor.0o">Infloresko</ref>
      spikforma kun ligneca akso kaj folioj:
      <ekz>
        <tld/>o de abio, pino, alno;
      </ekz>
      <ekz>
        ses eta&gcirc;ojn, plenaj de budoj, butikoj, <klr>[&#x2026;]</klr> magazenoj de ligno,
        olivoleo, greno, nuksoj, piniaj <tld/>oj, per kies grajnoj nutris sin la
        malri&ccirc;a lo&gcirc;antaro
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="konus.0o.BOT">konuso<sncref/></ref>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_sh;&c_k;&c_a;</trd>
  <trd lng="bg">&c_sh;&c_i;&c_sh;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="de">Zapfen</trd>
  <trdgrp lng="en">
    <trd>cone</trd>,
    <trd>strobilus</trd>
  </trdgrp>
  <trd lng="es">pi&ntilde;a</trd>
  <trd lng="fr">c&ocirc;ne</trd>
  <trd lng="hu">toboz</trd>
  <trd lng="ja">&#x7403;&#x679C;  <pr>&#x304D;&#x3085;&#x3046;&#x304B;</pr></trd>
  <trdgrp lng="nl">
    <trd>dennenappel</trd>,
    <trd>sparappel</trd>
  </trdgrp>
  <trd lng="pl">szyszka</trd>
  <trd lng="ru">&c_sh;&c_i;&c_sh;&c_k;&c_a; <klr tip="amb">(&c_je;&c_l;&c_o;&c_v;&c_a;&c_ja;,
  &c_s;&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;&c_ja; &c_i;
  &c_t;.&c_p;.)</klr></trd>
  <trd lng="uk">&c_sh;&c_i;&c_sh;&c_k;&c_a;</trd>
  <trd lng="zh">&#x677E;&#x679C; <pr>s&#x14D;nggu&#x1D2;</pr></trd>
</drv>
</art>
