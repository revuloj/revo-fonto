<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: later.xml,v 1.50 2024/01/15 16:14:08 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>later</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="later.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="later.0o.streko">
    <fnt><bib>MatTerm</bib><lok>p. 27, pri triangulo</lok></fnt>
    <dif>
      <ref tip="super" cel="strek.0o.segmento">Streko</ref>,
      limanta <ref cel="later.plur0o">plur<tld/>on</ref>; alternative:
      la longo de tiu streko:
      <ekz>
        la randojn de la <tld/>oj oni nomas <ref cel="vertic.0o">verticoj</ref>;
      </ekz>
      <ekz>
        la areo de rektangulo egalas al la produto de &gcirc;ia
        granda <tld/>o per la malgranda;
      </ekz>
      <ekz>
        <klr>[li]</klr> eniris malgrandan kvadratan korton, tri <tld/>ojn de kiu
        borderis provizejoj
        <fnt>
          <bib>MkM</bib>
          <lok>26</lok>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve;lar&gcirc;e de la korto, <klr>[&#x2026;]</klr> kurtenoj, havantaj la longon de dek kvin
        ulnoj, estu por unu <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 27:14</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="flank.0o.plurlatero">flanko</ref>.
    </refgrp>
    <trd lng="en">side<klr tip="ind"> (of a polygon)</klr></trd>
    <trd lng="pl">bok<klr tip="ind"> (wielok&aogonek;ta)</klr></trd>
  </snc>
  <snc mrk="later.0o.angulo">
    <fnt><bib>MatTerm</bib><lok>p. 26</lok></fnt>
    <dif>
      <klr>(de <ref cel="angul.0o.afina">angulo</ref>)</klr>
      &Ccirc;iu el la du duonrektoj, &gcirc;in konsistigantaj.
    </dif>
    <trdgrp lng="en">
      <trd>side<klr tip="ind"> (of an angle)</klr></trd>,
      <trd>arm<klr tip="ind"> (of an angle)</klr></trd>
    </trdgrp>
    <trd lng="pl">rami&eogonek;<klr tip="ind">
    (k&aogonek;ta)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x4FA7; <pr>c&#xE8;</pr></trd>,
    <trd>&#x5074; <pr>c&#xE8;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_o;&c_k; <klr tip="ind">(&c_v;&c_u;&c_g;&c_l;&c_a; &c_a;&c_b;&c_o;
  &c_m;&c_n;&c_o;&c_g;&c_a;&c_v;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_a;)</klr></trd>
  <trd lng="cs">strana <klr>(mnoho&uacute;heln&iacute;ka)</klr></trd>
  <trdgrp lng="de">
    <trd>Seite</trd>,
    <trd>Schenkel</trd>
  </trdgrp>
  <trd lng="es">lado<klr tip="amb"> (de un pol&iacute;gono o
  &aacute;ngulo)</klr></trd>
  <trdgrp lng="fr">
    <trd>c&ocirc;t&eacute;<klr tip="ind"> (d'un angle ou
  polygone)</klr></trd>,
    <trd>ar&ecirc;te<klr tip="ind"> (geom.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>oldal</trd>,
    <trd>sz&aacute;r</trd>
  </trdgrp>
  <trd lng="ja">&#x8FBA;  <pr>&#x308F;&#x305F;&#x308A;</pr></trd>
  <trd lng="nl">zijde<klr tip="ind"> (v.e.meetkundig
  voorwerp)</klr></trd>
  <trd lng="pt">lado</trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_a;<klr tip="ind">
  (&c_u;&c_g;&c_l;&c_a; &c_i;&c_l;&c_i;
  &c_m;&c_n;&c_o;&c_g;&c_o;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_k;&c_a;)</klr></trd>
  <trdgrp lng="sk">
    <trd>rameno</trd>,
    <trd>strana</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_ib;&c_k;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8FB9; <pr>bi&#x101;n</pr></trd>,
    <trd>&#x908A; <pr>bi&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="later.egal0a">
  <kap>egal<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib><lok>angulo</lok></fnt>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="later.plur0o">plur<tld/>o</ref>)</klr>
      Kies &ccirc;iuj <tld/>oj estas <ref cel="egal.0a.izometria">egalaj</ref>:
      <ekz>
        egal<tld/>a triangulo estas anka&ubreve;
        <ref cel="regul.0a.plurlatero">regula</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_w;&c_n;&c_a;&c_s;&c_t;&c_a;&c_r;&c_o;&c_n;&c_mol;&c_n;&c_ib;</trd>
  <trd lng="cs">rovnostrann&yacute;</trd>
  <trd lng="de">gleichseitig</trd>
  <trd lng="en">equilateral</trd>
  <trd lng="es">equil&aacute;tero</trd>
  <trd lng="fr">&eacute;quilat&eacute;ral</trd>
  <trd lng="hu">egyenl&odblac; oldal&uacute;</trd>
  <trd lng="nl">gelijkzijdig</trd>
  <trd lng="pl">r&oacute;wnoboczny</trd>
  <trdgrp lng="pt">
    <trd>eq&uuml;il&aacute;tero</trd>,
    <trd>equil&aacute;tero</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_v;&c_n;&c_o;&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_n;&c_i;&c_j;</trd>
  <trd lng="sk">rovnostrann&yacute;</trd>
  <trd lng="uk">&c_r;&c_ib;&c_v;&c_n;&c_o;&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_n;&c_ib;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x4E24;&#x8FB9;&#x76F8;&#x7B49; <pr>li&#x1CE;ngbi&#x101;nxi&#x101;ngd&#x11B;ng</pr></trd>,
    <trd>&#x5169;&#x908A;&#x76F8;&#x7B49; <pr>li&#x1CE;ngbi&#x101;nxi&#x101;ngd&#x11B;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="later.kontraux0a">
  <kap>kontra&ubreve;<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
  <snc>
    <dif>
      <klr>(p.p. du <ref cel="angul.0o.figuro">anguloj</ref>)</klr>
      Havantaj saman verticon kaj
      <ref cel="kontraux.0a.duonrekto">kontra&ubreve;ajn</ref>
      <tld/>ojn:
      <ekz>
        du kontra&ubreve;<tld/>aj anguloj estas
        <ref cel="egal.0a.izometria">egalaj</ref>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>entgegengesetzt<klr tip="ind"> (-er Winkel)</klr></trd>,
    <trd>Scheitel-<klr tip="amb">(winkel)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>opposite<klr tip="ind"> (angle)</klr></trd>,
    <trd>vertically opposite<klr tip="ind"> (angle)</klr></trd>
  </trdgrp>
  <trd lng="fr"><klr tip="ind">(angle) </klr>oppos&eacute; [par le
  sommet]</trd>
  <trd lng="hu">ellent&eacute;tes oldal&uacute;</trd>  
  <trd lng="pl"><klr tip="ind">(k&aogonek;t)
  </klr>wierzcho&lstroke;kowy</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_r;&c_t;&c_i;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_u;&c_g;&c_o;&c_l;)</klr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5BF9;&#x89D2; <pr>du&#xEC;ji&#x1CE;o</pr></trd>,
    <trd>&#x5C0D;&#x89D2; <pr>du&#xEC;ji&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="later.kvar0o">
  <kap>kvar<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="later.plur0o">Plur<tld/>o</ref> kun
      <frm>4</frm> lateroj.
    </dif>
    <refgrp tip="sub">
      Ekzemploj de kvarlateroj:
      <ref cel="deltoi.0o.figuro">deltoido</ref>,
      <ref cel="kvadra1.0o.figuro">kvadrato</ref>,
      <ref cel="lozang.0o.figuro">lozan&gcirc;o</ref>,
      <ref cel="angul.ort0o">ortangulo</ref>,
      <ref cel="parale2.0o">paralelogramo</ref>,
      <ref cel="romb.0o.figuro">rombo</ref>,
      <ref cel="trapez.0o.figuro">trapezo</ref>.
    </refgrp>
    <bld lok="../bld/e_kvarlateroj.svg" tip="svg" lrg="800" alt="400">
      specoj de kvar<tld/>oj
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_t;&c_y;&c_r;&c_o;&c_h;&c_v;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_ch;&c_a;&c_t;&c_y;&c_r;&c_o;&c_h;&c_k;&c_u;&c_t;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">&ccaron;ty&rcaron;stran</trd>
  <trd lng="de">Viereck</trd>
  <trd lng="en">quadrilateral</trd>
  <trd lng="es">cuadril&aacute;tero</trd>
  <trd lng="fr">quadrilat&egrave;re</trd>
  <trd lng="hu">n&eacute;gysz&ouml;g</trd>
  <trd lng="ja">&#x56DB;&#x8FBA;&#x5F62;  <pr>&#x3057;&#x3078;&#x3093;&#x3051;&#x3044;</pr></trd>
  <trd lng="nl">vierhoek</trd>
  <trd lng="pl">czworok&aogonek;t</trd>
  <trd lng="pt">quadril&aacute;tero</trd>
  <trd lng="ru">&c_ch;&c_je;&c_t;&c_y;&c_r;&c_jo;&c_h;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">&scaron;tvorsten</trd>
  <trd lng="uk">&c_ch;&c_o;&c_t;&c_i;&c_r;&c_i;&c_k;&c_u;&c_t;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#x56DB;&#x89D2;&#x5F62; <pr>s&#xEC;ji&#x1CE;ox&#xED;ng</pr></trd>,
    <trd>&#x56DB;&#x8FB9;&#x5F62; <pr>s&#xEC;bi&#x101;nx&#xED;ng</pr></trd>,
    <trd>&#x56DB;&#x908A;&#x5F62; <pr>s&#xEC;bi&#x101;nx&#xED;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="later.plur0o">
  <kap>plur<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Ebena <ref tip="super" cel="surfac.0o.MAT">surfaco</ref>, kiun
      limas
      finia nombro da tiaj <ref cel="strek.0o.segmento">strekoj</ref>,
      ke
      &ccirc;iu el &gcirc;iaj <ref cel="rand.0o.figuro">randoj</ref>
      estas rando de unu alia streko.
    </dif>
    <refgrp tip="vid">
      Atributoj de plurlatero:
      <ref cel="skrib.cxirkaux0ita">&ccirc;irka&ubreve;skribita</ref>
      cirklo,
      <ref cel="skrib.en0ita.enplurlateropluredro">enskribita</ref>
      cirklo,
      <ref cel="skrib.en0ita.encirklosfero">enskribita</ref> en cirklo,
      <ref cel="konkav.0a.subaro">konkava</ref>,
      <ref cel="konvek.0a.subaro">konveksa</ref>,
      <ref cel="angul.ort0a">ortangula</ref>,
      <ref cel="regul.0a.plurlatero">regula</ref>;
    </refgrp>
    <refgrp tip="prt">
      Karakterizaj partoj de plurlateroj:
      <ref cel="apotem.0o.plurlatero">apotemo</ref>,
      <ref cel="centr.0o.plurlatero">centro</ref>,
      <ref cel="diagon.0o.plurlatero">diagonalo</ref>,
      <ref cel="later.0o.streko">latero</ref>,
      <ref cel="vertic.0o.plurlatero">vertico</ref>;
    </refgrp>
    <refgrp tip="sub">
      Ekzemploj de plurlateroj:
      <ref cel="later.n0o">n-latero</ref>,
      <ref cel="angul.tri0o">triangulo</ref>,
      <ref cel="angul.rekt0o">rektangulo</ref>,
      <ref cel="kvadra1.0o.figuro">kvadrato</ref>,
      <ref cel="lozang.0o.figuro">lozan&gcirc;o</ref>,
      <ref cel="parale2.0o">paralelogramo</ref>,
      <ref cel="trapez.0o.figuro">trapezo</ref>.
    </refgrp>
    
    <rim>
      La studataj en elementa geometrio plur<tld/>oj estas plejofte
      <ref cel="konvek.0a.subaro">konveksaj</ref>, kaj oni plie postulas
      de
      ili, ke tri sinsekvaj verticoj ne estu
      <ref cel="rekt.sam0a">samrektaj</ref>.
    </rim>
    <rim>
      Bricard
      <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>, kiel estas &ccirc;e
      li
      kutime, uzas la prefikson <ctl>mult</ctl> anstata&ubreve;
      <ctl>plur</ctl>.
      Krome li aludas al senca diferenco inter <ctl>mult<tld/>o</ctl>
      kaj
      <ctl>multangulo</ctl>, sed &scirc;ajnas, ke tiu diferenco validas
      nur
      en kelkaj maloftaj figuroj, kiel la t.n. <ctl>plena
      kvarangulo</ctl>.
    </rim>
  </snc>
  <trd lng="en">polygon</trd>
  <trdgrp lng="be">
    <trd>&c_m;&c_n;&c_o;&c_g;&c_a;&c_v;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_m;&c_n;&c_o;&c_g;&c_a;&c_k;&c_u;&c_t;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vieleck</trd>,
    <trd>Polygon</trd>
  </trdgrp>
  <trd lng="es">pol&iacute;gono</trd>
  <trd lng="fr">polygone</trd>
  <trdgrp lng="hu">
    <trd>soksz&ouml;g</trd>,
    <trd>poligon</trd>
  </trdgrp>  
  <trd lng="ja">&#x591A;&#x8FBA;&#x5F62;  <pr>&#x305F;&#x3078;&#x3093;&#x3051;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>veelhoek</trd>,
    <trd>polygoon</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wielok&aogonek;t</trd>,
    <trd>wielobok</trd>
  </trdgrp>
  <trd lng="pt">pol&iacute;gono</trd>
  <trd lng="ru">&c_m;&c_n;&c_o;&c_g;&c_o;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#x591A;&#x8FB9;&#x5F62; <pr>du&#x14D;bi&#x101;nx&#xED;ng</pr></trd>,
    <trd>&#x591A;&#x908A;&#x5F62; <pr>du&#x14D;bi&#x101;nx&#xED;ng</pr></trd>,
    <trd>&#x591A;&#x89D2;&#x5F62; <pr>du&#x14D;ji&#x1CE;ox&#xED;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="later.n0o">
  <kap>n-<tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="later.plur0o">Plur<tld/>o</ref> kun
      <frm><k>n</k></frm> lateroj:
      <ekz>
        <ind>tri<tld/>o</ind>
        <klr>(pli kutime nomata <ref cel="angul.tri0o">triangulo</ref>)</klr>,        
        <trdgrp lng="be">
          <trd>&c_t;&c_r;&c_o;&c_h;&c_v;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
          <trd>&c_t;&c_r;&c_o;&c_h;&c_k;&c_u;&c_t;&c_n;&c_ib;&c_k;</trd>
        </trdgrp>
        <trd lng="de">Dreieck</trd>
        <trd lng="en">triangle</trd>
        <trd lng="fr">triangle</trd>
        <trd lng="hu">h&aacute;romsz&ouml;g</trd>
        <trd lng="pl">tr&oacute;jk&aogonek;t</trd>
        <trd lng="pt">tri&acirc;ngulo</trd>
        <trd lng="ru">&c_t;&c_r;&c_je;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
      </ekz>
      <ekz>
        <ref cel="later.kvar0o">kvar<tld/>o</ref>,
      </ekz>
      <ekz>
        <ind>kvin<tld/>o</ind>,
        <trdgrp lng="be">
          <trd>&c_p;&c_ja;&c_c;&c_ib;&c_v;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
          <trd>&c_p;&c_ja;&c_c;&c_ib;&c_k;&c_u;&c_t;&c_n;&c_ib;&c_k;</trd>
        </trdgrp>
        <trd lng="de">F&uuml;nfeck</trd>
        <trd lng="en">pentagon</trd>
        <trd lng="fr">pentagone</trd>
        <trdgrp lng="hu">
          <trd>&ouml;tsz&ouml;g</trd>,
          <trd>pentagon</trd>
        </trdgrp>
        <trdgrp lng="nl">
          <trd>vijfhoek</trd>,
          <trd>pentagoon</trd>
        </trdgrp>
        <trd lng="pl">pi&eogonek;ciok&aogonek;t</trd>
        <trd lng="pt">pent&aacute;gono</trd>
        <trd lng="ru">&c_p;&c_ja;&c_t;&c_i;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
      </ekz>
      <ekz>
        <ind>ses<tld/>o</ind>,
        <trdgrp lng="be">
          <trd>&c_sh;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_v;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
          <trd>&c_sh;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_k;&c_u;&c_t;&c_n;&c_ib;&c_k;</trd>
        </trdgrp>
        <trd lng="de">Sechseck</trd>
        <trd lng="en">hexagon</trd>
        <trd lng="fr">hexagone</trd>
        <trdgrp lng="hu">
          <trd>hatsz&ouml;g</trd>,
          <trd>hexagon</trd>
        </trdgrp>
        <trdgrp lng="nl">
          <trd>zeshoek</trd>,
          <trd>hexagoon</trd>
        </trdgrp>
        <trd lng="pl">sze&sacute;ciok&aogonek;t</trd>
        <trd lng="pt">hexagono</trd>
        <trd lng="ru">&c_sh;&c_je;&c_s;&c_t;&c_i;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
      </ekz>
      <ekz>
        <ind>sep<tld/>o</ind>,
        <trdgrp lng="be">
          <trd>&c_s;&c_ja;&c_m;&c_ib;&c_v;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
          <trd>&c_s;&c_ja;&c_m;&c_ib;&c_k;&c_u;&c_t;&c_n;&c_ib;&c_k;</trd>
        </trdgrp>
        <trd lng="de">Siebeneck</trd>
        <trd lng="en">heptagon</trd>
        <trd lng="fr">heptagone</trd>
        <trdgrp lng="hu">
          <trd>h&eacute;tsz&ouml;g</trd>,
