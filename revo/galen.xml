<vortaro>
<art mrk="$Id$">
<kap>
  <rad>galen</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="galen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="fak">MIN</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      PbS. Natura plumba sulfido, metalaspekta, grizeblua, uzata
      por <ref cel="detekt.0ilo">detektiloj</ref>.
    </dif>
  </snc>
  <trd lng="de">Bleiglanz</trd>
  <trd lng="el">&gamma;&alfa;&lambda;&alfa;&ny;&jota_ton;&tau;&eta;&sigma_fina;</trd>
  <trd lng="en">galena</trd>
  <trd lng="fr">gal&egrave;ne</trd>
  <trdgrp lng="hu">
    <trd>galenit</trd>,
    <trd>detektorkrist&aacute;ly</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loodglans</trd>,
    <trd>galeniet</trd>
  </trdgrp>
  <trd lng="pt">galena</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_je;&c_n;&c_i;&c_t;</trd>,
    <trd>&c_s;&c_v;&c_i;&c_n;&c_c;&c_o;&c_v;&c_y;&c_j; &c_b;&c_l;&c_je;&c_s;&c_k;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j; &c_s;&c_v;&c_i;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
</drv>
</art>
