<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gobi</rad>/o
</kap>
<drv mrk="gobi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gobi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda bongusta <ref tip="lst" cel="fisx.0o" 
      lst="voko:fi&scirc;oj">fi&scirc;o</ref>, vivanta surfunde de
      rapidfluaj sed anka&ubreve; senfluaj nesalaj akvoj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt"
      cel="ciprin1.0oj">ciprinedoj</ref>
      (<trd lng="la">Gobio</trd>).
    </dif>
    <ref tip="sin" cel="gugon.0o">gugono</ref>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_ch;&c_k;&c_u;&c_r;</trd>
  <trd lng="ca">gobi</trd>
  <trd lng="de">Gr&uuml;ndling</trd>
  <trd lng="fr">goujon</trd>
  <trd lng="hu">g&eacute;b</trd>
  <trdgrp lng="nl">
    <trd>grondel</trd>,
    <trd>grondeling</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>g&oacute;bio</trd>, 
   <trd>cadoz</trd>, 
   <trd>gobi&atilde;o</trd> 
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_s;&c_k;&c_a;&c_r;&c_mol;</trd>
</drv>
</art>
