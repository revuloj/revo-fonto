<art mrk="$Id: gobi.xml,v 1.24 2022/04/30 08:42:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gobi</rad>/o
</kap>
<drv mrk="gobi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gobi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda bongusta <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>, vivanta surfunde de
      rapidfluaj sed anka&ubreve; senfluaj nesalaj akvoj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="ciprin1.0oj">ciprinedoj</ref>
      (<trd lng="la">Gobio</trd>):
      <ekz>
        se ezoko pii&gcirc;is, <tld/>o ne dormu
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="gugon.0o">gugono</ref>
    <bld lok="&WCU;/thumb/5/54/Gudgeons_(Gobio_gobio)_(13533313875).jpg/640px-Gudgeons_(Gobio_gobio)_(13533313875).jpg" prm="CC BY-SA 2.0"><tld/>o (Gobio gobio) en la akvario de Val-de-Loire, Indre-et-Loire, Francujo
        <fnt>
          <aut>Bernard Dupont</aut>
          <lok><url ref="&WCW;/File:Gudgeons_(Gobio_gobio)_(13533313875).jpg">&WCW;/File:Gudgeons_(Gobio_gobio)_(13533313875).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_ch;&c_k;&c_u;&c_r;</trd>
  <trd lng="ca">gobi</trd>
  <trd lng="cs">pisko&rcaron;</trd>
  <trd lng="de">Gr&uuml;ndling</trd>
  <trdgrp lng="en">
    <trd>gudgeon</trd>,
    <trd>river goby</trd>  
  </trdgrp>
  <trd lng="fr">goujon</trd>
  <trd lng="hu">g&eacute;b</trd>
  <trdgrp lng="nl">
    <trd>grondel</trd>,
    <trd>grondeling</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>g&oacute;bio</trd>, 
   <trd>cadoz</trd>, 
   <trd>gobi&atilde;o</trd> 
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_s;&c_k;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sk">piskor</trd>
  <trd lng="uk">&#x43F;&#x456;&#x447;&#x43A;&#x443;&#x440;</trd>
</drv>
</art>
