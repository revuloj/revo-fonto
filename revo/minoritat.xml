<kap>
  <rad>minoritat</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="minoritat.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="minoritat.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="mult.malpli0o">Malplimulto</ref>:
      <ekz>
        diskriminacio de naciaj <tld/>oj
        <fnt>
          <aut>Sylvia Brylska</aut>
          <vrk><url ref="&Monato;/1993/A930801.php">&Ccirc;u reiri al naciaj
          radikoj?</url></vrk>
          <bib>Monato</bib>
          <lok>1993/08, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a registaro
        <fnt>
	<aut>Donald Rogers</aut>
	<vrk><url ref="&Monato;/2000/006819.php">Laborema koalicio</url></vrk>
	<bib>Monato</bib>
	<lok>2000/03, p. 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu ajn <tld/>a movado, e&ccirc; la plej serioza, riskas la dan&gcirc;eron 
        esti rigardata kiel sekteca
        <fnt>
          <aut>I. Lapenna</aut>,
          <vrk><url ref="http://www.angelfire.com/ny2/ts/lapenna.html">
          Por pli efika Informado</url></vrk>,
          <lok>1974</lok>
        </fnt>;
      </ekz>
      <ekz>
        programoj pri savado kaj protektado de pereontaj lingvoj 
        parolaj kaj skribaj de naci<tld/>oj estas plenumataj
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;121/2007/05/29/1@62266.htm">
          &Ccirc;inio klopodas protekti lingvojn...</url></vrk>,
          <lok>2007-05-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="ant" cel="majoritat.0o">majoritato, ma&jcirc;orio</ref>
  <trd lng="be">&c_m;&c_je;&c_n;&c_sh;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_m;&c_a;&c_l;&c_c;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="de">Minderheit</trd>
  <trd lng="en">minority</trd>
  <trd lng="es">minor&iacute;a</trd>
  <trd lng="fr">minorit&eacute;</trd>
  <trd lng="hu">kisebbs&eacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x5C11;&#x6570;&#x6D3E;  [<ind>&#x3057;&#x3087;&#x3046;&#x3059;&#x3046;&#x306F;</ind>]</trd>,
    <trd>&#x30DE;&#x30A4;&#x30CE;&#x30EA;&#x30C6;&#x30A3;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">minderheid<klr tip="ind"> (ontr.)</klr></trd>
  <trd lng="pl">mniejszo&sacute;&cacute;</trd>
  <trd lng="ru">&c_m;&c_je;&c_n;&c_mol;&c_sh;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>men&scaron;ina</trd>,
    <trd>minorita</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>men&scaron;ina</trd>,
    <trd>minorita</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_je;&#x43D;&c_sh;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43C;&c_je;&#x43D;&c_sh;&c_i;&#x43D;&c_a;</trd>
  </trdgrp>
</drv>
</art>

