<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kandid.xml,v 1.26 2023/12/03 08:00:22 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kandidat</rad>/o
</kap>
<drv mrk="kandid.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo, kiu aspiras al ofico, posteno a&ubreve; titolo kaj oficiale
      konigas
      sian aspiron:
      <ekz>
        <tld/>o al parlamento, al la posteno de direktoro, al la
        gimnazia
        diplomo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Esperanto estas nun la sola serioza <tld/>o en la
        mondo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_k;&c_a;&c_n;&c_d;&c_i;&c_d;&c_a;&c_t;</trd>
    <trdgrp lng="de">
      <trd>Kandidat</trd>,
      <trd>Anw&auml;rter</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>candidate <klr>(for office or honor)</klr></trd>,
      <trd>applicant</trd>,
      <trd>aspirant</trd>
    </trdgrp>
    <trd lng="es">candidato</trd>
    <trdgrp lng="fr">
      <trd>candidat</trd>,
      <trd>postulant</trd>
    </trdgrp>
    <trd lng="hu">jel&ouml;lt</trd>
    <trd lng="pl">kandydat</trd>
    <trd lng="pt">candidato</trd>
    <trd lng="ru">&c_k;&c_a;&c_n;&c_d;&c_i;&c_d;&c_a;&c_t;</trd>
  </snc>
  <snc>
    <uzo tip="fak">KRI</uzo>
    <uzo tip="klr">(en protestantaj landoj)</uzo>
    <dif>
      Homo, kiu aspiras al pastreco.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_d;&c_y;&c_d;&c_a;&c_t;</trd>
  <trd lng="ca">candidat</trd>
  <trdgrp lng="ja">
    <trd>&#x5019;&#x88DC;&#x8005;  <pr>&#x3053;&#x3046;&#x307B;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x5FD7;&#x9858;&#x8005;  <pr>&#x3057;&#x304C;&#x3093;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x51FA;&#x9858;&#x8005;  <pr>&#x3057;&#x3085;&#x3064;&#x304C;&#x3093;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trd lng="ms">pemohon</trd>
  <trdgrp lng="cs">
    <trd>kandid&aacute;t</trd>,
    <trd>&ccaron;ekatel</trd>
  </trdgrp>
  <trd lng="nl">kandidaat</trd>
  <trd lng="no">kandidat</trd>
  <trdgrp lng="sk">
    <trd>kandid&aacute;t</trd>,
    <trd>uch&aacute;dza&ccaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x430;&#x43D;&#x434;&#x438;&#x434;&#x430;&#x442;</trd>
  <trdgrp lng="zh">
    <trd>&#x5019;&#x9009; <pr>h&#xF2;uxu&#x1CE;n</pr></trd>,
    <trd>&#x5019;&#x9009;&#x4EBA; <pr>h&#xF2;uxu&#x1CE;nr&#xE9;n</pr></trd>,
    <trd>&#x7ADE;&#x6280;&#x8005; <pr>j&#xEC;ngj&#xEC;zh&#x11B;</pr></trd>,
    <trd>&#x4EBA;&#x9009; <pr>r&#xE9;nxu&#x1CE;n</pr></trd>,
    <trd>&#x53C2;&#x9009;&#x4EBA; <pr>c&#x101;nxu&#x1CE;nr&#xE9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kandid.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Klopodi por atingi aspirita&jcirc;on:
      <ekz>
        <tld/>i al, pri io;
      </ekz>
      <ekz>
        <tld/>i ambasadorecon
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ambici">ambicii</ref>,
      <ref cel="cel.0i">celi</ref>,
      <ref cel="dezir.0i">deziri</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_k;&c_a;&c_n;&c_d;&c_y;&c_d;&c_a;&c_t;&c_a;&c_m;</ind></trd>,
    <trd>&c_p;&c_r;&c_e;&c_t;&c_e;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_a;
    &c_p;&c_a;&c_s;&c_a;&c_d;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>aspirar a </trd>,
    <trd>&eacute;sser candidat</trd>
  </trdgrp>
  <trd lng="cs">kandidovat <klr>(na n&ecaron;co)</klr></trd>
  <trdgrp lng="de">
    <trd>kandidieren</trd>,
    <trd>sich zur<ind>Wahl</ind> stellen</trd>,
    <trd>sich <ind>bewerben</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>apply for</trd>,
    <trd>aspire to (an office)</trd>,
    <trd>seek<klr tip="ind"> (to obtain honor or office)</klr></trd>,
    <trd>stand as a <ind>candidate</ind> for</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>candidat</ind></trd>,
    <trd>postuler<klr tip="ind"> (&ecirc;tre candidat &agrave;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jel&ouml;lt <klr tip="ind">(vmire)</klr></trd>,
    <trd>p&aacute;ly&aacute;zik <klr tip="ind">(vmire)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7ACB;&#x5019;&#x88DC;&#x3059;&#x308B;  <pr>&#x308A;&#x3063;&#x3053;&#x3046;&#x307B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5FD7;&#x9858;&#x3059;&#x308B;  <pr>&#x3057;&#x304C;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zich <ind>kandidaat</ind> stellen</trd>,
    <trd>solliciteren</trd>
  </trdgrp>
  <trd lng="no">kandidere</trd>
  <trd lng="pl">kandydowa&cacute;</trd>
  <trd lng="pt">candidatar-se</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_k;&c_a;&c_n;&c_d;&c_i;&c_d;&c_a;&c_t;&c_o;&c_m; <klr>(&c_n;&c_a;
    &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_p;&c_r;&c_je;&c_t;&c_je;&c_n;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_n;&c_a;
    &c_d;&c_o;&c_l;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;)</klr></trd>
  </trdgrp>
  <trd lng="sk">kandidova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x441;&#x442;&#x443;&#x43F;&#x430;&#x442;&#x438; &#x432; &#x44F;&#x43A;&#x43E;&#x441;&#x442;&#x456; &#x43A;&#x430;&#x43D;&#x434;&#x438;&#x434;&#x430;&#x442;&#x430;</trd>,
    <trd>&#x432;&#x438;&#x441;&#x443;&#x432;&#x430;&#x442;&#x438; &#x441;&#x432;&#x43E;&#x44E; &#x43A;&#x430;&#x43D;&#x434;&#x438;&#x434;&#x430;&#x442;&#x443;&#x440;&#x443;</trd>
  </trdgrp>
  <trd lng="zh">&#x53C2;&#x9009; <pr>c&#x101;nxu&#x1CE;n</pr></trd>
</drv>
</art>
