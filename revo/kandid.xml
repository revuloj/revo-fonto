<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kandidat</rad>/o
</kap>
<drv mrk="kandid.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo, kiu aspiras al ofico, posteno a&ubreve; titolo kaj oficiale konigas
      sian aspiron:
      <ekz>
        <tld/>o al parlamento, al la posteno de direktoro, al la gimnazia
        diplomo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Esperanto estas nun la sola serioza <tld/>o en la
        mondo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kandidat</trd>,
      <trd>Anw&auml;rter</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>candidate <klr>(for office or honor)</klr></trd>,
      <trd>applicant</trd>,
      <trd>aspirant</trd>
    </trdgrp>
    <trd lng="es">candidato</trd>
    <trdgrp lng="fr">
      <trd>candidat</trd>,
      <trd>postulant</trd>
    </trdgrp>
    <trd lng="hu">jel&ouml;lt</trd>
    <trd lng="pt">candidato</trd>
    <trd lng="ru">&c_k;&c_a;&c_n;&c_d;&c_i;&c_d;&c_a;&c_t;</trd>
  </snc>
  <snc>
    <uzo tip="fak">KRI</uzo>
    <uzo tip="klr">(en protestantaj landoj)</uzo>
    <dif>
      Homo, kiu aspiras al pastreco.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_d;&c_y;&c_d;&c_a;&c_t;</trd>
  <trd lng="nl">kandidaat</trd>
</drv>

<drv mrk="kandid.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Klopodi por atingi aspirita&jcirc;on:
      <ekz>
        <tld/>i al, pri io;
      </ekz>
      <ekz>
        <tld/>i ambasadorecon
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ambici">ambicii</ref>,
      <ref cel="cel.0i">celi</ref>,
      <ref cel="dezir.0i">deziri</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_k;&c_a;&c_n;&c_d;&c_y;&c_d;&c_a;&c_t;&c_a;&c_m;</ind></trd>,
    <trd>&c_p;&c_r;&c_e;&c_t;&c_e;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_p;&c_a;&c_s;&c_a;&c_d;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kandidieren</trd>,
    <trd>sich zur<ind>Wahl</ind> stellen</trd>,
    <trd>sich <ind>bewerben</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>apply for</trd>,
    <trd>aspire to (an office)</trd>,
    <trd>seek<klr tip="ind"> (to obtain honor or office)</klr></trd>,
    <trd>stand as a <ind>candidate</ind> for</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>candidat</ind></trd>,
    <trd>postuler<klr tip="ind"> (&ecirc;tre candidat &agrave;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jel&ouml;lt <klr tip="ind">(vmire)</klr></trd>,
    <trd>p&aacute;ly&aacute;zik <klr tip="ind">(vmire)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zich <ind>kandidaat</ind> stellen</trd>,
    <trd>solliciteren</trd>
  </trdgrp>
  <trd lng="pt">candidatar-se</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_k;&c_a;&c_n;&c_d;&c_i;&c_d;&c_a;&c_t;&c_o;&c_m; <klr>(&c_n;&c_a; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_p;&c_r;&c_je;&c_t;&c_je;&c_n;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_n;&c_a; &c_d;&c_o;&c_l;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;)</klr></trd>
  </trdgrp>
</drv>
</art>
