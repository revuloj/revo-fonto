<art mrk="$Id$">
<kap>
<rad>ramn</rad>/o
</kap>
<drv mrk="ramn.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="ramn.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Rhamnus</trd>).
    </dif>
  </snc>
  <trd lng="de">Kreuzdorn</trd>
</drv>
<drv mrk="ramn.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Rhamnaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Kreuzdorngew&auml;chse</trd>
</drv>
</art>
