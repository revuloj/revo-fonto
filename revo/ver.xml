<drv mrk="ver.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="ver.0ismo.BELA">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Italdevena
      <ref tip="super" cel="real.0ismo.BELA">realisma</ref>
      skolo, precipe muzika kaj beletra,
      populara fine de la 19a kaj komence de la 20a
      jarcentoj:
      <ekz>
        <nom>Magda</nom> Olivero <klr>[&#8230;]</klr> fari&gcirc;is la plej granda
        kantisto-aktorino en <tld/>ismo de niaj tempoj
        <fnt>
          <aut>E. Spedicato</aut>,
          <vrk><url ref="http://www.unibg.it/dati/persone/636/2237.pdf">
          Oma&gcirc;e al du gloroj de la itala muziko...</url></vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">verizmus</trd>
  <trd lng="en">verismo</trd>
  <trd lng="fr">v&eacute;risme</trd>
