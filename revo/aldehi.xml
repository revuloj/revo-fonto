<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aldehid</rad>/o
</kap>
<drv mrk="aldehi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Organika kompona&jcirc;o, aroma, likva, senkolora, kiu rezultas el
      alkoholo, el kiu oni eltiris du atomojn de hidrogeno per oksidigo.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_d;&c_e;&c_g;&c_ib;&c_d;</trd>
  <trd lng="bg">&c_a;&c_l;&c_d;&c_je;&c_h;&c_i;&c_d;</trd>
  <trd lng="ca">aldehid</trd>
  <trd lng="en">aldehyde</trd>
  <trd lng="es">aldeh&iacute;do</trd>
  <trd lng="de">Aldehyd</trd>
  <trd lng="fr">ald&eacute;hyde</trd>
  <trd lng="hu">aldehid</trd>
  <trd lng="nl">aldehyde</trd>
  <trd lng="pt">alde&iacute;do</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_d;&c_je;&c_g;&c_i;&c_d;</trd>
  <trd lng="sk">aldehyd</trd>
  <trd lng="sv">aldehyd</trd>
</drv>
</art>
