<drv mrk="aldehi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Organika <ref cel="kombin.0ajxo.KEM">kombina&jcirc;o</ref>,
      aroma, likva, senkolora, kiu rezultas el
      alkoholo, el kiu oni eltiris du atomojn de hidrogeno per oksidigo:
      <ekz>
        la fabriko <klr>[&#x2026;]</klr> produktis aceto<tld/>on kiel industrian materialon
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2013/011021.php">Minamata, alia tragedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        volatila oleo el cinamata <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=752">Cinamo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
