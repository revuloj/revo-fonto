<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dominik1.xml,v 1.7 2018/02/03 17:10:14 revo Exp $">
<kap>
  <rad>dominik</rad>/o
</kap>

<drv mrk="dominik1.0o">
  <kap>
    <tld lit="D"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="dominik1.0o.GEOG">
    <mlg kod="ISO 3166">DM</mlg>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">Lando</ref> 
      en la Kariba Maro en la samnoma insulo kun &ccirc;efurbo Roseo,
      teritorio de Komunumo <tld lit="D"/>o:
      <ekz>
        kiel insula regno <tld lit="D"/>o ne havas landlimojn, &gcirc;in &ccirc;irka&ubreve;as la Kariba Maro, <tld lit="D"/>o havas areon de 754 km&quadrat;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=854">Dominiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>Dominika</trd>,
      <trd>Wsp&oacute;lnota <ind>Dominiki</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="dominik1.0o.insulo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">Insulo</ref>
      en la <ref tip="malprt" cel="mar.0o">Kariba Maro</ref>:
      <ekz>
        famaj insuloj Alventaj interalie estas <ref tip="vid" cel="barbad.B0o">Barbado</ref>,
        <tld lit="D"/>o, <ref tip="vid" cel="grenad1.0o.insulo">Grenado</ref>, Martiniko, 
        Sankta Lucio kaj Sankta Vincento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=234">Alventaj Insuloj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">Dominika</trd>
  </snc>
    <rim>
      Ekzistas anka&ubreve; anta&ubreve;nomo 
      <ref tip="hom" cel="dominik.0o">Dominiko</ref>.
    </rim>
  <trd lng="bg">&c_D;&c_o;&c_m;&c_i;&c_n;&c_i;&c_k;&c_a;</trd>
  <trd lng="de">Dominica</trd>
  <trd lng="en">Dominica</trd>
  <trd lng="fr">Dominique</trd>
</drv>
<trd lng="id">Dominika</trd>
<trd lng="pt">Dominica</trd>

</art>

