<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lakmus</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="lakmus.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Blua farbo, ekstraktata el likenoj a&ubreve; krotono, kiun acidoj
      ru&gcirc;igas kaj alkaloj ree bluigas.
    </dif>
  </snc>
  <trd lng="en">litmus</trd>
  <trd lng="nl">lakmoes</trd>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_m;&c_u;&c_s;</trd>
</drv>

<drv mrk="lakmus.0papero">
  <kap>
    <tld/>(a paper)o
  </kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Papero &scirc;mirita per <tld/>o, uzata por rekoni bazajn kaj acidajn
      reakciojn.
    </dif>
  </snc>
  <trd lng="en">litmus paper</trd>
  <trd lng="nl">lakmoespapier</trd>
  <trd lng="ru">&c_l;&c_a;&c_k;&c_m;&c_u;&c_s;&c_o;&c_v;&c_a;&c_ja; &c_b;&c_u;&c_m;&c_a;&c_g;&c_a;</trd>
</drv>
</art>
