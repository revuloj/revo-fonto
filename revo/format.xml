<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>format</rad>/o
</kap>
<drv mrk="format.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TIP</uzo>
  <snc>
    <dif>
      Dimensio de libro, la&ubreve; alteco k lar&gcirc;eco:
      <ekz>
        duonfolia <tld/>o, kvaronfolia, okonfolia
        <klr>(kies tipa paperfoliego estas faldita la&ubreve; du, kvar a&ubreve;
        ok folioj)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        verkisto de granda <tld/>o <klr>(eminenta)</klr>.
      </ekz>  
    </dif>
  </snc>
  <adm>
  Ekzistas alifakaj sencoj (ekz-e KOMP). <aut>MB</aut>
  </adm>
  <trdgrp lng="de">
     <trd>Format</trd>,
     <trd>Gr&ouml;&szlig;e</trd>,
     <trd>Abmessungen</trd></trdgrp>
  <trd lng="fr">format</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_r;&c_m;&c_a;&c_t;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_je;&c_r;</trd>
  </trdgrp>
</drv>
</art>

