<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gxenda.xml,v 1.23 2022/03/19 19:52:45 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>&gcirc;endarm</rad>/o
</kap>
<drv mrk="gxenda.0o">
  <kap><tld/>o</kap>
  <snc mrk="gxenda.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Armeano kun polica funkcio;
      <ref tip="sin" cel="karabe.0isto.MIL">karabenisto</ref>:
      <ekz>
        jam lin kaptas la <tld/>oj
        <fnt><bib>VivZam</bib>
        <lok>&ccirc;apitro 2a, p. 27a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_n;&c_d;&c_a;&c_r;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Landj&auml;ger</trd>,
    <trd>Landpolizist</trd>
  </trdgrp>
  <trd lng="ca">gendarme</trd>
  <trdgrp lng="en">
    <trd>gendarme</trd>,
    <trd>mounted patrol officer</trd>,
    <trd>constable</trd>,
    <trd>mountee</trd>
  </trdgrp>
  <trd lng="es">gendarme</trd>
  <trd lng="fa">&f_zh;&a_A;&a_n;&a_d;&a_A;&a_r;&a_m;</trd>
  <trd lng="fr">gendarme</trd>
  <trd lng="hu">csend&odblac;r</trd>
  <trdgrp lng="it">
    <trd>gendarme</trd>,
    <trd>carabiniere<klr> (come militare di PS)</klr></trd>
  </trdgrp>
  <trd lng="nl">gendarme</trd>
  <trd lng="pl">&zdot;andarm</trd>
  <trd lng="pt">gendarme</trd>
  <trd lng="ru">&c_zh;&c_a;&c_n;&c_d;&c_a;&c_r;&c_m;</trd>
  <trd lng="uk">&#x436;&#x430;&#x43D;&#x434;&#x430;&#x440;&#x43C;</trd>
</drv>

<drv mrk="gxenda.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="gxenda.0aro.MIL">
    <dif>
      Parto de <ref tip="malprt" cel="arme.0o.sxtata">armeo</ref>
      kun polica funkcio, prizorganta publikan ordon:
      <ekz>
        tiuj dungsoldatoj &ccirc;efe enkadrigis la katangan
        <tld/>aron, kiu fari&gcirc;is vera armeo anstata&ubreve;
        resti nura polico
        <fnt>
          <aut>C. Gacond</aut>
          <vrk><url ref="http://www.esperanto-gacond.ch/Claude_Gacond_Radioprelego-0036.pdf">
          36-a radioprelego</url></vrk>
          <lok>1963-08-07 kaj 10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_n;&c_d;&c_a;&c_r;&c_m;&c_e;&c_r;&c_y;&c_ja;</trd>
  <trd lng="fr">gendarmerie<klr> (corps)</klr></trd>
  <trd lng="uk">&#x436;&#x430;&#x43D;&#x434;&#x430;&#x440;&#x43C;&#x435;&#x440;&#x456;&#x44F;</trd>
</drv>
</art>
