<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>insul</rad>/o
</kap>
<drv mrk="insul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="insul.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Terparto, &ccirc;iuflanke &ccirc;irka&ubreve;ata de akvo.
      <ekz>
        ni devas transflugi la grandan maron, kaj sur nia vojo
        trovi&gcirc;as neniu <tld/>o, sur kiu ni povus pasigi la nokton
        <fnt><bib>Fab1</bib>, <lok>sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        akurate kombitaj <tld/>etoj <klr>(izolitaj tufoj)</klr> de la
        ru&gcirc;aj haroj sur lia brila kalvo atestis sufi&ccirc;an
        atenton al sia propra persono
        <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 3a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_s;&c_t;&c_r;&c_a;&c_w;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_p;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Insel</trd>
  <trd lng="en">island</trd>
  <trd lng="es">isla</trd>
  <trdgrp lng="fr">
    <trd>ile</trd>,
    <trd>&icirc;le</trd>
  </trdgrp>
  <trd lng="id">pulau</trd>
  <trd lng="nl">eiland</trd>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_r;&c_o;&c_v;</trd>
  <trd lng="vo">nisul</trd>
</drv>

<drv mrk="insul.0a">
  <kap><tld/>a</kap>
  <snc mrk="insul.0a.GEOG">
    <dif>
      Rilata al <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_a;&c_w;&c_n;&c_y;</trd>
  <trd lng="fr">insulaire<klr tip="ind"> (adj.)</klr></trd>
</drv>

<drv mrk="insul.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="insul.0aro.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Grupo da <tld/>oj.
    </dif>
    <ref tip="sin" cel="arkipe.0o.insularo">arkipelago</ref>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_p;&c_e;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="de">
    <trd>Inselgruppe</trd>,
    <trd>Archipel</trd>
  </trdgrp>
  <trd lng="en">archipelago</trd>
  <trd lng="es">archipi&eacute;lago</trd>
  <trdgrp lng="fr">
    <trd>archipel</trd>,
    <trd>chapelet d'&icirc;les</trd>
  </trdgrp>
  <trd lng="nl">archipel</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_p;&c_je;&c_l;&c_a;&c_g;</trd>
</drv>

<drv mrk="insul.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="insul.0ano.KOMUNE">
    <dif>
      Lo&gcirc;anto de <tld/>o:
      <ekz>
        ceteraj <tld/>anoj, kiuj havis malsanojn, venis kaj estis sanigitaj
        <fnt><bib>NT</bib>, <lok>&Ago; 28:9</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Inselbewohner</trd>
  <trd lng="en">islander</trd>
  <trd lng="es">isle&ntilde;o</trd>
  <trdgrp lng="fr">
    <trd>insulaire<klr tip="ind"> (subst.)</klr></trd>,
    <trd>&icirc;lien<klr tip="ind"> (subst.)</klr></trd>,
    <trd>ilien<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">eilandbewoner</trd>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_r;&c_o;&c_v;&c_i;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
  <trd lng="vo">nisulel</trd>
</drv>

<drv mrk="insul.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="insul.0eto.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Malvasta <tld/>o:
      <ekz>
        <tld/>eto, nomata Ka&ubreve;da
        <fnt><bib>NT</bib>, <lok>&Ago; 27:16</lok></fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ilot<klr tip="ind"> (petite &icirc;le)</klr></trd>,
    <trd>&icirc;lot<klr tip="ind"> (petite &icirc;le)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="insul.duon0o">
  <kap>duon<tld/>o</kap>
  <snc mrk="insul.duon0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Terparto &ccirc;irka&ubreve;ata de akvo escepte de unu flanko,
      per kiu &gcirc;i kuni&gcirc;as kun la kontinento:
      <ekz>
        la itala duon<tld/>o;
      </ekz>
      <ekz>
        la Jutlanda duon<tld/>o
        <fnt><bib>Fab4</bib>, <lok>filino de la mar&ccirc;a re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_v;&c_o;&c_s;&c_t;&c_r;&c_a;&c_w;</trd>
  <trd lng="de">Halbinsel</trd>
  <trd lng="en">peninsula</trd>
  <trd lng="es">pen&iacute;nsula</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;ninsule</trd>
  </trdgrp>
  <trd lng="nl">schiereiland</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_o;&c_s;&c_t;&c_r;&c_o;&c_v;</trd>
</drv>
</art>
