<drv mrk="insul.Pask0o">
  <kap>
    Pask<tld/>o
  </kap>
  <snc mrk="insul.Pask0o.insulo">
    <dif>
      Izolita <ref tip="lst" lst="voko:insuloj" cel="insul.0o.GEOG">insulo</ref> en
      <ref tip="malprt" cel="pacifik.P0o">Pacifiko</ref>,
      &ccirc;. 3500&#8239;km okcidente de la &ccirc;ilia bordo:
      <ekz>
        <nom>Egbert Richter</nom> <klr>[&#8230;]</klr> prezentis en la hamburga universitato 214-pa&gcirc;an
        doktoran diserta&jcirc;on titolitan La skribtabuloj de Pask<tld/>o
        <fnt>
          <aut>Rudiger Hauger</aut>
          <vrk><url ref="&Monato;/2001/007315.php">De&ccirc;ifrado de la paskinsula skribo</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p. 23</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_V;&c_o;&c_s;&c_t;&c_r;&c_a;&c_w; &c_P;&c_a;&c_s;&c_h;&c_ib;</trd>,
    <trd>&c_R;&c_a;&c_p;&c_a;&c_n;&c_u;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">Osterinsel</trd>
  <trd lng="en">Easter Island</trd>
  <trd lng="fr">&Icirc;le de P&acirc;ques</trd>
  <trd lng="ja">&#12452;&#12540;&#12473;&#12479;&#12540;&#23798;
  <pr>&#12452;&#12540;&#12473;&#12479;&#12540;&#12392;&#12358;</pr></trd>
  <trd lng="nl">Paaseiland</trd>
