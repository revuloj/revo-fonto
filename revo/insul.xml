<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>insul</rad>/o
</kap>
<drv mrk="insul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Terparto, &ccirc;iuflanke &ccirc;irka&ubreve;ata de akvo. 
    </dif>
  </snc>
  <trd lng="de">Insel</trd>
  <trd lng="en">island</trd>
  <trd lng="nl">eiland</trd>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_r;&c_o;&c_v;</trd>
</drv>
<drv mrk="insul.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Grupo da <tld/>oj; <ref tip="sin" cel="arkipe.0o">arkipelago</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Inselgruppe</trd>,
    <trd>Archipel</trd>
  </trdgrp>
  <trd lng="nl">archipel</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_p;&c_je;&c_l;&c_a;&c_g;</trd>
</drv>
<drv mrk="insul.0ano">
  <kap><tld/>ano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de <tld/>o. 
    </dif>
  </snc>
  <trd lng="de">Inselbewohner</trd>
  <trd lng="nl">eilandbewoner</trd>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_r;&c_o;&c_v;&c_i;&c_t;&c_ja;&c_n;&c_i;&c_n;</trd>
</drv>
<drv mrk="insul.duon0o">
  <kap>duon<tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Terparto &ccirc;irka&ubreve;ata de akvo escepte de unu flanko, per kiu &gcirc;i
      kuni&gcirc;as kun la kontinento:
      <ekz>la itala duon<tld/>o.</ekz>
    </dif>
  </snc>
  <trd lng="de">Halbinsel</trd>
  <trd lng="nl">schiereiland</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_o;&c_s;&c_t;&c_r;&c_o;&c_v;</trd>
</drv>
</art>
