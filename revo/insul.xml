<drv mrk="insul.I0odePrincoEduardo">
  <kap><tld lit="I"/>o de Princo Eduardo</kap>
  <snc mrk="insul.I0odePrincoEduardo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Provinco de
      <ref tip="malprt" cel="kanad.K0o.GEOG">Kanado</ref>
      en &gcirc;ia orienta parto:
      <ekz>
        <klr>...</klr> Akadianoj lo&gcirc;antaj <klr>...</klr> en partoj
        de
        la Kanadaj &ccirc;e-Atlantikaj provincoj Nov-Brunsviko,
        Nova-Skotio,
        <tld lit="I"/>o de Princo Eduardo
        <fnt>
          <aut>C. Richard</aut>,
          <vrk>PIV2: Akadio kaj franc-lingvaj Kanadanoj</vrk>,
          <lok><url ref="&SCE;">
          soc.culture.esperanto</url>, 1999-11-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_o;&c_s;&c_t;&c_r;&c_a;&c_w; &c_P;&c_r;&c_y;&c_n;&c_c;&c_a; &c_E;&c_d;&c_v;&c_a;&c_r;&c_d;&c_a;</trd>
  <trd lng="en">Prince Edward Island</trd>
  <trd lng="fr">Ile-du-Prince-&Eacute;douard</trd>
  <trd lng="nl">Prins Edwardeiland</trd>
