<art mrk="$Id: komedon.xml,v 1.9 2023/12/03 09:58:53 revo Exp $">
<kap>
  <rad>komedon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="komedon.0o">
  <kap><tld/>o</kap>
  <snc mrk="komedon.0o.KOS">
    <dif>
      &Scirc;topi&gcirc;o de
      <ref tip="vid" cel="por1.0o.ANA">poro</ref>
      pro sebumo ka&ubreve;zanta pustuleton:
      <ekz>
        Aureliano Jos&eacute; frotas klingon kontra&ubreve; ledan bendon por unuafoje 
        razi sin; sangas liaj <tld/>oj, vundi&gcirc;as lia supra lipo
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_g;&c_o;&c_r; <klr tip="amb">(&c_n;&c_a; &c_s;&c_k;&c_u;&c_r;&c_y;)</klr></trd>
  <trdgrp lng="cs">
    <trd>komedo</trd>,
    <trd>komedon</trd>,
    <trd>uher</trd>
  </trdgrp>
  <trd lng="de">Mitesser</trd>
  <trdgrp lng="fr">
    <trd>com&eacute;don</trd>,
    <trd>point noir</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>w&aogonek;gier</trd>,
    <trd>zask&oacute;rnik</trd>
  </trdgrp>  
  <trd lng="ro">co&#x219;</trd> 
  <trdgrp lng="sk">
    <trd>komedo</trd>,
    <trd>uhor</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_u;&c_g;&#x43E;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x9ED1;&#x5934; <pr>h&#x113;it&#xF3;u</pr></trd>,
    <trd>&#x9ED1;&#x5934;&#x75C5; <pr>h&#x113;it&#xF3;ub&#xEC;ng</pr></trd>,
    <trd>&#x9ED1;&#x5934;&#x9762;&#x6CE1; <pr>h&#x113;it&#xF3;umi&#xE0;np&#xE0;o</pr></trd>,
    <trd>&#x9ED1;&#x5934;&#x9E1F; <pr>h&#x113;it&#xF3;uni&#x1CE;o</pr></trd>
  </trdgrp>
</drv>
</art>

