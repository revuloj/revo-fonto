<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fortres</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fortres.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="fortik.0ajxo">Fortika&jcirc;o</ref> kun propra
      garnizono kaj necesa&jcirc;oj, por la defendo de urbo a&ubreve; lo&gcirc;ata areo:
      <ekz>
        oni povis lin vidi rajdanta al la <tld lit="F"/>o Antonia,
        kiu situis norde, tuj apud la Granda Templo
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 26a, p. 269a</lok></fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_r;&c_t;&c_e;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">fortalesa</trd>
  <trd lng="en">fortress</trd>
  <trd lng="es">fortaleza</trd>
  <trd lng="fr">forteresse</trd>
  <trdgrp lng="hu">
    <trd>er&odblac;d&iacute;tm&eacute;ny</trd>,
    <trd>er&odblac;d</trd>
  </trdgrp>
</drv>
</art>

