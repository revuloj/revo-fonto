<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: obskur.xml,v 1.22 2024/01/30 16:09:13 revo Exp $">
<kap>
  <rad>obskur</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="obskur.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="obskur.0a.malluma">
    <dif>
      Tre <ref tip="super" cel="lum.mal0a">malluma</ref>:
      <ekz>
        laboristoj, mortpafitaj el sonorilejo de la <tld/>a katedralo
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        neniumi &ccirc;e anguloj de <tld/>o kaj haladzo
        <fnt>
          <aut>Grigori Arosev</aut>
            <vrk><url ref="&Monato;/2000/007198.php">Poezio sed ne poemoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 28</lok>
          </fnt>;
      </ekz>
      <ekz>
        gajaj kvikoj sonis el <tld/>a angulo
        <fnt>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2004/008357.html">La unua nokto</url></vrk>
           <bib>Monato</bib>
          <lok>2004/11, p. 28</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tenebr.0o">tenebro</ref>
    <trd lng="be">&c_c;&c_jo;&c_m;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>dunkel</trd>,
      <trd>d&uuml;ster</trd>,
      <trd>finster</trd>,
    </trdgrp>
    <trd lng="en">dark</trd>
    <trdgrp lng="fa">
      <trd>&a_t;&f_y;&a_r;&a_h;<klr tip="ind"> (&a_S;&a_f;&a_t;)</klr></trd>,
      <trd>&a_t;&a_A;&a_r;&f_y;&f_k;</trd>
    </trdgrp>
    <trd lng="fr">t&eacute;n&eacute;breux</trd>
    <trd lng="hu">s&ouml;t&eacute;t</trd>
    <trdgrp lng="nl">
      <trd>donker</trd>,
      <trd>duister</trd>
    </trdgrp>
    <trd lng="pl">ciemny</trd>
    <trd lng="ru">&c_t;&c_jo;&c_m;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="obskur.0a.malklara">
    <dif>
      <ref tip="dif" cel="klar.mal0a">Malklara</ref>,
      nekomprenebla:
      <ekz>
        lumigu la <tld/>on de mia menso
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 45a, p.
        641a</lok></fnt>.
      </ekz>
      <ekz>
        ho triste povra, dura sort' de la moroz-<tld/>a vort'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Lamentado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj disputoj de mezepokaj teologoj pri la sankta triunuo a&ubreve; la
        transsubstancigo
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2003/007921.html">Ho, mistera pi!</url></vrk>
          <bib>Monato</bib>
          <lok>2003/06, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a stilo
        <fnt>
          <aut>Aimo Rantanen</aut>
          <vrk><url ref="&Monato;/2004/008427.html">Redaktoprincipoj</url></vrk>
          <bib>Monato</bib>
          <lok>2004/10, p. 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la koncerna entreprena regularo prezenti&gcirc;is iom <tld/>a
        <fnt>
          <aut>Filippo</aut>
          <vrk><url ref="&Monato;/2006/008898.php">Moderna po&scirc;to</url></vrk>
           <bib>Monato</bib>
          <lok>2006/04, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_ja;&c_ja;&c_s;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>unklar</trd>,
      <trd>unverst&auml;ndlich</trd>,
      <trd>verworren</trd>,
    </trdgrp>
    <trd lng="en">unclear</trd>
    <trdgrp lng="fa">
      <trd>&a_m;&a_b;&a_h;&a_m;</trd>,
      <trd>&f_g;&a_n;&f_g;</trd>,
      <trd>&a_n;&a_A;&a_m;&a_f;&a_h;&a_w;&a_m;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tiszt&aacute;zatlan</trd>,
      <trd>hom&aacute;lyos</trd>,
      <trd>zavaros</trd>,
      <trd>obsk&uacute;rus</trd>
    </trdgrp>
    <trd lng="nl">onduidelijk</trd>
    <trd lng="pl">niejasny</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_jo;&c_m;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_ja;&c_s;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="obskur.0a.nefama">
    <dif>
      Mal<ref tip="ant" cel="fam.0a">fama</ref>:
      <ekz>
        renoma a&ubreve; <tld/>a leganto, vi sendube atendis <klr>[&#x2026;]</klr> la
        aperon de &ccirc;i prefaco
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, anta&ubreve;parolo al la
        leganto, p. 393a</lok></fnt>;
      </ekz>
      <ekz>
        nur <tld/>aj negocistoj okupi&gcirc;as pri ili
       <fnt>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2001/007334.php">Lernout kaj Hauspie: revo fini&gcirc;is</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p. 8</lok>
          </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_l;&c_a;&c_v;&c_ja;&c_d;&c_o;&c_m;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_ja;&c_d;&c_o;&c_m;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>zwielichtig</trd>,
      <trd>zweifelhaft</trd>
    </trdgrp>
    <trd lng="en">unknown</trd>
    <trdgrp lng="fa">
      <trd>&a_n;&a_A;&a_sh;&a_n;&a_A;&a_s;</trd>,
      <trd>&f_g;&a_m;&a_n;&a_A;&a_m;</trd>
    </trdgrp>
    <trd lng="hu">ismeretlen</trd>
    <trdgrp lng="nl">
      <trd>onbekend</trd>,
      <trd>niet <ind>beroemd</ind></trd>
    </trdgrp>
    <trd lng="pl">nieznany</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_a;&c_l;&c_o;&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>obskurn&iacute;</trd>,
    <trd>pochybn&yacute;</trd>,
    <trd>podez&rcaron;el&yacute;</trd>,
    <trd>temn&yacute;</trd>
  </trdgrp>
  <trd lng="de">obskur</trd>
  <trd lng="en">obscure</trd>
  <trd lng="es">oscuro</trd>
  <trd lng="fr">obscur</trd>
  <trd lng="nl">obscuur</trd>
  <trd lng="pt">obscuro</trd>
  <trdgrp lng="sk">
    <trd>obsk&uacute;rny</trd>,
    <trd>temn&yacute;; pochybnej povesti</trd>,
    <trd>tmav&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_je;&#x43C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&#x44F;&c_s;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_v;&c_ib;&c_d;&#x43E;&#x43C;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_z;&#x43D;&c_a;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x66A7; <pr>&#xE0;i</pr></trd>,
    <trd>&#x66D6; <pr>&#xE0;i</pr></trd>,
    <trd>&#x5965; <pr>&#xE0;o</pr></trd>,
    <trd>&#x5967; <pr>&#xE0;o</pr></trd>,
    <trd>&#x609C; <pr>ch&#x11B;ng</pr></trd>,
    <trd>&#x6627;&#x6C92; <pr>m&#xE8;im&#xF2;</pr></trd>,
    <trd>&#x6627;&#x6CA1; <pr>m&#xE8;im&#xF2;</pr></trd>,
    <trd>&#x53C6; <pr>&#xE0;i</pr></trd>,
    <trd>&#x9749; <pr>&#xE0;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="obskur.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="obskur.0ismo.KOMUNE">
    <dif>
      Doktrino kontra&ubreve;a al la klerigado kaj mensa liberigo;
      <ref tip="sin" cel="obskurantism.0o.KOMUNE">obskurantismo</ref>:
      <ekz>
        la revolucio de 1908, post tridek jaroj da absoluta kaj sisteme 
        <tld/>isma regado de la sultano <nom>Abd-al-Hamido</nom> la II &scirc;uldi&gcirc;is
        al agado de junaj generacioj de armeaj oficiroj
        <fnt>
          <aut>J. R&eacute;gulo P&eacute;rez</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr12/enciklopediistoj.html">
          Enciklopediistoj en Kanariaj Insuloj</url></vrk>,
          <lok>Nica Literatura Revuo, 1955-11 &gcirc;is 12 (1/2), p.
          47a-51a</lok>
        </fnt>;
      </ekz>
      <ekz>
        certe mi malaprobus tiun &ccirc;i <tld/>ismon, se &gcirc;i ne apartenus al mia dumiljara
        kulturo
        <fnt>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2008/009545.php">Kapskuo</url></vrk>
          <bib>Monato</bib>
          <lok>2008/08, p. 10</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="kler.0ismo">klerismo</ref>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_s;&c_k;&c_u;&c_r;&c_a;&c_n;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="de">Obskurantismus</trd>
  <trd lng="fa">&a_t;&a_A;&a_r;&f_y;&f_k;&#x200C;&a_A;&a_n;&a_d;&f_y;&a_sh;&f_y;</trd>
  <trd lng="fr">obscurantisme</trd>
  <trd lng="nl">obscurantisme</trd>
  <trd lng="pt">obscurantismo</trd>
</drv>
</art>

