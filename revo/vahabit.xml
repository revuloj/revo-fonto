<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vahabit.xml,v 1.4 2019/09/30 06:10:17 revo Exp $">
<kap>
  <rad>vahabit</rad>/o
</kap>
<drv mrk="vahabit.0o">
  <kap><tld/>o</kap>
  <snc mrk="vahabit.0o.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      <ref tip="dif" cel="vahabism.0ano.ISL">Vahabismano</ref>:
      <ekz>
        <tld/>oj akceptas la Koranon kaj la Haditojn, kaj
        anka&ubreve; kelkajn komentajn verkojn pri ili
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Vahabismo">
          Vahabismo</url>, 2011-05-17</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekis kvereloj kun <nom>Aslan Mas&hcirc;adov</nom> kaj <nom>&Scirc;amil Basajev</nom>, kiujn li akuzis kiel
        pagitojn de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19243"
              >A&hcirc;mat Kadirov</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="salafist.0o">salafisto</ref>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_h;&c_a;&c_b;&c_ib;&c_t;</trd>
  <trd lng="ca">wahhabita</trd>
  <trd lng="de">Wahhabit</trd>
  <trd lng="fr">wahhabite<klr> (subst.)</klr></trd>
</drv>
</art>

