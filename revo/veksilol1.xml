<kap>
    <rad>veksilologi</rad>/o  <fnt><bib>PIV3</bib></fnt>
</kap>
<drv mrk="veksilol1.0o">
  <kap><tld/>o</kap>
  <snc mrk="veksilol1.0o.HER">
    <uzo tip="fak">HER</uzo>
    <dif>
      Flagoscienco:
      <ekz>
        se oni volas esti iom pedanta, <tld/>o estas pli studado ol scienco
        <fnt>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2014/011096.php">Skandinava veksilologio</url></vrk>
          <bib>Monato</bib>
          <lok>2014/03, p. 19-20</lok>
        </fnt>;
      </ekz>
      <ekz>
      <tld/>o estas scienco pri flagoj kaj standardoj
      <fnt>
        <aut>Ji&rcaron;&iacute; Patera</aut>
        <vrk><url ref="&Monato;/2002/007933.html"
            >Tridek jaroj de la Veksilologia Klubo en Prago</url>
        </vrk>
        <bib>Monato</bib>
        <lok>2002/10, p. 23</lok>
      </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_k;&c_s;&c_ib;&c_l;&c_ja;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Flaggenkunde</trd>,
    <trd>Vexillologie</trd>
  </trdgrp>
  <trd lng="en">vexillology</trd>
  <trdgrp lng="nl">
    <trd>vexillologie</trd>,
    <trd>vlaggenkunde</trd>
  </trdgrp>
</drv>
