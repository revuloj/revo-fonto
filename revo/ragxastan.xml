<vortaro>
<art mrk="$Id: ragxastan.xml,v 1.4 2022/07/31 21:28:23 revo Exp $">
<kap>
  <rad>Ra&gcirc;astan</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="ragxastan.0o">
  <kap><tld/>o</kap>
  <snc mrk="ragxastan.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Gubernio en la okcidenta parto de
      <ref tip="malprt" cel="hind.H0ujo.POL">Hindujo</ref>:
      <ekz>
        Keralao kaj <tld/>o havas sufi&ccirc;e 
        malsamajn <klr>...</klr> kulturojn
        <fnt>
          <aut>-, [trad. C. Bertin]</aut>,
          <vrk><url
          ref="http://www.bertin.biz/index.php?titre=Propositions%20du%20Collectif%20Roosevelt.htm">
          15 proponoj de Collectif Roosevelt</url></vrk>,
          <lok>[vidita en 2014]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    <em>Ra&jcirc;astano</em> en
    &leftquot;Landoj kaj lingvoj de la 
    mondo&rightquot;<fnt><bib>LanLin</bib></fnt>. 
  </rim>
  <trd lng="be">&c_R;&c_a;&c_d;&c_zh;&c_a;&c_s;&c_t;&c_h;&c_a;&c_n;</trd>
  <trd lng="fr">Rajasthan</trd>
  <trd lng="pl">Rad&zdot;astan</trd>
  <trd lng="ro">Rajasthan</trd>
  <trd lng="uk">&c_R;&c_a;&c_d;&c_zh;&c_a;&c_s;&c_t;&c_a;&#x43D; (&c_sh;&c_t;&c_a;&c_t;)</trd>
</drv>
