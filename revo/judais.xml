<drv mrk="judais.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <uzo tip="stl">EVI</uzo>
  <snc>
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="dif" cel="jud.0ismo">Judismo</ref>:
      <ekz>
        lingvoj helpas homojn trovi identecon, simbolas apartenon al etno, popolo, religio (islamo: araba, <tld/>o: hebrea ktp)
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2013/010677.php">Lingva konfuzo kaj Konfuceo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ju;&c_d;&c_a;&c_ib;&c_z;&c_m;</trd>
  <trd lng="br">yuzevegezh</trd>
  <trd lng="ca">judaisme</trd>
  <trdgrp lng="cs">
    <trd>judaizmus</trd>,
    <trd>mosaizmus</trd>,
    <trd>semitizmus</trd>,
    <trd>&zcaron;idovsk&eacute; n&aacute;bo&zcaron;enstv&iacute;</trd>,
    <trd>&zcaron;idovstv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Judaismus</trd>,
    <trd>j&uuml;dische Religion</trd>
  </trdgrp>
  <trd lng="el">&jota;&omikron;&ypsilon;&delta;&alfa;&jota_trema;&sigma;&my;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">Judaism</trd>
  <trd lng="es">juda&iacute;smo</trd>
  <trd lng="fr">juda&iuml;sme</trd>
  <trd lng="he">&yod;&he;&dalet;&vav;&tav;</trd>
  <trd lng="hu">judaizmus</trd>
  <trdgrp lng="it">
    <trd>giudaismo</trd>,
    <trd>ebraismo</trd>
  </trdgrp>
  <trd lng="nl">jodendom</trd>
  <trd lng="pt">juda&iacute;smo</trd>
  <trd lng="ru">&c_i;&c_u;&c_d;&c_a;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">n&aacute;bo&zcaron;enstvo a n&aacute;b. osveta &zcaron;idov</trd>
  <trd lng="sv">judendom</trd>
  <trd lng="sw">Uyahudi</trd>
  <trdgrp lng="uk">
    <trd>&#x456;&#x443;&#x434;&#x430;&#x457;&#x437;&#x43C;</trd>,
    <trd>&#x456;&#x443;&#x434;&#x435;&#x439;&#x441;&#x442;&#x432;&#x43E;</trd>
  </trdgrp>
  <trd lng="yi">&yod;&yod;&#x5B4;&dalet;&yod;&shin;&qof;&#x5F2;&#x5B7;&tet;</trd>
  <trd lng="zh">&#x72B9;&#x592A;&#x6559; <pr>Y&#xF3;ut&#xE0;iji&#xE0;o</pr></trd>
