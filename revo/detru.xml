<vortaro>
<art mrk="$Id: detru.xml,v 1.46 2026/02/04 20:51:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>detru</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="detru.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Perforte <ref tip="super" cel="romp.dis0i">disrompi</ref>, <ref tip="super"
      cel="pec.dis0igi">dispecigi</ref>;
      <ref tip="sin" cel="ruin.0igi.detrui">ruinigi</ref>,
      <ref tip="sin" cel="pere.0igi.detrui">pereigi</ref>,
      <ref tip="sin" cel="nul0.0igi.KOMUNE">nuligi</ref>,
      <ref tip="sin" cel="neni.0igi.detrui">neniigi</ref>:
      <ekz>
        hajlo <tld/>is la rikolton;
      </ekz>
      <ekz>
        tertremo <klr>[&#x2026;]</klr> <tld/>is la tri kvaronojn de Lisbono
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        per via malspriteco vi <tld/>is &ccirc;iujn miajn esperojn
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        pretaj fari ian fripona&jcirc;on <klr>[&#x2026;]</klr> kaj <tld/>i la trankvilon de lia animo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        Nero volas <tld/>i la urbon kaj ekstermi &ccirc;iujn lo&gcirc;antojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        via ekscelenco devas kolekti la armeojn <klr>[&#x2026;]</klr> kaj <tld/>i la ribelantojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita estas via potenco
        <fnt><bib>MT</bib>, <lok>&Jes; 23:14</lok></fnt>;
      </ekz>
      <ekz>
        la a&ubreve;tokracio <tld/>is la lastajn resta&jcirc;ojn de la antikva demokratio
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        paroli nun estus <tld/>i la tutan belan tagon
        <fnt>
          <bib>HsT</bib>
          <lok>Kuni&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj propraj fortoj estis <tld/>itaj, kaj en febro li ku&scirc;is en la lito
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lando pereis kaj estas <ind>brul<tld/>i</ind>ta kiel dezerto
        <fnt><bib>MT</bib>, <lok>&Jer; 9:12</lok></fnt>;
        <trd lng="fr">incendier</trd>
        <trd lng="hu">fel&eacute;get</trd>
        <trdgrp lng="id">
          <trd><baz>bakar</baz>membakar habis</trd>,
          <trd><baz>lalap</baz>melalap habis</trd>
        </trdgrp>
        <trd lng="nl">uitbranden</trd>
      </ekz>
      <ekz>
        la renversitaj cirkonstancoj <klr>[&#x2026;]</klr> prezentas al li la
        plej oportunan okazon <ind>for<tld/>i</ind> siajn personajn
        malamikojn
        <fnt><bib>I&scirc;tar</bib>,<lok>&ccirc;ap. 18</lok></fnt>;
        <trd lng="fr">exterminer</trd>
        <trd lng="hu">elirt</trd>
        <trdgrp lng="id">
          <trd><baz>binasa</baz>membinasakan</trd>,
          <trd><baz>lenyap</baz>melenyapkan</trd>,
          <trd><baz>musnah</baz>memusnahkan</trd>
        </trdgrp>
        <trd lng="nl">uitroeien</trd>
      </ekz>
      <ekz>
        li scias pri iliaj hontindaj metodoj <ind>el<tld/>i</ind> la
        antikvan kulton
        <fnt><bib>I&scirc;tar</bib>,<lok>&ccirc;ap. 18</lok></fnt>.
        <trd lng="fr">&eacute;radiquer</trd>
        <trd lng="hu">kiirt</trd>
        <trdgrp lng="id">
          <trd>basmi</trd>,
          <trd><baz>basmi</baz>membasmi</trd>,
          <trd>berantas</trd>,
          <trd><baz>berantas</baz>memberantas</trd>,
          <trd>tumpas</trd>,
          <trd><baz>tumpas</baz>menumpas</trd>
        </trdgrp>
        <trd lng="nl">ontwortelen</trd>
      </ekz>
      <ekz>
        <klr>[ili]</klr> laboras simple por detrui, <klr>[&#x2026;ili]</klr> per &ccirc;iuj fortoj
        penas <klr>[&#x2026;]</klr> subfosi
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        timi, ke lingvo internacia <tld/>os la lingvojn naciajn, estas tiel same
        ridinde, kiel ekzemple timi, ke la po&scirc;to <klr>[&#x2026;]</klr> minacas neniigi la
        bu&scirc;ajn interparoladojn
        <fnt>
          <bib>EE</bib>
          <lok>II</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita &gcirc;is la fundo de l' fundamento
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abol.0i">aboli</ref>,
      <ref cel="cindr.0igi">cindrigi</ref>,
      <ref cel="bat.dis0i">disbati</ref>,
      <ref cel="ekster1.0i">ekstermi</ref>,
      <ref cel="fal.0igi">faligi</ref>,
      <ref cel="bala.for0i">forbalai</ref>,
      <ref cel="for.0igi">forigi</ref>,
      <ref cel="strek.el0ifor0i">forstreki</ref>,
      <ref cel="visx.for0i">forvi&scirc;i</ref>,
      <ref cel="frakas.0i">frakasi</ref>,
      <ref cel="konsum.0i">konsumi</ref>,
      <ref cel="konstr.mal0i">malkonstrui</ref>,
      <ref cel="mort.0igi">mortigi</ref>,
      <ref cel="polv.0igi">polvigi</ref>,
      <ref cel="renver.0i">renversi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="hun.0o">huno</ref>,
      <ref cel="vandal.0o">vandalo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_ma;&t_drengbu;&t_da;&t_tsheg;&t_pa;&t_tsheg;&t_ba;&t_za;&t_naro;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>bourat</trd>,
    <trd>kazit</trd>,
    <trd>ma&rcaron;it</trd>,
    <trd>ni&ccaron;it</trd>,
    <trd>zbo&rcaron;it</trd>,
    <trd>zni&ccaron;it</trd>,
    <trd>zpusto&scaron;it</trd>
  </trdgrp>
  <trd lng="de">zerst&ouml;ren</trd>
  <trd lng="en">destroy</trd>
  <trd lng="es">destruir</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;molir</trd>,
    <trd>d&eacute;truire</trd>
  </trdgrp>
  <trd lng="he">&lamed;&he;&resh;&vav;&samekh;</trd>
  <trdgrp lng="hu">
    <trd>lerombol</trd>,
    <trd>t&ouml;nkretesz</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>hancur</baz>menghancurkan</trd>,
    <trd><baz>rusak</baz>merusak</trd>,
    <trd><baz>rusak</baz>merusakkan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7834;&#x58CA;&#x3059;&#x308B;  <pr>&#x306F;&#x304B;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x53D6;&#x308A;&#x58CA;&#x3059;  <pr>&#x3068;&#x308A;&#x3053;&#x308F;&#x3059;</pr></trd>,
    <trd>&#x6253;&#x7834;&#x3059;&#x308B;  <pr>&#x3060;&#x306F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x305D;&#x3053;&#x306A;&#x3046;</trd>
  </trdgrp>
  <trd lng="nl">vernietigen</trd>
  <trdgrp lng="pl">
    <trd>niszczy&cacute;</trd>,
    <trd>burzy&cacute;</trd>,
    <trd>demolowa&cacute;</trd>,
    <trd>wali&cacute;</trd>,
    <trd>rozwala&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>destruir</trd>,
    <trd>desfazer</trd>,
    <trd>demolir</trd>,
    <trd>subverter</trd>
  </trdgrp>
  <trd lng="ro">strica</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>b&uacute;ra&tcaron;</trd>,
    <trd>kazi&tcaron;</trd>,
    <trd>ni&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rst&ouml;ra</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_j;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_v;&c_a;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_i;&c_shch;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7838;&#x6BC0; <pr>z&aacute;hu&#x1D0;</pr></trd>,
    <trd>&#x5BB3;&#x6B7B; <pr>h&agrave;is&#x1D0;</pr></trd>,
    <trd>&#x5290; <pr>hu&omacron;</pr></trd>,
    <trd>&#x650B; <pr>l&agrave;</pr></trd>,
    <trd>&#x6BC0;&#x640D; <pr>hu&#x1D0;s&#x1D4;n</pr></trd>,
    <trd>&#x6467;&#x6B8B; <pr>cu&imacron;c&aacute;n</pr></trd>,
    <trd>&#x6BC0;&#x58DE; <pr>hu&#x1D0;hu&agrave;i</pr></trd>,
    <trd>&#x6B8B; <pr>c&aacute;n</pr></trd>,
    <trd>&#x6BC0;&#x58DE; <pr>hu&#x1D0;h&agrave;i</pr></trd>,
    <trd>&#x6BC1; <pr>hu&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="detru.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="D"/>anta:
      <ekz>
        <tld/>a veneno
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a potenco de fajro
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLII</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la subita interveno de la Eternulo, post Liaj <tld/>aj
        argumentoj, <nom>Elihu</nom> pentas pri sia senama ju&gcirc;emo
        <fnt><bib>I&scirc;tar</bib>,<lok>&ccirc;ap. 4</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>destruk&ccaron;n&iacute;</trd>,
    <trd>ni&ccaron;iv&yacute;</trd>
  </trdgrp>
  <trd lng="de">zerst&ouml;rerisch</trd>
  <trd lng="en">destructive</trd>
  <trd lng="es">destructor</trd>
  <trdgrp lng="fr">
    <trd>destructeur<klr tip="ind"> (adj.)</klr></trd>,
    <trd>destructrice<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="he">&he;&resh;&dalet;&nun;&yod;</trd>
  <trdgrp lng="hu">
    <trd>rombol&oacute;</trd>,
    <trd>destrukt&iacute;v</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>destruktif</trd>,
    <trd><baz>rusak</baz>merusak<klr tip="amb"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x7834;&#x58CA;&#x7684;&#x306A;  <pr>&#x306F;&#x304B;&#x3044;&#x3066;&#x304D;&#x306A;</pr></trd>
  <trd lng="nl">vernietigend</trd>
  <trdgrp lng="pl">
    <trd>niszcz&aogonek;cy</trd>,
    <trd>burz&aogonek;cy</trd>,
    <trd>destrukcyjny</trd>,
    <trd>destruktywny</trd>,
    <trd>wyniszczaj&aogonek;cy</trd>,
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">devasta&ccaron;n&yacute;</trd>
  <trd lng="sv">f&ouml;rst&ouml;rande</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_j;&c_n;&c_ib;&c_v;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_i;&c_shch;&c_ib;&c_v;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_g;&c_u;&c_b;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#x7834;&#x574F;&#x6027; <pr>p&ograve;hu&agrave;ix&igrave;ng</pr></trd>
</drv>

<drv mrk="detru.0anto">
  <kap>
    <tld/>anto
  </kap>
  <snc>
    <dif>
      Iu, kiu <tld/>as:
      <ekz>
        <nom>Menes</nom>, la unua egipta faraono, estis <ind><tld/>anto</ind> de l'
        ordo
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 1</lok></fnt>;
        <trd lng="fr">destructeur<klr tip="ind"> (subst.)</klr></trd>
        <trd lng="hu">lerombol&oacute;</trd>
      </ekz>
      <ekz>
        rabistoj kaj <tld/>antoj de virina honoro minacos vin
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        batalo kontra&ubreve; la <tld/>antoj de nia kredo
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ib;&c_sh;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Zerst&ouml;rer</trd>
  <trd lng="en">destroyer</trd>
<trdgrp lng="id">
  <trd><baz>hancur</baz>penghancur</trd>,
  <trd><baz>rusak</baz>perusak</trd>
</trdgrp>
</drv>

<drv mrk="detru.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="detru.0o.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        rigardu la farojn de la Eternulo, kiu faris <tld/>ojn sur la
        tero
        <fnt><bib>MT</bib>, <lok>&Psa; 46:8</lok></fnt>;
                                        n
      </ekz>
      <ekz>
        transdoni tian homon al Satano por la <tld/>o de la karno
        <fnt><bib>NT</bib>, <lok>&Ko1; 5:5</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[ili falas en]</klr> malutilajn dezirojn tiajn, kiaj
        dronigas
        la homojn en <tld/>o kaj pereo
        <fnt><bib>NT</bib>, <lok>&Ti1; 6:9</lok></fnt>;
      </ekz>
      <ekz>
        Mi anka&ubreve; kreis ekstermanton por <tld/>ado
        <fnt><bib>MT</bib>, <lok>&Jes; 54:16</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="id">
      <trd><baz>hancur</baz>penghancuran</trd>,
      <trd><baz>rusak</baz>perusakan</trd>,
      <trd>destruksi</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>niszczenie</trd>,
      <trd>destrukcja</trd>,
      <trd>demolka<klr> (pot.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="detru.0o.rezulto">
     <dif>
       Rezulto de <tld/>ado:
       <ekz>
         milito ka&ubreve;zas &ccirc;iam senprofitajn <tld/>ojn;
       </ekz>
       <ekz>
        a homaro ver&scirc;ajne estas malaperonta pro la <tld/>o de sia propra vivloko kaj
        el&ccirc;erpo de siaj vivrimedoj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
     </dif>
     <trdgrp lng="id">
       <trd><baz>hancur</baz>kehancuran</trd>,
       <trd><baz>rusak</baz>kerusakan</trd>
     </trdgrp>
     <trdgrp lng="pl">
        <trd>zniszczenie</trd>,
        <trd>demolka<klr> (pot.)</klr></trd>,
        <trd>destrukcja</trd>,
        <trd>zgliszcza</trd>
     </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>destrukce</trd>,
    <trd>devastace</trd>,
    <trd>ni&ccaron;en&iacute;</trd>,
    <trd>plen&ecaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Zerst&ouml;rung</trd>
  <trd lng="es">destrucci&oacute;n</trd>
  <trd lng="fr">destruction</trd>
  <trd lng="he">&he;&#x5B6;&resh;&#x5B6;&samekh;</trd>
  <trd lng="hu">rombol&aacute;s</trd>
  <trd lng="ja">&#x7834;&#x58CA;  <pr>&#x306F;&#x304B;&#x3044;</pr></trd>
  <trd lng="nl">vernietiging</trd>
  <trdgrp lng="pl">
      <trd>zniszczenie</trd>,
      <trd>demolka<klr> (pot.)</klr></trd>,
      <trd>destrukcja</trd>,
      <trd>zgliszcza</trd>
    </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_r;&c_u;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>demol&aacute;cia</trd>,
    <trd>zni&ccaron;enie</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rst&ouml;relse</trd>
  <trd lng="tok">pakala</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_j;&c_n;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_n;&c_i;&c_shch;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#x6EC5;&#x4EA1; <pr>mi&egrave;w&aacute;ng</pr></trd>
</drv>

<drv mrk="detru.0emo">
  <kap><tld/>emo</kap>
  <snc>
    <dif>
      Perversa emo al <tld/>ado:
      <ekz>
        tio estas <tld/>ado nur pro <tld/>emo;
      </ekz>
      <ekz>
