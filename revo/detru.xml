<drv mrk="detru.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari&gcirc;i <tld/>ita:
      <ekz>
        kia granda homo detrui&gcirc;is!<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu profunda plano <tld/>i&gcirc;is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mallevi&gcirc;is la mano <klr>[de Babel]</klr>, falis &gcirc;ia
        fundamento, <tld/>i&gcirc;is &gcirc;iaj muregoj
        <fnt><bib>MT</bib>, <lok>&Jer; 50:15</lok></fnt>;
      </ekz>
      <ekz>
        oni ne enver&scirc;as novan vinon en malnovajn felsakojn, alie
        la
        felsakoj <klr>...</klr> <tld/>i&gcirc;os
        <fnt><bib>NT</bib>, <lok>&Mat; 9:17</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>i&gcirc;o de la urbo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_b;&c_u;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_r;&c_u;&c_j;&c_n;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>an&eacute;antir</ind></trd>,
    <trd>s'<ind>&eacute;crouler</ind></trd>,
    <trd>s'<ind>effondrer</ind></trd>,
    <trd>se <ind>g&acirc;ter</ind><klr tip="ind"> (devenir
    inutilisable)</klr></trd>,
    <trd>se <ind>perdre</ind><klr tip="ind"> (devenir
    inutilisable)</klr></trd>,
    <trd>sombrer<klr tip="ind"> (s'an&eacute;antir)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5D4;&#x5D9;&#x5D4;&#x5E8;&#x5E1;</trd>
  <trdgrp lng="hu">
    <trd>lerombol&oacute;dik</trd>,
    <trd>leromlik</trd>,
    <trd>megsemmis&uuml;l</trd>,
    <trd>t&ouml;nkremegy</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7834;&#x58CA;&#x3055;&#x308C;&#x308B;  <pr>&#x306F;&#x304B;&#x3044;&#x3055;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x53F0;&#x7121;&#x3057;&#x306B;&#x306A;&#x308B;  <pr>&#x3060;&#x3044;&#x306A;&#x3057;&#x306B;&#x306A;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">vernietigd worden</trd>
  <trdgrp lng="pl">
    <trd>niszczy&cacute; si&eogonek;</trd>,
    <trd>burzy&cacute; si&eogonek;</trd>,
    <trd>rozpada&cacute; si&eogonek;</trd>,
    <trd>rozsypywa&cacute; si&eogonek;</trd>,
