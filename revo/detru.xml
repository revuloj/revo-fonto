  <trd lng="sk">devasta&ccaron;n&yacute;</trd>
  <trd lng="sv">f&ouml;rst&ouml;rande</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_j;&#x43D;&c_ib;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_i;&c_shch;&c_ib;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_g;&c_u;&c_b;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="zh">&#x7834;&#x574F;&#x6027; <pr>p&#xF2;hu&#xE0;ix&#xEC;ng</pr></trd>
</drv>

