<drv mrk="detru.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      Instrumento, rimedo por <tld/>i, precipe <ref tip="sub" cel="arm.0ilo">armilo</ref>:
      <ekz>
        ses homoj venis <klr>[&#x2026;]</klr> kaj &ccirc;iu havis en la mano
        sian <tld/>ilon
        <fnt><bib>MT</bib>, <lok>&Jeh; 9:2</lok></fnt>;
      </ekz>
      <ekz>
        la potencega nordamerika &scirc;tato planas denove stel-militon, <klr>[&#x2026;]</klr> li
        prezentis la grandiozan ideon, per <tld/>iloj en la kosmo protekti sin
        kontra&ubreve; la kanajloj en Kremlo
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_r;&c_o;&c_d;&c_a;&c_k; &c_z;&c_mol;&c_n;&c_ib;&c_sh;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_z;&c_b;&c_r;&c_o;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&aacute;stroj ni&ccaron;en&iacute;</trd>,
    <trd>prost&rcaron;edek ni&ccaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="de">Waffe</trd>
  <trd lng="fr">arme</trd>
  <trd lng="hu">puszt&iacute;t&oacute; fegyver</trd>
