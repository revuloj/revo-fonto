<drv mrk="arnik.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
       (<trd lng="la">Arnica</trd>), kies ekstrakto estas uzata
       kontra&ubreve; kontuzoj.
     </dif>
   </snc>
   <trd lng="de">Arnica</trd>
   <trdgrp lng="en">
    <trd>leopard's bane</trd>,
    <trd>arnica</trd>
   </trdgrp>
   <trd lng="es">&aacute;rnica</trd>
   <trd lng="fr">arnica</trd>
   <trd lng="hu">&aacute;rnika</trd>
   <trd lng="ru">&#1072;&#1088;&#1085;&#1080;&#1082;&#1072;</trd>
   <trd lng="sv">arnika</trd>
</drv>
</art>
