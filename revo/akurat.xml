<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>akurat</rad>/a
</kap>
<drv mrk="akurat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Faranta la farota&jcirc;on je la &gcirc;usta tempo, je la
      interkonsentita horo:
      <ekz>
        <tld/>a oficisto;
      </ekz>
      <ekz>
        <tld/>e veni al la servo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li venas &ccirc;iun tagon <tld/>e je la oka;
      </ekz>
      <ekz>
        <tld/>e pagi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>eco, kun kiu la suno levi&gcirc;as matene<fnt>B</fnt>;
      </ekz>
      <ekz>
        mi promesas <tld/>e servi al viaj mo&scirc;toj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e resendi la pruntitajn librojn;
      </ekz>
      <ekz>
        observi <tld/>ege la interkonsentojn de internacia juro.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      Ekzakta:
      <ekz>
        ni amu nin frate, sed ni kalkulu <tld/>e<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ponctuel</trd>,
    <trd>exact</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>accuraat</trd>,
    <trd>punctueel</trd>,
    <trd>stipt</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pontual</trd>,
    <trd>acurado</trd>,
    <trd>exato</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
