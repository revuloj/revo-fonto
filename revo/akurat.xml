<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>akurat</rad>/a
</kap>
<drv mrk="akurat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Faranta la farota&jcirc;on je la &gcirc;usta tempo, je la
      interkonsentita horo:
      <ekz>
        <tld/>a oficisto;
      </ekz>
      <ekz>
        <tld/>e veni al la servo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li venas &ccirc;iun tagon <tld/>e je la oka;
      </ekz>
      <ekz>
        <tld/>e pagi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>eco, kun kiu la suno levi&gcirc;as matene<fnt>B</fnt>;
      </ekz>
      <ekz>
        mi promesas <tld/>e servi al viaj mo&scirc;toj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e resendi la pruntitajn librojn;
      </ekz>
      <ekz>
        observi <tld/>ege la interkonsentojn de internacia juro.
      </ekz>
      <trd lng="es">puntual</trd>
    </dif>
    <trd 
lng="be">&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trd lng="de">p&uuml;nktlich</trd>
  <trdgrp lng="en">
    <trd>punctual</trd>,
    <trd>on-time</trd>
  </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <dif>
      Ekzakta:
      <ekz>
        ni amu nin frate, sed ni kalkulu <tld/>e<fnt><bib>PrV</bib></fnt>.
      </ekz>
      <trdgrp lng="be">
        <trd>&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_y;</trd>,
        <trd>&c_a;&c_k;&c_u;&c_r;&c_a;&c_t;&c_n;&c_y;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>genau</trd>,
        <trd>pr&auml;zise</trd>,
        <trd>exakt</trd>,
        <trd>akkurat<klr> [ark]</klr></trd>
      </trdgrp>
      <trd lng="es">exacto</trd>
    </dif>
    <trdgrp lng="de">
      <trd>genau</trd>,
      <trd>pr&auml;zise</trd>,
      <trd>exakt</trd>,
      <trd>akkurat<klr> [ark]</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>precise</trd>,
      <trd>exact</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>ponctuel</trd>,
    <trd>exact</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>genau</trd>,
    <trd>pr&auml;zise</trd>,
    <trd>exakt</trd>,
    <trd>akkurat</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>accuraat</trd>,
    <trd>punctueel</trd>,
    <trd>stipt</trd>
  </trdgrp>
  <trd lng="pl">punktualny</trd>
  <trdgrp lng="pt">
    <trd>pontual</trd>,
    <trd>acurado</trd>,
    <trd>exato</trd>
  </trdgrp>
  <trd lng="ro">punctual</trd>
  <trd 
lng="ru">&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">presn&yacute;</trd>
</drv>
</art>
