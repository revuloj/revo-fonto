<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akurat.xml,v 1.33 2023/10/28 08:33:04 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>akurat</rad>/e <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akurat.0e">
  <kap><tld/>e</kap>
  <snc mrk="akurat.0e.gxustatempe">
    <dif>
      Farante la farota&jcirc;on <ref tip="sin" cel="temp.gxusta0e.kiam">&gcirc;ustatempe</ref>, je la
      interkonsentita tempo:
      <ekz>
        hiera&ubreve; ni man&gcirc;is <tld/>e je l' tria horo
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi amas, ke oni <tld/>e pagu la luan pagon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        donadu <tld/>e la medikamenton
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi anka&ubreve; povos garantii al mi <klr>[&#x2026;]</klr> <tld/>econ en la venado al la
        laboro kaj en &gcirc;ia plenumado
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li promesis reveni al ni <tld/>e je la oka horo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>e venis al la servo
        <fnt>
          <bib>Hamlet</bib>
        </fnt>!
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trd lng="de">p&uuml;nktlich</trd>
    <trdgrp lng="en">
      <trd>punctually</trd>,
      <trd>on-time</trd>
    </trdgrp>
    <trd lng="es">puntual</trd>
    <trdgrp lng="fa">
      <trd>&a_w;&a_q;&a_t;&#x200C;&a_sh;&a_n;&a_A;&a_s;</trd>,
      <trd>&a_kh;&a_w;&a_sh;&#x200C;&a_q;&a_w;&a_l;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x5B88;&#x65F6; <pr>sh&#x1D2;ush&#xED;</pr></trd>,
    <trd>&#x5B88;&#x6642; <pr>sh&#x1D2;ush&#xED;</pr></trd>,
    <trd>&#x5C16; <pr>ji&#x101;n</pr></trd>,
    <trd>&#x5261; <pr>y&#x1CE;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="akurat.0e.ekzakte">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref cel="ekzakt.0a">Ekzakte</ref>:
      <ekz>
        ni amu nin frate, sed ni kalkulu <tld/>e<fnt>
        <bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kolekto ne nur ekstreme <tld/>a, sed anka&ubreve; tre sukcesa
        <fnt>
	<aut>Jorge Camacho</aut>
	<vrk><url ref="&Monato;/2000/007002.php">Kanzonoj por interna konsumo</url></vrk>
	<bib>Monato</bib>
	<lok>2000/07, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="gxust.0e.precize">&gcirc;uste<sncref/></ref>,
       <ref cel="preciz.0a">preciza</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_k;&c_u;&c_r;&c_a;&c_t;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>precisely</trd>,
      <trd>exactly</trd>
    </trdgrp>
    <trd lng="es">exacto</trd>
    <trd lng="fa">&a_d;&a_q;&f_y;&a_q;</trd>
  </snc>
  <trd lng="bg">&c_t;&c_o;&c_ch;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>akor&aacute;tn&iacute;</trd>,
    <trd>dochviln&yacute;</trd>,
    <trd>po&rcaron;&aacute;dn&yacute;</trd>,
    <trd>p&rcaron;esn&yacute;</trd>,
    <trd>&rcaron;&aacute;dn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>genau</trd>,
    <trd>pr&auml;zise</trd>,
    <trd>exakt</trd>,
    <trd>akkurat</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ponctuel</trd>,
    <trd>exact</trd>
  </trdgrp>
  <trd lng="he">&mem;&#x5B0;&dalet;&vav;&#x5BC;&yod;&#x5B8;&qof;</trd>
  <trd lng="ja">&#x6642;&#x9593;&#x3069;&#x304A;&#x308A;&#x306B;  <pr>&#x3058;&#x304B;&#x3093;&#x3069;&#x304A;&#x308A;&#x306B;</pr></trd>
  <trdgrp lng="nl">
    <trd>accuraat</trd>,
    <trd>punctueel</trd>,
    <trd>stipt</trd>
  </trdgrp>
  <trd lng="pl">punktualny</trd>
  <trdgrp lng="pt">
    <trd>pontual</trd>,
    <trd>acurado</trd>,
    <trd>exato</trd>
  </trdgrp>
  <trd lng="ro">punctual</trd>
  <trd lng="ru">&c_p;&c_u;&c_n;&c_k;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">presn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&#x43A;&c_u;&c_r;&c_a;&c_t;&#x43D;&#x43E;</trd>,
    <trd>&c_t;&#x43E;&c_ch;&#x43D;&#x43E;</trd>,
    <trd>&c_a;&#x43A;&c_u;&c_r;&c_a;&c_t;</trd>
  </trdgrp>
</drv>
</art>
