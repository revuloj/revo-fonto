<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: komat.xml,v 1.19 2023/12/03 09:58:53 revo Exp $">
<kap>
  <rad>komat</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="komat.0o">
  <kap><tld/>o</kap>
  <snc mrk="komat.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">Malsan</ref>eca peza dormo kun malesto de
      sentokapablo kaj de
      memvola movkapablo:
      <ekz>
        diabeta, duondelira <tld/>o.
      </ekz>
    </dif>
    <ref tip="vid" cel="letarg.0o">letargio</ref>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_m;&c_a;</trd>
  <trd lng="ca">coma</trd>
  <trdgrp lng="cs">
    <trd>hlubok&eacute; bezv&ecaron;dom&iacute;</trd>,
    <trd>k&oacute;ma</trd>
  </trdgrp>
  <trd lng="de">Koma</trd>
  <trd lng="en">coma</trd>
  <trd lng="es">coma</trd>
  <trd lng="fr">coma</trd>
  <trd lng="id">koma</trd>
  <trd lng="ja">&#x660F;&#x7761;  <pr>&#x3053;&#x3093;&#x3059;&#x3044;</pr></trd>
  <trd lng="nl">coma</trd>
  <trdgrp lng="pl">
    <trd>koma</trd>,
    <trd>&sacute;pi&aogonek;czka</trd>
  </trdgrp>
  <trd lng="pt">coma</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>hlbok&eacute; bezvedomie</trd>,
    <trd>k&oacute;ma</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x43E;&#x43C;&#x430;</trd>
  <trd lng="zh">&#x660F;&#x8FF7; <pr>h&#x16B;nm&#xED;</pr></trd>
</drv>

<drv mrk="komat.0a">
  <kap><tld/>a</kap>
  <snc mrk="komat.0a.MED">
    <dif>
      Rilatanta al <tld/>o:
      <ekz>
        la akra bleko de la &scirc;afo ek&scirc;iris &scirc;in el la komata stato 
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_a;&c_t;&c_o;&c_z;&c_n;&c_y;</trd>
  <trd lng="ca">comat&oacute;s</trd>
  <trd lng="de">komat&ouml;s</trd>
  <trd lng="en">comatose</trd>
  <trd lng="es">comatoso</trd>
  <trd lng="fr">comateux</trd>
  <trd lng="ja">&#x660F;&#x7761;&#x306E;  <pr>&#x3053;&#x3093;&#x3059;&#x3044;&#x306E;</pr></trd>
  <trd lng="nl">comateus</trd>
  <trd lng="pt">comatoso</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_a;&c_t;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43C;&#x430;&#x442;&#x43E;&#x437;&#x43D;&#x438;&#x439;</trd>
  <trd lng="zh">&#x660F;&#x8FF7; <pr>h&#x16B;nm&#xED;</pr></trd>
</drv>
</art>
