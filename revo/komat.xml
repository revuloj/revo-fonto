<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>komat</rad>/o
</kap>
<drv mrk="komat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malsaneca peza dormo kun malesto de sentokapablo kaj de
      memvola movkapablo:
      <ekz>diabeta, duondelira <tld/>o.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_m;&c_a;</trd>
  <trd lng="en">coma</trd>
  <trd lng="nl">coma</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_a;</trd>
</drv>
<drv mrk="komat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o:
      <ekz><tld/>a stato.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_a;&c_t;&c_o;&c_z;&c_n;&c_y;</trd>
  <trd lng="en">comatose</trd>
  <trd lng="nl">comateus</trd>
  <trd lng="ru">&c_k;&c_o;&c_m;&c_a;&c_t;&c_o;&c_z;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
