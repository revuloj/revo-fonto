<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vernis.xml,v 1.19 2023/10/21 16:50:36 revo Exp $">
<kap>
  <rad>vernis</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="vernis.0o">
  <kap><tld/>o</kap>
  <snc mrk="vernis.0o.KEM">
    <dif>
      <ref tip="dif" cel="lak.0o.KEM">Lako</ref>
      <ekz>
        mezurbendoj estas faritaj el maldika strio de tolo kovrita de <tld/>o kaj
        rulvolvita en plasta a&ubreve; metala skatolo
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Mezurbendo">Mezurbendo - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>vern&iacute;s</trd>,
    <trd>laca</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ferme&zcaron;</trd>,
    <trd>pakost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lack</trd>,
    <trd>Firnis</trd>,
    <trd>&Uuml;berzug</trd>
  </trdgrp>
  <trd lng="en">varnish</trd>
  <trdgrp lng="ja">
    <trd>&#12527;&#12491;&#12473;</trd>,
    <trd>&#12491;&#12473;</trd>
  </trdgrp>
  <trd lng="nl">vernis</trd>
  <trdgrp lng="pl">
    <trd>werniks</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_i;&c_t;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_o;&c_l;&c_i;&c_f;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">ferme&zcaron;</trd>
  <trd lng="uk">&c_l;&c_a;&c_k;</trd>
</drv>

<drv mrk="vernis.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vernis.0i.KEM">
    <dif>
      <ref tip="dif" cel="lak.0i.KEM">Laki</ref>:
      <ekz>
        mi jam <tld/>is la violonon okfoje
        <fnt>
        <aut>fotono</aut>,
        <vrk><url ref="http://photon.freeshell.org/weblog/">senvaloran
        min</url></vrk>,
        <lok>&scirc;an&gcirc;is la paperon de vitro, 2005-11-17</lok>
        </fnt>;
      </ekz>
      <ekz>
        oficejo, modernigita, reformita kaj &scirc;pruc<tld/>ita per
        buntaj fosforeskaj farboj
        <fnt>
          <bib>Monato</bib>
          <aut>Filippo</aut>
          <vrk><url ref="&Monato;/2006/008898.php">Moderna po&scirc;to</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>envernissar</trd>,
    <trd>lacar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ferme&zcaron;ovat</trd>,
    <trd>nat&iacute;rat ferme&zcaron;&iacute;</trd>
  </trdgrp>
  <trd lng="de">lackieren</trd>
  <trd lng="en">varnish</trd>
  <trd lng="ja">&#12491;&#12473;&#12434;&#22615;&#12427;  <pr>&#12491;&#12473;&#12434;&#12396;&#12427;</pr></trd>
  <trd lng="nl">vernissen</trd>
  <trd lng="pl">pokrywa&cacute; <ind>werniksem</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_p;&c_o;&c_l;&c_i;&c_t;&c_u;&c_r;&c_o;&c_j;</ind></trd>
    <trd>&c_o;&c_l;&c_i;&c_f;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">natrie&tcaron; ferme&zcaron;ou</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i; &c_l;&c_a;&c_k;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="vernis.ungo0o">
  <kap>ungo<tld/>o</kap>
  <snc mrk="vernis.ungo0o.ungolako">
    <dif>
      <ref tip="dif" cel="lak.ungo0o">Ungolako</ref>:
      <ekz>
        mi ser&ccirc;is pri apoteko kaj tiu a&jcirc;o por demeti la
        ungo-<tld/>on de la <klr>[&#8230;]</klr> ungoj
        <fnt>
          <aut>Mirna Marino</aut>,
          <vrk>
            <url ref="http://www.ipernity.com/blog/ladislao/84483">
            Kiel organizi pli bone Universalajn Kongresojn?</url>
          </vrk>,
          <lok>komentoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_k; &c_d;&c_l;&c_ja; &c_p;&c_a;&c_z;&c_n;&c_o;&c_g;&c_c;&c_ja;&c_w;</trd>
  <trd lng="bg">&c_l;&c_a;&c_k; &c_z;&c_a; &c_n;&c_o;&c_k;&c_t;&c_i;</trd>
  <trd lng="ca">esmalt d'ungles</trd>
  <trd lng="cs">lak na nehty</trd>
  <trd lng="de">Nagellack</trd>
  <trdgrp lng="en">
    <trd>nail varnish</trd>,
    <trd>nail polish</trd>
  </trdgrp>
  <trd lng="fr">vernis &agrave; ongles</trd>
  <trd lng="it">lacca per unghie</trd>
  <trd lng="nl">nagellak</trd>
  <trd lng="pl">lakier do paznokci</trd>
  <trd lng="sk">lak na nechty</trd>
</drv>

</art>

