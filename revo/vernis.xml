<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vernis</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="vernis.0o">
  <kap><tld/>o</kap>
  <snc mrk="vernis.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Likva&jcirc;o, kiu seki&gcirc;as sub libera aero, i&gcirc;ante travidebla 
&scirc;irma
      tavolo sur la &scirc;mirita a&jcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="emajl.0o.TEK">emajlo <sncref/></ref>,
      <ref cel="farb.0o">farbo</ref>,
      <ref cel="glace.0a">glacea</ref>,
      <ref cel="glazur.0o.TEK">glazuro <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="fr">vernis</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_i;&c_t;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_o;&c_l;&c_i;&c_f;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="vernis.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vernis.0i.TEK">
    <dif>
      &Scirc;miri, tegi per <tld/>o:
      <ekz>
        mi jam <tld/>is la violonon okfoje
        <fnt>
        <aut>fotono</aut>,
        <vrk><url ref="http://photon.freeshell.org/weblog/">senvaloran
        min</url></vrk>,
        <lok>&scirc;an&gcirc;is la paperon de vitro, 2005-11-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">vernir</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_k;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol; 
<ind>&c_p;&c_o;&c_l;&c_i;&c_t;&c_u;&c_r;&c_o;&c_j;</ind></trd>,
    <trd>&c_o;&c_l;&c_i;&c_f;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>

