<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: pum1.xml,v 1.2 2025/03/17 08:36:48 afido Exp $">
    <kap>
      <rad>pum</rad>/o
      <fnt><bib>PVEF</bib></fnt>
    </kap>

<drv mrk="pum1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="pum1.0o.ZOO">
    <dif>
      Amerika arbogrimpa <ref tip="super" cel="best.rabo0o">rabobesto</ref>, pli malgranda ol
      jaguaro,
      genro el familio <ref tip="malprt" cel="felise.0oj">felisedoj</ref> (<trd lng="la">Puma
      concolor</trd>):
      <ekz>
        Titikako (roko de pumo) estas la plej alta navigata lago de la mondo
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;7</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Puma</trd>,
      <trd>Bergl&ouml;we</trd>,
      <trd>Silberl&ouml;we</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cougar</trd>,
      <trd>mountain lion</trd>
    </trdgrp>
    <trd lng="fr">puma</trd>
    <trd lng="ru">&c_p;&c_u;&c_m;&c_a;</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_p;&c_u;&c_m;&c_a;</trd>,
  <trd>&c_k;&c_u;&c_g;&c_u;&c_a;&c_r;</trd>,
  <trd>&c_g;&c_o;&c_r;&c_n;&c_y; <ind>&c_l;&c_je;&c_w;</ind></trd>,
  <trd>&c_s;&c_je;&c_r;&c_a;&c_b;&c_r;&c_y;&c_s;&c_t;&c_y; <ind>&c_l;&c_je;&c_w;</ind></trd>
</trdgrp>
</drv>

  </art>

