<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>boj</rad>/i
</kap>
<drv mrk="boj.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc num="1">
    <dif>
      Hunde bleki:
      <ekz>
        hundo <tld/>as, homo vojas
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>  
        <klr>(oni ne devas zorgi senka&ubreve;zajn insultojn)</klr>.
      </ekz> 
    </dif>
    <trd lng="de">bellen</trd>
    <trdgrp lng="tr">
     <trd>havlamak</trd>,
     <trd>&uuml;r&uuml;mek</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Atake, ofende parola&ccirc;i:
      <ekz>
        li neniam estas kontenta kaj <tld/>as
        kontra&ubreve; &ccirc;iu k &ccirc;io.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">bark</trd>
  <trd lng="fr">aboyer</trd>
  <trd lng="nl">blaffen</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_v;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="boj.tir0i">
<kap>tir<tld/>i</kap>
<gra><vspec>ntr</vspec></gra>
<snc>
<dif>
A&ubreve;digi longan, senpa&ubreve;zan <tld/>on.</dif>
<refgrp tip="sin">
<ref cel="hojl.0i">hojli</ref>,
<ref cel="ulul.0i">ululi</ref>.
</refgrp>
</snc>
<trdgrp lng="en">
  <trd>bay</trd>,
  <trd>howl</trd>
</trdgrp>
<trd lng="fr">hurler</trd>
</drv>
</art>
