<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: boj.xml,v 1.44 2025/06/29 20:32:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>boj</rad>/i <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="boj.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="boj.0i.ZOO">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Hunde <ref tip="super" cel="blek.0i.beste">bleki</ref>:
      <ekz>
        <tld/>as hundo sen puno e&ccirc; kontra&ubreve; la suno
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;enhundo en palaco sidas en sunlumo fiere sur sia budo kaj <tld/>as kontra&ubreve;
        &ccirc;iu pasanto
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        batata hundo <tld/>as kaj mordas
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hundo plej la&ubreve;te <tld/>as, kiam &gcirc;i timas
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la nokto falas, malbonfaremaj potencoj vagas
        &ccirc;irka&ubreve; la domo; mi <tld/>as, por ke la mastro,
        avertita, forpelu ilin
        <fnt>
          <aut>A. France, trad. G. Moch</aut>,
          <vrk>Pensoj de Hen&ccirc;jo</vrk>,
          <lok>La Revuo, vol. 2a, 1907-09 &gcirc;is 1908-08</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>bordar</trd>,
      <trd>lladrar</trd>,
      <trd>jaupar</trd>
    </trdgrp>
    <trd lng="de">bellen</trd>
    <trd lng="pl">szczeka&cacute;</trd>
    <trdgrp lng="tr">
      <trd>havlamak</trd>,
      <trd>&uuml;r&uuml;mek</trd>
    </trdgrp>
    <trd lng="pt">latir</trd>
  </snc>
  <snc mrk="boj.0i.al">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Al<tld/>i:
      <ekz>
        &scirc;i anta&ubreve;e neniam vidis hundon, tiu &ccirc;i komencis tiel furioze &scirc;in <tld/>i, ke &scirc;i
        tute ektimi&gcirc;is
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne &ccirc;iu <tld/>ato estas &scirc;telisto
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        hundo <tld/>as la vojon, vento portas la <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>bordar <klr tip="amb">(a)</klr></trd>,
      <trd>lladrar <klr tip="amb">(a)</klr></trd>,
      <trd>jaupar <klr tip="amb">(a)</klr></trd>
    </trdgrp>
    <trd lng="de">verbellen</trd>
  </snc>
  <snc mrk="boj.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Atake, ofende parola&ccirc;i:
      <ekz>
        &leftquot;sufi&ccirc;e!&rightquot; <tld/>is la ra&ubreve;kulo, kies humoro percepteble rapide evoluis al
        furiozo
        <fnt>
          <bib>CKv</bib>
          <lok>10</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i preska&ubreve; <tld/>is, sed iom post iom &scirc;iaj trajtoj moli&gcirc;is
        <fnt>
          <bib>CKv</bib>
          <lok>27</lok>
        </fnt>;
      </ekz>
      <ekz>
        per akra la&ubreve;tkriego mi <tld/>as a&ubreve;toritate: &leftquot;Venu anta&ubreve;en!&rightquot;
        <fnt>
          <bib>KrM</bib>
          <lok>16. Malsa&gcirc;uloj Kreskas Mem, Sen Plugo Kaj Sem&#8217;</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>hundo <tld/>as, homo vojas</ind>
        <klr>(oni ne devas zorgi senka&ubreve;zajn insultojn)</klr>
        <trd lng="de">Die <ind>Hunde bellen</ind>, aber die Karawane
        zieht weiter.</trd>
        <trd lng="es">ladran, luego cabalgamos</trd>
        <trd lng="fr">les <ind>chiens</ind> aboient, la caravane
        passe</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">cridar</trd>
    <trdgrp lng="pl">
      <trd>szczeka&cacute;</trd>,
      <trd>ujada&cacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_w;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_h;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&amp;t_-a;&t_ba;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="br">harzhal</trd>
  <trdgrp lng="cs">
    <trd>hafat</trd>,
    <trd>&scaron;t&ecaron;kat <klr>(na n&ecaron;koho)</klr></trd>
  </trdgrp>
  <trd lng="en">bark</trd>
  <trd lng="es">ladrar</trd>
  <trd lng="fr">aboyer</trd>
  <trd lng="he">&lamed;&#1460;&nun;&#1456;&bet;&#1468;&vav;&#1468;&het;&#1463;</trd>
  <trdgrp lng="ja">
    <trd>&#12411;&#12360;&#12427;</trd>,
    <trd>&#12431;&#12417;&#12367;</trd>,
    <trd>&#12364;&#12415;&#12364;&#12415;&#35328;&#12358;
    <pr>&#12364;&#12415;&#12364;&#12415;&#12356;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">blaffen</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_v;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>&scaron;teka&tcaron;</trd>,
    <trd>brecha&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">sk&auml;lla</trd>
  <trd lng="th">&#3648;&#3627;&#3656;&#3634;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_v;&c_k;&c_a;&c_t;&c_i;&nbsp;(&c_t;&c_zh; &c_p;&c_je;&c_r;&c_je;&c_n;.)</trd>,
    <trd>&c_b;&c_r;&c_je;&c_h;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="boj.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="boj.0.SON">
    <dif>
      Ordinara <ref tip="lst" cel="blek.0o" lst="voko:blekoj">bleko</ref>
      de <ref tip="vid" cel="hund.0o.dombesto">hundo</ref>;
      <ref tip="sin" cel="vaux.0.SON">va&ubreve;</ref>:
      <ekz>
        mi havas malgrandan vilan nigran hundon nomitan Toto
        <klr>[&#8230;]</klr> lia sola parolo &ccirc;iam estas nur
        <ctl><tld/>! <tld/>!</ctl>
        <fnt>
          <aut>L. Frank Baum, trad. D. Broadribb</aut>,
          <vrk><url ref="http://ttt.esperanto.org/esperante/literatur/Baum/tiktoko/cxapitro25.html">
          Tiktoko de Oz</url></vrk>,
          <lok>[1996]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#27754;&#27754; <pr>w&amacron;ngw&amacron;ng</pr></trd>
</snc>
  <trd lng="be">&c_g;&c_a;&c_w;!</trd>
  <trd lng="ca">bub-bub</trd>
  <trd lng="de">wau</trd>
  <trd lng="fr">ouah !<klr> (chien)</klr></trd>
  <trd lng="es">&#161;guau!</trd>
  <trd lng="th">&#3650;&#3630;&#3656;&#3591;</trd>
</drv>

<drv mrk="boj.tir0i">
  <kap>tir<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="boj.tir0i.SON">
    <dif>
      A&ubreve;digi longan, senpa&ubreve;zan <tld/>on;
      <ref tip="sin" cel="hojl.0i">hojli</ref>,
      <ref tip="sin" cel="hurl.0i.lupo">hurli</ref>:
      <ekz>
        Princo tir<tld/>as: <esc>uuuuuu</esc>
        <fnt>
          <vrk><url ref="http://miresperanto.com/humuro/rikardo-cash-11.htm">Shercoj de rikardo
          CASH-11</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#24594;&#21564; <pr>n&ugrave;h&#466;u</pr></trd>
</snc>
  <trd lng="be">&c_v;&c_y;&c_c;&c_mol;</trd>
  <trd lng="br">yudal</trd>
  <trdgrp lng="ca">
    <trd>udolar</trd>,
    <trd>ulular</trd>,
    <trd>ganyolar</trd>
  </trdgrp>
  <trd lng="cs">v&yacute;t</trd>
  <trd lng="de">heulen</trd>
  <trdgrp lng="en">
    <trd>bay</trd>,
    <trd>howl</trd>
  </trdgrp>
  <trd lng="es">aullar</trd>
  <trd lng="fr">hurler</trd>
  <trd lng="ja">&#36960;&#21536;&#12360;&#12377;&#12427;  <pr>&#12392;&#12362;&#12412;&#12360;&#12377;&#12427;</pr></trd>
  <trd lng="pl">ujada&cacute;</trd>
  <trd lng="sk">vy&tcaron;</trd>
  <trd lng="sv">yla</trd>
  <trd lng="th">&#3627;&#3629;&#3609;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i; (&c_p;&c_r;&c_o;
    &c_s;&c_o;&c_b;&c_a;&c_k;&c_u;)</trd>
  </trdgrp>
</drv>
</art>
