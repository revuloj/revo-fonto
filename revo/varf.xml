<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: varf.xml,v 1.25 2023/10/21 16:50:35 revo Exp $">
<kap>
  <rad>varf</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="varf.0o">
  <kap><tld/>o</kap>
  <snc mrk="varf.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Speco de <ref tip="malprt" cel="haven.0o.MAR">havena<sncref/></ref>
      ponto, kiu elstaras de la <ref tip="vid" cel="kaj1.0o">kajo</ref>:
      <ekz>
        oni povas albolardigi sian boaton &ccirc;e kelkaj publikaj <tld/>oj de Insulo Galiano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=126032">Insulo Galiano</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <klr>[...]</klr> promenis reen la&ubreve; la <tld/>o al la bordo
        <fnt>
          <vrk><url
          ref="http://bulteno.esperanto-usa.org/2013/6/50-vojagxe-tra-usono.html">Lumineska akvo en
          Alabamo - Usona Esperantisto 2013:6</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_ch;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="ca">moll</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;&iacute;stavi&scaron;t&ecaron;</trd>,
    <trd>p&rcaron;&iacute;stavn&iacute; m&uring;stek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pier</trd>,
    <trd>Anleger</trd>,
    <trd>Landebr&uuml;cke</trd>
  </trdgrp>
  <trd lng="en">wharf</trd>
  <trdgrp lng="fr">
    <trd>appontement</trd>,
    <trd>embarcad&egrave;re</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kik&ouml;t&odblac;h&iacute;d</trd>,
    <trd>st&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x685F;&#x6A4B;  <pr>&#12373;&#12435;&#12400;&#12375;</pr></trd>,
    <trd>&#x6CE2;&#x6B62;&#22580;  <pr>&#x306F;&#12392;&#12400;</pr></trd>,
    <trd>&#x57E0;&#x982D;  <pr>&#12405;&#12392;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aanlegplaats</trd>,
    <trd>pier</trd>,
    <trd>steiger</trd>
  </trdgrp>
  <trd lng="pl">nabrze&zdot;e</trd>
  <trdgrp lng="pt">
    <trd>cais</trd>,
    <trd>desembarcadouro</trd>,
    <trd>p&iacute;er</trd>
  </trdgrp>
  <trd lng="ro">cheu</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_a;&c_l;</trd>
  </trdgrp>
  <trd lng="sk">pr&iacute;stavn&yacute; most&iacute;k</trd>
  <trdgrp lng="sv">
    <trd>kaj</trd>,
    <trd>brygga</trd>,
    <trd>pir</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_i;&c_s;&c_t;&c_a;&#x43D;&#x44C;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_ch;&c_a;&#x43B;</trd>
  </trdgrp>
</drv>

<drv mrk="varf.al0igxi">
  <kap>al<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="varf.al0igxi.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Proksimi&gcirc;i al <tld/>o kaj halti la&ubreve;longe de &gcirc;i:
      <ekz>
        duonplena pramo kun la nomo <nom>Merkurij</nom> al<tld/>i&gcirc;as
        <fnt>
          <aut>K. Kniivil&auml;</aut>
          <vrk>Krimeo estas nia</vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="bord.al0igxi.MAR">albordi&gcirc;i</ref>.
  </refgrp>
  <trdgrp lng="fr">
    <trd>aborder</trd>,
    <trd>mettre &agrave; quai</trd>
  </trdgrp>
</drv>
</art>
