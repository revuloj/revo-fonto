<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Heziod</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="heziod.0o">
  <kap><tld/>o</kap>
  <snc mrk="heziod.0o.PERS">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Helena poeto el Beotio (8a jarcento a.K. &ndash; 7a jarcento
      a.K.),
      a&ubreve;toro
      de poemoj kun kampara, morala, instrua enhavo:
      <ekz>
        la poemoj de Homero kaj <tld/>o
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Hesiod</trd>
  <trd lng="es">Hes&iacute;odo</trd>
  <trd lng="fr">H&eacute;siode</trd>
  <trd lng="it">Esiodo</trd>
  <trd lng="ca">Hes&iacute;ode</trd>
  <trd lng="pt">Hes&iacute;odo</trd>
</drv>
</art>

