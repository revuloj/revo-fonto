<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>sutan</rad>/o
</kap>
<drv mrk="sutan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Longa &gcirc;ispieda a&ubreve; &gcirc;isgenua vesto de ekleziuloj butonumata sur anta&ubreve;a
      flanko.
    </dif>
    <refgrp tip="sin">
      <ref cel="rob.0o">robo</ref>,
      <ref cel="tog.0o">togo</ref>,
      <ref cel="talar.0o">talaro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Sutane</trd>
  <trd lng="nl">soutane</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_t;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_ja;&c_s;&c_a;</trd>
  </trdgrp>
</drv>
</art>
