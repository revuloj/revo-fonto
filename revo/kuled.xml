<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kuled.xml,v 1.16 2023/09/01 15:46:46 revo Exp $">
<kap>
  <rad>kuled</rad>/oj
</kap>
<drv mrk="kuled.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo 
      <ref tip="malprt" cel="dipter.0oj">dipteroj</ref>
      (<trd lng="la">Culicidae</trd>):
      <ekz>
         <ref tip="prt" cel="moskit.0o">moskito</ref> estas membro de la familio <tld/>oj <klr>[&#x2026;]</klr>, 
        ili aperis jam anta&ubreve; pli ol 170 milionoj da jaroj, <tld/>oj
        havas du skvamajn flugilojn, maldikan abdomenon, kaj longajn krurojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=39409">Moskito</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="kul.0o">kulo</ref>
  </snc>
  <trd lng="ca">cul&iacute;cids</trd>
  <trd lng="de">Stechm&uuml;cken</trd>
  <trdgrp lng="en">
    <trd>Culicidae</trd>,
    <trd>mosquitoes</trd>, <trd>mosquito family</trd>
  </trdgrp>
  <trd lng="fr">Culicid&eacute;s</trd>
  <trd lng="hu">sz&uacute;nyogf&eacute;l&eacute;k</trd>
  <trd lng="ja">&#x30AB;&#x79D1;  [<ind>&#x30AB;&#x304B;</ind>]</trd>
  <trd lng="nl">steekmuggen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">komarowate</trd>
  <trd lng="pt">culic&iacute;deos</trd>
  <trd lng="ru">&c_k;&c_r;&c_o;&c_v;&c_o;&c_s;&c_o;&c_s;&c_u;&c_shch;&c_i;&c_je;
<ind>&c_k;&c_o;&c_m;&c_a;&c_r;&c_y;</ind> <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&c_s;&#x43F;&#x440;&#x430;&#x432;&#x436;&#x43D;&#x456; &#x43A;&#x43E;&#x43C;&#x430;&#x440;&#x456;</trd>
</drv>
</art>

