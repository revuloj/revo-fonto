<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxaled.xml,v 1.12 2024/12/03 02:03:49 revo Exp $">
<kap>
  <rad>&ccirc;aled</rad>/o <fnt><bib>NePiv</bib></fnt>,
  <var><kap><rad var="t">&ccirc;alet</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="cxaled.0o">
  <kap><tld/>o, <var><kap><tld var="t"/>o</kap></var></kap>
  <snc mrk="cxaled.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Montara ligno<ref tip="super" cel="dom.0o.konstruajxo">domo</ref>,
      kiajn oni tradicie konstruas en <ref tip="malprt" cel="alp.A0oj">Alpoj</ref>:
      <ekz>
        la partoprenantoj lo&gcirc;os en simpla bela alpa <tld/>o
        <fnt>
          <aut>C. Gacond</aut>,
          <vrk><url ref="http://www.esperanto-gacond.ch/Claude_Gacond_Radioprelego_0259.pdf">
          Jubileo Mahatma Gandhi...</url></vrk>,
          <lok>259-a radioprelego, 1968-09</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o staras &ccirc;e la vojeta fino
        <fnt>
          <aut>A. Pi&eacute;rard</aut>,
          <vrk>
<url ref="http://www.therapies-hypnose.ch/bonvenon/aliro/aliro.html">
          Kiel aliri</url></vrk>,
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni kutimis lupreni <tld var="t"/>on &ccirc;e <nom>Sch&ouml;nried</nom>
        en la apudberna montaro
        <fnt>
          <aut>R. Harry</aut>,
          <vrk><url ref="http://www.luin.se/inko/X24-6.pdf">
          La Diplomato Kiu Ridis</url></vrk>,
          <lok>1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu devis gardi la <tld var="t"/>on, dum aliulo devas &ccirc;asi
        <fnt>
          <aut>K. May, trad. M. Peick</aut>,
          <vrk><url ref="http://www.peick.de/tradukoj.eo.html">
          Olda Certmano</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="alberg.0o">albergo</ref>,
       <ref cel="dom.somer0o">somerdomo</ref>,
       <ref cel="vila.0o">vilao</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/1/1c/Wilderswil-Chalets.jpg/640px-Wilderswil-Chalets.jpg" prm="CC BY
    3.0">Malnovaj <tld/>oj en Wilderswil
        <fnt>
          <aut>Roland Zumbuehl, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Wilderswil-Chalets.jpg">&WCW;/File:Wilderswil-Chalets.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_l;&c_je;</trd>
  <trd lng="ca">xalet</trd>
  <trd lng="de">Chalet</trd>
  <trd lng="en">chalet</trd>
  <trd lng="es">casa de madera<klr tip="amb"> (chal&eacute; alpino de
  monta&ntilde;a)</klr></trd>
  <trd lng="fr">chalet</trd>
  <trd lng="he">&ayin;&bet;&resh;&yod;&tav;</trd>
  <trd lng="it">chalet</trd>
  <trd lng="nl">chalet</trd>
  <trd lng="pl">chatka w g&oacute;rach</trd>
  <trd lng="pt">chal&eacute;</trd>
  <trd lng="ru">&c_sh;&c_a;&c_l;&c_je;&#769;</trd>
  <trd lng="uk">&c_sh;&c_a;&#x43B;&c_je; (&c_b;&c_u;&c_d;&c_i;&#x43D;&#x43E;&c_ch;&#x43E;&#x43A; &c_u; &c_g;&#x43E;&c_r;&c_a;&c_h;)</trd>
</drv>
</art>

