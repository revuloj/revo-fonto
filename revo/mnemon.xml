<art mrk="$Id: mnemon.xml,v 1.19 2023/09/02 06:04:13 revo Exp $">
<kap>
  <rad>mnemonik</rad>/o
</kap>
<drv mrk="mnemon.0o">
  <kap><tld/>o</kap>
  <snc mrk="mnemon.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="art.memor0o.TEK">Memorarto</ref>:
      <ekz>
        tiamaniere li studis stenografion, <tld/>on, tre longan tempon li
        obstine sin okupis pri spiritismo... kaj pri ceteraj diabla&jcirc;oj... 
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>&ccirc;apitro 6a</lok></fnt>;
      </ekz>
      <ekz>
        iuj kleruloj kredas ke rongorongo ne &gcirc;uste estas plena skribsistemo, sed
        genealogio a&ubreve; <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49224">Rongorongo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_n;&c_e;&c_m;&c_o;&c_n;&c_ib;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_n;&c_e;&c_m;&c_a;&c_t;&c_e;&c_h;&c_n;&c_ib;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mnemonika</trd>,
    <trd>mnemotechnika</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mnemonik</trd>,
    <trd>Mnemotechnik</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>nemotecnia</trd>,
    <trd>mnemotecnia</trd>
  </trdgrp>
  <trd lng="fr">mn&eacute;motechnie</trd>
  <trdgrp lng="hu">
    <trd>mnemotechnika</trd>,
    <trd>mnemonika</trd>
  </trdgrp>
  <trd lng="ja">&#x8A18;&#x61B6;&#x8853;  [<ind>&#x304D;&#x304A;&#x304F;&#x3058;&#x3085;&#x3064;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>geheugenkunst</trd>,
    <trd>mnemotechniek</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mnemonika</trd>,
    <trd>sztuka zapami&eogonek;tywania</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>mnem&ocirc;nica</trd>,
   <trd>mnemot&eacute;cnica</trd>,
   <trd>mnemotecnia</trd>   
  </trdgrp>
  <trdgrp lng="ro">
    <trd>mnemonice</trd>,
    <trd>arta amintirii</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_n;&c_je;&c_m;&c_o;&c_n;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_n;&c_je;&c_m;&c_o;&c_t;&c_je;&c_h;&c_n;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mnemotechnika</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43D;&#x435;&#x43C;&#x43E;&#x43D;&#x456;&#x43A;&#x430;</trd>,
    <trd>&#x43C;&#x43D;&#x435;&#x43C;&#x43E;&#x442;&#x435;&#x445;&#x43D;&#x456;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
