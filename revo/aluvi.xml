<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aluvi.xml,v 1.24 2023/10/12 18:05:09 revo Exp $">
<kap>
  <rad>aluvi</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="aluvi.0o">
  <kap><tld/>o</kap>
  <snc mrk="aluvi.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="super" cel="tavol.0o.KOMUNE">Tavolo</ref>
      da tero alportita kaj delasita de la akvoj:
      <ekz>
        meandroj evoluas pro erozio: dum la fluo, pro pli granda
        rapido, erodas la konkavan bordon de la riverujo, &ccirc;e la konveksa
        bordo, pro malpli granda rapido, <tld/>oj surfundi&gcirc;as kaj sedimenti&gcirc;as
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=54108">Meandro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Misisipio <klr>[&#x2026;]</klr> &dash; la nordokcidenta resto de la &scirc;tato
        estas komponita de parto de la Misisipia <tld lit="A"/>a Ebenejo, alinome la Misisipa Delto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2845">Misisipio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_l;&c_ju;&c_v;&c_ib;&c_j;</trd>
    <trd lng="bg">&c_a;&c_l;&c_u;&c_v;&c_i;&c_j;</trd>
    <trd lng="ca">al&middot;luvi&oacute;</trd>
    <trdgrp lng="de">
      <trd>Schwemmboden</trd>,
      <trd>Alluvialboden</trd>,
      <trd>Anschwemmung</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>alluvium</trd>,
      <trd>alluvion</trd>
    </trdgrp>
    <trd lng="es">aluvi&oacute;n</trd>
    <trd lng="fa">&a_A_madda;&a_b;&a_r;&a_damma;&a_f;&a_t;</trd>
    <trd lng="fr">alluvion</trd>
    <trdgrp lng="hu">
      <trd>folyami <ind>lerak&oacute;d&aacute;s</ind></trd>,
      <trd>alluvium</trd>
    </trdgrp>
    <trd lng="nl">alluvium</trd>
    <trd lng="pl">aluwium</trd>
    <trd lng="ru">&c_a;&c_l;&c_l;&c_ju;&c_v;&c_i;&c_j;</trd>
    <trdgrp lng="sk">
      <trd>naplavenina</trd>,
      <trd>n&aacute;nos</trd>
    </trdgrp>
  </snc>
  <snc mrk="aluvi.0o.GEOL_ARK">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="holocen.0o.GEOL">holoceno</ref>
    <trd lng="ca">holoc&eacute;</trd>
    <trd lng="fa">&a_h;&a_w;&a_l;&a_w;&a_s;&a_n;</trd>
    <trd lng="pl">aluwium</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>aluvi&aacute;ln&iacute; naplaveniny</trd>,
    <trd>holoc&eacute;n</trd>,
    <trd>n&aacute;nos</trd>,
    <trd>n&aacute;plav</trd>
  </trdgrp>
  <trd lng="he">&#x5E1;&#x5D7;&#x5E3;</trd>
  <trd lng="ja">&#x6C96;&#x7A4D;&#x571F;  <pr>&#x3061;&#x3085;&#x3046;&#x305B;&#x304D;&#x3069;</pr></trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x43B;&#x44E;&#x432;&#x456;&#x439;</trd>,
    <trd>&#x43D;&#x430;&#x43D;&#x43E;&#x441; (&#x440;&#x456;&#x43A;&#x438;)</trd>
  </trdgrp>
</drv>
</art>
