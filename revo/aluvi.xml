<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aluvi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="aluvi.0o">
  <kap><tld/>o</kap>
  <snc mrk="aluvi.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Tavolo da tero alportita kaj delasita de la akvoj.
    </dif>
  <trd lng="be">&c_a;&c_l;&c_ju;&c_v;&c_ib;&c_j;</trd>
  <trdgrp lng="de">
      <trd>Anschwemmung</trd>,
      <trd>Alluvialboden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>alluvium</trd>,
    <trd>alluvion</trd>
  </trdgrp>
  <trd lng="es">aluvi&oacute;n</trd>
  <trd lng="fr">alluvion</trd>
  <trdgrp lng="hu">
    <trd>folyami <ind>lerak&oacute;d&aacute;s</ind></trd>,
    <trd>alluvium</trd>
  </trdgrp>
  <trd lng="nl">alluvium</trd>
  <trd lng="pl">aluwium</trd>
  <trd lng="ru">&c_a;&c_l;&c_l;&c_ju;&c_v;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>naplavenina</trd>,
    <trd>n&aacute;nos</trd>
  </trdgrp>
  </snc>

  <snc mrk="aluvi.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="holocen.0o.GEOL">holoceno</ref>
    
    <ref tip="vid" cel="diluvi.0o">diluvio</ref>
    <trd lng="pl">aluwium</trd>
  </snc>
</drv>
</art>
