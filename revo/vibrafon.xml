<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vibrafon</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="vibrafon.0o">
  <kap><tld/>o</kap>
  <snc mrk="vibrafon.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.frap0o.MUZ">Frapinstrumento</ref>
      kun serio de metalaj, batile sonigataj lamenoj
      super resonaj helichavaj tuboj,
      kiuj longigas kaj vibrigas la sonoron:
      <ekz>
        15-minuta peco de piano, gongo kaj <tld/>o
        perfekte transdonas la etoson de Indonezio
        <fnt>
          <aut>K. Offutt</aut>,
          <vrk>Lou Silver Harrison</vrk>,
          <lok><url
          ref="http://www.esperanto-chicago.org/newsletters/06_07.pdf">
          Saluton!, 2007-06 (6)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="marimb.0o.MUZ">marimbo</ref>
    </refgrp>
  </snc>
  <trd lng="fr">vibraphone</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>