<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vibrafon.xml,v 1.9 2023/11/12 15:25:21 revo Exp $">
<kap>
  <rad>vibrafon</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="vibrafon.0o">
  <kap><tld/>o</kap>
  <snc mrk="vibrafon.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.frap0o" lst="voko:frapinstrumentoj">Frapinstrumento</ref>
      kun serio de metalaj, batile sonigataj lamenoj
      super resonaj helichavaj tuboj,
      kiuj da&ubreve;rigas kaj vibrigas la sonoron:
      <ekz>
        15-minuta peco de piano, gongo kaj <tld/>o
        perfekte transdonas la etoson de Indonezio
        <fnt>
          <aut>K. Offutt</aut>,
          <vrk>Lou Silver Harrison</vrk>,
          <lok><url ref="http://www.esperanto-chicago.org/newsletters/06_07.pdf">
          Saluton!, 2007-06 (6)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="marimb.0o.MUZ">marimbo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_b;&c_r;&c_a;&c_f;&c_o;&c_n;</trd>
  <trd lng="ca">vibr&agrave;fon</trd>
  <trd lng="cs">vibrafon</trd>
  <trd lng="fr">vibraphone</trd>
  <trd lng="pl">wibrafon</trd>
  <trd lng="sk">vibraf&#xF3;n</trd>
  <trd lng="uk">&c_v;&c_ib;&c_b;&c_r;&c_a;&c_f;&#x43E;&#x43D;</trd>
</drv>
</art>

