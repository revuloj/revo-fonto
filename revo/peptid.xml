<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: peptid.xml,v 1.8 2024/02/17 06:43:06 revo Exp $">
<kap>
  <rad>peptid</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="peptid.0o">
  <kap><tld/>o</kap>
  <snc mrk="peptid.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="polimer.0o.molekulo">Polimero</ref>
      el <ref tip="prt" cel="acid.amin0o.FIZL">aminacidoj</ref>
      ligitaj per <tld/>aj ligoj:
      <ekz>
        li estis la unua, kiu proponis uzi proteinojn kaj 
        <tld/>ojn de insektoj kiel molekulojn-sondilojn por 
        esplori la homajn proteinojn
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Konstantin_Efetov">
          Konstantin Efetov</url>, 2013-10-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_p;&c_t;&c_y;&c_d;</trd>
  <trd lng="bg">&c_p;&c_je;&c_p;&c_t;&c_i;&c_d;</trd>
  <trd lng="ca">p&egrave;ptid</trd>
  <trd lng="da">peptid</trd>
  <trd lng="de">Peptid</trd>
  <trd lng="en">peptide</trd>
  <trd lng="es">p&eacute;ptido</trd>
  <trd lng="fr">peptide</trd>
  <trd lng="hu">peptid</trd>
  <trd lng="it">peptide</trd>
  <trd lng="nl">peptide</trd>
  <trd lng="pl">peptyd</trd>
  <trd lng="ro">peptid&abreve;</trd>
  <trd lng="uk">&#x43F;&c_je;&#x43F;&c_t;&c_i;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#x80BD; <pr>t&#xE0;i</pr></trd>,
    <trd>&#x7E2E;&#x6C28;&#x9178; <pr>su&#x14D;&#x101;nsu&#x101;n</pr></trd>,
    <trd>&#x7F29;&#x6C28;&#x9178; <pr>su&#x14D;&#x101;nsu&#x101;n</pr></trd>,
    <trd>&#x5454; <pr>t&#x101;i</pr></trd>
  </trdgrp>
</drv>
</art>

