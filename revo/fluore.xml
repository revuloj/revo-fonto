<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fluoresk</rad>/i
</kap>
<drv mrk="fluore.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">FOT</uzo>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Lumi dum la prilumigo. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>fluoreszieren</trd>,
     <trd>schillern</trd></trdgrp>
  <trd lng="ru">&c_f;&c_l;&c_u;&c_o;&c_r;&c_je;&c_s;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="fluore.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta econ <tld/>i. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>fluoreszierend</trd>,
     <trd>Fluoreszenz-</trd>,
     <trd>Leuchtstoff-</trd></trdgrp>
</drv>
<drv mrk="fluore.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Propreco <tld/>i:
      <ekz><tld/>eco estas nur dum prilumiga fosforeskeco.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fosfor1.0i">fosforeski</ref>,
      <ref cel="reflek.0i">reflekti</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Fluoreszenz</trd>
  <trd lng="ru">&c_f;&c_l;&c_u;&c_o;&c_r;&c_je;&c_s;&c_c;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
</drv>
</art>
