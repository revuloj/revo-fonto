<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fluore.xml,v 1.18 2022/04/27 17:19:11 revo Exp $">
<kap>
  <rad>fluoresk</rad>/i
</kap>
<drv mrk="fluore.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">FOT</uzo>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="lum.0i.FIZ">Lumi</ref> dum la prilumigo a&ubreve; tuj post 
      &gcirc;i dum tre mallonga tempo.
    </dif>
    <ref tip="super" cel="lumine.0i">lumineski</ref>
    <refgrp tip="vid">
      <ref cel="fosfor1.0i">fosforeski</ref>,
      <ref cel="reflek.0i">reflekti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ju;&c_a;&c_r;&c_e;&c_s;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>fluoreskovat</trd>,
    <trd>sv&ecaron;t&eacute;lkovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fluoreszieren</trd>,
    <trd>schillern</trd>
  </trdgrp>
  <trd lng="en">fluoresce</trd>
  <trd lng="fr">&ecirc;tre <ind>fluorescent</ind></trd>
  <trd lng="nl">fluoresceren</trd>
  <trd lng="pt">fluorescer</trd>
  <trd lng="ru">&c_f;&c_l;&c_u;&c_o;&c_r;&c_je;&c_s;&c_c;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>fluoreskova&tcaron;</trd>,
    <trd>svetielkova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x444;&#x43B;&#x443;&#x43E;&#x440;&#x435;&#x441;&#x446;&#x456;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x444;&#x43B;&#x44E;&#x43E;&#x440;&#x435;&#x441;&#x446;&#x456;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="fluore.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta econ <tld/>i:
      <ekz>
        oran&gcirc;e <tld/>a ve&scirc;to
        <fnt>
          <bib>MD08</bib>
          <vrk>Israelaj rigardoj al la okupado</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>an lampo</ind>;
        <trd lng="de">Leuchtstofflampe</trd>
        <trd lng="en">fluorescent lamp</trd>
        <trd lng="fr">tube fluorescent</trd>
        <trd lng="ru">&c_l;&c_ju;&c_m;&c_i;&c_n;&c_je;&c_s;&c_c;&c_je;&#x301;&c_n;&c_t;&c_n;&c_a;&c_ja; &c_l;&c_a;&#x301;&c_m;&c_p;&c_a;</trd>
      </ekz>
      <ekz>
        &ccirc;u mi tuj forigu &ccirc;iujn ampolojn kaj ekuzu <klr>[&#x2026;]</klr> <tld/>ajn lampojn en
        mia domo, a&ubreve; &ccirc;u el ekologia vidpunkto estas pli favore anstata&ubreve;igi ilin nur
        kiam ili difekti&gcirc;as
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2014/011361.php">Verdaj mitoj kaj herezoj</url></vrk>
          <lok>2014</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ju;&c_a;&c_r;&c_e;&c_s;&c_c;&c_e;&c_n;&c_t;&c_n;&c_y;</trd>
  <trd lng="cs">sv&ecaron;t&eacute;lkuj&iacute;c&iacute;</trd>
  <trdgrp lng="de">
    <trd>fluoreszierend</trd>,
    <trd>Fluoreszenz-</trd>,
    <trd>Leuchtstoff-</trd>
  </trdgrp>
  <trd lng="en">fluorescent</trd>
  <trd lng="es">fluorescente</trd>
  <trd lng="fr">fluorescent</trd>
  <trd lng="nl">fluorescerend</trd>
  <trd lng="ru">&c_f;&c_l;&c_ju;&c_o;&c_r;&c_je;&c_s;&c_c;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">svetielkuj&uacute;ci</trd>
</drv>

<drv mrk="fluore.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Propreco <tld/>i:
      <ekz>
        <tld/>o estas nur dum-prilumiga fosforesko.
      </ekz>
    </dif>
    <ref tip="super" cel="lumine.0o">luminesko</ref>
    <refgrp tip="vid">
      <ref cel="fosfor1.0i">fosforesko</ref>,
      <ref cel="reflek.0i">reflekti</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ju;&c_a;&c_r;&c_e;&c_s;&c_c;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>fluorescence</trd>,
    <trd>sv&ecaron;t&eacute;lkov&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Fluoreszenz</trd>
  <trd lng="en">fluorescence</trd>
  <trd lng="es">fluorescencia</trd>
  <trd lng="fr">fluorescence</trd>
  <trd lng="nl">fluorescentie</trd>
  <trd lng="ru">&c_f;&c_l;&c_u;&c_o;&c_r;&c_je;&c_s;&c_c;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">svetielkovanie</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x432;&#x456;&#x442;&#x456;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x444;&#x43B;&#x443;&#x43E;&#x440;&#x435;&#x441;&#x446;&#x435;&#x43D;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x444;&#x43B;&#x44E;&#x43E;&#x440;&#x435;&#x441;&#x446;&#x435;&#x43D;&#x446;&#x456;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
