<art mrk="$Id: epitaf.xml,v 1.22 2023/11/07 20:34:11 revo Exp $">
<kap>
  <rad>epitaf</rad>/o
  <fnt>Z</fnt></kap>
<drv mrk="epitaf.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tomba <ref tip="super" cel="skrib.sur0o">surskribo</ref>:
      <ekz>
        la <tld/>o de <nom>Seikilos</nom> estas fama pro tio, ke &gcirc;i estas la plej malnova ekzemplo
        de plena muzika kompona&jcirc;o <klr>[&#x2026;]</klr> en la okcidenta mondo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34171">Epitafo de Seikilos</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la pinto de la simfoniado estas la 13-minuta kron-kompona&jcirc;o &leftquot;<tld lit="E"/>o&#x201D;
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;lo Mo&jcirc;ajev kaj Jefim Zajdman</aut>
          <vrk><url ref="&Monato;/2005/008571.php">Kantoj belaj, e&ccirc; simfoniaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="epigra.0o.ARKE">epigrafo<sncref/></ref>
  </snc>
  <adm>
    Devas ekzisti anka&ubreve; snc BELE. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_t;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Grabplatte mit Inschrift</trd>,
    <trd>Epitaph</trd>
  </trdgrp>
  <trd lng="en">epitaph</trd>
  <trd lng="es">epitafio</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_n;&f_g;&zwnj;&a_n;&a_b;&a_sh;&a_t;&a_h;&a_hamza_sure;
    &a_m;&a_z;&a_A;&a_r;</trd>,
    <trd>&a_n;&a_w;&a_sh;&a_t;&a_h;&a_hamza_sure; <ind>&a_s;&a_n;&f_g;
    &a_q;&a_b;&a_r;</ind></trd>
  </trdgrp>
  <trd lng="fr">&eacute;pitaphe</trd>
  <trd lng="hu">s&iacute;rfelirat</trd>
  <trd lng="ja">&#x5893;&#x7891;&#x9298;  <pr>&#x307C;&#x3072;&#x3081;&#x3044;</pr></trd>
  <trd lng="nl">grafschrift</trd>
  <trdgrp lng="pl">
    <trd>epitafium</trd>,
    <trd>napis <ind>nagrobny</ind></trd>
  </trdgrp>
  <trd lng="pt">epit&aacute;fio</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_t;&c_a;&c_f;&c_i;&c_ja;</trd>
  <trd lng="sk">n&aacute;hrobn&yacute; n&aacute;pis</trd>
  <trdgrp lng="cs">
    <trd>epitaf</trd>,
    <trd>n&aacute;hrobn&iacute; n&aacute;pis</trd>
  </trdgrp>
  <trd lng="uk">&#x435;&#x43F;&#x456;&#x442;&#x430;&#x444;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x5893;&#x5FD7;&#x94ED; <pr>m&#xF9;zh&#xEC;m&#xED;ng</pr></trd>
</drv>
</art>
