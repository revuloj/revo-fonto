<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: etil.xml,v 1.15 2019/01/12 15:10:13 revo Exp $">
<kap>
  <rad>etil</rad>/o
</kap>
<drv mrk="etil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <frm>C<sub>2</sub>H<sub>5</sub></frm>; <ref tip="super" cel="radika1.0o.KEM">radikalo</ref>,
      kiu kombini&gcirc;as en multaj organikaj kompona&jcirc;oj:
      <ekz>
        <tld/>alkoholo, estigita dum la natura fermentado
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007033.html"
              >Centra Ameriko: Nepre &gcirc;ia nomo estas Meksiko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_y;&c_l;</trd>
  <trd lng="ca">etil</trd>
  <trd lng="cs">etyl</trd>
  <trdgrp lng="de">
     <trd>&Auml;thyl</trd>,
     <trd>Ethyl<klr> (nur fachl.)</klr></trd>
  </trdgrp>
  <trd lng="en">ethyl</trd>
  <trd lng="es">etilo</trd>
  <trd lng="fr">&eacute;thyle</trd>
  <trd lng="hu">etilgy&ouml;k</trd>
  <trd lng="pl">etyl</trd>
  <trdgrp lng="pt">
    <trd>alquil</trd>,
    <trd>etil</trd>,
    <trd>etila</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_t;&c_i;&c_l;</trd>
  <trd lng="sk">etyl</trd>
</drv>
</art>
