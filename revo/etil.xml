<art mrk="$Id: etil.xml,v 1.21 2023/11/07 20:34:12 revo Exp $">
<kap>
  <rad>etil</rad>/o
</kap>
<drv mrk="etil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <frm>C<sub>2</sub>H<sub>5</sub></frm>; <ref tip="super" cel="radika1.0o.KEM">radikalo</ref>,
      kiu kombini&gcirc;as en multaj organikaj kompona&jcirc;oj:
      <ekz>
        <tld/>alkoholo, estigita dum la natura fermentado
        <fnt>
          <aut>Fidel Figueroa</aut>
          <vrk><url ref="&Monato;/2000/007033.php">Nepre &gcirc;ia nomo estas Meksiko</url></vrk>
          <bib>Monato</bib>
          <lok>2000/10, p. 14</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_y;&c_l;</trd>
  <trd lng="ca">etil</trd>
  <trd lng="cs">etyl</trd>
  <trdgrp lng="de">
     <trd>&Auml;thyl</trd>,
     <trd>Ethyl<klr> (nur fachl.)</klr></trd>
  </trdgrp>
  <trd lng="en">ethyl</trd>
  <trd lng="es">etilo</trd>
  <trd lng="fr">&eacute;thyle</trd>
  <trd lng="hu">etilgy&ouml;k</trd>
  <trd lng="ja">&#x30A8;&#x30C1;&#x30EB;&#x57FA;  <pr>&#x30A8;&#x30C1;&#x30EB;&#x3082;&#x3068;</pr></trd>
  <trd lng="pl">etyl</trd>
  <trdgrp lng="pt">
    <trd>alquil</trd>,
    <trd>etil</trd>,
    <trd>etila</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_t;&c_i;&c_l;</trd>
  <trd lng="sk">etyl</trd>
  <trd lng="uk">&c_je;&c_t;&c_i;&#x43B;</trd>
  <trd lng="zh">&#x4E59;&#x57FA; <pr>y&#x1D0;j&#x12B;</pr></trd>
</drv>
