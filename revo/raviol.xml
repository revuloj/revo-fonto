<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: raviol.xml,v 1.12 2023/06/26 10:35:40 revo Exp $">
<kap><rad>raviol</rad>/oj</kap>
<drv mrk="raviol.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Itala speco de <ref tip="super" cel="pelmen.0oj">pelmenoj</ref>:
      <ekz>
        menciindas: <nac>pierogi</nac> &dash; pola versio de varenikoj, <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3343">Pollando</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        bovlo da pola klara (ru&gcirc;a) bar&ccirc;o servata &ccirc;efe dum Kristnaska vesperman&gcirc;o kun
        <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=124731">Bar&ccirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="pirog1.0o.KUI">pirogo</ref>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_v;&c_ib;&c_jo;&c_l;&c_ib;</trd>
  <trd lng="ca">ravioli</trd>
  <trdgrp lng="cs">
    <trd>raviol</trd>,
    <trd>raviola</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ravioli</trd>,
    <trd>Teigtaschen</trd>,
    <trd>Maultaschen</trd>
  </trdgrp>
  <trd lng="en">ravioli</trd>
  <trd lng="es">ravioli</trd>
  <trdgrp lng="he">
    <trd>&kaf;&yod;&samekh;&fnun;</trd>,
    <trd>&resh;&bet;&yod;&vav;&lamed;&yod;</trd>
  </trdgrp>
  <trd lng="it">ravioli</trd>
  <trd lng="nl">ravioli</trd>
  <trd lng="pl">ravioli</trd>
  <trd lng="ru">&c_r;&c_a;&c_v;&c_i;&c_o;&c_l;&c_i;</trd>
  <trd lng="sk">raviola</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_a;&c_v;&c_ib;&#x43E;&#x43B;&c_ib;</trd>,
    <trd>&c_v;&c_u;&c_sh;&#x43A;&c_a;</trd>,
    <trd>&#x43F;&c_je;&#x43B;&#x44C;&#x43C;&c_je;&#x43D;&c_ib;</trd>
  </trdgrp>
</drv>
</art>

