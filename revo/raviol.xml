<art mrk="$Id$">
<kap><rad>raviol</rad>/oj</kap>
<drv mrk="raviol.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Itala speco de <ref tip="super" cel="pelmen.0oj">pelmenoj</ref>.
    </dif>
  </snc>
  <trd lng="en">ravioli</trd>
  <trdgrp lng="he">
    <trd>&kaf;&yod;&samekh;&fnun;</trd>,
    <trd>&resh;&bet;&yod;&vav;&lamed;&yod;</trd>
  </trdgrp>
  <trd lng="it">ravioli</trd>
  <trd lng="nl">ravioli</trd> 
  <trd lng="pl">ravioli</trd>
  <trd lng="ru">&c_r;&c_a;&c_v;&c_i;&c_o;&c_l;&c_i;</trd>
</drv>
</art>

