<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: feldsp.xml,v 1.18 2022/03/20 10:19:21 revo Exp $">
<kap>
<rad>feldspat</rad>/o
</kap>
<drv mrk="feldsp.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="minera.0o">Mineralo</ref>
      konsistanta el aluminia, kalcia kaj alkalaj silikatoj,
      trovebla en granitecaj rokoj:
      <ekz>
        en Burundo trovi&gcirc;as gravaj minejoj de <tld/>o, kaolino, fosforo, metaloj de la platina grupo, kvarcitoj, rarateraj metaloj, vanado, kalko&scirc;tono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=447">Burundo</url></lok>
        </fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_v;&c_y; &c_sh;&c_p;&c_a;&c_t;</trd>
  <trd lng="bg">&c_F;&c_je;&c_l;&c_d;&c_sh;&c_p;&c_a;&c_t;</trd>
  <trd lng="cs">&zcaron;ivec</trd>
  <trd lng="de">Feldspat</trd>
  <trd lng="en">feldspar</trd>
  <trd lng="es">feldespato</trd>
  <trdgrp lng="fa">
    <trd>&a_f;&a_l;&a_d;&a_s;&f_p;&a_A;&a_t;</trd>,
    <trd>&a_f;&a_l;&a_d;&a_s;&f_p;&a_A;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">feldspath</trd>
  <trd lng="hu">f&ouml;ldp&aacute;t</trd>
  <trd lng="nl">veldspaat</trd>
  <trd lng="pt">feldspato</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je;&c_v;&c_o;&c_j; &c_sh;&c_p;&c_a;&c_t;</trd>
  <trd lng="sk">&zcaron;ivec</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43B;&#x44C;&#x43E;&#x432;&#x438;&#x439; &#x448;&#x43F;&#x430;&#x442;</trd>
</drv>
</art>
