<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: feldsp.xml,v 1.23 2025/03/03 17:12:28 revo Exp $">
<kap>
<rad>feldspat</rad>/o
<fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="feldsp.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc mrk="feldsp.0o.MIN">
    <dif>
      <ref tip="super" cel="minera.0o">Mineralo</ref>
      konsistanta el aluminia, kalcia kaj alkalaj silikatoj,
      trovebla en granitecaj rokoj:
      <ekz>
        granito <klr>[&#x2026;]</klr> el eroj da kvarco (sablo), <tld/>o kaj glimo
        <fnt>
          <vrk><url ref="http://www.eventoj.hu/steb/vortaroj/argilajxo.htm">La Argilo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en Burundo trovi&gcirc;as gravaj minejoj de <tld/>o, kaolino, fosforo <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=447">Burundo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="spat.0o.MIN">spato</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_v;&c_y; &c_sh;&c_p;&c_a;&c_t;</trd>
  <trd lng="bg">&c_F;&c_je;&c_l;&c_d;&c_sh;&c_p;&c_a;&c_t;</trd>
  <trd lng="cs">&zcaron;ivec</trd>
  <trd lng="de">Feldspat</trd>
  <trd lng="en">feldspar</trd>
  <trd lng="es">feldespato</trd>
  <trdgrp lng="fa">
    <trd>&a_f;&a_l;&a_d;&a_s;&f_p;&a_A;&a_t;</trd>,
    <trd>&a_f;&a_l;&a_d;&a_s;&f_p;&a_A;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">feldspath</trd>
  <trd lng="hu">f&ouml;ldp&aacute;t</trd>
  <trd lng="ja">&#x9577;&#x77F3;  <pr>&#x3061;&#x3087;&#x3046;&#x305B;&#x304D;</pr></trd>
  <trd lng="nl">veldspaat</trd>
  <trd lng="pt">feldspato</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je;&c_v;&c_o;&c_j; &c_sh;&c_p;&c_a;&c_t;</trd>
  <trd lng="sk">&zcaron;ivec</trd>
  <trd lng="uk">&c_p;&c_o;&c_l;&c_mol;&c_o;&c_v;&c_i;&c_j; &c_sh;&c_p;&c_a;&c_t;</trd>
  <trd lng="zh">&#x9577;&#x77F3; <pr>ch&aacute;ngsh&iacute;</pr></trd>
</drv>
</art>
