<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kodici.xml,v 1.15 2018/11/15 14:10:15 revo Exp $">
<kap>
  <ofc>2</ofc><rad>kodicil</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kodici.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Aldona <ref tip="super" cel="paragr.0o.KLA">paragrafo</ref>
      kompletiganta, modifanta a&ubreve; nuliganta
      <ref tip="malprt" cel="testam.0o.JUR">testamento<sncref/></ref>:
      <ekz>
        <tld/>o al la kontrakto indikis ke la Soveta Registaro povas
        havi
        la filmon libere por prezenti &gcirc;in ene de Sovetunio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=267143">Sergej
          Mi&hcirc;ajlovi&ccirc; Ejzen&scirc;tejn</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_d;&c_y;&c_c;&c_y;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_d;&c_a;&c_t;&c_a;&c_k; &c_d;&c_a; &c_t;&c_e;&c_s;&c_t;&c_a;&c_m;&c_e;&c_n;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">codicil</trd>
  <trdgrp lng="cs">
    <trd>kodicil</trd>,
    <trd>z&aacute;v&ecaron;tek</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kodizill</trd>,
     <trd>Testamentsnachtrag</trd>
  </trdgrp>
  <trd lng="en">codicil</trd>
  <trd lng="es">codicilo</trd>
  <trd lng="fr">codicille</trd>
  <trd lng="nl">codicil</trd>
  <trd lng="pl">kodycyl</trd>     
  <trd lng="ro">codicil</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_l;&c_je;&c_d;&c_n;&c_ja;&c_ja; &c_v;&c_o;&c_l;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_je;&c_n;&c_i;&c_je; &c_k; &c_z;&c_a;&c_v;&c_je;&c_shch;&c_a;&c_n;&c_i;&c_ju;</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;vetn&aacute; dolo&zcaron;ka</trd>
</drv>
</art>
