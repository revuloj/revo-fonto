<art mrk="$Id: kazein.xml,v 1.19 2023/12/03 08:00:25 revo Exp $">
<kap>
  <rad>kazein</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kazein.0o">
  <kap><tld/>o</kap>
  <snc mrk="kazein.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="vid" cel="albumi.0o">Albumina</ref> substanco de la
      <ref tip="malprt" cel="lakt.0o">lakto</ref>:
      <ekz>
        <tld/>o, la proteino de lakto, evidente enhavas &ccirc;iujn
        amino-acidojn necesajn por la konstruado de la
        proteinoj de infaneto
        <fnt><aut>W. F. Rolt</aut>, <vrk>El la Polvo de la Tero</vrk>, 
        <lok>1967</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="legumi.0o">legumino</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_e;&c_ib;&c_n;</trd>
  <trd lng="ca">case&iuml;na</trd>
  <trdgrp lng="cs">
    <trd>kasein</trd>,
    <trd>kazein</trd>,
    <trd>s&yacute;rovina</trd>
  </trdgrp>
  <trd lng="de">Kasein</trd>
  <trd lng="en">casein</trd>
  <trd lng="es">case&iacute;na</trd>
  <trd lng="fa">&f_k;&a_A;&a_z;&a_y_hamza;&f_y;&a_n;</trd>
  <trd lng="fr">cas&eacute;ine</trd>
  <trd lng="hu">kazein</trd>
  <trd lng="ja">&#x30AB;&#x30BC;&#x30A4;&#x30F3;</trd>
  <trd lng="nl">case&iuml;ne</trd>
  <trd lng="pl">kazeina</trd>     
  <trd lng="ro">cazein&abreve;</trd>
  <trd lng="ru">&c_k;&c_a;&c_z;&c_je;&c_i;&c_n;</trd>
  <trd lng="sk">kaze&iacute;n</trd>
  <trd lng="uk">&#x43A;&#x430;&#x437;&#x435;&#x457;&#x43D;</trd>
  <trd lng="zh">&#x4E7E;&#x916A;&#x7D20; <pr>g&#x101;nl&#xE0;os&#xF9;</pr></trd>
</drv>
