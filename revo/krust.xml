<kap>
  <ofc>*</ofc>
  <rad>krust</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="krust.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="krust.0o.KOMUNE">
    <dif>
      Ekstera parto de io, kiu malmoli&gcirc;is, farante <ref tip="super" cel="sxel.0o.KOMUNE">&scirc;elon</ref>:
      <ekz>
        <tld/>o de pano, kuko, vundo, kankro, ovo;
      </ekz>
      <ekz>
        a&ccirc;a ne&gcirc;o por skiado <klr>[&#8230;]</klr> la <tld/>o ankora&ubreve; ne portas por piediro
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo <klr>[&#8230;]</klr> formigas dikan <tld/>on interne de kaldronoj
        <fnt>
          <bib>KrM</bib>
          <lok>5. Petro Kornojn Tenas, Pa&ubreve;lo Lakton Prenas.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la valoj <klr>[&#8230;]</klr> konsistigas grandegan fendon en la marsa <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2010/009989.php">Voja&gcirc;o unudirekta</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;afa bulfroma&gcirc;o <klr>[&#8230;]</klr> devas iom post iom maturi&gcirc;i <klr>[&#8230;]</klr>, poste
        oni devas forigi la supran <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2013/011027.php">Oro blanka</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiom da polvo desolvi&gcirc;as de la kaldron<tld/>o
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 43</lok>
        </fnt>!
      </ekz>
      <ekz>
        iuj aviadiloj dronis en maro, &ccirc;ar la pilotoj opiniis, ke ili estas sur la &ccirc;ielo,
        kiu estas spegulata de la glaci<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2013/011057.php">Nerezisteble altira poluso</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nun la supra&jcirc;a <tld/>o estis tratran&ccirc;ita, kaj montri&gcirc;is la kor&scirc;ira vundo
        interna, ke &scirc;i amis Pa&ubreve;lon ne estis teatre
        <fnt>
          <bib>ChC</bib>
          <lok>5</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="karapa.0o.ZOO">karapaco<sncref/></ref>,
      <ref cel="tavol.0o">tavolo</ref>,
      <ref cel="surfac.0o">surfaco</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_r;&c_y;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>corfa</trd>,
    <trd>crosta</trd>,
    <trd>escor&ccedil;a</trd>,
    <trd>capa</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>k&uring;ra</trd>,
    <trd>k&uring;rka</trd>,
    <trd>strup</trd>,
    <trd>ztvrdl&yacute; povrch</trd>,
    <trd>&scaron;kraloup</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kruste</trd>,
    <trd>Schorf</trd>,
    <trd>Grind</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>crust</trd>,
    <trd>casing</trd>,
    <trd>deposit <klr>(on a surface)</klr></trd>,
    <trd>scale <klr>(on a surface)</klr></trd>,
    <trd>fur <klr>(on a surface)</klr></trd>,
    <trd>rind</trd>,
    <trd>scab <klr>(on a wound)</klr></trd>
  </trdgrp>
  <trd lng="fr">cro&ucirc;te</trd>
  <trdgrp lng="hu">
    <trd>h&eacute;j</trd>,
    <trd>k&eacute;reg</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#30828;&#12356;&#30382;  <pr>&#12363;&#12383;&#12356;&#12363;&#12431;</pr></trd>,
    <trd>&#22806;&#30382;  <pr>&#12364;&#12356;&#12402;</pr></trd>,
    <trd>&#29289;&#12398;&#30828;&#12356;&#34920;&#38754;  <pr>&#12406;&#12388;&#12398;&#12363;&#12383;&#12356;&#12402;&#12423;&#12358;&#12417;&#12435;</pr></trd>,
    <trd>&#27579;  <pr>&#12363;&#12356;</pr></trd>,
    <trd>&#12363;&#12373;&#12406;&#12383;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>korst</trd>,
    <trd>schaal<klr tip="ind"> (v.dieren)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>skorupa</trd>,
    <trd>strup</trd>,
    <trd>sk&oacute;rka <klr>(chleba)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>crosta <klr tip="amb">(&ocirc;)</klr></trd>,
    <trd>casca <klr tip="amb">(de p&atilde;o, etc.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>coaj&abreve;</trd>,
    <trd>r&acirc;ie</trd>,
    <trd>crusta <klr>(de p&acirc;ine)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">m&racute;tve tkanivo</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_ib;&c_r;&c_k;&c_a;</trd>,
    <trd><klr>(&c_h;&c_l;&c_ib;&c_b;&c_a; &c_ib; &c_t;.&c_p;.)</klr> &c_s;&c_k;&c_o;&c_r;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_r;&c_i;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_k;&c_u;&c_r;&c_i;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_u;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#32080;&#27580; <pr>ji&eacute;k&eacute;</pr></trd>,
    <trd>&#32467;&#22771; <pr>ji&eacute;k&eacute;</pr></trd>,
    <trd>&#30002;&#22771; <pr>ji&#462;qi&agrave;o</pr></trd>,
    <trd>&#30002;&#27580; <pr>ji&#462;qi&agrave;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="krust.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="krust.0ulo.ZOO">
    <dif>
      Iu el <ref tip="malprt" cel="krust.0uloj"><tld/>uloj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_k;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_a;&c_ja; &c_zh;&c_y;&c_v;&c_jo;&c_l;&c_a;</trd>
  <trd lng="cs">kor&yacute;&scaron;</trd>
  <trd lng="en">crustacean</trd>
  <trd lng="fr">crustac&eacute;</trd>
  <trd lng="pl">skorupiak</trd>
  <trdgrp lng="ro">
    <trd>crustaceu</trd>,
    <trd>rac</trd>
  </trdgrp>
  <trd lng="sk">k&ocirc;rovec</trd>
</drv>

<drv mrk="krust.0uloj">
  <kap><tld/>uloj</kap>
  <snc mrk="krust.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Bestoj kovritaj de karapaco
      kaj spirantaj per brankoj, plej ofte vivantaj en akvo,
      <ref tip="lst" cel="grup.0o.BIO" lst="voko:zoologiaj_grupoj">grupo</ref> de <ref tip="malprt" cel="artik.0uloj">artikuloj</ref>
      (<trd lng="la">Crustacea</trd>), al kiu apartenas i.a. la klaso
      <trd lng="la">Malacostraca</trd>:
      <ekz>
        <tld/>ulo <klr>[&#8230;]</klr> havas ekstreme kompleksan bu&scirc;on
        konsistantan de multaj partoj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 13a</lok></fnt>;
      </ekz>
      <ekz>
        a&ubreve;ko <klr>[&#8230;]</klr> nutras sin &ccirc;efe per fi&scirc;oj, <klr>[&#8230;]</klr> anka&ubreve; kelkaj
        krustuloj kaj marvermoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18811">A&ubreve;ko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="kankr.0o">kankro</ref>,
      <ref cel="krab.0o">krabo</ref>,
      <ref cel="omar.0o">omaro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_k;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;</trd>
  <trd lng="ca">crustacis</trd>
  <trdgrp lng="de">
    <trd>Krebstiere</trd>,
    <trd>Krustentiere</trd>
  </trdgrp>
  <trd lng="en">Crustacea</trd>
  <trd lng="fr">crustac&eacute;s</trd>
  <trd lng="hu">r&aacute;kok<klr tip="amb"> (oszt&aacute;ly)</klr></trd>
  <trd lng="ja">&#30002;&#27579;&#39006;  <pr>&#12371;&#12358;&#12363;&#12367;&#12427;&#12356;</pr></trd>
  <trd lng="nl">schaaldieren</trd>
  <trd lng="pl">skorupiaki</trd>
  <trd lng="ro">crustacee</trd>
  <trd lng="ru">&c_r;&c_a;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
  <trd lng="sk">k&ocirc;rovce</trd>
  <trd lng="uk">&c_r;&c_a;&c_k;&c_o;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#30002;&#22771;&#21160;&#29289; <pr>ji&#462;qi&agrave;od&ograve;ngw&ugrave;</pr></trd>,
    <trd>&#30002;&#27580;&#21205;&#29289; <pr>ji&#462;qi&agrave;od&ograve;ngw&ugrave;</pr></trd>,
    <trd>&#30002;&#22771;&#31867; <pr>ji&#462;qi&agrave;ol&egrave;i</pr></trd>,
    <trd>&#30002;&#27580;&#39006; <pr>ji&#462;qi&agrave;ol&egrave;i</pr></trd>,
    <trd>&#30002;&#22771;&#31867; <pr>ji&#462;k&eacute;l&egrave;i</pr></trd>,
    <trd>&#30002;&#27580;&#39006; <pr>ji&#462;k&eacute;l&egrave;i</pr></trd>,
    <trd>&#30002;&#22771;&#20122;&#38376; <pr>ji&#462;k&eacute;y&agrave;m&eacute;n</pr></trd>,
    <trd>&#30002;&#27580;&#20126;&#38272; <pr>ji&#462;k&eacute;y&agrave;m&eacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="krust.en0i">
  <kap>en<tld/>i<fnt>Z</fnt></kap>
  <snc mrk="krust.en0i.inkrusti">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="inkrus.0i.BELA">inkrusti</ref>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_k;&c_r;&c_u;&c_s;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">incrustar</trd>
  <trd lng="cs">inkrustovat</trd>
  <trdgrp lng="en">
    <trd>encrust</trd>,
    <trd>incrust</trd>
  </trdgrp>
  <trd lng="fr">incruster</trd>
  <trd lng="nl">omkorsten</trd>
  <trd lng="pl">inkrustowa&cacute;</trd>
  <trd lng="ro">&icirc;ncrusta</trd>
  <trd lng="ru">&c_i;&c_n;&c_k;&c_r;&c_u;&c_s;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">inkrustova&tcaron;</trd>
  <trd lng="uk">&c_ib;&c_n;&c_k;&c_r;&c_u;&c_s;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="krust.dekpiedaj0uloj">
  <kap>dekpiedaj <tld/>uloj</kap>
  <snc mrk="krust.dekpiedaj0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> de <ref tip="malprt" cel="krust.0uloj"><tld/>uloj</ref>
      kun dek piedoj (<trd lng="la">Decapoda</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_je;&c_s;&c_ja;&c_c;&c_ib;&c_n;&c_o;&c_g;&c_ib;&c_ja; <ind>&c_r;&c_a;&c_k;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;</ind></trd>
  <trd lng="ca">crustacis dec&agrave;podes</trd>
  <trd lng="de">Zehnfu&szlig;krebse</trd>
  <trd lng="en">decapod</trd>
  <trd lng="fr">d&eacute;capodes<klr> (subst.)</klr></trd>
  <trd lng="hu">t&iacute;zl&aacute;b&uacute; <ind>r&aacute;kok</ind></trd>
  <trd lng="nl">tienpootkreeften</trd>
  <trd lng="pl">dziesi&eogonek;cionogi</trd>
  <trd lng="ro">decapoda</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_i;&c_n;&c_o;&c_g;&c_i;&c_je; &c_r;&c_a;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
  <trd lng="zh">&#21313;&#36275;&#30446; <pr>sh&iacute;z&uacute;m&ugrave;</pr></trd>
</drv>

<drv mrk="krust.ter0o">
  <kap>ter<tld/>o</kap>
  <snc mrk="krust.ter0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      <ref tip="dif" cel="sxel.ter0o.GEOL">Ter&scirc;elo</ref>:
      <ekz>
        vulkanoj tipe esti&gcirc;as tie, kie la substanco de la ter<tld/>o estas premata de
        kontinent-deloki&gcirc;oj en la profundo
        <fnt><aut>Evgeni Georgiev</aut>:
          <vrk><url ref="&Monato;/2001/007234.html">Vulkanoj</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/02, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        siluroj <klr>[&#8230;]</klr> kaptas la frekvencojn de rompi&gcirc;onta ter<tld/>o kaj pro
        tio reagas al la elektro eligata dum tertremo
        <fnt>
          <bib>Monato</bib>
          <aut>HORI Yasuo</aut>
          <vrk><url ref="&Monato;/2003/008060.html">Siluroj, &ccirc;u supersti&ccirc;o a&ubreve; scienco?</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mantel.ter0o">termantelo</ref>
  </snc>
  <trd lng="be">&c_z;&c_ja;&c_m;&c_n;&c_a;&c_ja; &c_k;&c_a;&c_r;&c_a;</trd>
  <trd lng="cs">zemsk&aacute; k&uring;ra</trd>
  <trd lng="de">Erdkruste</trd>
  <trd lng="en">crust <klr>(of the Earth)</klr></trd>
  <trd lng="es">corteza terrestre</trd>
  <trdgrp lng="fr">
    <trd>croute terrestre</trd>,
    <trd>&eacute;corce terrestre</trd>
  </trdgrp>
  <trd lng="hu">f&ouml;ldk&eacute;reg</trd>
  <trd lng="ja">&#22320;&#27579;  <pr>&#12385;&#12363;&#12367;</pr></trd>
  <trd lng="nl">aardkorst</trd>
  <trd lng="pl">skorupa <ind>ziemska</ind></trd>
  <trd lng="ro">scoar&tcomma;a terestr&abreve;</trd>
  <trd lng="ru">&c_z;&c_je;&c_m;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_r;&c_a;</trd>
  <trd lng="sk">zemsk&aacute; k&ocirc;ra</trd>
  <trd lng="uk">&c_z;&c_je;&c_m;&c_n;&c_a; &c_k;&c_o;&c_r;&c_a;</trd>
</drv>
