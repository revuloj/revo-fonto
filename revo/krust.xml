<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>krust</rad>/o
</kap>
<drv mrk="krust.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malmola ekstera tavolo de io interne pli mola:
      <ekz>
        <tld/>o de pano, kuko, vundo, kankro, ovo;
      </ekz>
      <ekz>
        ter<tld/>o;
      </ekz>
      <ekz>
        kalka <tld/>o <klr>(lasita de akvo en kaldrono)</klr>;
      </ekz>
      <ekz>
        ni devas dividi kun ili la lastan pan<tld/>on.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="sxel.0o">&scirc;elo</ref>,
      <ref cel="tavol.0o">tavolo</ref>,
      <ref cel="surfac.0o">surfaco</ref>.
    </refgrp>
    <ref tip="sub" cel="karapa.0o">karapaco</ref>
  </snc>
  <trd lng="de">Kruste</trd>
  <trdgrp lng="en">
    <trd>crust <klr>(gxenerale, kaj p.p. pano)</klr></trd>,
    <trd>casing</trd>,
    <trd>hard outer <ind>covering</ind></trd>,
    <trd>deposit <klr>(depona&jcirc;o sur surfaco de io, ekz. kalko en bolilo)</klr></trd>,
    <trd>scale <klr>(deponaj&jcirc;o sur surfaco de io, ekz. kalko sur bolilo)</klr></trd>,
    <trd>fur <klr>(deponaj&jcirc;o sur surfaco de io, ekz. kalko sur bolilo)</klr></trd>,
    <trd lng="en">rind <klr>(p.p lardo)</klr></trd>,
    <trd>scab <klr>(p.p. ne tute resani&gcirc;inta vundo)</klr></trd>
</trdgrp>
  <trd lng="fr">cro&ucirc;te</trd>
</drv>
<drv mrk="krust.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="artik.0pieduloj">artikpieduloj</ref>
      (<trd lng="la">Crustacea</trd>), kovritaj de karapaco
      kaj spirantaj per brankoj, plej ofte vivantaj en akvo.
    </dif>
    <refgrp tip="sub">
      <ref cel="kankr.0o">kankro</ref>,
      <ref cel="krab.0o">krabo</ref>,
      <ref cel="omar.0o">omaro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Krebstiere</trd>,
    <trd>Krustentiere</trd>
  </trdgrp>
  <trd lng="en">crustacean</trd>
  <trd lng="fr">crustac&eacute;</trd>
</drv>
<drv mrk="krust.en0i">
  <kap>en<tld/>i<fnt>Z</fnt></kap>
  <snc>
    <uzo tip="klr">(Ev)</uzo>
    <dif>
      Inkrusti.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>encrust</trd>,
    <trd>incrust</trd>
  </trdgrp>
  <trd lng="fr">incruster</trd>
</drv>
<drv mrk="krust.dekpiedaj0uloj">
  <kap>dekpiedaj <tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="krust.0uloj"><tld/>uloj</ref>
      (<trd lng="la">Decapoda</trd>), kun dek piedoj.
    </dif>
  </snc>
  <trd lng="de">Zehnfu&szlig;krebse</trd>
</drv>
</art>
