<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krust</rad>/o
</kap>
<drv mrk="krust.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="krust.0o.KOMUNE">
    <dif>
      Ekstera parto de io, kiu malmoli&gcirc;is, farante <ref tip="super"
      cel="sxel.0o.KOMUNE">&scirc;elon</ref>:
      <ekz>
        <tld/>o de pano, kuko, vundo, kankro, ovo;
      </ekz>
      <ekz>
        ter<tld/>o;
      </ekz>
      <ekz>
        kalka <tld/>o <klr>(lasita de akvo en kaldrono)</klr>;
      </ekz>
      <ekz>
        ni devas dividi kun ili la lastan pan<tld/>on.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tavol.0o">tavolo</ref>,
      <ref cel="surfac.0o">surfaco</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_r;&c_y;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Kruste</trd>
  <trdgrp lng="en">
    <trd>crust<klr> (&gcirc;enerale, kaj p.p. pano)</klr></trd>,
    <trd>casing</trd>,
    <trd>hard outer <ind>covering</ind></trd>,
    <trd>deposit<klr> (depona&jcirc;o sur surfaco de io, ekz. kalko en
    bolilo)</klr></trd>,
    <trd>scale<klr> (depona&jcirc;o sur surfaco de io, ekz. kalko sur
    bolilo)</klr></trd>,
    <trd>fur<klr> (depona&jcirc;o sur surfaco de io, ekz. kalko sur
    bolilo)</klr></trd>,
    <trd>rind<klr> (p.p lardo)</klr></trd>,
    <trd>scab<klr> (p.p. ne tute resani&gcirc;inta vundo)</klr></trd>
  </trdgrp>
  <trd lng="fr">cro&ucirc;te</trd>
  <trdgrp lng="hu">
    <trd>h&eacute;j</trd>,
    <trd>k&eacute;reg</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="krust.0uloj">
  <kap><tld/>uloj</kap>
  <snc mrk="krust.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Klaso de <ref tip="super" cel="artik.0pieduloj">artikpieduloj</ref>
      (<trd lng="la">Crustacea</trd>), kovritaj de karapaco
      kaj spirantaj per brankoj, plej ofte vivantaj en akvo.
    </dif>
    <refgrp tip="sub">
      <ref cel="kankr.0o">kankro</ref>,
      <ref cel="krab.0o">krabo</ref>,
      <ref cel="omar.0o">omaro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Krebstiere</trd>,
    <trd>Krustentiere</trd>
  </trdgrp>
  <trd lng="en">crustacean</trd>
  <trd lng="fr">crustac&eacute;</trd>
  <trd lng="hu">r&aacute;kok<klr tip="amb"> (oszt&aacute;ly)</klr></trd>
  <trd lng="ru">&c_r;&c_a;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
</drv>

<drv mrk="krust.en0i">
  <kap>en<tld/>i<fnt>Z</fnt></kap>
  <snc mrk="krust.en0i.inkrusti">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="inkrus.0i.BELA">inkrusti</ref>
  </snc>
  <trdgrp lng="en">
    <trd>encrust</trd>,
    <trd>incrust</trd>
  </trdgrp>
  <trd lng="fr">incruster</trd>
  <trd lng="ru">&c_i;&c_n;&c_k;&c_r;&c_u;&c_s;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="krust.dekpiedaj0uloj">
  <kap>dekpiedaj <tld/>uloj</kap>
  <snc mrk="krust.dekpiedaj0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ordo el la klaso <ref tip="super" cel="krust.0uloj"><tld/>uloj</ref>
      (<trd lng="la">Decapoda</trd>), kun dek piedoj.
    </dif>
  </snc>
  <trd lng="de">Zehnfu&szlig;krebse</trd>
  <trd lng="hu">t&iacute;zl&aacute;b&uacute; <ind>r&aacute;kok</ind></trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_ja;&c_t;&c_i;&c_n;&c_o;&c_g;&c_i;&c_je; &c_r;&c_a;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
</drv>
</art>
