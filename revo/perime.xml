<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: perime.xml,v 1.23 2024/02/17 06:43:06 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>perimetr</rad>/o
</kap>
<drv mrk="perime.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
  <snc>
    <dif>
      <klr>(de ebena <ref cel="surfac.0o.MAT">surfaco</ref>)</klr>
      Linio resp. longo de &gcirc;ia <ref tip="super" cel="rand.0o.figuro">rando</ref>:
      <ekz>
        <nom>pi</nom> (&pi;) <klr>[estas]</klr> la kvociento de la <tld/>o de cirklo per &gcirc;ia diametro
        <fnt>
          <aut>Sergio Pokrovskij</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/KompLeks/P.html">Komputada leksikono</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la arkeologoj kun &gcirc;ojaj ekkrioj dis&scirc;uti&gcirc;is la&ubreve; la <tld/>o de la subtera halo
        <fnt>
          <aut>Ivan Efremov, trad. Jurij Finkel</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/andromedo_14.html">La nebulozo de Andromedo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cirkon.0o">cirkonferenco</ref>,
      <ref cel="perife.0o">periferio</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_y;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">per&iacute;metre</trd>
  <trdgrp lng="cs">
    <trd>d&eacute;lka obvodu</trd>,
    <trd>obvod</trd>
  </trdgrp>
  <trd lng="de">Umfang</trd>
  <trd lng="en">perimeter</trd>
  <trd lng="es">per&iacute;metro</trd>
  <trd lng="eu">perimetro</trd>
  <trd lng="fa">&a_T;&a_w;&a_l; &a_m;&a_H;&f_y;&a_T;</trd>
  <trd lng="fr">p&eacute;rim&egrave;tre</trd>
  <trd lng="hu">ker&uuml;let<klr tip="ind"> (s&iacute;kidom&eacute;)</klr></trd>
  <trd lng="it">perimetro</trd>
  <trd lng="ja">&#x5468;&#x56F2;&#x306E;&#x9577;&#x3055;  <pr>&#x3057;&#x3085;&#x3046;&#x3044;&#x306E;&#x306A;&#x304C;&#x3055;</pr></trd>
  <trd lng="nl">omtrek</trd>
  <trd lng="pl">obw&oacute;d</trd>
  <trd lng="pt">per&iacute;metro</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_i;&c_m;&c_je;&c_t;&c_r;</trd>
  <trd lng="sk">perimeter</trd>
  <trd lng="uk">&#x43F;&#x435;&#x440;&#x438;&#x43C;&#x435;&#x442;&#x440;</trd>
  <trdgrp lng="zh">
    <trd>&#x5468;&#x9577; <pr>zh&#x14D;uch&#xE1;ng</pr></trd>,
    <trd>&#x5468;&#x957F; <pr>zh&#x14D;uch&#xE1;ng</pr></trd>
  </trdgrp>
</drv>
</art>
