<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>perimetr</rad>/o
</kap>
<drv mrk="perime.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
  <snc>
    <dif>
      <klr>(de ebena <ref cel="surfac.0o.MAT">surfaco</ref>)</klr>
      Longo de &gcirc;ia <ref tip="super" cel="rand.0o.figuro">rando</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="cirkon.0o">cirkonferenco</ref>,
      <ref cel="perife.0o">periferio</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_y;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">per&iacute;metre</trd>
  <trd lng="de">Umfang</trd>
  <trd lng="en">perimeter</trd>
  <trd lng="es">per&iacute;metro</trd>
  <trd lng="eu">perimetro</trd>
  <trd lng="fa">&a_T;&a_w;&a_l; &a_m;&a_H;&f_y;&a_T;</trd>
  <trd lng="fr">p&eacute;rim&egrave;tre</trd>
  <trd lng="hu">ker&uuml;let<klr tip="ind"> (s&iacute;kidom&eacute;)</klr></trd>
  <trd lng="it">perimetro</trd>
  <trd lng="nl">omtrek</trd>
  <trd lng="pl">obw&oacute;d</trd>
  <trd lng="pt">per&iacute;metro</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_i;&c_m;&c_je;&c_t;&c_r;</trd>
</drv>
</art>
