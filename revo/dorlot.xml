<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dorlot</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dorlot.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dorlot.0i.iun">
    <uzo>(iun)</uzo>
    <dif>
      Ameme kaj karese trakti, tro indulgante dezirojn
      a&ubreve; kapricojn:
      <ekz>
        <tld/>i infanon;
      </ekz>
      <ekz>
        sin <tld/>i <klr>(trodelikate sin flegi)</klr>;
      </ekz>
      <ekz>
        <ind>kiu volas panon, ne <tld/>u la manon</ind>
        <klr>(ne &scirc;paru penon)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">ha kenyeret akarsz, ne
        <ind>k&iacute;m&eacute;ld</ind> a kezed</trd>
      </ekz>
      <ekz>
        li ekde infaneco kutimi&gcirc;is al <tld/>i&gcirc;ado.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hard.mal0i">malhardi</ref>,
      <ref cel="sibari.0o">sibarito</ref>.
    </refgrp>
    <adm>
      Necesus ekz-oj &ccirc;e "malhardi" por pravigi la referencon
      al &gcirc;i. <aut>MB</aut>
    </adm>
    <trd lng="bg">&c_g;&c_l;&c_je;&c_e;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>amoixar</trd>,
      <trd>acaronar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>cajoler</trd>,
      <trd>caresser</trd>,
      <trd>dorloter</trd>
    </trdgrp>
    <trd lng="de">verw&ouml;hnen</trd>
    <trd lng="es">mimar</trd>
    <trd lng="nl">verwennen</trd>
    <trdgrp lng="pl">
      <trd>rozpieszcza&cacute;</trd>,
      <trd>rozpuszcza&cacute;<klr> (kogo&sacute;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="dorlot.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <uzo>(ion)</uzo>
    <dif>
      Tro favori, tro
      <ref tip="vid" cel="komple4.0i">komplezi</ref>:
    </dif>
    <subsnc>
      <uzo tip="stl">FIG</uzo>
      <dif>
        Tro indulge permesadi malbona&jcirc;on &ccirc;e iu:
        <ekz>
          <tld/>i la fierecon, la maniojn de iu.
        </ekz>
      </dif>
      <trdgrp lng="ca">
        <trd>consentir</trd>,
        <trd>aviciar</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>consentir</trd>,
        <trd>malcriar</trd>,
        <trd>viciar</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>encourager<klr tip="ind"> (une faiblesse)</klr></trd>,
        <trd>flatter<klr tip="ind"> (une faiblesse)</klr></trd>
      </trdgrp>
      <trd lng="pl">pob&lstroke;a&zdot;a&cacute;</trd>
    </subsnc>
    <subsnc>
      <uzo tip="stl">FIG</uzo>
      <dif>
        Da&ubreve;rigi en si ion revatan trovante el &gcirc;i plezuron:
        <ekz>
          <ind><tld/>i esperon</ind>;
          <trdgrp lng="pl">
            <trd>&zdot;ywi&cacute; nadziej&eogonek;</trd>
            <trd>mie&cacute; nadziej&eogonek;</trd>
          </trdgrp>
        </ekz>
        <ekz>
          nur fantaziuloj povas <tld/>i tiun &hcirc;imeron.
        </ekz>
      </dif>
      <trdgrp lng="ca">
        <trd>sustentar</trd>,
        <trd>alimentar</trd>
      </trdgrp>
      <trd lng="es">alimentar</trd>
      <trdgrp lng="fr">
        <trd>caresser<klr tip="ind"> (un r&ecirc;ve, un
        projet)</klr></trd>,
        <trd>nourrir<klr tip="ind"> (un r&ecirc;ve, un espoir)</klr></trd>
      </trdgrp>
    </subsnc>
    <subsnc>
      <uzo tip="stl">FIG</uzo>
      <dif>
        Super&scirc;uti per favoroj:
        <ekz>
          lando <tld/>ata de la naturo.
        </ekz>
      </dif>
      <trd lng="ca">afavorir</trd>
      <trd lng="es">favorecer</trd>
      <trd lng="fr">b&eacute;nir<klr tip="ind"> (r&eacute;pandre ses
      bienfaits)</klr></trd>
      <trdgrp lng="pl">
        <trd>rozpieszca&cacute;</trd>,
        <trd>faworyzowa&cacute;</trd>
      </trdgrp>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="nutr.0i.senton">nutri<sncref/></ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">pamper</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;nyeztet</trd>,
    <trd>d&eacute;delget</trd>,
    <trd>melenget</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>acariciar</trd>,
    <trd>amimar</trd>,
    <trd>afagar</trd>,
    <trd>mimosear</trd>,
    <trd>favorecer</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="dorlot.el0i">
  <kap><ofc>*</ofc>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ekscese <tld/>i: 
      <ekz>
        la Egiptoj &gcirc;in tro el<tld/>is, ili e&ccirc; &scirc;topas &gcirc;in per
        bonodoraj herba&jcirc;oj
        <fnt><bib>Fab4</bib>, <lok>Filino de la mar&ccirc;a re&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>exal&ccedil;ar</trd>,
    <trd>amanyagar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>g&acirc;ter</trd>,
    <trd>choyer</trd>
  </trdgrp>
  </drv>
</art>
