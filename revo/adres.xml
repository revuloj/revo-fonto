       <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_y;&c_j; &c_a;&c_d;&c_r;&c_je;&c_s;</trd>
     </trdgrp>
     <trd lng="sk">internetov&aacute; <ind>adresa</ind></trd>
     <trd lng="th">&#xE17;&#xE35;&#xE48;&#xE2D;&#xE22;&#xE39;&#xE48;&#xE2D;&#xE35;&#xE40;&#xE21;&#xE25;</trd>
