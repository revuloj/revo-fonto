       <trd>&c_a;&c_d;&c_r;&c_je;&c_s;&c_n;&c_y;&c_j; &c_s;&c_p;&c_r;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>,
       <trd>&c_a;&c_d;&c_r;&c_je;&c_s;&c_n;&c_i;&c_k;</trd>
     </trdgrp>
     <trd lng="sk">adres&aacute;r</trd>
