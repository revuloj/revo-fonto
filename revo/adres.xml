<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>adres</rad>/o
</kap>

<drv mrk="adres.0o">
   <kap><tld/>o</kap>
   <snc mrk="adres.0o.KOMUNE">
     <dif>
       Precizaj indikoj por atingi iun, ion:
       <ekz>
         &scirc;i donis al li sian <tld/>on, prenis de li lian, &ndash; 
         kaj ili disi&gcirc;is amike
         <fnt><bib>FK</bib>, <lok>4, rakontoj, Arturo, (rakonto de V.
         Devjatnin), 2</lok></fnt>.
       </ekz>
     </dif>
     <subsnc mrk="adres.0o.POSX">
       <uzo tip="fak">POSX</uzo>
       <dif>
         Surskribo sur leterkoverto a&ubreve; paka&jcirc;o, montranta la
         nomon de iu kaj la precizan lokon, kie li lo&gcirc;as:
         <ekz>
           sur tia letero la diversaj partoj de la <tld/>o kuras unu sur
           alian
           <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj, el la
           po&scirc;to</lok></fnt>.
         </ekz>
       </dif>
       <trd lng="de">Anschrift</trd>
       <trd lng="en">address</trd>
       <trd lng="he">&mem;&ayin;&fnun;</trd>
       <trd lng="pl">adres<klr> pocztowy</klr></trd>
     </subsnc>
     <subsnc mrk="adres.0o.KOMP">
       <uzo tip="fak">KOMP</uzo>
       <dif>
         Nombro, kodo a&ubreve; nomo, indikanta parton de komputila
memoro,
         dispona&jcirc;on a&ubreve; retnodon:
         <ekz>
           <tld/>o plena, absoluta, relativa, fakta, fizika, virtuala;
         </ekz>
         <ekz>
           <tld/>a spaco.
         </ekz>
       </dif>
     </subsnc>
     <trdgrp lng="pl">
       <trd>adres<klr> elektroniczny</klr></trd>,
       <trd>adres<klr> sieciowy</klr></trd>,
     </trdgrp>
   </snc>
   <trd lng="be">&c_a;&c_d;&c_r;&c_a;&c_s;</trd>
   <trd lng="bg">&c_a;&c_d;&c_r;&c_je;&c_s;</trd>
   <trd lng="br">chomlec'h</trd>
   <trd lng="de">Adresse</trd>
   <trd lng="en">address</trd>
   <trdgrp lng="es">
     <trd>direcci&oacute;n</trd>,
     <trd>se&ntilde;as</trd>
   </trdgrp>
   <trd lng="fr">adresse</trd>
   <trd lng="he">&kaf;&tav;&vav;&bet;&tav;</trd>
   <trd lng="hu">c&iacute;m</trd>
   <trd lng="id">alamat</trd>
   <trd lng="it">indirizzo</trd>
   <trd lng="nl">adres</trd>
   <trd lng="pt">endere&ccedil;o</trd>
   <trd lng="ru">&c_a;&c_d;&c_r;&c_je;&c_s;</trd>
   <trd lng="tr">adres</trd>
   <trd lng="sk">adresa</trd>
   <trd lng="sv">adress</trd>
</drv>

<drv mrk="adres.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="adres.0i.POSX">
     <uzo tip="fak">POSX</uzo>
     <dif>
       Skribi adreson sur letero, paka&jcirc;o:
       <ekz>
         el la tri leteroj unu estis <tld/>ita: al Lia Episkopa
         Mo&scirc;to, Sinjoro N.
         <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="br">
       <trd>skriva&ntilde; ar <ind>chomlec'h</ind></trd>,
       <trd>kas</trd>
     </trdgrp>
     <trd lng="de">adressieren</trd>
     <trd lng="en">address <klr tip="ind">(a letter)</klr></trd>
     <trd lng="id">[<ind>alamat</ind>] mengalamati</trd>
     <trd lng="nl">adresseren</trd>
     <trd lng="pt">endere&ccedil;ar</trd>
     <trd lng="sv">adressera</trd>
   </snc>
   <snc mrk="adres.0i.sendi">
     <dif>
       Sendi ion al ies lo&gcirc;ejo.
     </dif>
     <trd lng="en">send <klr tip="ind">(by mail)</klr></trd>
     <trd lng="es">remitir</trd>
     <trdgrp lng="id">
       <trd>[<ind>alamat</ind>] mengalamatkan</trd>,
       <trd>[<ind>tuju</ind>] menujukan (surat)</trd>
     </trdgrp>
     <trdgrp lng="pt">
       <trd>remeter</trd>,
       <trd>enviar</trd>
     </trdgrp>
     <trd lng="nl">verzenden</trd>
   </snc>
   <snc mrk="adres.0i.alparoli">
     <dif>
       Direkti parolon al iu:
       <ekz>
         <tld/>i al iu sian dankon<fnt>Z</fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="direkt.sin0ial">sin direkti</ref>,
       <ref cel="turn.0igxisin0i">sin turni</ref>
     </refgrp>
     <trd lng="en">address <klr tip="ind">(a person)</klr></trd>
     <trd lng="he">&lamed;&pe;&nun;&vav;&tav; &alef;&lamed;</trd>
     <trd lng="nl">zich <ind>richten</ind> tot</trd>
     <trd lng="sv">rikta</trd>
   </snc>
   <trd lng="be">&c_a;&c_d;&c_r;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
   <trdgrp lng="fr">
     <trd>adresser</trd>,
     <trd>envoyer</trd>,
     <trd>exp&eacute;dier</trd>,
     <trd>exprimer</trd>,
     <trd>poster<klr tip="ind"> (envoyer par la poste)</klr></trd>,
     <trd>pr&eacute;senter</trd>
   </trdgrp>
   <trd lng="he">&lamed;&mem;&ayin;&fnun;</trd>
   <trd lng="hu">(meg)<ind>c&iacute;mez</ind></trd>
   <trd lng="it">indirizzare</trd>
   <trdgrp lng="ru">
     <trd>&c_a;&c_d;&c_r;&c_je;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
   </trdgrp>
   <trd lng="sk">adresova&tcaron;</trd>
</drv>

<drv mrk="adres.0anto">
   <kap><tld/>anto<fnt>Z</fnt></kap>
   <snc>
     <dif>
       Tiu, kiu <tld/>as.
     </dif>
     <trd lng="be">&c_a;&c_d;&c_r;&c_a;&c_s;&c_a;&c_n;&c_t;</trd>
     <trd lng="br">kaser</trd>
     <trd lng="de">Absender</trd>
     <trdgrp lng="fr">
       <trd>exp&eacute;diteur</trd>,
       <trd>destinateur</trd>
     </trdgrp>
     <trd lng="en">sender</trd>
     <trd lng="es">remitente</trd>
     <trd lng="he">&mem;&mem;&ayin;&fnun;</trd>
     <trdgrp lng="hu">
       <trd>felad&oacute;</trd>,
       <trd>k&uuml;ld&odblac;</trd>
     </trdgrp>
     <trd lng="id">[<ind>kirim</ind>] pengirim</trd>
     <trd lng="it">mittente</trd>
     <trd lng="nl">afzender</trd>
     <trdgrp lng="pt">
       <trd>remetente</trd>,
       <trd>expedidor</trd>
     </trdgrp>
     <trdgrp lng="ru">
 
<trd>&c_o;&c_t;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
       <trd>&c_a;&c_d;&c_r;&c_je;&c_s;&c_a;&c_n;&c_t;</trd>
     </trdgrp>
     <trd lng="tr">g&ouml;nderen</trd>
     <trd lng="sk">odosielate&lcaron;</trd>
     <trd lng="sv">avs&auml;ndare</trd>
   </snc>
</drv>

<drv mrk="adres.0ato">
   <kap><ofc>*</ofc><tld/>ato</kap>
   <snc>
     <dif>
       Tiu, kies nomon surhavas letero a&ubreve; paka&jcirc;o:
       <ekz>
         en korespondado kun personoj nekonataj estas bone &ccirc;iam 
peni uzadi
         nur vortojn el &leftquot;Fundamento&rightquot; &ccirc;ar nur 
pri tiaj vortoj ni povas
         esti certaj, ke nia adresato ilin nepre trovos en sia vortaro
         <fnt><bib>F</bib>, <lok>anta&ubreve;parolo</lok></fnt>.
       </ekz>
     </dif>
     <trd lng="be">&c_a;&c_d;&c_r;&c_a;&c_s;&c_a;&c_t;</trd>
     <trd lng="br">degemerer</trd>
     <trdgrp lng="de">
       <trd>Empf&auml;nger</trd>,
       <trd>Adressat</trd>
     </trdgrp>
     <trd lng="en">addressee</trd>
     <trd lng="es">destinatario</trd>
     <trd lng="fr">destinataire</trd>
     <trd lng="he">&nun;&mem;&ayin;&fnun;</trd>
     <trd lng="hu">c&iacute;mzett</trd>
     <trd lng="it">destinatario</trd>
     <trd lng="nl">geadresseerde</trd>
     <trd lng="pt">destinat&aacute;rio</trd>
     <trdgrp lng="ru">
 
<trd>&c_p;&c_o;&c_l;&c_u;&c_ch;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>,
       <trd>&c_a;&c_d;&c_r;&c_je;&c_s;&c_a;&c_t;</trd>
     </trdgrp>
     <trd lng="sk">adres&aacute;t</trd>
     <trd lng="sv">adressat</trd>
   </snc>
   <rim>
     La&ubreve; la gramatiko kaj la kutima modelo <em>Petro <tld/>as
     leteron al Pa&ubreve;lo</em>, la <tld/>ato devus esti la
     <em>letero</em> (kp <em>letero <tld/>ita al Pa&ubreve;lo</em>). La
     formo <tld/>ulo estas tute malpreciza, &ccirc;ar logike &gcirc;i
     egale
     ta&ubreve;gas indiki la <tld/>anton. Gramatike &gcirc;usta formo
     estus
     <em>al<tld/>ato</em>, tamen fakte &gcirc;i ne estas uzata. Tial en
     tia situacio oni prefere nomu Petron <em>sendanto</em> kaj
     Pa&ubreve;lon, <em>ricevonto</em>.
   </rim>
  <trdgrp lng="sk">
    <trd>prijímateľ</trd>,
    <trd>príjemca</trd>
  </trdgrp>
</drv>

<drv mrk="adres.0aro">
   <kap><tld/>aro<fnt>Z</fnt></kap>
   <snc>
     <dif>
       Kolekto da <tld/>oj en kajero, memorlibro a&ubreve; komputila
       dosiero:
       <ekz>
         SAT <klr>...</klr> eldonas &ccirc;iujare Jarlibron kun 
<tld/>aro de la
         membroj
         <fnt><bib>Lanti</bib>, <lok>Rutinemo</lok></fnt>;
       </ekz>
       <ekz>
         <!--url
         ref="http://wwwtios.cs.utwente.nl/adresaro/"--><tld/>aro de
         rete atingeblaj esperantistoj kaj
         esperanto-organizoj<!--/url-->
       </ekz>
     </dif>
     <trd lng="be">&c_a;&c_d;&c_r;&c_a;&c_s;&c_n;&c_y;
&c_d;&c_a;&c_v;&c_je;
     &c_d;&c_n;&c_ib;&c_k;</trd>
     <trd lng="bg">&c_a;&c_d;&c_r;&c_je;&c_s;&c_n;&c_a;
&c_k;&c_n;&c_i;&c_g;&c_a;</trd>
     <trdgrp lng="br">
       <trd>karned chomlec'hio&ugrave;</trd>,
       <trd>roll chomlec'hio&ugrave;</trd>
     </trdgrp>
     <trd lng="de">Adressbuch</trd>
     <trd lng="en">address book</trd>
     <trd lng="es">directorio</trd>
     <trd lng="fr">carnet d'adresses</trd>
     <trd lng="he">&pe;&nun;&qof;&samekh; 
&kaf;&tav;&vav;&bet;&vav;&tav;</trd>
     <trdgrp lng="hu">
       <trd>c&iacute;mjegyz&eacute;k</trd>,
       <trd>c&iacute;mt&aacute;r</trd>
     </trdgrp>
     <trd lng="id"><ind>buku</ind> alamat</trd>
     <trd lng="it">rubrica<klr> (di indirizzi)</klr></trd>
     <trdgrp lng="nl">
       <trd>adresboek</trd>,
       <trd>adressenbestand</trd>,
       <trd>adressenlijst</trd>
     </trdgrp>
     <trdgrp lng="pt">
       <trd>livro de endere&ccedil;os</trd>,
       <trd>diret&oacute;rio</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_a;&c_d;&c_r;&c_je;&c_s;&c_n;&c_a;&c_ja;
       &c_k;&c_n;&c_i;&c_zh;&c_k;&c_a;</trd>,
       <trd>&c_a;&c_d;&c_r;&c_je;&c_s;&c_n;&c_y;&c_j;
       &c_s;&c_p;&c_r;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>,
       <trd>&c_a;&c_d;&c_r;&c_je;&c_s;&c_n;&c_i;&c_k;</trd>
     </trdgrp>
     <trd lng="sk">adres&aacute;r</trd>
     <trd lng="sv">adressbok</trd>
   </snc>
  <trdgrp lng="sk">
    <trd>adresár, zoznam adries</trd>,
    <trd>zoznam adries</trd>
  </trdgrp>
</drv>

<drv mrk="adres.ret0o">
   <kap>ret<tld/>o</kap>
   <snc mrk="adres.ret0o.KOMP">
     <uzo tip="fak">KOMP</uzo>
     <dif>
       Signo&ccirc;eno uzata en komputila reto por indiki komputilon kaj
       komputadan objekton (ekz-e dosieron per Interreta <tld/>o)
       a&ubreve; uzanton (per retpo&scirc;ta <tld/>o):
       <ekz>
         bonvenaj estos mallongaj salutmesa&gcirc;oj tra mia ret<tld/>o
         <fnt>
         <aut>H. Masson</aut>,
         <vrk>ret-info: infanfestivalo</vrk>,
         <lok>soc.culture.esperanto, 1998-10-09</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="be">
       <trd>&c_s;&c_je;&c_c;&c_je;&c_v;&c_y; 
&c_a;&c_d;&c_r;&c_a;&c_s;</trd>,
 
<trd>&c_Ib;&c_n;&c_t;&c_e;&c_r;&c_n;&c_e;&c_t;-&c_a;&c_d;&c_r;&c_a;&c_s;
       </trd>,
       <trd>&c_a;&c_d;&c_r;&c_a;&c_s; 
&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;
       &c_n;&c_a;&c_j; &c_p;&c_o;&c_sh;&c_t;&c_y;</trd>
     </trdgrp>
     <trd lng="bg">&c_i;&c_m;&c_je;&c_j;&c_l;
&c_a;&c_d;&c_r;&c_je;&c_s;</trd>
     <trd lng="br">chomlec'h postel</trd>
     <trdgrp lng="de">
       <trd>E-Mail-Adresse</trd>,
       <trd>elektronische Adresse</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>email address</trd>
     </trdgrp>
     <trd lng="es">direcci&oacute;n de internet</trd>
     <trd lng="fr">adresse &eacute;lectronique</trd>
     <trd lng="id"><ind>alamat</ind> surel (e-mail)</trd>
     <trd lng="he">&kaf;&tav;&vav;&bet;&tav;
&dalet;&vav;&alef;"&lamed;
     </trd>
     <trdgrp lng="hu">
       <trd>e-mail <ind>c&iacute;m</ind></trd>,
       <trd>dr&oacute;tpostac&iacute;m</trd>
     </trdgrp>
     <trdgrp lng="it">
       <trd>email<klr> (indirizzo)</klr></trd>,
       <trd><ind>indirizzo</ind> di posta elettronica</trd>
     </trdgrp>
     <trd lng="nl">mailadres</trd>
     <trd lng="pt">endere&ccedil;o eletr&ocirc;nico</trd>
     <trdgrp lng="ru">
       <trd>&c_s;&c_je;&c_t;&c_je;&c_v;&c_o;&c_j;
       &c_a;&c_d;&c_r;&c_je;&c_s;</trd>,
       <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_n;&c_n;&c_y;&c_j;
       &c_a;&c_d;&c_r;&c_je;&c_s;</trd>
     </trdgrp>
     <trd lng="sk">internetov&aacute; <ind>adresa</ind></trd>
   </snc>
  <trdgrp lng="sk">
    <trd>adresa elektronickej pošty</trd>,
    <trd>internetová adresa</trd>
  </trdgrp>
</drv>
</art>
