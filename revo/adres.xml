<drv mrk="adres.0o">
   <kap><tld/>o</kap>
   <snc mrk="adres.0o.KOMUNE">
     <dif>
       Precizaj <ref tip="super" cel="indik.0o.sciigo">indiko</ref>j por atingi iun, ion:
       <ekz>
         &scirc;i donis al li sian <tld/>on, prenis de li lian, &ndash;
         kaj ili disi&gcirc;is amike
         <fnt><bib>FK</bib>, <lok>4, rakontoj, Arturo, (rakonto de V.
         Devjatnin), 2</lok></fnt>.
       </ekz>
     </dif>
     <subsnc mrk="adres.0o.POSX">
       <uzo tip="fak">POSX</uzo>
       <dif>
         <ref tip="super" cel="skrib.sur0o">Surskribo</ref> sur leterkoverto a&ubreve; paka&jcirc;o, montranta la
         nomon de iu kaj la precizan lokon, kie li lo&gcirc;as:
         <ekz>
           sur tia letero la diversaj partoj de la <tld/>o kuras unu sur
           alian
           <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj, el la
           po&scirc;to</lok></fnt>;
         </ekz>
        <ekz>
          siajn paperojn <klr>[&#8230;]</klr> &scirc;i metu ilin en koverton kaj skribu sur &ccirc;i tiu la
          <tld/>on de la bienmastro
          <fnt>
            <bib>Fab3</bib>
            <lok>Kora sufero</lok>
          </fnt>.
        </ekz>
       </dif>
       <trd lng="de">Anschrift</trd>
       <trd lng="he">&mem;&ayin;&fnun;</trd>
       <trdgrp lng="ja">
         <trd>&#x4F4F;&#25152; <pr>&#12376;&#12421;&#12358;&#12375;&#12423;</pr></trd>,
         <trd>&#12354;&#12390;&#20808; <pr>&#12354;&#12390;&#12373;&#x304D;</pr></trd>,
         <trd>&#x30A2;&#x30C9;&#x30EC;&#x30B9; <pr>&#12354;&#12393;&#x308C;&#12377;</pr></trd>
       </trdgrp>
       <trd lng="ms">alamat</trd>
       <trd lng="pl">adres pocztowy</trd>
       <trd lng="th">&#xE17;&#xE35;&#xE48;&#xE2D;&#xE22;&#xE39;&#xE48;</trd>
     </subsnc>
     <subsnc mrk="adres.0o.KOMP">
       <uzo tip="fak">KOMP</uzo>
       <uzo tip="fak">RET</uzo>
       <dif>
         Nombro, kodo a&ubreve; nomo, indikanta parton de komputila
         memoro, dispona&jcirc;on a&ubreve; retnodon:
         <ekz>
           <tld/>o plena, absoluta, relativa, fakta, fizika, virtuala;
         </ekz>
         <ekz>
           <tld/>a spaco;
         </ekz>
         <ekz>
          64-bita <tld/>o
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=37916">IEEE 1394</url></lok>
          </fnt>;
         </ekz>
         <ekz>
          &ccirc;iu komputilo kaj enkursigilo en la reto havas <nom>IP</nom>-<tld/>on
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=1871">IP-adreso</url></lok>
          </fnt>;
         </ekz>
         <ekz>
          kompreneble <nom>RTL.tv</nom> estus multe pli konvena adreso ol <nom>RTL.com</nom>
          <fnt><aut>Stefan Maul</aut>:
          <vrk><url ref="&Monato;/2000/007180.html">Ri&ccirc;a per .tv</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/12, p. 6</lok>
        </fnt>.
         </ekz>
       </dif>
    <refgrp tip="vid">
      <ref cel="montr.0ilo">montrilo</ref>,
      <ref cel="lig.0ilo.KOMP">ligilo<sncref/></ref>,
      <ref cel="refere.0o.KOMP">referenco<sncref/></ref>,
      <ref cel="regxistr.0o">re&gcirc;istro</ref>
    </refgrp>
     </subsnc>
     <trd lng="ja">&#x30A2;&#x30C9;&#x30EC;&#x30B9; <pr>&#12354;&#12393;&#x308C;&#12377;</pr></trd>
   </snc>
  <trd lng="be">&c_a;&c_d;&c_r;&c_a;&c_s;</trd>
  <trd lng="bg">&c_a;&c_d;&c_r;&c_je;&c_s;</trd>
  <trd lng="bo">&t_kha;&t_tsheg;&t_ba;&t_yata;&t_nga;&t_tsheg;</trd>
  <trd lng="br">chomlec'h</trd>
  <trd lng="cs">adresa</trd>
  <trd lng="de">Adresse</trd>
  <trd lng="en">address</trd>
  <trdgrp lng="es">
     <trd>direcci&oacute;n</trd>,
     <trd>se&ntilde;as</trd>
   </trdgrp>
  <trd lng="fr">adresse</trd>
  <trd lng="he">&kaf;&tav;&vav;&bet;&tav;</trd>
  <trd lng="hu">c&iacute;m</trd>
  <trd lng="id">alamat</trd>
  <trd lng="it">indirizzo</trd>
  <trd lng="nl">adres</trd>
  <trd lng="pl">adres</trd>
  <trd lng="pt">endere&ccedil;o</trd>
  <trd lng="ru">&c_a;&c_d;&c_r;&c_je;&c_s;</trd>
  <trd lng="sk">adresa</trd>
  <trd lng="sv">adress</trd>
  <trd lng="tr">adres</trd>
  <trd lng="uk">&c_a;&c_d;&c_r;&c_je;&c_s;&c_a;</trd>
  <trd lng="zh">&#22320;&#22336;<klr> [d&igrave;zh&#x1D0;]</klr></trd>
</drv>

<drv mrk="adres.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="adres.0i.POSX">
     <uzo tip="fak">POSX</uzo>
     <dif>
       <ref tip="super" cel="skrib.0i.signojn">Skribi</ref> <tld/>on sur letero, paka&jcirc;o:
       <ekz>
         el la tri leteroj unu estis <tld/>ita: al Lia Episkopa
         Mo&scirc;to, Sinjoro N.
         <fnt><bib>F</bib>, <lok>&FE; 42</lok></fnt>;
       </ekz>
      <ekz>
        mi <tld/>os divenprove strato Po&scirc;toficeja
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>.
      </ekz>
     </dif>
     <trdgrp lng="br">
       <trd>skriva&ntilde; ar <ind>chomlec'h</ind></trd>,
       <trd>kas</trd>
     </trdgrp>
     <trd lng="en">address <klr tip="ind">(a letter)</klr></trd>
     <trd lng="id"><baz>alamat</baz>mengalamati</trd>
     <trd lng="nl">adresseren</trd>
     <trd lng="pl">adresowa&cacute;</trd>
     <trd lng="pt">endere&ccedil;ar</trd>
     <trd lng="sv">adressera</trd>
     <trd
     lng="th">&#xE40;&#xE02;&#xE35;&#xE22;&#xE19;&#xE17;&#xE35;&#xE48;&#xE2D;&#xE22;&#xE39;&#xE48;</trd>
    <trdgrp lng="zh">
    <trd>&#20889;&#x4E0A;&#x59D3;&#x540D;&#22320;&#22336;
    <pr>xi&#x11B;sh&#xE0;ngx&#xEC;ngm&#xED;ngd&#xEC;zh&#x1D0;</pr></trd>,
    <trd>&#x5BEB;&#x4E0A;&#x59D3;&#x540D;&#22320;&#22336;
    <pr>xi&#x11B;sh&#xE0;ngx&#xEC;ngm&#xED;ngd&#xEC;zh&#x1D0;</pr></trd>,
    <trd>&#20889;&#26377;&#22320;&#22336; <pr>xi&#x11B;y&#x1D2;ud&#xEC;zh&#x1D0;</pr></trd>,
    <trd>&#x5BEB;&#26377;&#22320;&#22336; <pr>xi&#x11B;y&#x1D2;ud&#xEC;zh&#x1D0;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="adres.0i.sendi">
    <dif>
      <ref tip="super" cel="send.0i.ion">Sendi</ref> ion al ies <tld/>o:
      <ekz>
        mi permesas al mi <tld/>i al vi mian prezaron &gcirc;eneralan kaj samtempe kelkajn
        specimenojn de la vendobjektoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu &scirc;atus kunlabori povas sin <tld/>i al respondeculino
        pri tema programo <klr>[&ccirc;e]</klr> <frm>cbaro@yahoo.com</frm>
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;3</lok>
        </fnt>.
      </ekz>
    </dif>
     <trdgrp lng="de">
       <trd>schicken</trd>,
       <trd>senden</trd>
     </trdgrp>
     <trd lng="en">send <klr tip="ind">(by mail)</klr></trd>
     <trd lng="es">remitir</trd>
     <trdgrp lng="id">
       <trd><baz>alamat</baz>mengalamatkan</trd>,
       <trd><baz>tuju</baz>menujukan<klr> (surat)</klr></trd>
     </trdgrp>
     <trdgrp lng="pt">
       <trd>remeter</trd>,
       <trd>enviar</trd>
     </trdgrp>
     <trd lng="nl">verzenden</trd>
     <trd
     lng="th">&#xE2A;&#xE48;&#xE07;&#xE44;&#xE1B;&#xE15;&#xE32;&#xE21;&#xE17;&#xE35;&#xE48;&#xE2D;&#xE22;&#xE39;&#xE48;</trd>
    <trdgrp lng="zh">
    <trd>&#x4F20;&#x771F;&#x53CA;&#x90AE;&#x5BC4;&#36882;&#36865;
    <pr>chu&#xE1;nzh&emacron;nj&#xED;y&#xF3;uj&#xEC;d&#xEC;s&#xF2;ng</pr></trd>,
    <trd>&#x50B3;&#x771F;&#x53CA;&#x90F5;&#x5BC4;&#x905E;&#36865;
    <pr>chu&#xE1;nzh&emacron;nj&#xED;y&#xF3;uj&#xEC;d&#xEC;s&#xF2;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="adres.0i.alparoli">
    <dif>
       <ref tip="super" cel="direkt.0i.stiri">Direkti</ref> parolon k.a. al iu:
       <ekz>
         <tld/>i al iu sian dankon
         <fnt><bib>PV</bib></fnt>;
       </ekz>
      <ekz>
        la komandanto <klr>[&#8230;]</klr> lerte <tld/>is al &ccirc;iu &ccirc;eestanto &gcirc;entilan a&ubreve; spritan
        vorton
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deksepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas certa, ke tiun kisadon vi ne <tld/>as al alia viro
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="direkt.sin0ial">sin direkti</ref>,
       <ref cel="turn.0igxi">sin turni</ref>
    </refgrp>
     <trdgrp lng="de">
       <trd>richten <klr>(an)</klr></trd>,
       <trd>wenden <klr>(an)</klr></trd>
     </trdgrp>
     <trd lng="en">address <klr tip="ind">(a person)</klr></trd>
     <trd lng="he">&lamed;&pe;&nun;&vav;&tav; &alef;&lamed;</trd>
     <trd lng="nl">zich <ind>richten</ind> tot</trd>
     <trd lng="pl">adresowa&cacute;</trd>
     <trd lng="sv">rikta</trd>
   </snc>
  <trd lng="be">&c_a;&c_d;&c_r;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">adresovat</trd>
  <trd lng="de">adressieren</trd>
  <trdgrp lng="fr">
     <trd>adresser</trd>,
     <trd>envoyer</trd>,
     <trd>exp&eacute;dier</trd>,
     <trd>exprimer</trd>,
     <trd>poster<klr tip="ind"> (envoyer par la poste)</klr></trd>,
     <trd>pr&eacute;senter</trd>
   </trdgrp>
  <trd lng="he">&lamed;&mem;&ayin;&fnun;</trd>
  <trd lng="hu">(meg)<ind>c&iacute;mez</ind></trd>
  <trd lng="it">indirizzare</trd>
  <trdgrp lng="ja">
    <trd>&#36865;&#x308B;  <pr>&#x304A;&#x304F;&#x308B;</pr></trd>,
    <trd>&#x304B;&#12369;&#x308B;</trd>,
    <trd>&#21521;&#12369;&#x308B;  <pr>&#12416;&#12369;&#x308B;</pr></trd>,
    <trd>&#12354;&#12390;&#x540D;&#12434;&#x66F8;&#x304F;  [<pr>&#12354;&#12390;&#x306A;&#12434;&#x304B;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">adresseren</trd>
  <trdgrp lng="ru">
     <trd>&c_a;&c_d;&c_r;&c_je;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
   </trdgrp>
  <trd lng="sk">adresova&tcaron;</trd>
  <trd lng="uk">&c_a;&c_d;&c_r;&c_je;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

