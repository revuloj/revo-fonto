<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>preskrib</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="preskr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Ordoni skribe:
      <ekz>
        la kuracisto <tld/>as medikamenton al malsanulo.
      </ekz>
    </dif>
    <trd lng="de">verschreiben</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Ordoni formale:
      <ekz>
        konforme al la <tld/>o de sia patro David<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="de">
    <trd>anordnen</trd>,
    <trd>vorschreiben</trd>
  </trdgrp>
  <trd lng="nl">voorschrijven</trd>
</drv>
</art>
