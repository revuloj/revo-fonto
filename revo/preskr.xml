<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: preskr.xml,v 1.24 2023/09/03 12:45:11 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>preskrib</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="preskr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="preskr.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="ordon.0i">Ordoni</ref> skribe:
      <ekz>
        la medikamenton, kiun mi <tld/>is, alportu kiel eble plej balda&ubreve;
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>prescriure</trd>,
      <trd>receptar</trd>
    </trdgrp>
    <trd lng="de">verschreiben</trd>
    <trdgrp lng="es">
      <trd>prescribir</trd>,
      <trd>recetar<klr> (un tratamiento m&eacute;dico)</klr></trd>
    </trdgrp>
    <trd lng="hu">fel&iacute;r <klr tip="ind">(orvos)</klr></trd>
    <trd lng="pl">przepisa&cacute;</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="preskr.0i.KOMUNE">
    <dif>
      Ordoni formale:
      <ekz>
        se al la re&gcirc;o pla&ccirc;as, oni preskribu ekstermi ilin
        <fnt>
          <bib>MT</bib>
          <lok>Ester 3:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la diplomata ceremoniaro <tld/>as la titoladon al diversaj altranguloj en
        diversaj situacioj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>preceptuar</trd>,
      <trd>establir</trd>
    </trdgrp>
    <trd lng="en">stipulate</trd>
    <trd lng="hu">el&odblac;&iacute;r</trd>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="direkt.0i.ies_konduton">direkti<sncref/></ref>,
    <ref cel="instru1.0i.KOMUNE">instrukcii</ref>,
    <ref cel="regul.0igi">reguligi</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anordnen</trd>,
    <trd>vorschreiben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>prescribe</trd>,
    <trd>order</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enjoindre</trd>,
    <trd>prescrire</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x547D;&#x3058;&#x308B;  [<ind>&#x3081;&#x3044;&#x3058;&#x308B;</ind>]</trd>,
    <trd>&#x898F;&#x5B9A;&#x3059;&#x308B;  [<ind>&#x304D;&#x3066;&#x3044;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x51E6;&#x65B9;&#x3059;&#x308B;  [<ind>&#x3057;&#x3087;&#x307B;&#x3046;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">voorschrijven</trd>
  <trdgrp lng="pt">
    <trd>prescrever <klr tip="ind">(ordenar)</klr></trd>,
    <trd>ordenar</trd>,
    <trd>preceituar</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>predp&iacute;sa&tcaron;</trd>,
    <trd>vyrubi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>poru&ccaron;it</trd>,
    <trd>p&rcaron;edepsat</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_p;&c_i;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_l;&c_ib;&c_k;&c_i; &c_ib; &c_t;.&c_p;.)</trd>,
    <trd>&c_n;&c_a;&c_k;&c_a;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="preskr.0o">
  <kap><tld/>o</kap>
  <snc mrk="preskr.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Skriba indiko de uzotaj medikamentoj kaj farotaj a&ubreve; nefarotaj
      agoj:
      <ekz>
        malsanulo devas obei la <tld/>ojn de sia
        kuracisto<fnt>PIV2</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="recept.0o.MED">recepto</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>prescripci&oacute;</trd>,
      <trd>recepta</trd>
    </trdgrp>
    <trd lng="de">Verschreibung</trd>
    <trd lng="es">prescripci&oacute;n</trd>
    <trd lng="pl">recepta</trd>
  </snc>
  <snc>
    <dif>
      Detala a&ubreve;toritata <ref tip="super" cel="ordon.0o.volo">ordono</ref>:
      <ekz>
        li faris dek orajn kandelabrojn la&ubreve; la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 4:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        religia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 19:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        oratoro, pledanta anta&ubreve; la ju&gcirc;istaro, nepre devas observi tiujn <tld/>ojn
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili pekis kontra&ubreve; Viaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 9:29</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="direkt.0o.instrukcio">direkto<sncref/></ref>,
      <ref cel="direktiv.0o">direktivo</ref>,
      <ref cel="norm.0o.konvencio">normo<sncref/></ref>,
      <ref cel="regul.0o">regulo</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>precepte</trd>,
      <trd>disposici&oacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Vorschrift</trd>,
      <trd>Anordnung</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>prescripci&oacute;n</trd>,
      <trd>norma</trd>
    </trdgrp>
    <trd lng="pl">przepis</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_d;&c_p;&c_ib;&c_s;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_k;&c_a;&c_z;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dispozice</trd>,
    <trd>na&rcaron;&iacute;zen&iacute;</trd>,
    <trd>preskripce</trd>,
    <trd>p&rcaron;edpis</trd>,
    <trd>vym&ecaron;&rcaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="de">Verordnung</trd>
  <trdgrp lng="en">
    <trd>prescription</trd>,
    <trd>directions</trd>,
    <trd>orders</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commandements</trd>,
    <trd>directives</trd>,
    <trd>ordonnance</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5B9A;&#x3081;  [<ind>&#x3055;&#x3060;&#x3081;</ind>]</trd>,
    <trd>&#x898F;&#x5B9A;  [<ind>&#x304D;&#x3066;&#x3044;</ind>]</trd>,
    <trd>&#x51E6;&#x65B9;&#x7B8B;  [<ind>&#x3057;&#x3087;&#x307B;&#x3046;&#x305B;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">voorschrift</trd>
  <trd lng="sk">predpis</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_p;&c_i;&c_s;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_zh;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
