<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>preskrib</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="preskr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="preskr.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Ordoni skribe:
      <ekz>
        la kuracisto <tld/>as medikamenton al malsanulo.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>prescribir</trd>,
      <trd>receptar<klr> (un tractament m&egrave;dic)</klr></trd>
    </trdgrp>
    <trd lng="de">verschreiben</trd>
    <trdgrp lng="es">
      <trd>prescribir</trd>,
      <trd>recetar<klr> (un tratamiento m&eacute;dico)</klr></trd>
    </trdgrp>
    <trd lng="hu">fel&iacute;r <klr tip="ind">(orvos)</klr></trd>
    <trd lng="pl">przepisa&cacute;</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Ordoni formale:
      <ekz>
        konforme al la <tld/>o de sia patro David<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="en">stipulate</trd>
    <trd lng="hu">el&odblac;&iacute;r</trd>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anordnen</trd>,
    <trd>vorschreiben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>prescribe</trd>,
    <trd>order</trd>
  </trdgrp>
  <trd lng="nl">voorschrijven</trd>
  <trdgrp lng="pt">
    <trd>prescrever <klr tip="ind">(ordenar)</klr></trd>,
    <trd>ordenar</trd>,
    <trd>preceituar</trd>
  </trdgrp>
</drv>

<drv mrk="preskr.0o">
  <kap><tld/>o</kap>
  <snc mrk="preskr.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Skriba indiko de uzotaj medikamentoj kaj farotaj a&ubreve; nefarotaj agoj:
      <ekz>
        malsanulo devas obei la <tld/>ojn de sia kuracisto.<fnt>PIV2</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="recept.0o">recepto</ref>
    </refgrp>
    <trd lng="pl">recepta</trd>
  </snc>
  <snc>
    <dif>
      Detala a&ubreve;toritata ordono:
      <ekz>
        ili pekis kontra&ubreve; Viaj <tld/>oj.<fnt>Z</fnt>
      </ekz>
    </dif>
    <trd lng="pl">przepis</trd>
  </snc>
  <trdgrp lng="en">
    <trd>prescription</trd>,
    <trd>directions</trd>,
    <trd>orders</trd>
  </trdgrp>
</drv>
</art>
