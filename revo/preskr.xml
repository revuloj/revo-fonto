<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: preskr.xml,v 1.28 2024/04/03 14:22:20 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>preskrib</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="preskr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="preskr.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="ordon.0i">Ordoni</ref> skribe:
      <ekz>
        la medikamenton, kiun mi <tld/>is, alportu kiel eble plej balda&ubreve;
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>prescriure</trd>,
      <trd>receptar</trd>
    </trdgrp>
    <trd lng="de">verschreiben</trd>
    <trdgrp lng="es">
      <trd>prescribir</trd>,
      <trd>recetar<klr> (un tratamiento m&eacute;dico)</klr></trd>
    </trdgrp>
    <trd lng="hu">fel&iacute;r <klr tip="ind">(orvos)</klr></trd>
    <trd lng="pl">przepisa&cacute;</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
    <trdgrp lng="zh">
    <trd>&#24320;&#33647; <pr>k&amacron;iy&agrave;o</pr></trd>,
    <trd>&#38283;&#34277; <pr>k&amacron;iy&agrave;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="preskr.0i.KOMUNE">
    <dif>
      Ordoni formale:
      <ekz>
        se al la re&gcirc;o pla&ccirc;as, oni preskribu ekstermi ilin
        <fnt>
          <bib>MT</bib>
          <lok>Ester 3:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la diplomata ceremoniaro <tld/>as la titoladon al diversaj altranguloj en
        diversaj situacioj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>preceptuar</trd>,
      <trd>establir</trd>
    </trdgrp>
    <trd lng="en">stipulate</trd>
    <trd lng="hu">el&odblac;&iacute;r</trd>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_p;&c_i;&c_s;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="direkt.0i.ies_konduton">direkti<sncref/></ref>,
    <ref cel="instru1.0i.KOMUNE">instrukcii</ref>,
    <ref cel="regul.0igi">reguligi</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anordnen</trd>,
    <trd>vorschreiben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>prescribe</trd>,
    <trd>order</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enjoindre</trd>,
    <trd>prescrire</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21629;&#12376;&#12427;  <pr>&#12417;&#12356;&#12376;&#12427;</pr></trd>,
    <trd>&#35215;&#23450;&#12377;&#12427;  <pr>&#12365;&#12390;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#20966;&#26041;&#12377;&#12427;  <pr>&#12375;&#12423;&#12411;&#12358;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">voorschrijven</trd>
  <trdgrp lng="pt">
    <trd>prescrever <klr tip="ind">(ordenar)</klr></trd>,
    <trd>ordenar</trd>,
    <trd>preceituar</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>predp&iacute;sa&tcaron;</trd>,
    <trd>vyrubi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>poru&ccaron;it</trd>,
    <trd>p&rcaron;edepsat</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_p;&c_i;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_l;&c_ib;&c_k;&c_i; &c_ib; &c_t;.&c_p;.)</trd>,
    <trd>&c_n;&c_a;&c_k;&c_a;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21659; <pr>n&iacute;ng</pr></trd>,
    <trd>&#22144; <pr>n&iacute;ng</pr></trd>,
    <trd>&#21545;&#21648; <pr>f&emacron;nf&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="preskr.0o">
  <kap><tld/>o</kap>
  <snc mrk="preskr.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Skriba indiko de uzotaj medikamentoj kaj farotaj a&ubreve; nefarotaj
      agoj:
      <ekz>
        malsanulo devas obei la <tld/>ojn de sia
        kuracisto<fnt>PIV2</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="recept.0o.MED">recepto</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>prescripci&oacute;</trd>,
      <trd>recepta</trd>
    </trdgrp>
    <trd lng="de">Verschreibung</trd>
    <trd lng="es">prescripci&oacute;n</trd>
    <trdgrp lng="fr">
      <trd>prescription</trd>,
      <trd>ordonnance</trd>
    </trdgrp>
    <trd lng="pl">recepta</trd>
  </snc>
  <snc>
    <dif>
      Detala a&ubreve;toritata <ref tip="super" cel="ordon.0o.volo">ordono</ref>:
      <ekz>
        li faris dek orajn kandelabrojn la&ubreve; la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 4:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        religia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 19:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        oratoro, pledanta anta&ubreve; la ju&gcirc;istaro, nepre devas observi tiujn <tld/>ojn
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili pekis kontra&ubreve; Viaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 9:29</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="direkt.0o.instrukcio">direkto<sncref/></ref>,
      <ref cel="direktiv.0o">direktivo</ref>,
      <ref cel="norm.0o.konvencio">normo<sncref/></ref>,
      <ref cel="regul.0o">regulo</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>precepte</trd>,
      <trd>disposici&oacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Vorschrift</trd>,
      <trd>Anordnung</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>prescripci&oacute;n</trd>,
      <trd>norma</trd>
    </trdgrp>
    <trd lng="pl">przepis</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_d;&c_p;&c_ib;&c_s;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_k;&c_a;&c_z;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_r;&c_a;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dispozice</trd>,
    <trd>na&rcaron;&iacute;zen&iacute;</trd>,
    <trd>preskripce</trd>,
    <trd>p&rcaron;edpis</trd>,
    <trd>vym&ecaron;&rcaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="de">Verordnung</trd>
  <trdgrp lng="en">
    <trd>prescription</trd>,
    <trd>directions</trd>,
    <trd>orders</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commandements</trd>,
    <trd>directives</trd>,
    <trd>ordonnance</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#23450;&#12417;  <pr>&#12373;&#12384;&#12417;</pr></trd>,
    <trd>&#35215;&#23450;  <pr>&#12365;&#12390;&#12356;</pr></trd>,
    <trd>&#20966;&#26041;&#31627;  <pr>&#12375;&#12423;&#12411;&#12358;&#12379;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">voorschrift</trd>
  <trd lng="sk">predpis</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_p;&c_i;&c_s;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_zh;&c_je;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33647;&#26041; <pr>y&agrave;of&amacron;ng</pr></trd>,
    <trd>&#34277;&#26041; <pr>y&agrave;of&amacron;ng</pr></trd>,
    <trd>&#25351;&#31034; <pr>zh&#464;sh&igrave;</pr></trd>,
    <trd>&#26041;&#21058; <pr>f&amacron;ngj&igrave;</pr></trd>,
    <trd>&#26041;&#21137; <pr>f&amacron;ngj&igrave;</pr></trd>,
    <trd>&#25106;&#24459; <pr>ji&egrave;l&#476;</pr></trd>,
    <trd>&#35489;&#21629; <pr>ji&egrave;m&igrave;ng</pr></trd>,
    <trd>&#35819;&#21629; <pr>ji&egrave;m&igrave;ng</pr></trd>
  </trdgrp>
</drv>
</art>
