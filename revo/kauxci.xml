<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kauxci.xml,v 1.20 2022/03/21 17:51:57 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>ka&ubreve;ci</rad>/o
</kap>
<drv mrk="kauxci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Monsumo, kiun oni deponas, a&ubreve; kiun oni promesas pagi por garantii
      la plenumon de interkonsento a&ubreve; kontrakto farita de si mem a&ubreve; de
      alia:
      <ekz>doni, havigi <tld/>on;</ekz>
      <ekz>liberigi akuziton per, kontra&ubreve; <tld/>o.</ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_l;&c_o;&c_g;</trd>,
      <trd>&c_z;&c_a;&c_k;&c_l;&c_a;&c_d;</trd>
    </trdgrp>
    <trd lng="ca">fian&ccedil;a</trd>
    <trdgrp lng="de">
      <trd>Kaution</trd>,
      <trd>B&uuml;rgschaft</trd>,
      <trd>Sicherheit</trd>
    </trdgrp>
    <trd lng="en">bail</trd>
    <trd lng="es">fianza</trd>
    <trd lng="fr">caution</trd>
    <trdgrp lng="hu">
      <trd>&oacute;vad&eacute;k</trd>,
      <trd>kauci&oacute;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>cautie</trd>,
      <trd>borgsom</trd>,
      <trd>statiegeld</trd>,
      <trd>waarborg</trd>
    </trdgrp>
  </snc>
  <trd lng="pl">kaucja</trd>
  <trd lng="ro">cau&tcedil;iune</trd>
  <trd lng="ru">&c_z;&c_a;&c_l;&c_o;&c_g;</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x441;&#x442;&#x430;&#x432;&#x430;</trd>,
    <trd>&#x43F;&#x43E;&#x440;&#x443;&#x43A;&#x430;</trd>,
    <trd>&#x437;&#x430;&#x431;&#x435;&#x437;&#x43F;&#x435;&#x447;&#x435;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="kauxci.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Doni <tld/>on por iu.  
    </dif>
  </snc>
    <trdgrp lng="be">
      <trd>&c_u;&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_z;&c_a;&c_l;&c_o;&c_g;</ind></trd>,
      <trd>&c_u;&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_z;&c_a;&c_k;&c_l;&c_a;&c_d;</ind></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>fian&ccedil;ar</trd>,
      <trd>avalar</trd>
    </trdgrp>
  <trdgrp lng="de">
     <trd>eine <ind>Kaution stellen</ind></trd>,
     <trd>b&uuml;rgen</trd>,
     <trd>Sicherheiten hinterlegen</trd>
  </trdgrp>
  <trd lng="en">bail<klr> (one) out of jail</klr></trd>
  <trdgrp lng="fr">
    <trd>cautionner</trd>,
    <trd>se porter <ind>caution</ind> (pour)</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>&oacute;vad&eacute;kot tesz le</trd>,
     <trd>kauci&oacute;t tesz le</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>waarborgen</trd>,
    <trd>borg staan</trd>
  </trdgrp>
  <trd lng="pl">wp&lstroke;aci&cacute; <ind>kaucj&eogonek;</ind></trd>
  <trd lng="ro">pl&abreve;tit <ind>cau&tcedil;iunea</ind></trd>
  <trd lng="ru">&c_v;&c_n;&c_je;&c_s;&c_t;&c_i; &c_z;&c_a;&c_l;&c_o;&c_g;</trd>
  <trdgrp lng="uk">
    <trd>&#x434;&#x430;&#x432;&#x430;&#x442;&#x438; &#x437;&#x430;&#x441;&#x442;&#x430;&#x432;&#x443;</trd>,
    <trd>&#x43F;&#x43E;&#x440;&#x443;&#x447;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
