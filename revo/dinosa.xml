<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dinosa.xml,v 1.20 2022/06/05 18:34:14 revo Exp $">
<kap>
  <rad>dinosa&ubreve;r</rad>/oj <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="dinosa.0o">
  <kap><tld/>o</kap>
  <snc mrk="dinosa.0o.PAL">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="dinosa.0oj"><tld/>oj</ref>.
    </dif>
    <trd lng="be">&c_d;&c_y;&c_n;&c_a;&c_z;&c_a;&c_w;&c_r;</trd>
    <trd lng="de">Dinosaurier</trd>
    <trdgrp lng="en">
      <trd>dinosaur</trd>,
      <trd>dino</trd>
    </trdgrp>
    <trd lng="fr">dinosaure</trd>
    <trd lng="ja">&#24656;&#x7ADC; [<ind>&#x304D;&#12423;&#12358;&#x308A;&#12421;&#12358;</ind>]</trd>
    <trdgrp lng="nl">
      <trd>dinosaurus</trd>,
      <trd>dinosauri&euml;r</trd>,
      <trd>dino</trd>
    </trdgrp>
    <trd lng="pl">dinozaur</trd>
  </snc>
</drv>

<drv mrk="dinosa.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="dinosa.0oj.ZOO">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="grup.0o.BIO" lst="voko:zoologiaj_grupoj">Grupo</ref>
      de <ref tip="lst" cel="verteb.0ulo" lst="voko:dinosa&ubreve;roj">vertebruloj</ref>, kiuj dominis 
      de la meza <ref cel="trias.0o">triaso</ref>
      tra <ref cel="jxuras.0o">&jcirc;uraso</ref> &gcirc;is 
      la fino de <ref cel="kretac.0o">kretaceo</ref>,
      karakterizitaj per malgranda kranio, kruroj vertikale starantaj
      sub la korpo kaj forte disvolvitaj malanta&ubreve;aj kruroj,
      grandaj inter grandeco de kato &gcirc;is dekkvinmetra alto kaj 
      kvardekmetra longo, de kiuj plu vivas hodia&ubreve; nur la 
      <ref tip="prt" cel="bird.0oj">birdoj</ref>:
      <ekz>
        <tld/>oj ne kreskis je konstanta rapido kiel mamuloj,
        sed pli rapide kreskis dum varmaj periodoj; pro tio,
        oni povas vidi jarringojn en la ostoj
        <fnt>
          <aut>F. van Hertrooij</aut>,
          <vrk><url ref="https://scivolemo.wordpress.com/2018/02/28/la-gigantoj-de-la-pratempo/">
          La gigantoj de la&nbsp;pratempo</url></vrk>,
          <lok>Scivolemo, 2018-02-28</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj roloj en la ekosistemo, kiuj anta&ubreve;e estis plenigitaj de
        bestoj kiel <ind>nebirdaj <tld/>oj</ind>, subite estis malplenaj
        kaj tio donis eblecojn al aliaj bestoj
        <fnt>
          <aut>F. van Hertrooij</aut>,
          <vrk><url
          ref="https://scivolemo.wordpress.com/2019/07/03/krokodiloj-pli-mirindas-ol-vi-pensas/">
          Krokodiloj pli mirindas ol vi pensas</url></vrk>,
          <lok>Scivolemo, 2019-07-03</lok>
        </fnt>.
        <trd lng="de">Nichtvogeldinosaurier</trd>
        <trd lng="en">non-avian dinosaurs</trd>
        <trd lng="fr">dinosaures non aviaires</trd>
        <trd lng="ja">&#x975E;&#x9CE5;&#x985E;&#x578B;&#24656;&#x7ADC;
        [<ind>&#12402;&#12385;&#12423;&#12358;&#x308B;&#12356;&#12369;&#12356;&#x304D;&#12423;&#12358;&#x308A;&#12421;&#12358;</ind>]</trd>
        
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_n;&c_a;&c_z;&c_a;&c_w;&c_r;&c_y;</trd>
  <trdgrp lng="de">
    <trd>Dinosaurier</trd>,
    <trd>Dinosauria</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>dinosaurs</trd>,
    <trd>Dinosauria</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>dinosaures</trd>,
    <trd>Dinosauria</trd>
  </trdgrp>
  <trd lng="hu">dinoszauruszok</trd>
  <trd lng="ja">&#24656;&#x7ADC;&#x985E; [<ind>&#x304D;&#12423;&#12358;&#x308A;&#12421;&#12358;&#x308B;&#12356;</ind>]</trd>
  <trd lng="la">Dinosauria</trd>
  <trdgrp lng="nl">
    <trd>dinosauri&euml;rs</trd>,
    <trd>Dinosauria</trd>
  </trdgrp>
  <trd lng="pl">dinozaury</trd>
  <trd lng="pt">dinossauros</trd>
  <trd lng="ru">&c_d;&c_i;&c_n;&c_o;&c_z;&c_a;&c_v;&c_r;&c_y;</trd>
  <trd lng="uk">&c_d;&c_i;&#x43D;&#x43E;&c_z;&c_a;&c_v;&c_r;&c_i;</trd>
</drv>

</art>
