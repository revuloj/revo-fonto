<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dinosa&ubreve;r</rad>/oj
</kap>

<drv mrk="dinosa.0o">
  <kap><tld/>o</kap>
  <snc mrk="dinosa.0o.PAL">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="dinosa.0oj"><tld/>oj</ref>.
    </dif>
    <trd lng="de">Dinosaurier</trd>
    <trd lng="en">dinosaur</trd>
    <trd lng="fr">dinosaure</trd>
    <trd lng="pl">dinozaur</trd>
  </snc>
</drv>

<drv mrk="dinosa.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="dinosa.0oj.ZOO">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="grup.0o.BIO"
      lst="voko:zoologiaj_grupoj">Grupo</ref>
      de <ref tip="lst" cel="verteb.0ulo" 
      lst="voko:dinosa&ubreve;roj">vertebruloj</ref>, kiuj dominis 
      de la meza <ref cel="trias.0o">triaso</ref>
      tra <ref cel="jxuras.0o">&jcirc;uraso</ref> &gcirc;is 
      la fino de <ref cel="kretac.0o">kretaceo</ref>,
      karakterizitaj per malgranda kranio, kruroj vertikale starantaj
      sub la korpo kaj forte disvolvitaj malanta&ubreve;aj kruroj,
      grandaj inter grandeco de kato &gcirc;is dekkvinmetra alto kaj 
      kvardekmetra longo, de kiuj plu vivas hodia&ubreve; nur la 
      <ref tip="prt" cel="bird.0oj">birdoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_n;&c_a;&c_z;&c_a;&c_w;&c_r;&c_y;</trd>
  <trdgrp lng="de">
    <trd>Dinosaurier</trd>,
    <trd>Dinosauria</trd>
  </trdgrp>
  <trd lng="fr">dinosaures</trd>
  <trd lng="hu">dinoszauruszok</trd>
  <trd lng="pl">dinozaury</trd>
  <trd lng="pt">dinossauros</trd>
  <trd lng="ru">&c_d;&c_i;&c_n;&c_o;&c_z;&c_a;&c_v;&c_r;&c_y;</trd>
</drv>

</art>
