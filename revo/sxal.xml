<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;al</rad>/o
</kap>
<drv mrk="sxal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Vasta lana a&ubreve; silka tuko, kiun persono portas sur la &scirc;ultroj.
    </dif>
    <refgrp tip="sin">
      <ref cel="skarp.0o">skarpo</ref>,
      <ref cel="plejd.0o">plejdo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Schal</trd>
  <trd lng="en">shawl</trd>
  <trd lng="fr">chal</trd>
  <trd lng="fr">ch&acirc;le</trd>
  <trd lng="nl">sjaal</trd>
  <trd lng="ru">&c_sh;&c_a;&c_l;&c_mol;</trd>
  <trd lng="sv">schal</trd>
</drv>
</art>
