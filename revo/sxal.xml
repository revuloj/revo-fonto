<vortaro>
<art mrk="$Id: sxal.xml,v 1.27 2024/03/25 07:28:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;al</rad>/o
</kap>
<drv mrk="sxal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxal.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Vasta lana a&ubreve; silka tuko, per kiu oni kovras la &scirc;ultrojn:
      <ekz>
        &scirc;i nur ekmovis la manojn sub flava <tld/>o, kovranta
preska&ubreve; tutan
        &scirc;ian malgrasan korpon
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 16a, p. 87a</lok>
</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mantil.0o.TEKS">mantilo</ref>,
      <ref cel="plejd.0o">plejdo</ref>,
      <ref cel="skarp.0o">skarpo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_ib;&c_k;</trd>,
    <trd>&c_h;&c_u;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_sh;&c_a;&c_l;</trd>
  <trd lng="bo">&#xF41;&#xF0B;&#xF56;&#xF4F;&#xF42;&#xF66;&#xF0B;</trd>
  <trd lng="ca">xal</trd>
  <trd lng="de">Schal</trd>
  <trd lng="el">&sigma;&alfa_ton;&lambda;&jota;</trd>
  <trd lng="en">shawl</trd>
  <trd lng="es">chal</trd>
  <trd lng="fr">ch&acirc;le</trd>
  <trd lng="hu">v&aacute;llkend&odblac;</trd>
  <trd lng="it">scialle</trd>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30E7;&#x30FC;&#x30EB;</trd>,
    <trd>&#x80A9;&#x639B;&#x3051;  <pr>&#x304B;&#x305F;&#x304B;&#x3051;</pr></trd>
  </trdgrp>
  <trd lng="nl">sjaal</trd>
  <trd lng="pl">szal</trd>
  <trdgrp lng="pt">
    <trd>xale</trd>,
    <trd>xaile</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_a;&c_l;&c_mol;</trd>
  <trd lng="sv">schal</trd>
  <trdgrp lng="zh">
    <trd>&#x62AB;&#x80A9; <pr>p&#x12B;ji&#x101;n</pr></trd>,
    <trd>&#x62AB;&#x5DFE; <pr>p&#x12B;j&#x12B;n</pr></trd>
  </trdgrp>
</drv>
</art>
