<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;al</rad>/o
</kap>
<drv mrk="sxal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Vasta lana a&ubreve; silka tuko, kiun oni portas kiel <ref tip="super"
      cel="vest.sur0o">survesto</ref>n sur la &scirc;ultroj:
      <ekz>
        &scirc;i nur ekmovis la manojn sub flava <tld/>o, kovranta preska&ubreve; tutan
        &scirc;ian malgrasan korpon
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 16a, p. 87a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="plejd.0o">plejdo</ref>,
      <ref cel="skarp.0o">skarpo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_ib;&c_k;</trd>,
    <trd>&c_h;&c_u;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Schal</trd>
  <trd lng="el">&sigma;&alfa_ton;&lambda;&jota;</trd>
  <trd lng="en">shawl</trd>
  <trd lng="es">chal</trd>
  <trd lng="fr">ch&acirc;le</trd>
  <trd lng="hu">v&aacute;llkend&odblac;</trd>
  <trd lng="nl">sjaal</trd>
  <trdgrp lng="pt">
    <trd>xale</trd>,
    <trd>xaile</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_a;&c_l;&c_mol;</trd>
  <trd lng="sv">schal</trd>
</drv>
</art>
