<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arhxitektur.xml,v 1.15 2022/04/25 15:11:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>ar&hcirc;itektur</rad>/o,
  <var><kap><ofc>8</ofc><rad var="k">arkitektur</rad>/o</kap></var>
</kap>
<drv mrk="arhxitektur.0o">
  <kap><ofc>*</ofc><tld/>o,
    <var><kap><tld var="k"/>o</kap></var>
  </kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="lst" cel="art.bel0o" lst="voko:artoj">Belarto</ref>
      pri planado kaj farado de konstrua&jcirc;oj:
      <ekz>
        &ccirc;i tiuj gigantaj laboroj postulis, krom konoj de la astronomio,
        geometrio, me&hcirc;aniko kaj <tld/>o, anka&ubreve; perfektan organizon
        <fnt><bib>Far1</bib>, <lok>Anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        li okupis sin anka&ubreve; pri <tld/>o, grafiko, ilustrado de tekstoj, muzika komponado
        kaj desegnado
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2014/011335.php">Pentristo de la homo kaj de la naturo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="art.konstru0o.ARKI">konstruarto</ref>
    </refgrp>
  </snc>
  <snc mrk="arhxitektur.0o.FIG">
    <dif>
      &Gcirc;enerala <ref tip="super" cel="arangx.0o.KOMUNE">aran&gcirc;o</ref>,
      <ref tip="super" cel="strukt.0o.konstruo">strukturo</ref>
      de tuto en diversaj, kunfunkciaj partoj:
      <ekz>
        domo de tiel bela <tld/>o
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kial vi vizitis tiujn urbojn &#x2010;
        &ccirc;u por vidi la ma&ubreve;ran <tld var="k"/>on
        <fnt><bib>SatirRak</bib>, <lok>supersti&ccirc;o</lok></fnt>?
      </ekz>
      <ekz>
        imponas <tld/>o en <klr>[...]</klr> irakaj urboj &mdash; oficejoj,
        hospitaloj, e&ccirc; simplaj vendejoj okulfrapas pro (almena&ubreve; la&ubreve;
        mi) avangarda beleco
        <fnt>
          <aut>Paul BLASKOWICZ</aut>: <vrk>Iraka ar&hcirc;itekturo</vrk>.
          <bib>Monato</bib>, <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="arhxitektur.0o.KOMP">
      <uzo tip="fak">KOMP</uzo>
      <dif>
        &Gcirc;enerala aran&gcirc;o de komputika sistemo,
        kun &gcirc;iaj aplika&jcirc;oj, interkomunikoj, rutinoj,
        a&ubreve; kun &gcirc;ia ma&scirc;inaro:
        <ekz>
          Ana jam dekomence planis pli sekuran <tld var="k"/>on de la datumbazoj
          <fnt>
            <aut>M. Fettes</aut>
            <vrk>en: <url ref="http://www.liberafolio.org/2018/02/27/uea-promesas-novan-retejon-ene-de-kelkaj-monatoj/">
            UEA promesas novan retejon ene de kelkaj monatoj</url></vrk>
            <lok>libera folio, 2018-02-27</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_t;&c_e;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>  
  <trdgrp lng="br">      
    <trd>adeiladouriezh</trd>,    
    <trd>tisavouriezh</trd>
  </trdgrp>
  <trd lng="ca">arquitectura</trd>
  <trd lng="de">Architektur</trd>
  <trd lng="en">architecture</trd>
  <trd lng="es">arquitectura</trd>
  <trd lng="fa">&a_m;&a_ayn;&a_m;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">architecture</trd>
  <trd lng="he">&#x5D0;&#x5E8;&#x5DB;&#x5D9;&#x5D8;&#x5E7;&#x5D8;&#x5D5;&#x5E8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;p&iacute;t&odblac;m&udblac;v&eacute;szet</trd>,
    <trd>architekt&uacute;ra</trd>
  </trdgrp>
  <trd lng="id">arsitektur</trd>
  <trd lng="pl">architektura</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_t;&c_je;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="sv">arkitektur</trd>
  <trd lng="uk">&#x430;&#x440;&#x445;&#x456;&#x442;&#x435;&#x43A;&#x442;&#x443;&#x440;&#x430;</trd>
</drv>

<drv mrk="arhxitektur.0isto">
  <kap><tld/>isto,
    <var><kap><tld var="k"/>isto</kap></var>
  </kap>
  <snc>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Fakulo pri <tld/>o;
      <ref tip="sin" cel="arhxitekt.0o">ar&hcirc;itekto, arkitekto</ref>:
      <ekz>
        mi ricevos oficon en la oficejo de la <tld/>isto de la urbo Varsovio
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        li ser&ccirc;is la urbon havantan la fundamentojn, kies <tld/>isto kaj konstruanto
        estas Dio.
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 11:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi volas fari&gcirc;i <tld/>isto, eniri en la karieron artistan
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_h;&c_ib;&c_t;&c_e;&c_k;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="br">      
    <trd>adeiladour</trd>,    
    <trd>tisavour</trd>
  </trdgrp>
  <trd lng="ca">arquitecte</trd>
  <trd lng="de">Architekt</trd>
  <trd lng="en">architect</trd>
  <trd lng="es">arquitecto</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_ayn;&a_m;&a_A;&a_r;</trd>,
    <trd>&a_m;&a_h;&a_n;&a_d;&a_s; <ind>&a_m;&a_ayn;&a_m;&a_A;&a_r;&f_y;</ind></trd>,
    <trd>&a_A_madda;&a_r;&a_sh;&f_y;&a_t;&f_k;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">architecte</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5D3;&#x5B0;&#x5E8;&#x5B4;&#x5D9;&#x5DB;&#x5B8;&#x5DC;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;p&iacute;t&odblac;m&udblac;v&eacute;sz</trd>,
    <trd>&eacute;p&iacute;t&eacute;sz</trd>
  </trdgrp>
  <trd lng="id">arsitek</trd>
  <trd lng="pl">architekt</trd>
  <trd lng="ru">&c_a;&c_r;&c_h;&c_i;&c_t;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">architekt</trd>
  <trd lng="sv">arkitekt</trd>
</drv>
</art>

