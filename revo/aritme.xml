<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>aritmetik</rad>/o
    </kap>
    <drv mrk="aritme.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Bran&ccirc;o de matematiko studanta operaciojn super entjeroj kaj
          frakcioj (racionalaj nombroj, racionoj):
          <ekz>la kvar <tld/>aj operacioj estas adicio, subtraho,
          multipliko kaj divido</ekz>.   
        </dif>
      </snc>
      <trd lng="en">arithmetic</trd>
      <trd lng="ru">&#x430;&#x440;&#x438;&#x444;&#x43c;&#x435;&#x442;&#x438;&#x43a;&#x430;</trd>
    </drv>
    <drv mrk="aritme.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Plenumi <tld/>ajn operaciojn:
          <ekz>la komputilo <tld/>as en la duuma nombrosistemo.</ekz>
        </dif>
      </snc>
    </drv>
    <drv mrk="aritme.0ilo">
      <kap><tld/>ilo</kap>
      <snc>
        <dif>
          Rimedo por plenumi <tld/>ajn operaciojn; kalkulilo.
        </dif>
      </snc>
      <snc>
        <dif>
          Tiu organo de komputilo kiu plenumas <tld/>ajn (kaj ofte
          anka&ubreve; la logikajn) operaciojn.
        </dif>
      </snc>
      <trdgrp lng="en">
        <trd>arithmetic and logical unit</trd>,
        <trd>ALU</trd></trdgrp>
      <trdgrp lng="ru">
        <trd>&#x430;&#x440;&#x438;&#x444;&#x43c;&#x435;&#x442;&#x438;&#x447;&#x435;&#x441;&#x43a;&#x43e;&#x435; &#x443;&#x441;&#x442;&#x440;&#x43e;&#x439;&#x441;&#x442;&#x432;&#x43e;</trd>,
        <trd>&#x430;&#x440;&#x438;&#x444;&#x43c;&#x435;&#x442;&#x438;&#x447;&#x435;&#x441;&#x43a;&#x43e;-&#x43b;&#x43e;&#x433;&#x438;&#x447;&#x435;&#x441;&#x43a;&#x43e;&#x435; &#x443;&#x441;&#x442;&#x440;&#x43e;&#x439;&#x441;&#x442;&#x432;&#x43e;</trd>,
        <trd>&#x410;&#x41b;&#x423;</trd></trdgrp>
    </drv>
  </art>
