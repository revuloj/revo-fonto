<art mrk="$Id: sxankr.xml,v 1.20 2023/10/21 07:46:48 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>&scirc;ankr</rad>/o
</kap>
<drv mrk="sxankr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Speco de <ref tip="super" cel="ulcer.0o">ulcero</ref> per kiu
      infektoj facile eniras la korpon:
      <ekz>
        la parazito estigas tipon de <tld/>o &ccirc;efe trovitan en la bu&scirc;a-naza kavo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=538662">Trichomonas gallinae</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_n;&c_k;&c_r;</trd>
  <trd lng="ca">xancre</trd>
  <trd lng="de">Schanker</trd>
  <trdgrp lng="en">
    <trd>canker</trd>,
    <trd>chancre</trd>,
    <trd>venereal ulcer</trd>
  </trdgrp>
  <trd lng="es">chancro</trd>
  <trd lng="fr">chancre</trd>
  <trd lng="hu">sanker</trd>
  <trdgrp lng="it">
  <trd>piaga</trd>, 
  <trd>ulcera</trd>, 
  <trd>ulcerazione<klr> (lesione)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x4E0B;&#x75B3;  <pr>&#x3052;&#x304B;&#x3093;</pr></trd>
  <trd lng="nl">sjanker</trd>
  <trd lng="pl">szankier</trd>
  <trd lng="pt">cancro</trd>
  <trd lng="ru">&c_sh;&c_a;&c_n;&c_k;&c_r;</trd>
  <trd lng="uk">&#x448;&#x430;&#x43D;&#x43A;&#x435;&#x440; (&#x432;&#x438;&#x440;&#x430;&#x437;&#x43A;&#x430; &#x43D;&#x430; &#x441;&#x442;&#x430;&#x442;&#x435;&#x432;&#x438;&#x445; &#x43E;&#x440;&#x433;&#x430;&#x43D;&#x430;&#x445;)</trd>
</drv>
