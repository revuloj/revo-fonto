<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>visk</rad>/o
</kap>
<drv mrk="visk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="lorant1.0oj">lorantacoj</ref>
      (<trd lng="la">Viscum</trd>),
      duonparazita vegeta&jcirc;o kun blanketaj &scirc;ajnberoj,
      kreskanta sur arboj, el &gcirc;i oni ekstraktas gluon:
      <ekz>
        Eneo <klr>[&#8230;]</klr> akiris &scirc;oson de <tld/>o, kiu malfermos al li pordegon en la Subteran Mondon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=40123">Eneo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        Blanka <tld/>o a&ubreve; Blanka arboperlo estas globforma, duonparazita planto, vivanta sur deciduaj arboj kiaj robinio, poplo, tilio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21973">Blanka visko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_ja;&c_l;&c_a;</trd>
  <trd lng="bg">&c_b;&c_ja;&c_l; &c_i;&c_m;&c_je;&c_l;</trd>
  <trd lng="ca">vesc</trd>
  <trd lng="cs">jmel&iacute;</trd>
  <trd lng="de">Mistel</trd>
  <trd lng="en">mistletoe</trd>
  <trd lng="es">mu&eacute;rdago</trd>
  <trd lng="fa">&a_d;&a_A;&a_r;&a_w;&a_A;&a_sh;</trd>
  <trd lng="hu">fagy&ouml;ngy</trd>
  <trd lng="nl">maretak</trd>
  <trdgrp lng="pt">
    <trd>visco</trd>,
    <trd>visgo</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_m;&c_je;&c_l;&c_a;</trd>
  <trd lng="sk">imelo</trd>
</drv>
</art>
