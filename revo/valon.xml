<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>valon</rad>/o
</kap>
<drv mrk="valon.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="de">Wallone</trd>
  <trd lng="en">Walloon</trd>
  <trd lng="es">val&oacute;n</trd>
  <trd lng="fr">Wallon</trd>
  <trd lng="nl">Waal</trd>
</drv>
<drv mrk="valon.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj a&ubreve; ilia nacieco.
    </dif>
  </snc>
  <trd lng="de">wallonisch</trd>
  <trd lng="en">Walloon</trd>
  <trd lng="fr">wallon</trd>
  <trd lng="nl">Waals</trd>
</drv>
<drv mrk="valon.V0ujo">
  <kap><tld lit="V"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La suda franclingva kaj parte germanlingva regiono de 
      <ref tip="malprt" cel="belg.B0ujo">Belgujo</ref>,
      konsistanta el la provincoj 
      <ref tip="prt" cel="liegx.0o.prov">Lie&gcirc;o</ref>, 
      <ref tip="prt" cel="namur.0o.prov">Namuro</ref>,
      <ref tip="prt" cel="henegov.0o">Henegovio</ref>, 
      <ref tip="prt" cel="luksemb.0o.prov">Luksemburgo</ref> kaj 
      <ref tip="prt" cel="brabant.0o"><tld lit="V"/>a Brabanto</ref>: <ekz>Jos&eacute; Happart (PS)
      i&gcirc;is konata en la okdekaj jaroj kiel <tld/>a gvidanto la regiono Voer, grupo de ses
      flandraj vila&gcirc;etoj kiujn li volis aneksi al <tld lit="V"/>ujo. <fnt>
      <vrk>Flandra Gazetara Elekto 2001</vrk>
      </fnt>.</ekz>
    </dif>
  </snc>
 
  <ref tip="sin" cel="valoni.0o">Valonio</ref>
  <trd lng="de">Wallonien</trd> 
  <trd lng="cs">Valonsko</trd>
  <trd lng="en">Wallonia</trd> 
  <trd lng="es">Valonia</trd>
  <trd lng="fi">Vallonia</trd>
  <trd lng="fr">Wallonie</trd>
  <trd lng="nl">Walloni&euml;</trd>
  <trd lng="pl">Walonia</trd>
  <trd lng="sk">Val&oacute;nsko</trd>
  </drv>
</art>


