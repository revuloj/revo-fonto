<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: laf.xml,v 1.20 2019/10/05 17:10:22 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>laf</rad>/o
</kap>
<drv mrk="laf.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">GEOL</uzo>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="magm.0o.GEOL">Magmo</ref>,
      kiu elfluas el <ref tip="malprt" cel="vulkan.0o">vulkanoj</ref>
      kaj solidi&gcirc;as malvarmi&gcirc;ante:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kuirita en infer' &#8208; &gcirc;i estis brula kiel <tld/>o
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko"
              >La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        historie <nom>Oyama</nom> erupcias &ccirc;. &ccirc;iun 20an jaron: elfluas
        <tld/>o kaj post kelkaj
        semajnoj &ccirc;io fini&gcirc;as
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007195.html"
              >Japanio: Kiam verda insulo nigri&gcirc;as</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        fortaj eksplodoj ne okazas dum la erupcio de baza magmo, sed
        esti&gcirc;as rapide
        fluantaj <tld/>ofluoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007234.html"
              >Vulkanoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Pompejo <klr>[&#8230;]</klr> ne estis super&scirc;utita de &gcirc;ia <tld/>o
        <klr>[&#8230;]</klr> sed de
        10 metroj da cindro kaj <tld/>o&scirc;tonetoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010426.php"
              >Poluri la bildon de Pompejo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        jam erupciis <nom>Ni&scirc;ino&scirc;ima</nom> en 1973, sed 40 jarojn poste
        la <tld/>o grandigis kaj
        altigis la insulon
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2015/011447.php"
              >Kiam tremetoj fari&gcirc;as tremegoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_v;&c_a;
    <klr>(&c_v;&c_u;&c_l;&c_k;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
  <trdgrp lng="br">
    <trd>lava</trd>,
    <trd>maen-teuz</trd>
  </trdgrp>
  <trd lng="ca">lava</trd>
  <trd lng="de">Lava</trd>
  <trd lng="el">&lambda;&alfa_ton;&beta;&alfa;</trd>
  <trd lng="en">lava</trd>
  <trd lng="es">lava</trd>
  <trd lng="fr">lave</trd>
  <trd lng="hu">l&aacute;va</trd>
  <trdgrp lng="id">
    <trd>lahar</trd>,
    <trd>lava</trd>
  </trdgrp>
  <trd lng="nl">lava</trd>
  <trd lng="pl">lawa</trd>
  <trd lng="pt">lava</trd>
  <trd lng="ro">lav&abreve;</trd>
  <trd lng="ru">&c_l;&c_a;&c_v;&c_a;</trd>
  <trd lng="sv">lava</trd>
</drv>
</art>
