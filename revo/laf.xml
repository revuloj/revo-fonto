<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>laf</rad>/o
</kap>
<drv mrk="laf.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">GEOL</uzo>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Fandita varmega materio, kiu elfluas el vulkanoj kaj solidi&gcirc;as
      malvarmi&gcirc;ante.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_v;&c_a; 
    <klr>(&c_v;&c_u;&c_l;&c_k;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>
  <trd lng="en">lava</trd>
  <trd lng="es">lava</trd>
  <trd lng="fr">lave</trd>
  <trd lng="nl">lava</trd>
  <trd lng="ru">&c_l;&c_a;&c_v;&c_a;</trd>
  <trd lng="sv">lava</trd>
</drv>
</art>
