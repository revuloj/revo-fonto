<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>erbi</rad>/o
  </kap>
  <drv mrk="erbi.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">KEM</uzo>
    <mlg kod="IUPAC">Er</mlg>
    <snc>
      <dif>
        &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
        lst="voko:&hcirc;emiaj_elementoj" val="68">elemento</ref> kun 
        atomnumero 68, atompezo 167,26;
        ar&gcirc;entgriza <ref tip="super" cel="metal.0o">metalo</ref>;
        lantanoido.
      </dif>
    </snc>
    <trd lng="be">&c_e;&c_r;&c_b;&c_ib;&c_j;</trd>  
    <trd lng="bg">&c_je;&c_r;&c_b;&c_i;&c_j;</trd>
    <trd lng="br">erbiom</trd>
    <trd lng="ca">erbi</trd>
    <trd lng="de">Erbium</trd>
    <trd lng="en">erbium</trd>
    <trd lng="es">erbio</trd>
    <trd lng="fa">&a_A;&a_kasra;&a_r;&a_b;&f_y;&a_damma;&a_m;</trd>
    <trd lng="fr">erbium</trd>
    <trd lng="hu">erbium</trd>
    <trd lng="it">erbio</trd>
    <trd lng="la">Erbium</trd>
    <trd lng="pl">erb</trd>
    <trd lng="pt">&eacute;rbio</trd>
    <trd lng="ru">&c_e;&c_r;&c_b;&c_i;&c_j;</trd>
    <trd lng="sv">erbium</trd>
</drv>
</art>
