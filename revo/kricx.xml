<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kricx.xml,v 1.13 2023/12/03 09:59:01 revo Exp $">
<kap>
  <rad>kri&ccirc;</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="kricx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kricx.0i.SON">
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> a&ubreve; <ref tip="super" cel="blek.0i.beste">bleki</ref> akre, subite, kiel iuj rabobirdoj:
      <ekz>
        ili <tld/>is, kiel la papagoj kutimas
        <fnt><bib>SatirRak</bib>, <lok>supersti&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        metale vibrantaj akre <tld/>antaj frapoj
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        43a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">kreischen</trd>
  <trd lng="en">screech</trd>
  <trd lng="fr">glapir</trd>
  <trdgrp lng="ja">
    <trd>&#x91D1;&#x5207;&#x308A;&#x58F0;&#x3092;&#x4E0A;&#x3052;&#x308B;  <pr>&#x304B;&#x306A;&#x304D;&#x308A;&#x3054;&#x3048;&#x3092;&#x3042;&#x3052;&#x308B;</pr></trd>,
    <trd>&#x30AD;&#x30FC;&#x30C3;&#x3068;&#x9CF4;&#x304F;  <pr>&#x30AD;&#x30FC;&#x30C3;&#x3068;&#x306A;&#x304F;</pr></trd>,
    <trd>&#x30AD;&#x30FC;&#x30AD;&#x30FC;&#x9CF4;&#x308B;  <pr>&#x30AD;&#x30FC;&#x30AD;&#x30FC;&#x306A;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>krijsen</trd>,
    <trd>schreeuwen<klr tip="ind"> (krijsen)</klr></trd>
  </trdgrp>
  <trd lng="pl">kwili&cacute;<klr> (o ptakach drapie&zdot;nych)</klr></trd>
  <trd lng="pt">gritar <klr tip="amb">(como ave de rapina)</klr></trd>
  <trd lng="uk">&#x43A;&#x440;&#x438;&#x447;&#x430;&#x442;&#x438; (&#x43F;&#x440;&#x43E; &#x445;&#x438;&#x436;&#x438;&#x445; &#x43F;&#x442;&#x430;&#x445;&#x456;&#x432;)</trd>
  <trdgrp lng="zh">
    <trd>&#x5693; <pr>c&#x101;</pr></trd>,
    <trd>&#x58F0;&#x97F3;&#x5C16;&#x9510; <pr>sh&#x113;ngy&#x12B;nji&#x101;nru&#xEC;</pr></trd>,
    <trd>&#x8072;&#x97F3;&#x5C16;&#x92B3; <pr>sh&#x113;ngy&#x12B;nji&#x101;nru&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kricx.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="kricx.0.SON">
    <dif>
      Mallonga, akuta
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">bleko</ref>
      a&ubreve; simila
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">sono</ref>:
      <ekz>
        la vulpo trafas la kaptilon kaj krias <tld/>, <tld/>
        <fnt>
          <aut>trad. Simatani T.</aut>,
          <vrk><url ref="http://www.wombat.zaq.ne.jp/esperanto/noakom/n_konk.htm">
          Onklo-Vulpo</url></vrk>,
          <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>



<drv mrk="kricx.0o">
  <kap><tld/>o</kap>
  <snc mrk="kricx.0o.SON">
    <dif>
      Akra mallonga bruo:
      <ekz>
        super ni estis bruego, kriado kaj <tld/>ado
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;apitro 11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gekreische</trd>
  <trd lng="en">screech</trd>
  <trd lng="fr">glapissement</trd>
  <trdgrp lng="ja">
    <trd>&#x91D1;&#x5207;&#x308A;&#x58F0;  <pr>&#x304B;&#x306A;&#x304D;&#x308A;&#x3054;&#x3048;</pr></trd>,
    <trd>&#x30AD;&#x30FC;&#x30C3;&#x3068;&#x3044;&#x3046;&#x9CF4;&#x304D;&#x58F0;  <pr>&#x30AD;&#x30FC;&#x30C3;&#x3068;&#x3044;&#x3046;&#x306A;&#x304D;&#x3054;&#x3048;</pr></trd>,
    <trd>&#x30AD;&#x30FC;&#x30AD;&#x30FC;&#x9CF4;&#x308B;&#x97F3;  <pr>&#x30AD;&#x30FC;&#x30AD;&#x30FC;&#x306A;&#x308B;&#x304A;&#x3068;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>gekrijs</trd>,
    <trd>schreeuw</trd>
  </trdgrp>
  <trd lng="pl">kwilenie</trd>
  <trdgrp lng="zh">
    <trd>&#x5C16;&#x53EB; <pr>ji&#x101;nji&#xE0;o</pr></trd>,
    <trd>&#x5C16;&#x92B3;&#x7684;&#x8072;&#x97F3; <pr>ji&#x101;nru&#xEC;desh&#x113;ngy&#x12B;n</pr></trd>,
    <trd>&#x5C16;&#x9510;&#x7684;&#x58F0;&#x97F3; <pr>ji&#x101;nru&#xEC;desh&#x113;ngy&#x12B;n</pr></trd>
  </trdgrp>
</drv>
</art>

