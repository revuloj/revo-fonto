<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: revu.xml,v 1.31 2023/10/20 19:20:37 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>revu</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="revu.0o">
  <kap><tld/>o</kap>
  <snc mrk="revu.0o.MIL">
    <dif>
      Solena parada <ref tip="super" cel="inspek.0o">inspekt</ref>ado
      al soldata trupo ks:
      <ekz>
        <tld/>o de la armeo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        matena <tld/>o de la gvardio
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la generalo <nom>Kirsanov</nom>, eksigita post malsukcesa <tld/>o, transloki&gcirc;is kun la
        edzino Peterburgon
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ramzes</nom> postulis, ke oni <klr>[&#8230;]</klr> montru al li la ri&ccirc;a&jcirc;ojn de la provinco,
        la <tld/>o <klr>[&#8230;]</klr> da&ubreve;ris kelke da semajnoj, &ccirc;iutage <klr>[&#8230;]</klr>
        venis diversaj metiistaj korporacioj, <klr>[&#8230;]</klr> por montri al la princo
        siajn produktojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekspoz1.0o">ekspozicio</ref>,
      <ref cel="kontro.0o">kontrolo</ref>,
      <ref cel="parad.0o.MIL">parado<sncref/></ref>,
      <ref cel="rigard.super0o.kontrolo">superrigardo<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_a;&c_g;&c_l;&c_ja;&c_d;
    <klr>(&c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_y;)</klr></trd>
    <trdgrp lng="ca">
      <trd>ressenya</trd>,
      <trd>inspecci&oacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Truppenmusterung</trd>,
      <trd>Truppeninspektion</trd>,
      <trd>Truppenschau</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>d&iacute;szszemle</trd>,
      <trd>csapatszemle</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_o;&c_je;&c_n;&c_n;&c_y;&c_j; <ind>&c_s;&c_m;&c_o;&c_t;&c_r;</ind></trd>
  </snc>
  <snc mrk="revu.0o.INF_artikolo">
    <dif>
      Gazeta <ref tip="super" cel="artiko.0o.teksto">artikolo</ref> pri aktuala&jcirc;oj en iu aparta
      fako.
    </dif>
    <refgrp tip="vid">
      <ref cel="feliet.0o">felietono</ref>,
      <ref cel="kronik1.0o">kroniko</ref>,
      <ref cel="raport">raporto</ref>,
      <ref cel="recenz">recenzo</ref>,
      <ref cel="rigard.super0o.revuo">superrigardo<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_a;&c_g;&c_l;&c_ja;&c_d; <klr>(&c_u; &c_p;&c_r;&c_e;&c_s;&c_je;)</klr></trd>
    <trd lng="ca">gaseta</trd>
    <trd lng="de">Zeitungsbericht</trd>
    <trd lng="hu">szemle</trd>
    <trd lng="ru">&c_o;&c_b;&c_o;&c_z;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="revu.0o.INF_gazeto">
    <dif>
      Perioda, sed ne &ccirc;iutaga <ref tip="super" cel="pres.0ajxo">presa&jcirc;o</ref>
      en formo de <ref tip="super" cel="gazet.0o">gazeto</ref> a&ubreve; bro&scirc;uro kaj
      speciale dedi&ccirc;ita al iu aparta fako:
      <ekz>
        internacia scienca <tld/>o;
      </ekz>
      <ekz>
        tie a&ubreve; aliloke en diversaj gazetoj kaj <tld/>oj aperas artikoloj plenaj de aprobo
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        via edzo laboras &ccirc;e unu el la ilustritaj <tld/>oj, eble oni tie bezonus iun, kiu
        scias desegni
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        pedagogia <tld/>o, eldonita de la flandra ministrejo pri edukado
         <fnt>
          <aut>Guido van Damme</aut>
          <vrk><url ref="&Monato;/1995/A950102.php">Gejunula seksumado</url></vrk>
          <bib>Monato</bib>
          <lok>1995/01, p. 10</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       fak<ref cel="jxurna.0o.TIP">&jcirc;urnalo</ref>,
       <ref cel="kompen.0o">kompendio</ref>
    </refgrp>
    <trd lng="be">&c_ch;&c_a;&c_s;&c_o;&c_p;&c_ib;&c_s;</trd>
    <trd lng="ca">magaz&iacute;n</trd>
    <trdgrp lng="de">
      <trd>Zeitschrift</trd>,
      <trd>Umschau <klr>(Zeitschrift)</klr></trd>,
      <trd>Rundschau <klr>(Zeitschrift)</klr></trd>,
      <trd>Revue  <klr>(Zeitschrift)</klr></trd>
    </trdgrp>
    <trd lng="hu">foly&oacute;irat</trd>
    <trd lng="nl">tijdschrift</trd>
    <trd lng="ru">&c_zh;&c_u;&c_r;&c_n;&c_a;&c_l;</trd>
  </snc>
  <snc mrk="revu.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="super" cel="teatr.0ajxo">Teatra&jcirc;o</ref> temanta pri aktuala&jcirc;oj
      prezentataj en fantazia kaj &scirc;erca maniero:
      <ekz>
        fama svedlanda &jcirc;azkapelo el Stokholmo, la nesuperebla <tld/>o <klr>[&#8230;]</klr> kun
        la dek ok mondfamaj anglaj dancistinoj
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        rigardante solenajn viza&gcirc;ojn, jubile kantantajn la himnon en &ccirc;i tiu teatro de
        operetoj kaj <tld/>oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi e&ccirc; ne scias, kio min plej mirigis en &ccirc;i tiu luksega <tld/>ejo, &ccirc;u la teatro
        mem, &ccirc;u tio, kion mi surprize &ccirc;i tie a&ubreve;dis
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kabare.0o">kabareto</ref>,
      <ref cel="komedi.0o.TEA">komedio<sncref/></ref>,
      <ref cel="muzikal.0o">muzikalo</ref>,
      <ref cel="oper.0eto">opereto</ref>,
      <ref cel="variet.0o">varieteo</ref>,
      <ref cel="vodevi.0o">vodevilo</ref>
    </refgrp>
    <trd lng="be">&c_r;&c_e;&c_v;&c_ju;</trd>
    <trdgrp lng="de">
      <trd>Kabarett</trd>,
      <trd>Revue<ind> [ark]</ind></trd>
    </trdgrp>
    <trd lng="hu">boh&oacute;zat</trd>
    <trd lng="ru">&c_r;&c_je;&c_v;&c_ju;</trd>
  </snc>
  <trd lng="ca">revista</trd>
  <trd lng="cs">vojensk&aacute; p&rcaron;ehl&iacute;dka</trd>
  <trd lng="en">review</trd>
  <trd lng="es">revista</trd>
  <trd lng="fr">revue</trd>
  <trdgrp lng="ja">
    <trd>&#x5C02;&#38272;&#x96D1;&#x8A8C;  <pr>&#x305B;&#12435;&#12418;&#12435;&#12374;&#12387;&#12375;</pr></trd>,
    <trd>&#x8A55;&#x8AD6;&#x96D1;&#x8A8C;  <pr>&#12402;&#12423;&#12358;&#x308D;&#12435;&#12374;&#12387;&#12375;</pr></trd>,
    <trd>&#x8A55;&#x8AD6;  <pr>&#12402;&#12423;&#12358;&#x308D;&#12435;</pr></trd>,
    <trd>&#x95B2;&#20853;  <pr>&#12360;&#12387;&#x307A;&#12356;</pr></trd>,
    <trd>&#x95B2;&#20853;&#x5F0F;  <pr>&#12360;&#12387;&#12410;&#12356;&#12375;&#12365;</pr></trd>,
    <trd>&#x30EC;&#x30D3;&#x30E5;&#x30FC;  <pr>&#x308C;&#12403;&#12421;&#x30FC;</pr></trd>,
    <trd>&#x98A8;&#x523A;&#21127;  <pr>&#12405;&#12358;&#12375;&#12370;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">revue</trd>
  <trdgrp lng="pl">
    <trd>przegl&aogonek;d</trd>,
    <trd>czasopismo</trd>
  </trdgrp>
  <trd lng="pt">revista</trd>
  <trd lng="ro">vedere</trd>
  <trd lng="sk">vojensk&aacute; prehliadka</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_u;&c_r;&#x43D;&c_a;&#x43B;</trd>,
    <trd>&#x43E;&c_g;&#x43B;&#x44F;&c_d;</trd>,
    <trd>&c_v;&c_ib;&c_j;&c_s;&#x44C;&#x43A;&#x43E;&c_v;&c_i;&c_j; &#x43E;&c_g;&#x43B;&#x44F;&c_d;</trd>,
    <trd>&#x43F;&c_a;&c_r;&c_a;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="revu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="revu.0i.ekzameni">
    <dif>
      Fari <tld/>on <sncref ref="revu.0o.MIL"/> a&ubreve; <sncref ref="revu.0o.INF_artikolo"/>:
      <ekz>
        la re&gcirc;o <nom>Jehoram</nom> eliris el Samario kaj <tld/>is la tutan Izraelon
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 3:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum &jcirc;usa kontrolkunsido en <klr>[&#8230;]</klr> li <tld/>is la rolon de la ministerio
        <fnt>
          <vrk><url
          ref="http://www.radiohc.cu/eo/noticias/nacionales/173950-diaz-canel-elstarigas-la-rolon-de-turismo-kaj-fremdaj-investoj-por-la-kuba-ekonomio">D&iacute;az-Canel
          elstarigas la rolon de ...</url></vrk>
          <lok>Radio Havano Kubo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzame.0i.PED">ekzameni</ref>,
      <ref cel="kontro.0i.administracie">kontroli<sncref/></ref>,
      <ref cel="revizi.0i.dogane">revizii<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>revistar</trd>,
    <trd>passar revista</trd>
  </trdgrp>
  <trd lng="cs">p&rcaron;ehl&iacute;&zcaron;et</trd>
    <trdgrp lng="de">
      <trd>&uuml;berpr&uuml;fen</trd>,
      <trd>nachpr&uuml;fen</trd>,
      <trd>nachsehen</trd>,
      <trd>inspizieren</trd>,
      <trd>mustern</trd>
    </trdgrp>
  <trd lng="es">pasar revista</trd>
  <trd lng="fr">passer en revue</trd>
  <trdgrp lng="hu">
    <trd>megszeml&eacute;l</trd>,
    <trd>szeml&eacute;z</trd>,
    <trd>&aacute;ttekint</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8A55;&#x8AD6;&#12377;&#x308B;  <pr>&#12402;&#12423;&#12358;&#x308D;&#12435;&#12377;&#x308B;</pr></trd>,
    <trd>&#x95B2;&#20853;&#12377;&#x308B; <pr>&#12360;&#12387;&#12410;&#12356;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>een <ind>parade</ind> afnemen</trd>,
    <trd>inspecteren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dokonywa&cacute; przegl&aogonek;du</trd>,
    <trd>lustrowa&cacute;</trd>,
    <trd>napisa&cacute; przegl&aogonek;d</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>revista</trd>,
    <trd>passar em revista</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>inspecta</trd>,
    <trd>controla</trd>,
    <trd>scrie o recenzie</trd>
  </trdgrp>
  <trd lng="sk">prehliada&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_g;&#x43B;&#x44F;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i;/&#x43F;&c_r;&#x43E;&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i; &#x43E;&c_g;&#x43B;&#x44F;&c_d;</trd>
  </trdgrp>
</drv>
</art>
