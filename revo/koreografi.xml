<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: koreografi.xml,v 1.4 2022/05/01 08:09:25 afido Exp $">
    <kap>
      <rad>koreografi</rad>/o,
      <var><kap><rad var="&hcirc;">&hcirc;oreografi</rad>/o</kap></var>,
      <var><kap><rad var="e">koregrafi</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
    </kap>

<drv mrk="koreografi.0o">
  <kap><tld/>o, 
    <var><kap><tld var="&hcirc;"/>o</kap></var>,
    <var><kap><tld var="e"/>o</kap></var>
  </kap>
  <snc>
    <dif>
      Danc<ref tip="super" cel="arangx.0o.KOMUNE">aran&gcirc;o</ref>, 
      t.e. komponita vico de danc<ref tip="prt" cel="mov.0o">movo</ref>j, aparte por sceneja
      prezento:
      <ekz>
        neniu dubis, ke la litova teamo prezentinta dancon, kies <tld/>o harmoniis kun
        adaptita muziko de <nom>Johan Strauss</nom>, meritas la titolon de monda &ccirc;ampiono
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Straznickas</aut>
          <vrk><url ref="&Steloj;monato2/006824.html">Litovio: Monda &ccirc;ampionado
          de dancoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        majstro de <tld var="&hcirc;"/>o
        <fnt>
          <bib>KFl</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[&scirc;i]</klr> parolis kun kolegino, farante tutan nervan <tld var="e"/>on de
        rapidaj gestoj
        <fnt>
          <bib>ChC</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="balet.0o.teatrajxo">baleto<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_h;&c_a;&c_r;&c_e;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
    <trd lng="de">Choreografie</trd>
    <trd lng="en">choreography</trd>
    <trd lng="fr">chor&eacute;graphie</trd>
    <trd lng="ru">&c_h;&c_o;&c_r;&c_je;&c_o;&c_g;&c_r;&c_a;&#769;&c_f;&c_i;&c_ja;</trd>
  </snc>
</drv>

  </art>

