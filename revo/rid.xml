<drv mrk="rid.0eto">
  <kap><ofc>*</ofc><tld/>eto</kap>
  <snc mrk="rid.0eto.KOMUNE">
    <dif>
      Afabla bu&scirc;a kaj viza&gcirc;a esprimo de bonvolo:
      <ekz>
        sur lia viza&gcirc;o mi vidis &gcirc;ojan <tld/>eton
        <fnt><bib>F</bib>, <lok>&FE; 38</lok></fnt>;
      </ekz>
      <ekz>
        virina <tld/>eto pli kaptas ol reto
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
         <tld/>eto aperis sur &scirc;ia bu&scirc;o kaj la sango revenis en &scirc;iajn
         vangojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgranda knabino kun ru&gcirc;aj vangoj, kun <tld/>eto &ccirc;irka&ubreve; la bu&scirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eto ludis &ccirc;irka&ubreve; &scirc;ia bu&scirc;o
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>;
      </ekz>
      <ekz>
        bonkora <tld/>eto ornamis &scirc;iajn dikajn lipojn
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        melankolia <tld/>eto sur la lipoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
         kun <tld/>eto de kompato 
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>eto glacii&gcirc;is sur la duone malfermitaj lipoj 
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;erca <tld/>eto malaperis de la bu&scirc;o kaj okuloj de la
        nekonato
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">somr&iacute;s</trd>
  <trd lng="cs">&uacute;sm&ecaron;v</trd>
  <trd lng="de">L&auml;cheln</trd>
  <trd lng="es">sonrisa</trd>
  <trd lng="fr">sourire<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="hu">mosoly</trd>
  <trd lng="id">senyum</trd>
  <trd lng="nl">glimlach</trd>
  <trd lng="ro">z&acirc;mbet</trd>
  <trd lng="ru">&c_u;&c_l;&c_y;&c_b;&c_k;&c_a;</trd>
  <trd lng="sk">&uacute;smev</trd>
</drv>

<drv mrk="rid.0iga">
  <kap><tld/>iga</kap>
