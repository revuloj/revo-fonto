<drv mrk="rid.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rid.pri0i.KOMUNE">
    <dif>
      <tld lit="R"/>i pro amuzi&gcirc;o a&ubreve; pro
      <ref tip="vid" cel="mok.0i.PSI">moko</ref> pri...;
      <ref tip="vid" cel="rid.0i.priridi">ridi <sncref/></ref>:
      <ekz>
        li &scirc;ajnis mensogulo, kaj la parencoj pri<tld/>is lin
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo kovru vin, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        re&gcirc;ido Hiacinto <klr>[&#x2026;]</klr> ne komprenis, ke la homoj 
        la&ubreve;dantaj lian nazon pri<tld/>as lin
        <fnt>
          <aut>J.-M. Leprince de Beaumont, trad. D. J. Harlow</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Noveloj/blua/hiacinto.html">
          Re&gcirc;ido Hiacinto kaj la kara
          Re&gcirc;idineto</url></vrk>,
          <lok>[2007]</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; debati kaj pri<tld/>i aliajn pensmanierojn
        <klr>[&#x2026;]</klr> oni trovu solvon ta&ubreve;gan por &ccirc;iuj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010341.php">Kiom da homoj, tiom
          da gustoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>auslachen</trd>,
    <trd>verlachen</trd>
  </trdgrp>
  <trd lng="fr">rire de</trd>
</drv>

<drv mrk="rid.sat0i">
  <kap>sat<tld/>i</kap>
  <snc mrk="rid.sat0i.KOMUNE">
    <dif>
      <tld lit="R"/>i &gcirc;is la stato en kiu oni ne plu deziras
      <tld/>i:
      <ekz>
        sat<tld/>is la lo&gcirc;antaro, sed poste ili beda&ubreve;ris
        sinjoron
        paro&hcirc;estron
        <fnt>
          <aut>J. Mahen, trad. Esperantista Rondeto</aut>,
          <vrk><url ref="http://www.literatura.bucek.name/mahen/koboldo.html">
          La koboldo Ondra</url></vrk>,
          <lok> 1921 </lok>
        </fnt>;
      </ekz>
      <ekz>
        vi sat<tld/>os, &#x2010; flustris la du junuloj, transpasante
        la &ccirc;ambron
        <fnt>
          <aut>J. K. Jerome, trad. G. Badash</aut>,
          <vrk>Tri viroj en boato</vrk>,
          <lok>1934</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">enfitar-se de riure</trd>
  <trdgrp lng="es">
    <trd>hartarse de re&iacute;r</trd>,
    <trd>partirse de risa</trd>
  </trdgrp>
  <trd lng="nl">zich slap <ind>lachen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_s;&c_m;&c_je;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_h;&c_o;&c_h;&c_o;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="rid.sub0i">
  <kap>sub<tld/>i</kap>
  <snc mrk="rid.sub0i.KOMUNE">
    <dif>
      Ka&scirc;e <tld/>i, subpremi sian <tld/>on:
      <ekz>
        Ilu-ittia sub<tld/>is
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 11a, pluaj
        insidoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>riure sorneguerament</trd>,
    <trd>riure per sota el nas</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>potaj&iacute; se sm&aacute;t</trd>,
    <trd>sm&aacute;t se v duchu</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>re&iacute;r por lo bajo</trd>,
    <trd>reprimir la risa</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pouffer</trd>,
    <trd>rire dans sa barbe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kuncog</trd>,
    <trd>somolyog</trd>
  </trdgrp>
  <trd lng="nl">in zijn vuistje <ind>lachen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_m;&c_je;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_s;&c_m;&c_je;&c_h;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">potajomky sa smia&tcaron;</trd>
</drv>

<drv mrk="rid.trud0eti">
