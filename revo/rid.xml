<drv mrk="rid.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rid.0igi.KOMUNE">
    <dif>
      Vole a&ubreve; nevole ka&ubreve;zi ies <tld/>on:
      <ekz>
        lia malgaja viza&gcirc;o <tld/>igis lian amikon
        <fnt><bib>FK</bib>, <lok>nur unu vorton!</lok></fnt>;
      </ekz>
      <ekz>
        la reagoj de &Scaron;vejk <tld/>igas pro la &scirc;ajna manko de
        logika ligo inter burokrataj demandoj kaj liaj respondoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan MacGill</aut>
          <vrk><url ref="&Monato;/2006/008748.php">Ruzo en
          rifuzo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pri la x-sistemo li verdiktas: &leftquot;tiu Esperanto-karikatura&jcirc;o
        <klr>[&#x2026;]</klr>
        forte <tld/>igas niajn kontra&ubreve;ulojn kaj decidigas ilin neniam
        lerni lingvon tiagrade barbaran
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2010/009999.php">Interlingvo:
          komparo inter du solvoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="amuz.0i">amuzi</ref>,
      <ref cel="gaj.0igi">gajigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>fer riure</trd>,
    <trd>causar risa</trd>
  </trdgrp>
  <trd lng="cs">rozesm&aacute;t <klr>(n&ecaron;koho)</klr></trd>
  <trd lng="es">hacer re&iacute;r</trd>
  <trd lng="fr">faire rire</trd>
  <trd lng="hu">(meg)<ind>nevettet</ind></trd>
  <trd lng="nl">doen <ind>lachen</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;
    <ind>&c_s;&c_m;&c_je;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</ind></trd>,
    <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_s;&c_m;&c_je;&c_h;</ind></trd>
  </trdgrp>
  <trd lng="sk">rozosmia&tcaron; <klr>(niekoho)</klr></trd>
</drv>

<drv mrk="rid.0inda">
  <kap><tld/>inda</kap>
