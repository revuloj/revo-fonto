<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>signal</rad>/o
</kap>
<drv mrk="signal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Interkonsentita signo destinita por komunikigi malproksimen ordonon,
      averton, sciigon:
      <ekz>
        mi ne rimarkis la <tld/>on;
      </ekz>
      <ekz>
        tuj post la unua <tld/>o ekmovi&gcirc;u;
      </ekz>
      <ekz>
        fervoja <tld/>o;
      </ekz>
      <ekz>
        <tld/>fajro;
      </ekz>
      <ekz>
        <tld/>vorto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ib;&c_g;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Signal</trd>,
    <trd>Zeichen</trd>
  </trdgrp>
  <trd lng="fr">signal</trd>
  <trdgrp lng="hu">
    <trd>jel</trd>,
    <trd>jelz&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">signaal</trd>
  <trd lng="ru">&c_s;&c_i;&c_g;&c_n;&c_a;&c_l;</trd>
</drv>

<drv mrk="signal.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on, anonci per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ib;&c_g;&c_n;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_ib;&c_g;&c_n;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>signalisieren</trd>,
    <trd>anzeigen</trd>
  </trdgrp>
  <trd lng="fr">signaler</trd>
  <trd lng="hu">jelez</trd>  
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_g;&c_n;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_i;&c_g;&c_n;&c_a;&c_l;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_t;&c_mol; <ind>&c_s;&c_i;&c_g;&c_n;&c_a;&c_l;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="signal.voj0igo">
  <kap>voj<tld/>igo</kap>
  <snc>
    <dif>
      Ekipado de la vojoj per facile rimarkeblaj signoj por sendan&gcirc;erigi
      la trafikon.
    </dif>
  </snc>
  <trd lng="de">Verkehrszeichen</trd>
  <trd lng="fr">signalisation</trd>
  <trd lng="hu">k&ouml;z&uacute;ti <ind>jelz&eacute;s</ind></trd>  
  <trd lng="nl">bewegwijzering</trd>
  <trd lng="ru">&c_o;&c_s;&c_n;&c_a;&c_shch;&c_je;&c_n;&c_i;&c_je;
<ind>&c_d;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_y;&c_m;&c_i;
&c_z;&c_n;&c_a;&c_k;&c_a;&c_m;&c_i;</ind></trd>  
</drv>
</art>
