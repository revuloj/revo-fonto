  <ofc>*</ofc>
  <rad>signal</rad>/o
</kap>
<drv mrk="signal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Interkonsentita signo destinita por komuniki malproksimen
      ordonon, averton, sciigon:
      <ekz>
        sonigu la trumpeton, <klr>[&#x2026;]</klr> faru signalon;
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 6:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni trumpetis sur la re&gcirc;a &scirc;ipo, ripetante <tld/>on, donitan de la supro de l'
        rivero
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li donis la <tld/>on komenci la batalon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        per fajra <tld/>o li montris, ke li bezonas helpon
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        jam la duan fojon estas donita la <tld/>o per la sonorilo, kiu admonas &ccirc;iujn
        personojn, kiuj ne veturas, forlasi la &scirc;ipon
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la submajstro faris <tld/>on, ke oni suprenigu la ankron
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la melodio de dormo<tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vitalij</nom> venis la uzinon tuj post la siren<tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        brua <tld/>o de rapide preterpasanta a&ubreve;tomobilo
        <fnt>
          <bib>Tor</bib>
          <lok>IV.</lok>
        </fnt>;
      </ekz>
      <ekz>
        por averti la a&ubreve;tiston, poste mi mallevos la brakon kaj &ccirc;e proksimi&gcirc;o mi donos
        definitivan halt<tld/>on
        <fnt>
          <bib>KrM</bib>
          <lok>28. Tute Libera Kiel Birdo Aera.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la sorto de l' italaj tradiciaj partioj devus esti forta alarm<tld/>o por &ccirc;iuj
        grandaj partioj
       <fnt>
          <aut>Stefan Maul</aut>
          
          <vrk><url ref="&Monato;/1993/A930802.php">Alarmsignalo el Italio</url></vrk>
          <bib>Monato</bib>
          <lok>1993/08, p. 5</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ib;&c_g;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_i;&c_g;&c_n;&c_a;&c_l;</trd>
  <trdgrp lng="cs">
    <trd>impuls</trd>,
    <trd>informace</trd>,
    <trd>sign&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Signal</trd>,
    <trd>Zeichen</trd>
  </trdgrp>
  <trd lng="en">signal</trd>
  <trd lng="es">se&ntilde;al</trd>
  <trd lng="fr">signal</trd>
  <trdgrp lng="hu">
    <trd>jel</trd>,
    <trd>jelz&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4FE1;&#x53F7;  <pr>&#x3057;&#x3093;&#x3054;&#x3046;</pr></trd>,
    <trd>&#x5408;&#x56F3;  <pr>&#x3042;&#x3044;&#x305A;</pr></trd>,
    <trd>&#x6A19;&#x8B58;  <pr>&#x3072;&#x3087;&#x3046;&#x3057;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">signaal</trd>
  <trdgrp lng="pl">
  <trd>sygna&lstroke;</trd>,
  <trd>znak</trd>
  </trdgrp>
  <trd lng="pt">sinalr</trd>
  <trd lng="ru">&c_s;&c_i;&c_g;&c_n;&c_a;&c_l;</trd>
  <trdgrp lng="sk">
    <trd>impulz</trd>,
    <trd>inform&aacute;cia</trd>,
    <trd>sign&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_gu;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4FE1;&#x53F7; <pr>x&#xEC;nh&#xE0;o</pr></trd>,
    <trd>&#x4FE1;&#x865F; <pr>x&#xEC;nh&#xE0;o</pr></trd>,
    <trd>&#x53F7;&#x5FD7; <pr>h&#xE0;ozh&#xEC;</pr></trd>,
    <trd>&#x865F;&#x8A8C; <pr>h&#xE0;ozh&#xEC;</pr></trd>,
    <trd>&#x8A0A;&#x865F; <pr>x&#xF9;nh&#xE0;o</pr></trd>,
    <trd>&#x8BAF;&#x53F7; <pr>x&#xF9;nh&#xE0;o</pr></trd>,
    <trd>&#x9EBE; <pr>hu&#x12B;</pr></trd>,
    <trd>&#x4FE1;&#x6807; <pr>x&#xEC;nbi&#x101;o</pr></trd>,
    <trd>&#x4FE1;&#x6A19; <pr>x&#xEC;nbi&#x101;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="signal.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on, anonci per <tld/>o:
      <ekz>
        oni <tld/>is de la palaco
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is per la fajfilo, ke la kanajlaro forigu siajn vestojn
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vaporfajfilo <tld/>is laborkomencon
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patrolo flag<tld/>is &#x201A;atendu&#x2018;
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 44</lok>
        </fnt>;
      </ekz>
      <ekz>
        pafo el tercerolo <tld/>as batalon
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IX.</lok>
        </fnt>;
      </ekz>
      <ekz>
        paf! oni <tld/>is per kanono la alvenon de la nova jaro
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toj iras rapide kaj ne emas tro abrupte bremsi, kiam mi man<tld/>as
        <fnt>
          <bib>KrM</bib>
          <lok>28. Tute Libera Kiel Birdo Aera.</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia frato rapide <tld/>is per la posta&jcirc;o, ke mi tuj eliru
        <fnt>
          <bib>KrB</bib>
          <lok>Pacaj batalantoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        grava libro Kvaropo <klr>[&#x2026;]</klr> <tld/>is la debuton de la tielnomita &leftquot;skota
        skolo&rightquot;
        <fnt>
          <bib>Plumamikoj</bib>
          <lok>Esperanta poezio: mirakla rikolto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="alarm.0i">alarmi</ref>,
      <ref cel="anonc.0i">anonci</ref>,
      <ref cel="avert.0i">averti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ib;&c_g;&c_n;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_ib;&c_g;&c_n;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>d&aacute;t znamen&iacute;</trd>,
    <trd>signalizovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>signalisieren</trd>,
    <trd>anzeigen</trd>
  </trdgrp>
  <trd lng="en">signal</trd>
  <trdgrp lng="es">
    <trd>se&ntilde;alar</trd>,
    <trd>hacer se&ntilde;ales</trd>
  </trdgrp>
  <trd lng="fr">signaler</trd>
  <trd lng="hu">jelez</trd>
  <trdgrp lng="ja">
    <trd>&#x4FE1;&#x53F7;&#x3092;&#x9001;&#x308B;  <pr>&#x3057;&#x3093;&#x3054;&#x3046;&#x3092;&#x304A;&#x304F;&#x308B;</pr></trd>,
    <trd>&#x5408;&#x56F3;&#x3059;&#x308B;  <pr>&#x3042;&#x3044;&#x305A;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
  <trd>sygnalizowa&cacute;</trd>,
  <trd>dawa&cacute; <ind>znaki</ind></trd>
  </trdgrp>
  <trd lng="pt">sinalizar</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_g;&c_n;&c_a;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_i;&c_g;&c_n;&c_a;&c_l;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_t;&c_mol; <ind>&c_s;&c_i;&c_g;&c_n;&c_a;&c_l;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>da&tcaron; znamenie</trd>,
    <trd>signalizova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_gu;&c_n;&c_a;&c_l;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_s;&c_i;&c_gu;&c_n;&c_a;&c_l;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9EBE; <pr>hu&#x12B;</pr></trd>,
    <trd>&#x5F04;&#x4E0A;&#x6807;&#x8BB0; <pr>l&#xF2;ngsh&#xE0;ngbi&#x101;oj&#xEC;</pr></trd>,
    <trd>&#x5F04;&#x4E0A;&#x6A19;&#x8A18; <pr>l&#xF2;ngsh&#xE0;ngbi&#x101;oj&#xEC;</pr></trd>,
    <trd>&#x5F04;&#x4E0A;&#x8A18;&#x865F; <pr>l&#xF2;ngsh&#xE0;ngj&#xEC;hao</pr></trd>,
    <trd>&#x5F04;&#x4E0A;&#x8BB0;&#x53F7; <pr>l&#xF2;ngsh&#xE0;ngj&#xEC;hao</pr></trd>,
    <trd>&#x6709;&#x6807;&#x8BB0; <pr>y&#x1D2;ubi&#x101;oj&#xEC;</pr></trd>,
    <trd>&#x6709;&#x6A19;&#x8A18; <pr>y&#x1D2;ubi&#x101;oj&#xEC;</pr></trd>,
    <trd>&#x7528;&#x4FE1;&#x53F7;&#x4F20;&#x9012; <pr>y&#xF2;ngx&#xEC;nh&#xE0;ochu&#xE1;nd&#xEC;</pr></trd>,
    <trd>&#x7528;&#x4FE1;&#x865F;&#x50B3;&#x905E; <pr>y&#xF2;ngx&#xEC;nh&#xE0;ochu&#xE1;nd&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="signal.voj0igo">
  <kap>voj<tld/>igo</kap>
  <snc>
    <dif>
      Ekipado de la vojoj per facile rimarkeblaj signoj por sendan&gcirc;erigi
      la trafikon.
    </dif>
    <ref tip="vid" cel="sxild.trafik0o">trafik&scirc;ildo</ref>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_t;&c_a;&c_n;&c_o;&c_w;&c_k;&c_a; &c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_y;&c_h; &c_z;&c_n;&c_a;&c_k;&c_a;&c_w;</trd>
  <trd lng="de">Beschilderung</trd>
  <trd lng="fr">signalisation</trd>
  <trd lng="hu">k&ouml;z&uacute;ti <ind>jelz&eacute;s</ind></trd>  
  <trd lng="nl">bewegwijzering</trd>
  <trd lng="pl">znak drogowy</trd>
  <trd lng="pt">sinaliza&ccedil;&atilde;o vi&aacute;ria</trd>
  <trd lng="ru">&c_o;&c_s;&c_n;&c_a;&c_shch;&c_je;&c_n;&c_i;&c_je; <ind>&c_d;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_y;&c_m;&c_i; &c_z;&c_n;&c_a;&c_k;&c_a;&c_m;&c_i;</ind></trd>  
  <trdgrp lng="zh">
    <trd>&#x53F7;&#x5FD7; <pr>h&#xE0;ozh&#xEC;</pr></trd>,
    <trd>&#x865F;&#x8A8C; <pr>h&#xE0;ozh&#xEC;</pr></trd>,
    <trd>&#x6307;&#x793A;&#x724C; <pr>zh&#x1D0;sh&#xEC;p&#xE1;i</pr></trd>
  </trdgrp>
</drv>
