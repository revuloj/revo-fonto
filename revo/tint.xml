<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tint</rad>/i
</kap>
<drv mrk="tint.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Interfrapi&gcirc;ante eligi sekan sonon:
      <ekz>
        la amboso <tld/>ante resonegis per la pezaj marteloj de la
        for&gcirc;istoj;
      </ekz>
      <ekz>
        la glavoj kaj arma&jcirc;oj krak<tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igi glasojn, monerojn;
      </ekz>
      <ekz>
        tia malbeno, ke &ccirc;e &ccirc;iu, kiu tion a&ubreve;dus, ek<tld/>us en amba&ubreve; liaj
        oreloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve;ita de granda sekvantaro, kun bruo kaj <tld/>o, la faraono
        eniris en la salonon<fnt>B</fnt>;
      </ekz>
      <ekz>
        a&ubreve;di&gcirc;is <tld/>ado de armiloj, hufo<tld/>ado de &ccirc;evalo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_r;&c_a;&c_z;&c_g;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_r;&c_y;&c_n;&c_mol;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_b;&c_r;&c_y;&c_n;&c_mol;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">klirren</trd>
    <trd lng="fr">tinter</trd>
    <trdgrp lng="hu">
      <trd>cseng</trd>,
      <trd>kong</trd>,
      <trd>cs&ouml;r&ouml;g</trd>,
      <trd>kolompol</trd>,
      <trd>koccan</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_r;&c_ja;&c_c;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_b;&c_r;&c_je;&c_n;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_v;&c_ja;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_v;&c_ja;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mallerte ludi muzikilon:
      <ekz>
        li povas iom <tld/>i sur fortepiano<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ili <tld/>as sur psalteroj, pensante, ke ili estas muzikistoj
        egalaj al David<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_r;&c_y;&c_n;&c_mol;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_b;&c_r;&c_y;&c_n;&c_mol;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">klimpern</trd>
    <trdgrp lng="hu">
      <trd>p&ouml;ty&ouml;g <klr tip="ind">(hangszeren)</klr></trd>,
      <trd>klimp&iacute;rozik</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_r;&c_je;&c_n;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="en">tinkle</trd>
  <trdgrp lng="nl">
    <trd>rinkelen</trd>,
    <trd>kletteren</trd>,
    <trd>tingelen</trd>
  </trdgrp>
</drv>

<drv mrk="tint.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.frap0o">Frapinstrumento</ref>
      konsistanta el malplena metala sfereto entenanta metalan buleton,
      kiu per skui&gcirc;o estigas sonon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_z;&c_g;&c_o;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_m;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_m;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_v;&c_a;&c_n;&c_o;&c_ch;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Glocke</trd>
  <trd lng="fr">grelot</trd>
  <trdgrp lng="hu">
    <trd>cseng&odblac;</trd>,
    <trd>kolomp</trd>
  </trdgrp>
  <trd lng="nl">belletje</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_b;&c_je;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_b;&c_u;&c_b;&c_je;&c_n;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
