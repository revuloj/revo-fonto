<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tint.xml,v 1.34 2023/10/21 13:52:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tint</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tint.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tint.0i.SON">
    <dif>
      Interfrapi&gcirc;ante <ref tip="super" cel="son.0i.SON">soni</ref>
      per metaleca, vitreca sono:
      <ekz>
        gloru Lin per <tld/>antaj <ref cel="cimbal.0o">cimbalo</ref>j
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 150:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> iras kaj koketas, kaj <tld/>as per &ccirc;enetoj sur la piedoj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 3:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la glavoj kaj arma&jcirc;oj <klr>[&#x2026;]</klr> krak<tld/>is
        <fnt><bib>Fab2</bib>, <lok>malnova domo</lok></fnt>;
      </ekz>
      <ekz>
        Mi venigos <klr>[&#x2026;]</klr> tian malbonon, ke &ccirc;e &ccirc;iu,
        kiu tion a&ubreve;dos, ek<tld/>os en amba&ubreve; liaj oreloj
        <fnt><bib>MT</bib>, <lok>&Re2; 21:12</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>anta fera ringo vekas el la son&gcirc;oj de la libereco
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj teleroj <tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi a&ubreve;das, ke mia hakilo <tld/>as
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi dancis <klr>[&#x2026;]</klr> tiel, ke &ccirc;iuj sonoriletoj <tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klak.0i.KOMUNE">klaki</ref>,
      <ref cel="sonor.0i">sonori</ref>,
      <ref cel="tin2.00.SON">tin-tin</ref>,
      <ref cel="vibr.0i.sono">vibri<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_b;&c_r;&c_a;&c_z;&c_g;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_r;&c_y;&c_n;&c_mol;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_b;&c_r;&c_y;&c_n;&c_mol;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>klirren</trd>,
      <trd>klackern</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>sonner</trd>,
      <trd>tinter</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cseng</trd>,
      <trd>kong</trd>,
      <trd>cs&ouml;r&ouml;g</trd>,
      <trd>kolompol</trd>,
      <trd>koccan</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>rintoccare</trd>,
      <trd>tintinnare</trd>,
      <trd>vibrare<klr> (risuonare)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>brzd&aogonek;ka&cacute;</trd>,
      <trd>brzd&eogonek;ka&cacute;</trd>,
      <trd>brz&eogonek;cze&cacute;</trd>,
      <trd>dzwoni&cacute;</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>tinir</trd>,
    <trd>retinir</trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_r;&c_ja;&c_c;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_b;&c_r;&c_je;&c_n;&c_ch;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_v;&c_ja;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_v;&c_ja;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tint.0i.FIG">
    <uzo tip="fak">MUZ</uzo>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mallerte <ref tip="super" cel="lud.0i.MUZ">ludi</ref> muzikilon:
      <ekz>
        iom <tld/>i sur fortepiano &scirc;i povas
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        tiuj <klr>[&#x2026;]</klr> <tld/>as sur <ref cel="psalte.0o">psaltero</ref>j, pensante, ke ili
        estas
        muzikistoj egalaj al <nom>David</nom>
        <fnt><bib>MT</bib>, <lok>&Amo; 6:3-5</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_r;&c_y;&c_n;&c_mol;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_b;&c_r;&c_y;&c_n;&c_mol;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">klimpern</trd>
    <trdgrp lng="hu">
      <trd>p&ouml;ty&ouml;g <klr tip="ind">(hangszeren)</klr></trd>,
      <trd>klimp&iacute;rozik</trd>
    </trdgrp>
    <trd lng="it">strimpellare</trd>
    <trdgrp lng="pl">
      <trd>brzd&aogonek;ka&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_r;&c_je;&c_n;&c_ch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>b&rcaron;inkat</trd>,
    <trd>cinkat</trd>,
    <trd>klinkat</trd>,
    <trd>&rcaron;in&ccaron;et</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tinkle</trd>,
    <trd>jingle</trd>,
    <trd>clink</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C1;&#x30F3;&#x30C1;&#x30F3;&#x3068;&#x9CF4;&#x308B;  <pr>&#x30C1;&#x30F3;&#x30C1;&#x30F3;&#x3068;&#x306A;&#x308B;</pr></trd>,
    <trd>&#x30AB;&#x30C1;&#x30E3;&#x30AB;&#x30C1;&#x30E3;&#x9CF4;&#x308B;  <pr>&#x30AB;&#x30C1;&#x30E3;&#x30AB;&#x30C1;&#x30E3;&#x306A;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>rinkelen</trd>,
    <trd>kletteren</trd>,
    <trd>tingelen</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cvend&zcaron;a&tcaron;</trd>,
    <trd>rin&ccaron;a&tcaron;</trd>,
    <trd>&scaron;trnga&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&#x44F;&c_z;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_r;&#x44F;&c_zh;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_z;&c_v;&#x44F;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_z;&c_je;&#x43D;&#x44C;&#x43A;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="tint.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="tint.0o.SON">
    <dif>
      Iom akuta, metaleca a&ubreve; vitreca <ref tip="lst" cel="son.0o.SON" lst="voko:sonoj">sono</ref> 
      de io frapata:
      <ekz>
        li <klr>[&#x2026;]</klr> &ccirc;irka&ubreve;ita de granda sekvantaro de
        generaloj, kun bruo kaj <tld/>o eniris en la <klr>[&#x2026;]</klr> salonon
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz>
      <ekz>
        al unu &scirc;velis la lango kaj lipoj, alia havis <tld/>adon en la oreloj kaj nigrajn
        makulojn anta&ubreve; la okuloj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="en">
    <trd>tinkle</trd>,
    <trd>jingle</trd>,
    <trd>clink</trd>
  </trdgrp>
  <trd lng="fr">tintement</trd>
  <trdgrp lng="hu">
    <trd>cseng&eacute;s</trd>,
    <trd>bong&aacute;s</trd>,
    <trd>cs&ouml;rg&eacute;s</trd>,
    <trd>kolompol&aacute;s</trd>,
    <trd>koccan&aacute;s</trd>
  </trdgrp>  
  <trdgrp lng="it">
      <trd>rintocco</trd>,
      <trd>tintinnio</trd>,
      <trd>squillo<klr> (di telefono o campanello)</klr></trd>
    </trdgrp>
  <trd lng="pl">brzd&eogonek;k</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>hu&ccaron;en&iacute; v u&scaron;&iacute;ch</trd>,
    <trd>u&scaron;n&iacute; &scaron;elesty</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Geklirr</trd>,
    <trd>Klingeln</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C1;&#x30F3;&#x30C1;&#x30F3;&#x9CF4;&#x308B;&#x97F3;  <pr>&#x30C1;&#x30F3;&#x30C1;&#x30F3;&#x306A;&#x308B;&#x304A;&#x3068;</pr></trd>,
    <trd>&#x30AB;&#x30C1;&#x30E3;&#x30AB;&#x30C1;&#x30E3;&#x3068;&#x3044;&#x3046;&#x97F3;  <pr>&#x30AB;&#x30C1;&#x30E3;&#x30AB;&#x30C1;&#x30E3;&#x3068;&#x3044;&#x3046;&#x304A;&#x3068;</pr></trd>
  </trdgrp>
  <trd lng="sk">hu&ccaron;anie v u&scaron;iach</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&#x44F;&c_z;&#x43A;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_b;&c_r;&#x44F;&c_z;&#x43A;&c_ib;&c_t;</trd>,
    <trd>&c_b;&c_r;&#x44F;&c_zh;&c_ch;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_d;&c_z;&c_v;&#x44F;&#x43A;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_d;&c_z;&c_v;&c_je;&#x43D;&#x44C;&#x43A;&c_ib;&c_t;</trd>,
    <trd>&c_d;&c_z;&c_je;&#x43D;&#x44C;&#x43A;&c_ib;&c_t;</trd>,
    <trd>&c_d;&c_z;&c_je;&#x43D;&#x44C;&#x43A;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="tint.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tint.0igi.SON">
    <dif>
      Estigi <tld/>on ekzemple frapante metala&jcirc;on, vitron...:
      <ekz>
        &ccirc;iuj babilis, ridis, <tld/>igis la glasojn
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2014/011227.php">La piano</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el ili levi&gcirc;is, alvenis kun sia glaso, <tld/>igis
        &gcirc;in al la mia kaj tostis
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
        18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">rozcinkat</trd>
  <trd lng="de">ansto&szlig;en <klr>(z.B. mit Gl&auml;sern)</klr></trd>
  <trdgrp lng="en">
    <trd>tinkle</trd>,
    <trd>jingle</trd>,
    <trd>clink</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire <ind>sonner</ind></trd>,
    <trd>faire <ind>tinter</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csenget</trd>,
    <trd>kolompol</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fare <ind>suonare</ind></trd>,
    <trd>fare <ind>tintinnare</ind></trd>,
    <trd>fare <ind>vibrare</ind><klr> (risuonare)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x30C1;&#x30EA;&#x30F3;&#x30FB;&#x30AB;&#x30C1;&#x30F3;&#x3068;&#x9CF4;&#x3089;&#x3059;  <pr>&#x30C1;&#x30EA;&#x30F3;&#x30FB;&#x30AB;&#x30C1;&#x30F3;&#x3068;&#x306A;&#x3089;&#x3059;</pr></trd>
  <trdgrp lng="pl">
    <trd>brzd&aogonek;ka&cacute;</trd>,
    <trd>dzwoni&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zacenga&tcaron;</trd>,
    <trd>zacvend&zcaron;a&tcaron;</trd>,
    <trd>zarin&ccaron;a&tcaron; <klr>(nie&ccaron;&iacute;m)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_c;&#x43E;&#x43A;&c_a;&c_t;&c_i;&c_s;&#x44F; <klr>(&c_ch;&c_a;&c_r;&#x43A;&#x43E;&#x44E;)</klr></trd>,
    <trd>&c_ch;&c_a;&c_r;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="tint.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="tint.0ilo.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.frap0o" lst="voko:frapinstrumentoj">Frapinstrumento</ref>
      a&ubreve; ornama&jcirc;o, malplena metala sfereto kun ena bulo, kiu skuate estigas sonon:
      <ekz>
        faru <klr>[&#x2026;]</klr> &ccirc;irka&ubreve;e sur &gcirc;ia malsupra rando, kaj inter ili &ccirc;irka&ubreve;e orajn
        <tld/>ilojn
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>iloj de la &ccirc;evaloj
        <fnt>
          <bib>MT</bib>
          <lok>Ze&hcirc;arja 14:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        li rakontis pri la lumaj vintraj noktoj, kiam la <tld/>iloj de
        la glitveturiloj <tld/>adis
        <fnt><bib>Fab2</bib>, <lok>monteto de elfoj</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>ilo de la kla&ubreve;no <tld/>is kaj asertis, ke &gcirc;i estas la <ref cel="sonor.0ilo">sonorilo</ref> de
        pre&gcirc;ejo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_z;&c_g;&c_o;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_m;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_m;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_v;&c_a;&c_n;&c_o;&c_ch;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">zvonek j&iacute;zdn&iacute;ho kola a pod.</trd>
  <trdgrp lng="de">
    <trd>Schelle</trd>,
    <trd>Gl&ouml;ckchen</trd>
  </trdgrp>
  <trd lng="en">jingle bell</trd>
  <trdgrp lng="fr">
    <trd>grelot</trd>,
    <trd>sonnaille</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cseng&odblac;</trd>,
    <trd>kolomp</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>campanellino</trd>,
    <trd>campanello</trd>,
    <trd>sonaglio</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x308A;&#x3093;</trd>,
    <trd>&#x9234;  <pr>&#x3059;&#x305A;</pr></trd>,
    <trd>&#x30D9;&#x30EB;  <pr>&#x3079;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">belletje</trd>
  <trd lng="pl">dzwonek</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_b;&c_je;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_b;&c_u;&c_b;&c_je;&c_n;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ceng&aacute;&ccaron;</trd>,
    <trd>zvon&ccaron;ek</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&#x44F;&c_z;&#x43A;&c_a;&#x43B;&#x44C;&c_c;&c_je;</trd>,
    <trd>&c_b;&c_a;&#x43B;&c_a;&c_b;&#x43E;&#x43D;&c_ch;&c_i;&#x43A;</trd>,
    <trd>&c_b;&c_u;&c_b;&#x43E;&#x43D;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
