<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>ginece</rad>/o</kap>
<drv mrk="ginece.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="klr">(&ccirc;e antikvuloj)</uzo>
    <dif>
      Aparta lo&gcirc;ejo por la virinoj en la hejmo.
    </dif>
    <refgrp tip="vid">
      <ref cel="harem.0o.ejo">haremo</ref>,
      <ref cel="serajl.0o.haremo">serajlo</ref>.
    </refgrp>
    <trd lng="de">Frauengemach</trd>
    <trdgrp lng="hu">
      <trd>gynaeceum</trd>,
      <trd>n&odblac;i <ind>lakoszt&aacute;ly</ind></trd>
    </trdgrp>
    <trd lng="nl">vrouwenvertrek</trd>
    <trd lng="pt">gineceu</trd>
    <trd lng="ru">&c_g;&c_i;&c_n;&c_je;&c_k;&c_je;&c_j;</trd>
  </snc>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      La tuto de la inaj organoj de la
      <ref tip="malprt" cel="flor.0o.BOT">floro</ref>,
      formita el la <ref tip="prt" cel="frukt.0ofolio">fruktofolioj</ref>.
    </dif>
    <trdgrp lng="hu">
      <trd>term&odblac;t&aacute;j</trd>,
      <trd>gynaeceum</trd>
    </trdgrp>
    <trd lng="la">Gynoeceum</trd>
    <trd lng="pt">gineceu<klr tip="amb"> (bot&acirc;nica)</klr></trd>
    <trd lng="ru">&c_p;&c_je;&c_s;&c_t;&c_i;&c_k;</trd>
  </snc>
  <trd lng="fr">gyn&eacute;c&eacute;e<klr tip="ind"> (t.s.)</klr></trd>
</drv>
</art>
