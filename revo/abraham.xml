<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: abraham.xml,v 1.14 2024/10/07 08:36:32 revo Exp $">
<kap>
    <rad>Abraham</rad>/o <fnt><bib>MT</bib></fnt>
</kap>
<drv mrk="abraham.0o">
  <kap><tld/>o</kap>
  <snc mrk="abraham.0o.prapatro">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Prapatro de <ref tip="vid" cel="izrael.0o">Izraelo</ref>:
      <ekz>
        Dio provis <tld/>on, kaj diris al li: <klr>[...]</klr> prenu
        vian
        filon, vian solan, kiun vi amas, Isaakon, kaj iru en la landon
        Morija,
        kaj oferu lin tie kiel bruloferon sur unu el la montoj
        <fnt><bib>MT</bib>, <lok>Genezo 22:2</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x4E9A;&#x4F2F;&#x62C9;&#x7F55; <pr>y&#xE0;b&#xF3;l&amacron;h&#x1CE;n</pr></trd>,
    <trd>&#x4E9E;&#x4F2F;&#x62C9;&#x7F55; <pr>y&#xE0;b&#xF3;l&amacron;h&#x1CE;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="abraham.0o.nomo">
  <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> aramedevena.
    </dif>
    <trdgrp lng="zh">
    <trd>&#x4E9A;&#x4F2F;&#x62C9;&#x7F55; <pr>y&#xE0;b&#xF3;l&amacron;h&#x1CE;n</pr></trd>,
    <trd>&#x4E9E;&#x4F2F;&#x62C9;&#x7F55; <pr>y&#xE0;b&#xF3;l&amacron;h&#x1CE;n</pr></trd>,
    <trd>&#x4E9E;&#x5DF4;&#x90CE; <pr>y&#xE0;b&amacron;l&#xE1;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_b;&c_r;&c_a;&c_m;</trd>,
    <trd>&c_A;&c_w;&c_r;&c_a;&c_m;</trd>,
    <trd>&c_A;&c_b;&c_r;&c_a;&c_g;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">Abraham</trd>
  <trd lng="cs">Abraham</trd>
  <trd lng="da">Abraham</trd>
  <trd lng="de">Abraham</trd>
  <trd lng="en">Abraham</trd>
  <trd lng="fr">Abraham</trd>
  <trdgrp lng="id">
    <trd>Ibrahim</trd>,
    <trd>Abraham</trd>
  </trdgrp>
  <trd lng="ja">&#x30A2;&#x30D6;&#x30E9;&#x30CF;&#x30E0;  <pr>&#12354;&#12406;&#12425;&#x306F;&#12416;</pr></trd>
  <trd lng="nl">Abraham</trd>
  <trd lng="sk">Abrah&aacute;m</trd>
</drv>
</art>

