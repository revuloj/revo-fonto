<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxelak.xml,v 1.19 2024/03/25 07:28:39 revo Exp $">
<kap>
  <rad>&scirc;elak</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="sxelak.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="rezin.0o.BOT">Rezina</ref>
      <ref tip="super" cel="substa.0o.FIZ">substanco</ref>, produktita el ekskremento de
      lakaj <ref cel="kocxed.0oj">ko&ccirc;edoj</ref>
      (<nom>Kerria lacca</nom>) sur bran&ccirc;oj, uzata i.a. por protekti surfacojn
      de sondiskoj, muzikiloj, franda&jcirc;oj:
      <ekz>
        sigelvakso estas miksa&jcirc;o el kolofono kun terebinta oleo, <tld/>o, koloriga&jcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18248">Sigelvakso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estis uzita ekde 1895 kiel bazmaterialo por sondiskoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=39455">Gramofono</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        por malhelpi galvanan korodon inter la kuproha&ubreve;to kaj la fera subtenstrukturo,
        <nom>Eiffel</nom> izolis la ha&ubreve;ton per asbesto impregnita el <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18983">Statuo de Libereco</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_e;&c_l;&c_a;&c_k;</trd>
  <trd lng="bg">&c_sh;&c_je;&c_l;&c_l;&c_a;&c_k;</trd>
  <trd lng="ca">goma laca</trd>
  <trd lng="de">Schellack</trd>
  <trd lng="en">shellac</trd>
  <trdgrp lng="fr">
    <trd>gomme-laque</trd>,
    <trd>shellac</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30A7;&#x30E9;&#x30C3;&#x30AF;</trd>,
    <trd>&#x30BB;&#x30E9;&#x30C3;&#x30AF;</trd>
  </trdgrp>
  <trd lng="nl">schellak</trd>
  <trdgrp lng="hu">
    <trd>sellak</trd>,
    <trd>lakkgumi</trd>,
    <trd>lakkm&eacute;zga</trd></trdgrp>
  <trd lng="it">gommalacca</trd>
  <trd lng="pl">szelak</trd>
  <trd lng="pt">goma-laca</trd>
  <trd lng="ru">&c_sh;&c_je;&c_l;&c_l;&c_a;&c_k;</trd>
  <trd lng="uk">&c_sh;&c_je;&#x43B;&c_a;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#x866B;&#x80F6; <pr>ch&#xF3;ngji&#x101;o</pr></trd>,
    <trd>&#x87F2;&#x81A0; <pr>ch&#xF3;ngji&#x101;o</pr></trd>
  </trdgrp>
</drv>
</art>
