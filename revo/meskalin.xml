<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>meskalin</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="meskalin.0o">
  <kap><tld/>o</kap>
  <snc mrk="meskalin.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="alkalo.0o.KEM">Alkaloido</ref> kun 
      halucina efiko, &hcirc;emie C<sub>11</sub>H<sub>17</sub>NO<sub>3</sub>: 
      <ekz>
        pejotlo <klr>...</klr> entenas diversajn alkaloidojn, i.a. 
        <tld/>on, potence halucinan
        <fnt>
          <aut>J. Camacho</aut>,
          <vrk><url          
          ref="http://www.geocities.com/al_fab_et_o/capsterkverm.pdf">La
          &ccirc;apo de la sterko-vermo</url></vrk>,  
          <lok>eld. La Kancerkliniko, 1995</lok>
        </fnt>;
      </ekz>
      <ekz>
        vizioj influitaj de la <tld/>o
        <fnt>
          <aut>R. Goutarel k. a., trad. B. Coffinet</aut>,
          <vrk>La ibogaino en psikoterapio...</vrk>,
          <lok>[vidita en 2006-11]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">mescaline</trd>
  <trd lng="it">mescalina</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>