<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dinamo.xml,v 1.22 2023/10/14 11:26:53 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>dinamometr</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="dinamo.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.0o">Instrumento</ref> por mezuri la forton, kiu efikas al korpo:
      <ekz>
        per <tld/>o oni senpere determinas la valoron de la potenco de ma&scirc;ino,
        frot<tld/>o estas aparato, en kiu la mezuro de la frotado donas la valoron de
        la potenco
        <fnt>
          <aut>M. Rollet de l'Isle k.a.</aut>
          <vrk><url ref="http://www.esperanto.mv.ru/MaristaTerminaro/MaristaTerminaro.html">Provo de Marista Terminaro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="pes.0ilo">pesilo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_n;&c_a;&c_m;&c_o;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">dinam&ograve;metre</trd>
  <trdgrp lng="cs">
    <trd>dynamometr</trd>,
    <trd>dynamometrick&aacute; brzda</trd>
  </trdgrp>
  <trd lng="de">Dynamometer</trd>
  <trd lng="es">dinam&oacute;metro</trd>
  <trd lng="fa">&a_n;&f_y;&a_r;&a_w;&a_s;&a_n;&a_j;</trd>
  <trd lng="fr">dynamom&egrave;tre</trd>
  <trd lng="he">&mem;&#x5B7;&dalet; &kaf;&#x5BC;&#x5B9;&het;&#x5B7;</trd>
  <trdgrp lng="hu">
    <trd>er&odblac;m&eacute;r&odblac;</trd>,
    <trd>dinamom&eacute;ter</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x52D5;&#x529B;&#x8A08;  <pr>&#x3069;&#x3046;&#x308A;&#x3087;&#x304F;&#x3051;&#x3044;</pr></trd>,
    <trd>&#x529B;&#x91CF;&#x8A08;  <pr>&#x308A;&#x304D;&#x308A;&#x3087;&#x3046;&#x3051;&#x3044;</pr></trd>,
    <trd>&#x7B4B;&#x529B;&#x8A08;  <pr>&#x304D;&#x3093;&#x308A;&#x3087;&#x304F;&#x3051;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="pl">dynamometr</trd>
  <trd lng="pt">dinam&ocirc;metro</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_l;&c_o;&c_m;&c_je;&c_r;</trd>,
    <trd>&c_d;&c_i;&c_n;&c_a;&c_m;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">dynamometer</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x438;&#x43B;&#x43E;&#x43C;&#x456;&#x440;</trd>,
    <trd>&#x434;&#x438;&#x43D;&#x430;&#x43C;&#x43E;&#x43C;&#x435;&#x442;&#x440;</trd>
  </trdgrp>
</drv>


<drv mrk="dinamo.elektro0o">
  <kap>elektro<tld/>o <fnt><bib>PV</bib></fnt>
  </kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Aparato, ekipita per du elektraj bobenoj, kaj 
      servanta por mezuri kurenton, tension a&ubreve; alian elektran forton.  
    </dif>
    <refgrp tip="vid">
       <ref cel="amperm.0o">ampermetro</ref>,
       <ref cel="elektr13.0o">elektrometro</ref>,
       <ref cel="om.o0metro">omometro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_d;&c_y;&c_n;&c_a;&c_m;&c_o;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">electrodinam&ograve;metre</trd>
  <trd lng="en">Elektrodynamometer</trd>
  <trd lng="fr">&eacute;lectrodynamom&egrave;tre</trd>
  <trd lng="hu">elektrodinamom&eacute;ter</trd>
    <trd lng="ja">&#x96FB;&#x6D41;&#x529B;&#x8A08;  <pr>&#x3067;&#x3093;&#x308A;&#x3085;&#x3046;&#x3061;&#x304B;&#x3089;&#x3051;&#x3044;</pr></trd>
  <trd lng="pt">eletrodinam&ocirc;metro</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_d;&c_i;&c_n;&c_a;&c_m;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  <trd lng="sk">elektrodynamometer</trd>
  <trdgrp lng="cs">
    <trd>elektrodynamometr</trd>,
    <trd>elektrodynamometrick&aacute; brzda</trd>
  </trdgrp>
</drv>


</art>
