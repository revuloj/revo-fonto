<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: aken1.xml,v 1.4 2025/09/01 08:41:07 afido Exp $">
    <kap>
      <rad>aken</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="aken1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Unusema <ref tip="super" cel="frukt.ferm0o">fermfrukto</ref>,
      speco de <ref tip="super" cel="nuks.0o.BOT">nukso<sncref/></ref>,
      &ccirc;e kiu maldika, ledreca perikarpo,
      kunigita en sola loko, strikte &ccirc;irka&ubreve;as unuopan semgrajnon:
      <ekz>
        la fruktoj de sunfloro, leontodo, fragujo estas <tld/>oj;
      </ekz>
      <ekz>
        frago <klr>[&#8230;]</klr> estas frukto nur nutrovidpunkte, ne botanike: veraj fruktoj
        estas la <tld/>oj, la malgrandaj flavaj semoj kiuj estas sur la surfaco de la
        frago
        <fnt>
          <aut>Filomena Canzano</aut>
          <vrk><url ref="https://apetito.ikso.net/artikolo/fragoj"
              >Bonan Apetiton! Fragoj</url></vrk>
          <lok>Esperantista Vegetarano, n-ro 2, 2017</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Ach&auml;ne</trd>
    <trd lng="en">achene</trd>
    <trd lng="fr">ak&egrave;ne</trd>
    <trd lng="la">achenium</trd>
    <trd lng="ru">&c_s;&c_je;&c_m;&c_ja;&c_n;&c_k;&c_a;</trd>
  </snc>
  <ref tip="hom" cel="aken.0o">Akeno (urbo)</ref>
<trdgrp lng="be">
  <trd>&c_s;&c_je;&c_m;&c_ja;&c_ch;&c_k;&c_a;</trd>,
  <trd>&c_s;&c_ja;&c_m;&c_ja;&c_n;&c_k;&c_a;</trd>
</trdgrp>
</drv>

  </art>

