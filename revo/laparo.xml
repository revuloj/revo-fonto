<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: laparo.xml,v 1.20 2025/03/30 18:33:29 revo Exp $">
<kap>
  <rad>laparotomi</rad>/o
</kap>
<drv mrk="laparo.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">KIR</uzo>
    <dif>
      &Hcirc;irurgia malfermo de la ventra pario:
      <ekz>
        la unua <tld/>o kun gastrotomio sukcesis al <nom>Daniel
        Schwabe</nom> 1635 en
        Kenigsbergo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_p;&c_a;&c_r;&c_a;&c_t;&c_a;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="ca">laparotomia</trd>
  <trdgrp lng="de">
     <trd>Bauchschnitt</trd>,
     <trd>Laparotomie</trd>
  </trdgrp>
  <trd lng="el">&lambda;&alfa;&pi;&alfa;&rho;&omikron;&tau;&omikron;&my;&jota_ton;&alfa;</trd>
  <trdgrp lng="en">
    <trd>laparotomy</trd>,
    <trd>celiotomy</trd>
  </trdgrp>
  <trd lng="fr">laporatomie</trd>
  <trdgrp lng="hu">
    <trd>laparotomia</trd>,
    <trd>hasfalmetsz&eacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#38283;&#33145;&#34899;  <pr>&#12363;&#12356;&#12405;&#12367;&#12376;&#12421;&#12388;</pr></trd>
  <trd lng="nl">laparatomie</trd>
  <trd lng="pl">laparotomia</trd>
  <trd lng="pt">laparotomia</trd>
  <trd lng="ro">laparotomie</trd>
  <trd lng="ru">&c_l;&c_a;&c_p;&c_a;&c_r;&c_o;&c_t;&c_o;&c_m;&c_i;&c_ja;</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_p;&c_a;&c_r;&c_o;&c_t;&c_o;&c_m;&c_ib;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_t;&c_i;&c_n; &c_ch;&c_je;&c_r;&c_je;&c_v;&c_n;&c_o;&c_ji; &c_p;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_i;&c_n;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="laparo.prova0o">
  <kap>
    prova <tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="L"/>o farita poresplore.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_o;&c_b;&c_n;&c_a;&c_ja; <ind>&c_l;&c_ja;&c_p;&c_a;&c_r;&c_a;&c_t;&c_a;&c_m;&c_ib;&c_ja;</ind></trd>
  <trd lng="ca"><ind>laparotomia</ind> exploradora</trd>
  <trdgrp lng="en">
    <trd>exploratory <ind>laparotomy</ind></trd>,
    <trd>exploratory <ind>celiotomy</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>pr&oacute;ba-laparotomia</trd>,
    <trd>explorat&iacute;v <ind>laparotomia</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#35386;&#26619;&#38283;&#33145;&#34899;  <pr>&#12375;&#12435;&#12373;&#12363;&#12356;&#12405;&#12367;&#12376;&#12421;&#12388;</pr></trd>,
    <trd>&#35430;&#39443;&#38283;&#33145;  <pr>&#12375;&#12369;&#12435;&#12363;&#12356;&#12405;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>laparotomia pr&oacute;bna</trd>,
    <trd>laparotomia eksploracyjna</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>laparotomie de proces</trd>,
    <trd>laparotomie exploratorie</trd>
  </trdgrp>
</drv>
</art>
