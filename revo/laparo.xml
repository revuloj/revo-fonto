<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>laparotomi</rad>/o
</kap>
<drv mrk="laparo.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      &Hcirc;irurgia malfermo de la ventra pario.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Bauchschnitt</trd>,
     <trd>Laparotomie</trd>
  </trdgrp>
  <trd lng="el">&lambda;&alfa;&pi;&alfa;&rho;&omikron;&tau;&omikron;&my;&jota_ton;&alfa;</trd>
  <trdgrp lng="hu">
    <trd>laparotomia</trd>,
    <trd>hasfalmetsz&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">laparatomie</trd>
  <trd lng="pt">laparotomia</trd>
  <trd lng="ru">&c_l;&c_a;&c_p;&c_a;&c_r;&c_o;&c_t;&c_o;&c_m;&c_i;&c_ja;</trd>
</drv>

<drv mrk="laparo.prova0o">
  <kap>
    prova <tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="L"/>o farita poresplore.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>pr&oacute;ba-laparotomia</trd>,
    <trd>explorat&iacute;v <ind>laparotomia</ind></trd>
  </trdgrp>  
</drv>
</art>
