<drv mrk="komple1.rekta0o">
  <kap>rekta <tld/>o <fnt><bib>JED</bib></fnt>,
  <var><kap>senpera <tld/>o</kap></var></kap>
  <snc mrk="komple1.rekta0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <tld lit="K"/>o kunigata sen prepozicio al sia determinato:
      <ekz>
        vi simple esprimas vian opinion, ke en Esperanto oni uzas la infinitivon nur a&ubreve;
        en formo de rekta <tld/>o (ekzemple &leftquot;mi amas danci&rightquot;), a&ubreve; en formo de nerekta
        <tld/>o kun la prepozicioj &leftquot;por&rightquot;, &leftquot;anta&ubreve; (ol)&rightquot;, &leftquot;anstata&ubreve;&rightquot;
        <fnt>
          <bib>LR</bib>
          <lok>11. Verboj</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="objekt.akuzativa_0o">akuzativa objekto</ref>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_p;&c_a;&c_w;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je; &c_p;&c_r;&c_a;&c_m;&c_o;&c_je;</trd>
  <trd lng="fr">compl&eacute;ment direct</trd>
   <trd lng="ja">&#30452;&#25509;&#35036;&#35486;  <pr>&#12385;&#12423;&#12367;&#12379;&#12388;&#12411;&#12372;</pr></trd>
  <trdgrp lng="nl">
    <trd>direct <ind>object</ind></trd>,
    <trd>direct <ind>voorwerp</ind></trd>
  </trdgrp>
  <trd lng="pl">dope&lstroke;nienie bli&zdot;sze</trd>
  <trd lng="ro">complement direct</trd>
  <trd lng="ru">&c_p;&c_r;&c_ja;&c_m;&c_o;&c_je; &c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="komple1.nerekta0o">
  <kap>nerekta <tld/>o,
  <var><kap>pera <tld/>o</kap></var></kap>
  <snc mrk="komple1.nerekta0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <tld lit="K"/>o kunigata per <ref cel="prepoz.0o">prepozicio</ref> al sia determinato:
      <ekz>
        en Esperanto oni uzas la infinitivon nur a&ubreve; en formo de rekta <tld/>o (ekzemple
        &leftquot;mi amas danci&rightquot;), a&ubreve; en formo de nerekta <tld/>o kun la prepozicioj &leftquot;por&rightquot;,
        &leftquot;anta&ubreve; (ol)&rightquot;, &leftquot;anstata&ubreve;&rightquot;
        <fnt>
          <bib>LR</bib>
          <lok>Dua Parto &ndash; Gramatiko</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_p;&c_a;&c_w;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je; &c_w;&c_s;&c_k;&c_o;&c_s;&c_n;&c_a;&c_je;</trd>
  <trd lng="fr">compl&eacute;ment indirect</trd>
  <trd lng="ja">&#38291;&#25509;&#35036;&#35486;  <pr>&#12363;&#12435;&#12379;&#12388;&#12411;&#12372;</pr></trd>
  <trdgrp lng="nl">
    <trd>indirect <ind>object</ind></trd>,
    <trd>indirect <ind>voorwerp</ind></trd>
  </trdgrp>
  <trd lng="pl">dope&lstroke;nienie dalsze</trd>
  <trd lng="ro">complement indirect </trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je; &c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="uk">&c_n;&c_je;&c_p;&c_r;&c_ja;&c_m;&c_i;&c_j; &c_d;&c_o;&c_d;&c_a;&c_t;&c_o;&c_k;</trd>
</drv>

<drv mrk="komple1.objekta0o">
  <kap>objekta <tld/>o</kap>
  <snc mrk="komple1.objekta0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <tld lit="K"/>o montranta la celon de la ago esprimata de la
      verbo:
      <ekz>
        en Esperanto la objektaj <tld/>oj povas esti egale rektaj
        (obei iun) a&ubreve; nerektaj (obei al iu).
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_p;&c_a;&c_w;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Objekt <klr>(Gramm.)</klr></trd>
  <trdgrp lng="fr">
    <trd>compl&eacute;ment d'objet</trd>,
    <trd>objet<klr tip="ind"> (compl&eacute;ment d'objet)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#30446;&#30340;&#35036;&#35486;  <pr>&#12418;&#12367;&#12390;&#12365;&#12411;&#12372;</pr></trd>
  <trdgrp lng="nl">
    <trd>voorwerp<klr tip="ind"> (taalk.)</klr></trd>,
    <trd>object<klr tip="ind"> (taalk.)</klr></trd>
  </trdgrp>
  <trd lng="pl">dope&lstroke;nienie  podmiotowe</trd>
  <trd lng="ro">complement subiectiv</trd>
  <trd lng="uk">&c_d;&c_o;&c_d;&c_a;&c_t;&c_o;&c_k;</trd>
  <trd lng="zh">&#30452;&#25509;&#23486;&#35821; <pr>zh&iacute;ji&emacron;b&imacron;ny&#468;</pr></trd>
</drv>

<drv mrk="komple1.cirkonstanca0o">
  <kap>cirkonstanca <tld/>o</kap>
  <snc mrk="komple1.cirkonstanca0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      <tld lit="K"/>o montranta unu el la cirkonstancoj de la ago a&ubreve;
      stato esprimata de la verbo:
      <ekz>
        en Esperanto ekzistas cirkonstancaj <tld/>oj rektaj
