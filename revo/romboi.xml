<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>romboid</rad>/o
</kap>
<drv mrk="romboi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <ref tip="dif" cel="deltoi.0o.figuro">deltoido.</ref>
    <rim>
      La indikita fonto ver&scirc;ajne donas al tiu termino alian
      sencon. Vd rimarkon sub
      <ref cel="deltoi.0o.figuro">deltoido</ref>.
    </rim>
    <trdgrp lng="de">
      <trd>Deltoid</trd>,
      <trd>Drachenfigur</trd>,
      <trd>Rhomboid</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>deltoid</trd>,
      <trd>kite</trd>,
      <trd>rhomboid</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>delto&iuml;de<klr tip="ind"> (math., rhombo&iuml;de)</klr></trd>,
      <trd>rhombo&iuml;de</trd>
    </trdgrp>
    <trd lng="pl">deltoid</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_l;&c_mol;&c_t;&c_o;&c_i;&c_d;</trd>,
      <trd>&c_r;&c_o;&c_m;&c_b;&c_o;&c_i;&c_d;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
