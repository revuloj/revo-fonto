    <trd>&c_z;&c_v;&c_i;&c_ch;&c_a;&c_j;&c_n;&c_i;&c_j;/&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_j; &c_ch;&c_o;&c_l;&c_o;&c_v;&c_ib;&c_k;</trd>,
    <trd>&c_r;&c_ja;&c_d;&c_o;&c_v;&c_a;/&c_p;&c_je;&c_r;&c_je;&c_s;&c_ib;&c_ch;&c_n;&c_a; &c_o;&c_s;&c_o;&c_b;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
