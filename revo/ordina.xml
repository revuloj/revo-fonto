<kap>
  <ofc>*</ofc>
  <rad>ordinar</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ordina.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Ne diferenca je la kutimo:
      <ekz>
        <tld/>a vesto, man&gcirc;o, kuracisto;
      </ekz>
      <ekz>
        konsiderinte la <tld/>econ de la apero<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Ne diferenca je la normala grado:
      <ekz>
        <tld/>a inteligenteco, knabo;
      </ekz>
      <ekz>
        tiu vino estas tre <tld/>a;
      </ekz>
      <ekz>
        esprimi ion per <tld/>a maniero.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;</trd>
  <trd lng="bo">&#xF51;&#xF40;&#xFB1;&#xF74;&#xF66;&#xF0B;&#xF58;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>b&ecaron;&zcaron;n&yacute;</trd>,
    <trd>obvykl&yacute;</trd>,
    <trd>oby&ccaron;ejn&yacute;</trd>,
    <trd>prost&yacute;</trd>,
    <trd>&rcaron;adov&yacute;</trd>
  </trdgrp>
  <trd lng="de">gew&ouml;hnlich</trd>
  <trd lng="en">ordinary</trd>
  <trdgrp lng="es">
    <trd>com&uacute;n<klr> (ordinario)</klr></trd>,
    <trd>ordinario</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commun<klr> (ordinaire)</klr></trd>,
    <trd>ordinaire</trd>
  </trdgrp>
  <trd lng="ne">&#x938;&#x93E;&#x92E;&#x93E;&#x928;&#x94D;&#x92F;</trd>
  <trd lng="nl">gewoon</trd>
  <trdgrp lng="pl">
    <trd>potoczny</trd>,
    <trd>powszedni</trd>,
    <trd>zwyk&lstroke;y</trd>,
    <trd>zwyczajny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>comum</trd>,
    <trd>ordin&aacute;rio</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>familiar&abreve;</trd>,
    <trd>obi&#x219;nuit</trd>,
    <trd>uzual</trd>,
    <trd>normal</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_y;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_y;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>oby&ccaron;ajn&yacute;</trd>,
    <trd>ordin&aacute;rny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x432;&#x438;&#x447;&#x430;&#x439;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x431;&#x443;&#x434;&#x435;&#x43D;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x440;&#x44F;&#x434;&#x43E;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x441;&#x442;&#x438;&#x439;</trd>,
    <trd>&#x43E;&#x440;&#x434;&#x438;&#x43D;&#x430;&#x440;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x441;&#x456;&#x447;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x431;&#x430;&#x43D;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="ordina.0e">
  <kap><tld/>e</kap>
  <snc mrk="ordina.0e.KOMUNE">
    <dif>
      La&ubreve; <tld/>a maniero; la&ubreve;kutime; plejofte, regule:
      <ekz>
        &ccirc;iu amas <tld/>e personon, kiu estas simila al li<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni uzas <tld/>e vi anstata&ubreve; ci<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>oby&ccaron;ejn&ecaron;</trd>,
    <trd>oby&ccaron;ejn&ecaron;</trd>
  </trdgrp>
  <trd lng="de">gew&ouml;hnlich</trd>
  <trdgrp lng="fr">
    <trd>g&eacute;n&eacute;ralement</trd>,
    <trd>ordinairement</trd>
  </trdgrp>
  <trd lng="nl">gewoonlijk</trd>
  <trdgrp lng="pl">
    <trd>zazwyczaj</trd>,
    <trd>zwyczajnie</trd>
  </trdgrp>
  <trd lng="pt">ordinariamente</trd>
  <trdgrp lng="ro">
    <trd>de obicei</trd>,
    <trd>&icirc;n general</trd>,
    <trd>pur &scedil;i simplu</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_y;&c_ch;&c_n;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>be&zcaron;ne</trd>,
    <trd>oby&ccaron;ajne</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x432;&#x438;&#x447;&#x430;&#x439;&#x43D;&#x43E;</trd>,
    <trd>&#x431;&#x443;&#x434;&#x435;&#x43D;&#x43D;&#x43E;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x441;&#x442;&#x43E;</trd>,
    <trd>&#x43E;&#x440;&#x434;&#x438;&#x43D;&#x430;&#x440;&#x43D;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="ordina.0o">
  <kap><tld/>o</kap>
  <snc mrk="ordina.0o.KOMUNE">
    <dif>
      <tld lit="O"/>a iro de aferoj,
      atendebla stato de a&jcirc;oj:
      <ekz>
        kelkaj momentoj diamantigitaj |
        ekster karbonan <tld/>on |
        de la tempa fulgi&gcirc;o
        <fnt>
          <aut>E. de Kock</aut>
          <vrk>Oma&gcirc;o al Zamenhof</vrk>
          <lok>1987</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ordinaire<klr> (subst.)</klr></trd>
  <trdgrp lng="pl">
    <trd>zwyk&lstroke;o&sacute;&cacute;</trd>,
    <trd>potoczno&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>obi&scedil;nuin&tcedil;&abreve;</trd>,
    <trd>comunitatea</trd>
  </trdgrp>
</drv>

<drv mrk="ordina.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="ordina.0ulo.KOMUNE">
    <dif>
      <tld lit="O"/>a homo
      nek eminenta nek maleminenta,
      sen rimarkindaj aparta&jcirc;oj:
      <ekz>
        estas bone konate, ke scienculoj ne parolas
        la saman lingvon kiel <tld/>uloj
        <fnt><bib>Lanti</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">oby&ccaron;ejn&yacute; &ccaron;lov&ecaron;k</trd>
  <trd lng="fr">individu ordinaire</trd>
  <trd lng="pl">przeci&eogonek;tniak</trd>
  <trd lng="ro">mediocru</trd>
  <trd lng="sk">oby&ccaron;ajn&yacute; &ccaron;lovek</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x432;&#x438;&#x447;&#x430;&#x439;&#x43D;&#x430;/ &#x43F;&#x440;&#x43E;&#x441;&#x442;&#x430; &#x43B;&#x44E;&#x434;&#x438;&#x43D;&#x430;</trd>,
    <trd>&#x437;&#x432;&#x438;&#x447;&#x430;&#x439;&#x43D;&#x438;&#x439;/&#x43F;&#x440;&#x43E;&#x441;&#x442;&#x438;&#x439; &#x447;&#x43E;&#x43B;&#x43E;&#x432;&#x456;&#x43A;</trd>,
    <trd>&#x440;&#x44F;&#x434;&#x43E;&#x432;&#x430;/&#x43F;&#x435;&#x440;&#x435;&#x441;&#x456;&#x447;&#x43D;&#x430; &#x43E;&#x441;&#x43E;&#x431;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="ordina.ekster0a">
  <kap>ekster<tld/>a</kap>
  <snc mrk="ordina.ekster0a.KOMUNE">
    <dif>
      Diferenca je la kutima ordo:
      <ekz>
        ekster<tld/>a vorto, vesto;
      </ekz>
      <ekz>
        ekster<tld/>aj elspezoj;
      </ekz>
      <ekz>
        okazis io ekster<tld/>a.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ordina.ekster0a.lau_grado">
    <dif>
      Diferenca je la normala grado:
      <ekz>
        ekster<tld/>a talento, spirito, sukceso, gloro.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_a;&c_r;&c_d;&c_y;&c_n;&c_a;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>divn&yacute;</trd>,
    <trd>mimo&rcaron;&aacute;dn&yacute;</trd>,
    <trd>nev&iacute;dan&yacute;</trd>,
    <trd>nev&scaron;edn&iacute;</trd>
  </trdgrp>
  <trd lng="de">au&szlig;ergew&ouml;hnlich</trd>
  <trd lng="es">extraordinario</trd>
  <trdgrp lng="fr">
    <trd>exceptionnel<klr> (extraordinaire)</klr></trd>,
    <trd>extraordinaire</trd>
  </trdgrp>
  <trd lng="nl">buitengewoon</trd>
  <trdgrp lng="pl">
    <trd>nadzwyczajny</trd>,
    <trd>niebywa&lstroke;y</trd>
  </trdgrp>
  <trd lng="pt">extraordin&aacute;rio</trd>
  <trd lng="ro">ie&scedil;it din comun</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_k;&c_s;&c_t;&c_r;&c_a;&c_o;&c_r;&c_d;&c_i;&c_n;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_b;&c_y;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_b;&c_y;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>neoby&ccaron;ajn&yacute;</trd>,
    <trd>nie be&zcaron;n&yacute;</trd>,
    <trd>zvl&aacute;&scaron;tny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x430;&#x434;&#x437;&#x432;&#x438;&#x447;&#x430;&#x439;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x435;&#x43A;&#x441;&#x442;&#x440;&#x430;&#x43E;&#x440;&#x434;&#x438;&#x43D;&#x430;&#x440;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="ordina.ne0a">
  <kap>ne<tld/>a</kap>
  <snc mrk="ordina.ne0a.KOMUNE">
    <dif>
      Ne <tld/>a.
    </dif>
    <trdgrp lng="fr">
      <trd>particulier</trd>,
      <trd>singulier<klr> (particulier)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_o;&c_b;&c_y;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_o;&c_b;&c_y;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_s;&c_o;&c_b;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="ordina.ne0a.punkto_de_funkcio">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 21</lok></fnt>
    <dif>
      <klr>(p.p. punkto <frm><k>a</k></frm> rilate al kompleksa
      <ref cel="funkci.0o.MAT">funkcio</ref> <frm><k>f</k></frm>)</klr>
      Tia, ke <frm><k>f</k></frm> estas
      <ref cel="holomo.0a.cxie">holomorfa</ref> en
      malfermita <ref cel="disk.0o.en_kompleksa_ebeno">disko</ref> kun
      centro <frm><k>a</k></frm>, krom &ccirc;e <frm><k>a</k></frm> mem:
      <ekz>
        la funkcio <frm>(<k>z</k>-<k>a</k>)<sup>-1</sup></frm> akceptas
        ne<tld/>an punkton en <frm><k>a</k></frm>.
      </ekz>
    </dif>
    <ref tip="vid" cel="polus.0o.de_funkcio">poluso.</ref>
    <rim>
      Kelkaj matematikistoj uzas <ctl>singulara</ctl>
      <fnt><bib>MatStokTerm</bib><lok>p. 50</lok></fnt>
      &ccirc;i-sence. Tio &scirc;ajnas malkonsilinda pro la &gcirc;is nun
      pure gramatika senco de <ctl>singularo</ctl>. Bricard parolas
      pri <ctl>ne<tld/>a punkto</ctl> de funkcio, sed ja pri
      <ctl>singulara punkto</ctl> de kurbo. Tio ne aspektas tre
      konsekvenca uzado.

      Aliaj proponas <ctl>singulara&jcirc;o</ctl>
      <fnt><bib>EKV</bib><lok>&para;399</lok></fnt>
      a&ubreve; <ctl>aparta&jcirc;o</ctl>
      <fnt><bib>Deneva</bib></fnt> kun signifo
      <ctl>ne<tld/>a punkto</ctl>. Supozante, ke tiaj mallongigaj formoj
      estus
      utilaj, sufi&ccirc;us diri <ctl>ne<tld/>a&jcirc;o</ctl>.
    </rim>
    <trd lng="de">singul&auml;r<klr tip="ind"> (-e Stelle)</klr></trd>
    <trd lng="en">singular<klr tip="ind"> (point)</klr></trd>
    <trd lng="fr"><klr tip="ind">(point) </klr>singulier</trd>
    <trd lng="ru">&c_o;&c_s;&c_o;&c_b;&c_a;&c_ja;<klr tip="amb"> (&c_t;&c_o;&c_ch;&c_k;&c_a;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>neb&ecaron;&zcaron;n&yacute;</trd>,
    <trd>neoby&ccaron;ejn&yacute;</trd>,
    <trd>zvl&aacute;&scaron;tn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niepospolity</trd>,
    <trd>niezwyk&lstroke;y</trd>
  </trdgrp>
  <trd lng="ro">neobi&#x219;nuit</trd>
  <trdgrp lng="sk">
    <trd>neoby&ccaron;ajn&yacute;</trd>,
    <trd>nie be&zcaron;n&yacute;</trd>,
    <trd>zvl&aacute;&scaron;tny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x435;&#x437;&#x432;&#x438;&#x447;&#x430;&#x439;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x43E;&#x440;&#x434;&#x438;&#x43D;&#x430;&#x440;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="ordina.virina0ajxo">
  <kap>virina <tld/>a&jcirc;o<fnt>Z</fnt></kap>
  <uzo tip="klr">(evitvorte)</uzo>
  <snc mrk="ordina.virina0ajxo.FIZL">
    <ref tip="dif" cel="menstr.0o.FIZL">menstruo</ref>
  </snc>
  <trdgrp lng="pl">
    <trd>miesi&aogonek;czka</trd>,
    <trd>menstruacja</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ciclu</trd>,
    <trd>menstrua&tcedil;ie</trd>
  </trdgrp>
</drv>
</art>
