<drv mrk="ordina.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="ordina.0ulo.KOMUNE">
    <dif>
      <tld lit="O"/>a homo nek eminenta nek maleminenta,
      sen rimarkindaj aparta&jcirc;oj:
      <ekz>
        &ccirc;ar la patrino de sinjoro <nom>Broadbent</nom> apartenis al ia antikva, nobela familio, ili
        pensas, ke ili rajtas de supre, malestime rigardi al ni, <tld/>uloj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        fari tion kun plena silento estus superi la kapablon de <tld/>ulo
        <fnt>
          <bib>ChV</bib>
          <lok>3</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u <tld/>ulo &scirc;tele rigardas malanta&ubreve; sin
        <fnt>
          <bib>ChR</bib>
          <lok>Spiona rondo</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_y;
  <ind>&c_ch;&c_a;&c_l;&c_a;&c_v;&c_je;&c_k;</ind></trd>
