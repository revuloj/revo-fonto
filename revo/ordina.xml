<drv mrk="ordina.0o">
  <kap><tld/>o</kap>
  <snc mrk="ordina.0o.KOMUNE">
    <dif>
      <tld lit="O"/>a iro de aferoj,
      atendebla stato ilia:
      <ekz>
        amuzoj ne mankis kaj, kontra&ubreve; la <tld/>o, la nombro de la dancantoj estis granda
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tuj sekvas malfeli&ccirc;oj ekster <tld/>o
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VIII.</lok>
        </fnt>!
      </ekz>
      <ekz>
        kelkaj momentoj diamantigitaj &parsl;
        ekster karbonan <tld/>on &parsl;
        de la tempa fulgi&gcirc;o
        <fnt>
          <aut>E. de Kock</aut>
          <vrk>Oma&gcirc;o al Zamenhof</vrk>
          <lok>1987</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">das <ind>Gewohnte</ind></trd>
  <trd lng="fr">ordinaire<klr> (subst.)</klr></trd>
  <trdgrp lng="pl">
    <trd>zwyk&lstroke;o&sacute;&cacute;</trd>,
    <trd>potoczno&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>obi&scomma;nuin&tcomma;&abreve;</trd>,
    <trd>comunitatea</trd>
  </trdgrp>
  <trd lng="zh">&#x5E73;&#x5E38; <pr>p&#xED;ngch&#xE1;ng</pr></trd>
</drv>

<drv mrk="ordina.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="ordina.0ulo.KOMUNE">
    <dif>
      <tld lit="O"/>a homo nek eminenta nek maleminenta,
      sen rimarkindaj aparta&jcirc;oj:
      <ekz>
        &ccirc;ar la patrino de sinjoro <nom>Broadbent</nom> apartenis al ia antikva, nobela familio, ili
        pensas, ke ili rajtas de supre, malestime rigardi al ni, <tld/>uloj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        fari tion kun plena silento estus superi la kapablon de <tld/>ulo
        <fnt>
          <bib>ChV</bib>
          <lok>3</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u <tld/>ulo &scirc;tele rigardas malanta&ubreve; sin
        <fnt>
          <bib>ChR</bib>
          <lok>Spiona rondo</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">oby&ccaron;ejn&yacute; &ccaron;lov&ecaron;k</trd>
  <trdgrp lng="de">
    <trd><ind>gew&ouml;hnlich</ind>er Mensch</trd>,
    <trd>Normalb&uuml;rger</trd>,
    <trd><ind>einfach</ind>er B&uuml;rger</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ordinary person</trd>,
    <trd>everyday person</trd>,
    <trd>man on the street</trd>,
    <trd>Joe Blow</trd>,
    <trd>the average person</trd>
  </trdgrp>
  <trd lng="fr">individu ordinaire</trd>
  <trdgrp lng="ja">
    <trd>&#x51E1;&#x4EBA;  <pr>&#x307C;&#x3093;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x666E;&#x901A;&#x306E;&#x4EBA;  <pr>&#x3075;&#x3064;&#x3046;&#x306E;&#x3058;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="pl">przeci&eogonek;tniak</trd>
  <trd lng="ro">mediocru</trd>
  <trd lng="sk">oby&ccaron;ajn&yacute; &ccaron;lovek</trd>
