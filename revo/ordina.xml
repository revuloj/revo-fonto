<drv mrk="ordina.0o">
  <kap><tld/>o</kap>
  <snc mrk="ordina.0o.KOMUNE">
    <dif>
      <tld lit="O"/>a iro de aferoj,
      atendebla stato ilia:
      <ekz>
        amuzoj ne mankis kaj, kontra&ubreve; la <tld/>o, la nombro de la dancantoj estis granda
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tuj sekvas malfeli&ccirc;oj ekster <tld/>o
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VIII.</lok>
        </fnt>!
      </ekz>
      <ekz>
        kelkaj momentoj diamantigitaj &#11005;
        ekster karbonan <tld/>on &#11005;
        de la tempa fulgi&gcirc;o
        <fnt>
          <aut>E. de Kock</aut>
          <vrk>Oma&gcirc;o al Zamenhof</vrk>
          <lok>1987</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_v;&c_y;&c_k;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">normalitat</trd>
  <trd lng="de">das <ind>Gewohnte</ind></trd>
  <trd lng="en">ordinary</trd>
  <trd lng="fr">ordinaire<klr> (subst.)</klr></trd>
  <trdgrp lng="id">
    <trd><baz>biasa</baz>kebiasaan</trd>,
    <trd><baz>galib</baz>kegaliban</trd>,
    <trd><baz>lazim</baz>kelaziman</trd>,
    <trd><baz>umum</baz>keumuman</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zwyk&lstroke;o&sacute;&cacute;</trd>,
    <trd>potoczno&sacute;&cacute;</trd>
  </trdgrp>
