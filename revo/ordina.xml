<drv mrk="ordina.0e">
  <kap><tld/>e</kap>
  <snc mrk="ordina.0e.KOMUNE">
    <dif>
      La&ubreve; <tld/>a maniero; la&ubreve;kutime; plejofte, regule:
      <ekz>
        &ccirc;iu amas <tld/>e personon, kiu estas simila al li
        <fnt>
          <bib>F</bib>
          <lok>&FE; 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; &#x201A;ci&#x2018; oni uzas <tld/>e &#x201A;vi&#x2018;
        <fnt>
          <bib>F</bib>
          <lok>&FE; 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pagu tiom da mono, kiom virgulinoj <tld/>e ricevas dote
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 22:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia najbaro ordinare vizitas min &ccirc;iutage
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_y;&c_ch;&c_a;&c_j;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>oby&ccaron;ejn&ecaron;</trd>,
    <trd>oby&ccaron;ejn&ecaron;</trd>
  </trdgrp>
  <trd lng="de">gew&ouml;hnlich</trd>
  <trdgrp lng="fr">
    <trd>g&eacute;n&eacute;ralement</trd>,
    <trd>ordinairement</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x901A;&#x5E38;  <pr>&#x3064;&#x3046;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x666E;&#x901A;  <pr>&#x3075;&#x3064;&#x3046;</pr></trd>,
    <trd>&#x3075;&#x3060;&#x3093;</trd>
  </trdgrp>
  <trd lng="nl">gewoonlijk</trd>
