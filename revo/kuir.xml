<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kuir</rad>/i
</kap>
<drv mrk="kuir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kuir.0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Prepari nutra&jcirc;on, elmetante &gcirc;in al varmego por igi &gcirc;in
      man&gcirc;ebla:
      <ekz>
        <tld/>i viandon, supon<fnt>B</fnt>,
      </ekz>
      <ekz>
        teon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i terpomojn sub la cindro;
      </ekz>
      <ekz>
        malmole <tld/>ita ovo;
      </ekz>
      <ekz>
        restoracio kun bonfama <tld/>ado;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        plej <tld/>ita <klr>(kulturita, edukita)</klr> kampulo &ccirc;iam restos
        krudulo<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="bak.0i">baki</ref>,
      <ref cel="kuir.bol0i">bol<tld/>i</ref>,
      <ref cel="frit.0i">friti</ref>,
      <ref cel="rost.0i">rosti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_s;&c_t;&c_r;&c_a;&c_v;&c_u;)</klr></trd>,
    <trd>&c_k;&c_u;&c_h;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>kochen</trd>,
     <trd>zubereiten</trd>
  </trdgrp>
  <trd lng="cs">va&rcaron;it</trd>
  <trd lng="en">cook</trd>
  <trd lng="es">cocinar</trd>
  <trd lng="fr">cuire</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;sz&iacute;t <klr tip="ind">(&eacute;ltelt)</klr></trd>,
    <trd>f&odblac;z</trd>,
    <trd>s&uuml;t</trd>
  </trdgrp>
  <trd lng="nl">koken<klr tip="ind"> (van voedsel)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_o;&c_t;&c_o;&c_v;&c_i;&c_t;&c_mol; <klr>(&c_p;&c_i;&c_shch;&c_u;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kuir.0isto">
  <kap><ofc>*</ofc><tld/>isto</kap>
  <snc mrk="kuir.0isto.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Homo, kiu profesie preparas man&gcirc;a&jcirc;on:
      <ekz>
        ni havas diversajn servantojn: <tld/>iston, &ccirc;ambristinon,
        infanistinon
        <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_h;&c_a;&c_r;</trd>
  <trdgrp lng="de">
     <trd>Koch</trd>,
     <trd>K&ouml;chin</trd>
  </trdgrp>
  <trd lng="en">cook</trd>
  <trd lng="es">cocinero</trd>
  <trd lng="fr">cuisinier</trd>
  <trdgrp lng="hu">
    <trd>szak&aacute;cs</trd>,
    <trd>kukta</trd>
  </trdgrp>
  <trd lng="nl">kok</trd>
  <trd lng="ru">&c_p;&c_o;&c_v;&c_a;&c_r;</trd>
</drv>

<drv mrk="kuir.fusx0isto">
  <kap>fu&scirc;<tld/>isto</kap>
  <snc mrk="kuir.fusx0isto.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Mallerta <tld/>isto.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>schlechter <ind>Koch</ind></trd>
  </trdgrp>
  <trd lng="fr">g&acirc;te-sauce <klr>(mauvais cuisinier)</klr></trd>
  <trd lng="hu">kont&aacute;r <ind>szak&aacute;cs</ind></trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_h;&c_o;&c_j; <ind>&c_p;&c_o;&c_v;&c_a;&c_r;</ind></trd>
</drv>

<drv mrk="kuir.0ejo">
  <kap><ofc>*</ofc><tld/>ejo</kap>
  <snc mrk="kuir.0ejo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Parto de domo a&ubreve; de restoracio destinita por <tld/>i la
      man&gcirc;a&jcirc;ojn:
      <ekz>
        la <tld/>isto sidas en la <tld/>ejo
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_h;&c_n;&c_ja;</trd>
  <trdgrp lng="de">
     <trd>K&uuml;che</trd>,
     <trd>Kochstube</trd>
  </trdgrp>
  <trd lng="en">kitchen</trd>
  <trd lng="en">cocina</trd>
  <trd lng="fr">cuisine</trd>
  <trd lng="hu">konyha</trd>
  <trd lng="nl">keuken</trd>
  <trd lng="ru">&c_k;&c_u;&c_h;&c_n;&c_ja;</trd>
</drv>

<drv mrk="kuir.0arta">
  <kap><tld/>arta</kap>
  <snc mrk="kuir.0arta.KUI">
    <dif>
      Rilata al <tld/>ado kaj preparado de man&gcirc;a&jcirc;oj:
      <ekz>
        <tld/>artaj receptoj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kulina.0a">kulinara</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_ib;&c_n;&c_a;&c_r;&c_n;&c_y;</trd>
  <trd lng="en">culinary</trd>
  <trd lng="fr">culinaire</trd>
  <trdgrp lng="hu">
    <trd>konyhai</trd>,
    <trd>kulin&aacute;ris</trd>
  </trdgrp>
  <trd lng="nl">culinair</trd>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_i;&c_n;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="kuir.0poto">
  <kap><tld/>poto</kap>
  <snc mrk="kuir.0poto.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Poto metebla super fajro por kuiri nutra&jcirc;ojn en &gcirc;i.
    </dif>
    <refgrp tip="sub">
      <ref cel="kasero.0o">kaserolo</ref>,
      <ref cel="pat.0o">pato</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_s;&c_t;&c_r;&c_u;&c_l;&c_ja;</trd>,
    <trd>&c_g;&c_a;&c_r;&c_sh;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kochtopf</trd>
  </trdgrp>
  <trd lng="en">cooking utensil</trd>
  <trd lng="fr">pot &agrave; feu <klr>(cuisine)</klr></trd>
  <trdgrp lng="hu">
    <trd>faz&eacute;k</trd>,
    <trd>l&aacute;bas</trd>
  </trdgrp>
  <trd lng="nl">kookpan</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_t;&c_r;&c_ju;&c_l;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_t;&c_je;&c_l;&c_o;&c_k;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_sh;&c_o;&c_k; <klr>(&c_d;&c_l;&c_ja; &c_v;&c_a;&c_r;&c_k;&c_i;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kuir.bol0i">
  <kap>bol<tld/>i</kap>
  <snc mrk="kuir.bol0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="K"/>i man&gcirc;a&jcirc;on en bolanta akvo:
      <ekz>
        sa&ubreve;co a&ubreve; spica&jcirc;o <klr>{uzata}</klr> en bol<tld/>ado
        <fnt>
        <vrk>PIV2</vrk>, <lok>art. &leftquot;Sojo&rightquot;</lok>.
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="frit.0i">friti</ref>
  <trd lng="be">&c_v;&c_a;&c_r;&c_y;&c_c;&c_mol;
  <klr>(&c_s;&c_t;&c_r;&c_a;&c_v;&c_u;)</klr></trd>
  <trdgrp lng="de">
    <trd>frittieren</trd>,
    <trd>kochen<klr> (in kochendem Wasser)</klr></trd>
  </trdgrp>
  <trd lng="fr">faire <ind>bouillir</ind></trd>
  <trd lng="hu">f&odblac;z <klr>(l&eacute;ben)</klr></trd>
  <trd lng="nl">koken<klr tip="ind"> (in kokend water)</klr></trd>
  <trd lng="ru">&c_v;&c_a;&c_r;&c_i;&c_t;&c_mol; <klr>(&c_p;&c_i;&c_shch;&c_u;)</klr></trd>
</drv>
</art>
