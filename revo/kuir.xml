    <dif>
      <tld lit="K"/>i en vaporo:
      <ekz>
        pudingo <klr>[&#8230;]</klr> vapor<tld/>ita
        <fnt>
          <bib>Monato</bib>
          <aut>Petro De Smet'</aut>
          <vrk><url ref="&Monato;/2008/009292.php"
              >Sidante en pudingo, mi alvokas miajn savantojn!</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia nova &ccirc;ambro estas ekipita per <ind>vapor<tld/>ilo</ind> 
        <klr>(vd <ref cel="vapor.0umilo">vaporumilo</ref>)</klr> por moligi man&gcirc;a&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>HORI Yasuo</aut>
          <vrk><url ref="&Monato;/2003/008142.html"
              >Elefantoj ne forgesas ... kaj ne forgesitas</url></vrk>
          <lok>2003</lok>
        </fnt>.
        <trdgrp lng="de">
          <trd>D&auml;mpfer <klr>(Topf)</klr></trd>,
          <trd>Dampfgarer</trd>
        </trdgrp>
      </ekz>
    </dif>
    <ref tip="vid" cel="stuf.0i">stufi</ref>
  </snc>
  <trdgrp lng="de">
    <trd>d&auml;mpfen <klr>(garen)</klr></trd>,
    <trd>dampfgaren</trd>
  </trdgrp>
  <trd lng="en">steam <klr>(cook)</klr></trd>
</drv>






