<drv mrk="kuir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kuir.0i.KUI">
    <tezrad fak="KUI"/>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="prepar.0i.krei">Prepari</ref> nutra&jcirc;on, elmetante &gcirc;in al varmego por igi
      &gcirc;in man&gcirc;ebla:
      <ekz>
        la gvidanto <tld/>as por li pilafon <klr>[&#8230;]</klr> el kokina viando kaj rizo
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i enportigis la tema&scirc;inon, por <tld/>i por li bonan tason da sambuka teo
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li pulvorigis pagitajn honorajn versojn kaj mensogajn supertombajn parolojn
        <klr>[&#8230;]</klr> kaj <tld/>is ilin en larmoj, kiujn ploris la envio
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sor&ccirc;istino metis la kaldronon, por <tld/>i la sor&ccirc;an trinka&jcirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maljuna poeto <klr>[&#8230;]</klr> <tld/>is por li dol&ccirc;an vinon
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jakob</nom> <tld/>is <tld/>a&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 25:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        paron da bovoj <klr>[li]</klr> bu&ccirc;is ilin, kaj <klr>[&#8230;]</klr> <tld/>is la
        viandon kaj donis al la homoj
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 19:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        starigu la grandan kaldronon kaj <tld/>u supon
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 4:38</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        libere mi povas <tld/>i al mi &ccirc;iujn miajn teatra&jcirc;ojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Pupludisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li frapetas la &scirc;elon de mole <tld/>ita ovo
        <fnt>
          <bib>OIP</bib>
          <lok>Ombro sur interna pejza&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        kurso de <ind>&ccirc;ina <tld/>ado</ind>
        <fnt>
          <bib>ChC</bib>
          <lok>22</lok>
        </fnt>;
        <trd lng="de">chinesische <ind>K&uuml;che</ind> <klr>(Kochkunst)</klr></trd>
      </ekz>
      <ekz>
        kiu ka&ccirc;on <tld/>is, tiu &gcirc;in man&gcirc;u
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">die <ind>Suppe</ind> ausl&ouml;ffeln, die man sich eingebrockt hat</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        plej <tld/>ita <klr>(kulturita, edukita)</klr> kampulo
        &ccirc;iam restos krudulo
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="bak.0i.KUI">baki<sncref/></ref>,
      <ref cel="kuir.bol0i.KUI">bol<tld/>i</ref>,
      <ref cel="frit.0i">friti</ref>,
