<drv mrk="kuir.bol0i">
  <kap>bol<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kuir.bol0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="kuir.0i.KUI"><tld lit="K"/>i</ref>
      man&gcirc;a&jcirc;on en bolanta akvo:
      <ekz>
        sa&ubreve;co a&ubreve; spica&jcirc;o <klr>[uzata]</klr> en
        bol<tld/>ado
        <fnt>
        <vrk>PIV2</vrk>, <lok>art. &leftquot;Sojo&rightquot;</lok>.
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="frit.0i">friti</ref>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_r;&c_y;&c_c;&c_mol;
  <klr>(&c_s;&c_t;&c_r;&c_a;&c_v;&c_u;)</klr></trd>
  <trd lng="cs">va&rcaron;it ve vrouc&iacute; vod&ecaron;</trd>
  <trdgrp lng="de">
    <trd>frittieren</trd>,
    <trd>kochen<klr> (in kochendem Wasser)</klr></trd>
  </trdgrp>
  <trd lng="en">boil</trd>
  <trd lng="es">hervir</trd>
  <trd lng="fr">faire <ind>bouillir</ind></trd>
  <trd lng="hu">f&odblac;z <klr>(l&eacute;ben)</klr></trd>
  <trd lng="nl">koken<klr tip="ind"> (in kokend water)</klr></trd>
  <trd lng="pl">gotowa&cacute; <klr> (we wrz&aogonek;tku)</klr></trd>     
  <trd lng="ro">fierbe <klr> (&icirc;n ap&abreve; clocotit&abreve;)</klr></trd>
  <trd lng="ru">&c_v;&c_a;&c_r;&c_i;&c_t;&c_mol;
  <klr>(&c_p;&c_i;&c_shch;&c_u;)</klr></trd>
  <trd lng="sk">vari&tcaron; vo vriacej vode</trd>
  <trd lng="sv">koka</trd>
  <trd lng="tr">kaynatmak</trd>
