<drv mrk="kuir.0estro">
  <kap><tld/>estro</kap>
  <snc mrk="kuir.0estro.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Profesiulo direktanta <tld/>ejon, ekzemple en restoracio:
      <ekz>
        gustumu la froma&gcirc;okukojn <klr>...</klr>
        ili estas la fiero de la <tld/>estro
        <fnt>
          <aut>J. Ribillard</aut>,
          <vrk>Vagado sub palmoj</vrk>,
          <lok>1956</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_e;&c_f;-&c_k;&c_u;&c_h;&c_a;&c_r;</trd>,
    <trd>&c_k;&c_u;&c_h;&c_m;&c_ib;&c_s;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_k;&c_u;&c_h;&c_m;&c_a;&c_j;&c_s;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="en">head chef</trd>
  <trd lng="es">jefe de cocina</trd>
  <trdgrp lng="fr">
    <trd>chef cuisinier</trd>,
    <trd>maitre queux</trd>
  </trdgrp>
  <trd lng="pl">szef kuchni</trd>     
  <trd lng="ro">buc&abreve;tar-&#537;ef</trd>
</drv>

<drv mrk="kuir.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="kuir.0igxi.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      I&gcirc;i <tld/>ita:
      <ekz>
        fajro, &ccirc;e kiu <tld/>i&gcirc;is man&gcirc;a&jcirc;o
        <fnt>
          <aut>Eliza Orzeszko, trad. Dro L. L. Zamenhof</aut>,
          <vrk><url ref="http://www.steloj.de/esperanto/marta/">Marta</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">cuire</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="en">cook<klr> (become cooked)</klr></trd>
  <trd lng="es">cocinarse</trd>
  <trd lng="pl">gotowa&cacute; si&eogonek;</trd>     
  <trd lng="ro">s&abreve; g&abreve;te&#537;ti</trd>
</drv>

<drv mrk="kuir.0ilo">
