<kap>
  <ofc>1</ofc>
  <rad>hamak</rad>/o
</kap>
<drv mrk="hamak.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Scirc;nura reto a&ubreve; tola&jcirc;o, horizontale pendigita per
      siaj du ekstrema&jcirc;oj kaj formanta porteblan <ref tip="super" cel="lit.0o">liton</ref>, en kiu oni ku&scirc;as, a&ubreve; sin
      balancas:
      <ekz>
        post ne luksa vesperman&gcirc;o apud lumiga fajro, ni nin
        ku&scirc;igis en niajn <tld/>ojn por dormi
        <fnt><aut>Werner Schad</aut>:
        <vrk>Orinoko-delto: Indianoj, mar&ccirc;oj, arbarego</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 11a, p.
20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ja">&#x30CF;&#x30F3;&#x30E2;&#x30C3;&#x30AF;  <pr>&#x306F;&#x3093;&#x3082;&#x3063;&#x304F;</pr></trd>
  <trd lng="kek">ab&#x315;</trd>
  <trd lng="be">&c_g;&c_a;&c_m;&c_a;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>hamak</trd>,
    <trd>hamaka</trd>,
    <trd>visut&#xE1; s&#xED;&#x165;</trd>,
    <trd>z&#xE1;v&#x11B;sn&#xE1; houpa&#x10D;ka</trd>,
    <trd>z&#xE1;v&#x11B;sn&#xE9; l&#x16F;&#x17E;ko</trd>
  </trdgrp>
  <trd lng="de">H&auml;ngematte</trd>
  <trd lng="en">hammock</trd>
  <trd lng="es">hamaca</trd>
  <trd lng="fr">hamac</trd>
  <trd lng="hu">f&uuml;gg&odblac;&aacute;gy</trd>
  <trd lng="it">amaca</trd>
  <trd lng="nl">hangmat</trd>
  <trd lng="pl">hamak</trd>
  <trd lng="pt">rede<klr tip="amb">
  (de dormir, balan&ccedil;ar etc)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_m;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_je;&c_s;&c_n;&c_a;&c_ja;
    &c_k;&c_o;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hamak</trd>,
    <trd>visut&#xE1; poste&#x13E;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x433;&#x430;&#x43C;&#x430;&#x43A;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x432;&#x456;&#x441;&#x43D;&#x435; &#x43B;&#x456;&#x436;&#x43A;&#x43E;</trd>
  </trdgrp>
</drv>
</art>
