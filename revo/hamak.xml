<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>hamak</rad>/o
</kap>
<drv mrk="hamak.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      &Scirc;nura reto a&ubreve; tola&jcirc;o, horizontale pendigita per siaj du ekstrema&jcirc;oj
      kaj formanta porteblan <ref tip="super" cel="lit.0o">liton</ref>, en
      kiu oni ku&scirc;as, a&ubreve; sin balancas:
      <ekz>
        post ne luksa vesperman&gcirc;o apud lumiga fajro, ni nin ku&scirc;igis en
        niajn <tld/>ojn por dormi<fnt><aut>Werner Schad</aut>:
        <vrk>Orinoko-delto: Indianoj, mar&ccirc;oj, arbarego</vrk>,
        <bib>Monato</bib><lok>jaro 2000a, numero 11a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_m;&c_a;&c_k;</trd>
  <trd lng="de">H&auml;ngematte</trd>
  <trd lng="es">hamaca</trd>
  <trd lng="fr">hamac</trd>
  <trd lng="hu">f&uuml;gg&odblac;&aacute;gy</trd>
  <trd lng="nl">hangmat</trd>
  <trd lng="pt">rede<klr tip="amb"> (de dormir, balan&ccedil;ar etc)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_m;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_je;&c_s;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
