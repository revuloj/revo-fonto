<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>narval</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="narval.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="narval.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Monodon</trd>), kiu havas nur du dentojn, el kiuj unu 
      &ccirc;e la masklo kreskas spirale &gcirc;is la longo de 2-3m.
    </dif>
  </snc>
  <trd lng="de">Narwal</trd>
</drv>
<drv mrk="narval.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="cetac.0oj">cetacoj</ref>
      (<trd lng="la">Monodontidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Gr&uuml;ndelwale</trd>
</drv>
</art>
