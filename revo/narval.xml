<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>narval</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="narval.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zooligaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
      cel="narval1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Monodon</trd>), kiu havas nur du dentojn, el kiuj
      unu
      &ccirc;e la masklo kreskas spirale &gcirc;is la longo de 2-3m.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_r;&c_v;&c_a;&c_l;</trd>
  <trd lng="ca">narval</trd>
  <trd lng="de">Narwal</trd>
  <trd lng="el">&alfa;&rho;&kappa;&tau;&omikron;&phi;&alfa_ton;&lambda;&alfa;&jota;&ny;&alfa;</trd>
  <trdgrp lng="en">
    <trd>narwhal</trd>,
    <trd>narwhale</trd>
  </trdgrp>
  <trd lng="fr">narval</trd>
  <trd lng="hu">narv&aacute;l</trd>
  <trd lng="nl">narwal</trd>
  <trd lng="pt">narval</trd>
  <trd lng="ru">&c_n;&c_a;&c_r;&c_v;&c_a;&c_l;</trd>
</drv>
</art>
