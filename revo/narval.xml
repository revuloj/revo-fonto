<art mrk="$Id$">
<kap>
  <rad>narval</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="narval.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
      cel="narval1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Monodon</trd>), kiu havas nur du dentojn, el kiuj unu
      &ccirc;e la masklo kreskas spirale &gcirc;is la longo de 2-3m.
    </dif>
  </snc>
  <trd lng="de">Narwal</trd>
  <trdgrp lng="en">
    <trd>narwhal</trd>,
    <trd>narwhale</trd>
  </trdgrp>
  <trd lng="nl">narwal</trd>
  <trd lng="ru">&c_n;&c_a;&c_r;&c_v;&c_a;&c_l;</trd>
</drv>
</art>
