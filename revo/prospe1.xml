<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: prospe1.xml,v 1.21 2020/11/15 10:32:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>prosper</rad>/i <fnt><bib>FK</bib></fnt>
</kap>
<drv mrk="prospe1.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="prospe1.0i.ntr">
    <dif>
      <ref tip="super" cel="evolu.0i.disvolvigxi">Evolui</ref>
      en kontentiga, sukcesa maniero,
      eventuale atingante kontentigan rezulton
      <klr>(vd <ref cel="prospe1.0i_al"><tld/>i al</ref>)</klr>:
      <ekz>
        <klr>[la]</klr> malgranda rozarbeta&jcirc;o <klr>[&#8230;]</klr> kreskis kaj <tld/>is belege
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io, kion mi deziras, bone <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 23:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu koleras, tiu ne <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bril.0i">brili</ref>,
      <ref cel="flor.0i.prosperi">flori<sncref/></ref>,
      <ref cel="sukces.0i">sukcesi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_c;&c_mol;&c_v;&c_ib;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_v;&c_ib;&c_t;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_je;&c_c;&c_mol; <ind>&c_p;&c_o;&c_s;&c_mol;&c_p;&c_je;&c_h;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>da&rcaron;it se</trd>,
    <trd>m&iacute;t &uacute;sp&ecaron;ch</trd>,
    <trd>poda&rcaron;it se</trd>,
    <trd>prosperovat</trd>,
    <trd>prosp&iacute;vat</trd>,
    <trd>sv&ecaron;d&ccaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gedeihen</trd>,
    <trd>prosperieren</trd>,
    <trd><ind>Erfolg</ind> haben</trd>,
    <trd>gelingen</trd>
  </trdgrp>
  <trd lng="en">prosper</trd>
  <trd lng="es">prosperar</trd>
  <trdgrp lng="fr">
    <trd>prosp&eacute;rer</trd>,
    <trd>r&eacute;ussir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vir&aacute;gzik</trd>,
    <trd>el&odblac;rehalad</trd>,
    <trd>j&oacute;l megy</trd>,
    <trd>siker&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bloeien</trd>,
    <trd>gedijen</trd>,
    <trd>prospereren</trd>,
    <trd>geslaagd zijn</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_u;&c_s;&c_p;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_c;&c_v;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_m;&c_je;&c_t;&c_mol; &c_u;&c_s;&c_p;&c_je;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ma&tcaron; &uacute;spech</trd>,
    <trd>prosperova&tcaron;</trd>,
    <trd>prospie&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="prospe1.0a">
  <kap><tld/>a</kap>
  <snc mrk="prospe1.0a.KOMUNE">
    <dif>
      Bone vivanta, kreskanta, atinganta kontenton:
      <ekz>
        <tld/>aj urboj;
      </ekz>
      <ekz>
        <tld/>a stato;
      </ekz>
      <ekz>
        la kelkmonata regado de la vic-re&gcirc;o estis tre <tld/>a por <klr>[la lando]</klr>
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> cetere estis ne tute <tld/>a komparo
        <fnt>
          <bib>BdV</bib>
        </fnt>
        (<refgrp tip="vid">
          <ref cel="gxust.0a">&gcirc;usta</ref>,
          <ref cel="tauxg.0a">ta&ubreve;ga</ref>
        </refgrp>).
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="felicx.0a">feli&ccirc;a</ref>,
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>gedeihlich</trd>,
    <trd>prosperierend</trd>,
    <trd>erfolgreich</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>florissant</trd>,
    <trd>plantureux</trd>,
    <trd>prosp&egrave;re</trd>
  </trdgrp>
</drv>

<drv mrk="prospe1.0o">
  <kap><tld/>o</kap>
  <snc mrk="prospe1.0o.KOMUNE">
    <dif>
      Kontentiga stato:
      <ekz>
        lia subita ek<tld/>o mirigis &ccirc;iujn;
      </ekz>
      <ekz>
        post sufero venas <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sen ordo en afero ne ekzistas <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        tio donis monon, kaj al tio la grandkomercisto &scirc;uldis sian <tld/>on
        <fnt>
          <bib>Fab4</bib>
          <lok>Infana babilado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta malordo fini&gcirc;os per paco kaj &gcirc;enerala <tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="felicx.0o">feli&ccirc;o</ref>,
      <ref cel="flor.0ado">florado</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_c;&c_mol;&c_v;&c_ib;&c_t;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_r;&c_o;&c_s;&c_k;&c_v;&c_ib;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_mol;&c_p;&c_je;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rozkv&ecaron;t</trd>,
    <trd>rozvoj</trd>,
    <trd>zdar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gedeihen</trd>,
    <trd>Wohlstand</trd>
  </trdgrp>
  <trd lng="en">prosperity</trd>
  <trd lng="es">prosperidad</trd>
  <trdgrp lng="fr">
    <trd>prosp&eacute;rit&eacute;</trd>,
    <trd>r&eacute;ussite</trd>,
    <trd>succ&egrave;s</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vir&aacute;gz&aacute;s</trd>,
    <trd>siker</trd>,
    <trd>halad&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bloei</trd>,
    <trd>voorspoed</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_c;&c_v;&c_je;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_u;&c_s;&c_p;&c_je;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prosperita</trd>,
    <trd>rozkvet</trd>,
    <trd>rozvoj</trd>,
    <trd>&uacute;spech</trd>
  </trdgrp>
</drv>

<drv mrk="prospe1.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="prospe1.0igi.KOMUNE">
    <dif>
      Kreskigi, fruktigi, sukcesigi:
      <ekz>
        la bona Dio mem &gcirc;in plantis kaj <tld/>igis
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni provu diveni la &gcirc;eneralajn ka&ubreve;zojn kiuj <tld/>igas la 
        kart-a&ubreve;guran profesion
        <fnt>
          <aut>P. Cadei</aut>,
          <vrk><url ref="http://www.radio-vatikana-esperanto.org/elsendo.php?d=2004-05-13">
          Kial la kartodivenado furoras?</url></vrk>,
          <lok>Radio Vaticana, 2004-05-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>faire <ind>fructifier</ind><klr> (fig.)</klr></trd>,
    <trd>faire <ind>prosp&eacute;rer</ind></trd>,
  </trdgrp>
</drv>

<drv mrk="prospe1.mal0a">
  <kap>mal<tld/>a, <var><kap>ne<tld/>a</kap></var></kap>
  <snc mrk="prospe1.mal0a.KOMUNE">
    <dif>
      Ne sukcesanta, ne kreskanta:
      <ekz>
        ni kompreneble havos anta&ubreve; la okuloj ne la diversajn mal<tld/>ajn provojn
        anta&ubreve;ajn
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la plej mal<tld/>a konsisto de la kongreso en &gcirc;i tamen sidos homoj pensantaj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        la komenco estis ne<tld/>a
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne<tld/>a aludo
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Pli &gcirc;uste oni devus distingi inter &leftquot;ne<tld/>a&rightquot;, t.e. sen sukceso kaj
      &leftquot;mal<tld/>a&rightquot;, t.e. e&ccirc; dama&gcirc;a.
      <aut>WD</aut>
    </rim>
  </snc>
  <trdgrp lng="fr">
    <trd>ch&eacute;tif</trd>,
    <trd>chiche</trd>,
    <trd>inabouti</trd>
  </trdgrp>
</drv>

<drv mrk="prospe1.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="prospe1.mal0i.KOMUNE">
    <dif>
      Resti en malbona situacio, ne atingante la dezirindan staton:
      <ekz>
        mal<tld/>os al ili atingi, en la sama tago, la domon de <nom>Kedar</nom>
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 12a</lok></fnt>;
      </ekz>
      <ekz>
        mal<tld/>as <klr>[&#8230;]</klr> telefonkunligo de la najbaraj landoj
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="https://www.monato.be/2003/007918.html">Telefonmonopolisto malprosperas</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        se mal<tld/>as moralo kaj arto, justico ne trafas
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="https://www.monato.be/2013/010677.php">Lingva konfuzo kaj Konfuceo</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fiask.0i">fiaski</ref>,
      <ref cel="sukces.mal0i">malsukcesi</ref>
    </refgrp>
  </snc>
  <trd lng="cs">upadat</trd>
  <trdgrp lng="de">
    <trd>verfallen</trd>,
    <trd>scheitern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to languish</trd>,
    <trd>decline</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;cliner<klr> (d&eacute;p&eacute;rir)</klr></trd>,
    <trd>d&eacute;p&eacute;rir</trd>,
    <trd>&eacute;chouer</trd>,
    <trd>p&eacute;ricliter</trd>
  </trdgrp>
  <trd lng="nl">tegenspoed beleven</trd>
  <trd lng="sk">upada&tcaron;</trd>
</drv>

<drv mrk="prospe1.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="prospe1.mal0o.KOMUNE">
    <dif>
      Neatingo de ia bona&jcirc;o a&ubreve; dezirata&jcirc;o:
      <ekz>
        mi vagis tien kaj reen sur la ferdeko, kun kuspita kolumo kaj 
        la manoj enpo&scirc;e, pensante pri mia mal<tld/>o en la lernejo
        <fnt><bib>Kiso</bib>, <lok>la sakramento de la 
        komunio</lok></fnt>;
      </ekz>
      <ekz>
        la <klr>[&#8230;]</klr> mal<tld/>o de ka&scirc;e flegata miliononula konto
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="https://www.monato.be/2013/010812.php">Po&scirc;to perdita</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">&uacute;padek</trd>
  <trdgrp lng="de">
    <trd>Verfall</trd>,
    <trd>Mi&szlig;erfolg</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>decline</trd>,
    <trd>slump</trd>, <trd>stagnation</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;cadence</trd>,
    <trd>d&eacute;p&eacute;rissement</trd>,
    <trd>&eacute;chec</trd>,
    <trd>insucc&egrave;s</trd>
  </trdgrp>
  <trd lng="nl">tegenspoed</trd>
  <trdgrp lng="sk">
    <trd>neprospech</trd>,
    <trd>&uacute;padok</trd>
  </trdgrp>
</drv>

<drv mrk="prospe1.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="prospe1.mal0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi malkreskon, kadukecon, malbrili&gcirc;on:
      <ekz>
        tertremoj, kirloventoj, fajregoj kaj malsategoj okazis  
        lastajn jarojn en Kioto, kio  mal<tld/>igis la urbon &gcirc;is 
        ekstremo
        <fnt>
          <aut>R. Akutagawa, trad. T. Aoyama</aut>,
          <vrk><url ref="http://bluamonto.web.fc2.com/rashomon.pdf">
          Rash&#333;mon</url></vrk>,
          <lok>[vidita en 2017]</lok>
        </fnt>.
      </ekz>
      <ekz>
        maldungi laboristojn kaj maltrankviligi popolanojn signifas mal<tld/>igi
        kompaniojn
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="https://www.monato.be/2009/009770.php">Vila&gcirc;o por maldungitoj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekonomia mal<tld/>igo, korupto &ccirc;e &scirc;tataj oficistoj, e&ccirc; registara enmiksi&gcirc;o en
        mafiajn aferojn
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2005/008751.php">Post ok jaroj: &scirc;an&gcirc;o</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verschlechtern</trd>
  <trdgrp lng="fr">
    <trd>affaiblir</trd>,
    <trd>ruiner</trd>
  </trdgrp>
</drv>

<drv mrk="prospe1.0i_al">
  <kap>
    <tld/>i al
  </kap>
  <snc mrk="prospe1.0i_al.sukcesi">
    <dif>
      <ref tip="dif" cel="sukces.0i">Sukcesi</ref>:
      <ekz>
        la versfarado ne <tld/>as al mi
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        nenio <tld/>is al vi
        <fnt>
          <bib>NT</bib>
          <lok>Johano 12:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u <tld/>os al ili veni al interkonsento pri tiu &ccirc;i elekto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i afero iom <tld/>is al mi, kaj tiun &ccirc;i frukton de longatempaj persistaj
        laboroj mi proponas nun
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        nek al vi, nek al alia <tld/>os for&scirc;iri lin de mi
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>is al li konvinki almena&ubreve; kelkajn personojn
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">gelingen</trd>
</drv>


