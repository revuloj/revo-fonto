<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vanuatu.xml,v 1.11 2019/06/30 18:21:50 revo Exp $">
<kap>
  <rad>Vanuatu</rad>/o
</kap>

<drv mrk="vanuatu.0o">
  <kap>
    <tld/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">VU</mlg>
  <snc mrk="vanuatu.0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref> 
      en la <ref tip="vid" cel="oceani.O0o">Oceaniaj</ref> 
      insuloj kun &ccirc;efurbo Vilao:
      <ekz>
        tiu hipotezo eble klarigas la ekeston kaj la fortecon de kelkaj gigantaj
        tropikaj ciklonoj, kiuj lastatempe frapis <tld/>on, Filipinojn kaj
        Japanion
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2015/011497.php"
              >Ekstremaj veter-fenomenoj obli&gcirc;as</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        malgrandaj estas, ekzemple, <tld/>o, Tuvalo, Li&hcirc;ten&scirc;tejno kaj San-Marino
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2006/008875.php"
              >Mallonga historio de la evonima milito</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_V;&c_a;&c_n;&c_u;&c_a;&c_t;&c_u;</trd>
    <trd lng="bg">&c_V;&c_a;&c_n;&c_u;&c_a;&c_t;&c_u;</trd>
    <trdgrp lng="ca">
      <trd>Vanuatu</trd>,
      <trd>Rep&uacute;blica de Vanuatu</trd>
    </trdgrp>
    <trd lng="de">Vanuatu</trd>
    <trd lng="en">Vanuatu</trd>
    <trd lng="fr">Vanuatu</trd>
    <trd lng="id">Vanuatu</trd>
    <trdgrp lng="nl">
      <trd>Vanuatu</trd>,
      <trd>Nieuwe <ind>Hebriden</ind></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Republika <ind>Vanuatu</ind></trd>
    </trdgrp>
    <trd lng="pt">Vanuatu</trd>
</drv>

</art>

