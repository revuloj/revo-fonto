<drv mrk="intenc.0o">
  <kap><tld/>o</kap>
  <snc mrk="intenc.0o.KOMUNE">
    <dif>
      Konscia movo de la <ref cel="vol.0o">volo</ref> celanta ion:
      <ekz>
        <klr>[&scirc;i]</klr> faris &ccirc;iujn necesajn preparojn por efektivigi sian <tld/>on
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>David</nom> havis la <tld/>on konstrui domon
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 8:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o en la koro de homo estas profunda akvo, sed homo
        sa&gcirc;a &gcirc;in el&ccirc;erpos
        <fnt><bib>MT</bib>, <lok>&Sen; 20:5</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[lia]</klr> koro preparas malbonfarajn <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 6:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        li malka&scirc;is sian <tld/>on
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj fortiki&gcirc;as per konsilo
        <fnt><bib>MT</bib>, <lok>&Sen; 20:18</lok></fnt>;
      </ekz>
      <ekz>
        <ind>tia <tld/>o estas malproksime de li</ind>
        <fnt>
          <bib>VaK</bib>
          <lok>Deksepa &Ccirc;apitro</lok>
        </fnt>.
        <trd lng="de">das liegt ihm fern</trd>
        <trd lng="fr">c'est loin de son intention</trd>
      </ekz>
    </dif>
  </snc>
 <trd lng="en">intention</trd>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_m;&c_je;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_m;&c_a;</trd>,
