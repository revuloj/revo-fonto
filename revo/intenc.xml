<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>intenc</rad>/i
</kap>
<drv mrk="intenc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Konscie kaj klare ekvoli fari ion en nedifinita tempo:
      <ekz>
        <tld/>i fari ion<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <tld/>is plibonigi sian konduton, sed lia volo estas tro
        malforta;
      </ekz>
      <ekz>
        <tld/>i malbonon<fnt>Z</fnt>,
        malicon<fnt>Z</fnt>,
        ian fripona&jcirc;on;
      </ekz>
      <ekz>
        jam de longe mi <tld/>as viziti vin, sed mi nescias, &ccirc;u mi povos
        dum tiu &ccirc;i monato.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cel.0i">celi</ref>,
      <ref cel="plan.0i">plani</ref>,
      <ref cel="projek1">projekti</ref>,
      <ref cel="vol.0i">voli</ref>,
      <ref cel="propon">proponi al si</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>beabsichtigen</trd>,
    <trd>intendieren</trd>
  </trdgrp>
  <trd lng="en">intend</trd>
  <trdgrp lng="fr">
    <trd>avoir l'<ind>intention</ind> de</trd>,
    <trd>projeter de</trd>,
    <trd>tendre &agrave;</trd>,
    <trd>viser &agrave;</trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;nd&eacute;kozik</trd>
  <trd lng="nl">van <ind>plan</ind> zijn</trd>
  <trd lng="ru">&c_n;&c_a;&c_m;&c_je;&c_r;&c_je;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="intenc.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Konscia movo de la volo celanta ion:
      <ekz>
        <tld/>o en la koro de homo estas profunda akvo, sed homo sa&gcirc;a
        &gcirc;in el&ccirc;erpos
        <fnt><bib>MT</bib>, <lok>&Sen; 20:5</lok></fnt>;
      </ekz>
      <ekz>
        havi<fnt>Z</fnt>,
      </ekz>
      <ekz>
        prepari<fnt>Z</fnt>
      </ekz>
      <ekz>
        bonajn, malbonfarajn <tld/>ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>oj fortiki&gcirc;as per konsilo
        <fnt><bib>MT</bib>, <lok>&Sen; 20:18</lok></fnt>;
      </ekz>
      <ekz>
        malka&scirc;i sian <tld/>on al amiko;
      </ekz>
      <ekz>
        <ind>estas for de mi tia <tld/>o</ind>.
        <trd lng="fr">loin de moi cette intention</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Absicht</trd>,
    <trd>Intension</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>intention</trd>
    <trd>but<klr tip="ind"> (dessein)</klr></trd>
    <trd>dessein<klr tip="ind"> (but)</klr></trd>
    <trd>projet<klr tip="ind"> (but)</klr></trd>
    <trd>vis&eacute;e<klr tip="ind"> (but)</klr></trd>
    <trd>vue<klr tip="ind"> (but)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;nd&eacute;k</trd>,
    <trd>intenci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>doel</trd>,
    <trd>intentie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_m;&c_je;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_y;&c_s;&c_je;&c_l;</trd>
  </trdgrp>
</drv>

<drv mrk="intenc.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konscie volita:
      <ekz>
        tio estas <tld/>a mallerta&jcirc;o;
      </ekz>
      <ekz>
        <tld/>a fu&scirc;ado;
      </ekz>
      <ekz>
        parenc' al parenco ne malhelpas <ind><tld/>e</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trdgrp lng="fr">
          <trd>intentionnellement</trd>,
          <trd>&agrave; <ind>dessein</ind></trd>
        </trdgrp>
        <trd lng="hu">sz&aacute;nd&eacute;kosan</trd>
      </ekz>
      <ekz>
        tut<tld/>e mi parolas la&ubreve;te.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>beabsichtigt</trd>,
    <trd>absichtlich</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;lib&eacute;r&eacute;</trd>,
    <trd>intentionnel</trd>,
    <trd>voulu</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;nd&eacute;kos</trd>,
    <trd>akaratlagos</trd>
  </trdgrp>
  <trd lng="nl">gewild</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_n;&c_a;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_m;&c_y;&c_sh;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="intenc.bon0a">
  <kap>bon<tld/>a</kap>
  <snc>
    <dif>
      Havanta bonan <tld/>on:
      <ekz>
        <ind>bon<tld/>a</ind> sed vana klopodo.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">bien <ind>intentionn&eacute;</ind></trd>
  <trd lng="hu">j&oacute; <ind>akarat&uacute;</ind></trd>
</drv>

<drv mrk="intenc.malbon0a">
  <kap>malbon<tld/>a</kap>
  <snc>
    <dif>
      Havanta malbonan <tld/>on:
      <ekz>
        malvirtulo malbon<tld/>as kontra&ubreve; virtulo
        <fnt><bib>MT</bib><lok>&Psa; 37:12</lok></fnt>;
      </ekz>
      <ekz>
        li &jcirc;etis sur lin ian objekton <ind>sen malbon<tld/>o</ind>
        <fnt><bib>MT</bib><lok>&Nom; 35:22</lok></fnt>;
        <trd lng="fr">sans penser &agrave; <ind>mal</ind></trd>
      </ekz>
      <ekz>
        malpacienculo faras malsa&gcirc;a&jcirc;ojn 
        kaj malbon<tld/>ulo estas malamata
        <fnt><bib>MT</bib><lok>&Sen; 14:17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>mal <ind>intentionn&eacute;</ind></trd>,
    <trd>malintentionn&eacute;</trd>
  </trdgrp>
  <trd lng="hu">rossz <ind>akarat&uacute;</ind></trd>
</drv>

<drv mrk="intenc.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Nekonscie, nevole farita:
      <ekz>
        sen<tld/>a mortigo;
      </ekz>
      <ekz>
        mi devas &scirc;ajni sen<tld/>a<fnt>Z</fnt>
        <klr>(aganta sen<tld/>e)</klr>;
      </ekz>
      <ekz>
        pardonu, ke mi pu&scirc;is vin, &ccirc;ar mi tion faris tute
        <ind>sen<tld/>e</ind>.
        <trdgrp lng="fr">
          <trd>involontairement</trd>,
          <trd>sans le vouloir</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>akaratlanul</trd>,
          <trd>nem <ind>sz&aacute;nd&eacute;kosan</ind></trd>
        </trdgrp>
        <trd lng="ru">&c_n;&c_je;&c_ch;&c_a;&c_ja;&c_n;&c_n;&c_o;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>unbeabsichtigt</trd>,
    <trd>unabsichtlich</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>involontaire</trd>,
    <trd>non-intentionnel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>akaratlan</trd>,
    <trd>nem <ind>sz&aacute;nd&eacute;kos</ind></trd>
  </trdgrp>
  <trd lng="nl">ongewild</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_je;&c_d;&c_n;&c_a;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_ch;&c_a;&c_ja;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_m;&c_y;&c_sh;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
