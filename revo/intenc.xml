    <trd>&c_z;&c_u;&c_m;&c_i;&c_s;&c_n;&c_o;</trd>,
    <trd>&c_z;&c_u;&c_m;&c_i;&c_s;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_a;&c_r;&c_o;&c_ch;&c_i;&c_t;&c_o;</trd>
