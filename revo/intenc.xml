  </trdgrp>
</drv>

<drv mrk="intenc.bon0a">
  <kap>bon<tld/>a</kap>
  <snc mrk="intenc.bon0a.KOMUNE">
    <dif>
      Havanta bonan <tld/>on:
      <ekz>
        tiu estas politike bon<tld/>a, mi povas certigi al vi
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu kredo allogis ne malmultajn, bon<tld/>ajn virojn, al kiuj na&ubreve;zis la
        neesprimeble malpura idolkulto de ilia tempo
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;balda&ubreve; li malsani&gcirc;os, se li da&ubreve;ras plu!&rightquot; tiel babilis konsilantoj bon<tld/>aj
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        la konsilo, kiun ni donis al vi, estis bon<tld/>a &dash; sed stulta
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Kunvoja&gcirc;antoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_o;&c_b;&c_r;&c_a;&c_n;&c_a;&c_m;&c_je;&c_r;&c_n;&c_y;</trd>
  <trd lng="cs">dob&rcaron;e m&iacute;n&ecaron;n&yacute;</trd>
  <trdgrp lng="de">
    <trd>gutwillig</trd>,
    <trd>gutm&uuml;tig</trd>,
    <trd>wohlmeinend</trd>
  </trdgrp>
  <trd lng="en">good intentioned</trd>
  <trd lng="fr">bien <ind>intentionn&eacute;</ind></trd>
  <trd lng="hu">j&oacute; <ind>akarat&uacute;</ind></trd>
  <trd lng="ja">&#21892;&#x610F;&#x306E;  <pr>&#x305C;&#12435;&#12356;&#x306E;</pr></trd>
  <trd lng="nl">welgemeend</trd>
  <trdgrp lng="pl">
    <trd>z dobrymi <ind>ch&eogonek;ciami</ind></trd>,
    <trd>z dobrymi <ind>intencjami</ind></trd>,
    <trd>z dobr&aogonek; <ind>wol&aogonek;</ind></trd>,
    <trd>&zdot;yczliwy</trd>
  </trdgrp>
  <trd lng="pt">bem <ind>intencionado</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_n;&c_a;&c_m;&c_je;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s; <ind>&c_b;&c_l;&c_a;&c_g;&c_i;&c_m;&c_i;
    &c_n;&c_a;&c_m;&c_je;&c_r;&c_je;&c_n;&c_i;&c_ja;&c_m;&c_i;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dobrodu&scaron;n&yacute;</trd>,
    <trd>dobromyse&lcaron;n&yacute;</trd>,
    <trd>dobroprajn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_n;&c_a;&c_m;&c_ib;&c_r;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z; &c_d;&c_o;&c_b;&c_r;&c_i;&c_m;&c_i; &c_n;&c_a;&c_m;&c_ib;&c_r;&c_a;&c_m;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x597D;&#x610F; <pr>h&#x1CE;oy&#xEC;</pr></trd>
</drv>

<drv mrk="intenc.bon0o">
  <kap>bon<tld/>o</kap>
  <snc mrk="intenc.bon0o.KOMUNE">
    <dif>
      Deziro helpi, bone agi:
      <ekz>
        en iliaj okuloj mi vidis nenion krom la objektiva bon<tld/>o
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u oni povas klarigi per bon<tld/>oj de l&#8217; komunistoj, ke ili igas apena&ubreve;
        vestitajn virinojn kun su&ccirc;infanoj sur la brakoj staradi dum frostoj anta&ubreve; la
        kooperativoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> atestadas sian fratecan bon<tld/>on, por
        forturni la atenton pri siaj malamikaj ka&scirc;intencoj
        <fnt><bib>SatirRak</bib>, <lok>Kain kaj Abel</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">good intention</trd>
  <trd lng="be">&c_d;&c_o;&c_b;&c_r;&c_y; <ind>&c_n;&c_a;&c_m;&c_je;&c_r;</ind></trd>
  <trd lng="cs">dobr&yacute; &uacute;mysl</trd>
  <trdgrp lng="de">
    <trd>gute <ind>Absicht</ind></trd>,
    <trd>guter <ind>Wille</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bonne disposition</trd>,
    <trd>bonne intention</trd>,
    <trd>d&eacute;sir de bien faire</trd>,
    <trd>g&eacute;n&eacute;rosit&eacute;<klr tip="amb"> (d&eacute;sir du
    bien)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dobra <ind>wola</ind></trd>,
    <trd>dobre <ind>intencje</ind></trd>,
    <trd>dobre <ind>zamiary</ind></trd>,
    <trd>&zdot;yczliwo&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="pt">boa <ind>inten&ccedil;&atilde;o</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_i;&c_je;
    &c_n;&c_a;&c_m;&c_je;&c_r;&c_je;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_r;&c_y;&c_je;
    &c_n;&c_a;&c_m;&c_je;&c_r;&c_je;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_zh;&c_je;&c_l;&c_a;&c_n;&c_i;&c_je;
    &c_p;&c_o;&c_m;&c_o;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">dobr&yacute; &uacute;mysel</trd>
  <trdgrp lng="zh">
    <trd>&#x597D;&#x610F; <pr>h&#x1CE;oy&#xEC;</pr></trd>,
    <trd>&#21892;&#x610F; <pr>sh&#xE0;ny&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="intenc.kiel0e">
  <kap>kiel <tld/>e, <var><kap>kvaza&ubreve; <tld/>e</kap></var></kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="intenc.kiel0e.KOMUNE">
    <dif>
      Malprobable kaj tamen reale, kvaza&ubreve; iu <tld/>e farus:
      <ekz>
        kia &ccirc;agreno! kvaza&ubreve; <tld/>e neniu sin montras
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>!
      </ekz>
      <ekz>
        kvaza&ubreve; <tld/>e &ccirc;iuj renkontataj kamparanoj havis mizeran aspekton kaj rajdis
        malgrasajn &ccirc;evala&ccirc;ojn
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>III</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel mi timis, ke vi venos dum mia foresto! kaj, kiel <tld/>e,
        tio &ccirc;i okazis &gcirc;uste tiel
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 40a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="koinci.0o">koincido</ref>
    </refgrp>
  </snc>
  <trd lng="en">seemingly on purpose</trd>
  <trd lng="be">&c_b;&c_y;&c_c;&c_c;&c_a;&c_m; <ind>&c_n;&c_a;&c_w;&c_m;&c_y;&c_s;&c_n;&c_a;</ind></trd>
  <trdgrp lng="de">
    <trd>gleichsam <ind>absichtlich</ind></trd>,
    <trd>als ob es <ind>Absicht</ind> w&auml;re</trd>
  </trdgrp>
  <trd lng="fr">comme par un fait expr&egrave;s</trd>
</drv>

<drv mrk="intenc.malbon0a">
