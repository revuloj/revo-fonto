<drv mrk="intenc.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="intenc.0i.KOMUNE">
    <dif>
      Konscie kaj klare <ref tip="super" cel="vol.ek0i">ekvoli</ref> fari ion en nedifinita tempo:
      <ekz>
        el tiu knabo li <tld/>is fari famulon, &ccirc;u advokaton, &ccirc;u kuraciston
        <fnt>
          <bib>IdO</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la finan formon mi <tld/>is doni al la &leftquot;Lingvo Internacia&rightquot; ne pli frue ol en la
        fino de l' jaro 1888
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne e&ccirc; dum unu sola minuto li <tld/>is kulpigi Beatricon, kiu estis nur la
        senkulpa viktimo
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        poste, li <tld/>as viziti Romon kaj Parizon
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> foriris de ni kun mieno de persono, kiu <tld/>as ian fripona&jcirc;on
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> <tld/>as malicon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 16:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>is fari al mi malbonon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 50:20</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cel.0i">celi</ref>,
      <ref cel="plan.0i">plani</ref>,
      <ref cel="projek1.0i.plani">projekti<sncref/></ref>,
      <ref cel="propon">proponi al si</ref>,
      <ref cel="streb.0i.KOMUNE">strebi</ref>,
      <ref cel="vol.0i">voli</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_je;&c_c;&c_mol; <ind>&c_n;&c_a;&c_m;&c_je;&c_r;</ind></trd>,
    <trd>&c_n;&c_a;&c_m;&c_je;&c_r;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_m;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&iacute;t v &uacute;myslu</trd>,
    <trd>zam&yacute;&scaron;let</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beabsichtigen</trd>,
    <trd>intendieren</trd>,
    <trd>vorhaben</trd>
  </trdgrp>
  <trd lng="en">intend</trd>
  <trdgrp lng="es">
     <trd>tener la intenci&oacute;n</trd>,
     <trd>intentar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>avoir l'<ind>intention</ind> de</trd>,
    <trd>projeter de</trd>,
    <trd>viser &agrave;</trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;nd&eacute;kozik</trd>
  <trdgrp lng="ja">
    <trd>&#x610F;&#x56F3;&#12377;&#x308B;  <pr>&#12356;&#12392;&#12377;&#x308B;</pr></trd>,
    <trd>&#12418;&#x304F;&#x308D;&#12416;</trd>,
    <trd>&#x4F01;&#12390;&#x308B;  <pr>&#x304F;&#x308F;&#12384;&#12390;&#x308B;</pr></trd>,
    <trd>&#xFF5E;<ind>&#12377;&#x308B;&#12388;&#12418;&#x308A;&#12391;&#12354;&#x308B;</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>beogen</trd>,
    <trd>van zins zijn</trd>,
    <trd>van <ind>plan</ind> zijn</trd>,
    <trd>voorhebben</trd>,
    <trd>voornemens zijn</trd>,
    <trd>zich voorstellen</trd>,
    <trd>zinnens zijn</trd>
  </trdgrp>
  <trdgrp lng="pl">
