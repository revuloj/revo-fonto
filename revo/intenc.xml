<drv mrk="intenc.0e">
  <kap>
    <ofc>*</ofc>
    <tld/>e
  </kap>
  <snc mrk="intenc.0e.KOMUNE">
    <dif>
      Vole, celante la efektivan rezulton:
      <ekz>
        ili <tld/>e ne rigardas al li por montri al li sian mal&scirc;aton
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        Akcidente, &dash; ne <tld/>e, &dash; &#x2AFD; Mi sur vian &scirc;uon pa&scirc;is
        <fnt>
          <bib>LSB</bib>
          <lok>Ne Intence</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun okuloj pro indigno brilantaj &scirc;i ekkriis, la&ubreve;tegante <tld/>e la vo&ccirc;on
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>e sin dronigis
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        parenc' al parenco ne malhelpas <tld/>e
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
<trdgrp lng="en">
  <trd>intentionally</trd>,
  <trd>on purpose</trd>
</trdgrp>
    <trdgrp lng="pl">
      <trd>celowo</trd>,
      <trd>umy&sacute;lnie</trd>,
      <trd>naumy&sacute;lnie</trd>,
      <trd>z <ind>premedytacj&aogonek;</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_w;&c_m;&c_y;&c_s;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_y;&c_s;&c_n;&c_a;</trd>,
    <trd>&c_u;&c_m;&c_y;&c_s;&c_n;&c_a;</trd>,
    <trd>&c_z; <ind>&c_n;&c_a;&c_m;&c_je;&c_r;&c_a;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="de">absichtlich</trd>
  <trdgrp lng="fr">
    <trd>volontairement</trd>,
    <trd>&agrave; dessein</trd>,
    <trd>intentionnellement</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x610F;&#x56F3;&#30340;&#x306B;  <pr>&#12356;&#12392;&#12390;&#x304D;&#x306B;</pr></trd>,
    <trd>&#x308F;&#12374;&#12392;</trd>,
    <trd>&#25925;&#x610F;&#x306B;  <pr>&#12371;&#12356;&#x306B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>expres</trd>,
    <trd>met opzet</trd>,
    <trd>moedwillig</trd>,
    <trd>wetens</trd>,
    <trd>opzettelijk</trd>
  </trdgrp>
  <trdgrp lng="uk">
