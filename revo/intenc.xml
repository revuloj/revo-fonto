<drv mrk="intenc.sen0e">
  <kap><ofc>*</ofc>sen<tld/>e</kap>
  <snc mrk="intenc.sen0e.KOMUNE">
    <dif>
      Nevole, ne celinte la efektivan rezulton:
      <ekz>
        al homo, pekinta sen<tld/>e, Dio facile pardonas
        <fnt><bib>F</bib>, <lok>&FE; 22</lok></fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; kamarade kaj sen<tld/>e li metis sian brakon sur &scirc;iajn &scirc;ultrojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum li atendis, li sencele kaj sen<tld/>e iradis tien kaj reen en la parko,
        <klr>[&#8230;]</klr> pelate de interna maltrankvilo
        <fnt>
          <bib>Tor</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        kien povas forkuri mortiginto, kiu mortigis homon sen<tld/>e
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 35:11</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_n;&c_a;&c_w;&c_m;&c_y;&c_s;&c_n;&c_a;</trd>,
    <trd>&c_m;&c_ib;&c_m;&c_a;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_a;&c_d;&c_k;&c_o;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mimovoln&ecaron;</trd>,
    <trd>necht&ecaron;n&ecaron;</trd>,
