<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ligur.xml,v 1.14 2024/01/15 19:02:14 revo Exp $">
<kap>
  <rad>ligur</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ligur.0o">
  <kap><tld/>o</kap>
  <snc mrk="ligur.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Lo&gcirc;anto de
      <ref tip="malprt" cel="ligur.0ujo">Ligurujo</ref>.
    </dif>
  </snc>
  <snc mrk="ligur.0o.ETN">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Ano de anta&ubreve;-hinde&ubreve;ropa popolo, kiu lo&gcirc;is de
      Luni&gcirc;ianio &gcirc;is Pireneoj:
      <ekz>
        ili estas vere la solaj indaj posteuloj de la <tld lit="L"/>oj.
        <fnt>
          <aut>L. Beaucaire</aut>,
          <vrk>El la vivo de bervala senta&ubreve;gulo</vrk>,
          <lok>1974</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_g;&c_u;&c_r;&c_y;&c_je;&c_c;</trd>
  <trd lng="cs">Ligur</trd>
  <trd lng="de">Ligurer</trd>
  <trd lng="en">Ligurian</trd>
  <trd lng="fr">ligure<klr> (subst.)</klr></trd>
  <trd lng="it">Ligure</trd>
  <trd lng="ja">&#x30EA;&#x30B0;&#x30EA;&#x30A2;&#x4EBA;  <pr>&#x30EA;&#x30B0;&#x30EA;&#x30A2;&#x3058;&#x3093;</pr></trd>
  <trd lng="nl">Liguri&euml;r</trd>
  <trd lng="pl">Ligur</trd>
  <trd lng="sk">Ligur</trd>
  <trd lng="uk">&#x43B;&#x456;&#x491;&#x443;&#x440;&#x456;&#x454;&#x446;&#x44C;</trd>
</drv>

<drv mrk="ligur.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al la <tld lit="L"/>oj a&ubreve; al <tld lit="L"/>ujo.
    </dif>
    <ekz>
      <tld/>a lingvo; <tld/>a sa&ubreve;co; <tld lit="L"/>a Maro.
    </ekz>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_g;&c_u;&c_r;&c_y;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">ligurisch</trd>
  <trd lng="en">Ligurian</trd>
  <trd lng="fr">ligure<klr> (adj.)</klr></trd>
  <trd lng="it">ligure</trd>
  <trd lng="nl">Ligurisch</trd>
  <trd lng="pl">liguryjski</trd>
</drv>

<drv mrk="ligur.0ujo">
  <kap><tld lit="L"/>ujo</kap>
  <snc mrk="ligur.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Provinco en nord-okcidenta
      <ref tip="malprt" cel="ital.I0ujo">Italujo</ref> kun &ccirc;efurbo
      &Gcirc;enovo.
    </dif>
  </snc>
  <trd lng="be">&c_L;&c_ib;&c_g;&c_u;&c_r;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_L;&c_i;&c_g;&c_u;&c_r;&c_i;&c_ja;</trd>
  <trd lng="de">Ligurien</trd>
  <trd lng="en">Liguria</trd>
  <trd lng="it">Liguria</trd>
  <trd lng="fr">Ligurie</trd>
  <trd lng="nl">Liguri&euml;</trd>
  <trd lng="pl">Liguria</trd>     
  <trd lng="ro">Liguria</trd>
  <trdgrp lng="zh">
    <trd>&#x5229;&#x53E4;&#x91CC;&#x4E9A; <pr>L&#xEC;g&#x1D4;l&#x1D0;y&#xE0;</pr></trd>,
    <trd>&#x5229;&#x53E4;&#x91CC;&#x4E9E; <pr>L&#xEC;g&#x1D4;l&#x1D0;y&#xE0;</pr></trd>
  </trdgrp>
</drv>
</art>
