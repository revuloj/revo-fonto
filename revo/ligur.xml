<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ligur</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ligur.0o">
  <kap><tld/>o</kap>
  <snc mrk="ligur.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Lo&gcirc;anto de
      <ref tip="malprt" cel="ligur.Oujo">Ligurujo</ref>.
    </dif>
  </snc>
  <snc mrk="ligur.0o.ETN">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Ano de anta&ubreve;-hinde&ubreve;ropa popolo, kiu lo&gcirc;is de
      Luni&gcirc;ianio &gcirc;is Pireneoj:
      <ekz>
        ili estas vere la solaj indaj posteuloj de la <tld lit="L"/>oj.
        <fnt>
          <aut>L. Beaucaire</aut>,
          <vrk>El la vivo de bervala senta&ubreve;gulo</vrk>,
          <lok>1974</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">Ligurian</trd>
  <trd lng="fr">ligure<klr> (subst.)</klr></trd>
  <trd lng="it">Ligure</trd>
</drv>

<drv mrk="ligur.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al la <tld lit="L"/>oj a&ubreve; al <tld lit="L"/>ujo.
    </dif>
    <ekz>
      <tld/>a lingvo; <tld/>a sa&ubreve;co; <tld lit="L"/>a Maro.
    </ekz>
  </snc>
  <trd lng="en">Ligurian</trd>
  <trd lng="fr">ligure<klr> (adj.)</klr></trd>
  <trd lng="it">ligure</trd>
</drv>

<drv mrk="ligur.0ujo">
  <kap><tld lit="L"/>ujo</kap>
  <snc mrk="ligur.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Provinco en nord-okcidenta
      <ref tip="malprt" cel="ital.I0ujo">Italujo</ref> kun &ccirc;efurbo
      &Gcirc;enovo.
    </dif>
  </snc>
  <trd lng="it">Liguria</trd>
  <trd lng="fr">Ligurie</trd>
</drv>
</art>
