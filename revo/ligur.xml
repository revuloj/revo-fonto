<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ligur</rad>/o
</kap>
<drv mrk="ligur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="ligur.0o.popolo">
    <dif>
      Ano de anta&ubreve;-hinde&ubreve;ropa popolo, kiu situis de 
Luni&gcirc;ianio &gcirc;is Pireneoj.
    </dif>
    <ekz>
Ili estas vere la solaj indaj posteuloj de la <tld lit="L"/>oj.
<fnt>
<aut>L. Beaucaire</aut>
<vrk>El la vivo de bervala senta&ubreve;gulo</vrk>
</fnt>
    </ekz>
  </snc>
  <snc mrk="ligur.0o.logxanto">
    <dif>
Lo&gcirc;anto de 
<ref tip="malprt" cel="ligur.Oujo">Ligurujo</ref>.
    </dif>
  </snc>
  <trd lng="it">Ligure</trd>
  <trd lng="en">Ligurian</trd>
</drv>

<drv mrk="ligur.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
Rilatanta al la <tld lit="L"/>oj a&ubreve; al <tld lit="L"/>ujo.
    </dif>
    <ekz>
<tld/>a lingvo; <tld/>a sa&ubreve;co; <tld lit="L"/>a Maro.
      </ekz>
  </snc>
  <trd lng="it">ligure</trd>
  <trd lng="en">Ligurian</trd>
</drv>

<drv mrk="ligur.0ujo">
  <kap><tld lit="L"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
Regiono de nord-okcidenta 
<ref tip="malprt" cel="ital.I0ujo">Italujo</ref>. &Gcirc;ia &ccirc;efurbo 
estas &Gcirc;enovo.
    </dif>
<fnt>
<bib>Viki</bib>
<url ref="http://eo.wikipedia.org/wiki/Ligurujo">
Ligurujo</url></fnt>
</snc>
<trd lng="it">Liguria</trd>
</drv>

<drv mrk="ligur.0io">
  <kap><tld lit="L"/>io</kap>
<uzo tip="fak">GEOG</uzo>
<snc>
<ref tip="dif" cel="ligur.Oujo">Ligurujo</ref>
</snc>
<trd lng="it">Liguria</trd>
</drv>

</art>

<!--
$Log$
-->
</vortaro>