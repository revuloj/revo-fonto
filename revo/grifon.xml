<vortaro>
<art mrk="$Id$">
<kap><rad>grifon</rad>/o
<fnt>B</fnt></kap>
<drv mrk="grifon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Speco de hundo kun longaj hirtaj haroj.
    </dif>
    <refgrp tip="vid">
      <ref cel="pudel.0o">pudelo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Griffon</trd>,
    <trd>Rauhhaarpinscher</trd>
  </trdgrp>
  <trd lng="fr">griffon<klr tip="ind"> (chien)</klr></trd>
  <trd lng="nl">griffon</trd>
  <trd lng="ru">&c_g;&c_r;&c_i;&c_f;&c_o;&c_n; <klr>(&c_p;&c_o;&c_r;&c_o;&c_d;&c_a; &c_s;&c_o;&c_b;&c_a;&c_k;)</klr></trd>
</drv>
</art>
