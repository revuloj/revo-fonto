<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dizel.xml,v 1.28 2023/12/05 13:30:26 revo Exp $">
<kap>
  <rad>dizel</rad>/o
</kap>
<drv mrk="dizel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">AUT</uzo>
  <snc mrk="dizel.0o.brulajxo">
    <dif>
      <ref tip="super" cel="brul.0igajxo">Bruliga&jcirc;o</ref>,
      ordinare derivita el nafto, kiu injektita en
      forte densigitan aeron (de motoro), spontane ekbrulas:
      <ekz>
        ekzistas tre simpla rimedo por redukti tiun poluadon, ka&ubreve;zatan de <tld/>oj: fulgofiltriloj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008633.php">Polvo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        jam de pluraj monatoj la prezo de kruda petrolo, kaj sekve
        de &gcirc;iaj derivatoj kiel benzino kaj <tld/>oleo, da&ubreve;re kreskas
        <fnt>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2000/007184.html">Nehaltigeblaj prezalti&gcirc;oj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 8</lok>
          </fnt>;
      </ekz>
      <ekz>
        ekde la komenco de 2004 en Litovio alti&gcirc;is prezoj por benzino kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url ref="&Monato;/2004/008260.html">E&ubreve;ropaj prezoj minacas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_y;&c_z;&c_e;&c_l;&c_mol;&c_n;&c_a;&c_je; <ind>&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;</ind></trd>
    <trd lng="de">Diesel</trd>
    <trd lng="zh">&#x67F4;&#x6CB9;&#x53D1;&#x52A8;&#x673A;
    <pr>ch&#xE1;iy&#xF3;uf&amacron;d&#xF2;ngj&#x12B;</pr></trd>
</snc>
  <snc mrk="dizel.0o.motoro">
    <fnt><bib>SPV</bib></fnt>
    <dif>
      <ref tip="super" cel="motor.0o.TEK">Motoro</ref> inventita de
      <nom>Rudolf Diesel</nom> kaj patentita en 1892, funkciigata per
      <ref cel="dizel.0o.brulajxo">dizelo<sncref/></ref>:
      <ekz>
        plej granda merito estas la de li inventita kaj la&ubreve; li
        nomita <tld/>o, motoro kun interna brulado, en kiu la karbura&jcirc;o,
        injektita en forte densigitan aeron, spontane ekbrulas
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=3678">Rudolf Diesel</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">diesel motor</trd>
    <trdgrp lng="be">
      <trd>&c_d;&c_y;&c_z;&c_e;&c_l;&c_mol;</trd>,
      <trd>
      &c_d;&c_y;&c_z;&c_e;&c_l;&c_mol;&c_n;&c_y;
      &c_r;&c_u;&c_h;&c_a;&c_v;&c_ib;&c_k;
      </trd>
    </trdgrp>
    <trd lng="bg">&c_d;&c_i;&c_z;&c_je;&c_l;&c_o;&c_v; &c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_je;&c_l;</trd>
    <trd lng="ca">motor di&egrave;sel</trd>
    <trdgrp lng="cs">
      <trd>diesel</trd>,
      <trd>dieselov&yacute; motor</trd>
    </trdgrp>
    <trd lng="de">Dieselmotor</trd>
    <trd lng="el">&Ny;&tau;&jota;&zeta;&epsilon;&lambda;&omikron;&my;&eta;&chi;&alfa;&ny;&eta_ton;</trd>
    <trd lng="es">motor <ind>disel</ind></trd>
    <trd lng="fr">diesel<klr tip="ind"> (moteur)</klr></trd>
    <trd lng="he">&dalet;&yod;&zayin;&lamed;<klr tip="amb">
    (&mem;&nun;&vav;&ayin;)</klr></trd>
    <trd lng="hu">d&iacute;zelmotor</trd>
    <trd lng="it">diesel<klr> (motore)</klr></trd>
    <trd lng="nl">dieselmotor</trd>
    <trdgrp lng="pl">
      <trd>dizel<klr> (pot.)</klr></trd>,
      <trd>silnik Diesla</trd>,
      <trd>silnik wysokopr&eogonek;&zdot;ny</trd>
    </trdgrp>
    <trd lng="pt">motor diesel, motor a diesel</trd>
    <trd lng="ru">&c_d;&c_i;&c_z;&c_je;&c_l;&c_mol;</trd>
    <trd lng="sk">dieselov&yacute; motor</trd>
  </snc>
  <trd lng="ja">&#x30C7;&#x30A3;&#x30FC;&#x30BC;&#x30EB;&#x6A5F;&#x95A2; 
  <pr>&#x30C7;&#x30A3;&#x30FC;&#x30BC;&#x30EB;&#x304D;&#x304B;&#12435;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_i;&c_z;&c_je;&#x43B;&#x44C;</trd>,
    <trd>&c_d;&c_i;&c_z;&c_je;&#x43B;&#x44C;-&#x43C;&#x43E;&c_t;&#x43E;&c_r;</trd>
  </trdgrp>
</drv>
</art>
