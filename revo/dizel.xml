<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>dizel</rad>/o
</kap>
<drv mrk="dizel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>  
  <uzo tip="fak">AUT</uzo>
  <snc>
    <dif>
      Motoro kun interna brulado, en kiu la brula&jcirc;o, injektita
      en forte densigitan aeron, spontane ekbrulas.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_z;&c_e;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_y;&c_z;&c_e;&c_l;&c_mol;&c_n;&c_y; &c_r;&c_u;&c_h;&c_a;&c_v;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Dieselmotor</trd>
  <trd lng="es">motor <ind>disel</ind></trd>
  <trd lng="fr">diesel<klr tip="ind"> (moteur)</klr></trd>
  <trd lng="he">&dalet;&yod;&zayin;&lamed;<klr tip="amb"> (&mem;&nun;&vav;&ayin;)</klr></trd>
  <trd lng="hu">d&iacute;zelmotor</trd>
  <trd lng="ru">&c_d;&c_i;&c_z;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>

