<art mrk="$Id: hematolog.xml,v 1.7 2022/03/20 15:36:33 revo Exp $">
<kap>
    <rad>hematolog</rad>/o
</kap>
<drv mrk="hematolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="hematolog.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="kurac.0isto">Kuracisto</ref>, kies fako
      estas <ref tip="vid" cel="hematologi.0o">hematologio</ref>:
      <ekz>
        la germandevena <tld/>o <nom>Ernest Beutler</nom>, anka&ubreve; estis profesoro kaj departementa
        gvidanto en la esplorinstituto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=378492">Bruce Beutler</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">H&auml;matologe</trd>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_m;&c_a;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="cs">hematolog</trd>
  <trdgrp lng="en">
    <trd>haematologist</trd>,
    <trd>hematologist</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;matologue</trd>
  <trd lng="sk">hematol&oacute;g</trd>
  <trd lng="uk">&#x433;&#x435;&#x43C;&#x430;&#x442;&#x43E;&#x43B;&#x43E;&#x433;</trd>
</drv>
</art>

