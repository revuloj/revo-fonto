<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: faktur.xml,v 1.33 2023/11/09 19:13:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>faktur</rad>/o
</kap>
<drv mrk="faktur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Detala <ref tip="super" cel="not.0o.memoriga">noto</ref>
      (kutime <ref tip="super" cel="list.0o.KOMUNE">listo</ref>forma)
      pri la kvanto kaj prezo de venditaj komerca&jcirc;oj:
      <ekz>
        vi trovos tie &ccirc;i enfermitan la detalan <tld/>on
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        estus pli oportune havi &ccirc;iujn a&ccirc;etojn sur unu konto, &ccirc;ar tiel mi povus sendi la
        <tld/>on pri la suma elspezo al mia firmao por ricevi la repagon
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la filo de Kruko sendas sian <tld/>eton: por &ccirc;aselspezoj mil stelojn
        <fnt>
          <bib>KrB</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>numero
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        post la pago de la kostoj kaj diversaj <tld/>oj, restis la sumo de 3&#x202F;000 pundoj
        <fnt>
          <bib>Dan&gcirc;Lng</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sukceso de la po&scirc;telefona fenomeno rezultas <klr>[&#x2026;]</klr> parte
        de la manko de po&scirc;telefonaj <tld/>oj
        <fnt><aut>Christian Fabossou</aut>:
        <vrk>&Ccirc;u-vi-vidas-min-ismo</vrk>,
        <bib>Monato</bib><lok>jaro
        2001a, numero 12a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="kalkul.0o.fakturo">kalkulo<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_h;&c_u;&c_n;&c_a;&c_k;-&c_f;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_f;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>faktura</trd>,
    <trd>vystaven&yacute; &uacute;&ccaron;et</trd>
  </trdgrp>
  <trd lng="de">Rechnung</trd>
  <trd lng="en">invoice</trd>
  <trd lng="es">factura</trd>
  <trdgrp lng="fr">
    <trd>addition<klr> (facture)</klr></trd>,
    <trd>facture</trd>,
    <trd>note<klr> (facture)</klr></trd>
  </trdgrp><trd lng="fr">facture</trd>
  <trd lng="hu">sz&aacute;mla</trd>
  <trdgrp lng="id">
    <trd>faktur</trd>,
    <trd><ind>tagih</ind>an</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4ED5;&#x5207;&#x72B6;  <pr>&#x3057;&#x304D;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x9001;&#x308A;&#x72B6;  <pr>&#x304A;&#x304F;&#x308A;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x8ACB;&#x6C42;&#x66F8;  <pr>&#x305B;&#x3044;&#x304D;&#x3085;&#x3046;&#x3057;&#x3087;</pr></trd>
  </trdgrp>
  <trd lng="nl">factuur</trd>
  <trdgrp lng="pl">
    <trd>faktura</trd>,
    <trd>rachunek</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fatura</trd>,
    <trd>nota <klr tip="amb">(fiscal)</klr></trd>
  </trdgrp>
  <trd lng="ro">factur&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_ch;&c_jo;&c_t; &c_z;&c_a;
    &c_u;&c_s;&c_l;&c_u;&c_g;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_f;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">&uacute;&ccaron;et</trd>
  <trdgrp lng="tr">
    <trd>hesap</trd>,
    <trd>not</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&#x43A;&#x43B;&c_a;&c_d;&#x43D;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_h;&c_u;&#x43D;&#x43E;&#x43A;</trd>,
    <trd>&c_f;&c_a;&#x43A;&c_t;&c_u;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5355;&#x5B50; <pr>d&#x101;nzi</pr></trd>,
    <trd>&#x7D71;&#x4E00;&#x767C;&#x7968; <pr>t&#x1D2;ngy&#x12B;f&#x101;pi&#xE0;o</pr></trd>,
    <trd>&#x5E10; <pr>zh&#xE0;ng</pr></trd>,
    <trd>&#x8CEC; <pr>zh&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="faktur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Enskribi sur <tld/>on:
      <ekz>
        ili <tld/>us siajn elspezojn al la &scirc;ipestro, kaj tiu minusus &gcirc;in de la salajro
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kompanioj jam indikas &ccirc;iujn prezojn en e&ubreve;roj, kaj iom pli da ili jam <tld/>as
        en ili
        <fnt>
       <aut>Marc Vanden Bempt</aut>
          <vrk><url ref="&Monato;/2000/006841.php">E&ubreve;ro</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel multe <tld/>as doktoroj
        <fnt>
          <bib>LOdE</bib>
          <aut>Anton &Ccirc;e&hcirc;ov</aut>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">La vivo en demandoj kaj
          ekkrioj</url></vrk>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; <ind>&c_r;&c_a;&c_h;&c_u;&c_n;&c_a;&c_k;</ind></trd>,
    <trd>&c_u;&c_p;&c_ib;&c_s;&c_v;&c_a;&c_c;&c_mol; &c_u; <ind>&c_r;&c_a;&c_h;&c_u;&c_n;&c_a;&c_k;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fakturovat</trd>,
    <trd>vy&uacute;&ccaron;tovat</trd>,
    <trd>&uacute;&ccaron;tovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>berechnen</trd>,
    <trd>in <ind>Rechnung</ind> stellen</trd>
  </trdgrp>
  <trd lng="es">facturar</trd>
  <trd lng="fr">facturer</trd>
  <trd lng="hu">sz&aacute;ml&aacute;z</trd>
  <trdgrp lng="ja">
    <trd>&#x4ED5;&#x5207;&#x72B6;&#x3092;&#x4F5C;&#x6210;&#x3059;&#x308B;
    <pr>&#x3057;&#x304D;&#x308A;&#x3058;&#x3087;&#x3046;&#x3092;&#x3055;&#x304F;&#x305B;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8ACB;&#x6C42;&#x3059;&#x308B;  <pr>&#x305B;&#x3044;&#x304D;&#x3085;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">factureren</trd>
  <trdgrp lng="pl">
    <trd>fakturowa&cacute;</trd>,
    <trd>dopisywa&cacute; do <ind>rachunku</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fakturova&tcaron;</trd>,
    <trd>&uacute;&ccaron;tova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#x43F;&c_i;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i; &c_v; &#x43D;&c_a;&#x43A;&#x43B;&c_a;&c_d;&#x43D;&c_u;</trd>,
    <trd>&c_v;&#x43D;&#x43E;&c_s;&c_i;&c_t;&c_i; &c_d;&#x43E; &c_r;&c_a;&c_h;&c_u;&#x43D;&#x43A;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8003;&#x616E;&#x67D0;&#x4E8B; <pr>k&#x1CE;ol&#xFC;m&#x1D2;ush&#xEC;</pr></trd>,
    <trd>&#x5F00;&#x7ACB;&#x53D1;&#x7968; <pr>k&#x101;il&#xEC;f&#x101;pi&#xE0;o</pr></trd>
  </trdgrp>
</drv>
</art>
