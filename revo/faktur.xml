<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>faktur</rad>/o
</kap>
<drv mrk="faktur.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<uzo tip="fak">EKON</uzo>
<snc>
<dif>
Detala noto pri la kvanto k prezo de venditaj komerca&jcirc;oj. 
</dif>
</snc>
<trd lng="de">Rechnung</trd>
<trd lng="en">invoice</trd>
<trd lng="fr">facture</trd>
<trd lng="nl">factuur</trd>
<trdgrp lng="ru">
  <trd>&c_s;&c_ch;&c_jo;&c_t; &c_z;&c_a; &c_u;&c_s;&c_l;&c_u;&c_g;&c_i;</trd>,
  <trd>&c_n;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_a;&c_ja;</trd>,
  <trd>&c_f;&c_a;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
</trdgrp>
</drv>
<drv mrk="faktur.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<uzo tip="fak">EKON</uzo>
<snc>
<dif>
Enskribi sur <tld/>on.  
</dif>
</snc>
<trdgrp lng="de">
     <trd>berechnen</trd>,
     <trd>in <ind>Rechnung stellen</ind></trd></trdgrp>
<trd lng="fr">facturer</trd>
<trd lng="nl">factureren</trd>
</drv>
</art>
