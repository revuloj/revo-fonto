<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ilici</rad>/o
</kap>
<drv mrk="ilici.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
cel="magnoli1.0oj">magnoliacoj</ref>
      (<trd lng="la">Illicium</trd>), kies fruktoj estas uzataj kiel
      spica&jcirc;o.
    </dif>
    <ref tip="prt" cel="aniz.stel0o">stelanizo</ref>
  </snc>
  <trd lng="de">Sternanisbaum</trd>
  <trd lng="en">star anise</trd>
  <trdgrp lng="hu">
    <trd>csillag&aacute;nizs</trd>,
    <trd>&aacute;nizscsillag</trd>
  </trdgrp>
  <trd lng="nl">steranijsboom</trd>
  <trdgrp lng="pt">
   <trd>badiana</trd>, 
   <trd>anis-estrelado</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_d;&c_mol;&c_ja;&c_n;</trd>,
    <trd>&c_z;&c_v;&c_jo;&c_z;&c_d;&c_ch;&c_a;&c_t;&c_y;&c_j;
&c_a;&c_n;&c_i;&c_s;</trd>
  </trdgrp>
</drv>
</art>

