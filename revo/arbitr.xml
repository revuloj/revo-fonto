<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>arbitr</rad>/a
</kap>
<drv mrk="arbitr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Libervola, propradecida, la&ubreve;kaprica, senkontrola, despota:
      <ekz>
        <tld/>a ago, ju&gcirc;o, regulo, povo;
      </ekz>
      <ekz>
        &ccirc;iuj vortoj de Volap&uuml;k estis <tld/>e
        elpensitaj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_m;&c_a;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_w;&c_p;&c_r;&c_a;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_v;&c_a;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">willk&uuml;rlich</trd>
  <trd lng="en">arbitrary</trd>
  <trd lng="es">arbitrario</trd>
  <trd lng="fr">arbitraire (adj.)</trd>
  <trd lng="hu">&ouml;nk&eacute;nyes</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_v;&c_o;&c_je;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_u;&c_p;&c_r;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">svojvo&lcaron;n&yacute;</trd>
  <trd lng="sv">godtycklig</trd>
</drv>

<drv mrk="arbitr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld/>a ago:
      <ekz>
        severaj reguloj permesantaj
        nenian escepton nek <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        agi la&ubreve; sia <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni ne povas krei lingvon la&ubreve; sia <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_v;&c_a;&c_v;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_v;&c_a;&c_v;&c_o;&c_l;&c_mol;&c_l;&c_je;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_w;&c_p;&c_r;&c_a;&c_w;&c_n;&c_a;&c_s;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_w;&c_p;&c_r;&c_a;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_d;&c_u;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Willk&uuml;r</trd>
  <trd lng="es">arbitrariedad</trd>
  <trd lng="fr">arbitraire (subst.)</trd>
  <trd lng="hu">&ouml;nk&eacute;ny</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_o;&c_l;</trd>,
    <trd>&c_s;&c_v;&c_o;&c_je;&c_v;&c_o;&c_l;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_o;&c_u;&c_p;&c_r;&c_a;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">svojv&ocirc;&lcaron;a</trd>
</drv>
</art>
