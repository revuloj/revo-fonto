<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: somnol.xml,v 1.9 2023/09/30 20:23:20 revo Exp $">
<kap>
  <rad>somnol</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="somnol.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="somnol.0i.KOMUNE">
    <dif>
      Nek dormi, nek maldormi,
      t.e. <ref tip="dif" cel="dorm.duon0i.KOMUNE">duondormi</ref>:
      <ekz>
        &gcirc;i aspektas iom senvive, sed &gcirc;i nur <tld/>as,
        vidu, kiel moveti&gcirc;as la antenoj
        <fnt>
          <aut>J. Camacho</aut>,
          <vrk>La Majstro kaj Martinelli</vrk>,
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hebet.0a">hebeta</ref>,
      <ref cel="somnam.0i">somnambuli</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>kl&iacute;mat</trd>,
    <trd>pod&rcaron;imovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><ind>benommen</ind> sein</trd>,
    <trd>halb <ind>schlafen</ind></trd>,
    <trd><ind>verschlafen</ind> sein</trd>
  </trdgrp>
  <trd lng="pl">&sacute;ni&cacute; na jawie</trd>
  <trd lng="sk">podriemkava&tcaron;</trd>
</drv>

<drv mrk="somnol.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="somnol.0o.KOMUNE">
    <dif>
      Stato de reduktita vigleco,
      <ref tip="dif" cel="dorm.duon0o.KOMUNE">duondormo</ref>:
      <ekz>
        ankora&ubreve; en <tld/>o, &scirc;i trenis sin al la fenestro
        <fnt>
          <aut>M. Fern&aacute;ndez</aut>,
          <vrk>Du Gardenioj</vrk>,
          <lok>Ekstremoj, 1997</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="letarg.0o.FIG">letargio<sncref/></ref>,
      <ref cel="stupor.0o">stuporo</ref>,
      <ref cel="torpor.0o.KOMUNE">torporo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>d&rcaron;&iacute;mota</trd>,
    <trd>polosp&aacute;nek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Somnolenz</trd>,
    <trd>Benommenheit</trd>,
    <trd>Halbschlaf</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>p&oacute;&lstroke;sen</trd>,
    <trd>hipnagogia</trd>
  </trdgrp>
  <trd lng="sk">polosp&aacute;nok</trd>
  <trd lng="uk">&c_f;&c_ib;&c_z;&c_ib;&#x43E;&#x43B;</trd>
</drv>
</art>

