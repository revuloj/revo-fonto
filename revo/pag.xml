<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>pag</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pag.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pag.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Kvitigi sin de materia a&ubreve; morala &scirc;uldo per dono de
      mono a&ubreve; alia valora&jcirc;o:
      <ekz>
        mi <tld/>is du frankojn por tiu libro;
      </ekz>
      <ekz>
        <tld/>i por fremda festeno
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>ataj kamparanoj helpis nin;
      </ekz>
      <ekz>
        por lia amikeco konfidon <tld/>u<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i al iu malbonon por bono<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni ne <tld/>as per gloro al sia tajloro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ni devas konservi tiun bona&jcirc;on e&ccirc; <tld/>ante
        &gcirc;in per kelkaj foroferoj;
      </ekz>
      <ekz>
        <tld/>i &scirc;uldon, tributon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        salajron, imposton;
      </ekz>
      <ekz>
        <tld/>i siajn servistojn<fnt>B</fnt>,
      </ekz>
      <ekz>
        kreditorojn, la man&gcirc;on, la lo&gcirc;ejon<fnt>B</fnt>,
      </ekz>
      <ekz>
        la viziton de kuracisto;
      </ekz>
      <ekz>
        kia ago, tia <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>atesto, <tld/>kvitanco;
      </ekz>
      <ekz>
        <tld/>dato, <tld/>limo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>bezahlen</trd>,
      <trd>zahlen</trd>
    </trdgrp>
  </snc>
  <snc mrk="pag.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="vid" cel="kompen1.0i.KOMUNE">Kompensi</ref> agon,
      precipe malbonan agon:
    </dif>
    <subsnc mrk="pag.0i.vengxi">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Trakti iun same malbone, kiel li nin a&ubreve; aliulon traktis,
        <ref cel="vengx.0i">ven&gcirc;i</ref> al iu:
        <ekz>
          mi <tld/>os al &scirc;i
          por <klr>(pro)</klr> &ccirc;io &ccirc;i tio<fnt>Z</fnt>;
        </ekz>
        <ekz>
          kiam li falos en niajn manojn, ni <tld/>os al li la
          &ccirc;agrenojn <klr>(ni punos lin pro la &ccirc;agrenoj)</klr>,
          kiujn li ka&ubreve;zis al la kronprinco<fnt>B</fnt>.
        </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="pag.re0i.vengxi">re<tld/>i <sncref/></ref>
      </refgrp>
      <trd lng="de">heimzahlen</trd>
    </subsnc>
    <subsnc mrk="pag.0i.ricevi_kompensan_malbonon">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Esti traktata same malbone, kiel oni traktis aliulon, suferi
        ven&gcirc;on de iu; ricevi punon, kompensan malbonon:
        <ekz>
          kiu rompis, tiu <tld/>u
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          <tld/>os <klr>(estos punata)</klr> la lupo por
          <klr>(pro)</klr>
          la
          &scirc;afo<fnt>Z</fnt>;
        </ekz>
        <ekz>
          por &ccirc;iu ago venas la tempo de <tld/>o
          <fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          <tld/>as maljunaj jaroj por junaj eraroj
          <fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="pag.re0i.ricevi_kompensan_malbonon">re<tld/>i
        <sncref/></ref>,
        <ref cel="pag.pun0i">pun<tld/>i</ref>.
      </refgrp>
      <refgrp tip="vid">
        <ref cel="respon.pri0i0eci0umi">prirespondi</ref>.
      </refgrp>
      <trd lng="de">bezahlen</trd>
    </subsnc>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="cs">platit</trd>
  <trd lng="en">pay</trd>
  <trd lng="fr">payer</trd>
  <trdgrp lng="hu">
    <trd>fizet</trd>,
    <trd>megfizet</trd>
  </trdgrp>
  <trd lng="nl">betalen</trd>
  <trd lng="no">betale</trd>
  <trdgrp lng="pt">
    <trd>pagar</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="pag.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
    <trdgrp lng="de">
      <trd>Abgelten</trd>,
      <trd>Auszahlen</trd>,
      <trd>Bezahlen</trd>,
      <trd>Einzahlen</trd>,
      <trd>Verg&uuml;ten</trd>,
      <trd>Zahlen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fizet&eacute;s</trd>,
      <trd>kifizet&eacute;s</trd>,
      <trd>megfizet&eacute;s</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Tio, kion oni <tld/>as.
    </dif>
    <trdgrp lng="de">
      <trd>Abgeltung</trd>,
      <trd>Auszahlung</trd>,
      <trd>Bezahlung</trd>,
      <trd>Einzahlung</trd>,
      <trd>Verg&uuml;tung</trd>,
      <trd>Zahlung</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fizet&eacute;s <klr tip="ind">(fizets&eacute;g)</klr></trd>,
      <trd>fizets&eacute;g</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="en">payment</trd>
  <trd lng="fr">paiement</trd>
  <trd lng="nl">betaling</trd>
  <trd lng="no">betaling</trd>
</drv>

<drv mrk="pag.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="pag.0ejo.EKON">
    <dif>
      Gi&ccirc;eto, budo, oficejo a&ubreve; ajna loko por <tld/>ado:
      <ekz>
        la buso haltas &ccirc;e <tld/>ejo
        <fnt>
        <aut>M. Lahtinen</aut>,
        <vrk><url
        ref="http://users.jyu.fi/~mlahtine/Chin2004/TTT/aug10.htm">
        &Ccirc;inio en Transformi&gcirc;o</url></vrk>,
        <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>caisse<klr> (endroit o&ugrave; on paie)</klr></trd>,
    <trd>p&eacute;age</trd>
  </trdgrp>
  <trd lng="nl">kassa</trd>
  <trd lng="no">kasse</trd>
</drv>

<drv mrk="pag.al0o">
  <kap>al<tld/>o, <var><kap>pli<tld/>o</kap></var></kap>
  <snc mrk="pag.al0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Tio, kion oni devas <tld/>i plie ol la ordinara a&ubreve;
      konsentita prezo.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Mehrzahlung</trd>,
    <trd>&Uuml;berzahlung</trd>,
    <trd>Zuzahlung</trd>,
    <trd>zus&auml;tzliche Zahlung</trd>
  </trdgrp>
  <trd lng="fr">paiement en <ind>suppl&eacute;ment</ind></trd>
  <trdgrp lng="hu">
    <trd>fel&aacute;r</trd>,
    <trd>t&ouml;bbletfizet&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">bijbetaling</trd>
  <trd lng="ru">&c_d;&c_o;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
</drv>

<drv mrk="pag.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pag.antaux0i.EKON">
    <dif>
      <tld lit="P"/>i pli frue ol la deviga fiksita <tld/>dato.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_z;&c_a;&c_g;&c_a;&c_d;&c_z;&c_ja;,
    &c_n;&c_a;&c_p;&c_je;&c_r;&c_a;&c_d;)</klr></trd>,
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_p;&c_l;&c_a;&c_t;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anzahlen</trd>,
    <trd>vorausbezahlen</trd>,
    <trd><klr>im </klr>Voraus <ind>bezahlen</ind></trd>,
    <trd><klr>im </klr>Voraus leisten</trd>,
  </trdgrp>
  <trd lng="en">prepay</trd>
  <trdgrp lng="fr">
    <trd>payer par avance</trd>,
    <trd>pr&eacute;payer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&odblac;re <ind>fizet</ind></trd>,
    <trd>el&odblac;leget <ind>fizet</ind></trd>,
    <trd>megel&odblac;legez</trd>
  </trdgrp>
  <trd lng="nl">betalen op voorhand</trd>
  <trd lng="no">forh&aring;ndsbetale</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol; &c_v;&c_p;&c_je;&c_r;&c_jo;&c_d;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_p;&c_r;&c_je;&c_d;&c_o;&c_p;&c_l;&c_a;&c_t;&c_u;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="pag.de0o">
  <kap>de<tld/>o<fnt>Z</fnt></kap>
  <snc mrk="pag.de0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Tio, kion oni devas <tld/>i kiel imposton.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_l;&c_ib;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_y;&c_l;&c_ib;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Abgabe</trd>
  </trdgrp>
  <trd lng="fr">contribution financi&egrave;re</trd>
  <trdgrp lng="hu">
    <trd>ad&oacute;</trd>,
    <trd>illet&eacute;k</trd>,
    <trd>levon&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">belasting</trd>
  <trd lng="no">skatt</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_ch;&c_i;&c_s;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_v;&c_y;&c_ch;&c_je;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="pag.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="pag.re0i.redoni_0on">
    <dif>
      <tld lit="P"/>i al iu tion, kion li <tld/>is anstata&ubreve; vi
      a&ubreve; por vi:
      <ekz>
        mi re<tld/>os al vi la kostojn de la voja&gcirc;o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zur&uuml;ckzahlen</trd>,
      <trd>tilgen</trd>
    </trdgrp>
    <trd lng="fr">rembourser</trd>
    <trdgrp lng="hu">
      <trd>visszafizet</trd>,
      <trd>megt&eacute;r&iacute;t</trd>
    </trdgrp>
    <trd lng="nl">terugbetalen</trd>
    <trd lng="no">betale tilbake</trd>
    <trd lng="fr">rembourser</trd>
    <trd lng="ru">&c_o;&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="pag.re0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="pag.0i.FIG"><tld/>i <sncref/></ref>
    <subsnc mrk="pag.re0i.vengxi">
      <ref tip="dif" cel="pag.0i.vengxi"><tld/>i <sncref/></ref>
      <ekz>
        ni mortigis multajn Rusojn, sed ili poste malavare re<tld/>is
        al ni;
      </ekz>
      <ekz>
        re<tld/>u al li la&ubreve; liaj agoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        lia faro estos re<tld/>ita al li<fnt>Z</fnt>.
      </ekz>
      <trd lng="de">heimzahlen</trd>
      <trd lng="hu">megfizet <klr>vmi&eacute;rt</klr></trd>
      <trd lng="nl">betaald zetten</trd>
      <trd lng="ru">&c_o;&c_t;&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
    </subsnc>
    <subsnc mrk="pag.re0i.ricevi_kompensan_malbonon">
      <ref tip="dif" cel="pag.0i.ricevi_kompensan_malbonon"><tld/>i
      <sncref/></ref>
      <ekz>
        morga&ubreve; la homaro devos re<tld/>i la kulpojn <klr>(a&ubreve;:
        pro la kulpoj)</klr> de la nuna facilanima generacio.
      </ekz>
      <refgrp tip="sin">
        <ref cel="pag.pun0i">pun<tld/>i</ref>.
      </refgrp>
      <trd lng="de">daf&uuml;r <ind>bezahlen</ind></trd>
      <trd lng="hu">megb&udblac;nh&odblac;dik</trd>
      <trd
      lng="ru">&c_r;&c_a;&c_s;&c_p;&c_l;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;
      <klr>(&c_z;&c_a; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
    </subsnc>
    <trd lng="fr">rendre<klr> (le mal pour le mal etc.)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_p;&c_l;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_p;&c_l;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_l;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="pag.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="pag.sen0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Ricevebla sen <tld/>o:
      <ekz>
        de kantado sen<tld/>a doloras la gor&gcirc;o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        sen<tld/>a eniro;
      </ekz>
      <ekz>
        sen<tld/>e disdonita flugfolio.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_s;&c_p;&c_l;&c_a;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_r;&c_m;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="de">kostenlos</trd>
  <trd lng="fr">gratuit</trd>
  <trd lng="hu">ingyenes</trd>
  <trd lng="nl">gratis</trd>
  <trd lng="no">gratis</trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_l;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="pag.pun0i">
  <kap>pun<tld/>i</kap>
  <snc>
    <ref tip="dif" cel="pag.0i.ricevi_kompensan_malbonon"><tld/>i
    <sncref/></ref>
    <ekz>
      li pun<tld/>as sian nesingardon;
    </ekz>
    <ekz>
      kiel Kristo krucumita pro la kulpoj homaj, tiel Polujo
      dis&scirc;irita pun<tld/>is la pekojn de la &scirc;tatoj
      <fnt><bib>VivZam</bib><lok>1</lok></fnt>.
    </ekz>
    <refgrp tip="sin">
      <ref cel="pag.re0i.ricevi_kompensan_malbonon">re<tld/>i
      <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_p;&c_l;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">b&uuml;&szlig;en</trd>
  <trd lng="fr">expier</trd>
  <trd lng="hu">megb&udblac;nh&odblac;dik</trd>
  <trd lng="nl">boeten</trd>
  <trd lng="no">b&oslash;te</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_l;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;
  <klr>(&c_z;&c_a; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
</drv>

<drv mrk="pag.pun0o">
  <kap>pun<tld/>o</kap>
  <snc>
    <dif>
      Ago a&ubreve; stato de pun<tld/>anto.
    </dif>
    <trd lng="fr">expiation</trd>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  </snc>
  <snc mrk="pag.pun0o.monpuno">
    <uzo tip="stl">RAR</uzo>
    <ref tip="dif" cel="pun.mon0o">monpuno</ref>
    <ekz>
      ili kondamnu lin al pun<tld/>o
      de cent ar&gcirc;entaj moneroj
      <fnt><bib>MT</bib><lok>&Rea; 22:19</lok></fnt>.
    </ekz>
    <trd lng="fr">amende<klr> (paiement)</klr></trd>
    <trd lng="nl">boete</trd>
    <trd lng="no">bot</trd>
    <trd lng="ru">&c_sh;&c_t;&c_r;&c_a;&c_f;</trd>
  </snc>
</drv>
</art>
