    <trd>&c_v;&c_n;&c_je;&c_s;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="pag.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="pag.0ejo.EKON">
    <dif>
      Gi&ccirc;eto, budo, oficejo a&ubreve; ajna loko por <tld/>ado:
      <ekz>
        la buso haltas &ccirc;e <tld/>ejo
        <fnt>
        <aut>M. Lahtinen</aut>,
        <vrk><url ref="http://users.jyu.fi/~mlahtine/Chin2004/TTT/aug10.htm">
        &Ccirc;inio en Transformi&gcirc;o</url></vrk>,
        <lok>2004</lok>
        </fnt>;
      </ekz><ekz>
        kondukantoj <klr>[&#8230;]</klr> alveninte al la <tld/>obudoj,
        tutsimple deklaris: &leftquot;Saluton! Mi ne volas <tld/>i&#x201D; por protesti
        <klr>[&#8230;]</klr>,
        &ccirc;ar la laboristoj de la <tld/>ejo ne apartenas al publika
        a&ubreve;toritato,
        estas nele&gcirc;e, ke ili retenu la veturilon
        <fnt>
          <bib>Monato</bib>
          <aut>Jaume Clotet</aut>
          <vrk><url ref="&Monato;/2012/010745.php">Mi
           ne volas pagi ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kasse</trd>,
      <trd>Schalter <klr>(Kasse)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_s;&c_a; <klr>(&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_p;&c_u;&c_n;&c_k;&c_t; <ind>&c_a;&c_p;&c_l;&c_a;&c_t;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&iacute;sto platby</trd>,
    <trd>pokladna</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>caja<klr> (lugar donde se paga)</klr></trd>,
    <trd>peaje</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caisse<klr> (endroit o&ugrave; on paie)</klr></trd>,
    <trd>p&eacute;age</trd>
  </trdgrp>
  <trd lng="id">kasir</trd>
