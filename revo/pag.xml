</drv>

<drv mrk="pag.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="P"/>i por io (servo, varo, puno ktp):
      <ekz>
        monon por pri<tld/>i partoprenon en la
        seminario, mi, beda&ubreve;rinde, ne havis
        <fnt><bib>Monato</bib></fnt>;
      </ekz><ekz>
        pri<tld/>u la lo&gcirc;ejon, hejtadon, elektron
        <fnt><bib>Metrop</bib></fnt>;
      </ekz><ekz>
        Se l' vivo eble la klopodojn
        Pri<tld/>os per nenia prez' <klr>[&#x2026;]</klr>
        <fnt>
          <aut>S. Pet&odblac;fi tr. K. Kalocsay</aut>
          <vrk><url ref="https://egalite.hu/kalocsay/petofi/la%20poetoj%20de%20la%20xix%20centjaro.htm">La poetoj de la XIX jc</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">bezahlen</trd>
  <trd lng="ru">&c_o;&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="pag.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc mrk="pag.re0i.redoni_0on">
    <dif>
      <tld lit="P"/>i al iu tion, kion li <tld/>is anstata&ubreve; a&ubreve; por alia:
      <ekz>
        mi re<tld/>os al vi la kostojn de la voja&gcirc;o;
      </ekz><ekz>
        li &scirc;uldas al ni monon kaj ne povis tion re<tld/>i
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz><ekz>
        tia ri&ccirc;a&jcirc;o permesus al li re<tld/>i la bienojn, kiujn li donis
        al la pastroj kiel pruntgarantion
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zur&uuml;ckzahlen</trd>,
      <trd>tilgen</trd>
    </trdgrp>
    <trd lng="es">pagar&eacute;</trd>
    <trd lng="fr">rembourser</trd>
    <trdgrp lng="hu">
      <trd>visszafizet</trd>,
      <trd>megt&eacute;r&iacute;t</trd>
    </trdgrp>
    <trd lng="nl">terugbetalen</trd>
    <trd lng="no">betale tilbake</trd>
    <trd lng="ru">&c_o;&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="pag.re0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="pag.0i.FIG"><tld lit="P"/>i<sncref/></ref>:
    </dif>
    <subsnc mrk="pag.re0i.vengxi">
      <dif>
        <ref tip="dif" cel="pag.0i.vengxi"><tld lit="P"/>i<sncref/></ref>:
        <ekz>
          ni mortigis multajn Rusojn, sed ili poste malavare re<tld/>is
          al ni;
        </ekz><ekz>
          re<tld/>u al li la&ubreve; liaj agoj
          <fnt><bib>MT</bib><lok>Ho&scirc;ea 12:2</lok></fnt>
          <fnt>
            <bib>MT</bib>
            <lok>Ijob</lok>
          </fnt>
          <fnt>
            <bib>MT</bib>
            <lok>Jeremia</lok>
          </fnt>;
        </ekz><ekz>
          la faroj de liaj manoj estos re<tld/>itaj al li
          <fnt>
            <bib>MT</bib><lok>Jesaja 3:11</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="de">heimzahlen</trd>
      <trd lng="hu">megfizet <klr>vmi&eacute;rt</klr></trd>
      <trd lng="nl">betaald zetten</trd>
      <trd lng="ru">&c_o;&c_t;&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
    </subsnc>
    <subsnc mrk="pag.re0i.ricevi_kompensan_malbonon">
      <dif>
        <ref tip="dif" cel="pag.0i.ricevi_kompensan_malbonon"><tld lit="P"/>i<sncref/></ref>:
        <ekz>
          morga&ubreve; la homaro devos re<tld/>i la kulpojn <klr>(a&ubreve;:
          pro la kulpoj)</klr> de la nuna facilanima generacio.
        </ekz>
      </dif>
      <refgrp tip="sin">
        <ref cel="pag.pun0i">pun<tld/>i</ref>.
      </refgrp>
      <trd lng="de">daf&uuml;r <ind>bezahlen</ind></trd>
      <trd lng="hu">megb&udblac;nh&odblac;dik</trd>
      <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_l;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;
      <klr>(&c_z;&c_a; &c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
    </subsnc>
    <trd lng="es">devolver<klr> (el mal por el mal, etc.)</klr></trd>
    <trd lng="fr">rendre<klr> (le mal pour le mal etc.)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_p;&c_l;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_p;&c_l;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_l;&c_a;&c_ch;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odplatit</trd>,
    <trd>odpl&aacute;cet</trd>,
    <trd>splatit</trd>,
    <trd>spl&aacute;cet</trd>,
    <trd>vr&aacute;tit <klr>(placen&iacute;m)</klr></trd>,
    <trd>vyrovnat dluh</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odplati&tcaron;</trd>,
    <trd>splati&tcaron;</trd>,
    <trd>vyrovna&tcaron; dlh</trd>
  </trdgrp>
</drv>

