<art mrk="$Id: markot.xml,v 1.22 2022/05/01 12:05:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>markot</rad>/o
</kap>
<drv mrk="markot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="markot.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">HOR</uzo>
    <dif>
      Vegeta&jcirc;<ref tip="super" cel="id.0o.BIO">ido</ref> kreskanta el bran&ccirc;o kaj tu&scirc;anta la teron tie radikanta:
      <ekz>
        tio estas <tld/>o de Mia plantado
        <fnt><bib>MT</bib>, <lok>&Jes; 60:21</lok></fnt>.
      </ekz>
      <ekz>
        kiel bele tie staras la floreto, tio estas <tld/>o de tiu mirtujo, kiun vi
        donacis al mi
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iam la bona onklino havis ian kreska&jcirc;on a&ubreve; kelke da junaj <tld/>oj en akvo,
        kiel ajn malmilda estus la vintro
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi estas la <tld/>o kaj ido de <nom>David</nom>
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 22:16</lok>
        </fnt>;
      </ekz>
    </dif>
    <ref tip="vid" cel="stik.0ajxo">stika&jcirc;o</ref>
    <bld lok="&WCU;/thumb/0/0a/2014-06_Erdbeeren-auf-Dach.jpg/640px-2014-06_Erdbeeren-auf-Dach.jpg" prm="CC BY-SA 4.0">sova&gcirc;a fragujo kun <tld/>oj
        <fnt>
          <aut>Yvonne Bentele, propra verko</aut>
          <lok><url ref="&WCW;/File:2014-06_Erdbeeren-auf-Dach.jpg">&WCW;/File:2014-06_Erdbeeren-auf-Dach.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_a;&c_d;&c_v;&c_o;&c_d;&c_a;&c_k;</trd>
    <trdgrp lng="ca">
      <trd>capficat</trd>,
      <trd>murg&oacute;</trd>
    </trdgrp>
    <trd lng="en">layer</trd>
    <trd lng="fr">marcotte</trd>
    <trdgrp lng="pt">
      <trd>alporque</trd>,
      <trd>mergulhia</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_v;&c_o;&c_d;&c_o;&c_k;</trd>
  </snc>
<!--
  <snc mrk="markot.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ido, idaro:
      <ekz>
        eliros <tld/>o el la trunko  de <nom>Ji&scirc;aj</nom>
        <fnt><bib>MT</bib>, <lok>&Jes; 11:1</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">rejeton</trd>
  </snc>
-->
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_s;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_a;&c_t;&c_o;&c_zh;&c_y;&c_l;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>h&rcaron;&iacute;&zcaron;enec</trd>,
    <trd>zako&rcaron;en&ecaron;n&aacute; odno&zcaron;</trd>,
    <trd>&scaron;lahoun</trd>
  </trdgrp>
  <trd lng="de">Ableger</trd>
  <trd lng="hu">bujtv&aacute;ny</trd>
  <trdgrp lng="nl">
    <trd>aflegger</trd>,
    <trd>loot</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szczep <klr>(ro&sacute;l.)</klr></trd>,
    <trd>odk&lstroke;ad</trd>,
    <trd>p&eogonek;d</trd>
  </trdgrp>
  <trd lng="pt">descend&ecirc;ncia</trd>
  <trd lng="ro">tulpina <klr>(planta)</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_b;&c_je;&c_g; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  <trdgrp lng="sk">
    <trd>odno&zcaron;</trd>,
    <trd>v&yacute;honok</trd>,
    <trd>&scaron;&lcaron;ah&uacute;&ncaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x434;&#x441;&#x430;&#x434;&#x43E;&#x43A;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x433;&#x456;&#x43B;&#x43E;&#x43A;</trd>,
    <trd>&#x43F;&#x430;&#x433;&#x456;&#x43D;</trd>,
    <trd>&#x441;&#x442;&#x43E;&#x43B;&#x43E;&#x43D;</trd>,
    <trd>&#x43F;&#x456;&#x434;&#x437;&#x435;&#x43C;&#x43D;&#x438;&#x439; &#x43F;&#x430;&#x433;&#x456;&#x43D;</trd>
  </trdgrp>
</drv>

<drv mrk="markot.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="markot.0i.HOR">
    <dif>
      Reprodukti per <tld/>oj:
      <ekz>
        <tld/>i dianton;
      </ekz>
      <ekz>
        <tld/>i rozujon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="greft.0i.BOT">grefti<sncref/></ref>,
      <ref cel="stik.0i">stiki</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>capficar</trd>,
    <trd>murgonar</trd>
  </trdgrp>
  <trd lng="cs">mno&zcaron;it h&rcaron;&iacute;&zcaron;en&iacute;m</trd>
  <trd lng="en">layer</trd>
  <trd lng="fr">marcotter</trd>
  <trdgrp lng="hu">
    <trd>bujt</trd>,
    <trd>bujt&aacute;ssal szapor&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">afleggen</trd>
  <trd lng="pl">ablegrowa&cacute;</trd>     
  <trd lng="ro">strat</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; <ind>&c_o;&c_t;&c_v;&c_o;&c_d;&c_k;&c_a;&c_m;&c_i;</ind></trd>
  <trd lng="sk">mno&zcaron;i&tcaron; odno&zcaron;ovan&iacute;m</trd>
  <trd lng="uk">&#x440;&#x43E;&#x437;&#x43C;&#x43D;&#x43E;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438; &#x432;&#x456;&#x434;&#x441;&#x430;&#x434;&#x43A;&#x430;&#x43C;&#x438;</trd>
</drv>
</art>
