<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: markot.xml,v 1.19 2020/07/16 20:11:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>markot</rad>/o
</kap>
<drv mrk="markot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="markot.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">HOR</uzo>
    <dif>
      Vegeta&jcirc;ido kreskanta el bran&ccirc;o tu&scirc;anta la teron kaj tie radikanta:
      <ekz>
        tio estas <tld/>o de Mia plantado
        <fnt><bib>MT</bib>, <lok>&Jes; 60:21</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_d;&c_v;&c_o;&c_d;&c_a;&c_k;</trd>
    <trdgrp lng="ca">
      <trd>capficat</trd>,
      <trd>murg&oacute;</trd>
    </trdgrp>
    <trd lng="en">layer</trd>
    <trd lng="fr">marcotte</trd>
    <trdgrp lng="pt">
      <trd>alporque</trd>,
      <trd>mergulhia</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_v;&c_o;&c_d;&c_o;&c_k;</trd>
  </snc>
  <snc mrk="markot.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ido, idaro:
      <ekz>
        eliros <tld/>o el la trunko  de Ji&scirc;aj
        <fnt><bib>MT</bib>, <lok>&Jes; 11:1</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">rejeton</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_s;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_a;&c_t;&c_o;&c_zh;&c_y;&c_l;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>h&rcaron;&iacute;&zcaron;enec</trd>,
    <trd>zako&rcaron;en&ecaron;n&aacute; odno&zcaron;</trd>,
    <trd>&scaron;lahoun</trd>
  </trdgrp>
  <trd lng="de">Ableger</trd>
  <trd lng="hu">bujtv&aacute;ny</trd>
  <trdgrp lng="nl">
    <trd>aflegger</trd>,
    <trd>loot</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szczep <klr>(ro&sacute;l.)</klr></trd>,
    <trd>odk&lstroke;ad</trd>,
    <trd>p&eogonek;d</trd>
  </trdgrp>
  <trd lng="pt">descend&ecirc;ncia</trd>
  <trd lng="ro">tulpina <klr>(planta)</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_b;&c_je;&c_g; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  <trdgrp lng="sk">
    <trd>odno&zcaron;</trd>,
    <trd>v&yacute;honok</trd>,
    <trd>&scaron;&lcaron;ah&uacute;&ncaron;</trd>
  </trdgrp>
</drv>

<drv mrk="markot.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="markot.0i.HOR">
  <dif>
    Fari <tld/>on, ku&scirc;igante bran&ccirc;on en tero:
    <ekz>
      <tld/>i dianton;
    </ekz>
    <ekz>
      <tld/>i rozujon.
    </ekz>
  </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>capficar</trd>,
    <trd>murgonar</trd>
  </trdgrp>
  <trd lng="cs">mno&zcaron;it h&rcaron;&iacute;&zcaron;en&iacute;m</trd>
  <trd lng="en">layer</trd>
  <trd lng="fr">marcotter</trd>
  <trdgrp lng="hu">
    <trd>bujt</trd>,
    <trd>bujt&aacute;ssal szapor&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">afleggen</trd>
  <trd lng="pl">ablegrowa&cacute;</trd>     
  <trd lng="ro">strat</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; <ind>&c_o;&c_t;&c_v;&c_o;&c_d;&c_k;&c_a;&c_m;&c_i;</ind></trd>
  <trd lng="sk">mno&zcaron;i&tcaron; odno&zcaron;ovan&iacute;m</trd>
</drv>
</art>
