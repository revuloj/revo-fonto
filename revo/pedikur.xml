<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pedikur.xml,v 1.6 2021/04/16 11:50:49 revo Exp $">
<kap>
  <rad>pedikur</rad>/i 
</kap>

<drv mrk="pedikur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pedikur.0i.KOS">
    <dif>
      Piedflegi:
      <ekz>
        mi ne kredas, ke tia birdo <tld/>us siajn ungojn sola.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="manikur.0i.KOS">manikuri</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_p;&c_e;&c_d;&c_y;&c_k;&c_ju;&c_r;</ind></trd>
  <trd lng="ca">fer la pedicura</trd>
  <trd lng="fr">p&eacute;dicurer</trd>
</drv>

<drv mrk="pedikur.0o">
  <kap><tld/>o</kap>
  <snc mrk="pedikur.0o.KOS">
  <uzo tip="fak">MED</uzo>
    <dif>
      Piedflego.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_d;&c_y;&c_k;&c_ju;&c_r;</trd>
  <trd lng="ca">pedicura</trd>
  <trdgrp lng="cs">
    <trd>pedik&uacute;ra</trd>,
    <trd>p&ecaron;st&ecaron;n&iacute; nohou</trd>
  </trdgrp>
  <trd lng="fr">p&eacute;dicurie</trd>
  <trdgrp lng="sk">
    <trd>o&scaron;etrenie n&ocirc;h</trd>,
    <trd>pedik&uacute;ra</trd>
  </trdgrp>
</drv>

<drv mrk="pedikur.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pedikur.0isto.KOS">
    <dif>
      Piedflegisto.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_j;&c_s;&c_t;&c_a;&c_r; <ind>&c_p;&c_e;&c_d;&c_y;&c_k;&c_ju;&c_r;&c_u;</ind></trd>
  <trdgrp lng="ca">
    <trd>pedicur</trd>,
    <trd>callista</trd>
  </trdgrp>
  <trd lng="cs">pedik&eacute;r</trd>
  <trd lng="fr">p&eacute;dicure</trd>
  <trd lng="sk">pedik&eacute;r</trd>
</drv>
</art>


