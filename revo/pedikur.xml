<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pedikur.xml,v 1.12 2024/02/17 06:43:05 revo Exp $">
<kap>
  <rad>pedikur</rad>/i
</kap>

<drv mrk="pedikur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pedikur.0i.KOS">
    <dif>
      <ref tip="super" cel="fleg.0i.KOMUNE">Flegi</ref> ha&ubreve;ton kaj ungojn de la piedoj:
      <ekz>
        mi ne kredas, ke tia birdo <tld/>us siajn ungojn sola.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="manikur.0i.KOS">manikuri</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
  <ind>&c_p;&c_e;&c_d;&c_y;&c_k;&c_ju;&c_r;</ind></trd>
  <trd lng="ca">fer la pedicura</trd>
  <trd lng="de">die F&uuml;&szlig;e <ind>pflegen</ind></trd>
  <trdgrp lng="en">
    <trd>pedicure</trd>,
    <trd>give a pedicure</trd>
  </trdgrp>
  <trd lng="fr">p&eacute;dicurer</trd>
  <trd lng="pl">robi&cacute;  <ind>pedicure</ind></trd>
  <trd lng="ro">face o <ind>pedichiur&abreve;</ind></trd>
  <trd lng="uk">&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_p;&c_je;&c_d;&c_i;&c_k;&c_ju;&c_r; </trd>
  <trdgrp lng="zh">
    <trd>&#x4FEE;&#x811A; <pr>xi&#x16B;ji&#x1CE;o</pr></trd>,
    <trd>&#x4FEE;&#x8173; <pr>xi&#x16B;ji&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pedikur.0o">
  <kap><tld/>o</kap>
  <snc mrk="pedikur.0o.KOS">
  <uzo tip="fak">MED</uzo>
    <dif>
      <ke>Piedflego</ke>:
      <ekz>
        fi&scirc;a <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=266422">Kuracistofi&scirc;o</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kadre de manikuro kaj <tld/>o oni kutime &scirc;miras sur la purigitajn ungojn tri a&ubreve;
        kvar tavolojn da lako: unu bazan, unu a&ubreve; du &ccirc;efajn kaj unu tegan
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=31411">Ungolako</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_d;&c_y;&c_k;&c_ju;&c_r;</trd>
  <trd lng="ca">pedicura</trd>
  <trdgrp lng="cs">
    <trd>pedik&uacute;ra</trd>,
    <trd>p&ecaron;st&ecaron;n&iacute; nohou</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Fu&szlig;pflege</trd>,
    <trd>Pedik&uuml;re</trd>
  </trdgrp>
  <trd lng="en">pedicure</trd>
  <trd lng="fr">p&eacute;dicurie</trd>
  <trd lng="pl">pedicure</trd>
  <trd lng="ro">pedichiur&abreve;</trd>
  <trdgrp lng="sk">
    <trd>o&scaron;etrenie n&ocirc;h</trd>,
    <trd>pedik&uacute;ra</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_je;&c_d;&c_i;&c_k;&c_ju;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x4FEE;&#x811A; <pr>xi&#x16B;ji&#x1CE;o</pr></trd>,
    <trd>&#x4FEE;&#x8173; <pr>xi&#x16B;ji&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pedikur.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pedikur.0isto.KOS">
    <dif>
      <ke>Piedflegisto</ke>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_j;&c_s;&c_t;&c_a;&c_r;
  <ind>&c_p;&c_e;&c_d;&c_y;&c_k;&c_ju;&c_r;&c_u;</ind></trd>
  <trdgrp lng="ca">
    <trd>pedicur</trd>,
    <trd>callista</trd>
  </trdgrp>
  <trd lng="cs">pedik&eacute;r</trd>
  <trd lng="de">Fu&szlig;pfleger</trd>
  <trd lng="en">pedicurist</trd>
  <trd lng="fr">p&eacute;dicure</trd>
  <trd lng="pl">pedikiurzysta</trd>
  <trd lng="ro">pedichiurist</trd>
  <trd lng="sk">pedik&eacute;r</trd>
  <trdgrp lng="zh">
    <trd>&#x4FEE;&#x811A;&#x5E08; <pr>xi&#x16B;ji&#x1CE;osh&#x12B;</pr></trd>,
    <trd>&#x4FEE;&#x8173;&#x5E2B; <pr>xi&#x16B;ji&#x1CE;osh&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>

