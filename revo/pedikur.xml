<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pedikur</rad>/i 
</kap>

<drv mrk="pedikur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pedikur.0i.KOS">
    <dif>
      Piedflegi:
      <ekz>
        mi ne kredas, ke tia birdo <tld/>us siajn ungojn sola.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="manikur.0i.KOS">manikuri</ref>
    </refgrp>
  </snc>
  <trd lng="ca">fer la pedicura</trd>
</drv>

<drv mrk="pedikur.0o">
  <kap><tld/>o</kap>
  <snc mrk="pedikur.0o.KOS">
    <dif>
      Piedflego.
    </dif>
  </snc>
  <trd lng="ca">pedicura</trd>
  <trdgrp lng="sk">
    <trd>ošetrenie nôh</trd>,
    <trd>pedikúra</trd>
  </trdgrp>
</drv>

<drv mrk="pedikur.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="pedikur.0isto.KOS">
    <dif>
      Piedflegisto.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>pedicur</trd>,
    <trd>callista</trd>
  </trdgrp>
  <trd lng="sk">pedikér</trd>
</drv>
</art>


