<drv mrk="miks.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="miks.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>ita a&ubreve; sin <tld/>i:
      <ekz>
        odoro de fumo <tld/>i&gcirc;is kun parfumo de rozoj;
      </ekz>
      <ekz>
        rido <tld/>i&gcirc;is kun larmoj;
      </ekz>
      <ekz>
        via vino <tld/>i&gcirc;is kun akvo.
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 1:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        sonoj de trumpetoj <tld/>i&gcirc;is kun bruo de malproksimaj tondroj
        <fnt>
          <bib>Far1</bib>
          <lok>&ccirc;apitro 20a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la koloroj de la belaj sagoj tute inter<tld/>i&gcirc;is pro la malseka
        vetero
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malbonodoro de brando <tld/>i&gcirc;as kun la malbona fumo de
        tabako
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj haroj <tld/>i&gcirc;is kun la haregoj de la barbaro
        <fnt>
          <bib>Far2</bib>
          <lok>&ccirc;apitro 8a</lok>
        </fnt>;
      </ekz>
      <ekz>
        violon&ccirc;elo ludis harmonie grandan verkon, kies tonoj plenigis
        la ombrojn de la parko, <tld/>i&gcirc;is kun la malla&ubreve;ta murmuro de la
        arboj
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fand">kunfandi&gcirc;i</ref>,
      <ref cel="konfuz">konfuzi&gcirc;i</ref>,
      <ref cel="plekt">interplektigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_m;&c_je;&c_sh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_m;&c_je;&c_sh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="br">en em veska&ntilde;</trd>
  <trd lng="de">sich <ind>vermischen</ind></trd>
  <trd lng="fr">se <ind>m&eacute;langer</ind></trd>
  <trd lng="hu">keveredik</trd>
  <trdgrp lng="id">
