<drv mrk="miks.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="miks.0ajxo.KOMUNE">
    <dif>
      Io konsistanta el <tld/>itaj aferoj:
      <ekz>
        <tld/>a&jcirc;o el kuiritaj fruktoj;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o de ruzemo kun naiveco;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o de instruoj kaj legendoj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        plej strangaj <tld/>a&jcirc;oj el birdoj, floroj kaj spirala&jcirc;oj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o de deziro, sopiro kaj &jcirc;aluzo
        <fnt>
          <bib>Far2</bib>
          <lok>&ccirc;apitro 7a</lok>
        </fnt>;
      </ekz>
      <ekz>
        eksplodis polvokarba <tld/>a&jcirc;o
        <fnt><aut>Araneo</aut>,
        <vrk><url ref="&Monato;/2000/006949.php">Subtera katastrofo</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/10, p. 12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_m;&c_je;&c_s;&c_mol;</trd>
  <trd lng="br">meskad</trd>
  <trd lng="de">Gemisch</trd>
  <trd lng="es">mezcla</trd>
