<drv mrk="miks.0a">
  <kap><tld/>a</kap>
  <snc mrk="miks.0a.miksita">
    <dif>
      <tld lit="M"/>ita:
      <ekz>
        la societo estas <tld/>a, &ccirc;ar re&gcirc;o kaj almozulo, geniulo kaj
        kreteno, sidas apude
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas nur kelkaj pursangaj &ccirc;evalrasoj, inter kiuj &leftquot;Pursanga
        arabo&rightquot; kaj &leftquot;Pursanga anglo&rightquot;, &leftquot;Angl-arabo&rightquot; estas fakte <tld/>a raso
        inter tiuj du
        <fnt>
       <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2000/006864.php">&Ccirc;evalrasoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_m;&c_ja;&c_sh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_m;&c_ja;&c_sh;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="br">mesket</trd>
  <trdgrp lng="cs">
    <trd>halucinace sluchov&eacute;</trd>,
    <trd>sm&iacute;&scaron;en&eacute;</trd>,
    <trd>t&ecaron;lesn&eacute;</trd>,
    <trd>zrakov&eacute;</trd>,
    <trd>&ccaron;ichov&eacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>vermischt</trd>,
      <trd>vermengt</trd>,
      <trd>gemischt</trd>
    </trdgrp>
  <trd lng="es">mezclado<klr tip="amb"> (adjetivo)</klr></trd>
  <trd lng="fr">m&eacute;lang&eacute;</trd>
  <trdgrp lng="hu">
