<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: filtr.xml,v 1.21 2023/08/31 13:11:29 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>filtr</rad>/i
</kap>
<drv mrk="filtr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Pasigi tra penetrebla tavolo flua&jcirc;on por &gcirc;in purigi,
      kaj eligi la restantajn solidajn pulvorecajn partojn, fe&ccirc;ojn ktp.
    </dif>
    <refgrp tip="vid">
      <ref cel="kribr.0i">kribri</ref>.
    </refgrp>
    <trd lng="de">filtern</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_c;&c_je;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="fak">FOT</uzo>
    <dif>
      Traigi la lumon tra ekrano por haltigi parton de la kolorradioj.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>cedit</trd>,
    <trd>filtrovat</trd>,
    <trd>pros&#xE1;knout</trd>,
    <trd>protla&#x10D;it tekutinu</trd>
  </trdgrp>
  <trd lng="es">filtrar</trd>
  <trd lng="fr">filtrer<klr tip="ind"> (tr.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x3053;&#x3059;</trd>,
    <trd>&#x308D;&#x904E;&#x3059;&#x308B;  [<ind>&#x308D;&#x304B;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x30D5;&#x30A3;&#x30EB;&#x30BF;&#x30FC;&#x306B;&#x304B;&#x3051;&#x308B;  [<ind>&#x3075;&#x3043;&#x308B;&#x305F;&#x30FC;&#x306B;&#x304B;&#x3051;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">filteren</trd>
  <trd lng="pt">filtrar</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_l;&c_mol;&c_t;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_f;&c_i;&c_l;&c_mol;&c_t;&c_r;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>cedi&#x165;</trd>,
    <trd>filtrova&#x165;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x444;&#x456;&#x43B;&#x44C;&#x442;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x446;&#x456;&#x434;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x446;&#x456;&#x434;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="filtr.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="filtr.0ilo.KOMUNE">
    <dif>
      Aparato por <tld/>i:
      <ekz>
        papera <tld/>ilo;
      </ekz>
      <ekz>
        Pasteur'a <tld/>ilo;
      </ekz>
      <ekz>
        <tld/>ilo por kafo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="filtr.0ilo.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>EKV</bib><lok>&para;131,
    &leftquot;filtro&rightquot;</lok></fnt>
    <dif>
      <klr>(super aro <frm><g>E</g></frm>)</klr>
      Tia nemalplena <ref tip="super" cel="ar.0o.MAT">aro</ref> de
      subaroj de <frm><g>E</g></frm>, ke:
      (1) la malplena aro ne apartenas al &gcirc;i;
      (2) &ccirc;iu superaro de iu ajn el &gcirc;iaj elementoj apartenas
      al &gcirc;i;
      (3) &ccirc;iu <ref cel="komun.0ajxo.aro">komuna&jcirc;o</ref> de finia
      nombro da &gcirc;iaj elementoj apartenas al &gcirc;i:
      <ekz>
        la aro de &ccirc;iuj
        <ref cel="cxirka.0ajxo.topologio">&ccirc;irka&ubreve;a&jcirc;oj</ref>
        de donita punkto de topologia spaco estas <tld/>ilo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fajn.pli0a.aro_de_subaroj">pli fajna</ref>,
      <ref cel="konver.0a.filtrilo">konver&gcirc;a</ref>.
    </refgrp>
    <trd lng="de">Filter</trd>
    <trd lng="en">filter</trd>
  </snc>
  <trd lng="cs">filtr <klr>(i program)</klr></trd>
  <trd lng="de">Filter</trd>
  <trd lng="es">filtro</trd>
  <trd lng="fr">filtre</trd>
  <trdgrp lng="ja">
    <trd>&#x308D;&#x6750;  [<ind>&#x308D;&#x3056;&#x3044;</ind>]</trd>,
    <trd>&#x30D5;&#x30A3;&#x30EB;&#x30BF;&#x30FC;</trd>,
    <trd>&#x308D;&#x904E;&#x5668;  [<ind>&#x308D;&#x304B;&#x304D;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">filteren</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_mol;&c_t;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>cedidlo</trd>,
    <trd>filter</trd>
  </trdgrp>
  <trd lng="uk">&#x444;&#x456;&#x43B;&#x44C;&#x442;&#x440;</trd>
</drv>

<drv mrk="filtr.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Klari&gcirc;i a&ubreve; puri&gcirc;i per <tld/>ado.
    </dif>
    <refgrp tip="vid">
      <ref cel="flu.tra0eti">traflueti</ref>.
    </refgrp>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_f;&c_i;&c_l;&c_mol;&c_t;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Diseri&gcirc;e trairi kvaza&ubreve; tra <tld/>ilo:
      <ekz>
        lumo <tld/>i&gcirc;as tra kurtenoj de fenestro;
      </ekz>
      <ekz>
        flustra interparolado <tld/>i&gcirc;is tra la fermita pordo.
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_o;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <trd lng="fr">filtrer<klr tip="ind"> (intr.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x308D;&#x904E;&#x3059;&#x308B;  [<ind>&#x308D;&#x304B;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x3057;&#x307F;&#x51FA;&#x308B;  [<ind>&#x3057;&#x307F;&#x3067;&#x308B;</ind>]</trd>,
    <trd>&#x6F0F;&#x308C;&#x308B;  [<ind>&#x3082;&#x308C;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x440;&#x43E;&#x441;&#x43E;&#x447;&#x443;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x446;&#x456;&#x434;&#x438;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x446;&#x456;&#x434;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x444;&#x456;&#x43B;&#x44C;&#x442;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
