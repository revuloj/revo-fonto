<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>filtr</rad>/i
</kap>
<drv mrk="filtr.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Pasigi tra penetrebla tavolo flua&jcirc;on por &gcirc;in purigi,
      k eligi la restantajn solidajn pulvorecajn partojn, fe&ccirc;ojn ktp. 
    </dif>
    <refgrp tip="vid">
      <ref cel="kribr.0i">kribri</ref>.
    </refgrp>
    <trd lng="de">filtern</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_c;&c_je;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Traigi la lumon tra ekrano por haltigi parton de la kolorradioj. 
    </dif>
  </snc>
  <trd lng="es">filtrar</trd>
  <trd lng="fr">filtrer<klr tip="ind"> (tr.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_l;&c_mol;&c_t;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_f;&c_i;&c_l;&c_mol;&c_t;&c_r;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="filtr.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Aparato por <tld/>i:
      <ekz>papera <tld/>ilo;</ekz>
      <ekz>Pasteur'a <tld/>ilo;</ekz>
      <ekz><tld/>ilo por kafo.</ekz>
    </dif>
  </snc>
  <trd lng="de">Filter</trd>
  <trd lng="es">filtro</trd>
  <trd lng="fr">filtre</trd>
  <trd lng="ru">&c_f;&c_i;&c_l;&c_mol;&c_t;&c_r;</trd>
</drv>
<drv mrk="filtr.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc num="1">
    <dif>
      Klari&gcirc;i a&ubreve; puri&gcirc;i per <tld/>ado.
    </dif>
    <refgrp tip="vid">
      <ref cel="flu.tra0eti">traflueti</ref>.
    </refgrp>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_f;&c_i;&c_l;&c_mol;&c_t;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Diseri&gcirc;e trairi kvaza&ubreve; tra <tld/>ilo:
      <ekz>lumo <tld/>i&gcirc;as tra kurtenoj de fenestro;</ekz>
      <ekz>flustra interparolado <tld/>i&gcirc;is tra la fermita pordo.</ekz>
    </dif>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_o;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <trd lng="fr">filtrer<klr tip="ind"> (intr.)</klr></trd>
</drv>
</art>
