<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id: kampan3.xml,v 1.13 2018/11/18 19:10:18 revo Exp $">
<kap><rad>kampanulac</rad>/oj</kap>
<drv mrk="kampan3.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Campanulaceae</trd>):
      <ekz>
        la <tld/>oj <klr>[&#8230;]</klr> estas preska&ubreve; tutmonde disvastigitaj, kelkaj specioj
      kaj kulturvarioj estas uzataj kiel ornamplantoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=512007"
              >Kampanulacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
       <ref cel="kampan2.0o">kampanulo</ref>,
       <ref cel="lobeli.0o">lobelio</ref>
    </refgrp>
  </snc>
  <trd lng="ca">campanul&agrave;cies</trd>
  <trd lng="cs">zvonkovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Glockenblumengew&auml;chse</trd>
  <trdgrp lng="en">
    <trd>Campanulaceae</trd>,
    <trd>bellflower family</trd>
  </trdgrp>
  <trd lng="fr">campanulac&eacute;es</trd>
  <trd lng="hu">harangvir&aacute;gf&eacute;l&eacute;k</trd>
  <trd lng="nl">klokjesfamilie</trd>
  <trd lng="pl">dzwonkowate</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">zvonkovit&eacute;</trd>
</drv>
</art>

