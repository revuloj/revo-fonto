<art mrk="$Id: indr.xml,v 1.9 2022/10/24 15:42:58 revo Exp $">
<kap>
  <rad>Indr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="indr.0o">
  <kap><tld/>o</kap>
  <snc mrk="indr.0o.HIN">
    <uzo tip="fak">HIN</uzo>
    <dif>
      Dio de &ccirc;ielo kaj fulmotondro, &ccirc;efdio en antikva hinduismo: 
      <ekz>
        &ccirc;e alia muro videblas plur-viza&gcirc;a <tld/>o sidanta sur dorso de
        aglo
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;1/2004/07/05/1@11766.htm">
          Monda mirinda&jcirc;o &scirc;tonkavernaro Yungang</url></vrk>,
          <lok>2004-07-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ib;&c_n;&c_d;&c_r;&c_a; <klr tip="amb">(&c_ib;&c_n;&c_d;&c_y;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_b;&c_a;&c_g;&c_ib;&c_n;&c_ja;)</klr></trd>
  <trd lng="cs">Indra</trd>
  <trd lng="de">Indra</trd>
  <trd lng="es">Indra</trd>
  <trd lng="fr">Indra</trd>
  <trd lng="it">Indra</trd>
  <trd lng="pt">Indra</trd>
  <trd lng="sk">Indra</trd>
  <trd lng="uk">&c_Ib;&#x43D;&c_d;&c_r;&c_a;</trd>
</drv>
</art>

