<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: step.xml,v 1.24 2024/03/25 07:28:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>step</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="step.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="step.0o.ebena">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Senarbara regiono kun herbeca vegeta&jcirc;aro en moderaj kaj
      subtropikaj klimatzonoj de la norda kaj suda duongloboj;
      en E&ubreve;razio la Granda <tld lit="S"/>o etendi&gcirc;as
      latitude, de Mongolujo &gcirc;is Hungarujo; en la Amerikoj,
      meridiane, suden de Montano kaj norden ekde Argentino
      (<refgrp tip="sub">
        <ref cel="preri.0o">prerio</ref>,
        <ref cel="pamp.0o">pampo</ref>
      </refgrp>); anka&ubreve; en Orienta Azio kaj A&ubreve;stralio:
      <ekz>
        &ccirc;i tie la sova&gcirc;aj &ccirc;evaloj galopas tra la
        vastaj <tld/>oj, kiel ni tion vidis en nia infaneco
        <fnt><bib>Fab3</bib>, <lok>Cigna nesto</lok></fnt>;
      </ekz>
      <ekz>
        Moseo supreniris de la <tld/>o de Moab sur la monton
        Nebo, sur la supron de Pisga, kiu estas kontra&ubreve;
        Jeri&hcirc;o
        <fnt><bib>MT</bib>, <lok>&Rea; 34:1.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="step.0o.montara">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Simila altozono en montaro.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_p;</trd>
  <trd lng="bg">&c_s;&c_t;&c_je;&c_p;</trd>
  <trd lng="cs">step <klr>(porost)</klr></trd>
  <trd lng="de">Steppe</trd>
  <trd lng="en">steppe</trd>
  <trd lng="es">estepa</trd>
  <trd lng="fr">steppe</trd>
  <trdgrp lng="hu">
    <trd>sztyepp</trd>,
    <trd>f&uuml;ves <ind>puszta</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x30B9;&#x30C6;&#x30C3;&#x30D7;</trd>
  <trd lng="nl">steppe</trd>
  <trd lng="pl">step</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_p;&c_mol;</trd>
  <trd lng="sk">step</trd>
  <trdgrp lng="sv">
    <trd>st&auml;pp</trd>,
    <trd>hed</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x442;&#x435;&#x43F;</trd>
  <trdgrp lng="zh">
    <trd>&#x8349;&#x539F; <pr>c&#x1CE;oyu&#xE1;n</pr></trd>,
    <trd>&#x6C99;&#x5730; <pr>sh&#x101;de</pr></trd>
  </trdgrp>
</drv>

<drv mrk="step.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="step.0ano.GEOG">
    <dif>
      Lo&gcirc;anto de <tld/>o.
    </dif>
    <ref tip="vid" cel="nomad.0o">nomado</ref>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_p;&c_n;&c_ja;&c_k;</trd>
  <trd lng="bg">&c_s;&c_t;&c_je;&c_p;&c_je;&c_n;
  &c_o;&c_b;&c_i;&c_t;&c_a;&c_t;&c_je;&c_l;</trd>
  <trd lng="de">Steppenbewohner</trd>
  <trd lng="hu">sztyeppelak&oacute;</trd>
  <trd lng="pl">mieszkaniec <ind>stepu</ind></trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_p;&c_n;&c_ja;&c_k;</trd>
</drv>

<drv mrk="step.arbar0o">
  <kap>arbar<tld/>o</kap>
  <snc mrk="step.arbar0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Regiono en kiu <tld/>o alternas kun arbaro.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_s;&c_a;&c_s;&c_t;&c_e;&c_p;</trd>
  <trd lng="cs">lesostep</trd>
  <trd lng="de">Waldsteppe</trd>
  <trd lng="en">wooded steppe</trd>
  <trd lng="fr">steppe arbor&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>erd&odblac;s <ind>sztyepp</ind></trd>,
    <trd>erd&odblac;s <ind>puszta</ind></trd>
  </trdgrp>
  <trd lng="pl">lasostep</trd>
  <trd lng="ru">&c_l;&c_je;&c_s;&c_o;&c_s;&c_t;&c_je;&c_p;&c_mol;</trd>
  <trd lng="sk">lesostep</trd>
</drv>

</art>
