<drv mrk="krad.lat0o">
  <kap>lat<tld/>o</kap>
  <snc mrk="krad.lat0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      <tld lit="K"/>a <ref cel="lat.0aro">lataro</ref> uzata en &gcirc;ardenoj por surkreskigi
      lianojn, a&ubreve; kiel ornama&jcirc;o:
      <ekz>
        oni kondukas <nom>Kandid</nom> en la&ubreve;bon <klr>[&#x2026;]</klr> el lat<tld/>oj, kiuj entenis
        papagojn, kolibrojn, numidojn
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        gvatis de malanta&ubreve; sia kukuma lat<tld/>o
        <fnt>
          <vrk><url ref="http://literaturo.esperanto.net/bk/verk/brokant.html">Belartaj Konkursoj:
          Brokante</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lauxb.0o">la&ubreve;bo</ref>,
      <ref cel="pergol.0o">pergolo</ref>,
      <ref cel="spalir.0o">spaliro</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Kletterhilfe <klr>(Garten)</klr></trd>,
      <trd>Rankger&uuml;st</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_sh;&c_e;&c_c;&c_ib;&c_s;&c_t;&c_a;&c_ja; &c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_a;</trd>
  <trd lng="cs">&scaron;pal&iacute;r</trd>
  <trd lng="en">trellis</trd>
  <trdgrp lng="fr">
    <trd>treillage</trd>,
    <trd>treillis<klr> (treillage)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>l&eacute;cr&aacute;cs</trd>,
    <trd>lugasr&aacute;cs</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kratownica</trd>,
    <trd>treja&zdot;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>grind&abreve; cu z&abreve;brele</trd>,
    <trd>spaliere</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_jo;&c_t;&c_ch;&c_a;&c_t;&c_a;&c_ja; &c_z;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_k;&c_a;</trd>
  <trd lng="sk">&scaron;palier</trd>
</drv>

