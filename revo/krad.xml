        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="bar.cxirkaux0i.fermi">&ccirc;irka&ubreve;bari<sncref/></ref>
  </snc>
  <trdgrp lng="de">
    <trd>mit einem Gitter <ind>umgeben</ind></trd>,
    <trd>umz&auml;unen <klr>(Gitter)</klr></trd>
  </trdgrp>
  <trd lng="fr">grillager</trd>
  <trd lng="pl">ogrodzi&cacute; krat&aogonek;</trd>     
  <trd lng="ro">gard z&abreve;brele</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_b;&c_g;&c_o;&c_r;&c_o;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i; &c_r;&c_je;&c_sh;&c_ib;&c_t;&c_k;&c_o;&c_ju;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_h;&c_je;&c_t;&c_a;&c_m;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x56F4;&#x8D77; <pr>w&#xE9;iq&#x1D0;</pr></trd>
</drv>

