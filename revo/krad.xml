    <trd>kock&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x683C;&#x5B50;&#x306E;  <pr>&#x3053;&#x3046;&#x3057;&#x306E;</pr></trd>,
    <trd>&#x683C;&#x5B50;&#x306E;&#x4ED8;&#x3044;&#x305F;  <pr>&#x3053;&#x3046;&#x3057;&#x306E;&#x3064;&#x3044;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kratowy</trd>,
    <trd>okratowany</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>z&abreve;brele</trd>,
    <trd>barat</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_jo;&c_t;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>mre&zcaron;ovit&yacute;</trd>,
    <trd>mrie&zcaron;kovit&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_gu;&c_r;&c_a;&c_t;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_sh;&c_ib;&c_t;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_ib;&c_t;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_sh;&c_t;&c_a;&c_h;&c_je;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_s;&c_n;&c_i;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="krad.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Objekto konsistanta el <tld/>o, provizita per <tld/>o:
      <ekz>
        &ccirc;irka&ubreve; la krono estis <tld/>a&jcirc;o kaj granatoj, &ccirc;io el kupro
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 52:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        post la blanke farbita ligna <tld/>a&jcirc;o <klr>[&#x2026;]</klr> staris rozarbeto
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lokon oni &ccirc;irka&ubreve;baris per ora <tld/>a&jcirc;o, kaj tage kaj nokte apud &gcirc;i staris
        gardostaranto
        <fnt>
          <bib>Fab3</bib>
          <lok>Folio el la &ccirc;ielo</lok>
        </fnt>;
      </ekz>
      <ekz>
        eleganta kameneto, &ccirc;irka&ubreve;ita de fera, arte aran&gcirc;ita <tld/>a&jcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Verhau</trd>,
    <trd>Dratgeflecht</trd>
  </trdgrp>
  <trd lng="ja">&#x683C;&#x5B50;&#x7D44;  <pr>&#x3053;&#x3046;&#x3057;&#x304F;&#x307F;</pr></trd>
  <trd lng="zh">&#x9435;&#x7D72;&#x7DB2; <pr>ti&#x11B;s&#x12B;w&#x1CE;ng</pr></trd>
</drv>

<drv mrk="krad.0ita">
  <kap><tld/>ita</kap>
  <snc mrk="krad.0ita.kun_grado">
    <refgrp tip="vid">
       <ref cel="krad.0a">krada</ref>,
       <ref cel="krad.0i">kradi</ref>
    </refgrp>
  </snc>
  <snc mrk="krad.0ita.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ornamita per <tld/>forme interkruci&gcirc;antaj linioj:
      <ekz>
        silkaj vestoj <klr>[de kiuj]</klr> unu flanko estis &ccirc;ielarke <ref cel="stri.0i">strii</ref>ta kaj la dua
        mikskolore <tld/>ita
        <fnt>
          <bib>RBa</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kvadra1.0umi0i">kvadratita</ref>
    <ref tip="sin" cel="plejd.0o">plejda</ref>
    <trd lng="cs">zam&rcaron;&iacute;&zcaron;ovan&yacute;</trd>
    <trd lng="de">kariert</trd> 
    <trd lng="en">plaid</trd>
    <trdgrp lng="fr">
      <trd>&eacute;cossais<klr> (motif)</klr></trd>
      <trd>vichy<klr> (motif)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>r&aacute;csos</trd>,
      <trd>kock&aacute;s</trd>
    </trdgrp>
    <trd lng="pl">kraciasty</trd>     
    <trd lng="ro">cadrilat</trd>
    <trd lng="ru">&c_sh;&c_o;&c_t;&c_l;&c_a;&c_n;&c_d;&c_k;&c_a;
    <klr>(&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
    <trd lng="sk">zamre&zcaron;ovan&yacute;</trd>
    <trdgrp lng="zh">
    <trd>&#x683C;&#x5B50;&#x5462; <pr>g&#xE9;zin&#xED;</pr></trd>,
    <trd>&#x683C;&#x5B50;&#x82B1;&#x5462; <pr>g&#xE9;zihu&#x101;n&#xED;</pr></trd>
  </trdgrp>
</snc>
</drv>

<drv mrk="krad.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <fnt><bib>PIV2</bib></fnt>
  <snc mrk="krad.cxirkaux0i.KOMUNE">
    <dif>
