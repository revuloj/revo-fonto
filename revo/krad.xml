<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krad</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krad.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="krad.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Aro da lignaj a&ubreve; feraj stangoj, interspace kunigitaj per
      transversaj samspecaj stangoj, kaj servanta kiel forta barilo:
      <ekz>
        la <tld/>oj de parko, domo, fenestro, mona&hcirc;ejo,
        malliberejo;
      </ekz>
      <ekz>
        kameno &ccirc;irka&ubreve;ita de arte aran&gcirc;ita
        <tld/>a&jcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gitter</trd>,
      <trd>Gitterzaun</trd>,
      <trd>Lattenzaun</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>crossbars <klr>(p.p. malliberejo)</klr></trd>,
      <trd>grating</trd>,
      <trd>grill</trd>,
      <trd>grate</trd>,
      <trd>grille</trd>,
      <trd>lattice</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>reja</trd>,
      <trd>verja</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>grillage</trd>,
      <trd>grille</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>r&aacute;cs</trd>,
      <trd>rost&eacute;ly</trd>,
      <trd>l&eacute;cker&iacute;t&eacute;s</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>grade</trd>,
      <trd>gradil</trd>,    
      <trd>gradeamento</trd>,
      <trd>cancela</trd>    
    </trdgrp>
  </snc>
  <snc mrk="krad.0o.komp">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Nomo de la signo # (Askie 35).
    </dif>
    <rim>
      En Askion la signo trafis pro la Usona negocista tradicio,
      kie &gcirc;i servis por enkonduki numeron (<ctl>track #3</ctl> t.e.
      <ctl>trako n-ro 3</ctl>) a&ubreve; funtojn (<ctl>a 5# sack of
      sugar</ctl> t.e. <ctl>kvinfunta sukersako</ctl>). En
      komputado &gcirc;i balda&ubreve; ricevis aliajn uzojn, ofte tute
      senrilatajn kun la indikitaj (ekz-e kiel surogato por signi
      neegalon, anstata&ubreve; &#8800;). La nomoj
      <ctl>numersigno</ctl> kaj <ctl>funtsigno</ctl> estas
      nerekomendindaj &ccirc;ar ekster Usono ili estas malklaraj kaj
      eble e&ccirc; erarigaj (oni povas havi aliajn
      <ctl>numersignojn</ctl>, ekz-e &#8470; en Rusio).
    </rim>
    <ref tip="sin" cel="dies.0o.komp">dieso</ref>
    <ref tip="malprt" cel="aski.0o">Askio</ref>
    <trdgrp lng="en">
      <trd>number sign</trd>, <trd>hash</trd>, <trd>pound sign</trd>,
      <trd>grid</trd>, <trd>octothorpe</trd>, <trd>sharp</trd>,
    </trdgrp>
    <trd lng="es">almohadilla<klr tip="amb"> (s&iacute;mbolo)</klr></trd>
    <trd lng="fr">di&egrave;se<klr> (symbole graphique)</klr></trd>
    <trdgrp lng="hu">
      <trd>sz&aacute;mjel</trd>,
      <trd>kett&odblac;s <ind>kereszt</ind></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>cerquilha</trd>,
      <trd>tralha</trd>,    
      <trd>quadrado</trd>,
      <trd>sustenido</trd>    
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_r;&c_a;&c_sh;&c_o;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&rcaron;&iacute;&zcaron;</trd>,
    <trd>m&rcaron;&iacute;&zcaron;ka</trd>,
    <trd>m&rcaron;&iacute;&zcaron;ov&iacute;</trd>,
    <trd>ro&scaron;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Lattenzaun</trd>,
    <trd>Raute</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_jo;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>mre&zcaron;a</trd>,
    <trd>ro&scaron;t</trd>
  </trdgrp>
</drv>

<drv mrk="krad.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provizi per <tld/>o:
      <ekz>
        malgranda birdo alflugas al la <tld/>ita fenestro
        <fnt><bib>Fab2</bib>, <lok>Bildo el kastelremparo</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_mol; &c_k;&c_r;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_mol; &c_r;&c_a;&c_sh;&c_o;&c_t;&c_k;&c_u;</trd>
  </trdgrp>
  <trd lng="cs">zam&rcaron;&iacute;&zcaron;ovat</trd>
  <trd lng="es">enrejar</trd>
  <trd lng="fr">grillager</trd>
  <trdgrp lng="hu">
    <trd>r&aacute;csoz</trd>,
    <trd>ber&aacute;csoz</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;
  <ind>&c_r;&c_je;&c_sh;&c_jo;&c_t;&c_k;&c_i;</ind></trd>
  <trdgrp lng="sk">
    <trd>omre&zcaron;ova&tcaron;</trd>,
    <trd>zamre&zcaron;ova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="krad.0ita">
  <kap><tld/>ita</kap>
  <snc mrk="krad.0ita.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ornamita per <tld/>forme interkruci&gcirc;antaj linioj:
      <ekz>
        mikskolore <tld/>ita &scirc;tofo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kvadra1.0umi0i">kvadratita</ref>
    <ref tip="sin" cel="plejd.0o">plejda</ref>
  </snc>
  <trd lng="cs">zam&rcaron;&iacute;&zcaron;ovan&yacute;</trd>
  <trd lng="de">kariert</trd>
  <trd lng="en">plaid</trd>
  <trdgrp lng="fr">
    <trd>&eacute;cossais<klr> (motif)</klr></trd>
    <trd>vichy<klr> (motif)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>r&aacute;csos</trd>,
    <trd>kock&aacute;s</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_o;&c_t;&c_l;&c_a;&c_n;&c_d;&c_k;&c_a;
  <klr>(&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
  <trd lng="sk">zamre&zcaron;ovan&yacute;</trd>
</drv>

<drv mrk="krad.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <fnt><bib>PIV2</bib></fnt>
  <snc mrk="krad.cxirkaux0i.KOMUNE">
    <dif>
      &Ccirc;irka&ubreve;igi per <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">grillager</trd>
</drv>

<drv mrk="krad.de0i">
  <kap>de<tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="krad.de0i.KOMUNE">
    <dif>
      Apartigi per <tld/>o.
    </dif>
  </snc>
  <trd lng="cs">odstranit m&rcaron;&iacute;&zcaron;</trd>
  <trd lng="fr">grillager</trd>
  <trdgrp lng="hu">
    <trd>r&aacute;ccsal elv&aacute;laszt</trd>,
    <trd>r&aacute;ccsal lev&aacute;laszt</trd>
  </trdgrp>
  <trd lng="sk">odstr&aacute;ni&tcaron; mre&zcaron;u</trd>
</drv>

<drv mrk="krad.0eca">
  <kap><tld/>a<fnt>Z</fnt>, <var><kap><tld/>eca</kap></var></kap>
  <snc mrk="krad.0eca.FORM">
    <dif>
      Prezentanta intencajn interspacojn, same kiel <tld/>o;
      <ref tip="vid" cel="ajxur.0a">a&jcirc;ura</ref>:
      <ekz>
        tra la <tld/>a aperturo de la pordego<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_sh;&c_e;&c_c;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_t;&c_ch;&c_a;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">m&rcaron;&iacute;&zcaron;ovit&yacute;</trd>
  <trd lng="fr">&agrave; <ind>claire-voie</ind></trd>
  <trdgrp lng="hu">
    <trd>r&aacute;csos</trd>,
    <trd>kock&aacute;s</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_jo;&c_t;&c_ch;&c_a;&c_t;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>mre&zcaron;ovit&yacute;</trd>,
    <trd>mrie&zcaron;kovit&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="krad.lat0o">
  <kap>lat<tld/>o</kap>
  <snc mrk="krad.lat0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      <tld lit="K"/>a lataro uzata en &gcirc;ardenoj por surkreskigi lianojn,
      a&ubreve; kiel ornama&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_sh;&c_e;&c_c;&c_ib;&c_s;&c_t;&c_a;&c_ja; &c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_a;</trd>
  <trd lng="cs">&scaron;pal&iacute;r</trd>
  <trd lng="en">trellis</trd>
  <trdgrp lng="fr">
    <trd>treillage</trd>,
    <trd>treillis<klr> (treillage)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>l&eacute;cr&aacute;cs</trd>,
    <trd>lugasr&aacute;cs</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_jo;&c_t;&c_ch;&c_a;&c_t;&c_a;&c_ja; &c_z;&c_a;&c_g;&c_o;&c_r;&c_o;&c_d;&c_k;&c_a;</trd>
  <trd lng="sk">&scaron;palier</trd>
</drv>

<drv mrk="krad.0rosti">
  <kap><tld/>rosti</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kuir.0i">Kuiri</ref> man&gcirc;a&jcirc;on
sur krado super
      fajro.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_a;&c_zh;&c_y;&c_c;&c_mol; &c_n;&c_a;
<ind>&c_g;&c_r;&c_y;&c_l;&c_ib;</ind></trd>
  <trd lng="cs">op&eacute;kat na ro&scaron;tu</trd>
  <trd lng="en">grill</trd>
  <trd lng="fr">faire <ind>griller</ind></trd>
  <trdgrp lng="hu">
    <trd>roston s&uuml;t</trd>,
    <trd>grillez</trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_a;&c_r;&c_i;&c_t;&c_mol; &c_n;&c_a;
<ind>&c_g;&c_r;&c_i;&c_l;&c_je;</ind></trd>
  <trd lng="sk">opeka&tcaron; na ro&scaron;te</trd>
</drv>

<drv mrk="krad.0rostilo">
  <kap><tld/>rostilo</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Kuireja aparato, konsistanta el paralelaj ferstangetoj, sur
      kiujn oni metas viandon a&ubreve; fi&scirc;on, por &gcirc;in
ekrosti per vigla
      fajro.
    </dif>
    <trd lng="be">&c_g;&c_r;&c_y;&c_l;&c_mol; <klr
tip="amb">(&c_r;&c_a;&c_sh;&c_o;&c_t;&c_k;&c_a; &c_d;&c_l;&c_ja;
&c_s;&c_m;&c_a;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
    <trd lng="de">Grill</trd>
    <trd lng="en">grill</trd>
    <trd lng="es">parrilla</trd>
    <trd lng="fr">grill</trd>
    <trdgrp lng="hu">
      <trd>s&uuml;t&odblac;rost&eacute;ly</trd>,
      <trd>grills&uuml;t&odblac;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_sh;&c_p;&c_je;&c_r;</trd>,
      <trd>&c_r;&c_je;&c_sh;&c_jo;&c_t;&c_k;&c_a;<klr tip="amb">
(&c_d;&c_l;&c_ja; &c_zh;&c_a;&c_r;&c_k;&c_i;)</klr></trd>,
      <trd>&c_g;&c_r;&c_i;&c_l;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>ro&scaron;t k op&eacute;k&aacute;n&iacute;</trd>,
    <trd>ro&scaron;tov&yacute; gril</trd>
  </trdgrp>
  <trd lng="sk">ro&scaron;t na pe&ccaron;enie m&auml;sa</trd>
</drv>
</art>
