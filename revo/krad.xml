    <dif>
      Provizita per <tld/>o:
      <ekz>
        la vento fajfadis tra la <tld/>a fenestro
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &gcirc;ardenon &ccirc;irka&ubreve;is fera <ind><tld/>a barilo</ind>
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>.
        <trd lng="de">Gitterzaun</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_sh;&c_e;&c_c;&c_ib;&c_s;&c_t;&c_y;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_t;&c_ch;&c_a;&c_t;&c_y;</trd>
