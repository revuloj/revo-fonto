<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aerost.xml,v 1.34 2023/10/27 13:42:09 revo Exp $">
<kap>
  <rad>aerostat</rad>/o
  <fnt><bib>Fab3</bib></fnt>
</kap>
<drv mrk="aerost.0o">
  <kap><tld/>o</kap>
  <snc mrk="aerost.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="aparat.flug0o">Flugaparato</ref>, &gcirc;enerale
      sfer- a&ubreve; cigarforma, entenanta gason pli malpezan ol la aero, kaj
      servanta por sin levi kaj veturi en la aero:
      <ekz>
        kian veturrimedon mi elektu: &ccirc;u la <tld/>oj estas jam elpensitaj?
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        suprenflugi kun la balono
        <klr>[&#x2026;]</klr> &leftquot;tio estas stranga ideo, tiel levi&gcirc;i en la aeron!&rightquot;
        <klr>[&#x2026;]</klr> &gcirc;i estis ja <klr>[&#x2026;]</klr> <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="balon.0o.aerostato">balono</ref>
    <ref tip="sub" cel="sxip.aer0o">aer&scirc;ipo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_e;&c_r;&c_a;&c_s;&c_t;&c_a;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_t;&c_r;&c_a;&c_n;&c_y; &c_sh;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="bo">&#xF63;&#xF92;&#xF44;&#xF0B;&#xF55;&#xF74;&#xF42;&#xF0B;&#xF42;&#xF53;&#xF58;&#xF0B;&#xF42;&#xFB2;&#xF74;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>aerostat</trd>,
    <trd>bal&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Luftschiff</trd>,
    <trd>Ballon</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>balloon</trd>,
    <trd>aerostat</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>a&eacute;rostat</trd>,
    <trd>ballon<klr tip="ind"> (a&eacute;rostat)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B7;&#x5DC;&#x5D5;&#x5B9;&#x5DF;</trd>
  <trdgrp lng="hu">
    <trd>l&eacute;gg&ouml;mb</trd>,
    <trd>l&eacute;ghaj&oacute;</trd>
  </trdgrp>
  <trd lng="it">aerostato</trd>
  <trd lng="ja">&#x8EFD;&#x822A;&#x7A7A;&#x6A5F;  <pr>&#x3051;&#x3044;&#x3053;&#x3046;&#x304F;&#x3046;&#x304D;</pr></trd>
  <trdgrp lng="nl">
    <trd>aerostaat</trd>,
    <trd>luchtballon</trd>,
    <trd>luchtschip</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>aerostat</trd>,
    <trd>sterowiec</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>aer&oacute;stato</trd>,
   <trd>bal&atilde;o</trd>
  </trdgrp>
  <trd lng="ro">balon cu aer cald</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j; &c_sh;&c_a;&c_r;</trd>,
    <trd>&c_a;&c_e;&c_r;&c_o;&c_s;&c_t;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bal&oacute;n</trd>,
    <trd>bal&oacute;n</trd>,
    <trd>vzducholo&dcaron;</trd>,
    <trd>vzducholo&dcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x432;&#x456;&#x442;&#x440;&#x44F;&#x43D;&#x430; &#x43A;&#x443;&#x43B;&#x44F;</trd>,
    <trd>&#x430;&#x435;&#x440;&#x43E;&#x441;&#x442;&#x430;&#x442;</trd>
  </trdgrp>
</drv>

<drv mrk="aerost.varmaera0o">
  <kap>varmaera <tld/>o</kap>
  <snc mrk="aerost.varmaera0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="super" cel="aerost.0o"><tld lit="A"/>o</ref> uzanta
      varmigitan aeron por levi&gcirc;i.
    </dif>
    <trdgrp lng="zh">
    <trd>&#x70ED;&#x6C14;&#x7403; <pr>r&#xE8;q&#xEC;qi&#xFA;</pr></trd>,
    <trd>&#x71B1;&#x6C23;&#x7403; <pr>r&#xE8;q&#xEC;qi&#xFA;</pr></trd>,
    <trd>&#x70ED;&#x6C7D;&#x7403; <pr>r&#xE8;q&#xEC;qi&#xFA;</pr></trd>,
    <trd>&#x71B1;&#x6C7D;&#x7403; <pr>r&#xE8;q&#xEC;qi&#xFA;</pr></trd>
  </trdgrp>
</snc>
  <bld lok="../bld/aerostat.jpg">
    varmaera <tld/>o
  </bld>
  <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_a;&c_l;&c_mol;&c_f;'&c_je;&c_r;</trd>
  <trd lng="de">Hei&szlig;luftballon</trd>
  <trd lng="en">hot-air balloon</trd>
  <trdgrp lng="fr">
    <trd>montgolfi&egrave;re</trd>
    <trd><ind>ballon</ind> &agrave; air chaud</trd>
  </trdgrp>
  <trd lng="hu">h&odblac;l&eacute;gballon</trd>
  <trd lng="it">mongolfiera</trd>
  <trd lng="nl">heteluchtballon</trd>
  <trd lng="pl">balon</trd>
  <trdgrp lng="pt">
   <trd>bal&atilde;o de ar quente</trd>
  </trdgrp>
  <trd lng="ro">balon cu aer cald</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_g;&c_o;&c_l;&c_mol;&c_f;&c_mol;&c_je;&c_r;</trd>
</drv>
</art>
