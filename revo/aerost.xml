<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>aerostat</rad>/o<fnt>Z</fnt>
</kap>
<drv mrk="aerost.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Aparato, &gcirc;enerale sfer- a&ubreve; cigarforma, entenanta gason
      pli malpezan ol la aero, kaj servanta por sin levi kaj veturi en la
      aero.
    </dif>
    <ref tip="sub" cel="balon.0o">balono</ref>
  </snc>
  <bld lok="../bld/aerostat.jpg">
    <ind>varmaera aerostato</ind>
    <klr>(uzanta varmigitan aeron por levi&gcirc;i)</klr>
    <trd lng="de">Hei&szlig;luftballon</trd>
  </bld>
  <trdgrp lng="de">
    <trd>Luftschiff</trd>,
    <trd>Ballon</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>a&eacute;rostat</trd>,
    <trd>montgolfi&egrave;re</trd>
  </trdgrp>
  <trd lng="nl">a&euml;rostaat</trd>
  <trd lng="pt">aer&oacute;stato</trd>
  <trdgrp lng="ru">
    <trd>&#1074;&#1086;&#1079;&#1076;&#1091;&#1096;&#1085;&#1099;&#1081;
    &#1096;&#1072;&#1088;</trd>,
    <trd>&#1072;&#1101;&#1088;&#1086;&#1089;&#1090;&#1072;&#1090;</trd>
  </trdgrp>
</drv>
</art>
