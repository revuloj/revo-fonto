<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kokon.xml,v 1.20 2023/10/18 17:23:24 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kokon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kokon.0o">
  <kap><tld/>o</kap>
  <snc mrk="kokon.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Fadena envolva&jcirc;o, kiun &scirc;pinas preska&ubreve; &ccirc;iuj larvoj de lepidopteroj,
      kaj en kiu fari&gcirc;as la lasta metamorfozo:
      <ekz>
        <tld/>o de silka ra&ubreve;po <klr>(uzata por fari silkfadenojn)</klr>;
      </ekz>
      <ekz>bobeni <tld/>on.</ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kokon</trd>,
      <trd>Insektengespinst</trd>
    </trdgrp>
  </snc>
  <snc mrk="kokon.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Tiu envolva&jcirc;o kaj la entenata
      <ref tip="vid" cel="krizal.0o.ZOO">krizalido</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="pup.0o.ZOO">pupo<sncref/></ref>
    </refgrp>
    <trd lng="de">Kokon</trd>
    <trd lng="en">cocoon</trd>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_k;&c_a;&c_n;</trd>
  <trd lng="bg">&c_p;&c_a;&c_sh;&c_k;&c_u;&c_l;</trd>
  <trd lng="cs">z&aacute;motek housenky</trd>
  <trd lng="de">Kokon</trd>
  <trd lng="fr">cocon</trd>
  <trdgrp lng="ja">
    <trd>&#x7E6D;  <pr>&#x307E;&#x3086;</pr></trd>,
    <trd>&#x5375;&#x56A2;  <pr>&#x3089;&#x3093;&#x306E;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">cocon</trd>
  <trdgrp lng="pl">
    <trd>kokon</trd>,
    <trd>oprz&eogonek;d <klr>(larw)</klr> </trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>cocon</trd>,
    <trd>pup&abreve;</trd>
  </trdgrp>                                                             
                                                                        
                                                                        
                                                                        
                                                                        
                                                                        
                                                        
  <trd lng="ru">&c_k;&c_o;&c_k;&c_o;&c_n;</trd>
  <trd lng="sk">z&aacute;motok h&uacute;senice</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x43A;&#x43E;&#x43D;</trd>
</drv>
</art>
