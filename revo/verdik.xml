<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>verdikt</rad>/o
</kap>
<drv mrk="verdik.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Decido de ju&gcirc;antaro pri akuzito:
      <ekz>
        diri<fnt>Z</fnt>,
      </ekz>
      <ekz>
        skribi<fnt>Z</fnt>,
      </ekz>
      <ekz>
        konfirmi<fnt>B</fnt>
      </ekz>
      <ekz>
        <tld/>on.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Decido de ju&gcirc;antaro pri konkurantoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Urteil</trd>,
    <trd>Urteilsspruch</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>verdict</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vonnis</trd>,
    <trd>uitspraak<klr tip="ind"> (vonnis)</klr></trd>,
    <trd>verdict</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_g;&c_o;&c_v;&c_o;&c_r;</trd>,
    <trd>&c_v;&c_je;&c_r;&c_d;&c_i;&c_k;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="verdik.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Diri <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>prononcer le <ind>verdict</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_n;&c_je;&c_s;&c_t;&c_i; <ind>&c_p;&c_r;&c_i;&c_g;&c_o;&c_v;&c_o;&c_r;</ind></trd>,
    <trd>&c_v;&c_y;&c_n;&c_je;&c_s;&c_t;&c_i; <ind>&c_v;&c_je;&c_r;&c_d;&c_i;&c_k;&c_t;</ind></trd>
  </trdgrp>
</drv>
</art>
