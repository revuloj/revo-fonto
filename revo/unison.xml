<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: unison.xml,v 1.29 2025/10/03 20:30:20 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>unison</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="unison.0o">
  <kap><tld/>o</kap>
  <snc mrk="unison.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="akord.0o.MUZ">Akordo</ref> de pluraj
      vo&ccirc;oj a&ubreve; muzikiloj a&ubreve;digantaj samtempe saman noton:
      <ekz>
        la miksi&gcirc;o de la ebria krio kaj <tld/>a kanto kelkfoje donis strangajn impresojn
        <fnt>
          <bib>VRA</bib>
          <lok>Erik Asklund</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kantojn Kaj Tiel Plu prezentas &hcirc;ore (jen <tld/>e, jen plurvo&ccirc;e)
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2005/008811.php"
              >Katalunio, Okcitanio Kaj Tiel Plu</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        al <klr>[la fremdaj vortoj]</klr> <tld/>e devas soni anka&ubreve;
        &ccirc;iuj aliaj vortoj de
        la vortaro
        <fnt>
          <bib>EE</bib>
          <lok>VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la regionestro <klr>[&#x2026;]</klr> <tld/>e kun la vo&ccirc;oj el la &ccirc;efurbo, deklaris ke la
        kaliningradanoj rifuzos la senvizan transiton
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116; 11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_n;&c_ib;&c_s;&c_o;&c_n;</trd>
  <trd lng="ca">un&iacute;son</trd>
  <trdgrp lng="cs">
    <trd>jednohlas</trd>,
    <trd>souzvuk</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Unisono</trd>,
     <trd>&Uuml;bereinstimmung</trd>,
     <trd>Einm&uuml;tigkeit</trd>
   </trdgrp>
  <trd lng="el">&omikron;&my;&omikron_ton;&phi;&omega;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">unison</trd>
  <trd lng="fa">&a_h;&a_m;&a_n;&a_w;&a_A;&f_y;&f_y;</trd>
  <trdgrp lng="fi">
     <trd>unisono</trd>,
     <trd>yksi&auml;&auml;ninen sointu t. esitys</trd>
   </trdgrp>
  <trd lng="fr">unisson</trd>
  <trdgrp lng="hu">
     <trd>egybehangz&aacute;s</trd>,
     <trd>unisono</trd>
   </trdgrp>
  <trd lng="it">unisono<klr> (mus.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30E6;&#x30CB;&#x30BE;&#x30F3;</trd>,
    <trd>&#x540C;&#x97F3;  <pr>&#x3069;&#x3046;&#x304A;&#x3093;</pr></trd>,
    <trd>&#x6589;&#x5531;  <pr>&#x305B;&#x3044;&#x3057;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">unisono</trd>
  <trd lng="pl">unisono</trd>
  <trd lng="pt">un&iacute;ssono</trd>
  <trd lng="ru">&c_u;&c_n;&c_i;&c_s;&c_o;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>jednohlasn&yacute; s&uacute;zvuk</trd>,
    <trd>zhoda</trd>
  </trdgrp>
  <trd lng="uk">&c_u;&c_n;&c_ib;&c_s;&c_o;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#x5168;&#x4F53;&#x4E00;&#x81F4; <pr>qu&aacute;nt&#x1D0;y&imacron;zh&igrave;</pr></trd>,
    <trd>&#x5168;&#x9AD4;&#x4E00;&#x81F4; <pr>qu&aacute;nt&#x1D0;y&imacron;zh&igrave;</pr></trd>,
    <trd>&#x6C92;&#x6709;&#x7570;&#x8B70; <pr>m&eacute;iy&#x1D2;uy&igrave;y&igrave;</pr></trd>,
    <trd>&#x6CA1;&#x6709;&#x5F02;&#x8BAE; <pr>m&eacute;iy&#x1D2;uy&igrave;y&igrave;</pr></trd>,
    <trd>&#x4E00;&#x81F4; <pr>y&imacron;zh&igrave;</pr></trd>
  </trdgrp>
</drv>
</art>
