<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>1</ofc>
   <rad>unison</rad>/o
</kap>
<drv mrk="unison.0o">
   <kap><tld/>o</kap>
   <snc mrk="unison.0o.MUZ">
     <uzo tip="fak">MUZ</uzo>
     <dif>
       <ref tip="super" cel="akord.0o.MUZ">Akordo</ref> de pluraj
vo&ccirc;oj a&ubreve;
       muzikiloj a&ubreve;digantaj samtempe saman noton:
       <ekz>
         <uzo tip="stl">FIG</uzo>
         al la fremdaj vortoj <tld/>e devas soni anka&ubreve;
&ccirc;iuj aliaj vortoj de
         la vortaro<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_u;&c_n;&c_ib;&c_s;&c_o;&c_n;</trd>
   <trdgrp lng="de">
     <trd>Unisono</trd>,
     <trd>&Uuml;bereinstimmung</trd>,
     <trd>Einm&uuml;tigkeit</trd>
   </trdgrp>
   <trd
lng="el">&omikron;&my;&omikron_ton;&phi;&omega;&ny;&omikron;&sigma_fina;</trd>
   <trd lng="en">unison</trd>
   <trdgrp lng="fi">
     <trd>unisono</trd>,
     <trd>yksi&auml;&auml;ninen sointu t. esitys</trd>
   </trdgrp>
   <trd lng="fa">&a_h;&a_m;&a_n;&a_w;&a_A;&f_y;&f_y;</trd>
   <trd lng="fr">unisson</trd>
   <trdgrp lng="hu">
     <trd>egybehangz&aacute;s</trd>,
     <trd>unisono</trd>
   </trdgrp>
   <trd lng="it">unisono<klr> (mus.)</klr></trd>
   <trd lng="nl">unisono</trd>
   <trd lng="pt">un&iacute;ssono</trd>
   <trd lng="ru">&c_u;&c_n;&c_i;&c_s;&c_o;&c_n;</trd>
</drv>
</art>
