<art mrk="$Id: unison.xml,v 1.24 2023/09/05 09:43:52 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>unison</rad>/o
</kap>
<drv mrk="unison.0o">
   <kap><tld/>o</kap>
   <snc mrk="unison.0o.MUZ">
     <uzo tip="fak">MUZ</uzo>
     <dif>
       <ref tip="super" cel="akord.0o.MUZ">Akordo</ref> de pluraj
vo&ccirc;oj a&ubreve;
       muzikiloj a&ubreve;digantaj samtempe saman noton:
       <ekz>
         <uzo tip="stl">FIG</uzo>
         al la fremdaj vortoj <tld/>e devas soni anka&ubreve;
&ccirc;iuj aliaj vortoj de
         la vortaro<fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_u;&c_n;&c_ib;&c_s;&c_o;&c_n;</trd>
  <trd lng="ca">un&iacute;son</trd>
  <trdgrp lng="cs">
    <trd>jednohlas</trd>,
    <trd>souzvuk</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Unisono</trd>,
     <trd>&Uuml;bereinstimmung</trd>,
     <trd>Einm&uuml;tigkeit</trd>
   </trdgrp>
  <trd lng="el">&omikron;&my;&omikron_ton;&phi;&omega;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">unison</trd>
  <trd lng="fa">&a_h;&a_m;&a_n;&a_w;&a_A;&f_y;&f_y;</trd>
  <trdgrp lng="fi">
     <trd>unisono</trd>,
     <trd>yksi&auml;&auml;ninen sointu t. esitys</trd>
   </trdgrp>
  <trd lng="fr">unisson</trd>
  <trdgrp lng="hu">
     <trd>egybehangz&aacute;s</trd>,
     <trd>unisono</trd>
   </trdgrp>
  <trd lng="it">unisono<klr> (mus.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30E6;&#x30CB;&#x30BE;&#x30F3;</trd>,
    <trd>&#x540C;&#x97F3;  [<ind>&#x3069;&#x3046;&#x304A;&#x3093;</ind>]</trd>,
    <trd>&#x6589;&#x5531;  [<ind>&#x305B;&#x3044;&#x3057;&#x3087;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">unisono</trd>
  <trd lng="pt">un&iacute;ssono</trd>
  <trd lng="ru">&c_u;&c_n;&c_i;&c_s;&c_o;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>jednohlasn&#xFD; s&#xFA;zvuk</trd>,
    <trd>zhoda</trd>
  </trdgrp>
  <trd lng="uk">&#x443;&#x43D;&#x456;&#x441;&#x43E;&#x43D;</trd>
</drv>
</art>
