<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diagon.xml,v 1.36 2023/11/04 14:29:01 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>diagonal</rad>/o
</kap>
<drv mrk="diagon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="diagon.0o.plurlatero">
    <fnt><bib>MatTerm</bib><lok>p. 28</lok></fnt>
    <dif>
      <klr>(de <ref cel="later.plur0o">plurlatero</ref>)</klr>
      <ref cel="rekt.0o.elementa">Rekto</ref> a&ubreve;
      <ref cel="strek.0o.segmento">streko</ref>, kunliganta du verticojn, kiuj
      ne apartenas al la sama latero:
      <ekz>
        ne ekzistas <tld/>o en triangulo;
      </ekz>
      <ekz>
        en kvarlatero ekzistas du <tld/>oj;
      </ekz>
      <ekz>
        la <tld/>oj de paralelogramo sin intersekcas en siaj mezoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diamet.0o.subaro">diametro</ref>.
    </refgrp>
  </snc>
  <snc mrk="diagon.0o.de_matrico">
    <fnt><bib>EKV</bib><lok>&para;77</lok></fnt>
    <dif>
      <klr>(de <ref cel="matric.n_p0o">(n,n)-matrico</ref>)</klr>
      La <frm><k>n</k></frm>-opo, konsistanta el &ccirc;iuj &gcirc;iaj 
      elementoj kun indico <frm>(<k>i</k>,<k>i</k>)</frm>.
    </dif>
  </snc>
  <bld lok="../bld/diagon.gif">
    <tld/>o de matrico
  </bld>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_g;&c_a;&c_n;&c_a;&c_l;&c_mol;</trd>
  <trd lng="de">Diagonale</trd>
  <trd lng="en">diagonal</trd>
  <trd lng="es">diagonal</trd>
  <trd lng="fr">diagonale</trd>
  <trd lng="he">&#x5D0;&#x5B2;&#x5DC;&#x5B7;&#x5DB;&#x5E1;&#x5D5;&#x5B9;&#x5DF;</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tl&oacute;</trd>,
    <trd>f&odblac;&aacute;tl&oacute;</trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5BFE;&#x89D2;&#x7DDA;  <pr>&#x305F;&#x3044;&#x304B;&#x304F;&#x305B;&#x3093;</pr></trd>,
    <trd>&#x540C;&#x8272;&#x306E;&#x659C;&#x3081;&#x306B;&#x9023;&#x306A;&#x308B;&#x307E;&#x3059;&#x76EE;  <pr>&#x3069;&#x3046;&#x3044;&#x308D;&#x306E;&#x306A;&#x306A;&#x3081;&#x306B;&#x3064;&#x3089;&#x306A;&#x308B;&#x307E;&#x3059;&#x3081;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przek&aogonek;tna</trd>,
    <trd>przek&aogonek;tnia</trd></trdgrp> 
  <trd lng="pt">diagonal</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_g;&c_o;&c_n;&c_a;&c_l;&c_mol;</trd>
  <trd lng="sk">uhloprie&ccaron;ka</trd>
  <trd lng="sk">diagon&#xE1;la na &#x161;achovnici</trd>
  <trdgrp lng="cs">
    <trd>diagon&#xE1;la</trd>,
    <trd>&#xFA;hlop&#x159;&#xED;&#x10D;ka</trd>
  </trdgrp>
  <trd lng="uk">&#x434;&#x456;&#x430;&#x491;&#x43E;&#x43D;&#x430;&#x43B;&#x44C;</trd>
</drv>

<drv mrk="diagon.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="diagon.0a.streko">
    <fnt><bib>VdE</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="strek.0o.segmento">streko</ref> a&ubreve;
      <ref cel="rekt.0o.MAT">rekto</ref>)</klr>
      Samdirekta kiel la <ref cel="diagon.0o.plurlatero"><tld/>o <sncref/></ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="oblikv.0a">oblikva</ref>,
      <ref cel="transv.0a">transversa</ref>.
    </refgrp>
    <trd lng="fr">diagonal</trd>
    <trdgrp lng="hu">
      <trd>&aacute;tl&oacute;s</trd>,
      <trd>diagon&aacute;lis</trd>
    </trdgrp>
    <trd lng="pl">przek&aogonek;tny</trd>
  </snc>
  <snc>
    <subsnc mrk="diagon.0a.elemento">
      <fnt><bib>PIV2</bib><lok>spuro</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="elemen.0o.matrico">elemento</ref> de
        (n,n)-matrico)</klr>
        Estanta sur &gcirc;ia 
        <ref cel="diagon.0o.de_matrico"><tld/>o <sncref/></ref>:
        <ekz>
          matrico kun egalaj <tld/>aj elementoj.
        </ekz>
      </dif>
      <trd lng="de">Diagonal-<klr tip="amb">(element)</klr></trd>
      <trd lng="en">diagonal<klr tip="ind"> (element)</klr></trd>
      <trd lng="fr"><klr tip="ind">(&eacute;l&eacute;ment) </klr>diagonal</trd>
      <trdgrp lng="hu">
        <trd>diagon&aacute;l-</trd>,
        <trd>f&odblac;&aacute;tl&oacute;beli</trd>
      </trdgrp>
      <trd lng="pl"><klr tip="ind">(element) </klr>przek&aogonek;tny</trd>
      <trd lng="ru">&c_d;&c_i;&c_a;&c_g;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
    </subsnc>
    <subsnc mrk="diagon.0a.matrico">
      <fnt><bib>MatVort</bib></fnt>
      <dif>
        <klr>(p.p. <ref cel="matric.n_p0o">(n,n)-matrico</ref>)</klr>
        Kies &ccirc;iuj elementoj estas nulaj, krom tiuj situantaj
        sur &gcirc;ia 
        <ref cel="diagon.0o.de_matrico"><tld/>o <sncref/></ref>:
        <ekz>
          la unuomatrico estas <tld/>a.
        </ekz>
      </dif>
      <trd lng="de">Diagonal-<klr tip="amb">(matrix)</klr></trd>
      <trd lng="en">diagonal<klr tip="ind"> (matrix)</klr></trd>
      <trd lng="fr"><klr tip="amb">(matrice) </klr>diagonale</trd>
      <trdgrp lng="hu">
        <trd>diagon&aacute;lis<klr tip="ind"> (m&aacute;trix)</klr></trd>,
        <trd>&aacute;tl&oacute;s<klr tip="ind"> (m&aacute;trix)</klr></trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd><klr tip="amb">(macierz) </klr>diagonalna</trd>,
        <trd><klr tip="amb">(macierz) </klr>przek&aogonek;tniowa</trd></trdgrp> 
      <trd lng="ru">&c_d;&c_i;&c_a;&c_g;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja;<klr tip="amb"> (&c_m;&c_a;&c_t;&c_r;&c_i;&c_c;&c_a;)</klr></trd>
    </subsnc>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_g;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>diagonal</trd>
  </trdgrp>
  <trd lng="en">diagonal</trd>
  <trd lng="es">diagonal</trd>
  <trd lng="he">&#x5D0;&#x5B2;&#x5DC;&#x5B7;&#x5DB;&#x5E1;&#x5D5;&#x5B9;&#x5E0;&#x5B4;&#x5D9;</trd>
  <trdgrp lng="ja">
    <trd>&#x5BFE;&#x89D2;&#x7DDA;&#x306E;  <pr>&#x305F;&#x3044;&#x304B;&#x304F;&#x305B;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x306F;&#x3059;&#x304B;&#x3044;&#x306E;</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_g;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="uk">&#x434;&#x456;&#x430;&#x491;&#x43E;&#x43D;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x5BF9;&#x89D2; <pr>du&#xEC;ji&#x1CE;o</pr></trd>,
    <trd>&#x659C;&#x5BF9; <pr>xi&#xE9;du&#xEC;</pr></trd>,
    <trd>&#x5BF9;&#x89D2;&#x7EBF; <pr>du&#xEC;ji&#x1CE;oxi&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="diagon.0igebla">
  <kap><tld/>igebla</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="diagon.0igebla.endomorfio">
    <dif>
      <klr>(p.p. <ref cel="endomo.0o">endomorfio</ref> en
      <ref cel="spac.vektora0o">vektora spaco</ref> <frm><g>E</g></frm>)</klr>
      Tia, ke <frm><g>E</g></frm> egalas al la
      <ref cel="rekt.0a.sumo">rekta</ref>
      <ref cel="sum.0o.vektorasubspaco">sumo</ref> de
      &gcirc;iaj <ref cel="ajgen.0subspaco">ajgensubspacoj</ref>:
      <ekz>
        la matrico de <tld/>igebla endomorfio rilate al bazo konsistanta
        el ajgenvektoroj estas <ref cel="diagon.0a.matrico"><tld/>a</ref>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="diagon.0igebla.matrico">
    <dif>
      <klr>(p.p. <ref cel="matric.n_p0o">(n,n)-matrico</ref>)</klr>
      Tia, ke ekzistas <tld/>igebla <sncref ref="diagon.0igebla.endomorfio"/> 
      endomorfio, kies matrico &gcirc;i
      estas.
    </dif>
  </snc>
  <adm>
    Mi ne trovis auxtoritatan fonton. <aut>MB</aut>
  </adm>
  <trd lng="en">diagonalizable</trd>
  <trd lng="de">diagonalisierbar</trd>
  <trd lng="fr">diagonalisable</trd>
  <trd lng="he">&#x5E0;&#x5D9;&#x5EA;&#x5DF; &#x5DC;&#x5D0;&#x5DC;&#x5DB;&#x5E1;&#x5D5;&#x5DF;</trd>
  <trd lng="hu">diagoniz&aacute;lhat&oacute;</trd>  
  <trd lng="pl">diagonalizowalny</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_g;&c_o;&c_n;&c_a;&c_l;&c_i;&c_z;&c_u;&c_je;&c_m;&c_y;&c_j;</trd>
</drv>
</art>
