<vortaro>
  <art mrk="$Id: gxibut.xml,v 1.5 2023/11/11 11:36:38 afido Exp $">
    <kap>
      <rad>&Gcirc;ibut</rad>/o
    </kap>

<drv mrk="gxibut.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc mrk="gxibut.0o.urbo">
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="gxibuti.GX0o">&Gcirc;ibutio</ref>:
      <ekz>
        la fervojo inter Addis-Abebo kaj <tld/>o ekfunkciis en 2018
        <fnt>
          <vrk><url ref="http://esperanto.china.org.cn/2018-09/30/content_64399880.htm">Fakulo de UN: La fervojo inter Addis-Abebo kaj &Gcirc;ibuto ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a Federacio de Manpilkado
        <fnt>
          <vrk><url ref="&Viki;/%C4%9Cibuta_Federacio_de_Manpilkado">&Gcirc;ibuta Federacio de Manpilkado - Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> dankas la &ccirc;inan registaron kaj la &ccirc;inan popolon por helpo kaj
        subteno al <tld/>o
        <fnt>
          <vrk><url ref="&CRI;2521/2019/01/09/179s202938.htm">&Ccirc;ina kaj &gcirc;ibutia &scirc;tatestroj inter&scirc;an&gcirc;is gratulojn okaze de ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_D;&c_zh;&c_y;&c_b;&c_u;&c_t;&c_y; <klr tip="amb">(&c_g;&c_o;&c_r;&c_a;&c_d;)</klr></trd>
    <trd lng="de">Dschibuti <klr tip="ind">(Stadt)</klr></trd>
    <trd lng="en">Djibouti <klr tip="ind">(city)</klr></trd>
    <trd lng="fr">Djibouti <klr tip="ind">(ville)</klr></trd>
    <trd lng="nl">Djibouti <klr tip="ind">(stad)</klr></trd>
    <trd lng="zh">&#x5409;&#x5E03;&#x63D0; <pr>j&#xED;b&#xF9;t&#xED;</pr></trd>
</snc>
</drv>

