    <trd>bel</trd>,
    <trd>bell</trd>
  </trdgrp>
  <trd lng="de">das <ind>Sch&ouml;ne</ind></trd>
  <trdgrp lng="en">
    <trd>beauty</trd>,
    <trd>pulchritude</trd>,
    <trd>comeliness</trd>,
    <trd>handsomeness</trd>
