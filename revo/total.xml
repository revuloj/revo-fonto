<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>total</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="total.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      &Ccirc;ion ampleksanta, &ccirc;ioma:
      <ekz>
        la <tld/>a nombro de la &ccirc;eestantoj;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komple2.0a">kompleta</ref>,
      <ref cel="tut.0a">tuta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_s;&c_je;&c_a;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">total<klr> (adj.)</klr></trd>
  <trdgrp lng="de">
    <trd>v&ouml;llig</trd>,
    <trd>vollst&auml;ndig</trd>,
    <trd>total</trd>
  </trdgrp>
  <trd lng="es">total<klr> (adj.)</klr></trd>
  <trd lng="fr">total<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>tot&aacute;lis</trd>,
    <trd>&ouml;sszes</trd>
  </trdgrp>
  <trd lng="nl">totaal</trd>
  <trd lng="pt">total</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_s;&c_je;&c_o;&c_b;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>celistvý</trd>,
    <trd>totálny</trd>
  </trdgrp>
</drv>

<drv mrk="total.0o">
  <kap><tld/>o</kap>
  <snc mrk="total.0o.SNC">
    <dif>
      La tuta kvanto:
      <ekz>
        ili pereigis la <ind><tld/>o</ind>n de la popolo
      </ekz>;
      <ekz>
        la planitaj investoj &gcirc;is 2003 faras totalon de 200 milionoj da
        USD
        <fnt>
          <aut>Evgeni Georgiev</aut>,
          <vrk><url ref="http://www.esperanto.be/fel/mon/">Artikoloj el
          Monato - Balkanio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">total<klr> (subst.)</klr></trd>
  <trdgrp lng="es">
    <trd>total<klr> (subst.)</klr></trd>,
    <trd>resultado</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>total<klr> (nom)</klr></trd>,
    <trd>totalit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>celé množstvo</trd>,
    <trd>všetko</trd>
  </trdgrp>
</drv>

<drv mrk="total.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="total.0ismo.POL">
  <uzo tip="fak">POL</uzo>
    <dif>
      Politika <ref tip="super" cel="regxim.0o.POL">re&gcirc;imo</ref>, en kiu
      la rajtoj de la civitanoj estas kvaza&ubreve; ensorbitaj de la &scirc;tato, kaj
      en kiu povas ekzisti nur unu partio, tiu de la &scirc;tatistoj:
      <ekz>
        <ind><tld/>isma</ind> re&gcirc;imo.
        <trd lng="be">&c_t;&c_a;&c_t;&c_a;&c_l;&c_ib;&c_t;&c_a;&c_r;&c_n;&c_y;</trd>
        <trd lng="ca">totalitari</trd>
        <trd lng="de">totalit&auml;r</trd>
        <trdgrp lng="es">
          <trd>totalitario</trd>,
          <trd>autoritario</trd>
        </trdgrp>
        <trd lng="fr">totalitaire</trd>
        <trdgrp lng="hu">
          <trd>&ouml;nk&eacute;nyuralmi</trd>,
          <trd>totalit&aacute;rius</trd>
        </trdgrp>
        <trd lng="nl">autoritaire</trd>
        <trd lng="pt">totalit&aacute;rio</trd>
        <trd lng="ru">&c_t;&c_o;&c_t;&c_a;&c_l;&c_i;&c_t;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_t;&c_a;&c_l;&c_ib;&c_t;&c_a;&c_r;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">totalitarisme</trd>
  <trd lng="de">Totalitarismus</trd>
  <trd lng="es">totalitarismo</trd>
  <trd lng="fr">totalitarisme</trd>
  <trdgrp lng="hu">
    <trd>&ouml;nk&eacute;nyuralom</trd>,
    <trd>totalitarizmus</trd>
  </trdgrp>
  <trd lng="nl">totalitarisme</trd>
  <trd lng="pt">totalitarismo</trd>
  <trd lng="ru">&c_t;&c_o;&c_t;&c_a;&c_l;&c_i;&c_t;&c_a;&c_r;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">totalizmus</trd>
</drv>
</art>
