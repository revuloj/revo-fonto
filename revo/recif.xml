<vortaro>
  <art mrk="$Id: recif.xml,v 1.7 2024/04/13 20:15:52 afido Exp $">
    <kap>
      <rad>Recif</rad>/o
    </kap>

<drv mrk="recif.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda haven<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en la oriento de <ref tip="malprt" cel="brazil.B0o">Brazilo</ref>,
      &ccirc;. 680&#x202F;km nordoriente de <ref cel="salvador.S0o.de_Bahio">Salvadoro de Bahio</ref>:
      <ekz>
        <tld/>o, fondita en 1537 estas la plej antikva el la landaj &ccirc;efurboj
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Recife">Recife</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mia patro naski&gcirc;is en 1600, en la urbo <tld/>o, kiam li a&gcirc;is dudek kvin jarojn,
        li enordeni&gcirc;is kiel franciskano
        <fnt>
          <aut>Joaquim Maria Machado De Assis, trad. Josenilton kaj Madragoa</aut>
          <vrk><url ref="https://rl.art.br/arquivos/4619335.pdf?1387750926">La
          Senmortulo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi lo&gcirc;is 6 jarojn en <tld/>o, 2 jarojn en Amapao
        <fnt>
          <vrk><url ref="http://www.npoint.com.br/sementeira/Ria.htm">Ria -
          npoint.com.br</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la karnavalo en <tld/>o <klr>[&#x2026;]</klr> aperis slogan&scirc;ildoj kun la vortoj
        &leftquot;2017 estas la &ccirc;ina jaro de koko&rightquot;
        <fnt>
          <vrk><url ref="http://esperanto.china.org.cn/2017-02/27/content_40368339.htm">&Ccirc;inaj
          elementoj en brazila karnavalo - esperanto.china.org.cn</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_R;&c_e;&c_s;&c_y;&c_f;&c_e;</trd>
    <trd lng="de">Recife</trd>
    <trd lng="en">Recife</trd>
    <trd lng="fr">Recife</trd>
    <trd lng="pl">Recife</trd>
    <trd lng="ro">Recife</trd>
  </snc>
  <trd lng="ja">&#x30EC;&#x30B7;&#x30D5;&#x30A7;</trd>
  <trd lng="uk">&c_R;&c_je;&c_s;&c_ib;&c_f;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x7D2F;&#x897F;&#x8153; <pr>l&#xE8;ix&#x12B;f&#xE9;i</pr></trd>,
    <trd>&#x7E8D;&#x897F;&#x8153; <pr>l&#xE8;ix&#x12B;f&#xE9;i</pr></trd>,
    <trd>&#x7D2F;&#x897F;&#x8153; <pr>l&#xE8;ix&#x12B;f&#xE9;i</pr></trd>
  </trdgrp>
</drv>

