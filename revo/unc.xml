<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: unc.xml,v 1.28 2024/05/12 09:20:57 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>unc</rad>/o
   <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="unc.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="unc.0o.MEZ">
    <dif>
      Malgranda <ref tip="super" cel="unu.mezur0o.SCI">mezurunuo</ref>
      de pezo varia la&ubreve; la landoj, valoranta proksimume 20 a&ubreve; 30
      gramojn:
      <ekz>
        &ccirc;i tiu marmelado &scirc;ajnas al mi bona, pesu al mi du <tld/>ojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
         dek <tld/>oj da oro
         <fnt>
           <aut>D. K. Jordan</aut>,
           <vrk>La Avara Muelisto</vrk>,
           <lok><url ref="http://esperanto.ca/kea/j-enkonduko.html"
               >La Fabeloj de Jordan</url>, [vidita en 2006-05-24]</lok>
         </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> &scirc;ipanaro ne havis <tld/>on da viando nek pano dum dek tagoj
        <fnt>
          <bib>Rob</bib>
          <lok>Parto V</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiuj junaj korpoj &ndash; la du knabinoj e&ccirc; ne havis unu <tld/>on da graso
        <fnt>
          <bib>ChV</bib>
          <lok>24</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la ar&hcirc;ivaj raportoj rivelis neniun interesan fakton, <klr>[&#8230;]</klr> nur etajn
        &scirc;telojn, akcidentojn, perdojn sen <tld/>o da mistero
        <fnt>
          <bib>ChB</bib>
          <lok>18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_u;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trd lng="br">o&ntilde;s</trd>
  <trd lng="ca">un&ccedil;a</trd>
  <trd lng="cs">unce</trd>
  <trd lng="de">Unze</trd>
  <trd lng="en">ounce</trd>
  <trd lng="es">onza</trd>
  <trd lng="fi">unssi</trd>
  <trd lng="fr">once</trd>
  <trd lng="he">&alef;&vav;&nun;&qof;&yod;&he;</trd>
  <trd lng="hu">uncia</trd>
  <trd lng="it">oncia</trd>
  <trd lng="ja">&#12458;&#12531;&#12473;</trd>
  <trd lng="nl">ons<klr tip="ind"> (gewicht)</klr></trd>
  <trd lng="pl">uncja</trd>
  <trd lng="pt">on&ccedil;a</trd>
  <trd lng="ru">&c_u;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">unca <klr>(v&aacute;ha)</klr></trd>
  <trd lng="uk">&c_u;&c_n;&c_c;&c_ib;&c_ja; (&c_m;&c_ib;&c_r;&c_a; &c_v;&c_a;&c_g;&c_i;)</trd>
  <trdgrp lng="zh">
    <trd>&#30414;&#21496; <pr>&agrave;ngs&imacron;</pr></trd>,
    <trd>&#21858; <pr>li&#462;ng</pr></trd>,
    <trd>&#23433;&#22763; <pr>&amacron;nsh&igrave;</pr></trd>
  </trdgrp>
</drv>
</art>
