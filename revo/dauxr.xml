  </trdgrp>
  <trdgrp lng="pt">
   <trd>per&iacute;odo de vida</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_r;&c_je;&c_m;&c_ja; &c_zh;&c_i;&c_z;&c_n;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_d;&c_o;&c_l;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;
