<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: salvi.xml,v 1.33 2023/09/03 12:09:44 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>salvi</rad>/o
</kap>
<drv mrk="salvi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="salvi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Salvia</trd>);
      <ref tip="lst" cel="herb.0o" lst="voko:lamioparencoj">herbo</ref>
      a&ubreve; arbusto, ofte kun mallongaj haroj sur &ccirc;iuj superteraj partoj,
      kun simplaj &gcirc;is dividitaj folioj kaj &scirc;ajnverticiloj
      de dulipaj floroj kun du fekundaj stamenoj;
      la vera <tld/>o
      estas kulturata kiel <ref tip="lst" cel="spic.0o" lst="voko:spicoj">spic</ref>-
      kaj kuracherbo:
      <ekz>
        miriko, rosmareno, koriandro, junipero, cinamo, anizo, kariofilo, <tld/>o, la&ubreve;ro
        <fnt>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2001/007144.php">Bierlando Belgio</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la brusto &scirc;veli&gcirc;is spirante la puran aeron, tra kiu solvi&gcirc;is
        la odoro de <tld/>o kaj piceo
        <fnt>
          <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
          <vrk><url ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
          Surprizitaj nuptofestantoj</url></vrk>,
          <lok>inko, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/b/ba/Salvia_spec_LC0058.jpg/640px-Salvia_spec_LC0058.jpg" prm="CC BY-SA     3.0 de"><ind>vera <tld/>o</ind> (Salvia officinalis), Thasos, Grekujo
         <fnt>
           <aut>J&ouml;rg Hempel, propra verko</aut>
           <lok><url ref="&WCW;/File:Salvia_spec_LC0058.jpg">&WCW;/File:Salvia_spec_LC0058.jpg</url></lok>
         </fnt>
      <trd lng="be">&c_sh;&c_a;&c_l;&c_v;&c_je;&c_ja; &c_l;&c_je;&c_k;&c_a;&c_v;&c_a;&c_ja;</trd>
      <trd lng="ca">s&agrave;lvia comuna</trd>
      <trd lng="fa">&a_m;&a_r;&f_y;&a_m;&#x200C;&f_g;&a_l;&f_y; &a_d;&a_A;&a_r;&a_w;&f_y;&f_y;</trd>
      <trd lng="he">&mem;&resh;&vav;&vav;&he; &resh;&pe;&vav;&alef;&yod;&tav;</trd>
      <trdgrp lng="hu">
        <trd>orvosi <ind>zs&aacute;lya</ind></trd>,
        <trd>kerti <ind>zs&aacute;lya</ind></trd>
      </trdgrp>
      <trd lng="it">salvia<klr> comune</klr></trd>
      <trd lng="la">Salvia officinalis</trd>
      <trd lng="ru">&c_sh;&c_a;&c_l;&c_f;&c_je;&c_j; &c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </bld>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_l;&c_v;&c_je;&c_ja; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="ca">s&agrave;lvia</trd>
  <trd lng="de">Salbei</trd>
  <trd lng="fa">&a_m;&a_r;&f_y;&a_m;&#x200C;&f_g;&a_l;&f_y;</trd>
  <trdgrp lng="en">
    <trd>sage</trd>,
    <trd>common sage</trd>
    <trd>garden sage</trd>
  </trdgrp>
  <trd lng="fr">sauge</trd>
  <trd lng="he">&mem;&resh;&vav;&vav;&he;<klr tip="ind"> (&samekh;&vav;&gimel; &tsadi;&mem;&het;)</klr></trd>
  <trd lng="hu">zs&aacute;lya</trd>
  <trd lng="it">salvia</trd>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30EB;&#x30D3;&#x30A2;</trd>,
    <trd>&#x30A2;&#x30AD;&#x30AE;&#x30EA;</trd>,
    <trd>&#x30BB;&#x30FC;&#x30B8;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>salie</trd>,
    <trd>salvia</trd>
  </trdgrp>
  <trd lng="pl">sza&lstroke;wia lekarska</trd>
  <trdgrp lng="pt">
    <trd>s&aacute;lvia</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_a;&c_l;&c_f;&c_je;&c_j; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="sk">&scaron;alvia</trd>
  <trd lng="cs">&scaron;alv&ecaron;j</trd>
  <trd lng="uk">&c_sh;&c_a;&c_v;&#x43B;&c_ib;&#x44F;</trd>
</drv>
</art>
