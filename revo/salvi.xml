<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>salvi</rad>/o
</kap>
<drv mrk="salvi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Salvia</trd>), la vera <tld/>o
      estas kulturata kiel spic- kaj kuracherbo.
    </dif>
  </snc>
  <bld lok="../bld/salvi.jpg">
    <ind>vera <tld/>o</ind>
    <trd lng="la">Salvia officinalis</trd>
    <trd lng="ru">&c_sh;&c_a;&c_l;&c_f;&c_je;&c_j; &c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </bld>
  <trd lng="de">Salbei</trd>
  <trd lng="fr">sauge</trd>
  <trdgrp lng="nl">
    <trd>salie</trd>,
    <trd>salvia</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_a;&c_l;&c_f;&c_je;&c_j; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
</drv>
</art>
