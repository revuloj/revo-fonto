<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>salvi</rad>/o
</kap>
<drv mrk="salvi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="salvi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Vegeta&jcirc;o el mezvarmaj landoj,
      genro el familio <ref tip="super" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Salvia</trd>), la vera <tld/>o
      estas kulturata kiel <ref tip="super" cel="spic.0o">spic</ref>-
      kaj kuracherbo:
      <ekz>
        la brusto &scirc;veli&gcirc;is spirante la puran aeron, tra kiu solvi&gcirc;is la
        odoro de <tld/>o kaj piceo
        <fnt>
        <aut>E. Kumi&ccaron;i&cacute;, trad. S. Rukelj</aut>,
        <vrk><url ref="http://free-vz.t-com.hr/Stanko/Surprizitaj.pdf">
        Surprizitaj nuptofestantoj</url></vrk>,
        <lok>inko, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/salvi.jpg">
    <ind>vera <tld/>o</ind>
    <trd lng="be">&c_sh;&c_a;&c_l;&c_v;&c_je;&c_ja; &c_l;&c_je;&c_k;&c_a;&c_v;&c_a;&c_ja;</trd>
    <trd lng="he">&mem;&resh;&vav;&vav;&he; &resh;&pe;&vav;&alef;&yod;&tav;</trd>
    <trdgrp lng="hu">
      <trd>orvosi <ind>zs&aacute;lya</ind></trd>,
      <trd>kerti <ind>zs&aacute;lya</ind></trd>
    </trdgrp>
    <trd lng="la">Salvia officinalis</trd>
    <trd lng="ru">&c_sh;&c_a;&c_l;&c_f;&c_je;&c_j; &c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </bld>
  <trd lng="be">&c_sh;&c_a;&c_l;&c_v;&c_je;&c_ja; <klr tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
  <trd lng="de">Salbei</trd>
  <trd lng="fr">sauge</trd>
  <trd lng="he">&mem;&resh;&vav;&vav;&he;<klr tip="ind"> (&samekh;&vav;&gimel; &tsadi;&mem;&het;)</klr></trd>
  <trd lng="hu">zs&aacute;lya</trd>
  <trdgrp lng="nl">
    <trd>salie</trd>,
    <trd>salvia</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>s&aacute;lvia</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_a;&c_l;&c_f;&c_je;&c_j; <klr
  tip="amb">(&c_r;&c_o;&c_d;)</klr></trd>
</drv>
</art>
