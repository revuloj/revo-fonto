<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>salvi</rad>/o
</kap>
<drv mrk="salvi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Salvia</trd>), la vera <tld/>o
      estas kulturata kiel spic- kaj kuracherbo.
    </dif>
  </snc>
  <bld lok="../bld/salvi.jpg">
    <ind>vera <tld/>o</ind> 
    <trd lng="la">Salvia officinalis</trd>
  </bld>
  <trd lng="de">Salbei</trd>
  <trd lng="fr">sauge</trd>
  <trdgrp lng="nl">
    <trd>salie</trd>,
    <trd>salvia</trd>
  </trdgrp>
</drv>
</art>
