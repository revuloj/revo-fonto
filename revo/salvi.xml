<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>salvi</rad>/o
</kap>
<drv mrk="salvi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lami.0acoj">lamiacoj</ref>
      (<trd lng="la">Salvia</trd>), la vera <tld/>o
      estas kulturata kiel spic- kaj kuracherbo.
    </dif>
  </snc>
  <bld lok="../bld/salvi.jpg">
    <ind>vera <tld/>o</ind> 
    <trd lng="la">Salvia officinalis</trd>
  </bld>
  <trd lng="de">Salbei</trd>
  <trd lng="fr">sauge</trd>
  <trd lng="nl">salie</trd>
</drv>
</art>
