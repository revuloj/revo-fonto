<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>tungsten</rad>/o
    </kap>
 
    <drv mrk="tungst.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">KEM</uzo>
      <uzo tip="stl">ARK</uzo>
      <snc>
        <ref tip="dif" cel="volfram.0o">volframo</ref>
        <ekz>
          &gcirc;i konsistas el treege akra pinto el <tld/>o, lokata je malmultaj
          anstromoj (dekmiliononoj de milimetro) super la ekzamenota
          surfaco
          <fnt><aut>Gianfranco Cazzaro</aut>:
            <vrk>Mikroskope vastaj horizontoj</vrk>,
            <bib>Monato</bib><lok>jaro 1997a, numero 2a, p. 12a</lok></fnt>.
        </ekz>
      </snc>
      <trd lng="de">Tungsten</trd>
      <trd lng="fr">tungst&egrave;ne</trd>
      <trd lng="hu">volfr&aacute;m</trd>
      <trd lng="nl">tungsteen</trd>
      <trd lng="pt">tungst&ecirc;nio</trd>
    </drv>
  </art>
