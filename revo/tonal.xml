<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tonal</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="tonal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Tona framo de muziko, kun propraj baza tono,
      <ref tip="vid" cel="gam.0o">gamo</ref>, sonserioj, akordoj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>gamme <klr> (tonalit&eacute;)</klr></trd>,
    <trd>ton alit&eacute;</trd>
  </trdgrp>
</drv>
</art>

