<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tonal</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="tonal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Tona framo de muziko, kun propraj baza tono,
      <ref tip="vid" cel="gam.0o">gamo</ref>, sonserioj, akordoj.
    </dif>
  </snc>
  <trd
  lng="be">&c_t;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Tonart</trd>
  <trd lng="es">tonalidad</trd>
  <trdgrp lng="fr">
    <trd>gamme <klr> (tonalit&eacute;)</klr></trd>,
    <trd>tonalit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hangnem</trd>,
    <trd>tonalit&aacute;s</trd>
  </trdgrp>
  <trd lng="pl">tonacja</trd>
  <trdgrp lng="pt">
    <trd>tom</trd>,
    <trd>modo</trd>
  </trdgrp>
  <trd
  lng="ru">&c_t;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>

