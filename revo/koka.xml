<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>koka</rad>/o
</kap>
<drv mrk="koka.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio el la familio 
      <ref tip="super" cel="gerani1.0oj">geraniacoj</ref>
      (<trd lng="la">Erythroxylum coca</trd>), sudamerika arbedo,
      kies folioj enhavas <ref tip="prt" cel="kokain.0o">kokainon</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_k;&c_a;</trd>
  <trd lng="de">Kokastrauch</trd>
  <trd lng="en">coca bush</trd>
  <trd lng="fr">coca<klr tip="ind"> (arbuste)</klr></trd>
  <trd lng="hu">kokacserje</trd>
  <trd lng="nl">coca</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_a;</trd>
</drv>
</art>
