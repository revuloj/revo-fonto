<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: koka.xml,v 1.20 2023/09/01 15:46:40 revo Exp $">
<kap>
  <rad>koka</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="koka.0o">
  <kap><tld/>o</kap>
  <snc mrk="koka.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Sudamerika arbedo, kies folioj enhavas
      <ref tip="prt" cel="kokain.0o">kokainon</ref>;
      specio el la familio 
      <ref tip="super" cel="gerani1.0oj">geraniacoj</ref>
      (<trd lng="la">Erythroxylum coca</trd> kaj
      <trd lng="la">Erythroxylum novogranatense</trd>).
    </dif>
  </snc>
  <snc mrk="koka.0o.KUI">
    <dif>
      <tld lit="K"/>folio ma&ccirc;ata pro sia stimula efiko
      kaj uzata por pluraj kuracaj celoj:
      <ekz>
        la tradicia kuracarto Anda estas utiliginta <tld/>on
        dum jarcentoj por mildigi gastrajn problemojn,
        &ccirc;esigi sangelver&scirc;ojn, fermi vundojn, kaj malakrigi
        doloron
        <fnt>
          <aut>E. Morales, trad. R. Platteau</aut>,
          <vrk><url ref="http://mas-eo.org/spip.php?article59">
          Parolado</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_k;&c_a;</trd>
  <trd lng="bg">&c_k;&c_o;&c_k;&c_a;</trd>
  <trd lng="ca">coca</trd>
  <trd lng="cs">koka</trd>
  <trd lng="de">Koka</trd>
  <trd lng="en">coca</trd>
  <trd lng="fr">coca</trd>
  <trd lng="hu">kokacserje</trd>
  <trd lng="ja">&#x30B3;&#x30AB;</trd>
  <trd lng="nl">coca</trd>
  <trdgrp lng="pl">
    <trd>koka</trd>,
    <trd>krasnodrzew</trd>
  </trdgrp>
  <trd lng="ro">coca</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_a;</trd>
  <trd lng="sk">koka</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43A;&#x430;</trd>,
    <trd>&#x43A;&#x43E;&#x43A;&#x430;&#x457;&#x43D;&#x43E;&#x432;&#x435; &#x434;&#x435;&#x440;&#x435;&#x432;&#x43E;</trd>,
    <trd>&#x43A;&#x43E;&#x43A;&#x430;&#x457;&#x43D;&#x43E;&#x432;&#x438;&#x439; &#x43A;&#x443;&#x449;</trd>
  </trdgrp>
</drv>
</art>
