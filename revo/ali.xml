<drv mrk="ali.0o">
  <kap><tld/>o</kap>
  <snc mrk="ali.0o.KOMUNE">
    <dif>
      Io <tld/>a:
      <ekz>
         tio kaj <tld/>o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        nek io, nek <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sed <tld/>on volis la sorto
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fingreto</lok>
        </fnt>!
      </ekz>
     </dif>
     <trdgrp lng="th">
       <trd>&#xE2A;&#xE34;&#xE48;&#xE07;&#xE2D;&#xE37;&#xE48;&#xE19;</trd>,
       <trd>&#xE2D;&#xE22;&#xE48;&#xE32;&#xE07;&#xE2D;&#xE37;&#xE48;&#xE19;</trd>
     </trdgrp>
   </snc>
  <trd lng="be">&c_ib;&c_n;&c_sh;&c_a;&c_je;</trd>
  <trdgrp lng="bg">
     <trd>&c_n;&c_je;&c_shch;&c_o;
     <ind>&c_d;&c_r;&c_u;&c_g;&c_o;</ind></trd>,
     <trd>&c_o;&c_shch;&c_je; &c_n;&c_je;&c_shch;&c_o;</trd>
   </trdgrp>
  <trdgrp lng="ca">
     <trd>una altra cosa</trd>,
     <trd>el contrari <klr>(se kontra&Aring;&#xAD;a)</klr></trd>
   </trdgrp>
  <trd lng="cs">n&ecaron;co jin&eacute;ho</trd>
  <trd lng="en">something else</trd>
  <trd lng="es">otra cosa</trd>
  <trdgrp lng="fi">
     <trd><klr tip="amb">(jotakin) </klr>muuta</trd>,
     <trd>muu<klr> (asia)</klr></trd>
   </trdgrp>
  <trd lng="fr">autre chose</trd>
  <trd lng="nl">iets <ind>anders</ind></trd>
  <trd lng="no">noe <ind>annet</ind></trd>
  <trdgrp lng="pl">
     <trd>co innego</trd>,
     <trd>co&sacute; innego</trd>
   </trdgrp>
  <trdgrp lng="pt">
     <trd>outra coisa</trd>,
     <trd>al <klr>(ark.)</klr></trd>
   </trdgrp>
  <trd lng="ru">&c_d;&c_r;&c_u;&c_g;&c_o;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>in&eacute;</trd>,
    <trd>nie&ccaron;o in&eacute;</trd>
  </trdgrp>
</drv>

<drv mrk="ali.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ali.0igi.KOMUNE">
     <dif>
       Fari ion <tld/>a, <ref tip="sin" cel="sxangx.0i.modifi">&scirc;an&gcirc;i</ref>,
       <ref tip="sin" cel="modif.0i">modifi</ref>,
       <ref cel="form.ali0i.KOMUNE">aliformi</ref>:
       <ekz>
         por ke oni ne rekonu
         lin, li <tld/>igis sian vo&ccirc;on;
       </ekz>
      <ekz>
        tio estas nur kontuzo?... kiel &gcirc;i <tld/>igas la viza&gcirc;on!
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio ne <tld/>igis la bonan humoron de aliaj: ili batalis kun malica &gcirc;ojo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        per magneto oni <tld/>igas la energistaton de tiuj vapori&gcirc;intaj atomoj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007024.html">Tempo: Pli ekzakte ne eblas: Radi-regata horlo&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la multjarcenta historio multon <tld/>igis
        <fnt>
          <bib>Monato</bib>
          <aut>Mihail Korotkov</aut>
          <vrk><url ref="https://www.monato.be/2013/010760.php">Plej granda invento de la homaro</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="th">
       <trd>&#xE17;&#xE33;&#xE43;&#xE2B;&#xE49;&#xE40;&#xE1B;&#xE47;&#xE19;&#xE2D;&#xE22;&#xE48;&#xE32;&#xE07;&#xE2D;&#xE37;&#xE48;&#xE19;</trd>,
       <trd>&#xE40;&#xE1B;&#xE25;&#xE35;&#xE48;&#xE22;&#xE19;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
     <trd>&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_mol;</trd>,
     <trd>&c_z;&c_mol;&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_mol;</trd>
   </trdgrp>
  <trd lng="bg">&c_i;&c_z;&c_m;&c_je;&c_n;&c_ja;&c_v;&c_a;&c_m;</trd>
  <trd lng="ca">transformar</trd>
  <trd lng="es">transformar</trd>
  <trdgrp lng="cs">
    <trd><klr>(po)</klr>zm&ecaron;nit</trd>,
    <trd>m&ecaron;nit</trd>,
    <trd>p&rcaron;em&ecaron;nit</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>&auml;ndern</trd>,
     <trd>ver&auml;ndern</trd>,
     <trd>ab&auml;ndern</trd>
   </trdgrp>
  <trd lng="en">modify</trd>
  <trdgrp lng="fi">
     <trd>muuttaa<klr tip="amb"> (toiseksi, toisenlaiseksi)</klr>
     </trd>
   </trdgrp>
  <trd lng="fr">alt&eacute;rer</trd>
  <trdgrp lng="nl">
     <trd>veranderen</trd>,
     <trd>aanpassen</trd>
   </trdgrp>
  <trd lng="no">endre</trd>
  <trdgrp lng="pl">
     <trd>zmienia&cacute;</trd>,
     <trd>przeinacza&cacute;</trd>,
     <trd>przemienia&cacute;</trd>
   </trdgrp>
  <trdgrp lng="pt">
     <trd>modificar</trd>,
     <trd>mudar</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>,
     <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>,
     <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>pozmeni&tcaron;</trd>,
    <trd>upravi&tcaron;</trd>,
    <trd>zmeni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="ali.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ali.0igxi.KOMUNE">
    <dif>
      Fari&gcirc;i <tld/>a:
      <ekz>
        dum li pre&gcirc;is, la aspekto de lia viza&gcirc;o <tld/>i&gcirc;is
        <fnt>
          <bib>NT</bib>
          <lok>Luko 9:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io &scirc;an&gcirc;i&gcirc;as, &ccirc;io <tld/>i&gcirc;as
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la publiko komencis ridi, sed subite &gcirc;ia &gcirc;ojo <tld/>i&gcirc;is en krion de teruro
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malla&ubreve;ta kolero rapide <tld/>i&gcirc;is en malla&ubreve;tajn interkonsili&gcirc;ojn, kiuj e&ccirc;
        similis konspiron
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="sxangx.0igxi">&scirc;an&gcirc;i&gcirc;i</ref>
    <trd lng="th">&#xE01;&#xE25;&#xE32;&#xE22;&#xE40;&#xE1B;&#xE47;&#xE19;&#xE2D;&#xE22;&#xE48;&#xE32;&#xE07;&#xE2D;&#xE37;&#xE48;&#xE19;</trd>
  </snc>
   <trd lng="be">&c_z;&c_mol;&c_m;&c_ja;&c_n;&c_ib;&c_c;&c_c;&c_a;</trd>
   <trdgrp lng="de">
     <trd><klr>sich </klr>&auml;ndern</trd>,
     <trd><klr>sich </klr>ver&auml;ndern</trd>,
     <trd>anders werden</trd>
   </trdgrp>
   <trd lng="ca">transformar-se</trd>
   <trd lng="es">transformarse</trd>
   <trd lng="en">undergo modification</trd>
   <trdgrp lng="fi">
     <trd>muuttua <klr tip="amb"> (toiseksi, toisenlaiseksi)</klr>
     </trd>
   </trdgrp>
   <trd lng="fr">s'<ind>alt&eacute;rer</ind></trd>
   <trd lng="nl">veranderen</trd>
   <trd lng="no">endre seg</trd>
   <trdgrp lng="pl">
     <trd>zmienia&cacute; si&eogonek;</trd>,
     <trd>przemienia&cacute; si&eogonek;</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>modificar-se</trd>,
     <trd>mudar-se</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;
     </trd>
   </trdgrp>
   <trd lng="sk">zmeni&tcaron; sa</trd>
</drv>

<drv mrk="ali.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="ali.0ulo.KOMUNE">
    <dif>
      <tld lit="A"/>a homo;
      <ref tip="sin" cel="ali.0a.pron">alia<sncref/></ref>:
      <ekz>
        lasante sian bruton, ke &gcirc;i difektu kampon de <tld/>ulo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 22:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        sekreton de <tld/>ulo ne malka&scirc;u
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 25:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        iliaj domoj transiros al <tld/>uloj
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 6:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        agu kun <tld/>uloj tiel, kiel vi dezirus, ke <tld/>uloj agu kun vi
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        diras la proverbo, ke <tld/>ula sperto lernigas neniun
        <fnt>
          <aut>-, trad. B. Ragnarsson</aut>,
          <vrk>Torstein la Stangofrapito</vrk>,
          <lok>en: Islandaj Pravo&ccirc;oj, 1964</lok>
        </fnt>.
      </ekz>      
    </dif>
    <trd lng="th">&#xE04;&#xE19;&#xE2D;&#xE37;&#xE48;&#xE19;</trd>
  </snc>
  <trd lng="cs">n&ecaron;kdo jin&yacute;</trd>
  <trd lng="fr">quelqu'un d'autre</trd>
  <trd lng="he">&#x5DE;&#x5D9;&#x5E9;&#x5D4;&#x5D5; &#x5D0;&#x5D7;&#x5E8;</trd>
  <trd lng="sk">niekto in&yacute;</trd>
</drv>

<drv mrk="ali.inter0e">
  <kap>inter<tld/>e</kap>
  <snc mrk="ali.inter0e.KOMUNE">
    <dif>
      Inter <tld/>aj aferoj:
      <ekz>
        inter<tld/>e la interparolado ektu&scirc;is anka&ubreve; la epokon de la mezaj jarcentoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        terure multe da paka&jcirc;o li trenis kun si, inter<tld/>e anka&ubreve; anglan plugilon
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;torino esploris kelkajn en-esperantigojn, inter<tld/>e en la libro &leftquot;Nuda
        inter lupoj&rightquot; tradukita el la germana
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006813.html">Sa&gcirc;a kapo duonvorton komprenas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="th">
      <trd>&#xE17;&#xE48;&#xE32;&#xE21;&#xE01;&#xE25;&#xE32;&#xE07;&#xE2A;&#xE34;&#xE48;&#xE07;&#xE2D;&#xE37;&#xE48;&#xE19;</trd>,
      <trd>&#xE17;&#xE48;&#xE32;&#xE21;&#xE01;&#xE25;&#xE32;&#xE07;&#xE40;&#xE23;&#xE37;&#xE48;&#xE2D;&#xE07;&#xE2D;&#xE37;&#xE48;&#xE19;</trd>
    </trdgrp>
    </snc>
  <trd lng="be">&c_p;&c_a;&c_m;&c_ib;&c_zh;
  &c_ib;&c_n;&c_sh;&c_y;&c_m;</trd>
  <trd lng="bg">&c_m;&c_je;&c_zh;&c_d;&c_u;
  <ind>&c_d;&c_r;&c_u;&c_g;&c_o;&c_t;&c_o;</ind></trd>
  <trd lng="ca">entre d'altres <klr>(coses)</klr></trd>
  <trdgrp lng="cs">
    <trd>mezi jin&yacute;m</trd>,
    <trd>mimo jin&eacute;</trd>,
    <trd>mimochodem</trd>
  </trdgrp>
  <trd lng="de">unter <ind>anderem</ind></trd>
  <trdgrp lng="en">
    <trd>among others</trd>,
    <trd>inter alia</trd>
  </trdgrp>
  <trd lng="es">entre otros</trd>
  <trd lng="fi">muun muassa</trd>
  <trdgrp lng="fr">
    <trd>entre <ind>autres</ind></trd>,
    <trd>notamment</trd>
  </trdgrp>
  <trd lng="he">&bet;&yod;&fnun; &he;&shin;&alef;&resh;</trd>
  <trd lng="nl">onder <ind>andere</ind></trd>
  <trd lng="no">blant <ind>annet</ind></trd>
  <trd lng="pl">mi&eogonek;dzy <ind>innymi</ind></trd>
  <trd lng="pt">entre <ind>otras</ind></trd>
  <trd lng="ru">&c_m;&c_je;&c_zh;&c_d;&c_u;
  &c_p;&c_r;&c_o;&c_ch;&c_i;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>medzi in&yacute;m</trd>,
    <trd>mimochodom</trd>,
    <trd>okrem <ind>in&eacute;ho</ind></trd>
  </trdgrp>
</drv>

<drv mrk="ali.kun0e">
  <kap>kun<tld/>e</kap>
  <snc mrk="ali.kun0e.KOMUNE">
    <dif>
      Sendistinge kun <tld/>a&jcirc;oj:
      <ekz>
        kredeble mi metis &gcirc;in kun<tld/>e en mian
        pakon
        <fnt>
          <bib>Fab3</bib>
          <lok>Folio el la &ccirc;ielo</lok>
        </fnt>;
      </ekz>
      <ekz>
        feino, kies perlo kun<tld/>e apartenas al la plej bonaj donoj de la vivo
        <fnt>
          <bib>Fab3</bib>
          <lok>Lasta perlo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">amb altres</trd>
  <trd lng="es">con otras cosas</trd>
  <trd lng="cs">spolu s n&ecaron;&ccaron;&iacute;m jin&yacute;m</trd>
  <trdgrp lng="de">
    <trd>neben <ind>anderem</ind></trd>,
    <trd>neben <ind>anderen</ind></trd>,
    <trd><klr>neben </klr>anderen <ind>Dingen</ind></trd>
  </trdgrp>
  <trd lng="en">with others</trd>
  <trdgrp lng="fi">
    <trd>muun mukana</trd>
    <trd>muiden (tavaroiden) mukana</trd>
  </trdgrp>
  <trd lng="fr">sans distinction</trd>
  <trd lng="pt"><ind>junto</ind> com outras coisas</trd>
  <trd lng="sk">spolu s nie&ccaron;&iacute;m in&yacute;m</trd>
</drv>
