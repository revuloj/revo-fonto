<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: vuvuzel.xml,v 1.3 2021/04/23 07:04:20 revo Exp $">
<kap><rad>vuvuzel</rad>/o</kap>
<drv mrk="vuvuzel.0o">
  <kap><tld/>o</kap>
  <snc mrk="vuvuzel.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Afrikdevena trumpetforma <ref tip="vid"
      cel="korn.0o.MUZ">korno<sncref/></ref>, nuntempe kutime plasta,
      kiu produktas la&ubreve;tan zumadon:
      <ekz>
        la <tld/>o estas karakteriza&jcirc;o de la du grandaj sud-afrikaj
        futbalteamoj
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/wiki/Vuvuzelo">Vuvuzelo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        komentistoj diversmaniere priskribis la sonon de <tld/>o kiel
        "&gcirc;ena" kaj "satana" kaj komparis &gcirc;in kun "arego da elefantoj",
        "surdiga akridosvarmo", "kapro survoje al bu&ccirc;i&gcirc;o", "giganta
        abelujo plena de koleraj abeloj", kaj "amfetamine drogita anaso"
        <fnt>
          <aut>Tatoeba</aut>,
          <vrk><url
          ref="https://tatoeba.org/epo/sentences/show/461769">Frazo
          461769a</url></vrk>,
          <lok>[vidita en 2020]</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/3/37/Vuvuzela_red.jpg/480px-Vuvuzela_red.jpg"
    prm="CC BY-SA 3.0">
      Moderna plasta <tld/>o
      <fnt>
        <aut>Berndt Meyer, propra verko</aut>
        <lok><url
        ref="&WCW;/File:Vuvuzela_red.jpg">https://commons.wikimedia.org/wiki/File:Vuvuzela_red.jpg</url></lok>
      </fnt>
    </bld>
  </snc>
  <trd lng="af">vuvuzela</trd>
  <trd lng="be">&c_v;&c_u;&c_v;&c_u;&c_z;&c_e;&c_l;&c_a;</trd>
  <trd lng="de">Vuvuzela</trd>
  <trd lng="en">vuvuzela</trd>
  <trd lng="es">vuvuzela</trd>
  <trd lng="fr">vuvuzela</trd>
  <trd lng="hu">vuvuzela</trd>
  <trd lng="it">vuvuzela</trd>
  <trd lng="nl">vuvuzela</trd>
  <trd lng="no">vuvuzela</trd>
  <trd lng="pt">vuvuzela</trd>
  <trd lng="ru">&c_v;&c_u;&c_v;&c_u;&c_z;&c_je;&c_l;&c_a;</trd>
  <trd lng="sv">vuvuzela</trd>
  <trd lng="uk">&c_v;&c_u;&c_v;&c_u;&c_z;&c_je;&c_l;&c_a;</trd>
</drv>
</art>

