<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Isaak</rad>/o
    </kap>

<drv mrk="isaak.0o">
  <kap><tld/>o</kap>
  <snc mrk="isaak.0o.nomo">
    <uzo tip="fak">NOM</uzo>   
    <dif>
      Vira <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">persona nomo</ref> hebredevena:
      <ekz>
        <tld/>o <ref cel="neuxto.0o">Ne&ubreve;tono</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1803"
              >Isaako Ne&ubreve;tono</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la unuan templon dedi&ccirc;itan al sanktulo <tld/>o de Dalmatio (monako predikinta en
        la 4-a jarcento) establis caro Petro la Granda
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=428911"
              >Katedralo Sankta Isaako de Sankt Peterburgo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">BIB</uzo>
    <dif>
      Filo de <ref cel="abraham.0o.prapatro">Abrahamo</ref> 
      kaj patro de <ref cel="jakob.0o.BIB">Jakobo</ref>:
      <ekz>
        <nom>Abraham</nom> naskigis <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 1:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        prenu vian filon, vian solan, kiun vi amas, <tld/>on, kaj <klr>[&#8230;]</klr> oferu
        lin tie kiel bruloferon sur unu el la montoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 22:2</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Isaak</trd>
</drv>

  </art>

<!--
$Log$
-->
</vortaro>