<drv mrk="blank.0o">
  <kap><tld/>o</kap>
  <snc mrk="blank.0o.koloro">
    <dif>
      <tld lit="B"/>a
      <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>
      a&ubreve; kolorilo:
      <ekz>
        la malhela verdo de la cipresoj bele kontrastis la <tld/>on de la muroj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fotografaparato pentras la <tld/>on nigra, sed nur tiel povas elformi&gcirc;i
        bildo, ke la nigran &gcirc;i desegnas reciproke <tld/>a
        <fnt>
          <bib>VaK</bib>
          <lok>Oka &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu kompreno sufi&ccirc;is por ke &scirc;i pali&gcirc;u al mort-aspekta <tld/>o
        <fnt>
          <bib>ChV</bib>
          <lok>18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vestita en <tld/>o
        <fnt>
          <bib>VRA</bib>
          <lok>Bj&ouml;rn Erik H&ouml;ijer</lok>
        </fnt>;
      </ekz>
      <ekz>
        nigro <klr>(inko, tu&ccirc;o)</klr> sur <tld/>o <klr>(papero)</klr> pruvas sen manko
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kolor.sen0a">senkolora</ref>
    <trd lng="pl">biel</trd>
    <trd lng="ro">alb</trd>
  </snc>
  <snc mrk="blank.0o.blankajxo">
    <dif>
      Io <tld/>a; <ref tip="dif" cel="blank.0ajxo">blanka&jcirc;o</ref>:
      <ekz>
        la knaboj devis <klr>[&#x2026;]</klr> alporti ovon, kaj per &gcirc;ia <tld/>o li &scirc;miris la
        birdon de &ccirc;iuj flankoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj turni&gcirc;is supren, montrante la <tld/>on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Wei&szlig;es</trd>
  </snc>
  <snc mrk="blank.0o.ludanto">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Unu el la du ludantoj en ludoj kiel
      <ref cel="sxak.0o">&scirc;ako</ref>,
      <ref cel="go.0o">goo</ref>,
      <ref cel="dam.0oj0ludo">damoj</ref>,
      ks:
      <ekz>
        <tld/>o komencas la ludon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4074"
              >&Scirc;ako</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">bia&lstroke;e</trd>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_mol;</trd>
  <trd lng="br">gwenn</trd>
  <trd lng="ca">blanc<klr tip="amb"> (color)</klr></trd>
  <trdgrp lng="cs">
    <trd>b&ecaron;loba</trd>,
    <trd>b&ecaron;lost</trd>
  </trdgrp>
  <trd lng="de">Wei&szlig;</trd>
  <trd lng="en">white</trd>
  <trd lng="es">blanco<klr tip="amb"> (color)</klr></trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_f;&f_y;&a_d;<klr tip="ind"> (&a_r;&a_n;&f_g;)</klr></trd>,
    <trd>&a_s;&a_f;&f_y;&a_d;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">blanc <klr>(couleur)</klr></trd>
  <trd lng="hu">feh&eacute;r sz&iacute;n</trd>
  <trd lng="id">putih</trd>
  <trd lng="ku">sp&icirc;</trd>
  <trd lng="nl">het <ind>wit</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_l;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_b;&c_je;&c_l;&c_i;&c_z;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>belos&tcaron;</trd>,
    <trd>biela farba</trd>,
    <trd>bieloba</trd>
  </trdgrp>
  <trd lng="vo">viet</trd>
</drv>

<drv mrk="blank.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="blank.0ajxo.KOMUNE">
    <dif>
      Io <tld/>a:
      <ekz>
        la infekta&jcirc;o aliformi&gcirc;is en <tld/>a&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis malvarma, seka, severa homo, en griza&jcirc;o kaj <tld/>a&jcirc;o
        <klr>(en grizaj kaj blankaj vestoj)</klr>
        <fnt>
          <aut>C. Dickens, trad. L. L. Zamenhof</aut>
          <vrk>La batalo de l' vivo</vrk>
          <lok>1891</lok>
        </fnt>;
      </ekz>
      <ekz>
        la brilanta <tld/>a&jcirc;o ne estis la muro a&ubreve; la plafono, tio estis la grandaj
        flugiloj sur la &scirc;ultroj de an&gcirc;elo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Ccirc;e la plej ekstrema maro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_o;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>b&ecaron;loba</trd>,
    <trd>b&ecaron;lost</trd>
  </trdgrp>
  <trd lng="en">white</trd>
  <trd lng="fr">blanc<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="he">
    <trd>&lamed;&vav;&bet;&fnun;</trd>,
    <trd>&tsadi;&het;&vav;&resh;</trd>
  </trdgrp>
  <trd lng="hu">feh&eacute;rs&eacute;g<klr tip="ind"> (feh&eacute;r valami)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x767D;  <pr>&#x306F;&#x304F;</pr></trd>,
    <trd>&#x767D;&#x8272;  <pr>&#x3057;&#x308D;&#x3044;&#x308D;</pr></trd>
  </trdgrp>
  <trd lng="nl">iets <ind>wit</ind>s</trd>
  <trdgrp lng="pl">
    <trd>rzecz <ind>bia&lstroke;a</ind></trd>,
    <trd>znak niedrukowalny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_ch;&c_t;&c_o; <ind>&c_b;&c_je;&c_l;&c_o;&c_je;
    </ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>belos&tcaron;</trd>,
    <trd>biela farba</trd>,
    <trd>bieloba</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_ib;&c_l;&c_je;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_i;&c_j; &c_k;&c_o;&c_l;&c_ib;&c_r;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_a; &c_f;&c_a;&c_r;&c_b;&c_a;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_i;&c_l;&c_o;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="vo">din vietik</trd>
</drv>

<drv mrk="blank.0eta">
  <kap><tld/>eta</kap>
  <snc mrk="blank.0eta.KOL">
    <dif>
      Dube<tld/>a, duon<tld/>a:
      <ekz>
        <tld/>eta lumo penetris de ekstere
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eta &scirc;a&ubreve;mo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eta rebrilo <klr>[&#x2026;]</klr> falis sur la supran parton de &scirc;ia viza&gcirc;o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eta helo, &parsl;
        En orienta flanko, montras sur &ccirc;ielo &parsl;
        Kie suno levi&gcirc;is
        <fnt>
          <aut>A. Mickiewicz, trad. A. Grabowski</aut>
          <vrk>Sinjoro Tadeo</vrk>
          <lok>1918</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x6709;&#x4E9B;&#x82CD;&#x767D; <pr>y&#x1D2;uxi&emacron;c&amacron;ngb&aacute;i</pr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_l;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_l;&c_ja;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>arwenn</trd>,
    <trd>damwenn</trd>
  </trdgrp>
  <trd lng="ca">blanquin&oacute;s</trd>
  <trd lng="cs">blanketov&yacute;</trd>
  <trd lng="de">wei&szlig;lich</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_f;&f_y;&a_d;&f_g;&a_w;&a_n;</trd>,
    <trd>&a_sh;&f_y;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">blanch&acirc;tre</trd>
  <trd lng="hu">feh&eacute;res</trd>
  <trd lng="id">ke<ind>putih</ind>-putihan</trd>
  <trd lng="ja">&#x767D;&#x3063;&#x307D;&#x3044;  <pr>&#x3057;&#x308D;&#x3063;&#x307D;&#x3044;</pr></trd>
