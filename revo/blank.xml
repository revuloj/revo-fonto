<?xml version="1.0"?>

<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>blank</rad>/a
</kap>

<drv mrk="blank.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta koloron de ne&gcirc;o, eburo a&ubreve; lakto:
      <ekz>
        lavu min, kaj mi estos pli <tld/>a ol ne&gcirc;o
        <fnt><bib>MT</bib> <lok>&Psa; 51:7</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">
  &a_A_hamza_sure;&a_fatha;&a_b;&a_sukun;&a_y;&a_fatha;&a_D;&a_damma;</trd>
  <trd lng="be">&c_b;&c_je;&c_l;&c_y;</trd>
  <trd lng="bg">&c_b;&c_ja;&c_l;</trd>
  <trd lng="br">gwenn</trd>
  <trd lng="cs">b&iacute;l&yacute;</trd>
  <trd lng="de">wei&szlig;</trd>
  <trd lng="en">white</trd>
  <trd lng="es">blanco</trd>
  <trd lng="fa">&a_s;&a_f;&f_y;&a_d;</trd>
  <trd lng="fr">blanc <klr>(adj.)</klr></trd>
  <trd lng="hu">feh&eacute;r</trd>
  <trd lng="ja">
  &#30333;&#12356; [<ind>&#12375;&#12429;&#12356;</ind>]</trd>
  <trd lng="nl">wit</trd>
  <trd lng="pl">bia&lstroke;y</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_y;&c_j;</trd>
  <trd lng="sk">biely</trd>
  <trd lng="sv">vit</trd>
  <trd lng="tp">walo</trd>
  <trdgrp lng="tr">
    <trd>beyaz</trd>,
    <trd>ak</trd>
  </trdgrp>
  <trd lng="vo">vietik</trd>
</drv>

<drv mrk="blank.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="B"/>a <ref tip="super" cel="kolor.0o.propre">koloro
      </ref> a&ubreve; kolorilo.
    </dif>
    <trd lng="pl">biel</trd>
  </snc>
  <snc mrk="blank.0o.ludanto">
    <dif>
      Unu el la du ludantoj en ludoj kiel <ref cel="sxak.0o">&scirc;ako
      </ref>,
      <ref cel="go.0o">goo</ref>, <ref cel="dam.0oj0ludo">damoj</ref>, 
      ktp.
    </dif>
    <trd lng="pl">bia&lstroke;y</trd>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_mol;</trd>
  <trd lng="br">gwenn</trd>
  <trd lng="de">Wei&szlig;</trd>
  <trd lng="en">white</trd>
  <trd lng="es">blanco</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_f;&f_y;&a_d;<klr tip="ind"> (&a_r;&a_n;&f_g;)</klr></trd>,
    <trd>&a_s;&a_f;&f_y;&a_d;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">blanc <klr>(couleur)</klr></trd>
  <trd lng="hu">feh&eacute;r sz&iacute;n</trd>
  <trd lng="nl">het <ind>wit</ind></trd>
  <trd lng="vo">viet</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_l;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>,
    <trd>&c_b;&c_je;&c_l;&c_i;&c_z;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="ku">sp&icirc;</trd>
</drv>

<drv mrk="blank.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <snc>
    <dif>
      Io <tld/>a:
      <ekz>
        vestita en griza&jcirc;o kaj <tld/>a&jcirc;o;
        <trdgrp lng="pl">
          <trd>ubrany na szaro i bia&lstroke;o</trd>,
          <trd>ubrany w szaro&sacute;ci i biel</trd>
       </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_o;&c_k;</trd>
  <trd lng="en">white</trd>
  <trd lng="fr">blanc<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">feh&eacute;rs&eacute;g<klr tip="ind"> 
  (feh&eacute;r valami)</klr></trd>
  <trd lng="nl">iets <ind>wit</ind>s</trd>
  <trd lng="vo">din vietik</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_ch;&c_t;&c_o; <ind>&c_b;&c_je;&c_l;&c_o;&c_je;
    </ind></trd>
  </trdgrp>
</drv>

<drv mrk="blank.ov0o">
  <kap>ov<tld/>(a&jcirc;)o</kap>
  <snc>
    <dif>
      La <tld/>a parto de ovo,
      <ref tip="sin" cel="albumi.0o">albumino</ref>:
      <ekz>
        ov<tld/>a&jcirc;o <klr>(senkolora, <tld/>i&gcirc;anta per 
        kuirado, albumina parto de ovo)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fa">&a_s;&a_f;&f_y;&a_d;&a_h;&a_hamza_sure; &a_t;&a_kh;&a_m;&zwnj;&a_m;&a_r;&a_gh;</trd>
  <trd lng="pl">bia&lstroke;ko</trd>
  <trd lng="nl">eiwit</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_l;&c_o;&c_k;<klr tip="amb"> 
    (&c_ja;&c_j;&c_c;&c_a;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="blank.0eta">
  <kap><tld/>eta</kap>
  <snc>
    <dif>
      Dube<tld/>a, duon<tld/>a.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_l;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_l;&c_ja;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>arwenn</trd>,
    <trd>damwenn</trd>
  </trdgrp>
  <trd lng="de">wei&szlig;lich</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_f;&f_y;&a_d;&f_g;&a_w;&a_n;</trd>,
    <trd>&a_sh;&f_y;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">blanch&acirc;tre</trd>
  <trd lng="hu">feh&eacute;res</trd>
  <trd lng="nl">gebroken <ind>wit</ind></trd>
  <trd lng="pl">bia&lstroke;awy</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_o;&c_v;&c_a;&c_t;&c_y;&c_j;</trd>
  <trd lng="tr">beyaz&inodot;ms&inodot;</trd>
  <trd lng="vo">vietilik</trd>
</drv>

<drv mrk="blank.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari ion <tld/>a:
      <ekz>
        nigran kornikon sapo ne <tld/>igos
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>igi muron;
      </ekz>
      <ekz>
        <tld/>igi kanabtolon &ccirc;e suno;
      </ekz>
      <ekz>
        de zorgoj, ne de jaroj, <tld/>i&gcirc;as la
        haroj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">gwenna&ntilde;</trd>
  <trdgrp lng="de">
    <trd>wei&szlig;en</trd>, <trd>bleichen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>whiten</trd>, <trd>bleach</trd>
  </trdgrp>
  <trd lng="es">blanquear</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_f;&f_y;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_b;&f_y;&zwnj;&a_r;&a_n;&f_g; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">blanchir</trd>
  <trd lng="hu">feh&eacute;r&iacute;t</trd>
  <trdgrp lng="nl">
    <trd>witten</trd>,
    <trd>wit maken</trd>
  </trdgrp>
  <trd lng="pl">wybiela&cacute;</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">bieli&tcaron;</trd>
</drv>

<drv mrk="blank.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo el <tld/>ha&ubreve;ta raso:
      <ekz>
        la soleca vivo de <tld/>a virino for de aliaj <tld/>uloj
        en kabano <klr>[...]</klr> estis tre suspekteme rigardata
        de la loka lo&gcirc;antaro
        <fnt><aut>Wim M. A. De Smet</aut>:
        <vrk>Kiu murdis Dian Fossey?</vrk>,
        <bib>Monato</bib><lok>jaro 1995a, numero
        12a, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_y;<klr> (&c_n;&c_a;&c_z;.)</klr>
  </trd>
  <trd lng="br">den <ind>gwenn</ind></trd>
  <trd lng="de">Wei&szlig;er</trd>
  <trd lng="es">blanco</trd>
  <trd lng="fa">&a_s;&a_f;&f_y;&a_d;&f_p;&a_w;&a_s;&a_t;</trd>
  <trd lng="fr">blanc <klr>(homme)</klr></trd>
  <trd lng="hu">feh&eacute;r ember</trd>
  <trd lng="ja">
  &#30333;&#20154; [<ind>&#12399;&#12367;&#12376;&#12435;</ind>]</trd>
  <trd lng="nl">blanke</trd>
  <trd lng="pl">bia&lstroke;y</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_y;&c_j;<klr tip="amb"> 
  (&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="sk">beloch</trd>
  <trd lng="tr">beyaz</trd>
  <trd lng="vo">vietel</trd>
</drv>
</art>
