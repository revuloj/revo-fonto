<drv mrk="blank.0ajxo">
  <kap><tld/>a&jcirc;o, <var><kap><tld/>o</kap></var></kap>
  <snc mrk="blank.0ajxo.KOMUNE">
    <dif>
      Io <tld/>a:
      <ekz>
        li estis malvarma, seka, severa homo, en griza&jcirc;o kaj <tld/>a&jcirc;o
        <klr>(en grizaj kaj blankaj vestoj)</klr>
        <fnt>
          <aut>C. Dickens, trad. L. L. Zamenhof</aut>
          <vrk>La batalo de l' vivo</vrk>
          <lok>1891</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj okuloj turni&gcirc;is supren, montrante la <tld/>on
        <fnt>
          <bib>Far1</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_o;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>b&ecaron;loba</trd>,
    <trd>b&ecaron;lost</trd>
  </trdgrp>
  <trd lng="en">white</trd>
  <trd lng="fr">blanc<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="he">
    <trd>&lamed;&vav;&bet;&fnun;</trd>,
    <trd>&tsadi;&het;&vav;&resh;</trd>
  </trdgrp>
  <trd lng="hu">feh&eacute;rs&eacute;g<klr tip="ind"> (feh&eacute;r valami)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#30333;  <pr>&#12399;&#12367;</pr></trd>,
    <trd>&#30333;&#33394;  <pr>&#12375;&#12429;&#12356;&#12429;</pr></trd>
  </trdgrp>
  <trd lng="nl">iets <ind>wit</ind>s</trd>
  <trdgrp lng="pl">
    <trd>rzecz <ind>bia&lstroke;a</ind></trd>,
    <trd>znak niedrukowalny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_ch;&c_t;&c_o; <ind>&c_b;&c_je;&c_l;&c_o;&c_je;
    </ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>belos&tcaron;</trd>,
    <trd>biela farba</trd>,
    <trd>bieloba</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_ib;&c_l;&c_je;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_i;&c_j; &c_k;&c_o;&c_l;&c_ib;&c_r;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_a; &c_f;&c_a;&c_r;&c_b;&c_a;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_i;&c_l;&c_o;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="vo">din vietik</trd>
</drv>

