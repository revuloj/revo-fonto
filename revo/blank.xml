<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>blank</rad>/a
</kap>
<drv mrk="blank.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta koloron de ne&gcirc;o, eburo a&ubreve; lakto. 
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_y;</trd>
  <trd lng="cs">b&iacute;l&yacute;</trd>
  <trd lng="de">wei&szlig;</trd>
  <trd lng="en">white</trd>
  <trd lng="nl">wit</trd>
  <trd lng="fr">blanc <klr tip="ind">(adj.)</klr></trd>
  <trd lng="pl">bia&lstroke;y</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_y;&c_j;</trd>
  <trd lng="sv">vit</trd>
  <trdgrp lng="tr">
   <trd>beyaz</trd>,
   <trd>ak</trd>
  </trdgrp>
</drv>
<drv mrk="blank.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="B"/>a <ref tip="super" cel="kolor.0o">koloro</ref>
      a&ubreve; kolorilo.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_mol;</trd>
  <trd lng="de">Wei&szlig;</trd>
  <trd lng="fr">blanc <klr tip="ind">(couleur)</klr></trd>
</drv>
<drv mrk="blank.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <snc>
    <dif>
      Io <tld/>a:
      <ekz>
        vestita en griza&jcirc;o k <tld/>a&jcirc;o;
      </ekz>
      <ekz>
        ov<tld/>a&jcirc;o <klr>(senkolora, <tld/>i&gcirc;anta per kuirado,
        albumina parto de ovo)</klr>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_o;&c_k;</trd>
  <trd lng="fr">blanc <klr tip="ind">(subst.)</klr></trd>
</drv>
<drv mrk="blank.0eta">
  <kap><tld/>eta</kap>
  <snc>
    <dif>
      Dube<tld/>a, duon<tld/>a. 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_je;&c_l;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>, 
    <trd>&c_b;&c_ja;&c_l;&c_ja;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="de">wei&szlig;lich</trd>
  <trd lng="fr">blanch&acirc;tre</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_o;&c_v;&c_a;&c_t;&c_y;&c_j;</trd>
  <trd lng="tr">beyaz&inodot;ms&inodot;</trd>
</drv>
<drv mrk="blank.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari ion <tld/>a:
      <ekz>nigran kornikon sapo ne <tld/>igos<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <tld/>igi muron;
      </ekz>
      <ekz>
        <tld/>igi kanabtolon &ccirc;e suno;
      </ekz>
      <ekz>de zorgoj, ne de jaroj, <tld/>i&gcirc;as la haroj<fnt><bib>PrV</bib></fnt>.</ekz> 
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="de"><trd>wei&szlig;en</trd>, <trd>bleichen</trd></trdgrp>
  <trd lng="fr">blanchir</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="blank.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo el <tld/>kolora raso.  
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_y;<klr> (&c_n;&c_a;&c_z;.)</klr></trd>
  <trd lng="de">Wei&szlig;er</trd>
  <trd lng="fr">blanc <klr tip="ind">(homme)</klr></trd>
  <trd lng="nl">blanke</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_y;&c_j; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="tr">beyaz</trd>
</drv>
</art>
