<drv mrk="blank.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="blank.0ulo.ETN">
    <dif>
      Homo kun hela ha&ubreve;tkoloro:
      <ekz>
        &scirc;ipistoj, nigruloj, brunuloj, <tld/>uloj, duonnegroj
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la soleca vivo de <tld/>a virino for de aliaj <tld/>uloj
        en kabano <klr>[...]</klr> estis tre suspekteme rigardata
        de la loka lo&gcirc;antaro
        <fnt>
          <aut>W. M. A. De Smet</aut>
          <vrk>Kiu murdis Dian Fossey?</vrk>
          <bib>Monato</bib>
          <lok>1995:12, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_je;&c_l;&c_y;<klr> (&c_n;&c_a;&c_z;.)</klr>
    </trd>
    <trd lng="br">den <ind>gwenn</ind></trd>
    <trd lng="ca">blanc<klr tip="amb"> (persona)</klr></trd>
    <trd lng="cs">b&ecaron;logvard&ecaron;jec</trd>
    <trd lng="de">Wei&szlig;er</trd>
    <trd lng="es">blanco<klr tip="amb"> (persona)</klr></trd>
    <trd lng="fa">&a_s;&a_f;&f_y;&a_d;&f_p;&a_w;&a_s;&a_t;</trd>
    <trd lng="fr">blanc <klr>(homme)</klr></trd>
    <trd lng="he">&lamed;&bet;&fnun; &shin;&yod;&ayin;&resh;</trd>
    <trd lng="hu">feh&eacute;r ember</trd>
    <trdgrp lng="id">
      <trd>bulai</trd>,
      <trd>bule</trd>
    </trdgrp>
    <trd lng="ja">
    &#x767D;&#x4EBA; <pr>&#x306F;&#x304F;&#x3058;&#x3093;</pr></trd>
    <trd lng="nl">blanke</trd>
    <trd lng="pl">bia&lstroke;y</trd>
    <trd lng="ru">&c_b;&c_je;&c_l;&c_y;&c_j;<klr tip="amb"> (&c_s;&c_u;&c_shch;.)</klr></trd>
    <trdgrp lng="sk">
      <trd>beloch</trd>,
      <trd>bielogvardejec</trd>
    </trdgrp>
    <trd lng="tr">beyaz</trd>
    <trdgrp lng="uk">
      <trd>&c_b;&c_ib;&c_l;&c_i;&c_j;</trd>,
      <trd>&c_b;&c_ib;&c_l;&c_o;&c_gu;&c_v;&c_a;&c_r;&c_d;&c_ib;&c_jeu;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="vo">vietel</trd>
  </snc>
  <snc mrk="blank.0ulo.partiano">
    <uzo tip="fak">POL</uzo>
    <dif>
      Ano de <tld/>a partio:
      <ekz>
        &ccirc;iuj <tld lit="B"/>uloj (la kontra&ubreve;partianoj) estis persekutataj en terura
        maniero
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        poste venis revolucio, bol&scirc;evikoj, kontra&ubreve;revolucio, <tld/>uloj
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>Unua skizo:</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la interna milito &ndash; ili forkuris kun la <tld/>uloj
        <fnt>
          <bib>Metrop</bib>
        </fnt>?
      </ekz>
    </dif>
  <trd lng="de">Wei&szlig;gardist</trd>
  </snc>
</drv>

<drv mrk="blank.ovo0o">
  <kap><ofc>*</ofc>ov<tld/>o <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url ref="&AdE;/fundamento/universala_vortaro.html#av-5135">ov'blank'</url></lok></fnt>,
  <var><kap>ovo<tld/>o</kap></var>,
  <var><kap>ovo<tld/>a&jcirc;o</kap></var></kap>
  <snc mrk="blank.ovo0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      La flua, diafana, proteina, nutra parto de <ref tip="malprt" cel="ov.0o.KUI">ovo</ref>,
      <ref tip="sin" cel="albumi.0o">albumino</ref>, &ccirc;irka&ubreve;anta la
      <ref cel="flav.ov0o">ovoflavo</ref>n, <tld/>a post kuirado:
      <ekz>
        disbatita ovo<tld/>o
        <klr>(&scirc;a&ubreve;migita per kirlbatado)</klr>
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        preni <klr>[&#x2026;]</klr> da ovo<tld/>o bone batita 10 gramojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="malprt" cel="makaro.0o">makarono</ref>
        estas dol&ccirc;a&jcirc;o farita el kirlita ovo<tld/>o miksita kun
        glazursukero kaj pistitaj migdaloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=490619">Makarono</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="malprt" cel="mering.0o">meringo</ref>
        estas malpeza bakata dol&ccirc;a&jcirc;o el kirlita ovo<tld/>o kaj sukero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=490622">Meringo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x86CB;&#x6DF8; <pr>d&agrave;nq&imacron;ng</pr></trd>,
    <trd>&#x86CB;&#x9751; <pr>d&agrave;nq&imacron;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_o;&c_k; <klr>(&c_ja;&c_je;&c_ch;&c_n;&c_y;)</klr></trd>
