<drv mrk="blank.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="blank.0igi.KOL">
    <dif>
      Fari ion <tld/>a:
      <ekz>
        la suno <tld/>igos iliajn ostojn
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj vestoj fari&gcirc;is brilantaj, treege blankaj, kiel fulisto <klr>[&#x2026;]</klr> ne
        povas <tld/>igi
        <fnt>
          <bib>NT</bib>
          <lok>Marko 9:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la murojn <klr>[per]</klr> kalko <klr>[&#x2026;]</klr> oni videble fre&scirc;e <klr>[&#x2026;]</klr>
        <tld/>igis
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;evaloj <klr>[&#x2026;]</klr> al&jcirc;etis sur la pavimon la &scirc;a&ubreve;mon, kiu <tld/>igis
        iliajn enbu&scirc;a&jcirc;ojn
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        nigran kornikon sapo ne <tld/>igos
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="br">gwenna&ntilde;</trd>
  <trdgrp lng="ca">
    <trd>emblanquir</trd>,
    <trd>emblanquinar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&iacute;lit</trd>,
    <trd>b&ecaron;lit</trd>,
    <trd>ob&iacute;lit</trd>,
    <trd>vyb&iacute;lit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wei&szlig;en</trd>,
    <trd>bleichen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>whiten</trd>, <trd>bleach</trd>
  </trdgrp>
