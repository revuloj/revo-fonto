<drv mrk="blank.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="B"/>a
      <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloro</ref>
      a&ubreve; kolorilo.
    </dif>
    <ref tip="super" cel="kolor.sen0a">senkolora</ref>
    <trd lng="pl">biel</trd>
  </snc>
  <snc mrk="blank.0o.ludanto">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Unu el la du ludantoj en ludoj kiel
      <ref cel="sxak.0o">&scirc;ako</ref>,
      <ref cel="go.0o">goo</ref>,
      <ref cel="dam.0oj0ludo">damoj</ref>, 
      a&ubreve; simila.
