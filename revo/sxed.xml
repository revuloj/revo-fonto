<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;ed</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sxed.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxed.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      &Scirc;irmejo por brutoj a&ubreve; iloj, kun tegmento kaj subtenaj fostoj,
      ordinare ligna kaj sen vandoj:
      <ekz>
        la hejtligna <tld/>o de la vilao
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 7a, p. 64a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="remiz.0o">remizo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Unterstand</trd>,
    <trd>Schuppen</trd>
  </trdgrp>
  <trd lng="fr">cabane<klr> (agr.)</klr></trd>
  <trdgrp lng="hu">
    <trd>pajta</trd>,
    <trd>sz&iacute;n</trd>
  </trdgrp>
</drv>

<drv mrk="sxed.abel0o">
  <kap>abel<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="sxed.abel0o.ABE">
    <dif>
      &Scirc;irmejo por <ref tip="prt" cel="abel.0ujo">abelujoj</ref>.
    </dif>
  </snc>
  <trd lng="fr">rucher<klr> (batiment)</klr></trd>
  <trd lng="hu">m&eacute;hes</trd>  
</drv>

<drv mrk="sxed.garbo0o">
  <kap>garbo<tld/>o, garb<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="sxed.garbo0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Kampara <ref tip="super" cel="garb.0ejo">garbejo</ref> por fre&scirc;e
      rikoltita greno.
    </dif>
  </snc>
  <trd lng="fr">grange</trd>
  <trdgrp lng="hu">
    <trd>cs&udblac;r</trd>,
    <trd>mag&aacute;r</trd>
  </trdgrp>
</drv>
</art>

