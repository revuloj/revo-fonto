<vortaro>
<art mrk="$Id: biofizik.xml,v 1.11 2023/10/29 19:11:02 revo Exp $">
<kap>
  <rad>biofizik</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="biofizik.0o">
  <kap><tld/>o</kap>
  <snc mrk="biofizik.0o.SCI">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Apliko de <ref tip="malprt" cel="fizik.0o.FIZ">fizikaj</ref>
      metodoj, mezuroj kaj principoj en
      <ref tip="malprt" cel="biolog1.0o.SCI">biologia</ref> studo de
      viva&jcirc;oj:
      <ekz>
        <nom>Bruce Alberts</nom> <klr>[&#x2026;]</klr> estas profesoro de bio&hcirc;emio kaj
        <tld/>o en Kalifornia Universitato
        <fnt>
          <aut>B. Alberts, k.a.</aut>,
          <vrk><url ref="http://www.volny.cz/espero-publ/eo/biologio.html">
          Baza &ccirc;ela biologio</url></vrk>,
          <lok>Espero Publishing, [2001?]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x751F;&#x7269;&#x7269;&#x7406;&#x5B66; <pr>sh&#x113;ngw&#xF9;w&#xF9;l&#x1D0;xu&#xE9;</pr></trd>
</snc>
  <trd lng="be">&c_b;&c_ib;&c_ja;&c_f;&c_ib;&c_z;&c_y;&c_k;&c_a;</trd>
  <trd lng="cs">biofyzika</trd>
  <trd lng="de">Biophysik</trd>
  <trd lng="es">biof&iacute;sica</trd>
  <trd lng="fr">biophysique</trd>
  <trd lng="he">&bet;&#x5BC;&#x5B4;&yod;&vav;&#x5B9;&pe;&#x5B4;&yod;&samekh;&#x5B4;&yod;&qof;&#x5B8;&he;</trd>
  <trd lng="ja">&#x751F;&#x7269;&#x7269;&#x7406;&#x5B66;  <pr>&#x305B;&#x3044;&#x3076;&#x3064;&#x3076;&#x3064;&#x308A;&#x304C;&#x304F;</pr></trd>
  <trd lng="nl">biofysica</trd>
  <trd lng="sk">biofyzika</trd>
</drv>

<drv mrk="biofizik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="biofizik.0isto.SCI">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Fakulo pri <ref tip="vid" cel="biofizik.0o.SCI"><tld/>o</ref>:
      <ekz>
        la <tld/>isto <nom>Sergej Kovaljov</nom> <klr>[&#x2026;]</klr> redaktis sep
        numerojn de la gazeto
        <fnt>
          <aut>K. Kniivil&auml;</aut>,
          <vrk><url ref="http://www.kniivila.net/2008/naskightago-de-kontrausovetia-malpropagandilo/">
          Naski&gcirc;tago de kontra&ubreve;sovetia malpropagandilo</url></vrk>,
          <lok>diverse, 2008-05-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ib;&c_ja;&c_f;&c_ib;&c_z;&c_y;&c_k;</trd>
  <trd lng="de">Biophysiker</trd>
  <trd lng="fr">biophysicien</trd>
    <trd lng="es">biof&iacute;sico</trd>
  <trd lng="nl">biofysicus</trd>
</drv>
</art>

