<drv mrk="sxtal.0a">
  <kap><tld/>a</kap>
  <snc mrk="sxtal.0a.KEM">
    <dif>
      El <tld/>o:
      <ekz>
        <nom>Sargon</nom> levis sian <tld/>an glavon, ekbatis
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> prenis el la komodo ledan zonon kun <tld/>a buko
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj trikiloj brilis en la radioj de la suno, kvaza&ubreve; fajreroj
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        pecoj da drapoj, toloj, batistoj, muslinoj, inter kiuj brilis <tld/>aj tondiloj,
        kretoj kaj krajonoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiun angulon okupis <tld/>a &scirc;nurego, kiu estis la kondukanta fadeno de
        fulm&scirc;irmilo starigita sur tiu turo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">stalowy</trd>
  </snc>
  <snc mrk="sxtal.0a.kolora">
    <dif>
      <tld lit="&Scirc;"/>kolora:
      <ekz>
        <tld/>aj okuloj;
      </ekz>
      <ekz>
        <tld/>a strio sur la malluma &ccirc;ielo
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sxtal.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmola, necedema kiel <tld/>o:
      <ekz>
        Viktoro havis <tld/>ajn muskolojn
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas bona kontra&ubreve; la marmalsano, se oni havas <tld/>an stomakon
        <fnt>
          <bib>Fab2</bib>
          <lok>Flikilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i faras rondon post la rondo, tute ne laci&gcirc;ante, &ccirc;iam kun la sama elasta
        <tld/>a figuro
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        por fari&gcirc;i policano, necesas havi <tld/>ajn nervojn
        <fnt>
          <bib>ChV</bib>
          <lok>25</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">&zdot;elazny</trd>
  </snc>
