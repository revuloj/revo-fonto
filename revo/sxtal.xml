<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;tal</rad>/o
</kap>
<drv mrk="sxtal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Metalo tre malmola, kvankam fleksebla, produktata per kombino de
      fero kun karbono.
    </dif>
  </snc>
  <trd lng="de">Stahl</trd>
  <trd lng="en">steel</trd>
  <trd lng="fr">acier</trd>
  <trd lng="nl">staal</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_l;&c_mol;</trd>
  <trd lng="sv">st&aring;l</trd>
  <trd lng="tr">&ccedil;elik</trd>
</drv>

<drv mrk="sxtal.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      El <tld/>o.
    </dif>
  </snc>
  <snc>
    <dif>
      <tld lit="&Scirc;"/>kolora.
    </dif>
  </snc>
  <snc>
    <dif>
      Malmola kiel <tld/>o.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Morale fortika, necedema:
      <ekz>
        <tld/>a energio.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">st&auml;hlern</trd>
  <trd lng="nl">stalen</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_j;</trd>
</drv>
</art>
