<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;tal</rad>/o
</kap>
<drv mrk="sxtal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxtal.0o.KEM">
    <dif>
      <ref tip="super" cel="metal.0o.KEM">Metalo</ref> tre malmola, kvankam fleksebla, produktata per kombino de      fero kun karbono.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_l;&c_mol;</trd>
  <trd lng="de">Stahl</trd>
  <trdgrp lng="el">
    <trd>&alfa;&tau;&sigma;&alfa_ton;&lambda;&jota;</trd>,
    <trd>&chi;&alfa_ton;&lambda;&ypsilon;&beta;&alfa;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">steel</trd>
  <trd lng="fr">acier</trd>
  <trd lng="hu">ac&eacute;l</trd>
  <trd lng="nl">staal</trd>
  <trd lng="pt">a&ccedil;o</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_l;&c_mol;</trd>
  <trd lng="sv">st&aring;l</trd>
  <trd lng="tr">&ccedil;elik</trd>
</drv>

<drv mrk="sxtal.0a">
  <kap><tld/>a</kap>
  <snc mrk="sxtal.0a.KEM">
    <dif>
      El <tld/>o.
    </dif>
  </snc>
  <snc mrk="sxtal.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmola, necedema kiel <tld/>o:
      <ekz>
        <tld/>a energio.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_l;&c_jo;&c_v;&c_y;</trd>
  <trd lng="de">st&auml;hlern</trd>
  <trd lng="fr">d'<ind>acier</ind></trd>
  <trdgrp lng="hu">
    <trd>ac&eacute;l-</trd>,
    <trd>ac&eacute;los</trd>,
    <trd>ac&eacute;lkem&eacute;ny</trd>
  </trdgrp>
  <trd lng="nl">stalen</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_j;</trd>
</drv>
</art>
