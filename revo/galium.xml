  <trd lng="es">galio</trd>
  <trd lng="fr">gallium</trd>
  <trd lng="hu">gallium</trd>
  <trd lng="it">gallio</trd>
  <trd lng="la">Gallium</trd>
  <trd lng="nl">gallium</trd>
  <trd lng="pl">gal</trd>
  <trd lng="pt">g&aacute;lio</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_l;&c_i;&c_j;</trd>
</drv>
</art>
