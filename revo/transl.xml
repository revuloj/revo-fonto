<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: transl.xml,v 1.19 2024/07/19 15:29:16 revo Exp $">
<kap>
  <rad>translaci</rad>/o
  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="transl.0o">
  <kap><tld/>o</kap>
  <snc mrk="transl.0o.kinematiko">
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="klr">(kinematiko)</uzo>
    <dif>
      <klr>(de <ref cel="solid.0o.kinematiko">solido</ref>)</klr>
      Tia movo, ke &ccirc;iu <ref cel="par.punkto0o">punktoparo</ref>
      de &gcirc;i restas <ref cel="ekvipo.0a.strekoj">ekvipolenta</ref> al si mem.
    </dif>
    <trd lng="de">Translation</trd>
    <trd lng="nl">verplaatsing<klr tip="ind"> (fys.)</klr></trd>
    <trd lng="hu">transzl&aacute;ci&oacute;<klr tip="ind"> (fizika)</klr></trd>
    <trd lng="pt">transla&ccedil;&atilde;o</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_a;&c_l;&c_l;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;</trd>
    </trdgrp>
  </snc>
  <snc mrk="transl.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(en <ref cel="spac.afina0o">afina spaco</ref>,
      la&ubreve; vektoro <frm am="tau">&tau;</frm>) </klr>
      <ref cel="afin.0a.bildigo">Afina</ref>
      <ref tip="super" cel="bild.0igo.MAT">bildigo</ref>, kiu &jcirc;etas
      punkton
      <frm am="M">M</frm> al <frm am="tau+M">&tau;+M</frm>; alidire: <ref
      cel="operac.ekstera0o.elemento">operacio</ref>
      de vektoro <frm am="tau">&tau;</frm> super la koncerna afina spaco:
      <ekz>
        per <tld/>o la longoj, areoj kaj anguloj restas egalaj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Translation</trd>,
      <trd>Parallelverschiebung</trd>
    </trdgrp>
    <trd lng="en">translation<klr tip="ind"> (math.)</klr></trd>
    <trd lng="hu">eltol&aacute;s<klr tip="ind"> (matematika)</klr></trd>
    <trd lng="nl">verplaatsing<klr tip="ind"> (wisk.)</klr></trd>
    <trdgrp lng="pl">
      <trd>przesuni&eogonek;cie</trd>,
      <trd>translacja</trd></trdgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_a;&c_l;&c_l;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;</trd>
    </trdgrp>
    <trd lng="zh">&#24179;&#31227; <pr>p&iacute;ngy&iacute;</pr></trd>
  </snc>
  <snc mrk="transl.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Transigo de rajto.
    </dif>
    <trd lng="hu">&aacute;truh&aacute;z&aacute;s<klr tip="ind"> (jog&eacute;)</klr></trd>
    <trd lng="nl">overdracht<klr tip="ind"> (recht)</klr></trd>
    <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_ch;&c_a; <klr tip="amb">(&c_p;&c_r;&c_a;&c_v;)</klr></trd>
  </snc>
  <snc mrk="transl.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref cel="rivolu.0o.AST">Rivoluo</ref> de planedo,
      la &scirc;ajna movi&gcirc;o de alia planedo anta&ubreve; la stela fono:
      <ekz>
        la <tld/>a <klr>(rivolua)</klr> periodo de Urano da&ubreve;ras 84 terajn jarojn
        <fnt>
          <aut>Alves de Moura</aut>
          <vrk><url ref="http://www.eventoj.hu/steb/astronomio/tago84jaroj/tago84jaroj.htm"
              >Tago egala al 42 jaroj kaj nokto egala same al 42 jaroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">siderische Umlaufzeit</trd>
  </snc>
  <snc mrk="transl.0o.transsendo">
    <uzo tip="fak">TEL</uzo>
    <uzo tip="stl">ARK</uzo>
    <dif>
      Transsendo:
      <ekz>
        ili venis por saluti esperantlingve a&ubreve;skultantojn de radiotelefonaj
        esperanto-lecionoj, sed &ccirc;i tage pro eksperimenta <tld/>o de opero el Granda
        Oper-Teatro la leciono ne okazis
        <fnt>
          <vrk><url ref="https://files.libcom.org/files/Sennaciulo_1925_n33_maj14.pdf"
              >Interparolo kun K-dino E.Suchardova</url></vrk>
          <lok>Sennaciulo n.ro 33, 14a maj. 1925, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la tera surfaco oni uzas multajn <tld/>ojn por dissendi la televidan
        programon sur pli granda distanco
        <fnt>
          <vrk><url ref="https://archive.org/details/InternaciaJhurnalisto_1968_n4_okt-dec"
              >La Telekomunikado kaj la Kosmo en Servo de Vasta Informado</url></vrk>
          <lok>Internacia Jurnalisto, n-ro4, okt-dec 1968</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_mol;&c_l;&c_ja;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_ch;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_n;&c_o;&c_s;</trd>
  </trdgrp>
  <trd lng="ca">translaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>posuvn&yacute; pohyb po p&rcaron;&iacute;mce</trd>,
    <trd>p&rcaron;enos</trd>,
    <trd>translace</trd>
  </trdgrp>
  <trd lng="fr">translation<klr tip="ind"> (m&eacute;c., math.,
  droit)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#20006;&#36914;  <pr>&#12408;&#12356;&#12375;&#12435;</pr></trd>,
    <trd>&#24179;&#34892;&#31227;&#21205;  <pr>&#12408;&#12356;&#12371;&#12358;&#12356;&#12393;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>premiestnenie</trd>,
    <trd>prenesenie</trd>,
    <trd>transl&aacute;cia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_ch;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_l;&c_ja;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_l;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_p;&c_je;&c_r;&c_je;&c_n;&c_o;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="transl.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
  </snc>
  <snc mrk="transl.0i.ARK">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref cel="trans.0igi">Transigi</ref>, transsendi:
      <ekz>
        la radio senzorge <tld/>as <klr>(sendas)</klr> <nom>charlestown</nom>&#8217;on
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sola solvo estas &ndash; filmi eventon per videokamero,
        munti la filmon kaj <tld/>i &gcirc;in sur la ekranon
        <fnt>
          <bib>KFl</bib>
          <lok>V. Pri unueco kaj lukto, kaj anka&ubreve; pri &rightquot;tipeco de l&#8217; disdivido&#8221;</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<trdgrp lng="be">
  <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_mol;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
  <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
</trdgrp>
</drv>

</art>
