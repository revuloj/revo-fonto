<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arob.xml,v 1.12 2023/07/16 16:35:37 revo Exp $">
<kap>
  <rad>arob</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="arob.0o">
  <kap><tld/>o</kap>
  <mlg>@</mlg>
  <snc>
    <dif>
      Antikva e&ubreve;ropa <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">mezurunuo</ref>
      de volumeno kaj maso, uzata aparte por flua&jcirc;oj, valoranta inter 12 kaj 15 kilogramojn
      la&ubreve; lokoj kaj epokoj:
      <ekz>
        ses <tld/>oj da ru&gcirc;a vino<fnt><bib>DonKihxoto</bib>,
        <lok>&ccirc;apitro
        37a, p. 281a</lok></fnt>;
      </ekz>
      <ekz>
        en Bolivio oni vendas farunon, sukeron, fabojn k.a. en malgrandaj <tld/>sakoj
        (11,5&#8239;kg), kvar <tld/>oj estas unu <ref tip="malprt"
        cel="kvintal.0o.MEZ.funta">kvintalo</ref> (46&#8239;kg).
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="helik.0o.ascii">heliko<sncref/></ref>,
      <ref cel="po.0_signo">po-signo</ref>
    </refgrp>
  </snc>
  <trd lng="cs">arroba</trd>
  <trd lng="de">Arroba</trd>
  <trd lng="es">arroba</trd>
  <trd lng="fr">arrobe</trd>
  <trd lng="hu">arroba</trd>
  <trd lng="it">arroba</trd>
  <trd lng="nl">arroba</trd>
  <trd lng="ru">&c_a;&c_r;&c_r;&c_o;&c_b;&c_a;</trd>
  <trd lng="sk">arroba</trd>
  <trd lng="uk">&c_a;&c_r;&c_o;&c_b;&c_a;</trd>
</drv>
</art>

