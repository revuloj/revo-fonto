<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>arob</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="arob.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Antikva e&ubreve;ropa <ref tip="super" cel="unu.mezur0o">mezurunuo</ref>
      de
      kapacito kaj maso, valoranta inter 12 kaj 15 kilogramojn la&ubreve; lokoj
      kaj epokoj:
      <ekz>
        ses <tld/>oj da ru&gcirc;a vino<fnt><bib>DonKihxoto</bib>,
        <lok>&ccirc;apitro
        37a, p. 281a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">arroba</trd>
  <trd lng="fr">arrobe</trd>
  <trd lng="hu">arroba</trd>
  <trd lng="it">arroba</trd>
  <trd lng="nl">arroba</trd>
  <trd lng="ru">&c_a;&c_r;&c_r;&c_o;&c_b;&c_a;</trd>
</drv>
</art>

