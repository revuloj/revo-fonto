<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: portep.xml,v 1.15 2023/10/20 16:38:17 revo Exp $">
<kap>
  <rad>portepe</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="portep.0o">
  <kap><tld/>o</kap>
  <snc mrk="portep.0o.mantenilo">
    <dif>
      <ref tip="super" cel="rimen.0o">Rimeno</ref>, volvita &ccirc;irka&ubreve; 
      manartiko kaj la tenilo de glavo, por ne perdi &gcirc;in dum la batalo:
      <ekz>
        li ekfajfis malla&ubreve;te ian gajan melodion, ludante per la manoj 
        je la kvastetoj de la <tld/>o
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>&ccirc;apitro 19a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="balte.0o">balteo</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_t;&c_u;&c_p;&c_e;&c_ja;</trd>
  <trd lng="de">Portepee</trd>
  <trd lng="fr">porte-&eacute;p&eacute;e</trd>
  <trd lng="hu">kardsz&iacute;j</trd>
  <trd lng="ja">&#x5263;&#x5E2F;  <pr>&#x3051;&#x3093;&#x305F;&#x3044;</pr></trd>
  <trd lng="nl">koppelriem</trd>
  <trd lng="pl">temblak  <klr>(bro&nacute;)</klr></trd>
  <trdgrp lng="pt">
    <trd>cinto</trd>,
    <trd>cintur&atilde;o</trd>
  </trdgrp>
  <trd lng="ro">pra&#x219;tie  <klr>(arm&abreve;)</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_t;&c_u;&c_p;&c_je;&c_ja;</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_r;&c_t;&c_u;&#x43F;&c_je;&#x44F;</trd>
</drv>
</art>
