<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: madrig.xml,v 1.22 2023/10/19 15:13:16 revo Exp $">
<kap>
  <rad>madrigal</rad>/o
</kap>
<drv mrk="madrig.0o">
  <kap><tld/>o</kap>
  <snc mrk="madrig.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Aminduma, delikate sprita komplimento, plej ofte versa.
    </dif>
  </snc>
  <snc mrk="madrig.0o.MUZ">
    <dif>
       plurvo&ccirc;a muzika&jcirc;o kun sekulara enhavo, ofta dum renesanco kaj
       frua baroko:
      <ekz>
        <nom>Heinrich Sch&uuml;tz</nom> <klr>[&#x2026;]</klr> komencis danke al
        stipendio de la landgrafo
        trijaran studon en Venecio <klr>[&#x2026;]</klr>, kiun li finis per la
        <tld/>kolekto
        &leftquot;<nac>Il Primo libro di Madrigali</nac>&rightquot; publikigita en 1611
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=13169">Heinrich Sch&uuml;tz</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_d;&c_r;&c_y;&c_g;&c_a;&c_l;</trd>
  <trd lng="ca">madrigal</trd>
  <trd lng="cs">madrigal</trd>
  <trd lng="de">Madrigal</trd>
  <trd lng="en">madrigal</trd>
  <trd lng="es">madrigal</trd>
  <trd lng="fr">madrigal</trd>
  <trd lng="hu">madrig&aacute;l</trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30C9;&#x30EA;&#x30AC;&#x30EB;</trd>,
    <trd>&#x604B;&#x6B4C;  <pr>&#x3053;&#x3044;&#x3046;&#x305F;</pr></trd>,
    <trd>&#x604B;&#x306E;&#x7518;&#x8A00;  <pr>&#x3053;&#x3044;&#x306E;&#x304B;&#x3093;&#x3052;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">madrigaal</trd>
  <trd lng="pl">madryga&lstroke;</trd> 
  <trd lng="pt">madrigal</trd>
  <trd lng="ro">madrigal</trd>
  <trd lng="ru">&c_m;&c_a;&c_d;&c_r;&c_i;&c_g;&c_a;&c_l;</trd>
  <trd lng="sk">madrigal</trd>
  <trd lng="uk">&#x43C;&#x430;&#x434;&#x440;&#x438;&#x491;&#x430;&#x43B;</trd>
</drv>
</art>
