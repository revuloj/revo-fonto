<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>madrigal</rad>/o
</kap>
<drv mrk="madrig.0o">
  <kap><tld/>o</kap>
  <snc mrk="madrig.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Aminduma, delikate sprita komplimento, plej ofte versa.
    </dif>
  </snc>
  <snc mrk="madrig.0o.MUZ">
    <dif>
       plurvo&ccirc;a muzika&jcirc;o kun sekulara enhavo, ofta dum renesanco kaj frua baroko:
      <ekz>
        <nom>Heinrich Sch&uuml;tz</nom> <klr>[&#8230;]</klr> komencis danke al stipendio de la landgrafo
        trijaran studon en Venecio <klr>[&#8230;]</klr>, kiun li finis per la <tld/>kolekto
        &leftquot;<nac>Il Primo libro di Madrigali</nac>&rightquot; publikigita en 1611
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=13169"
              >Heinrich Sch&uuml;tz</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_d;&c_r;&c_y;&c_g;&c_a;&c_l;</trd>
  <trd lng="ca">madrigal</trd>
  <trd lng="cs">madrigal</trd>
  <trd lng="de">Madrigal</trd>
  <trd lng="en">madrigal</trd>
  <trd lng="es">madrigal</trd>
  <trd lng="fr">madrigal</trd>
  <trd lng="hu">madrig&aacute;l</trd>
  <trd lng="nl">madrigaal</trd>
  <trd lng="pt">madrigal</trd>
  <trd lng="ru">&c_m;&c_a;&c_d;&c_r;&c_i;&c_g;&c_a;&c_l;</trd>
  <trd lng="sk">madrigal</trd>
</drv>
</art>
