  <trd lng="hu">determin&aacute;ns</trd>
  <trd lng="nl">determinant</trd>
  <trd lng="ru">&c_o;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
