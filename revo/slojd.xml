<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>slojd</rad>/o
</kap>
<drv mrk="slojd.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Metia manlaboro, enkondukita en multaj lernejoj kun la celo &gcirc;enerale
      eduki la personecon de lernantoj.
    </dif>
  </snc>
  <trd lng="de">Werken</trd>
  <trdgrp lng="nl">
    <trd>sl&ouml;jd</trd>,
    <trd>handenarbeid</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_u;&c_d; <klr tip="amb">(&c_sh;&c_k;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_p;&c_r;&c_je;&c_d;&c_m;&c_je;&c_t;)</klr></trd>
  <trd lng="sv">sl&ouml;jd</trd>
</drv>
</art>
