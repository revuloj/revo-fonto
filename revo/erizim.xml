<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>erizim</rad>/o
</kap>
<drv mrk="erizim.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Erysimum</trd>), 
      <ref tip="lst" cel="herb.0o.BO"
      lst="voko:brasikoparencoj">herbo</ref>
      kun flavaj floraroj kaj krevantaj gu&scirc;oj.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="de">Sch&ouml;terich</trd>
  <trd lng="fa">&a_kh;&a_A;&f_k;&a_sh;&f_y;&a_r; &a_t;&a_l;&a_kh;</trd>
  <trd lng="hu">repcs&eacute;ny</trd>
  <trd lng="pl">gorczycznik</trd>
  <trd lng="pt">er&iacute;simo</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_u;&c_sh;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
