<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: erizim.xml,v 1.21 2023/08/31 11:49:30 revo Exp $">
<kap>
<rad>erizim</rad>/o
</kap>
<drv mrk="erizim.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio
      <ref tip="malprt" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Erysimum</trd>), <ref tip="lst" cel="herb.0o.BOT" lst="voko:brasikoparencoj">herbo</ref> kun flavaj floraroj kaj krevantaj gu&scirc;oj:
      <ekz>
        la Kampa <tld/>o (<nom>Erysimum cheiranthoides</nom>) estas ofta najbaro de
        <ref cel="papav.0eto">papaveto</ref>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/b/b5/Erysimum_Odoratum_(Pannonicum).jpg/317px-Erysimum_Odoratum_(Pannonicum).jpg" prm="CC BY-SA 4.0">Odoranta <tld/>o (Erysimum odoratum)
        <fnt>
          <aut>Petr Vodi&ccaron;ka, propra verko</aut>
          <lok><url ref="&WCW;/File:Erysimum_Odoratum_(Pannonicum).jpg">&WCW;/File:Erysimum_Odoratum_(Pannonicum).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_w;&c_t;&c_u;&c_sh;&c_k;&c_a;</trd>
  <trd lng="de">Sch&ouml;terich</trd>
  <trd lng="fa">&a_kh;&a_A;&f_k;&a_sh;&f_y;&a_r; &a_t;&a_l;&a_kh;</trd>
  <trd lng="fr">erysimum</trd>
  <trd lng="hu">repcs&eacute;ny</trd>
  <trdgrp lng="ja">
    <trd>&#x30A8;&#x30BE;&#x30B9;&#x30BA;&#x30B7;&#x30ED;</trd>,
    <trd>&#x30A8;&#x30EA;&#x30B7;&#x30DE;&#x30E0;</trd>
  </trdgrp>
  <trd lng="pl">gorczycznik</trd>
  <trd lng="pt">er&iacute;simo</trd>
  <trd lng="ru">&c_zh;&c_je;&c_l;&c_t;&c_u;&c_sh;&c_n;&c_i;&c_k;</trd>
  <trd lng="uk">&#x436;&#x43E;&#x432;&#x442;&#x443;&#x448;&#x43D;&#x438;&#x43A;</trd>
</drv>
</art>
