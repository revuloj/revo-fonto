<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: decigr.xml,v 1.21 2023/08/31 10:43:58 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>decigram</rad>/o
</kap>
<drv mrk="decigr.0o">
  <kap><tld/>o</kap>
  <mlg>dg</mlg>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de maso, dekono de 
      <ref tip="malprt" cel="gram.0o">gramo</ref>:
      <ekz>
        apotekistoj &gcirc;is la fino de la 19a jc uzis pezilojn de <tld/>oj kaj centigramoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_c;&c_y;&c_g;&c_r;&c_a;&c_m;</trd>
  <trd lng="cs">decigram</trd>
  <trd lng="de">Dezigramm</trd>
  <trd lng="en">decigram</trd>
  <trd lng="es">decigramo</trd>
  <trd lng="fa">&a_d;&a_s;&f_y;&zwnj;&f_g;&a_r;&a_m;</trd>
  <trd lng="fr">d&eacute;cigramme</trd>
  <trd lng="he">&#x5D3;&#x5E6;&#x5D9;&#x5D2;&#x5E8;&#x5DD;</trd>
  <trd lng="hu">decigramm</trd>
  <trd lng="ja">&#x30C7;&#x30B7;&#x30B0;&#x30E9;&#x30E0;</trd>
  <trd lng="nl">decigram</trd>
  <trd lng="pl">decygram</trd>
  <trd lng="pt">decigrama</trd>
  <trd lng="ru">&c_d;&c_je;&c_c;&c_i;&c_g;&c_r;&c_a;&c_m;&c_m;</trd>
  <trd lng="sk">decigram</trd>
  <trd lng="sv">decigram</trd>
</drv>
</art>
