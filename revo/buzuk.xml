<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: buzuk.xml,v 1.8 2022/10/31 08:46:03 revo Exp $">
<kap>
  <rad>buzuk</rad>/o
</kap>
<drv mrk="buzuk.0o">
  <kap><tld/>o</kap>
  <snc mrk="buzuk.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Greka
      <ref tip="lst" cel="instru2.kord0o.MUZ"
      lst="voko:kordinstrumentoj">kordinstrumento</ref>,
      <ref tip="super" cel="liut.0o">liuto</ref> kun longa kolo, 
      ses (foje kvar paroj da) kordoj sonigataj per plektro:
      <ekz>
        la plektro de la <tld/>o altu&scirc;as la kordon
        <fnt><aut>E. de Zilah</aut>, <vrk>Vivi sur barko</vrk>, 
        <lok>1994</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Luca Giacometti</nom> ludis <tld/>on
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="&Viki;/Modena_City_Ramblers">
          Modena City Ramblers</url>, 2009-07-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_z;&c_u;&c_k;&c_ib; <klr tip="amb">(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;)</klr></trd>
  <trd lng="de">Bouzouki</trd>
  <trd lng="fr">bouzouki</trd>
  <trd lng="it">bouzouki</trd>
</drv>
</art>

