    <trd>p&rcaron;&iacute;vr&zcaron;enec kr&aacute;lovstv&iacute;</trd>,
    <trd>roajalista</trd>,
    <trd>royalista</trd>
  </trdgrp>
  <trd lng="de">Royalist</trd>
  <trd lng="en">royalist</trd>
  <trd lng="fr">royaliste</trd>
  <trdgrp lng="hu">
    <trd>royalista</trd>,
    <trd>kir&aacute;lyp&aacute;rti</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x738B;&#x653F;&#x4E3B;&#x7FA9;&#x8005;  <pr>&#x304A;&#x3046;&#x305B;&#x3044;&#x3057;&#x3085;&#x304E;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x738B;&#x515A;&#x54E1;  <pr>&#x304A;&#x3046;&#x3068;&#x3046;&#x3044;&#x3093;</pr></trd>,
    <trd>&#x738B;&#x515A;&#x6D3E;  <pr>&#x304A;&#x3046;&#x3068;&#x3046;&#x306F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>royalist</trd>,
    <trd>koningsgezinde</trd>
  </trdgrp>
  <trd lng="pl">rojalista</trd>
  <trdgrp lng="pt">
    <trd>monarquista</trd>,
    <trd>realista</trd>
  </trdgrp>
  <trd lng="ro">roialist</trd>
  <trd lng="ru">&c_r;&c_o;&c_ja;&c_l;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>monarchista</trd>,
    <trd>roajalista</trd>,
    <trd>royalista</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&#x43E;&#x44F;&#x43B;&c_ib;&c_s;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x4FDD;&#x7687;&#x515A; <pr>b&#x1CE;ohu&#xE1;ngd&#x1CE;ng</pr></trd>,
    <trd>&#x4FDD;&#x7687;&#x9EE8; <pr>b&#x1CE;ohu&#xE1;ngd&#x1CE;ng</pr></trd>,
    <trd>&#x4FDD;&#x7687;&#x515A;&#x5458; <pr>b&#x1CE;ohu&#xE1;ngd&#x1CE;ngyu&#xE1;n</pr></trd>,
    <trd>&#x4FDD;&#x7687;&#x9EE8;&#x54E1; <pr>b&#x1CE;ohu&#xE1;ngd&#x1CE;ngyu&#xE1;n</pr></trd>,
