<vortaro>
<art mrk="$Id: rojali1.xml,v 1.17 2018/11/25 18:10:16 revo Exp $">
  <kap>
    <ofc>1</ofc>
    <rad>rojalist</rad>/o
  </kap>

<drv mrk="rojali1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="monarh.0isto">Monar&hcirc;isto</ref>
      <klr>(en formo rilata al re&gcirc;a re&gcirc;imo)</klr>:
      <ekz>
        milito okazinta de 1793 &gcirc;is 1793 dum la Franca Revolucio inter <tld/>aj kaj
        katolikaj vendeanoj (la &leftquot;blankuloj&rightquot;) kaj la sekularaj respublikanoj (la
        &leftquot;bluuloj&rightquot;)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=295696"
              >Charles-Louis Largeteau</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_ja;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">reialista</trd>
  <trdgrp lng="cs">
    <trd>monarchista</trd>,
    <trd>p&rcaron;&iacute;vr&zcaron;enec kr&aacute;lovstv&iacute;</trd>,
    <trd>roajalista</trd>,
    <trd>royalista</trd>
  </trdgrp>
  <trd lng="de">Royalist</trd>
  <trd lng="en">royalist</trd>
  <trd lng="fr">royaliste</trd>
  <trdgrp lng="hu">
    <trd>royalista</trd>,
    <trd>kir&aacute;lyp&aacute;rti</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>royalist</trd>,<trd>koningsgezinde</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>monarquista</trd>,
    <trd>realista</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_ja;&c_l;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>monarchista</trd>,
    <trd>roajalista</trd>,
    <trd>royalista</trd>
  </trdgrp>
</drv>
  </art>
