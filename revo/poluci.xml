<vortaro>
<art mrk="$Id$">
<kap>
  <rad>poluci</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="poluci.0o">
  <kap><tld/>o</kap>
  <snc mrk="poluci.0o.vir">
    <dif>
      Nevola elfluo de spermo sen koitado, precipe dum son&gcirc;ado.
    </dif>
    <trdgrp lng="fr">
      <trd>pollution nocturne</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&eacute;jszakai mag&ouml;ml&eacute;s</trd>,
      <trd>poll&uacute;ci&oacute;</trd>,
    </trdgrp>
    <trd lng="de">Pollution</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_l;&c_ju;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <snc mrk="poluci.0o.medi">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="dif" cel="polu.0ado">poluado</ref>
      <ekz>
        la publiko ne sufi&ccirc;e konsideras la eksterajn kostojn de
        transportilo: <tld/>on, bruon, klimat&scirc;an&gcirc;on kaj
        akcidentojn<fnt><aut>Roland Rotsaert</aut>: <vrk>Medio-indulgo de
        la &ccirc;efaj ekonomiaj sektoroj</vrk>, <bib>Monato</bib><lok>jaro
        2001a, numero 3a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>pollution</trd>
    </trdgrp>
    <trd lng="de">Umweltverschmutzung</trd>
    <trd lng="hu">k&oacute;rnyezetszennyez&eacute;s</trd>
  </snc>
  <trd lng="nl">pollutie</trd>
</drv>
</art>
