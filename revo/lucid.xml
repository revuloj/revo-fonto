<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lucid.xml,v 1.11 2024/01/15 19:32:08 revo Exp $">
<kap>
  <rad>lucid</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lucid.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="lucid.0a.PSI">
    <dif>
      <ref tip="dif" cel="mens.klar0a.PSI">Klarmensa</ref>:
      <ekz>
        vi &scirc;ajnas esti multe pli <tld/>a ol alia junulo kiun oni trovis 
        hiera&ubreve; for en la direkto al <nom>Bogee</nom>, absolute nuda kaj
        absolute 
        delirante freneza
        <fnt>
          <aut>M. Hailstone</aut>,
          <vrk><url ref="http://www.busswarble.dragnet.com.au/sekreto.htm">
          la Sekreto de Insulo Roan</url></vrk>,
          <lok>1999-2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne preterlasu la <tld/>an jubile-jaran interpretadon fare de la
        koruso
        <klr>[&#x2026;]</klr> &ndash; &leftquot;pura&#x201D; muziko por eonoj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2014/011202.php">Pura
          muziko por eonoj</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ajnis kvaza&ubreve; penetra <tld/>o vidigus al &scirc;i la realon de la 
        aferoj
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_s;&c_n;&c_y; <klr>(&c_p;&c_r;&c_a; &c_r;&c_o;&c_z;&c_u;&c_m;)</klr></trd>
  <trdgrp lng="cs">
    <trd>jasn&yacute;</trd>,
    <trd>lucidn&iacute;</trd>,
    <trd>sv&ecaron;tl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>klarsichtig</trd>,
    <trd>aufgeweckt</trd>,
    <trd>helle <klr>(Geist)</klr></trd>
  </trdgrp>
  <trd lng="fr">lucide</trd>
  <trdgrp lng="nl">
    <trd>scherpzinnig<klr tip="ind"> (zeldz.)</klr></trd>,
    <trd>lucide<klr tip="ind"> (zeldz.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przytomny</trd>,
    <trd>jasny</trd>,
    <trd>&sacute;wietlany</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>con&#x219;tient</trd>,
    <trd>evident</trd>,
    <trd>lumin&abreve;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jasn&yacute;</trd>,
    <trd>lucidn&yacute;</trd>,
    <trd>svetl&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x44F;&c_s;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_v;&c_ib;&c_t;&#x43B;&c_i;&c_j; (&#x43F;&c_r;&#x43E; &c_r;&#x43E;&c_z;&c_u;&#x43C;</trd>,
    <trd>&c_s;&c_v;&c_ib;&c_d;&#x43E;&#x43C;&c_ib;&c_s;&c_t;&#x44C;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7CBE;&#x8F9F; <pr>j&#x12B;ngp&#xEC;</pr></trd>,
    <trd>&#x7CBE;&#x95E2; <pr>j&#x12B;ngp&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>

