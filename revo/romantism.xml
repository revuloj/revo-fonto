<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: romantism.xml,v 1.13 2023/07/18 16:37:29 revo Exp $">
<kap>
  <rad>romantism</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="romantism.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="romant.0ismo">Romantikismo</ref>:
      <ekz>
        &ccirc;io &ccirc;i estas <tld/>o, sensenca&jcirc;o, putreco, artismo
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 7a, p. 35a</lok></fnt>;
      </ekz>
      <ekz>
        tiu homa <esc>atitudo</esc> <klr>(sinteno)</klr> <klr>[&#8230;]</klr> formi&gcirc;is nur anta&ubreve; apena&ubreve;
        pli ol du jarcentoj
        surbaze de la idearo de <tld/>o
        <fnt>
          <aut>Vilmos Benczik</aut>
          <vrk><url ref="https://mek.oszk.hu/16800/16840/16840.pdf">Pri la natureco kaj artefariteco
          de lingvoj</url></vrk>
          <lok>La nocio &leftquot;natureco&rightquot;</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_a;&c_n;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="bg">&c_r;&c_o;&c_m;&c_a;&c_n;&c_t;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="ca">romanticisme</trd>
  <trd lng="cs">romantizmus</trd>
  <trd lng="de">Romantik</trd>
  <trd lng="es">romanticismo</trd>
  <trdgrp lng="fr">
    <trd>romantisme</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>romantika<klr tip="amb"> (st&iacute;lusir&aacute;ny)</klr></trd>,
    <trd>romanticizmus</trd>
  <trd lng="it">romanticismo</trd>
  </trdgrp>
  <trd lng="nl">romantiek</trd>
  <trd lng="pl">romantyzm</trd>
  <trdgrp lng="pt">
    <trd>romantismo</trd>,
    <trd>romanticismo</trd>
  </trdgrp>
  <trd lng="ro">romantism</trd>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">romantizmus</trd>
  <trd lng="uk">&c_r;&#x43E;&#x43C;&c_a;&#x43D;&c_t;&c_i;&c_z;&#x43C;</trd>
</drv>
</art>

