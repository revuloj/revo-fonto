<vortaro>
<art mrk="$Id$">
<kap>
  <rad>romantism</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="romantism.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="romant.0ismo">romantikismo</ref>
    <ekz>
      &ccirc;io &ccirc;i estas <tld/>o, sensenca&jcirc;o putreco, artismo
      <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 7a, p. 35a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_a;&c_n;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="de">Romantik</trd>
  <trd lng="es">romanticismo</trd>
  <trdgrp lng="fr">
    <trd>romantisme</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>romantika<klr tip="amb"> (st&iacute;lusir&aacute;ny)</klr></trd>,
    <trd>romanticizmus</trd>
  <trd lng="it">romanticismo</trd>
  </trdgrp>
  <trd lng="nl">romantiek</trd>
  <trdgrp lng="pt">
    <trd>romantismo</trd>,
    <trd>romanticismo</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;&c_t;&c_i;&c_z;&c_m;</trd>
</drv>
</art>

