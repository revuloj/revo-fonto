</dif>
<refgrp tip="sin">
<ref cel="diktat.0o">diktatoro</ref>.
</refgrp>
</snc>
<trdgrp lng="ru">
  <trd>&c_d;&c_je;&c_s;&c_p;&c_o;&c_t;&c_i;&c_z;&c_m;</trd>,
  <trd>&c_d;&c_je;&c_s;&c_p;&c_o;&c_t;&c_i;&c_ja;</trd>
</trdgrp>
</drv>
