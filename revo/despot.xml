<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>despot</rad>/o
</kap>
<drv mrk="despot.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Regnestro, kies a&ubreve;toritato estas absoluta, a&ubreve;tokrata, tirana.
    </dif>
    <refgrp tip="vid">
      <ref cel="diktat.0o">diktatoro</ref>,
      <ref cel="tiran.0o">tirano</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_s;&c_p;&c_a;&c_t;</trd>
  <trd lng="bg">&c_d;&c_je;&c_s;&c_p;&c_o;&c_t;</trd>
  <trdgrp lng="de">
     <trd>Gewaltherrscher</trd>,
     <trd>Despot</trd>
  </trdgrp>
  <trd lng="en">despot</trd>
  <trd lng="es">d&eacute;spota</trd>
  <trd lng="fr">despote</trd>
  <trdgrp lng="hu">
     <trd>despota</trd>,
     <trd>k&eacute;ny&uacute;r</trd>,
     <trd>zsarnok</trd>
  </trdgrp>
  <trd lng="pt">d&eacute;spota</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_s;&c_p;&c_o;&c_t;</trd>,
    <trd>&c_t;&c_i;&c_r;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tyran</trd>,
    <trd>despota</trd>
  </trdgrp>
</drv>

<drv mrk="despot.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Regado de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_e;&c_s;&c_p;&c_a;&c_t;&c_y;&c_z;&c_m;</trd>,
    <trd>&c_d;&c_e;&c_s;&c_p;&c_a;&c_t;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_je;&c_s;&c_p;&c_o;&c_t;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trdgrp lng="de">
     <trd>Gewaltherrschaft</trd>,
     <trd>Despotismus</trd>
  </trdgrp>
  <trd lng="en">despotism</trd>
  <trd lng="es">despotismo</trd>
  <trd lng="fr">despotisme</trd>
  <trdgrp lng="hu">
     <trd>&ouml;nk&eacute;nyuralom</trd>,
     <trd>zsarnoks&aacute;g</trd>,
     <trd>despotizmus</trd>
  </trdgrp>
  <trd lng="pt">despotismo</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_s;&c_p;&c_o;&c_t;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_d;&c_je;&c_s;&c_p;&c_o;&c_t;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">tyrania</trd>
</drv>
</art>
