<vortaro>
<art mrk="$Id$">
<kap>
  <rad>trejn</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="trejn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trejn.0i.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Prepari por sporto, batalado ktp, <ref tip="super"
      cel="ekzerc.0i">ekzercante</ref> kaj hardante:
      <ekz>
        <tld/>i &ccirc;evalon por vetkurado, soldaton por skermado;
      </ekz>
      <ekz>
        intensa <ind><tld/>ado</ind>.
        <trd lng="be">&c_t;&c_r;&c_e;&c_n;&c_ib;&c_r;&c_o;&c_w;&c_k;&c_a;</trd>
        <trd lng="hu">edz&eacute;s</trd>
        <trd lng="ru">&c_t;&c_r;&c_je;&c_n;&c_i;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_e;&c_n;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">train</trd>
  <trd lng="es">entrenar</trd>
  <trd lng="fr">entrainer<klr> (sport etc.)</klr></trd>
  <trd lng="hu">edz</trd>
  <trd lng="nl">trainen</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
