<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>profesor</rad>/o
</kap>
<drv mrk="profes1.0o">
  <kap><tld/>o</kap>
  <snc mrk="profes1.0o.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Altranga <ref tip="super" cel="instru.0isto.PED">instruisto</ref> en
      supera lernejo:
      <ekz>
        licea, universitata <tld/>o pri &hcirc;emio, lingvistiko;
      </ekz>
      <ekz>
        <ind>gimnazia <tld/>o</ind>;
        <trd lng="hu">gimn&aacute;ziumi <ind>tan&aacute;r</ind></trd>
      </ekz>
      <ekz>
        akiri <tld/>econ
        <klr>(titolon de <tld/>o)</klr>;
      </ekz>
      <ekz>
        universitata <tld/>o pri genetiko<fnt><aut>Marteno Ecott</aut>,
        <vrk>Himno al toleremo</vrk>, <bib>Monato</bib>, <lok>jaro 1999a,
        numero 5a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_f;&c_e;&c_s;&c_a;&c_r;</trd>
  <trd lng="de">Professor</trd>
  <trdgrp lng="fr">
    <trd>professeur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tan&aacute;r</trd>,
    <trd>professzor</trd>
  </trdgrp>
  <trd lng="pt">professor <klr>(universit&aacute;rio)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_f;&c_je;&c_s;&c_s;&c_o;&c_r;</trd>
  <trd lng="sv">professor</trd>
</drv>
</art>
