<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: defini.xml,v 1.20 2021/03/08 14:29:16 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>definitiv</rad>/a <fnt><bib>PV</bib></fnt>

</kap>
<drv mrk="defini.0a">
  <kap><tld/>a</kap>
  <snc mrk="defini.0a.KOMUNE">
    <dif>
      Tia, ke oni ne plu povas &scirc;an&gcirc;i ion en &gcirc;i,
      <ref cel="sxangx.ne0ebla.KOMUNE">ne&scirc;an&gcirc;ebla</ref>:
      <ekz>
        <tld/>aj kondi&ccirc;oj;
      </ekz>
      <ekz>
        <tld/>a respondo, prezo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="decid.0a">decida</ref>,
      <ref cel="fin.0ofara">finofara</ref>,
      <ref cel="instan">lastinstanca</ref>,
      <ref cel="proviz1">malprovizora</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_ch;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_e;&c_f;&c_ib;&c_n;&c_ib;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>definitiu</trd>,
    <trd>decisiu</trd>,
    <trd>concloent</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>definitivn&iacute;</trd>,
    <trd>kone&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>endg&uuml;tig</trd>,
    <trd>abschlie&szlig;end</trd>,
    <trd>unwiderruflich</trd>,
    <trd>definitiv</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>definitive</trd>,
    <trd>definite</trd>
  </trdgrp>
  <trd lng="es">definitivo</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;finitif</trd>,
    <trd>irr&eacute;vocable</trd>,
    <trd>ultime</trd>
  </trdgrp>
  <trd lng="he">&#x5E1;&#x5D5;&#x5B9;&#x5E4;&#x5B4;&#x5D9;</trd>
  <trd lng="hu">v&eacute;gleges</trd>
  <trd lng="pl">definitywny</trd>
  <trdgrp lng="pt">
    <trd>definitivo</trd>,
    <trd>decisivo</trd>
  </trdgrp>
  <trd lng="ro">definitiv</trd>
  <trd lng="ru">&c_o;&c_k;&c_o;&c_n;&c_ch;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>definit&iacute;vny</trd>,
    <trd>kone&ccaron;n&yacute;</trd>,
    <trd>trval&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="defini.0o">
  <kap><tld/>o</kap>
  <snc mrk="defini.0o.KOMUNE">
    <dif>
      <tld lit="D"/>a stato de aferoj:
      <ekz>
        Manjo iom post iom akceptis la <tld/>on de la aran&gcirc;o
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">caract&egrave;re <ind>d&eacute;finitif</ind></trd>
</drv>

<drv mrk="defini.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="defini.0igi.KOMUNE">
    <dif>
      Igi <tld/>a:
      <ekz>
        post la <tld/>i&gcirc;o de la lingvo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fiks.0i">fiksi</ref>,
      <ref cel="decid.0i">decidi</ref>,
      <ref cel="difin.0i">difini</ref>,
      <ref cel="firm.0igi.KOMUNE">firmigi</ref>,
      <ref cel="solv.0i">solvi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>confirmar</trd>,
    <trd>refermar</trd>,
    <trd>ratificar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>confirm</trd>,
    <trd>ratify</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>arr&ecirc;ter<klr tip="ind"> (une d&eacute;cision)</klr></trd>,
    <trd>fixer<klr tip="ind"> (par d&eacute;cision)</klr></trd>
  </trdgrp>
  <trd lng="hu">v&eacute;gleges&iacute;t</trd>
  <trd lng="pl">zdecydowa&cacute; ostatecznie</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_f;&c_i;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_o;&c_n;&c_ch;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o; &c_o;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
