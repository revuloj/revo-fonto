<trdgrp lng="fr">
  <trd>d&eacute;finitif</trd>,
  <trd>irr&eacute;vocable</trd>,
  <trd>ultime</trd></trdgrp> 
<trd lng="ru">&c_o;&c_k;&c_o;&c_n;&c_ch;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="defini.0igi">
<kap><tld/>igi</kap>
<snc>
<dif>
Igi <tld/>a: <ekz>post la <tld/>i&gcirc;o de la lingvo<fnt>Z</fnt>.
</ekz> 
</dif>
<refgrp tip="vid">
<ref cel="solv.0i">solvi</ref>,
<ref cel="fiks.0i">fiksi</ref>,
<ref cel="firm">firmigi</ref>,
<ref cel="decid.0i">decidi</ref>,
<ref cel="difin.0i">difini</ref>.
</refgrp>
</snc>
<trdgrp lng="fr">
  <trd>arr&ecirc;ter<klr tip="ind"> (une d&eacute;cision)</klr></trd>,
  <trd>fixer<klr tip="ind"> (par d&eacute;cision)</klr></trd></trdgrp> 
</drv>
