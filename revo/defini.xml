<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: defini.xml,v 1.25 2022/10/27 07:31:55 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>definitiv</rad>/a <fnt><bib>PV</bib></fnt>

</kap>
<drv mrk="defini.0a">
  <kap><tld/>a</kap>
  <snc mrk="defini.0a.KOMUNE">
    <dif>
      Tia, ke oni ne plu povas &scirc;an&gcirc;i ion en &gcirc;i,
      <ref cel="sxangx.ne0ebla.KOMUNE">ne&scirc;an&gcirc;ebla</ref>:
      <ekz>
        <tld/>a decido
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a opinio
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        dioj, kiuj &ccirc;iam agas kaj ne faras ion <tld/>an
        <fnt>
          <bib>Kandid</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>e indignis
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a teksto de tiuj &ccirc;i dogmoj estos fiksita nur poste
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        tiuj internaj demandoj <klr>[&#8230;]</klr> ne ricevis <tld/>an respondon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la liberigo estis plena, kaj tiufoje <tld/>a
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia bonfarto unue provizora fari&gcirc;is tute <tld/>a
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="decid.0a">decida</ref>,
      <ref cel="fin.0ofara">finofara</ref>,
      <ref cel="instan">lastinstanca</ref>,
      <ref cel="proviz1">malprovizora</ref>,
      <ref cel="perfek.0a.siaspece">perfekta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_ch;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_e;&c_f;&c_ib;&c_n;&c_ib;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>definitiu</trd>,
    <trd>decisiu</trd>,
    <trd>concloent</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>definitivn&iacute;</trd>,
    <trd>kone&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>endg&uuml;tig</trd>,
    <trd>abschlie&szlig;end</trd>,
    <trd>unwiderruflich</trd>,
    <trd>definitiv</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>definitive</trd>,
    <trd>definite</trd>
  </trdgrp>
  <trd lng="es">definitivo</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;finitif</trd>,
    <trd>irr&eacute;vocable</trd>,
    <trd>ultime</trd>
  </trdgrp>
  <trd lng="he">&samekh;&vav;&#x5B9;&pe;&#x5B4;&yod;</trd>
  <trd lng="hu">v&eacute;gleges</trd>
  <trd lng="pl">definitywny</trd>
  <trdgrp lng="pt">
    <trd>definitivo</trd>,
    <trd>decisivo</trd>
  </trdgrp>
  <trd lng="ro">definitiv</trd>
  <trd lng="ru">&c_o;&c_k;&c_o;&c_n;&c_ch;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>definit&iacute;vny</trd>,
    <trd>kone&ccaron;n&yacute;</trd>,
    <trd>trval&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_s;&c_t;&c_a;&c_t;&#x43E;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43A;&c_ib;&#x43D;&c_c;&c_je;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="defini.0o">
  <kap><tld/>o, <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="defini.0o.KOMUNE">
    <dif>
      <tld lit="D"/>a stato de aferoj:
      <ekz>
        Manjo iom post iom akceptis la <tld/>on de la aran&gcirc;o
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> ne sciis pri la <tld/>eco de tiu &ccirc;i forlaso
        <fnt>
          <bib>KKr</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_ch;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y; &c_s;&c_t;&c_a;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Endg&uuml;ltigkeit</trd>,
    <trd>Unwideruflichkeit</trd>
  </trdgrp><trd lng="fr">caract&egrave;re <ind>d&eacute;finitif</ind></trd>
</drv>

<drv mrk="defini.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="defini.0igi.KOMUNE">
    <dif>
      Igi <tld/>a:
      <ekz>
        estis preparataj la administraj formala&jcirc;oj por <tld/>igi la transakcion, kiam
        unu monaton poste venis oferto de tri aliaj bankoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2008/009505.php">Kreditkrizo da&ubreve;ras</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igo de la kandidatlistoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;11</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiun disi&gcirc;on religie kvaza&ubreve; <tld/>igis la reciprokaj malbenoj
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2014/011397.php">Kiel senkulpe trafi mezen de fratbu&ccirc;a
          milito</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fiks.0i">fiksi</ref>,
      <ref cel="decid.0i">decidi</ref>,
      <ref cel="difin.0i">difini</ref>,
      <ref cel="firm.0igi.KOMUNE">firmigi</ref>,
      <ref cel="solv.0i">solvi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_c;&c_mol; <klr>(&c_k;&c_a;&c_n;&c_ch;&c_a;&c_t;&c_k;&c_o;&c_v;&c_a;)</klr></trd>,
    <trd>&c_z;&c_a;&c_c;&c_mol;&c_v;&c_ja;&c_r;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_f;&c_ib;&c_k;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>confirmar</trd>,
    <trd>refermar</trd>,
    <trd>ratificar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>abschlie&szlig;en</trd>,
    <trd>fixieren</trd>
  </trdgrp><trdgrp lng="en">
    <trd>confirm</trd>,
    <trd>ratify</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>arr&ecirc;ter<klr tip="ind"> (une d&eacute;cision)</klr></trd>,
    <trd>fixer<klr tip="ind"> (par d&eacute;cision)</klr></trd>
  </trdgrp>
  <trd lng="hu">v&eacute;gleges&iacute;t</trd>
  <trd lng="pl">zdecydowa&cacute; ostatecznie</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_f;&c_i;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_k;&c_o;&c_n;&c_ch;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o; &c_o;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
