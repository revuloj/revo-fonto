   </trdgrp>
</drv>

<drv mrk="acxet.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="acxet.0ajxo.EKON">
    <dif>
      Tio, kion oni ekhavas kontra&ubreve; mono:
      <ekz>
        &scirc;i devis fari en la firmo <nom>Louvre</nom> kelkajn <tld/>a&jcirc;ojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        sendu tiujn <tld/>a&jcirc;ojn hejmen, &scirc;i diris al la kasisto
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deksepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        la du oraj moneroj kuni&gcirc;is kun la aliaj en la sakon en
        kiun li enmetis siajn <tld/>a&jcirc;ojn
        <fnt><aut>H. Vallienne</aut>, <vrk>Kastelo de Prelongo</vrk>,
        <lok>1907</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="var.0o">varo</ref>
    </refgrp>
    <trd
    lng="th">&#xE02;&#xE2D;&#xE07;&#xE0B;&#xE37;&#xE49;&#xE2D;&#xE02;&#xE2D;&#xE07;&#xE02;&#xE32;&#xE22;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_u;&c_p;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_v;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>compra</trd>,
    <trd>adquisici&oacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Einkauf</trd>,
    <trd>Besorgung</trd>
  </trdgrp>
  <trd lng="en">purchase<klr tip="ind"> (thing that has been
  bought)</klr></trd>
  <trd lng="es">producto <ind>comprado</ind></trd>
  <trdgrp lng="fr">
    <trd>achat<klr> (objet achet&eacute;)</klr></trd>,
    <trd>course<klr> (objet achet&eacute;)</klr></trd>,
    <trd>emplette</trd>
  </trdgrp>
  <trd lng="id"><ind>barang</ind> belian</trd>
  <trdgrp lng="ja">
    <trd>&#x8CB7;&#12356;&#29289; <pr>&#x304B;&#12356;&#12418;&#x306E;</pr></trd>,
    <trd>&#x8CFC;&#20837;&#x54C1; <pr>&#12371;&#12358;&#x306B;&#12421;&#12358;&#12402;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aankoop<klr tip="ind"> (gekocht waar)</klr></trd>,
    <trd>aanschaf<klr tip="ind"> (gekocht waar)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
