    <trd>rescatar</trd>,
    <trd>redimir</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>freikaufen</trd>,
    <trd>ausl&ouml;sen <klr>(freikaufen)</klr></trd>,
    <trd>loskaufen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pay ransom</trd>,
    <trd>bail somebody out</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>redimir</trd>,
    <trd>rescatar</trd>,
    <trd>recuperar</trd>
  </trdgrp>
  <trd lng="fa">&a_b;&a_A;&a_z;&a_kh;&a_r;&f_y;&a_d;
  &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
