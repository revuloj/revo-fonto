<drv mrk="acxet.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="acxet.0i.EKON">
    <dif>
      <ref tip="super" cel="akir.0i">Akiri</ref> per mono:
      <ekz>
        mi <tld/>is dekon da ovoj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 32</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi <tld/>os el la manoj de via proksimulo, unu ne malprofitigu la alian
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 25:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        li malkare <tld/>is la &ccirc;evalon
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i malmultekoste
        <fnt>
          <bib>ChV</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        li a&ccirc;etis por la du knabinoj belajn vestojn, perlojn kaj multekostajn &scirc;tonojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi venas al vi kun peto, ke vi volu <tld/>i de mi mian tempon kaj la laboron de
        miaj manoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi volas <tld/>i senmortecon per bruligmortigoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>?
      </ekz>
      <ekz>
        per la fruktoj de l&#8217; farita krimo &parsl; <tld lit="A"/>i la favoron de la le&gcirc;o
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bankiero <tld/>is <klr>[&#8230;]</klr> kampodomon &ccirc;irka&ubreve;itan de granda &gcirc;ardeno
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &gcirc;in <tld/>os por la prezo, kiun vi petos
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ove Ramel</nom> <klr>[&#8230;]</klr> <tld/>is per a&ubreve;kcio la kastelon kaj la tutan meblaron
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis la glora epoko de financistoj kiuj <tld/>is, 
	<ref tip="ant" cel="vend.0i">vendi</ref>s kaj
        dispecigis kompaniojn
        <fnt>
          <aut>R. Rotsaert</aut>,
          <vrk>Piramidaj investoj...</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1997a, numero 7a, p. 24a</lok>
        </fnt>;
      </ekz>
      <ekz>
        mar&ccirc;andado <tld/>i ne devigas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mankis al Petro klopodoj, li <tld/>is al si domon
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
