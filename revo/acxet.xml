      <ref cel="lu.0i">lui</ref>,
      <ref cel="pag.0i.EKON">pagi<sncref/></ref>,
      <ref cel="labor.per0i">perlabori</ref>,
      <ref cel="prunt.0i">prunti</ref>
    </refgrp>
