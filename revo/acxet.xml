<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap><ofc>*</ofc><rad>a&ccirc;et</rad>/i</kap>

<drv mrk="acxet.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Akiri per mono: 
      <ekz><tld/>i firme, kontante a&ubreve; kredite;</ekz> 
      <ekz><tld/>i ion de, el iu<fnt>Z</fnt>;  </ekz> 
      <ekz>de<tld/>i ion de iu.</ekz>
    </dif>
  </snc>
    <trdgrp lng="en">
    <trd>buy</trd>,
    <trd>purchase</trd>
  </trdgrp>
  <trd lng="be">&c_k;&c_u;&c_p;&c_l;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="cs">koupit</trd>
  <trd lng="de">kaufen</trd>
  <trd lng="es">comprar</trd>
  <trd lng="fr">acheter</trd>
  <trd lng="nl">kopen</trd>
  <trd lng="pt">comprar</trd>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">k&ouml;pa</trd>
  <trdgrp lng="it">
    <trd>acquistare</trd>,
    <trd>comprare</trd>
  </trdgrp>
</drv>

<drv mrk="acxet.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Povanta esti <tld/>ata:  
      <ekz><tld/>ebla domo, ju&gcirc;isto.</ekz>
    </dif>
  </snc>
  <trd lng="en">for <ind>sale</ind></trd>
  <trdgrp lng="fr">
    <trd>achetable</trd>,
    <trd>&agrave; <ind>vendre</ind></trd>
  </trdgrp>
  <trd lng="it">acquistabile</trd>
  <trd lng="nl">te <ind>koop</ind></trd>
  <trdgrp lng="pt">
    <trd>compr&aacute;vel</trd>,
    <trd>adquir&iacute;vel</trd>,
    <trd>&agrave; <ind>venda</ind></trd>
  </trdgrp>
</drv>

<drv mrk="acxet.el0i">
  <kap>el<tld/>i</kap>
  <snc>
    <dif>
      Pagi por liberigi iun el  kaptiteco fizika a&ubreve; morala: 
      <ekz>&ccirc;e la Romanoj, la sklavoj povis  sin el<tld/>i;</ekz> 
      <ekz>
        la&ubreve; kristana kredo, Jesuo el<tld/>is la homojn 
        per sia  morto.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">ransom</trd>
  <trd lng="be">&c_v;&c_y;&c_k;&c_u;&c_p;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>racheter</trd>,
    <trd>affranchir</trd>
  </trdgrp>
  <trd lng="it">liberare, affrancare</trd>
  <trd lng="nl">afkopen</trd>
  <trdgrp lng="pt">
    <trd>remir</trd>,
    <trd>resgatar</trd>,
    <trd>alforriar</trd>
  </trdgrp>
  <trd lng="sv"><ind>k&ouml;pa</ind> loss</trd>
</drv>

<drv mrk="acxet.sub0i">
  <kap>sub<tld/>i</kap>
  <snc>
    <dif>
      Delogi per mono, korupti.
    </dif>
    <refgrp tip="vid">
      <ref cel="sxmir.0i">&scirc;miri</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>bribe</trd>,
      <trd>corrupt</trd>
    </trdgrp>
    <trd lng="be">&c_p;&c_a;&c_d;&c_k;&c_u;&c_p;&c_ib;&c_c;&c_mol;</trd>
    <trdgrp lng="fr">
      <trd>acheter</trd>,
      <trd>corrompre</trd>
    </trdgrp>
    <trd lng="it">corrompere</trd>
    <trd lng="nl">omkopen</trd>
    <trdgrp lng="pt">
      <trd>subornar</trd>,
      <trd>corromper</trd>
    </trdgrp>
    <trd lng="sv">muta</trd>
  </snc>
</drv>
</art>
