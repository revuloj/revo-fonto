<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxaligram.xml,v 1.8 2024/09/10 21:27:11 revo Exp $">
<kap>
  <rad>&scirc;aligram</rad>/o
</kap>
<drv mrk="sxaligram.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxaligram.0o.HIN">
    <uzo tip="fak">HIN</uzo>
    <dif>
      &Scirc;tono kun amonita fosilio, trovebla en kelkaj lokoj de Himalajo,
      kultata kiel formo de la dio <ref tip="vid"
      cel="visxnu.0o.HIN">Vi&scirc;nuo</ref>:
      <ekz>
        &Scirc;akuntala volis trovi <tld/>on
        <fnt>
        <aut>R. Manandhar</aut>,
        <vrk><url ref="http://razeno.blog.com/">Mia voja&gcirc;o al
        Muktinath</url></vrk>,
        <lok>2006-04-18</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/1/17/Shaligram_(2).jpg/640px-Shaligram_(2).jpg"
      prm="CC BY-SA 4.0"
      ><tld/>oj
        <fnt>
          <aut>AswiniKP, propra verko</aut>
          <lok><url ref="&WCW;/File:Shaligram_(2).jpg"
              >https://commons.wikimedia.org/wiki/File:Shaligram_(2).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_l;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;-&c_sh;&c_y;&c_l;&c_a;</trd>
  <trd lng="ca">f&ograve;ssil d'ammonit</trd>
  <trdgrp lng="de">
    <trd>Salagram</trd>,
    <trd>Shaligram</trd>
  </trdgrp>
  <trd lng="fr">shaligram</trd>
  <trd lng="hu">saligram</trd>
  <trdgrp lng="nl">
    <trd>saligram<klr tip="ind"> (ammoniet)</klr></trd>,
    <trd>shaligram<klr tip="ind"> (ammoniet)</klr></trd>
  </trdgrp>
  <trd lng="pl">szaligram</trd>
</drv>
</art>

