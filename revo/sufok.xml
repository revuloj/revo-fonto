<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sufok.xml,v 1.28 2023/10/21 07:46:48 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sufok</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sufok.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sufok.0i.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Malhelpi la spiradon, senigi je aero, eventuale <ref tip="super" cel="mort.0igi.malvivigi">mortigi</ref>;
      <ref tip="sin" cel="asfiks.0i.FIZL">asfiksii</ref>:
      <ekz>
        <tld/>i iun premegante la kolon<fnt>B</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>anta varmego</ind>.
        <trd lng="hu">fullaszt&oacute; h&odblac;s&eacute;g</trd>
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>asphyxier<klr> (tr.)</klr></trd>
      <trd>&eacute;touffer<klr tip="amb"> (tr.)</klr></trd>,
      <trd>suffoquer<klr tip="amb"> (tr.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fojt</trd>,
      <trd>megfojt</trd>
    </trdgrp>
  </snc>
  <snc mrk="sufok.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="help.mal0i">Malhelpi</ref> per ia da&ubreve;ra
      surpremo:
      <ekz>
        &scirc;ia vo&ccirc;o estis malforta, obtuza; &gcirc;in tute <klr>...</klr>
        <tld/>is
        la strata bruo
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        li <tld/>is en si la kompaton
        <fnt><bib>MT</bib>, <lok>&Amo; 1:11</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiuj sa&gcirc;aj konsideroj ne povis tamen <tld/>i malgrandan,
        sed
        turmentantan maltrankvilon en la koro de l' kronprinco
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 12a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i ribelon;
      </ekz>
      <ekz>
        la vila&gcirc;anoj estis <tld/>ataj per la impostoj
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 17a</lok></fnt>;
      </ekz>
      <ekz>
        tiuj persekutoj ne povos <tld/>i la veron.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="damp.0i">dampi</ref>,
      <ref cel="dron.0igi">dronigi</ref>,
      <ref cel="prem.sub0i">subpremi</ref>
    </refgrp>
    <trdgrp lng="fr">
      <trd>faire taire</trd>,
      <trd>oppresser</trd>,
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elfojt</trd>,
      <trd>elnyom</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_d;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>dusit</trd>,
    <trd>rdousit</trd>,
    <trd>tlumit</trd>,
    <trd>udusit</trd>,
    <trd>utla&ccaron;ovat</trd>,
    <trd>utlumit</trd>,
    <trd>zadusit</trd>,
    <trd>za&scaron;krtit</trd>,
    <trd>&scaron;krtit</trd>
  </trdgrp>
  <trd lng="de">ersticken</trd>
  <trd lng="en">suffocate</trd>
  <trd lng="es">sofocar</trd>
  <trdgrp lng="ja">
    <trd>&#x7A92;&#x606F;&#x6B7B;&#x3055;&#x305B;&#x308B;  <pr>&#x3061;&#x3063;&#x305D;&#x304F;&#x3057;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x7D5E;&#x3081;&#x6BBA;&#x3059;  <pr>&#x3057;&#x3081;&#x3053;&#x308D;&#x3059;</pr></trd>,
    <trd>&#x7A92;&#x606F;&#x3055;&#x305B;&#x308B;  <pr>&#x3061;&#x3063;&#x305D;&#x304F;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x606F;&#x3092;&#x8A70;&#x307E;&#x3089;&#x305B;&#x308B;  <pr>&#x3044;&#x304D;&#x3092;&#x3064;&#x307E;&#x3089;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x3082;&#x307F;&#x6D88;&#x3059;  <pr>&#x3082;&#x307F;&#x3051;&#x3059;</pr></trd>,
    <trd>&#x62BC;&#x3057;&#x6BBA;&#x3059;  <pr>&#x304A;&#x3057;&#x3053;&#x308D;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>doen <ind>stikken</ind></trd>,
    <trd>smoren</trd>,
    <trd>wurgen</trd>
  </trdgrp>
  <trd lng="pl">dusi&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dusi&tcaron;</trd>,
    <trd>utl&aacute;&ccaron;a&tcaron;</trd>,
    <trd>zadusi&tcaron;</trd>,
    <trd>&scaron;krti&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x443;&#x448;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x438;&#x434;&#x443;&#x448;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x433;&#x43B;&#x443;&#x448;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x433;&#x43B;&#x443;&#x448;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="sufok.0a">
  <kap><tld/>a</kap>
  <snc mrk="sufok.0a.FIZL">
    <dif>
      Kiu baras la spiron, a&ubreve; preme malhelpas vivan agadon:
      <ekz>
        <tld/>a tusado
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la hejmo de la metiisto,
        (de
        nov-jorka kuracisto, rerakontita de E.
        Weilsh&auml;user)</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a timo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>dusiv&yacute;</trd>,
    <trd>dusn&yacute;</trd>
  </trdgrp>
  <trd lng="en">suffocating</trd>
  <trdgrp lng="fr">
    <trd>&eacute;touffant</trd>,
    <trd>oppressant</trd>,
    <trd>suffoquant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fojt&oacute;</trd>,
    <trd>fojtogat&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7A92;&#x606F;&#x3059;&#x308B;&#x3088;&#x3046;&#x306A;  <pr>&#x3061;&#x3063;&#x305D;&#x304F;&#x3059;&#x308B;&#x3088;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x606F;&#x82E6;&#x3057;&#x3044;  <pr>&#x3044;&#x304D;&#x3050;&#x308B;&#x3057;&#x3044;</pr></trd>,
    <trd>&#x606F;&#x8A70;&#x307E;&#x308B;  <pr>&#x3044;&#x304D;&#x3065;&#x307E;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verstikkend</trd>,
    <trd>benauwd</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>duszny</trd>,
    <trd>dusz&aogonek;cy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_d;&c_u;&c_sh;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dusiv&yacute;</trd>,
    <trd>dusn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x434;&#x443;&#x448;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x437;&#x430;&#x434;&#x443;&#x448;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="sufok.0o">
  <kap><tld/>o</kap>
  <snc mrk="sufok.0o.FIZL">
    <dif>
      Malhelpo de spirado,
      <ref tip="sin" cel="asfiks.0o.FIZL">asfiksio</ref>.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>du&scaron;en&iacute;</trd>,
    <trd>sufokace</trd>,
    <trd>udu&scaron;en&iacute;</trd>,
    <trd>zadu&scaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>asphyxie</trd>,
    <trd>suffocation</trd>
  </trdgrp>
  <trd lng="pl">duszno&sacute;&cacute;</trd>
  <trdgrp lng="sk">
    <trd>dusenie</trd>,
    <trd>udusenie</trd>,
    <trd>zadusenie</trd>
  </trdgrp>
</drv>

<drv mrk="sufok.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sufok.0igxi.FIZL">
    <dif>
      Penegi por spirado, suferi pro malhelpa premo eventuale &gcirc;is
      morto:
      <ekz>
        mortu kiel hundo a&ubreve; <tld/>i&gcirc;u por &ccirc;iam
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 11a</lok></fnt>!
      </ekz>
      <ekz>
        <tld/>i&gcirc;i pro kolero, pro rido.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_d;&c_y;&c_h;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_d;&c_u;&c_sh;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="en">suffocate</trd>
  <trdgrp lng="fr">
    <trd>&eacute;touffer<klr tip="amb"> (intr.)</klr></trd>,
    <trd>suffoquer<klr tip="amb"> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="es">sofocar se<klr tip="amb"> (reflexivo)</klr></trd>
  <trdgrp lng="hu">
    <trd>fullad</trd>,
    <trd>megfullad</trd>,
    <trd>fulladozik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7A92;&#x606F;&#x3059;&#x308B;  <pr>&#x3061;&#x3063;&#x305D;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x606F;&#x304C;&#x8A70;&#x307E;&#x308B;  <pr>&#x3044;&#x304D;&#x304C;&#x3064;&#x307E;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">stikken</trd>
  <trd lng="pl">dusi&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_d;&c_y;&c_h;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_o;&c_h;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x443;&#x441;&#x438;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x437;&#x430;&#x434;&#x438;&#x445;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="sufok.0itajxo">
  <kap><tld/>ita&jcirc;o</kap>
  <snc mrk="sufok.0itajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Viando el besto mortigita per <tld/>o, sen ellaso de la sango:
      <ekz>
        ili detenu sin de malpuri&gcirc;oj &ccirc;e idoloj kaj de mal&ccirc;asteco kaj
        de <tld/>ita&jcirc;oj
        <fnt><bib>NT</bib>, <lok>&Ago; 15:20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">viande &eacute;touff&eacute;e</trd>
  <trd lng="hu">f&uacute;lvaholt &aacute;llat</trd>
  <trd lng="nl">gesmoord <ind>vlees</ind></trd>
</drv>
</art>
