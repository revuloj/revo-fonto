<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>leon</rad>/o
</kap>
<drv mrk="leon.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="panter.0o.genro">pantero</ref>
      (<trd lng="la">Panther leo</trd>), kies virseksulo portas densan
      kolhararon.
    </dif>
  </snc>
  <trd lng="cs">lev</trd>
  <trd lng="de">L&ouml;we</trd>
  <trd lng="en">lion</trd>
  <trd lng="fr">lion</trd>
  <trd lng="es">le&oacute;n</trd>
  <trd lng="nl">leeuw</trd>
  <trd lng="pl">lew</trd>
  <trd lng="ru">&c_l;&c_je;&c_v;</trd>
  <trd lng="sv">lejon</trd>
  <trd lng="tr">aslan</trd>
</drv>

<drv mrk="leon.L0o">
  <kap>
    <tld lit="L"/>o
  </kap>
  <snc>
    <uzo tip="fak">AST</uzo>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt"
      cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Leo</trd>):
      <ekz>
        oni vidis la stelaron de Leono
        <fnt>(<aut>Prus, trad. Kabe</aut>: <vrk>La Faraono, 1a vol.</vrk>,
        <lok>&ccirc;ap. 24</lok>)</fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">L&ouml;we</trd>
  <trd lng="en">Leo</trd>
  <trd lng="es">le&oacute;n</trd>
  <trd lng="nl">leeuw</trd>
  <trd lng="pl">lew</trd>
  <trd lng="ru">&c_L;&c_je;&c_v;</trd>
  <trd lng="sv">Lejonet</trd>
  <trd lng="tr">Aslan</trd>
</drv>

<drv mrk="leon.0fauxko">
  <kap>
    <tld/>fa&ubreve;ko
  </kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="sin" cel="antiri.0o">Antirino</ref>
    </dif>
  </snc>
  <trd lng="de">L&ouml;wenmaul</trd>
  <trdgrp lng="fr">
    <trd>muflier</trd>,
    <trd>gueule-de-loup</trd>
  </trdgrp>
  <trd lng="hu">oroszl&aacute;nsz&aacute;j</trd>
  <trd lng="nl">leeuwenbek</trd>
  <trd lng="ru">&c_l;&c_mol;&c_v;&c_i;&c_n;&c_y;&c_j; &c_z;&c_je;&c_v;</trd>
  <trd lng="sv">lejongap</trd>
</drv>
</art>
