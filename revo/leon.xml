<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>leon</rad>/o
</kap>
<drv mrk="leon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="panter.0o.2">pantero 2</ref>
      (<trd lng="la">Panther leo</trd>), kies virseksulo portas densan
      kolhararon.
   </dif>
  </snc>
  <trd lng="cs">lev</trd>
  <trd lng="de">L&ouml;we</trd>
</drv>
<drv mrk="L0o">
  <kap><tld lit="L"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Leo</trd>), tra kiu la suno pasas de la 23a de julio
      &gcirc;is la 25a de a&ubreve;gusto.
    </dif>
  </snc>
  <trd lng="de">L&ouml;we</trd>
</drv>
</art>
