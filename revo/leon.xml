<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: leon.xml,v 1.52 2024/01/22 01:39:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>leon</rad>/o
</kap>
<drv mrk="leon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="leon.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">Specio</ref>
       de <ref tip="super" cel="panter.0o.genro">pantero</ref>
      (<trd lng="la">Panther leo</trd>),
      <ref tip="lst" cel="best.rabo0o" lst="voko:rabobestoj">rabobesto</ref>
      el Afriko kaj Okcidenta Azio,
      kies virseksulo portas densan kolhararon:
      <ekz>
        al <tld/>o ne donu la manon
        <fnt><bib>F</bib>, <lok>&FE; 7</lok></fnt>;
      </ekz>
      <ekz>
        kion rajtas <tld/>o, ne rajtas azeno
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne naskas porko <tld/>idon, nek korniko aglidon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o nur unu fojon ekblekis, kaj &ccirc;iuj forsaltis.
        <fnt><bib>ElektFab</bib>, <lok>Du fratoj</lok></fnt>;
      </ekz>
      <ekz>
        jen la popolo stari&gcirc;as kiel <tld/>ino Kaj levi&gcirc;as kiel <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 23:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej kura&gcirc;a, kiu havas koron, similan al la koro de leono
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 17:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur &gcirc;iaj muretoj li skulptis kerubojn, <tld/>ojn kaj palmojn
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel kriego de <tld/>o estas la kolero de re&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 19:12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_w;</trd>
  <trd lng="bg">&c_l;&c_malmol;&c_v;</trd>
  <trd lng="bo">&t_sa;&t_drengbu;&t_nga;&t_tsheg;&t_ga;&t_drengbu;&t_tsheg;</trd>
  <trd lng="br">leon</trd>
  <trd lng="ca">lle&oacute;</trd>
  <trd lng="cs">lev</trd>
  <trd lng="de">L&ouml;we</trd>
  <trd lng="el">&lambda;&jota;&omikron;&ny;&tau;&alfa_ton;&rho;&jota;</trd>
  <trd lng="en">lion</trd>
  <trd lng="es">le&oacute;n</trd>
  <trd lng="fr">lion</trd>
  <trd lng="he">&alef;&resh;&yod;&he;</trd>
  <trd lng="hu">oroszl&aacute;n</trd>
  <trd lng="id">singa</trd>
  <trd lng="it">leone</trd>
  <trd lng="nl">leeuw</trd>
  <trd lng="pl">lew</trd>
  <trd lng="pt">le&atilde;o</trd>
  <trd lng="ro">leu</trd>
  <trd lng="ru">&c_l;&c_je;&c_v;</trd>
  <trd lng="sk">lev</trd>
  <trd lng="sv">lejon</trd>
  <trd lng="tr">aslan</trd>
  <trdgrp lng="zh">
    <trd>&#x72EE;&#x5B50; <pr>sh&#x12B;zi</pr></trd>,
    <trd>&#29509;&#x5B50; <pr>sh&#x12B;zi</pr></trd>,
    <trd>&#x72EE; <pr>sh&#x12B;</pr></trd>,
    <trd>&#29509; <pr>sh&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="leon.L0o">
  <kap><tld lit="L"/>o</kap>
  <snc mrk="leon.0o.AST">
    <uzo tip="fak">AST</uzo>
    <mlg>Leo</mlg>
    <mlg>&#x264C;&#xFE0E;</mlg>
    <dif>
      <ref tip="lst" cel="stel.0figuro" lst="voko:zodiakaj_signoj" val="5">Stelfiguro</ref> kaj
      signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Leo</trd>):
      <ekz>
        oni vidis la stelaron de <tld lit="L"/>o
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 24a</lok></fnt>;
      </ekz>
      <ekz>
        la alfa stelo, kiu nomi&gcirc;as Re&gcirc;eto, situas en la brusto de la <tld lit="L"/>o,
        Malgranda <tld lit="L"/>o sekvas &gcirc;in, situante inter la Granda Ursino kaj la
        <tld lit="L"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16875">Leono (konstelacio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_L;&c_malmol;&c_v;<klr tip="amb">
    (&c_s;&c_malmol;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
    <trd lng="de">L&ouml;we<klr tip="amb"> (Sternbild)</klr></trd>
    <trd lng="es">Leo</trd>
    <trd lng="fr">Lion<klr tip="ind"> (constellation)</klr></trd>
    <trdgrp lng="id">
      <trd>Asad</trd>,
      <trd>Leo</trd>,
      <trd>Singa</trd>
    </trdgrp>
    <trd lng="pl">Lew  <klr>(gwiazdozbi&oacute;r)</klr></trd>
    <trd lng="ro">Leul  <klr>(constela&#x21B;ie)</klr></trd>
  </snc>
  <snc mrk="leon.L0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>:
      <ekz>
        la granda verkisto kaj filozofo
        <ind><tld lit="L"/>o Tolstoj</ind>
        <fnt><bib>EE</bib>, <lok>&ccirc;apitro 5a</lok></fnt>.
        <trd lng="ru">&c_L;&c_je;&c_v;
        <ind>&c_T;&c_o;&c_l;&c_s;&c_t;&c_o;&c_j;</ind></trd>
      </ekz>
    </dif>
    <trd lng="de">Leo<klr tip="amb"> (Name)</klr></trd>
    <trd lng="es">Le&oacute;n</trd>
    <trd lng="fr">L&eacute;on</trd>
    <trd lng="pl">Lew</trd>
    <trd lng="ro">Leon</trd>
  </snc>
  <trd lng="be">&c_L;&c_je;&c_w;</trd>
  <trd lng="bo">&#xF66;&#xF7A;&#xF44;&#xF0B;&#xF42;&#xF7A;&#xF0B;</trd>
  <trd lng="br">Leon</trd>
  <trd lng="ca">Leo</trd>
  <trd lng="cs">lev</trd>
  <trd lng="el">&Lambda;&epsilon_ton;&omega;&ny;</trd>
  <trd lng="en">Leo</trd>
  <trd lng="he">&mem;&zayin;&lamed; &alef;&resh;&yod;&he;</trd>
  <trd lng="hu">Oroszl&aacute;n</trd>
  <trd lng="it">Leone</trd>
  <trdgrp lng="ja">
    <trd>&#x30E9;&#x30A4;&#x30AA;&#x30F3;</trd>,
    <trd>&#29509;&#x5B50;  <pr>&#12375;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">leeuw</trd>
  <trd lng="pt">Le&atilde;o</trd>
  <trd lng="ru">&c_L;&c_je;&c_v;</trd>
  <trd lng="sk">lev</trd>
  <trd lng="sv">Lejonet</trd>
  <trd lng="tr">Aslan</trd>
</drv>
<drv mrk="leon.MalgrandaL0o">
  <kap>Malgranda <tld lit="L"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>LMi</mlg>
  <snc mrk="leon.MalgrandaL0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Leo Minor</trd>):
      <ekz>
        NGC 2971 estas galaksio en konstelacio Malgranda <tld lit="L"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=112911">NGC 2971</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kleiner <ind>L&ouml;we</ind> <klr>(Sternbild)</klr></trd>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_l;&c_y; &c_L;&c_je;&c_w; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="en">Leo Minor</trd>
  <trd lng="fr">Petit Lion<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="pl">Ma&lstroke;y Lew  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Leul Mic  <klr>(constela&#x21B;ie)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x5C0F;&#x72EE;&#x5EA7; <pr>Xi&#x1CE;osh&#x12B;zu&#xF2;</pr></trd>,
    <trd>&#x5C0F;&#29509;&#x5EA7; <pr>Xi&#x1CE;osh&#x12B;zu&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="leon.0fauxko">
  <kap><tld/>fa&ubreve;ko</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="leon.0fauxko.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="antiri.0o">Antirino</ref>:
      <ekz>
        cirklo da diverskoloraj <tld/>fa&ubreve;koj
        <fnt><bib>Jamburg</bib>, <lok>&ccirc;apitro 12a, p.
        72a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_l;&c_mol;&c_v;&c_ib;&c_n;&c_y;
  &c_z;&c_je;&c_w;</trd>
  <trd lng="bg">&c_k;&c_u;&c_ch;&c_je;&c_n;&c_c;&c_je;</trd>
  <trd lng="br">geol-bleiz</trd>
  <trd lng="ca">antirr&iacute;num</trd>
  <trd lng="de">L&ouml;wenmaul</trd>
  <trd lng="en">snapdragon</trd>
  <trdgrp lng="fr">
    <trd>muflier</trd>,
    <trd>gueule-de-loup</trd>
  </trdgrp>
  <trd lng="he">&lamed;&vav;&ayin;-&he;&alef;&resh;&yod;</trd>
  <trd lng="hu">oroszl&aacute;nsz&aacute;j</trd>
  <trd lng="it">bocca di <ind>leone</ind></trd>
  <trd lng="nl">leeuwenbek</trd>
  <trdgrp lng="pl">
    <trd>wy&zdot;lin</trd>,
    <trd>lwia paszcza</trd>
  </trdgrp>
  <trd lng="ro">gura leului</trd>
  <trd lng="ru">&c_l;&c_mol;&c_v;&c_i;&c_n;&c_y;&c_j;
  &c_z;&c_je;&c_v;</trd>
  <trd lng="sv">lejongap</trd>
  <trd lng="uk">&c_r;&#x43E;&c_t;&c_i;&#x43A;&c_i;</trd>
</drv>
</art>
