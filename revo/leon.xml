<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>leon</rad>/o
</kap>
<drv mrk="leon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="panter.0o.2">pantero 2</ref>
      (<trd lng="la">Panther leo</trd>), kies virseksulo portas densan
      kolhararon.
   </dif>
  </snc>
  <trd lng="cs">lev</trd>
  <trd lng="de">L&ouml;we</trd>
  <trd lng="tr">aslan</trd>
</drv>
<drv mrk="leon.L0o">
  <kap><tld lit="L"/>o</kap>
  <snc num="1">
  <uzo tip="fak">AST</uzo>
    <dif>
     Konstelacio en la <ref cel="zodiak.0o">zodiako 1</ref>
     (<trd lng="la">Leo</trd>), inter Kankro kaj Virgulino.
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Signo de la <ref tip="malprt" cel="zodiak.0o">zodiako 2</ref>
      (<trd lng="la">Leo</trd>), tra kiu la suno pasas 
      proksimume de la 23a de julio
      &gcirc;is la 25a de a&ubreve;gusto.
    </dif>
  </snc>
  <trd lng="de">L&ouml;we</trd>
  <trd lng="tr">Aslan</trd>
</drv>
</art>
