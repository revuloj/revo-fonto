  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_a;</trd>
  <trd lng="br">gavr</trd>
  <trd lng="ca">cabra</trd>
  <trd lng="en">nanny</trd>
  <trd lng="fr">ch&egrave;vre<klr tip="ind"> (animal)</klr></trd>
  <trd lng="io">kaprino</trd>
  <trd lng="ja">&#x96CC;&#x30E4;&#x30AE; <pr>&#x3081;&#x3059;&#x3084;&#x304E;</pr></trd>
  <trdgrp lng="nl">
    <trd>geit</trd>
    <trd>sik</trd>
  </trdgrp>
  <trd lng="pl">koza</trd>
  <trd lng="ro">capr&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_a;</trd>
  <trd lng="uk">&c_k;&c_o;&c_z;&c_a;</trd>
  <trd lng="vo">jikapar</trd>
  <trdgrp lng="zh">
    <trd>&#x7F8A; <pr>y&#xE1;ng</pr></trd>,
    <trd>&#x9EA3; <pr>y&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kapr.0ido">
  <kap>
    <tld/>ido
  </kap>
  <snc mrk="kapr.0ido.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Ido de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_z;&c_mol;&c_l;&c_ja;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_z;&c_mol;&c_l;&c_ja;&c_n;&c_jo;</trd>
  </trdgrp>
  <trd lng="en">young goat</trd>
  <trd lng="fr">chevreau</trd>
  <trdgrp lng="io">
    <trd>kaproyuno</trd>
    <trd>kapryuno</trd>
  </trdgrp>
  <trd lng="it">capretto</trd>
  <trd lng="ja">&#x5B50;&#x30E4;&#x30AE; <pr>&#x3053;&#x3084;&#x304E;</pr></trd>
  <trd lng="nl">lam</trd>
  <trdgrp lng="pl">
    <trd>kozio&lstroke;ek</trd>,
    <trd>ko&zacute;l&eogonek;</trd>
  </trdgrp>
  <trd lng="ro">flea&#x219;c</trd>
  <trd lng="uk">&c_c;&c_a;&c_p;&c_je;&c_n;&c_ja;</trd>
  <trd lng="vo">kapar&uuml;l</trd>
</drv>

<drv mrk="kapr.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc mrk="kapr.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Viando de <tld/>o.
    </dif>
    <trd lng="be">&c_k;&c_a;&c_z;&c_mol;&c_l;&c_ja;&c_c;&c_ib;&c_n;&c_a;</trd>
    <trd lng="de">Ziegenfleisch</trd>
    <trdgrp lng="en">
      <trd>goatmeat</trd>,
      <trd>goat meat</trd>,
      <trd>chevon</trd>
    </trdgrp>
    <trd lng="fr">viande de ch&egrave;vre</trd>
    <trd lng="ja">&#x30E4;&#x30AE;&#x306E;&#x8089; <pr>&#x3084;&#x304E;&#x306E;&#x306B;&#x304F;</pr></trd>
    <trdgrp lng="pl">
      <trd>kozina</trd>,
      <trd>ko&zacute;l&eogonek;cina</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_z;&c_l;&c_ja;&c_t;&c_i;&c_n;&c_a;</trd>
    <trd lng="vo">kaparamit</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_z;&c_l;&c_ja;&c_t;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_z;&c_l;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_c;&c_a;&c_p;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kapr.0aro">
  <kap>
    <tld/>aro
  </kap>
  <snc>
    <uzo tip="fak">AGR</uzo>
    <dif>
      Aro de kune vivantaj <tld/>oj:
      <ekz>
        du malgrandaj <tld/>aroj
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 20:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        balda&ubreve; kreskis lia <tld/>aro de 30 &gcirc;is 700 bestoj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2015/011588.php">De famo al fako</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="greg.0o.ZOO">grego</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_ib;&c_n;&c_y; &c_s;&c_t;&c_a;&c_t;&c_a;&c_k;</trd>
  <trd lng="de">Ziegenherde</trd>
</drv>

<drv mrk="kapr.propeka0o">
  <kap>propeka <tld/>o<fnt>Z</fnt>, <var><kap>pekporta
  <tld/>o</kap></var>, <var><kap>pekofera
  <tld/>o</kap></var></kap>
  <snc>
    <uzo tip="fak">BIB</uzo>
    <dif>
      Ofera <tld/>o, kiun la hebrea &ccirc;efpastro &scirc;ar&gcirc;is per
      &ccirc;iuj
      pekoj de Izrael:
      <ekz>
        Kaj li alportis la oferon de la popolo, kaj li prenis la
        propekan <tld/>on de la popolo, kaj bu&ccirc;is &gcirc;in kaj faris
        el
        &gcirc;i pekoferon kiel el la anta&ubreve;a
        <fnt><bib>MT</bib>, <lok>&Lev; 9:15</lok></fnt>;
      </ekz>
      <ekz>
        Kaj li bu&ccirc;os la pekoferan <tld/>on de la popolo
        <fnt><bib>MT</bib>, <lok>&Lev; 16:15</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>boc expiatori</trd>,
      <trd>boc emissari</trd>
    </trdgrp>
    <trd lng="pl">kozio&lstroke;ek ofiarny</trd>
  </snc>
  <snc>
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Homo a&ubreve; afero, kulpigata a&ubreve; punata pro eraroj de
      aliuloj.
    </dif>
    <trd lng="ca">cap de turc</trd>
  </snc>
  <trd lng="be">&c_a;&c_h;&c_v;&c_ja;&c_r;&c_n;&c_y; <ind>&c_k;&c_a;&c_z;&c_jo;&c_l;</ind></trd>
  <trd lng="br">bouc'h ar pec'hedo&ugrave;</trd>
  <trd lng="cs">h&rcaron;&iacute;&scaron;n&aacute; koza</trd>
  <trd lng="de">S&uuml;ndenbock</trd>
  <trd lng="en">scapegoat</trd>
  <trd lng="es">chivo expiatorio</trd>
  <trd lng="fr">bouc &eacute;missaire</trd>
  <trd lng="hu">b&udblac;nbak</trd>
  <trdgrp lng="ja">
    <trd>&#x8D16;&#x7F6A;&#x306E;&#x30E4;&#x30AE; 
    <pr>&#x3057;&#x3087;&#x304F;&#x3056;&#x3044;&#x306E;&#x30E4;&#x30AE;</pr></trd>,
    <trd>&#x8EAB;&#x4EE3;&#x308F;&#x308A;&#x306E;&#x30E4;&#x30AE; 
    <pr>&#x307F;&#x304C;&#x308F;&#x308A;&#x306E;&#x30E4;&#x30AE;</pr></trd>,
    <trd>&#x4ED6;&#x4EBA;&#x306E;&#x7F6A;&#x3092;&#x8CA0;&#x308F;&#x3055;&#x308C;&#x308B;&#x4EBA; 
    <pr>&#x305F;&#x306B;&#x3093;&#x306E;&#x3056;&#x3044;&#x3092;&#x304A;&#x308F;&#x3055;&#x308C;&#x308B;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x30B9;&#x30B1;&#x30FC;&#x30D7;&#x30B4;&#x30FC;&#x30C8;</trd>
  </trdgrp>
  <trd lng="nl">zondebok</trd>
  <trd lng="pl">kozio&lstroke; ofiarny</trd>
  <trd lng="ro">&#x21B;ap isp&abreve;&#x219;itor</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_jo;&c_l; &c_o;&c_t;&c_p;&c_u;&c_shch;&c_je;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">hrie&scaron;na koza</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_z;&c_je;&c_l; &c_v;&c_ib;&c_d;&c_p;&c_u;&c_shch;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_o;&c_f;&c_ib;&c_r;&c_n;&c_i;&c_j; &c_c;&c_a;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4EE3;&#x7F6A;&#x7F94;&#x7F8A; <pr>d&#xE0;izu&#xEC;g&#x101;oy&#xE1;ng</pr></trd>,
    <trd>&#x66FF;&#x7F6A;&#x7F8A; <pr>t&#xEC;zu&#xEC;y&#xE1;ng</pr></trd>,
    <trd>&#x66FF;&#x7F6A;&#x7F94;&#x7F8A; <pr>t&#xEC;zu&#xEC;g&#x101;oy&#xE1;ng</pr></trd>,
    <trd>&#x57AB;&#x80CC; <pr>di&#xE0;nb&#xE8;i</pr></trd>
  </trdgrp>
</drv>
