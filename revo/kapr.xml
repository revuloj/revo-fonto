<vortaro>
<art mrk="$Id: kapr.xml,v 1.49 2023/12/03 08:00:22 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kapr</rad>/o
</kap>

<drv mrk="kapr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio
      <ref tip="malprt" cel="boved.0oj">bovedoj</ref>
      (<trd lng="la">Capra</trd>), kun mentonbarbo kaj sabroformaj a&ubreve;
      &scirc;ra&ubreve;be turnitaj <ref tip="prt" cel="korn.0o.besta">kornoj</ref>:
      <ekz>
        tro nutrata <tld/>o fari&gcirc;as kiel apro
        <klr>(feli&ccirc;o fierigas)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        barbori&ccirc;a kiel <ind><tld/>oviro</ind>;
        <trdgrp lng="ca">
          <trd>boc</trd>,
          <trd>cabrot</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>ino</ind>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/kapr.jpg">
    hejma <tld/>o
  </bld>
  <bld lok="../bld/alpakapr.jpg">
    <ind>alpa <tld/>o</ind>
    <trd lng="la">Capra ibex ibex</trd>
    <trd lng="de">Alpen<ind>steinbock</ind></trd>
    <trd lng="fr">bouquetin</trd>
  </bld>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_z;&c_jo;&c_l;</trd>,
    <trd>&c_k;&c_a;&c_z;&c_a; <klr>(&c_v;&c_ib;&c_d;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_o;&c_z;&c_a;</trd>
  <trd lng="bo">&t_ra;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>bouc'h</trd>,
    <trd>gavr</trd>
  </trdgrp>
  <trd lng="ca">cabra</trd>
  <trd lng="cs">koza</trd>
  <trd lng="de">Ziege</trd>
  <trd lng="en">goat</trd>
  <trd lng="es">cabra</trd>
  <trdgrp lng="fr">
    <trd>bouc</trd>,
    <trd>ch&egrave;vre<klr tip="ind"> (animal)</klr></trd>
  </trdgrp>
  <trd lng="hu">kecske</trd>
  <trd lng="id">kambing</trd>
  <trd lng="io">kapro</trd>
  <trd lng="ja">&#x30E4;&#x30AE;<klr> (&#x5C71;&#x7F8A;)</klr> <pr>&#x3084;&#x304E;</pr></trd>
  <trd lng="nl">geit</trd>
  <trd lng="pl">koza</trd>
  <trd lng="ro">capr&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_jo;&c_l;</trd>
  <trd lng="sk">koza</trd>
  <trdgrp lng="uk">
    <trd>&c_c;&c_a;&c_p;</trd>,
    <trd>&c_k;&c_o;&c_z;&c_je;&c_l;</trd>,
    <trd>&c_k;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="vo">kapar</trd>
  <trd lng="zh">&#x5C71;&#x7F8A; <pr>sh&#x101;ny&#xE1;ng</pr></trd>
</drv>

<drv mrk="kapr.vir0o">
  <kap>
    vir<tld/>o
  </kap>
  <snc mrk="kapr.vir0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Virseksa <tld/>o:
      <ekz>
        <ind>eksvir<tld/>o</ind> <klr>(kastrita <tld/>o)</klr>.
        <trd lng="nl">hamel</trd>
        <trd lng="vo">hokapar</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_jo;&c_l;</trd>
  <trd lng="en">billy</trd>
  <trd lng="fr">bouc</trd>
  <trd lng="hu">kecskebak</trd>
  <trd lng="io">kaprulo</trd>
  <trd lng="ja">&#x96C4;&#x30E4;&#x30AE; <pr>&#x304A;&#x3059;&#x3084;&#x304E;</pr></trd>
  <trd lng="nl">bok</trd>
  <trdgrp lng="pl">
    <trd>kozio&lstroke;</trd>,
    <trd>cap</trd>
  </trdgrp>
  <trd lng="ro">&#x21B;ap</trd>
  <trd lng="uk">&c_c;&c_a;&c_p;</trd>
  <trd lng="vo">hikapar</trd>
</drv>

<drv mrk="kapr.0ino">
  <kap>
    <tld/>ino
  </kap>
  <snc mrk="kapr.0ino.ZOO">
