<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kapr.xml,v 1.41 2021/05/28 12:15:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kapr</rad>/o
</kap>

<drv mrk="kapr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio
      <ref tip="malprt" cel="boved.0oj">bovedoj</ref>
      (<trd lng="la">Capra</trd>), kun mentonbarbo kaj sabroformaj a&ubreve;
      &scirc;ra&ubreve;be turnitaj <ref tip="prt" cel="korn.0o.besta">kornoj</ref>:
      <ekz>
        tro nutrata <tld/>o fari&gcirc;as kiel apro
        <klr>(feli&ccirc;o fierigas)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        barbori&ccirc;a kiel <ind><tld/>oviro</ind>;
        <trdgrp lng="ca">
          <trd>boc</trd>,
          <trd>cabrot</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>ino</ind>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/kapr.jpg">
    hejma <tld/>o
  </bld>
  <bld lok="../bld/alpakapr.jpg">
    <ind>alpa <tld/>o</ind>
    <trd lng="la">Capra ibex ibex</trd>
    <trd lng="de">Alpen<ind>steinbock</ind></trd>
    <trd lng="fr">bouquetin</trd>
  </bld>
  <trd lng="be">&c_k;&c_a;&c_z;&c_jo;&c_l;</trd> 
  <trd lng="bg">&c_k;&c_o;&c_z;&c_a;</trd>
  <trd lng="bo">&#xF62;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>bouc'h</trd>,
    <trd>gavr</trd>
  </trdgrp>
  <trd lng="ca">cabra</trd>
  <trd lng="cs">koza</trd>
  <trd lng="de">Ziege</trd>
  <trd lng="en">goat</trd>
  <trd lng="es">cabra</trd>
  <trdgrp lng="fr">
    <trd>bouc</trd>,
    <trd>ch&egrave;vre<klr tip="ind"> (animal)</klr></trd>
  </trdgrp>
  <trd lng="hu">kecske</trd>
  <trd lng="id">kambing</trd>
  <trd lng="io">kapro</trd>
  <trd lng="ja">&#x30E4;&#x30AE;<klr> (&#x5C71;&#x7F8A;)</klr> [<ind>&#x3084;&#x304E;</ind>]</trd>
  <trd lng="nl">geit</trd>
  <trd lng="pl">koza</trd>     
  <trd lng="ro">capr&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_jo;&c_l;</trd>
  <trd lng="sk">koza</trd>
  <trd lng="vo">kapar</trd>
</drv>

<drv mrk="kapr.vir0o">
  <kap>
    vir<tld/>o
  </kap>
  <snc mrk="kapr.vir0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Virseksa <tld/>o:
      <ekz>
        <ind>eksvir<tld/>o</ind> <klr>(kastrita <tld/>o)</klr>.
        <trd lng="nl">hamel</trd>
        <trd lng="vo">hokapar</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="en">billy</trd>
  <trd lng="fr">bouc</trd>
  <trd lng="hu">kecskebak</trd>
  <trd lng="io">kaprulo</trd>
  <trd lng="ja">&#x96C4;&#x30E4;&#x30AE; [<ind>&#x304A;&#x3059;&#x3084;&#x304E;</ind>]</trd>
  <trd lng="nl">bok</trd>
  <trdgrp lng="pl">
    <trd>kozio&lstroke;</trd>,
    <trd>cap</trd>
  </trdgrp>     
  <trd lng="ro">&#x21B;ap</trd>
  <trd lng="vo">hikapar</trd>
</drv>

<drv mrk="kapr.0ino">
  <kap>
    <tld/>ino
  </kap>
  <snc mrk="kapr.0ino.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Inseksa <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_a;</trd>  
  <trd lng="br">gavr</trd>
  <trd lng="ca">cabra</trd>
  <trd lng="en">nanny</trd>
  <trd lng="fr">ch&egrave;vre<klr tip="ind"> (animal)</klr></trd>
  <trd lng="io">kaprino</trd>
  <trd lng="ja">&#x96CC;&#x30E4;&#x30AE; [<ind>&#x3081;&#x3059;&#x3084;&#x304E;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>geit</trd>
    <trd>sik</trd>
  </trdgrp>
  <trd lng="pl">koza</trd>     
  <trd lng="ro">capr&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_a;</trd>
  <trd lng="vo">jikapar</trd>
</drv>

<drv mrk="kapr.0ido">
  <kap>
    <tld/>ido
  </kap>
  <snc mrk="kapr.0ido.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Ido de <tld/>o.
    </dif>
  </snc>
  <trd lng="en">young goat</trd>
  <trd lng="fr">chevreau</trd>
  <trdgrp lng="io">
    <trd>kaproyuno</trd>
    <trd>kapryuno</trd>
  </trdgrp>
  <trd lng="it">capretto</trd>
  <trd lng="ja">&#x5B50;&#x30E4;&#x30AE; [<ind>&#x3053;&#x3084;&#x304E;</ind>]</trd>
  <trd lng="nl">lam</trd>
  <trdgrp lng="pl">
    <trd>kozio&lstroke;ek</trd>,
    <trd>ko&zacute;l&eogonek;</trd>
  </trdgrp>    
  <trd lng="ro">flea&#x219;c</trd>
  <trd lng="vo">kapar&uuml;l</trd>
</drv>

<drv mrk="kapr.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc mrk="kapr.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Viando de <tld/>o.
    </dif>
    <trd lng="de">Ziegenfleisch</trd>
    <trdgrp lng="en">
      <trd>goatmeat</trd>,
      <trd>goat meat</trd>,
      <trd>chevon</trd>
    </trdgrp>
    <trd lng="fr">viande de ch&egrave;vre</trd>
    <trd lng="ja">&#x30E4;&#x30AE;&#x306E;&#x8089; [<ind>&#x3084;&#x304E;&#x306E;&#x306B;&#x304F;</ind>]</trd>
    <trdgrp lng="pl">
      <trd>kozina</trd>,
      <trd>ko&zacute;l&eogonek;cina</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_o;&c_z;&c_l;&c_ja;&c_t;&c_i;&c_n;&c_a;</trd>
    <trd lng="vo">kaparamit</trd>
  </snc>
</drv>

<drv mrk="kapr.propeka0o">
  <kap>propeka <tld/>o<fnt>Z</fnt>, <var><kap>pekporta
  <tld/>o</kap></var>, <var><kap>pekofera
  <tld/>o</kap></var></kap>
  <snc>
    <uzo tip="fak">BIB</uzo>
    <dif>
      Ofera <tld/>o, kiun la hebrea &ccirc;efpastro &scirc;ar&gcirc;is per
      &ccirc;iuj
      pekoj de Izrael:
      <ekz>
        Kaj li alportis la oferon de la popolo, kaj li prenis la
        propekan <tld/>on de la popolo, kaj bu&ccirc;is &gcirc;in kaj faris
        el
        &gcirc;i pekoferon kiel el la anta&ubreve;a
        <fnt><bib>MT</bib>, <lok>&Lev; 9:15</lok></fnt>;
      </ekz>
      <ekz>
        Kaj li bu&ccirc;os la pekoferan <tld/>on de la popolo
        <fnt><bib>MT</bib>, <lok>&Lev; 16:15</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>boc expiatori</trd>,
      <trd>boc emissari</trd>
    </trdgrp>
    <trd lng="pl">kozio&lstroke;ek ofiarny</trd>     
  </snc>
  <snc>
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Homo a&ubreve; afero, kulpigata a&ubreve; punata pro eraroj de
      aliuloj.
    </dif>
    <trd lng="ca">cap de turc</trd>
  </snc>
  <trd lng="br">bouc'h ar pec'hedo&ugrave;</trd>
  <trd lng="cs">h&rcaron;&iacute;&scaron;n&aacute; koza</trd>
  <trd lng="de">S&uuml;ndenbock</trd>
  <trd lng="en">scapegoat</trd>
  <trd lng="es">chivo expiatorio</trd>
  <trd lng="fr">bouc &eacute;missaire</trd>
  <trd lng="hu">b&udblac;nbak</trd>
  <trd lng="nl">zondebok</trd>
  <trd lng="pl">kozio&lstroke; ofiarny</trd>     
  <trd lng="ro">&#x21B;ap isp&abreve;&#x219;itor</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_jo;&c_l; &c_o;&c_t;&c_p;&c_u;&c_shch;&c_je;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">hrie&scaron;na koza</trd>
</drv>
</art>
