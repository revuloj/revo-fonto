<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kapr</rad>/o
</kap>
<drv mrk="kapr.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
      cel="boved.0oj">bovedoj</ref>
      (<trd lng="la">Capra</trd>), kun mentonbarbo kaj sabroformaj a&ubreve;
      &scirc;ra&ubreve;be turnitaj kornoj:
      <ekz>
        tro nutrata <tld/>o fari&gcirc;as kiel apro<fnt>Z</fnt>
        <klr>(feli&ccirc;o fierigas)</klr>;
      </ekz>
      <ekz>
        barbori&ccirc;a kiel <tld/>oviro;
      </ekz>
      <ekz>
        propeka<fnt>Z</fnt>, pekporta <tld/>o
        <klr>(kiun la hebrea &ccirc;efpastro &scirc;ar&gcirc;is per &ccirc;iuj
        pekoj de Izrael)</klr>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/kapr.jpg"><tld/>o</bld>
  <trd lng="de">Ziege</trd>
  <trd lng="en">goat</trd>
  <trdgrp lng="fr">
    <trd>bouc</trd>, <trd>ch&egrave;vre</trd>
  </trdgrp>
  <trd lng="nl">bok</trd>
  <trd lng="ru">&c_k;&c_o;&c_z;&c_jo;&c_l;</trd>
</drv>
</art>
