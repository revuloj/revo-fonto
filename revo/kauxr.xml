<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>ka&ubreve;r</rad>/i
</kap>
<drv mrk="kauxr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Sidi sur la kalkanoj:
      <ekz>
        la malfeli&ccirc;a knabino <tld/>is en angulo de la &ccirc;ambro;
      </ekz>
      <ekz>
        <tld/>e sidi&gcirc;i<fnt>Z</fnt>..
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>kauern</trd>,
      <trd>hocken</trd>
    </trdgrp>
    <trd lng="fr">se tenir <ind>accroupi</ind></trd>
    <trd lng="hu">guggol</trd>
  </snc>
  <snc>
    <dif>
      Sidi kuntiri&gcirc;ante por sin ka&scirc;i a&ubreve; por eksalti:
      <ekz>
        la tigro <tld/>is en la altaj herboj.
      </ekz>
    </dif>
    <trd lng="de">kauern</trd>
    <trd lng="fr">&ecirc;tre <ind>tapi</ind></trd>
    <trd lng="hu">kuporog</trd>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_d;&c_z;&c_je;&c_c;&c_mol; &c_n;&c_a; &c_k;&c_u;&c_k;&c_ib;&c_sh;&c_k;&c_a;&c_h;</trd>
  <trd lng="en">cower</trd>
  <trd lng="nl">hurken</trd>
  <trd lng="ru">&c_s;&c_i;&c_d;&c_je;&c_t;&c_mol; &c_n;&c_a; &c_k;&c_o;&c_r;&c_t;&c_o;&c_ch;&c_k;&c_a;&c_h;</trd>
</drv>

<drv mrk="kauxr.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Sidigi sin sur kalkanoj.
    </dif>
    <trd lng="de">sich <ind>hinkauern</ind></trd>
    <trd lng="fr">s'<ind>accroupir</ind></trd>
    <trd lng="hu">leguggol</trd>
  </snc>
  <snc>
    <dif>
      Kuntiri sin.
    </dif>
    <trd lng="fr">se <ind>tapir</ind></trd>
    <trd lng="hu">&ouml;sszekuporodik</trd>
  </snc>
</drv>
</art>
