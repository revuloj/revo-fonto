<drv mrk="kopi.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Aparato per kiu oni faras <tld/>ojn:
      <ekz>
        estas neniu problemo preni
        folion kaj la&ubreve;bezone &scirc;ovi &gcirc;in tra la firmaa
        <tld/>ilo
        <fnt><aut>Pejno Simono</aut>:
        <vrk><url ref="&Monato;/rec/subt.html">Foliaro
        plukinda</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 12/1999</lok>
        </fnt>
      </ekz>;
      <ekz>
        moderna oficeja foto<tld/>ilo uzeblas anka&ubreve; kiel A3-skanilo
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2012/010663.php">De
          papero al komputilo: bitigo de presitaj dokumentoj</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        tele<tld/>ilo + 49 40 656 06 59
        <klr>(vd <ref cel="faks.0ilo">faksilo</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9309-noticoj.txt">Esperanto
          sen peno</url></vrk>
          <lok>93/09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_ja;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_a;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  <trd lng="ca">fotocopiadora</trd>
  <trd lng="cs">kop&iacute;rka</trd>
  <trdgrp lng="de">
    <trd>Kopierer</trd>,
    <trd>Kopierger&auml;t</trd>,
    <trd>Lichtpausger&auml;t</trd>
  </trdgrp>
  <trd lng="en">copier</trd>
  <trdgrp lng="es">
    <trd>copiadora</trd>,
    <trd>fotocopiadora</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>copieur<klr> (machine)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&aacute;sol&oacute;g&eacute;p</trd>,
    <trd>f&eacute;nym&aacute;sol&oacute;</trd>
  </trdgrp>
  <trd lng="nl">kopieerapparaat</trd>
  <trd lng="pl">kserokopiarka</trd>     
  <trd lng="ro">copie</trd>
  <trd lng="ru">&c_k;&c_o;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  <trd lng="sk">kop&iacute;rka</trd>
</drv>

<drv mrk="kopi.sav0o">
  <kap>sav<tld/>o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Rezerva <tld/>o de <ref tip="vid" cel="datum.0o">datumoj</ref> por
      protekti kontra&ubreve; perdo de la originalo:
      <ekz>
        regule faru sav<tld/>ojn, kaj enfermu ilin en <klr>[...]</klr>
        sekura loko
        <fnt>
          <bib>Monato</bib>
          <aut>Rob Ebenau</aut>
          <vrk><url ref="&Monato;/2005/008528.php">Kiel
          protekti kontra&ubreve; perdo kaj &scirc;telo?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        en komputiko, sekur<tld/>o, rezerv<tld/>o a&ubreve; sav<tld/>o estas
        <tld/>o de 
        la datumoj kiuj trovi&gcirc;as en komputilo sur alia memorejo
        <fnt>
          <bib>Viki</bib>
          <vrk><url>https://eo.wikipedia.org/?curid=115867</url></vrk>
          <lok><url ref="https://eo.wikipedia.org/?curid=115867">Sekurkopio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_z;&c_e;&c_r;&c_v;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="ca">c&ograve;pia de <ind>seguretat</ind></trd>
  <trdgrp lng="cs">
    <trd>bezpe&ccaron;nostn&iacute; kopie <klr>(souboru)</klr></trd>,
    <trd>z&aacute;lo&zcaron;n&iacute; soubor</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sicherungskopie</trd>,
    <trd>Backup [komputilado]</trd>
  </trdgrp>
  <trd lng="en">back up copy</trd>
  <trdgrp lng="es">
    <trd>respaldo<klr> (de datos)</klr></trd>,
    <trd>copia<klr> de seguridad</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>copie de sauvegarde</trd>
  </trdgrp>
  <trd lng="hu">biztons&aacute;gi m&aacute;solat</trd>
  <trd lng="nl">veiligheidscopie</trd>
  <trd lng="pl">kopia bezpiecze&nacute;stwa</trd>     
  <trd lng="ro">copie de rezerv&abreve;</trd>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_je;&c_r;&c_v;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_p;&c_i;&c_ja;</trd>
  <trd lng="sk">z&aacute;lo&zcaron;n&yacute; s&uacute;bor</trd>
</drv>
