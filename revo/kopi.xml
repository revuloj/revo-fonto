<kap>
  <ofc>*</ofc>
  <rad>kopi</rad>/i
</kap>
<drv mrk="kopi.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kopi.0i.tekston">
    <dif>
      Reprodukti tekston per unu a&ubreve; pluraj ekzempleroj:
      <ekz>
        <tld/>i libron, leteron, dokumenton;
      </ekz>
      <ekz>
        <tld/>i en oficejoj fremdajn skriba&jcirc;ojn
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">copiar</trd>
    <trdgrp lng="de">
      <trd>nachdrucken</trd>,
      <trd>kopieren</trd>,
      <trd>lichtpausen</trd>
    </trdgrp>
    <trd lng="es">copiar</trd>
  </snc>
  <snc mrk="kopi.0i.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Reprodukti belartan verkon:
      <ekz>
        <tld/>i pentra&jcirc;on, statuon;
      </ekz>
      <ekz>
        mi ne estas tia artistino, kiel tiu, kiu desegnis &ccirc;i tiun
        belegan bildeton, 
        &scirc;i pensis, sed <tld/>i ies laboron mi espereble povos
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>reproduir</trd>,
      <trd>calcar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>kopieren</trd>,
      <trd>plagiieren</trd>,
      <trd>nachahmen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>reproducir</trd>,
      <trd>calcar</trd>
    </trdgrp>
  </snc>
  <snc mrk="kopi.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Speguli:
      <ekz>
        la akvo <klr>[estis]</klr> klara kiel spegulo, oni vidis en &gcirc;i 
        &ccirc;iun arbon, &ccirc;iun arbeta&jcirc;on &ccirc;e la bordo <klr>[...]</klr> &ccirc;io
        klare 
        kopii&gcirc;is en &gcirc;i, nur kun la diferenco, ke &ccirc;io staris sur la kapo
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">reflectir</trd>
    <trd lng="de">spiegeln</trd>
    <trd lng="es">reflejar</trd>
  </snc>
  <snc mrk="kopi.0i.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Reprodukti sur papero pozitivan bildon de negativo.
    </dif>
    <trd lng="ca">imprimir</trd>
    <trdgrp lng="de">
      <trd>abziehen</trd>,
      <trd>Abzug machen</trd>
    </trdgrp>
    <trd lng="es">imprimir</trd>
  </snc>
  <snc mrk="kopi.0i.imiti">
    <dif>
      Imiti:
      <ekz>
        <tld/>i la gestojn de iu, la vo&ccirc;on de aktoro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;ion strangan kaj malbelan Kay povosciis <tld/>i tre trafe, kaj
        la homoj diris: La knabo havas bonegajn kapablojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Fabelo en sep rakontoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">imitar</trd>
    <trdgrp lng="de">
      <trd>nachahmen</trd>,
      <trd>nach&auml;ffen</trd>
    </trdgrp>
    <trd lng="es">imitar</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&#xF56;&#xF64;&#xF74;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>kop&iacute;rovat</trd>,
    <trd>napodobovat</trd>,
    <trd>obkreslit</trd>,
    <trd>reprodukovat</trd>
  </trdgrp>
  <trd lng="en">copy</trd>
  <trdgrp lng="fr">
    <trd>copier</trd>,
    <trd>recopier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&aacute;sol</trd>,
    <trd>lem&aacute;sol</trd>
  </trdgrp>
  <trd lng="nl">kopi&euml;ren</trd>
  <trdgrp lng="pl">
    <trd>kopiowa&cacute;</trd>,
    <trd>na&sacute;ladowa&cacute;</trd>,
    <trd>wypisa&cacute; </trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>copiar</trd>,
    <trd>transcrever</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>copia</trd>,
    <trd>imita</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kop&iacute;rova&tcaron;</trd>,
    <trd>napodobni&tcaron;</trd>,
    <trd>reprodukova&tcaron;</trd>,
    <trd>urobi&tcaron; odpis</trd>
  </trdgrp>
</drv>

<drv mrk="kopi.0o">
  <kap><tld/>o</kap>
  <snc mrk="kopi.0o.kopiajxo">
    <dif>
      Nova a&jcirc;o, <tld/>e farita, imita de <ref tip="ant" cel="origin1.0o">originalo</ref>:
      <ekz>
        tia traduko donas mal&gcirc;ustan <tld/>on de la tradukita
        verko<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o de la letero estis transdonota kiel le&gcirc;o en &ccirc;iun landon
        <fnt>
          <bib>MT</bib>
          <lok>Ester 3:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu pentra&jcirc;o estas nur mallerta <tld/>o;
      </ekz>
      <ekz>
        mi prenis el la tirkesto la pezajn <tld/>ojn de la romano
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 13a, p. 126a</lok></fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="af">afskrif</trd>
  <trd lng="am">&#x1245;&#x1305;</trd>
  <trd lng="ar">&#x646;&#x633;&#x62E;&#x629;</trd>
  <trd lng="az">sur&#x259;ti</trd>
