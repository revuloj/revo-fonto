<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kopi</rad>/i
</kap>
<drv mrk="kopi.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kopi.0i.tekston">
    <dif>
      Reprodukti tekston per unu a&ubreve; pluraj ekzempleroj:
      <ekz>
        <tld/>i libron, leteron, dokumenton.
      </ekz>
    </dif>
    <trd lng="ca">copiar</trd>
    <trdgrp lng="de">
      <trd>nachdrucken</trd>,
      <trd>kopieren</trd>,
      <trd>lichtpausen</trd>
    </trdgrp>
    <trd lng="es">copiar</trd>
  </snc>
  <snc mrk="kopi.0i.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Reprodukti belartan verkon:
      <ekz>
        <tld/>i pentra&jcirc;on, statuon.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>reproduir</trd>,
      <trd>calcar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>kopieren</trd>,
      <trd>plagiieren</trd>,
      <trd>nachahmen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>reproducir</trd>,
      <trd>calcar</trd>
    </trdgrp>
  </snc>
  <snc mrk="kopi.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Speguli:
      <ekz>
        la arboj <tld/>i&gcirc;is en la rivero<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ca">reflectir</trd>
    <trd lng="de">spiegeln</trd>
    <trd lng="es">reflejar</trd>
  </snc>
  <snc mrk="kopi.0i.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Reprodukti sur papero pozitivan bildon de negativo.
    </dif>
    <trd lng="ca">imprimir</trd>
    <trdgrp lng="de">
      <trd>abziehen</trd>,
      <trd>Abzug machen</trd>
    </trdgrp>
    <trd lng="es">imprimir</trd>
  </snc>
  <snc mrk="kopi.0i.imiti">
    <dif>
      Imiti:
      <ekz>
        <tld/>i la gestojn de iu, la vo&ccirc;on de aktoro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ca">imitar</trd>
    <trdgrp lng="de">
      <trd>nachahmen</trd>,
      <trd>nach&auml;ffen</trd>
    </trdgrp>
    <trd lng="es">imitar</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">copy</trd>
  <trdgrp lng="fr">
    <trd>copier</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&aacute;sol</trd>,
    <trd>lem&aacute;sol</trd>
  </trdgrp>
  <trd lng="nl">kopi&euml;ren</trd>
  <trdgrp lng="pt">
    <trd>copiar</trd>,
    <trd>transcrever</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kopírovať</trd>,
    <trd>napodobniť</trd>,
    <trd>reprodukovať</trd>,
    <trd>urobiť odpis</trd>
  </trdgrp>
</drv>

<drv mrk="kopi.0o">
  <kap><tld/>o</kap>
  <snc mrk="kopi.0o.kopiajxo">
    <dif>
      Nova a&jcirc;o, <tld/>e farita, imita de <ref tip="ant"
      cel="origin1.0o">originalo</ref>:
      <ekz>
        tia traduko donas mal&gcirc;ustan <tld/>on de la tradukita
        verko<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu pentra&jcirc;o estas nur mallerta <tld/>o;
      </ekz>
      <ekz>
        mi prenis el la tirkesto la pezajn <tld/>ojn de la romano
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 13a, p. 126a</lok></fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_p;&c_ib;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>c&ograve;pia</trd>,
    <trd>reproducci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kopie</trd>,
    <trd>Abzug</trd>,
    <trd>Durchschlag</trd>,
    <trd>Pause</trd>
  </trdgrp>
  <trd lng="en">copy</trd>
  <trdgrp lng="es">
    <trd>copia</trd>,
    <trd>reproducci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>copie</trd>
  </trdgrp>
  <trd lng="hu">m&aacute;solat</trd>
  <trd lng="nl">kopie</trd>
  <trd lng="ru">&c_k;&c_o;&c_p;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>kópia</trd>,
    <trd>odpis</trd>,
    <trd>prieklep</trd>,
    <trd>priepis</trd>
  </trdgrp>
</drv>

<drv mrk="kopi.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Aparato per kiu oni faras <tld/>ojn:
      <ekz>
        estas neniu problemo preni
        folion kaj la&ubreve;bezone &scirc;ovi &gcirc;in tra la firmaa
        <tld/>ilo
        <fnt><aut>Pejno Simono</aut>:
        <vrk><url ref="&Monato;/rec/subt.html">Foliaro
        plukinda</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 12/1999</lok>
        </fnt>
      </ekz>
      .
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_p;&c_ib;&c_ja;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_a;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  <trd lng="ca">fotocopiadora</trd>
  <trdgrp lng="de">
    <trd>Kopierer</trd>,
    <trd>Kopierger&auml;t</trd>,
    <trd>Lichtpausger&auml;t</trd>
  </trdgrp>
  <trd lng="en">copier</trd>
  <trdgrp lng="es">
    <trd>copiadora</trd>,
    <trd>fotocopiadora</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>copieur<klr> (machine)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&aacute;sol&oacute;g&eacute;p</trd>,
    <trd>f&eacute;nym&aacute;sol&oacute;</trd>
  </trdgrp>
  <trd lng="nl">kopieerapparaat</trd>
  <trd lng="ru">&c_k;&c_o;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  <trd lng="sk">kopírka</trd>
</drv>

<drv mrk="kopi.sav0o">
  <kap>sav<tld/>o</kap>
  <snc>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Rezerva <tld/>o de <ref tip="vid" cel="datum.0o">datumoj</ref> por
      protekti kontra&ubreve; perdo de la originalo.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_z;&c_e;&c_r;&c_v;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="ca">c&ograve;pia de <ind>seguretat</ind></trd>
  <trdgrp lng="de">
    <trd>Sicherungskopie</trd>,
    <trd>Backup [komputilado]</trd>
  </trdgrp>
  <trd lng="en">back up copy</trd>
  <trdgrp lng="es">
    <trd>respaldo<klr> (de datos)</klr></trd>,
    <trd>copia<klr> de seguridad</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>copie de sauvegarde</trd>
  </trdgrp>
  <trd lng="hu">biztons&aacute;gi m&aacute;solat</trd>
  <trd lng="nl">veiligheidscopie</trd>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_je;&c_r;&c_v;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_p;&c_i;&c_ja;</trd>
  <trd lng="sk">záložný súbor</trd>
</drv>
</art>
