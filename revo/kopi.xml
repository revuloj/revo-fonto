<drv mrk="kopi.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kopi.0i.tekston">
    <uzo tip="fak">TIP</uzo>
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="produk.re0i.TEK">Reprodukti</ref>
      tekston, dosieron, datumojn, produkton je unu a&ubreve; pluraj ekzempleroj:
      <ekz>
        <tld/>i libron, leteron, dokumenton;
      </ekz>
      <ekz>
        anta&ubreve; ol cindrigi la leteron, mi <tld/>is &gcirc;in stenografie
        <fnt>
          <bib>ChV</bib>
          <lok>30</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i en oficejoj fremdajn skriba&jcirc;ojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi ricevis permeson <tld/>i diskojn por eduka celo
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li posedis francan vortaron de <nom>Larouse</nom> kun multaj ilustra&jcirc;oj, kaj <tld/>is por
        mi preska&ubreve; &ccirc;iujn ilustra&jcirc;ojn
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">FOT</uzo>
        <ind><tld/>i</ind> pozitivan bildon de negativo sur papero;
        <trd lng="ca">imprimir</trd>
        <trdgrp lng="de">
          <trd>abziehen</trd>,
          <trd>Abzug machen</trd>
        </trdgrp>
        <trd lng="es">imprimir</trd>
      </ekz>
      <ekz>
        en &ccirc;iuj landoj ekzistas striktaj le&gcirc;oj pri <ref cel="rajt.auxtor0o"><tld/>rajto</ref>,
        kiuj malpermesas <tld/>i literaturan verkon, bildon, muzikon ktp, sen anta&ubreve;e &ndash; kutime kontra&ubreve; pago &ndash;
        akiri la rajton por tio
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2005/008680.php"
              >Creative Commons: a&ubreve;toraj rajtoj la&ubreve;mezure</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> havigis tajpilon, sur kiu speciale dungita persono <tld/>as la
        manuskriptojn
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>&Ccirc;e Lev Tolstoj en Jasnaja Poljana</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pauxs.0i">pa&ubreve;si</ref>,
      <ref cel="skrib.trans0i.kopii">transskribi<sncref/></ref>
    </refgrp>
    <trd lng="ca">copiar</trd>
    <trdgrp lng="de">
      <trd>nachdrucken</trd>,
      <trd>kopieren</trd>,
      <trd>lichtpausen</trd>
    </trdgrp>
    <trd lng="es">copiar</trd>
  </snc>
  <snc mrk="kopi.0i.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Reprodukti belartan verkon:
      <ekz>
        <tld/>i pentra&jcirc;on, statuon;
      </ekz>
      <ekz>
        mi ne estas tia artistino, kiel tiu, kiu desegnis &ccirc;i tiun
        belegan bildeton,
        &scirc;i pensis, sed <tld/>i ies laboron mi espereble povos
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu estas la originala verkisto de tiuj kompona&jcirc;oj, kaj el kiu verko li <tld/>is
        ilin
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu firmao &ccirc;iam <tld/>is nian kuirilon kaj fabrikis &gcirc;in en Britujo
        <fnt>
          <bib>KrM</bib>
          <lok>38. Du Botoj Faras Paron.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>reproduir</trd>,
      <trd>calcar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>kopieren</trd>,
      <trd>plagiieren</trd>,
      <trd>nachahmen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>reproducir</trd>,
      <trd>calcar</trd>
    </trdgrp>
  </snc>
  <snc mrk="kopi.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="spegul.0i.rektasence">Speguli</ref>:
      <ekz>
        la akvo <klr>[estis]</klr> klara kiel spegulo, oni vidis en &gcirc;i
        &ccirc;iun arbon, &ccirc;iun arbeta&jcirc;on &ccirc;e la bordo <klr>[...]</klr> &ccirc;io
        klare <tld/>i&gcirc;is en &gcirc;i, nur kun la diferenco, ke &ccirc;io staris sur la kapo
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">reflectir</trd>
    <trd lng="de">spiegeln</trd>
    <trd lng="es">reflejar</trd>
  </snc>
  <snc mrk="kopi.0i.imiti">
    <dif>
      <ref tip="dif" cel="imit.0i.agi">Imiti</ref>:
      <ekz>
        <tld/>i la gestojn de iu, la vo&ccirc;on de aktoro;
      </ekz>
      <ekz>
        <tld/>i hipokritajn sintenojn devus esti nenies fiero
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ion strangan kaj malbelan <nom>Kay</nom> povosciis <tld/>i tre trafe, kaj
        la homoj diris: la knabo havas bonegajn kapablojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Fabelo en sep rakontoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        germanoj tradicie ne nur adoras sed anka&ubreve; <tld/>as la usonan vivmanieron
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009234.php"
              >Dikeco</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu <klr>[&#x2026;]</klr> devus havi unikan stilon, alikaze oni nur <tld/>as
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">imitar</trd>
    <trdgrp lng="de">
      <trd>nachahmen</trd>,
      <trd>nach&auml;ffen</trd>
    </trdgrp>
    <trd lng="es">imitar</trd>
  </snc>
