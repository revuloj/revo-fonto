<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>nitr</rad>...
    </kap>

    <drv mrk="nitr.0">
      <kap><tld/>(o)...</kap>
      <uzo tip="fak">KEM</uzo>
      <snc>
	<dif>
	  Parto de internaciaj &hcirc;emiaj terminoj indikanta eneston al
	  <ref tip="sin" cel="azot.0o">azoto</ref>:
	  <ekz>
	    <tld/>okombinoj <klr>(= azotkombinoj)</klr>;
	  </ekz><ekz>
	    <ref cel="nitrog.0o"><tld/>ogeno</ref>;
	  </ekz><ekz>
	    <tld/>ato, <tld/>ito, <tld/>ido, <tld/>eno, <tld/>ozogrupo.
	  </ekz>
	</dif>
      </snc>
      <rim>
	La vortfarada sistemo, uzanta la terminformilon <tld/>(o)...
	estas parto de faka lingvosistemo internacia, regata per
	siaj propraj, eksteresperantaj reguloj.
	<refgrp tip="vid">
	  <ref cel="fosf.0">fosf(o)...</ref>,
	  <ref cel="sulf.0">sulf(o)...</ref>
	</refgrp>.
      </rim>
      <trdgrp lng="be">
    	<trd>&c_n;&c_ib;&c_t;&c_r;&c_a;-</trd>,
    	<trd>&c_n;&c_ib;&c_t;&c_r;-</trd>,
    	<trd>&c_a;&c_z;&c_o;&c_t;-</trd>
      </trdgrp>
      <trd lng="de">Nitro-</trd>
      <trd lng="el">&ny;&jota;&tau;&rho;&omikron;-</trd>
      <trd lng="hu">nitro-</trd>
      <trd lng="nl">stikstof-</trd>
      <trd lng="pt">nitro-</trd>
      <trdgrp lng="ru">
    	<trd>&c_a;&c_z;&c_o;&c_t;-</trd>,
    	<trd>&c_n;&c_i;&c_t;&c_r;-</trd>,
    	<trd>&c_n;&c_i;&c_t;&c_r;&c_o;-</trd>
      </trdgrp>
    </drv>
    <drv mrk="nitr.0ato">
      <kap><tld/>ato</kap>
      <snc>
        <dif>
          Salo de <ref cel="azot.0acido">azotacido</ref>.
        </dif>
      </snc>
      <trd lng="be">&c_n;&c_ib;&c_t;&c_r;&c_a;&c_t;</trd>
      <trd lng="de">Nitrat</trd>
      <trd lng="en">nitrate</trd>
      <trd lng="fr">nitrate</trd>
      <trd lng="ru">&c_n;&c_i;&c_t;&c_r;&c_a;&c_t;</trd>
      <trd lng="uk">&c_n;&c_ib;&c_t;&c_r;&c_a;&c_t;</trd>
    </drv>
    <drv mrk="nitr.0ito">
      <kap><tld/>ito</kap>
      <snc>
        <dif>
          Salo de <tld/>ita acido (HNO<sub>2</sub>).
        </dif>
      </snc>
      <trd lng="be">&c_n;&c_ib;&c_t;&c_r;&c_y;&c_t;</trd>
      <trd lng="de">Nitrit</trd>
      <trd lng="en">nitrite</trd>
      <trd lng="fr">nitrite</trd>
      <trd lng="ru">&c_n;&c_i;&c_t;&c_r;&c_i;&c_t;</trd>
      <trd lng="uk">&c_n;&c_ib;&c_t;&c_r;&c_i;&c_t;</trd>
    </drv>
    <drv mrk="nitr.0ido">
      <kap><tld/>ido</kap>
      <snc>
        <dif>
          Kombina&jcirc;o de azoto kun alia elemento, kutime metalo.
        </dif>
      </snc>
      <trd lng="be">&c_n;&c_ib;&c_t;&c_r;&c_y;&c_d;</trd>
      <trd lng="de">Nitrid</trd>
      <trd lng="en">nitrite</trd>
      <trd lng="fr">nitrure</trd>
      <trd lng="ru">&c_n;&c_i;&c_t;&c_r;&c_i;&c_d;</trd>
      <trd lng="uk">&c_n;&c_ib;&c_t;&c_r;&c_i;&c_d;</trd>
    </drv>
  </art>
