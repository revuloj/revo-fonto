<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nitr</rad>/o
</kap>
<drv mrk="nitr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Mallongigita nomo de la
      <ref tip="sin" cel="nitrog.0o">nitrogeno</ref>,
      kiun oni uzas en la nomoj de kombinoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ib;&c_t;&c_r;&c_a;-</trd>,
    <trd>&c_n;&c_ib;&c_t;&c_r;-</trd>,
    <trd>&c_a;&c_z;&c_o;&c_t;-</trd>
  </trdgrp>
  <trd lng="de">Nitro-</trd>
  <trd lng="el">&ny;&jota;&tau;&rho;&omikron;-</trd>
  <trd lng="hu">nitro-</trd>
  <trd lng="nl">stikstof-</trd>
  <trd lng="pt">nitro-</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_t;&c_r;&c_o;-</trd>,
    <trd>&c_n;&c_i;&c_t;&c_r;-</trd>,
    <trd>&c_a;&c_z;&c_o;&c_t;-</trd>
  </trdgrp>
</drv>
</art>
