<drv mrk="regul.viv0o">
  <kap>viv<tld/>o</kap>
  <snc mrk="regul.viv0o.FIL">
    <dif>
      <tld lit="R"/>o pri &ccirc;iutaga vivo,
      pri agado, pri <ref cel="mor.0o">moro</ref>:
      <ekz>
        la junaj inaj generacioj facile kaj rapide konformi&gcirc;is al la
        novaj viv<tld/>oj
        <fnt>
          <aut>N. Cescotti Covelli</aut>,
          <vrk><url ref="http://www.ueci.it/kongresoj/kng_2007/Prelego_1.htm">
          La virino en la islama mondo</url></vrk>,
          <lok>[21a kongreso de UECI], 2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Volter</nom> opiniis necesa la kredon je dio por starigo de moralaj principoj kaj
        viv<tld/>oj inter la homoj
        <fnt>
          <bib>Kandid</bib>
        </fnt>;
      </ekz>
      <ekz>
        la ceteraj viv<tld/>oj de la behinoj estas ja amaso da freneza&jcirc;oj
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="deviz.0o">devizo</ref>,
      <ref cel="konven1.0o.gxenerale">konvencio<sncref/></ref>,
      <ref cel="norm.0o.konvencio">normo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="fr">r&egrave;gle de vie</trd>
  <trd lng="pl">regu&lstroke;a &zdot;ycia</trd>
</drv>

