    <trd>ireguler</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nieregularny</trd>,
    <trd>nienormalny</trd>,
    <trd>nieforemny</trd>,
    <trd>anormalny</trd>
  </trdgrp>
  <trd lng="pt">irregular</trd>
  <trdgrp lng="zh">
    <trd>&#19981;&#35215;&#31684; <pr>b&ugrave;gu&#299;f&agrave;n</pr></trd>,
