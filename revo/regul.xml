<drv mrk="regul.0igi">
  <kap><tld/>igi,
  <var><kap>reguli</kap> <uzo tip="stl">RAR</uzo></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="regul.0igi.konformigi">
    <dif>
      <ref tip="super" cel="konfor.0igi">Konformigi</ref> al <tld/>o:
      <ekz>
        la popolo lo&gcirc;anta en la valo de Nilo devis <klr>[&#x2026;]</klr> <tld/>igi la akvojn
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ne <tld/>igis definitive la kontojn kun <klr>[li]</klr>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro 21a</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekzistas inter ni malnova konto <tld/>igota pli poste, da&ubreve;rigis malvarme la
        kapitano
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        unu el tiuj lampoj pendis anta&ubreve; la vapormetro,
        por ke la hejtisto povu legi kaj <tld/>i la vaporpremon
        <fnt>
          <aut>B. Traven, trad. H. G. Kaiser</aut>
          <vrk>Mortula &scirc;ipo</vrk>
          <lok>1995</lok>
        </fnt>.
      </ekz>
<!--
       <ekz>
         <ind><tld/>igi la kontojn</ind> kun iu<fnt>B</fnt>.
         <trd lng="ru">&c_s;&c_v;&c_je;&c_s;&c_t;&c_i;
         &c_s;&c_ch;&c_jo;&c_t;&c_y;</trd>
       </ekz>
-->
     </dif>
     <refgrp tip="vid">
        <ref cel="ekvili.0igi">ekvilibrigi</ref>,
        <ref cel="nivel.0i">niveli</ref>
     </refgrp>
     <trdgrp lng="de">
       <trd>ausgleichen</trd>
     </trdgrp>
     <trd lng="fr">r&eacute;gulariser</trd>
     <trd lng="hu">rendez</trd>
  </snc>
  <snc mrk="regul.0igi.submeti">
    <dif>
      <ref tip="super" cel="met.sub0i">Submeti</ref> al <tld/>aro:
      <ekz>
        <tld/>igi la rilatojn de tiu &ccirc;i grupo al la &ccirc;irka&ubreve;anta mondo
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la parolarto <klr>[&#x2026;]</klr> estas konscie <tld/>igita elokventeco
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi la inter&scirc;an&gcirc;on de la varoj
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
     </dif>
    <refgrp tip="vid">
      <ref cel="ord.0igi">ordigi</ref>,
      <ref cel="organi2.0i">organizi</ref>,
      <ref cel="strukt.0i">strukturi</ref>
