<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>hidr</rad>/o
</kap>
<drv mrk="hidr.0o">
  <kap><tld/>o</kap>
  <snc mrk="hidr.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Fabela serpento kun sep a&ubreve; na&ubreve; kapoj, el kiuj &ccirc;iu,
      kiam oni &gcirc;in tran&ccirc;is, estis tuj anstata&ubreve;ata de alia.
    </dif>
  </snc>
  <snc mrk="hidr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="hidr.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Hydra</trd>).
    </dif>
  </snc>
<trd lng="es">hidra</trd>
</drv>
<drv mrk="hidr.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="hidr.0uloj"><tld/>uloj</ref>
      (<trd lng="la">Hydridae</trd>), vivantaj en dol&ccirc;akvo.
    </dif>
  </snc>
  <trd lng="de">S&uuml;&szlig;wasserpolypen <klr>(proks.)</klr></trd>
</drv>
<drv mrk="hidr.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="hidroz.0oj">hidrozooj</ref>
      (<trd lng="la">Hydroidea</trd>).
    </dif>
  </snc>
</drv>
</art>

