<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hidr.xml,v 1.31 2022/09/28 21:34:11 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>hidr</rad>/o
</kap>

<drv mrk="hidr.0o">
  <kap><tld/>o</kap>
  <snc mrk="hidr.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      <ref tip="super" cel="monstr.0a">Monstra</ref> akvo<ref cel="serpen.0o">serpento</ref> 
      kun sep a&ubreve; na&ubreve; kapoj, el kiuj &ccirc;iu, kiam oni &gcirc;in detran&ccirc;is, estis tuj 
      anstata&ubreve;ata de du:
      <ekz>
        kvaza&ubreve; provi fortran&ccirc;i kapon de <tld/>o
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estis <tld/>o, tuj ni ekkonis tion, &ccirc;ar &gcirc;i
        havis sep kapojn
        <fnt>
        <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>,
        <vrk><url ref="http://mujweb.cz/Kultura/malovec/naufabel.htm">
        Na&ubreve; fabeloj</url></vrk>,
        <lok>Granda polica fabelo, [2002?]</lok>
        </fnt>;  
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la tutmonda hidro de la kontra&ubreve;revolucio volis, ke la dinamita bombo
        <klr>[&#8230;]</klr> eksplodu &gcirc;uste dum la tagman&gcirc;o
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>6. La neatendita bombeksplodo.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_h;&c_i;&c_d;&c_r;&c_a;</trd>
    <trd lng="ca">Hidra</trd>
    <trd lng="de">Hydra</trd>
    <trd lng="en">Hydra</trd>
    <trd lng="nl">hydra</trd>
    <trd lng="pl">Hydra</trd>
    <trd lng="ro">Hidra</trd>
  </snc>
  <snc mrk="hidr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="hidred.0oj"><tld/>edoj
      </ref>
      (<trd lng="la">Hydra</trd>).
    </dif>
    <trd lng="ca">g&egrave;nere Hydra</trd>
    <trd lng="de">Wasserschlange</trd>
    <trd lng="en">hydra</trd>
    <trd lng="nl">waterslang</trd>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;</trd>
  <trd lng="bg">&c_h;&c_i;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>Hydra</trd>,
    <trd>hydra</trd>
  </trdgrp>
  <trd lng="el">&ypsilon_ton;&delta;&rho;&alfa;</trd>
  <trd lng="es">hidra</trd>
  <trd lng="fr">hydre</trd>
  <trd lng="hu">hidra</trd>
  <trd lng="it">idra</trd>
  <trd lng="pl">hydra</trd>
  <trd lng="pt">hidra</trd>
  <trd lng="ro">hidra</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>nezmar</trd>,
    <trd>polyp</trd>
  </trdgrp>
</drv>

<drv mrk="hidr.0uloj">
  <kap><tld/>uloj</kap>
  <snc mrk="hidr.0uloj.ZOO">
  <uzo tip="fak">ZOO</uzo>
  <uzo tip="stl">ARK</uzo>
    <dif>
      Iama ordo el la klaso <ref tip="super" cel="hidroz.0oj">hidrozooj</ref>
      (<trd lng="la">Hydroidea</trd>), proksimume ampleksanta la nunan
      subklason <trd lng="la">Hydroidolina</trd>.
    </dif>
  </snc>
  <trd lng="ca">hidro&iuml;deus</trd>
  <trd lng="fr">hydraires</trd>
  <trd lng="hu">hidraszer&udblac;ek</trd>
  <trd lng="it">idroida</trd>
  <trd lng="pl">stu&lstroke;bie</trd>
  <trd lng="uk">&c_g;&c_ib;&c_d;&c_r;&#x43E;&c_z;&#x43E;&c_ji;</trd>
</drv>

<drv mrk="hidr.H0o">
  <kap><tld lit="H"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Hya</mlg>
  <snc mrk="hidr.H0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Hydra</trd>):
      <ekz>
        NGC 2586 estas triopa stelo en konstelacio <tld lit="H"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=112490">NGC 2586</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_H;&c_i;&c_d;&c_r;&c_a;</trd>
  <trd lng="ca">l'Hidra Femella</trd>
  <trdgrp lng="cs">
    <trd>Hydra</trd>,
    <trd>hydra</trd>
  </trdgrp>
  <trd lng="de">Wasserschlange</trd>
  <trd lng="en">Hydra</trd>
  <trd lng="es">Hidra</trd>
  <trd lng="fr">Hydre<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="it">Idra</trd>
  <trd lng="pl">Hydra  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Hidra  <klr>(constela&#539;ie)</klr></trd>
  <trdgrp lng="sk">
    <trd>nezmar</trd>,
    <trd>polyp</trd>
  </trdgrp>
  <trd lng="uk">&c_G;&c_ib;&c_d;&c_r;&c_a;</trd>
</drv>

<drv mrk="hidr.Vir0o">
  <kap>Vir<tld/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Hyi</mlg>
  <snc mrk="hidr.Vir0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Hydrus</trd>):
      <ekz>
        NGC 643B estas galaksio en konstelacio Vir<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=110225">NGC 643B</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_V;&c_o;&c_d;&c_n;&c_a; &c_z;&c_m;&c_i;&c_ja;</trd>
  <trd lng="ca">l'Hidra Mascle</trd>
  <trd lng="de">Kleine Wasserschlange</trd>
  <trd lng="en">Hydrus</trd>
  <trd lng="es">Hidra macho</trd>
  <trd lng="fr">Hydre m&acirc;le<klr tip="ind"> (constellation)</klr></trd>
  <trdgrp lng="it">
    <trd>Serpente di Mare<klr> (astr.)</klr></trd>, 
    <trd>Idra maschio<klr> (astr.)</klr></trd>
  </trdgrp>
  <trd lng="pl">W&aogonek;&zdot; Wodny  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Hidra Austral&abreve;  <klr>(constela&#539;ie)</klr></trd>
</drv>
</art>
