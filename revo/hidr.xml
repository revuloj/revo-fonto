<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>hidr</rad>/o
</kap>
<drv mrk="hidr.0o">
  <kap><tld/>o</kap>
  <snc mrk="hidr.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Fabela serpento kun sep a&ubreve; na&ubreve; kapoj, el kiuj &ccirc;iu,
      kiam oni &gcirc;in tran&ccirc;is, estis tuj anstata&ubreve;ata de alia.
    </dif>
    <trd lng="de">Hydra</trd>
  </snc>
  <snc mrk="hidr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="hidred.0oj"><tld/>edoj</ref>
      (<trd lng="la">Hydra</trd>).
    </dif>
    <trd lng="de">Wasserschlange</trd>
  </snc>
  <trd lng="es">hidra</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;</trd>
</drv>

<drv mrk="hidr.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="hidroz.0oj">hidrozooj</ref>
      (<trd lng="la">Hydroidea</trd>).
    </dif>
  </snc>
</drv>
</art>

