<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>hidr</rad>/o
</kap>
<drv mrk="hidr.0o">
  <kap><tld/>o</kap>
  <snc mrk="hidr.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Monstra akvoserpento kun sep a&ubreve; na&ubreve; kapoj, el kiuj
      &ccirc;iu, kiam oni &gcirc;in detran&ccirc;is, estis tuj 
      anstata&ubreve;ata de du:
      <ekz>
        &gcirc;i estis <tld/>o, tuj ni ekkonis tion, &ccirc;ar &gcirc;i havis sep kapojn
        <fnt>
        <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>,
        <vrk><url ref="http://mujweb.cz/Kultura/malovec/naufabel.htm">Na&ubreve; fabeloj</url></vrk>,
        <lok>Granda polica fabelo, [2002?]</lok>
        </fnt>.      
      </ekz>
    </dif>
    <trd lng="de">Hydra</trd>
    <trd lng="en">Hydra</trd>
    <trd lng="nl">hydra</trd>
  </snc>
  <snc mrk="hidr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="hidred.0oj"><tld/>edoj</ref>
      (<trd lng="la">Hydra</trd>).
    </dif>
    <trd lng="de">Wasserschlange</trd>
    <trd lng="en">hydra</trd>
    <trd lng="nl">waterslang</trd>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_a;</trd>
  <trd lng="el">&ypsilon_ton;&delta;&rho;&alfa;</trd>
  <trd lng="es">hidra</trd>
  <trd lng="fr">hydre</trd>
  <trd lng="hu">hidra</trd>
  <trd lng="pt">hidra</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_a;</trd>
</drv>

<drv mrk="hidr.H0o">
  <kap><tld lit="H"/>o</kap>
  <mlg>Hya</mlg>
  <snc mrk="hidr.H0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="super" cel="stel.0figuro">stelfiguro</ref>
      (<trd lng="la">Hydra</trd>).
    </dif>
  </snc>
  <trd lng="de">Wasserschlange</trd>
  <trd lng="en">Hydra</trd>
  <trd lng="es">Hidra</trd>
</drv>

<drv mrk="hidr.0uloj">
  <kap><tld/>uloj</kap>
  <snc mrk="hidr.0uloj.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Ordo el la klaso <ref tip="super" cel="hidroz.0oj">hidrozooj</ref>
      (<trd lng="la">Hydroidea</trd>).
    </dif>
  </snc>
  <trd lng="fr">hydraires</trd>
  <trd lng="hu">hidraszer&udblac;ek</trd>
</drv>

<drv mrk="hidr.Vir0o">
  <kap>Vir<tld/>o</kap>
  <mlg>Hyi</mlg>
  <snc mrk="hidr.Vir0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="super" cel="stel.0figuro">stelfiguro</ref>
      (<trd lng="la">Hydrus</trd>).
    </dif>
  </snc>
  <trd lng="de">Kleine Wasserschlange</trd>
  <trd lng="en">Hydrus</trd>
  <trd lng="es">Hidra macho</trd>
</drv>
</art>
