<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kalk.xml,v 1.35 2023/12/02 21:20:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kalk</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="kalk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kalk.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Blanka minerala substanco, produktata per kalcinado de
      <ref cel="kalk.0osxtono"><tld/>o&scirc;tonoj</ref>.
    </dif>
    <subsnc mrk="kalk.0o.kauxstika">
      <dif>
        <ref tip="dif" cel="kalk.0okauxstika"><tld lit="K"/>o ka&ubreve;stika</ref>:
        <ekz>
          la popoloj estos kiel bruligita <tld/>o, kiel dornoj dehakitaj ili forbrulos en
          fajro
          <fnt>
            <bib>MT</bib>
            <lok>Jesaja 33:12</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="nl">
        <trd>ongebluste <ind>kalk</ind></trd>,
        <trd>calciumoxide</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kalk.0o.estingita">
      <dif>
        <ref tip="dif" cel="kalk.0oestingita"><tld lit="K"/>o estingita</ref>:
        <ekz>
          starigu al vi grandajn &scirc;tonojn kaj &ccirc;irka&ubreve;&scirc;miru ilin per <tld/>o
          <fnt>
            <bib>MT</bib>
            <lok>Readmono 27:2</lok>
          </fnt>;
        </ekz>
        <ekz>
          muroj kurbaj de maljuneco, malzorge blankigitaj per <tld/>o
          <fnt>
            <bib>IK</bib>
            <lok>&Ccirc;apitro I</lok>
          </fnt>;
        </ekz>
        <ekz>
          <klr>[de]</klr> la muroj <klr>[&#x2026;]</klr> blovis malsekeca odoro de <tld/>o, per
          kiu oni videble fre&scirc;e ilin blankigis
          <fnt>
            <bib>Marta</bib>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="nl">
        <trd>gebluste <ind>kalk</ind></trd>,
        <trd>calciumhydroxide</trd>
      </trdgrp>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="kalcit.0o">kalcito</ref>
    </refgrp>
  </snc>
  <snc mrk="kalk.0o.kalkajxo">
    <dif>
      <ref tip="dif" cel="kalk.0ajxo"><tld lit="K"/>a&jcirc;o</ref>:
      <ekz>
        li rigardis la muron kontra&ubreve;e, de kiu defalis la <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la kunlaboranton &scirc;uti&gcirc;is brikoj kaj <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kalkputz</trd>
    <trd lng="en">lime plaster</trd>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_p;&c_n;&c_a;</trd>
  <trd lng="br">raz<klr tip="ind"> (danvez)</klr></trd>
  <trd lng="ca">cal&ccedil;</trd>
  <trd lng="de"><ofc>*</ofc>Kalk</trd>
  <trd lng="en"><ofc>*</ofc>lime</trd>
  <trd lng="es">cal</trd>
  <trd lng="fa">&a_A_madda;&a_h;&f_k;</trd>
  <trdgrp lng="fr">
    <trd><ofc>*</ofc>chaux</trd>,
    <trd>oxyde de <ind>calcium</ind></trd>
  </trdgrp>
  <trd lng="hu">m&eacute;sz</trd>
  <trd lng="ja">&#x77F3;&#x7070;  <pr>&#x305B;&#x3063;&#x304B;&#x3044;</pr></trd>
  <trd lng="nl">kalk</trd>
  <trd lng="pl"><ofc>*</ofc>wapno</trd>
  <trdgrp lng="ro">
    <trd>varul nestins</trd>,
    <trd>varul ars</trd>,
    <trd>var stins</trd>
  </trdgrp>
  <trd lng="ru"><ofc>*</ofc>&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sv">kalk</trd>
  <trd lng="sk">v&aacute;pno</trd>
  <trdgrp lng="cs">
    <trd>neha&scaron;en&eacute; v&aacute;pno</trd>,
    <trd>p&aacute;len&eacute; v&aacute;pno</trd>,
    <trd>v&aacute;pno</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_a;&c_p;&c_n;&c_o;</trd>
  <trdgrp lng="zh">
    <trd>&#x7070; <pr>hu&#x12B;</pr></trd>,
    <trd>&#x77F3;&#x7070; <pr>sh&#xED;hu&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kalk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kalk.0i.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Surkovri surfacon per <tld/>a&jcirc;o, por &gcirc;in blankigi:
      <ekz>
        la muroj estis <tld/>itaj, jam iom grizigitaj de polvo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni prenu alian <tld/>a&jcirc;on kaj <tld/>u la domon
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 14:42</lok>
        </fnt>;
      </ekz>
      <ekz>
        la najbaroj rimarkis, ke la viro ofte <tld/>is murojn dum
        profundaj noktoj
        <fnt>
          <aut>-, trad. Sun Jian</aut>,
          <vrk>La Terura Sperto en Mia Domluado</vrk>,
          <lok><url ref="http://www.elerno.cn/penseo/PEN162.pdf">
          Penseo, 2005-04, numero 162a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_ja;&c_l;&c_ib;&c_c;&c_mol; <klr>(&c_v;&c_a;&c_p;&c_n;&c_a;&c_j;)</klr></trd>
    <trdgrp lng="br">
      <trd>raza&ntilde;<klr tip="ind"> (adeilad.)</klr></trd>,
      <trd>gwennraza&ntilde;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>encalcinar</trd>,
      <trd>emblanquinar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>kalken</trd>,
      <trd>t&uuml;nchen</trd>,
      <trd>wei&szlig;eln</trd>,
      <trd>wei&szlig;en</trd>
    </trdgrp>
    <trd lng="en">whitewash</trd>
    <trd lng="es">encalar</trd>
    <trd lng="fa">&a_s;&a_f;&f_y;&a_d;&f_k;&a_A;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>chauler<klr tip="ind"> (blanchir &agrave; la chaux)</klr></trd>,
      <trd>blanchir<klr tip="ind"> (&agrave; la chaux)</klr></trd>
    </trdgrp>
    <trd lng="hu">meszel</trd>
    <trd lng="nl">kalken</trd>
    <trd lng="ru">&c_b;&c_je;&c_l;&c_i;&c_t;&c_mol; <klr>(&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_mol;&c_ju;)</klr></trd>
    <trd lng="sv">kalka</trd>
    <trdgrp lng="zh">
    <trd>&#x572C; <pr>w&#x16B;</pr></trd>,
    <trd>&#x6747; <pr>w&#x16B;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>b&iacute;lit v&aacute;pnem</trd>,
    <trd>l&iacute;&ccaron;it v&aacute;pnem</trd>,
    <trd>nat&iacute;rat v&aacute;pnem</trd>
  </trdgrp>
  <trd lng="ja">&#x77F3;&#x7070;&#x3092;&#x5857;&#x308B;  <pr>&#x305B;&#x3063;&#x304B;&#x3044;&#x3092;&#x306C;&#x308B;</pr></trd>
  <trd lng="pl">wapnem bieli&cacute;</trd>
  <trd lng="ro">lapte de var var</trd>
  <trd lng="sk">neutralizova&tcaron; v&aacute;pnom</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_ib;&c_l;&c_i;&c_t;&c_i; (&c_h;&c_a;&c_t;&c_u;)</trd>,
    <trd>&c_v;&c_a;&c_p;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_gu;&c_r;&c_u;&c_n;&c_t;)</trd>
  </trdgrp>
</drv>

<drv mrk="kalk.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="morter.0o">Mortero</ref> de estingita <tld/>o kaj aliaj aldona&jcirc;oj,
      uzata por kovri muron:
      <ekz>
        la domon oni priskrapu interne &ccirc;irka&ubreve;e, kaj la <tld/>a&jcirc;on, kiun oni deskrapis,
        oni &jcirc;etu ekster la urbon
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 14:41</lok>
        </fnt>;
      </ekz>
      <ekz>
        Frederiko kolere de&scirc;iris la <tld/>a&jcirc;on de l&#x2019; muro kaj &jcirc;etis sur la lacertojn en
        la &gcirc;ardeno
        <fnt>
          <bib>IKr</bib>
          <lok>Neniam Plu!</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ni]</klr> forfrapis la <tld/>a&jcirc;on de kompletaj brikoj
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 11 (61)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gips.0o">gipso</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Kalkputz</trd>,
    <trd>Kalkmilch</trd>,
    <trd>T&uuml;nche</trd>
  </trdgrp>
  <trd lng="en">lime plaster</trd>
  <trd lng="ja">&#x77F3;&#x7070;&#x5857;&#x6599;  <pr>&#x305B;&#x3063;&#x304B;&#x3044;&#x3068;&#x308A;&#x3087;&#x3046;</pr></trd>
  <trd lng="zh">&#x7070;&#x6CE5; <pr>hu&#x12B;n&#xED;</pr></trd>
</drv>

<drv mrk="kalk.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kalk.0umi.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Sur&jcirc;eti <tld/>on sur kampon, por plibonigi la teron.
    </dif>
    <refgrp tip="vid">
      <ref cel="fekund.0igi">fekundigi</ref>,
      <ref cel="gras.0igi.sterki">grasigi<sncref/></ref>,
      <ref cel="sterk.0i">sterki</ref>
    </refgrp>
    <trd lng="be">&c_v;&c_a;&c_p;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_g;&c_l;&c_je;&c_b;&c_u;)</klr></trd>
    <trd lng="br">raza&ntilde;<klr tip="ind"> (gounez.)</klr></trd>
    <trdgrp lng="ca">
      <trd>encalcinar<klr> (el camp)</klr></trd>,
      <trd>empolsinar amb cal&ccedil;</trd>
    </trdgrp>
    <trd lng="de">kalken <klr>(Boden)</klr></trd>
    <trd lng="en">lime</trd>
    <trd lng="fa">&a_A_madda;&a_h;&f_k; &a_d;&a_A;&a_d;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>chauler<klr tip="ind"> (amender &agrave; la chaux)</klr></trd>,
      <trd>amender<klr tip="ind"> (&agrave; la chaux)</klr></trd>
    </trdgrp>
    <trd lng="hu">meszez</trd>
    <trd lng="nl">met <ind>kalk</ind> bemesten</trd>
    <trd lng="ru">&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;<klr> (&c_p;&c_o;&c_ch;&c_v;&c_u;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>b&iacute;lit v&aacute;pnem</trd>,
    <trd>l&iacute;&ccaron;it v&aacute;pnem</trd>,
    <trd>nat&iacute;rat v&aacute;pnem</trd>
  </trdgrp>
  <trd lng="ja">&#x77F3;&#x7070;&#x3092;&#x307E;&#x304F;  <pr>&#x305B;&#x3063;&#x304B;&#x3044;&#x3092;&#x307E;&#x304F;</pr></trd>
  <trd lng="pl">wapnowa&cacute;</trd>
  <trd lng="ro">lapte de var</trd>
  <trdgrp lng="sk">
    <trd>hnoji&tcaron; v&aacute;pnom</trd>,
    <trd>v&aacute;pni&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kalk.0argilo">
  <kap><tld/>argilo <fnt><bib>PV</bib></fnt></kap>
  <snc mrk="kalk.0argilo.MIN">
    <uzo tip="fak">MIN</uzo>
    <ref tip="dif" cel="marn.0o">marno</ref>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_r;&c_g;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_m;&c_a;&c_r;&c_g;&c_ib;&c_l;&c_mol;</trd>
    </trdgrp>
    <trd lng="ca">marga</trd>
    <trd lng="de">Mergel</trd>
    <trd lng="en">marl</trd>
    <trdgrp lng="fa">
      <trd>&a_A_madda;&a_h;&f_k;&a_r;&a_damma;&a_s;</trd>,
      <trd>&a_m;&a_A;&a_r;&a_l;</trd>
    </trdgrp>
    <trd lng="fr">marne</trd>
    <trd lng="hu">m&aacute;rga</trd>
    <trd lng="nl">mergel</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_je;&c_r;&c_g;&c_je;&c_l;&c_mol;</trd>,
      <trd>&c_r;&c_u;&c_h;&c_l;&c_ja;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x6CE5;&#x7070; <pr>n&#xED;hu&#x12B;</pr></trd>,
    <trd>&#x6CE5;&#x7070;&#x571F; <pr>n&#xED;hu&#x12B;t&#x1D4;</pr></trd>,
    <trd>&#x6CE5;&#x7070;&#x5CA9; <pr>n&#xED;hu&#x12B;y&#xE1;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="cs">sl&iacute;n</trd>
  <trd lng="ja">&#x6CE5;&#x7070;&#x571F;  <pr>&#x3069;&#x308D;&#x306F;&#x3044;&#x3064;&#x3061;</pr></trd>
  <trdgrp lng="pl">
    <trd>i&lstroke;</trd>,
    <trd>margiel</trd>
  </trdgrp>
  <trd lng="ro">marn&abreve;</trd>
  <trd lng="sk">slie&ncaron;</trd>
  <trd lng="uk">&c_m;&c_je;&c_r;&c_gu;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="kalk.0osxtono">
  <kap><tld/>o&scirc;tono <fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="kalk.0osxtono.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Tre disvastigita sedimenta&jcirc;o, konsistanta el
      <ref cel="kalcit.0o">kalcito</ref> a&ubreve;
      kalciti&gcirc;intaj fosilioj:
      <ekz>
        dispeci&gcirc;intaj <tld/>&scirc;tonoj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 27:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la glataj, poluritaj kolonoj kun ri&ccirc;e ornamitaj kapiteloj kaj altaj frisoj estis
        el <tld/>&scirc;tono
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kipra <tld/>o&scirc;tona grundo siaflanke favoras la arbojn, donante al ties
        fruktoj aparte pla&ccirc;an guston
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010535.php">Sanige ... kaj sensukere</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="gips.0o">gipso</ref>,
      <ref cel="kret.0o">kreto</ref>,
      <ref cel="marmor.0o">marmoro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_p;&c_n;&c_ja;&c_k;</trd>
  <trd lng="br">maen-raz</trd>
  <trd lng="ca">calc&agrave;ria</trd>
  <trd lng="de">Kalkstein</trd>
  <trd lng="en">limestone</trd>
  <trd lng="fa">&a_s;&a_n;&f_g; &a_A_madda;&a_h;&f_k;</trd>
  <trdgrp lng="fr">
    <trd>calcaire</trd>,
    <trd>pierre &agrave; <ind>chaux</ind></trd>
  </trdgrp>
  <trd lng="hu">m&eacute;szk&odblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x77F3;&#x7070;&#x5CA9;  <pr>&#x305B;&#x3063;&#x304B;&#x3044;&#x304C;&#x3093;</pr></trd>,
    <trd>&#x77F3;&#x7070;&#x77F3;  <pr>&#x305B;&#x3063;&#x304B;&#x3044;&#x305B;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">kalksteen</trd>
  <trd lng="pl">wapie&nacute;</trd>
  <trd lng="ro">calcar</trd>
  <trd lng="ru">&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_ja;&c_k;</trd>
  <trd lng="uk">&c_v;&c_a;&c_p;&c_n;&c_ja;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#x77F3;&#x7070;&#x5CA9; <pr>sh&#xED;hu&#x12B;y&#xE1;n</pr></trd>,
    <trd>&#x77F3;&#x7070;&#x77F3; <pr>sh&#xED;hu&#x12B;sh&#xED;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kalk.0okauxstika">
  <kap><tld/>o ka&ubreve;stika</kap>
  <snc mrk="kalk.0okauxstika.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Kalcioksido, <frm>CaO</frm>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_g;&c_a;&c_sh;&c_a;&c_n;&c_a;&c_ja; <ind>&c_v;&c_a;&c_p;&c_n;&c_a;</ind></trd>,
    <trd>&c_n;&c_ja;&c_t;&c_u;&c_sh;&c_a;&c_n;&c_a;&c_ja; <ind>&c_v;&c_a;&c_p;&c_n;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="br">raz bev</trd>
  <trdgrp lng="ca">
    <trd>cal&ccedil;</trd>,
    <trd><ind>cal&ccedil;</ind> viva</trd>
  </trdgrp>
  <trd lng="de">gebrannter Kalk</trd>
  <trdgrp lng="en">
    <trd>quicklime</trd>,
    <trd>burnt lime</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_h;&f_k; &a_z;&a_n;&a_d;&a_h;</trd>,
    <trd>&a_A_madda;&a_h;&f_k; &a_n;&a_sh;&f_k;&a_f;&a_t;&a_h;</trd>,
    <trd>&a_A_madda;&a_h;&f_k; &a_A_madda;&a_b;&#x200C;&a_n;&a_d;&f_y;&a_d;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">chaux vive</trd>
  <trd lng="hu">&eacute;getett <ind>m&eacute;sz</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_mol; &c_n;&c_je;&c_g;&c_a;&c_sh;&c_jo;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_mol; &c_je;&c_d;&c_k;&c_a;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ongebluste <ind>kalk</ind></trd>,
    <trd>calciumoxide</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wapno palone</trd>,
    <trd>tlenek wapnia</trd>
  </trdgrp>
  <trd lng="ro">oxid de calciu</trd>
