<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>konfederaci</rad>/o
</kap>
<drv mrk="konfed1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      Da&ubreve;ra asocii&gcirc;o de apartaj &scirc;tatoj, sendependaj pri ilia interna
      politiko, kaj formantaj unu kolektivan &scirc;taton pri la &gcirc;enerala
      ekstera politiko:
      <ekz>
        la <tld/>o de Svisujo inter 1798 kaj 1815; 
        la Germana Unio de 1815 &gcirc;is 1866; 
        la Brita Komonvelto.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Bund</trd>,
      <trd>Staatengemeinschaft</trd>,
      <trd>Konf&ouml;deration</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Aro da statute ligitaj federacioj de sindikatoj a&ubreve; similaj organizoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="auxton.0a">a&ubreve;tonoma</ref>
      <ref cel="federa.0o">federacio</ref>,
      <ref cel="lig.0o">ligo</ref>,
      <ref cel="alianc.0o">alianco</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Bund</trd>,
      <trd>Verband</trd>,
      <trd>Vereinigung</trd>
    </trdgrp>
  </snc>
  <adm>
    Ekzemploj sxangxendaj. "Komonvelto" ne estas difinita.
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_k;&c_a;&c_n;&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="fr">conf&eacute;d&eacute;ration</trd>
  <trd lng="nl">confederatie</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="konfed1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kunigi per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;'&c_ja;&c_d;&c_n;&c_a;&c_c;&c_mol; &c_u; <ind>&c_k;&c_a;&c_n;&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ju;</ind></trd>
  <trdgrp lng="fr">
    <trd>conf&eacute;d&eacute;rer</trd>,
    <trd>unir en une <ind>conf&eacute;d&eacute;ration</ind></trd>
  </trdgrp>
  <trd lng="nl">tot een <ind>confederatie</ind> verenigen</trd>
  <trd lng="ru">&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_mol; &c_v; &c_k;&c_o;&c_n;&c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ju;</trd>
</drv>

<drv mrk="konfed1.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eniri en <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;'&c_ja;&c_d;&c_n;&c_a;&c_c;&c_c;&c_a; &c_u; <ind>&c_k;&c_a;&c_n;&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ju;</ind></trd>,
    <trd>&c_u;&c_v;&c_a;&c_j;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_k;&c_a;&c_n;&c_f;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ju;</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>conf&eacute;d&eacute;rer</ind></trd>,
    <trd>entrer en <ind>conf&eacute;d&eacute;ration</ind></trd>
  </trdgrp>
  <trd lng="nl">zich tot een <ind>confederatie</ind> aaneensluiten</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_malmol;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_v; &c_k;&c_o;&c_n;&c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ju;</trd>,
    <trd>&c_v;&c_o;&c_j;&c_t;&c_i; &c_v; &c_k;&c_o;&c_n;&c_f;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ju;</trd>
  </trdgrp>
</drv>
</art>
