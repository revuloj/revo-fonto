<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pariz.xml,v 1.10 2019/06/30 18:21:48 revo Exp $">
<kap>
    <ofc>*</ofc><ofc>7</ofc><rad>Pariz</rad>/o
</kap>
<drv mrk="pariz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pariz.0o.SNC">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="urb.cxef0o" lst="voko:&ccirc;efurboj">&Ccirc;efurbo</ref> 
      kaj la plej granda urbo de <ref tip="malprt" cel="franc.F0ujo">Francujo</ref> &ccirc;e la
      bordoj de la rivero <ref cel="sejn.0o">Sejno</ref>,
      nordcentre de la lando:
      <ekz>
        e&ccirc; en <tld/>o herbo ne fari&gcirc;as cerbo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        morga&ubreve; mi veturos <tld/>on (a&ubreve; en <tld/>on)
        <fnt><bib>F</bib>, <lok>&FE; 28</lok></fnt>;
      </ekz>
      <ekz>
        kion <tld/>o apla&ubreve;das, Berlino malla&ubreve;das
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        scienculoj <klr>[&#8230;]</klr> naskotaj sur la kratero de E&ubreve;ropo: en <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Post jarmiloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        fervojoj ligas Kopenhagon kun <klr>[&#8230;]</klr> Parizo, Londono, Vieno, Neapolo
        <fnt>
          <bib>Fab4</bib>
          <lok>Peco da perlovico</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidis vin, civito, en <tld/>o, sur Triumfa Arko
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        la deka Universala Kongreso de l' Esperantistoj, okazonta en <tld/>o en a&ubreve;gusto 1914
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Paris</trd>
  <trd lng="en">Paris</trd>
  <trd lng="es">Par&iacute;s</trd>
  <trd lng="fr">Paris</trd>
  <trd lng="it">Parigi</trd>
  <trd lng="pl">Pary&zdot;</trd>
</drv>

<drv mrk="pariz.0ano">
  <kap><ofc>*</ofc> <tld/>ano,
  <var><kap><tld lit="p"/>ano</kap></var>
  </kap>
  <snc mrk="pariz.0ano.popolano">
    <dif>
      Lo&gcirc;anto de Parizo:
      <ekz>
        la <tld/>anoj estas gajaj homoj
        <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>;
      </ekz>
      <ekz>
        la lo&gcirc;antoj de unu el la diritaj urboj povus kompreni
        la lo&gcirc;anton de alia urbo ne pli bone, ol ekzemple
        la Berlinano la Londonanon a&ubreve; la <tld/>ano la Holandanon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="p"/>ano Marc Bavant <klr>[&#8230;]</klr> okupi&gcirc;as 
       sufi&ccirc;e multe pri leksikografio kaj instruado de Esperanto
        <fnt>
          <bib>Monato</bib>
          <aut>Boris KOLKER</aut>
          <vrk><url ref="https://www.monato.be/2003/007813.html">Revivinta
           Fundamento</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tiuj &ccirc;i dandoj, nomataj anka&ubreve; &leftquot;<tld lit="p"/>anoj&#8221;, &ccirc;ar 
        ili pasigis tempon en la franca &ccirc;efurbo <tld/>o, sin
        vestas per luksaj vesta&jcirc;oj, e&ccirc; frakoj
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Louis Madzella/pg</aut>
          <vrk><url ref="&Monato;/2006/009015.php">La stilo estas la homo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Pariser</trd>
  <trd lng="en">Parisian</trd>
  <trd lng="es">parisiense</trd>
  <trd lng="fr">Parisien</trd>
  <trd lng="it">parigino</trd>
  <trd lng="pl">pary&zdot;anin</trd>
</drv>


<drv mrk="pariz.0anino">
  <kap><tld/>anino,
  <var><kap><tld lit="p"/>anino</kap></var></kap>
  <snc mrk="pariz.0anino.popolanino">
    <dif>
      Lo&gcirc;antino de Parizo:
      <ekz>
        li &scirc;uldas promesplenumon al la <tld lit="p"/>anino
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo Silas</aut>
          <vrk><url ref="&Monato;/2015/011507.php">La lastaj tagoj de Lillo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        septembra blago: la bela enpikita &scirc;ika <tld lit="p"/>anino
        <fnt>
          <vrk>Septembra blago</vrk>,
          <lok><url ref="http://pauledouard.over-blog.com/article-septembra-blago-la-bela-enpikita-ika-par-110225331.html">le
        blog pauledouard </url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Pariserin</trd>
</drv>
</art>

