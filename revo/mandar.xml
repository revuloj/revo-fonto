<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mandar.xml,v 1.21 2024/01/25 20:10:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>mandaren</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="i">mandarin</rad>/o <fnt><bib>FK</bib></fnt></kap></var>
</kap>
<drv mrk="mandar.0o">
  <kap><tld/>o,
    <var><kap><tld var="i"/>o</kap></var>
  </kap>
  <snc mrk="mandar.0o.POL">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Altranga diplomita <ref tip="super" cel="ofic.sxtat0isto">&scirc;tatoficisto</ref>
      en malnova &Ccirc;inujo:
      <ekz>
        malsupre sur la strato la <tld/>oj ricevis batojn, la bambuaj kanoj estis
        rompitaj sur iliaj &scirc;ultroj, kaj tamen tio estis oficistoj de la unua &gcirc;is la
        na&ubreve;a gradoj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la nacia lingvo de &Ccirc;inio estas &ccirc;efe reprezentita de la norda <tld/>a, precipe el
        la regiono de Pekino
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; 300&#x202F;000&#x202F;000 el ili parolas la lingvon <tld var="i" lit="M"/>an, &ccirc;iuj <tld var="i"/>oj devas
        koni tiun &ccirc;i lingvon, kaj &ccirc;iuj, kiuj &gcirc;in ankora&ubreve; ne parolas, devas &gcirc;in lerni
        <fnt>
          <bib>FK</bib>
          <lok>El la Vivo kaj Sciencoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o <nom>Fu Kong</nom> lar&gcirc;e malfermis la okulojn pro la surprizo
        <fnt>
          <aut>Hevesi L., trad. Vizi L.</aut>,
          <vrk><url ref="https://sites.google.com/site/laaventurojdeandreojelky">
          La aventuroj de Andreo Jelky</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <ref tip="hom" cel="mandar1.0o">mandarino (frukto)</ref>
  <trd lng="be">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_y;&c_n; <klr>(&c_ch;&c_y;&c_n;&c_o;&c_w;&c_n;&c_ib;&c_k; &c_u; &c_K;&c_ib;&c_t;&c_a;&c_ib;)</klr></trd>
  <trd lng="br">mandarin<klr tip="ind"> (den)</klr></trd>
  <trd lng="ca">mandar&iacute;</trd>
  <trd lng="cs">mandar&iacute;n</trd>
  <trd lng="de">Mandarin</trd>
  <trd lng="en">mandarin</trd>
  <trd lng="es">mandar&iacute;n</trd>
  <trd lng="fr">mandarin</trd>
  <trd lng="hu">mandarin<klr tip="ind"> (hivatalnok)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30DE;&#x30F3;&#x30C0;&#x30EA;&#x30F3;</trd>,
    <trd>&#x30DF;&#x30AB;&#x30F3;  <pr>&#x307F;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">mandarijn<klr tip="ind"> (Chinees ambtenaar)</klr></trd>
  <trd lng="pl">mandaryn</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_i;&c_n; <klr tip="amb">(&c_k;&c_i;&c_t;&c_a;&c_j;&c_s;&c_k;&c_i;&c_j; &c_v;&c_je;&c_l;&c_mol;&c_m;&c_o;&c_zh;&c_a;)</klr></trd>
  <trd lng="sk">mandar&iacute;n</trd>
  <trd lng="uk">&c_m;&c_a;&c_n;&c_d;&c_a;&c_r;&c_i;&c_n;</trd>
  <trd lng="zh">&#x6587;&#x5B98; <pr>w&#xE9;ngu&#x101;n</pr></trd>
</drv>
</art>
