<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: agit.xml,v 1.32 2022/05/11 17:38:37 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>agit</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="agit.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="agit.0i.MOV">
    <dif>
      Vigle
      <ref tip="vid" cel="sku.0i.MOV_svingi">skuetadi<sncref/></ref>,
      moveti rapide kaj ripete:
      <ekz>
        Baskojn de l' frako vivige <tld/>is // Vento petola tra vintra pejza&gcirc;'
        <fnt>
          <bib>LSB</bib>
          <lok>Tro Homa Sorto De Iu Ne&gcirc;homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iajn membrojn <tld/>is konvulsiaj spasmoj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la voja&gcirc;antinoj eniris en la vastan hotelegon, <klr>[&#8230;]</klr> ilin mirigis
        la tumulta <tld/>ado, kiu regis
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maro estis <tld/>ata de maltrankviliga movado
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_e;&c_s;&c_mol;&c_c;&c_ib;</trd>
    <trd lng="fr">agiter</trd>
    <trd lng="nl">schudden</trd>
    <trdgrp lng="pt">
      <trd>agitar</trd>,
      <trd>sacolejar</trd>,
      <trd>sacudir</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>sch&uuml;tteln</trd>,
      <trd>r&uuml;tteln</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_s;&c_t;&c_r;&c_ja;&c_h;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sk">trias&tcaron;</trd>
    <trdgrp lng="tr">
      <trd>&ccedil;alkalamak</trd>,
      <trd>sallay&inodot;p kar&inodot;&scedil;t&inodot;rmak</trd>
    </trdgrp>
  </snc>
  <snc mrk="agit.0i.MENS">
    <dif>
      Malkvietigi, <ref tip="super" cel="trankv.mal0igi">maltrankviligi</ref>.
    </dif>
    <subsnc mrk="agit.0i.MENS_pasive">
      <dif>
        Naski en animo sinsekvajn sentojn;
        <ref tip="vid" cel="sku.0i.FIG">skui<sncref/></ref>:
        <ekz>
          lian amikon <tld/>as nerva tremado
          <fnt>
            <bib>ChL</bib>
            <lok>&Ccirc;apitro Unua</lok>
          </fnt>;
        </ekz>
        <ekz>
          <klr>[la]</klr> viza&gcirc;o <klr>[&#8230;]</klr> ne perfidu la kortu&scirc;a&jcirc;ojn, kiuj <tld/>as
          lian animon
          <fnt>
            <bib>ChL</bib>
            <lok>&Ccirc;apitro Dua</lok>
          </fnt>;
        </ekz>
        <ekz>
          unuflanke malpacienco, aliflanke timo lin <tld/>is
          <fnt>
            <bib>VivZam</bib>
            <lok>Vivo de Zamenhof</lok>
          </fnt>;
        </ekz>
        <ekz>
          Fernando havis dormon malkvietan, <tld/>itan de inkubson&gcirc;oj
          <fnt>
            <bib>ChL</bib>
            <lok>&Ccirc;apitro Unua</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="ekscit.0i">eksciti</ref>,
        <ref cel="emoci.0i">emocii</ref>
      </refgrp>
      <trd lng="be">&c_h;&c_v;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">aufw&uuml;hlen</trd>
      <trdgrp lng="fr">
        <trd>agiter</trd>,
        <trd>troubler</trd>
      </trdgrp>
      <trd lng="nl">opwinden</trd>
      <trdgrp lng="pt">
        <trd>agitar</trd>,
        <trd>sacolejar</trd>,
        <trd>preocupar</trd>
      </trdgrp>
      <trd lng="ru">&c_v;&c_o;&c_l;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      <trd lng="tr">&scedil;a&scedil;&inodot;rtmak</trd>
    </subsnc>
    <subsnc mrk="agit.0i.MENS_aktive">
      <dif>
        &Ccirc;iuflanke <ref tip="super" cel="pens.pri0i">pripensi</ref>, 
        <ref tip="super" cel="ekzame.0i.esplori">ekzameni</ref> kaj 
        <ref cel="diskut.0i">diskuti</ref> ideon:
        <ekz>
          li <tld/>is en sia animo grandiozajn projektojn.
       </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="akusx.0i.FIG">aku&scirc;i<sncref/></ref>,
        <ref cel="kov.el0i.FIG">elkovi<sncref/></ref>,
        <ref cel="konsid.0i">konsideri</ref>
      </refgrp>
      <trdgrp lng="be">
        <trd>&c_a;&c_b;&c_d;&c_u;&c_m;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_a;&c_b;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>soumettre &agrave; <ind>examen</ind></trd>,
        <trd>cogiter</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_o;&c_b;&c_d;&c_u;&c_m;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_b;&c_s;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>durchdenken</trd>,
        <trd>hin- und her<ind>w&auml;lzen</ind></trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="agit.0i.POL">
      <uzo tip="fak">POL</uzo>
      <dif>
        Malkvietigi, inciti homamasojn por politika celo:
        <ekz>
          li komisiis <klr>[&#8230;]</klr> <tld/>i kontra&ubreve; la pastroj
          <fnt>
            <bib>Far3</bib>
            <lok>&Ccirc;apitro XIII</lok>
          </fnt>;
        </ekz>
        <ekz>
          la fa&scirc;istoj kaj socialdemokratoj <tld/>as por siaj kandidatoj
          <fnt>
            <bib>Metrop</bib>
          </fnt>;
        </ekz>
        <ekz>
          mi <tld/>is tiam kontra&ubreve; la drinkado
          <fnt>
            <bib>Metrop</bib>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="propag.0i">propagandi</ref>
      </refgrp>
      <trd lng="be">&c_a;&c_g;&c_ib;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trdgrp lng="de">
        <trd>aufr&uuml;tteln</trd>,
        <trd>agitieren</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>exciter <klr>(les foules)</klr></trd>,
        <trd>susciter <klr>(l'engouement)</klr></trd>
      </trdgrp>
      <trd lng="nl">opruien</trd>
      <trdgrp lng="pt">
        <trd>agitar</trd>,
        <trd>amotinar</trd>,
        <trd>rebelar</trd>
      </trdgrp>
      <trd lng="ru">&c_a;&c_g;&c_i;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      <trdgrp lng="tr">
        <trd>ayakland&inodot;rmak</trd>,
        <trd>k&inodot;&scedil;k&inodot;rtmak</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trdgrp lng="cs">
    <trd>agitovat</trd>,
    <trd>burcovat</trd>,
    <trd>citov&ecaron; ot&rcaron;&aacute;st</trd>,
    <trd>m&iacute;chat</trd>,
    <trd>zat&rcaron;epat</trd>
  </trdgrp>
  <trd lng="en">agitate</trd>
  <trd lng="es">agitar</trd>
  <trdgrp lng="he">
    <trd>&lamed;&he;&samekh;&ayin;&yod;&resh;</trd>,
    <trd>&lamed;&he;&samekh;&yod;&tav;</trd>
  </trdgrp>
  <trd lng="it">agitare</trd>
  <trd lng="nl">overdenken</trd>
  <trd lng="sk">mie&scaron;a&tcaron; <klr>(kvapalinu mie&scaron;adlom)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_v;&c_i;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_b;&c_u;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_b;&c_u;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="agit.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="agit.0ado.KOMUNE">
    <dif>
      Malkvietigo, skuego, perturbego:
      <ekz>
        la Volapukistoj havis por sia <tld/>ado grandajn rimedojn kaj agadis per la plej
        vasta <klr>[&#8230;]</klr> reklamo
        <fnt>
          <bib>EE</bib>
          <lok>VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; kvieti&gcirc;i, la <tld/>ado de ondoj aliformi&gcirc;is en veran renversegon
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub la influo de <tld/>ado de diversaj niaj malamikoj oni ofte kulpigas nin pri
        celoj, kiujn ni neniam havis
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado en la Guildhall (Londono) en la 21a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ado <klr>[&#8230;]</klr> degeneris en civilan 
        militon
        <fnt>
          <aut>S. Zoppellaro</aut>,
          <vrk><url ref="http://disvastigo.esperanto.it/index.php/eo-eo/component/content/article/74-novajhoj-1/pliprofundigoj/3414-la-siriaj-rifugintoj-en-la-respubliko-de-armenio-memoro-integrigaj-sancoj-kaj-novaj-lingvaj-defioj">
          La siriaj rifu&gcirc;intoj en la Respubliko de
          Armenujo...</url></vrk>,
          <lok>2013-11-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_ib;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>agitace</trd>,
    <trd>agitov&aacute;n&iacute;</trd>,
    <trd>citov&eacute; ot&rcaron;&aacute;s&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Agitation</trd>
  <trd lng="es">agitaci&oacute;n</trd>
  <trd lng="fr">agitation</trd>
  <trd lng="he">&he;&samekh;&tav;&he;</trd>
  <trdgrp lng="sk">
    <trd>agit&aacute;cia</trd>,
    <trd>z&iacute;skavanie pr&iacute;vr&zcaron;encov</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&c_gu;&c_ib;&c_t;&c_a;&c_c;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="agit.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="agit.0igxi.KOMUNE">
    <dif>
      Malkvieti&gcirc;i:
      <ekz>
        liaj manoj konvulsie <tld/>i&gcirc;is
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia menso bildigis la &jcirc;usan pasintecon kaj mia cerbo <tld/>i&gcirc;is
        <fnt>
          <bib>KrM</bib>
          <lok>29. Sorto Flatas, Sorto Batas.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ctl>vidu, mi agiti&gcirc;as,</ctl> li ripro&ccirc;is,
        perdinte la anta&ubreve;an fadenon
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sku.0igxi">skui&gcirc;i</ref>,
      <ref cel="trem.0i">tremi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_r;&c_u;&c_sh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_z;&c_b;&c_u;&c_d;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>sch&uuml;tteln</ind></trd>,
    <trd>in <ind>Unruhe</ind> geraten</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>agiter</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_b;&c_u;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_h;&c_v;&c_i;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_je;&c_n;&c_t;&c_je;&c_zh;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="agit.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="agit.0igxo.KOMUNE">
    <dif>
      Ekmovi&gcirc;ado, malkvieti&gcirc;o:
      <ekz>
        emocia <tld/>i&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2013/011124.php"
              >Atentu la koron</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia vivo estos plena de <tld/>i&gcirc;o, batalo, rezultoj, kiujn tuj bagateligos
        malsukcesoj
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>i&gcirc;o de lia suferigata animo direkti&gcirc;is al 
        la kreado
        <fnt>
          <aut>T. Mann, trad. P. Buijnsters</aut>,
          <vrk>La morto en Venecio</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekscit.0igxo">eksciti&gcirc;o</ref>,
      <ref cel="tumult.0o">tumulto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_b;&c_u;&c_d;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_u;&c_z;&c_r;&c_u;&c_sh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Unruhe</trd>,
    <trd>Agitation</trd>
  </trdgrp>
  <trd lng="fr">agitation</trd>
</drv>

<drv mrk="agit.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="agit.0isto.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Homo, kiu <ref tip="vid" cel="agit.0i.POL"><tld/>as<sncref/></ref>,
      provas ka&ubreve;zi politikan malkvieton:
      <ekz>
        la spiritoj de la vulgaruloj bolas, sed tio estas faro de <tld/>istoj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;en &ccirc;iujn niajn kolektivojn ni volonte akceptas la knabojn&rightquot; &ndash; &leftquot;bonege&rightquot;, &ndash;
        konsentis la <tld/>isto, &ndash; &leftquot;sed tio tamen estas maskita formo de ekspluatado&rightquot;
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>4. La fatala sa&gcirc;eco.</lok>
        </fnt>;
      </ekz>
      <ekz>
        banalaj <tld/>istoj <klr>[&#8230;]</klr> &ccirc;iam tro facile falas
        en la stulta&jcirc;on ekzameni la privatan vivon de kapitalistaj
        gigantoj
        <fnt>
          <aut>B. Traven</aut>,
          <vrk><url ref="http://www.karapaco.de/u/B.Traven/b_traven_rozo_blanka.pdf">
          Rozo Blanka</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="propag.0isto">propagandisto</ref>
    <trd lng="be">&c_a;&c_g;&c_ib;&c_t;&c_a;&c_t;&c_a;&c_r;</trd>
    <trd lng="fr">agitateur</trd>
    <trdgrp lng="it">
      <trd>agitatore</trd>,
      <trd>sobillatore</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">agit&aacute;tor</trd>
  <trd lng="de">Agitator</trd>
  <trd lng="es">agitador</trd>
  <trd lng="he">&mem;&#1461;&samekh;&#1460;&yod;&tav;</trd>
  <trd lng="sk">agit&aacute;tor</trd>
</drv>
</art>
