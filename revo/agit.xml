<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: agit.xml,v 1.28 2021/02/28 14:48:40 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>agit</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="agit.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="agit.0i.MOV">
    <dif>
      Vigle
      <ref tip="vid" cel="sku.0i.MOV_svingi">skuetadi<sncref/></ref>,
      moveti rapide kaj ripete:
      <ekz>
        <tld/>u la boteleton anta&ubreve; ol trinki la kuracilon!
      </ekz>
      <ekz>
        malpacience li sin <tld/>is;
      </ekz>
      <ekz>
        mi a&ubreve;dis ek<tld/>on de flugiloj.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_e;&c_s;&c_mol;&c_c;&c_ib;</trd>
    <trd lng="fr">agiter</trd>
    <trd lng="nl">schudden</trd>
    <trdgrp lng="pt">
      <trd>agitar</trd>,
      <trd>sacolejar</trd>,
      <trd>sacudir</trd>
    </trdgrp>
    <trd lng="de">sch&uuml;tteln</trd>
    <trd lng="ru">&c_v;&c_s;&c_t;&c_r;&c_ja;&c_h;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="sk">trias&tcaron;</trd>
    <trdgrp lng="tr">
      <trd>&ccedil;alkalamak</trd>,
      <trd>sallay&inodot;p kar&inodot;&scedil;t&inodot;rmak</trd>
    </trdgrp>
  </snc>
  <snc mrk="agit.0i.MENS">
    <dif>
      Malkvietigi, maltrankviligi.
    </dif>
    <subsnc mrk="agit.0i.MENS_pasive">
      <dif>
        Naski en animo sinsekvajn sentojn;
        <ref tip="vid" cel="sku.0i.FIG">skui<sncref/></ref>:
        <ekz>
          &scirc;ia spirito estis <tld/>ata de maltrankviligaj pensoj.
        </ekz>
      </dif>
      <trd lng="be">&c_h;&c_v;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trd lng="de">aufw&uuml;hlen</trd>
      <trdgrp lng="fr">
        <trd>agiter</trd>,
        <trd>troubler</trd>
      </trdgrp>
      <trd lng="nl">opwinden</trd>
      <trdgrp lng="pt">
        <trd>agitar</trd>,
        <trd>sacolejar</trd>,
        <trd>preocupar</trd>
      </trdgrp>
      <trd lng="ru">&c_v;&c_o;&c_l;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      <trd lng="tr">&scedil;a&scedil;&inodot;rtmak</trd>
    </subsnc>
    <subsnc mrk="agit.0i.MENS_aktive">
      <dif>
        &Ccirc;iuflanke pripensi, ekzameni kaj diskuti ideon:
        <ekz>
          li <tld/>is en sia animo grandiozajn projektojn.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_a;&c_b;&c_d;&c_u;&c_m;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_a;&c_b;&c_m;&c_ja;&c_r;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>soumettre &agrave; <ind>examen</ind></trd>,
        <trd>cogiter</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_o;&c_b;&c_d;&c_u;&c_m;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_b;&c_s;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>durchdenken</trd>,
        <trd>hin- und herw&auml;lzen</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="agit.0i.POL">
      <uzo tip="fak">POL</uzo>
      <dif>
        Malkvietigi, inciti homamasojn por politika celo:
        <ekz>
          li komencis vastan sisteman <tld/>adon
          per publikaj paroladoj<fnt>Z</fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="propag.0i">propagandi</ref>
      </refgrp>
      <trd lng="be">&c_a;&c_g;&c_ib;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trdgrp lng="de">
        <trd>aufr&uuml;tteln</trd>,
        <trd>agitieren</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>exciter <klr>(les foules)</klr></trd>,
        <trd>susciter <klr>(l'engouement)</klr></trd>
      </trdgrp>
      <trd lng="nl">opruien</trd>
      <trdgrp lng="pt">
        <trd>agitar</trd>,
        <trd>amotinar</trd>,
        <trd>rebelar</trd>
      </trdgrp>
      <trd lng="ru">&c_a;&c_g;&c_i;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      <trdgrp lng="tr">
        <trd>ayakland&inodot;rmak</trd>,
        <trd>k&inodot;&scedil;k&inodot;rtmak</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <trdgrp lng="cs">
    <trd>agitovat</trd>,
    <trd>burcovat</trd>,
    <trd>citov&ecaron; ot&rcaron;&aacute;st</trd>,
    <trd>m&iacute;chat</trd>,
    <trd>zat&rcaron;epat</trd>
  </trdgrp>
  <trd lng="en">agitate</trd>
  <trd lng="es">agitar</trd>
  <trdgrp lng="he">
    <trd>&#x5DC;&#x5D4;&#x5E1;&#x5E2;&#x5D9;&#x5E8;</trd>,
    <trd>&#x5DC;&#x5D4;&#x5E1;&#x5D9;&#x5EA;</trd>
  </trdgrp>
  <trd lng="it">agitare</trd>
  <trd lng="nl">overdenken</trd>
  <trd lng="sk">mie&scaron;a&tcaron; <klr>(kvapalinu mie&scaron;adlom)</klr></trd>
</drv>

<drv mrk="agit.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="agit.0ado.KOMUNE">
    <dif>
      Malkvietigo, skuego, perturbego:
      <ekz>
        la <tld/>ado <klr>...</klr> degeneris en civilan 
        militon
        <fnt>
          <aut>S. Zoppellaro</aut>,
          <vrk><url ref="http://disvastigo.esperanto.it/index.php/eo-eo/component/content/article/74-novajhoj-1/pliprofundigoj/3414-la-siriaj-rifugintoj-en-la-respubliko-de-armenio-memoro-integrigaj-sancoj-kaj-novaj-lingvaj-defioj">
          La siriaj rifu&gcirc;intoj en la Respubliko de
          Armenujo...</url></vrk>,
          <lok>2013-11-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>agitace</trd>,
    <trd>agitov&aacute;n&iacute;</trd>,
    <trd>citov&eacute; ot&rcaron;&aacute;s&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="es">agitaci&oacute;n</trd>
  <trd lng="fr">agitation</trd>
  <trd lng="he">&#x5D4;&#x5E1;&#x5EA;&#x5D4;</trd>
  <trdgrp lng="sk">
    <trd>agit&aacute;cia</trd>,
    <trd>z&iacute;skavanie pr&iacute;vr&zcaron;encov</trd>
  </trdgrp>
</drv>

<drv mrk="agit.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="agit.0igxi.KOMUNE">
    <dif>
      Malkvieti&gcirc;i:
      <ekz>
        maro <tld/>i&gcirc;as furioze;
      </ekz>
      <ekz>
        <ctl>vidu, mi agiti&gcirc;as,</ctl> li ripro&ccirc;is,
        perdinte la anta&ubreve;an fadenon
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">s'<ind>agiter</ind></trd>
</drv>

<drv mrk="agit.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="agit.0igxo.KOMUNE">
    <dif>
      Ekmovi&gcirc;ado, malkvieti&gcirc;o:
      <ekz>
        febra <tld/>i&gcirc;o;
      </ekz>
      <ekz>
        la <tld/>i&gcirc;o de lia suferigata animo direkti&gcirc;is al 
        la kreado
        <fnt>
          <aut>T. Mann, trad. P. Buijnsters</aut>,
          <vrk>La morto en Venecio</vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">agitation</trd>
</drv>

<drv mrk="agit.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="agit.0isto.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Homo, kiu <ref tip="vid" cel="agit.0i.POL"><tld/>as<sncref/></ref>,
      provas ka&ubreve;zi politikan malkvieton:
      <ekz>
        banalaj <tld/>istoj <klr>...</klr> &ccirc;iam tro facile falas
        en la 
        stulta&jcirc;on ekzameni la privatan vivon de kapitalistaj
        gigantoj
        <fnt>
          <aut>B. Traven</aut>,
          <vrk><url ref="http://www.karapaco.de/u/B.Traven/b_traven_rozo_blanka.pdf">
          Rozo Blanka</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_g;&c_ib;&c_t;&c_a;&c_t;&c_a;&c_r;</trd>
    <trd lng="fr">agitateur</trd>
    <trdgrp lng="it">
      <trd>agitatore</trd>,
      <trd>sobillatore</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">agit&aacute;tor</trd>
  <trd lng="es">agitador</trd>
  <trd lng="he">&#x5DE;&#x5B5;&#x5E1;&#x5B4;&#x5D9;&#x5EA;</trd>
  <trd lng="sk">agit&aacute;tor</trd>
</drv>
</art>
