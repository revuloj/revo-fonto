<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>posti&ccirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="posticx.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="posticx.0o.KOMUNE">
    <dif>
      Falsa, alisubstanca <ref tip="super"
      cel="imit.0ajxo.KOMUNE">imita&jcirc;o</ref> de membro, peco, a&jcirc;o:
      <ekz>
        nomo nuda kaj simpla, sen aldonoj, ornamoj kaj <tld/>oj
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 5a, p.
        423a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="protez.0o.MED">protezo</ref>
    </refgrp>
  </snc>
</drv>

<drv mrk="posticx.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="posticx.0a.KOMUNE">
    <dif>
      <ref tip="super" cel="fals.0a.malauxtenta">Falsa</ref>, nur pli-malpli bone
      imitanta la aspekton:
      <ekz>
        <tld/>a nazo de bufono.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">postiche<klr tip="amb"> (adj.)</klr></trd>
</drv>
</art>

