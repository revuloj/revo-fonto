<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: posticx.xml,v 1.9 2024/03/17 09:22:34 revo Exp $">
<kap>
  <rad>posti&ccirc;</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="posticx.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="posticx.0o.KOMUNE">
    <dif>
      Falsa, alisubstanca <ref tip="super" cel="imit.0ajxo.KOMUNE">imita&jcirc;o</ref> de membro, peco,
      a&jcirc;o:
      <ekz>
        nomo nuda kaj simpla, sen aldonoj, ornamoj kaj <tld/>oj
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 5a, p.
        423a</lok></fnt>;
      </ekz>
      <ekz>
        <ind>har<tld/>o</ind> <klr>(vd <ref cel="peruk.0o">peruko</ref>)</klr>;
        <trd lng="ca">post&iacute;s (subst.)</trd>
        <trd lng="es">postizo<klr> (pelo)</klr></trd>
        <trdgrp lng="nl">
          <trd>haarstukje</trd>,
          <trd>valse <ind>vlecht</ind></trd>
        </trdgrp>
      </ekz>
      <ekz>
        la rusa armeo nun instalis &ccirc;e kelkaj landlimoj balon-tankojn, do <tld/>ojn kun
        aero interne
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2010/010189.php"
              >Du mil dek</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="maket.0o">maketo</ref>,
      <ref cel="protez.0o.MED">protezo</ref>
    </refgrp>
    <rim>
      Ne konfuzu kun <ref tip="hom" cel="pastic.0o">pasti&ccirc;o</ref>.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_m;&c_ib;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_m;&c_u;&c_l;&c_ja;&c_zh;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>imitaci&oacute;</trd>,
    <trd>farsa</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Attrappe</trd>,
    <trd>Nachahmung</trd>,
    <trd>Nachbildung</trd>,
    <trd>F&auml;lschung</trd>,
    <trd>Imitation</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>decoy</trd>,
    <trd>dummy</trd>
  </trdgrp>
  <trd lng="nl">postiche</trd>
  <trdgrp lng="pl">
    <trd>atrapa</trd>,
    <trd>proteza</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>manechin</trd>,
    <trd>copie</trd>,
    <trd>protez&abreve;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_m;&c_ib;&c_t;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_a;&c_f;&c_o;&c_r;&c_ib;&c_ja;</trd>,
    <trd>&c_m;&c_u;&c_l;&c_ja;&c_zh;</trd>
  </trdgrp>
</drv>

<drv mrk="posticx.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="posticx.0a.KOMUNE">
    <dif>
      <ref tip="super" cel="fals.0a.malauxtenta">Falsa</ref>, nur
      pli-malpli bone
      imitanta la aspekton:
      <ekz>
        <tld/>a nazo de bufono.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_y;</trd>,
    <trd>&c_sh;&c_t;&c_u;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_ib;&c_m;&c_ib;&c_t;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">post&iacute;s <klr>(adj.)</klr></trd>
  <trdgrp lng="de">
    <trd>falsch<klr>(er Bart etc.)</klr></trd>,
    <trd>Kunst<klr>(-nase usw.)</klr></trd>,
    <trd>k&uuml;nstlich</trd>
  </trdgrp>
  <trd lng="es">postizo</trd>
  <trd lng="fr">postiche<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="nl">vals</trd>
  <trd lng="pl">sztuczny</trd>
  <trd lng="ro">artificial</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_a;&c_f;&c_o;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_sh;&c_t;&c_u;&c_ch;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

