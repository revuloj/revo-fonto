<kap>
    <rad>dol</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="dol.0o">
  <kap><tld/>o</kap>
  <snc mrk="dol.0o.SNC">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Volo direktita al plenumo de delikta ago, precipe en kriminala k
      civila juro:
      <ekz>
        dola hommortigo estas murdo.
        <fnt><bib/>, <lok/></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_m;&c_je;&c_r; <klr>(&c_z;&c_l;&c_a;&c_ch;&c_y;&c_n;&c_n;&c_y;)</klr></trd>
  <trd lng="ca">dol</trd>
  <trdgrp lng="cs">
    <trd>dolus</trd>,
    <trd>zl&yacute; &uacute;mysl</trd>
  </trdgrp>
  <trd lng="es">dolo</trd>
  <trd lng="fr">dol</trd>
  <trd lng="he">&#x5DB;&#x5D5;&#x5D5;&#x5E0;&#x5D4; &#x5DC;&#x5E4;&#x5E9;&#x5D5;&#x5E2;</trd>
  <trd lng="it">dolo</trd>
  <trd lng="la">dolus</trd>
  <trd lng="oc">d&ograve;l</trd>
  <trd lng="pl">oszustwo</trd>
  <trd lng="pt">dolo</trd>
  <trdgrp lng="sk">
    <trd>dolus</trd>,
    <trd>zl&yacute; &uacute;mysel</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#x43C;&c_ib;&c_r;</trd>,
    <trd>&#x43D;&c_a;&#x43C;&c_ib;&c_r;</trd>,
    <trd>&c_u;&#x43C;&c_i;&c_s;&c_je;&#x43B;</trd>
  </trdgrp>
</drv>
</art>

