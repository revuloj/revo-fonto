  <trd lng="he">&#1499;&#1493;&#1493;&#1504;&#1492; &#1500;&#1508;&#1513;&#1493;&#1506;</trd>
  <trd lng="it">dolo</trd>
  <trd lng="la">dolus</trd>
  <trd lng="oc">d&ograve;l</trd>
  <trd lng="pl">oszustwo</trd>
  <trd lng="pt">dolo</trd>
  <trdgrp lng="sk">
    <trd>dolus</trd>,
    <trd>zl&yacute; &uacute;mysel</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#1084;&c_ib;&c_r;</trd>,
    <trd>&#1085;&c_a;&#1084;&c_ib;&c_r;</trd>,
    <trd>&c_u;&#1084;&c_i;&c_s;&c_je;&#1083;</trd>
  </trdgrp>
</drv>
