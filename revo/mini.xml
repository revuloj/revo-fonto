<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mini.xml,v 1.20 2023/09/02 06:04:12 revo Exp $">
<kap>
  <rad>mini</rad>/o <fnt><bib>PV</bib></fnt> 
  <fnt>Z</fnt>
</kap>
<drv mrk="mini.0o">
  <kap><tld/>o</kap>
  <snc mrk="mini.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <frm>Pb<sub>3</sub>O<sub>4</sub></frm>;
      oran&gcirc;ru&gcirc;a plumba oksido 
      uzata interalie por &scirc;miri feron kontra&ubreve; &gcirc;ia 
      oksidi&gcirc;o, rusti&gcirc;o:
      <ekz>
        la likimunecon de nita junto oni atingas per papera a&ubreve; tola enmeta&jcirc;o impregnita per oleo a&ubreve; <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=98093">Nito</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_r;&c_y;&c_k;</trd>,
    <trd>
    &c_v;&c_o;&c_k;&c_ib;&c_s; &c_s;&c_mol;&c_v;&c_ib;&c_n;&c_c;&c_u;
    </trd>
  </trdgrp>
  <trd lng="ca">mini</trd>
  <trdgrp lng="cs">
    <trd>minium</trd>,
    <trd>su&rcaron;&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Mennige</trd>
  <trdgrp lng="en">
    <trd>minium</trd>,
    <trd>lead tetroxide</trd>,
    <trd>triplumbic tetroxide</trd>,
    <trd>red lead</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_r;&a_n;&a_j;</trd>
  <trd lng="fr">minium</trd>
  <trd lng="hu">m&iacute;nium</trd>
  <trd lng="ja">&#x925B;&#x4E39;  [<ind>&#x3048;&#x3093;&#x305F;&#x3093;</ind>]</trd>
  <trd lng="nl">menie</trd>
  <trd lng="pl">minia</trd>
  <trdgrp lng="pt">
   <trd>m&iacute;nio</trd>,
   <trd>acarto</trd>,
   <trd>azarc&atilde;o</trd>   
   <trd>zarc&atilde;o</trd>   
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_r;&c_i;&c_k;</trd>,
    <trd>
    &c_o;&c_k;&c_i;&c_s;&c_mol; &c_s;&c_v;&c_i;&c_n;&c_c;&c_a;
    </trd>
  </trdgrp>
  <trd lng="sk">m&iacute;nium</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x443;&#x440;&#x438;&#x43A;</trd>,
    <trd>&#x43E;&#x43A;&#x438;&#x441; &#x441;&#x432;&#x438;&#x43D;&#x446;&#x44E;</trd>
  </trdgrp>
</drv>
</art>
