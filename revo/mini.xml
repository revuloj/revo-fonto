<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mini</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="mini.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Pb<sub>3</sub>O<sub>4</sub> Oran&gcirc;ru&gcirc;a plumba oksido 
      uzatai. a. por &scirc;miri feron kontra&ubreve; &gcirc;ia 
      oksidi&gcirc;o, rusti&gcirc;o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_r;&c_y;&c_k;</trd>,
    <trd>
    &c_v;&c_o;&c_k;&c_ib;&c_s; &c_s;&c_mol;&c_v;&c_ib;&c_n;&c_c;&c_u;
    </trd>
  </trdgrp>
  <trd lng="de">Mennige</trd>
  <trd lng="hu">m&iacute;nium</trd>
  <trd lng="nl">menie</trd>
  <trd lng="pl">minia</trd>
  <trdgrp lng="pt">
   <trd>m&iacute;nio</trd>,
   <trd>acarto</trd>,
   <trd>azarc&atilde;o</trd>   
   <trd>zarc&atilde;o</trd>   
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_r;&c_i;&c_k;</trd>,
    <trd>
    &c_o;&c_k;&c_i;&c_s;&c_mol; &c_s;&c_v;&c_i;&c_n;&c_c;&c_a;
    </trd>
  </trdgrp>
</drv>
</art>
