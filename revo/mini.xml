<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mini.xml,v 1.22 2024/02/04 09:20:02 revo Exp $">
<kap>
  <rad>mini</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="mini.0o">
  <kap><tld/>o</kap>
  <snc mrk="mini.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <frm>Pb<sub>3</sub>O<sub>4</sub></frm>; plumba <ref tip="super" cel="oksid.0o">oksido</ref>;
      oran&gcirc;ru&gcirc;a <ref tip="super" cel="minera.0o">mineralo</ref> uzata kiel pigmento, interalie por
      malhelpi oksidi&gcirc;on, rusti&gcirc;on de fero:
      <ekz>
        la likimunecon de nita junto oni atingas per papera a&ubreve; tola enmeta&jcirc;o impregnita per oleo a&ubreve; <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=98093">Nito</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/c/c5/Minium-232909.jpg/353px-Minium-232909.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o el mino en Arizono
        <fnt>
          <aut>Rob Lavinsky, iRocks.com</aut>
          <lok><url ref="&WCW;/File:Minium-232909.jpg"
              >https://commons.wikimedia.org/wiki/File:Minium-232909.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_r;&c_y;&c_k;</trd>,
    <trd>
    &c_v;&c_o;&c_k;&c_ib;&c_s; &c_s;&c_mol;&c_v;&c_ib;&c_n;&c_c;&c_u;
    </trd>
  </trdgrp>
  <trd lng="ca">mini</trd>
  <trdgrp lng="cs">
    <trd>minium</trd>,
    <trd>su&rcaron;&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Mennige</trd>
  <trdgrp lng="en">
    <trd>minium</trd>,
    <trd>lead tetroxide</trd>,
    <trd>triplumbic tetroxide</trd>,
    <trd>red lead</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_r;&a_n;&a_j;</trd>
  <trd lng="fr">minium</trd>
  <trd lng="hu">m&iacute;nium</trd>
  <trd lng="ja">&#37467;&#20025;  <pr>&#12360;&#12435;&#12383;&#12435;</pr></trd>
  <trd lng="nl">menie</trd>
  <trd lng="pl">minia</trd>
  <trdgrp lng="pt">
   <trd>m&iacute;nio</trd>,
   <trd>acarto</trd>,
   <trd>azarc&atilde;o</trd>
   <trd>zarc&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_r;&c_i;&c_k;</trd>,
    <trd>
    &c_o;&c_k;&c_i;&c_s;&c_mol; &c_s;&c_v;&c_i;&c_n;&c_c;&c_a;
    </trd>
  </trdgrp>
  <trd lng="sk">m&iacute;nium</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_r;&c_i;&c_k;</trd>,
    <trd>&c_o;&c_k;&c_i;&c_s; &c_s;&c_v;&c_i;&c_n;&c_c;&c_ju;</trd>
  </trdgrp>
</drv>
</art>
