<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bigami.xml,v 1.27 2023/10/29 19:11:01 revo Exp $">
<kap>
  <rad>bigami</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bigami.0o">
  <kap><tld/>o</kap>
  <snc mrk="bigami.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Stato de homo havanta samtempe du edz(in)ojn:
      <ekz>
        en &Ccirc;ita pli ol du dekduojn da tiaj <tld/>oj oni nuligis
        <fnt><aut>J. Baghy</aut>, <vrk>Sur Sanga Tero</vrk>,
        <lok>1933</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="monoga.0o">monogamio,</ref>
    <ref tip="super" cel="poliga.0o">poligamio</ref>
    <trd lng="de">Doppelehe</trd>
    <trdgrp lng="pl">
      <trd>dwu&zdot;e&nacute;stwo</trd>,
      <trd>bigamia</trd>
    </trdgrp>
    <trd lng="sv">bigami</trd>
  </snc>
  <snc mrk="bigami.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Delikto de tiu, kiu vivas <tld/>e en la landoj, kie tio estas le&gcirc;e
      malpermesata.
    </dif>
    <trd lng="de">Bigamie</trd>
    <trd lng="pl">bigamia</trd>
    <trdgrp lng="sv">
      <trd>bigami</trd>,
      <trd>tvegifte</trd>
    </trdgrp>
    <trd lng="zh">&#x91CD;&#x5A5A; <pr>ch&#xF3;ngh&#x16B;n</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_v;&c_a;&c_ja;&c_sh;&c_l;&c_ju;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_v;&c_u;&c_sh;&c_l;&c_ju;&c_b;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">big&agrave;mia</trd>
  <trdgrp lng="cs">
    <trd>bigamie</trd>,
    <trd>dvoj&zcaron;enstv&iacute;</trd>
  </trdgrp>
  <trd lng="en">bigamy</trd>
  <trd lng="es">bigamia</trd>
  <trd lng="fr">bigamie</trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B4;&#x5D9;&#x5D2;&#x5B7;&#x5DE;&#x5B0;&#x5D9;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>kett&odblac;s <ind>h&aacute;zass&aacute;g</ind></trd>,
    <trd>big&aacute;mia</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E8C;&#x91CD;&#x7D50;&#x5A5A;  <pr>&#x306B;&#x3058;&#x3085;&#x3046;&#x3051;&#x3063;&#x3053;&#x3093;</pr></trd>,
    <trd>&#x91CD;&#x5A5A;  <pr>&#x3058;&#x3085;&#x3046;&#x3053;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">bigamie</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_o;&c_je;&c_b;&c_r;&c_a;&c_ch;&c_i;&c_je;</trd>,
    <trd>&c_d;&c_v;&c_o;&c_je;&c_zh;&c_jo;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_d;&c_v;&c_o;&c_je;&c_m;&c_u;&c_zh;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bigamia</trd>,
    <trd>dvoj&zcaron;enstvo</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_ib;&c_g;&c_a;&#x43C;&c_ib;&#x44F;</trd>,
    <trd>&c_d;&c_v;&#x43E;&c_sh;&#x43B;&#x44E;&c_b;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_d;&c_v;&#x43E;&c_jeu;&c_zh;&c_je;&#x43D;&c_s;&c_t;&c_v;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="bigami.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Tiu, kiu vivas <tld/>e.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_v;&c_a;&c_ja;&c_zh;&c_e;&c_n;&c_je;&c_c;</trd>
  <trd lng="ca">b&iacute;gam<klr> (subst.)</klr></trd>
  <trd lng="cs">bigamista</trd>
  <trd lng="de">Bigamist</trd>
  <trd lng="en">bigamist</trd>
  <trd lng="es">b&iacute;gamo</trd>
  <trd lng="fr">bigame</trd>
  <trd lng="he">&#x5E0;&#x5E9;&#x5D5;&#x5D9; &#x5DC;&#x5E9;&#x5EA;&#x5D9;
  &#x5E0;&#x5E9;&#x5D9;&#x5DD;</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;tnej&udblac;</trd>,
    <trd>big&aacute;mista</trd>
  </trdgrp>
  <trd lng="ja">&#x91CD;&#x5A5A;&#x8005;  <pr>&#x3058;&#x3085;&#x3046;&#x3053;&#x3093;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">bigamist</trd>
  <trdgrp lng="pl">
    <trd>bigamista<klr> {m}</klr></trd>
    <trd>bigamistka<klr> {f}</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_v;&c_o;&c_je;&c_zh;&c_jo;&c_n;&c_je;&c_c;</trd>
  <trd lng="sk">bigamista</trd>
  <trd lng="sv">bigamist</trd>
  <trd lng="uk">&c_d;&c_v;&#x43E;&c_jeu;&c_zh;&c_je;&#x43D;&c_je;&c_c;&#x44C;</trd>
</drv>
</art>
