<art mrk="$Id: citiz.xml,v 1.20 2022/03/14 15:25:39 revo Exp $">
<kap>
  <rad>citiz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="citiz.0o">
  <kap><tld/>o</kap>
  <snc mrk="citiz.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbusto kun flavaj a&ubreve; violaj floroj, &scirc;atataj
      de la kaproj, vivanta en E&ubreve;ropo, NordAfriko, Okcidentazio;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Cytisus</trd>):
      <ekz>
        maldekstre de granda <tld/>o
        lumo estingi&gcirc;is en tereta&gcirc;a fenestro
        <fnt>
          <aut>S. Johansson</aut>
          <vrk><url ref="http://esperanto.net/literaturo/bk/verk/brokant.html">
          Brokante</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_ja;&c_n;&c_o;&c_v;&c_je;&c_c;</trd>
  <trdgrp lng="ca">
    <trd>ginesta</trd>,
    <trd>g&egrave;nere Cytisus</trd>
  </trdgrp>
  <trd lng="de">Besenginster</trd>
  <trdgrp lng="en">
    <trd>Cytisus</trd>,
    <trd>broom <klr>(flowering plant)</klr></trd>
  </trdgrp>
  <trd lng="fr">cytise</trd>
  <trd lng="he">&#x5E7;&#x5D9;&#x5D8;&#x5D9;&#x5E1;&#x5D5;&#x5E1;</trd>
  <trd lng="hu">sepr&odblac;zan&oacute;t</trd>
  <trdgrp lng="pl">
    <trd>szczodrzeniec</trd>,
    <trd>&zdot;arnowiec</trd>
  </trdgrp>
  <trd lng="pt">citiso</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_k;&c_i;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_c;&c_i;&c_t;&c_i;&c_z;&c_u;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x456;&#x43D;&#x43E;&#x432;&#x430;&#x442;&#x44C;</trd>,
    <trd>&#x440;&#x43E;&#x43A;&#x438;&#x442;&#x43D;&#x438;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
