<vortaro>
<art mrk="$Id$">
<kap>
<rad>citiz</rad>/o
</kap>
<drv mrk="citiz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Cytisus</trd>), arbusto kun flavaj a&ubreve; violaj
      floroj, &scirc;atataj
      de la kaproj.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_ja;&c_n;&c_o;&c_v;&c_je;&c_c;</trd>
  <trd lng="de">Besenginster</trd>
  <trd lng="fr">cytise</trd>
  <trd lng="hu">sepr&odblac;zan&oacute;t</trd>
  <trdgrp lng="pl">
    <trd>szczodrzeniec</trd>,
    <trd>&zdot;arnowiec</trd>
  </trdgrp>
  <trd lng="pt">citiso</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_k;&c_i;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_c;&c_i;&c_t;&c_i;&c_z;&c_u;&c_s;</trd>
  </trdgrp>
</drv>
</art>
