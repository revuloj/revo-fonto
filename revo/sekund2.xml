<vortaro>
<art mrk="$Id$">
<kap>
   <rad>sekundar</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sekund2.0a">
   <kap><tld/>a</kap>
   <snc mrk="sekund2.0a.MIN">
     <uzo tip="fak">MIN</uzo>
     <uzo tip="klr">(pri epoko)</uzo>
     <dif>
       Apartenanta al la dua epoko.
     </dif>
     <refgrp tip="vid">
       <ref cel="kvater.0a">kvaternara</ref>,
       <ref cel="primar.0a">primara</ref>,
       <ref cel="tercia.0a">terciara</ref>.
     </refgrp>
   </snc>
   <trd lng="de">sekund&auml;r</trd>
   <trd lng="fr">secondaire</trd>
   <trd lng="hu">m&aacute;sodkori</trd>   
   <trdgrp lng="nl">
     <trd>Secundair<klr tip="ind"> (tijdperk)</klr></trd>,
     <trd>Mesozo&iuml;cum</trd>
   </trdgrp>
   <trdgrp lng="pl">
     <trd>drugorz&eogonek;dowy</trd>,
     <trd>mezozoiczny</trd>
   </trdgrp>
   <trd lng="ru">&c_v;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
