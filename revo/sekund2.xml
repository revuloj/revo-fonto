<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sekund2.xml,v 1.26 2025/04/05 15:16:35 revo Exp $">
<kap>
   <rad>sekundar</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sekund2.0a">
  <kap><tld/>a</kap>
  <snc mrk="sekund2.0a.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Dua, duaranga duagrada.
    </dif>
    <subsnc mrk="sekund2.0a.GEOL">
      <uzo tip="fak">GEOL</uzo>
      <uzo tip="klr">(pri epoko)</uzo>
      <dif>
        Apartenanta al la dua epoko.
      </dif>
    </subsnc>
    <subsnc mrk="sekund2.0a.EKON">
      <uzo tip="fak">EKON</uzo>
      <dif>
        Kiu transformas kruda&jcirc;ojn al konsuma&jcirc;oj:
        <ekz>
          <tld/>a sektoro.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="primar.0a">primara</ref>,
      <ref cel="tercia.0a">terciara</ref>,
      <ref cel="kvater.0a">kvaternara</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>duaranga</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_u;&c_g;&c_a;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_r;&c_u;&c_g;&c_a;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_d;&c_r;&c_u;&c_g;&c_a;&c_r;&c_a;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>druhotn&yacute;</trd>,
    <trd>m&eacute;n&ecaron; d&uring;le&zcaron;it&yacute;</trd>,
    <trd>postrann&iacute;</trd>,
    <trd>sekund&aacute;rn&iacute;</trd>,
    <trd>vedlej&scaron;&iacute;</trd>
  </trdgrp>
  <trd lng="de">sekund&auml;r</trd>
  <trd lng="fr">secondaire<klr> (&eacute;poque)</klr></trd>
  <trd lng="hu">m&aacute;sodkori</trd>
  <trdgrp lng="it">
    <trd>Secondario<klr tip="ind"> (era geologica)</klr></trd>,
    <trd>Mesozoico</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#31532;&#20108;&#12398;  <pr>&#12384;&#12356;&#12405;&#12383;&#12398;</pr></trd>,
    <trd>&#20108;&#27425;&#12398;  <pr>&#12405;&#12383;&#12376;&#12398;</pr></trd>,
    <trd>&#31532;&#20108;&#32000;&#12398;  <pr>&#12384;&#12356;&#12395;&#12365;&#12398;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Secundair<klr tip="ind"> (tijdperk)</klr></trd>,
    <trd>Mesozo&iuml;cum</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>drugorz&eogonek;dowy</trd>,
    <trd>mezozoiczny</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>druhorad&yacute;</trd>,
    <trd>druhotn&yacute;</trd>,
    <trd>sekund&aacute;rny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_t;&#1086;&c_r;&c_i;&#1085;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_r;&c_u;&c_g;&#1086;&c_r;&#1103;&c_d;&#1085;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20108;&#20195; <pr>&#232;rd&#224;i</pr></trd>,
    <trd>&#20210; <pr>zh&#242;ng</pr></trd>,
    <trd>&#31532;&#20108;&#32026; <pr>d&#236;&#232;rj&#237;</pr></trd>,
    <trd>&#31532;&#20108;&#32423; <pr>d&#236;&#232;rj&#237;</pr></trd>,
    <trd>&#27425;&#32026; <pr>c&#236;j&#237;</pr></trd>,
    <trd>&#27425;&#32423; <pr>c&#236;j&#237;</pr></trd>,
    <trd>&#27425;&#35201; <pr>c&#236;y&#224;o</pr></trd>
  </trdgrp>
</drv>
</art>
