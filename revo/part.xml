  <trd lng="ru">&c_d;&c_je;&c_j;&c_s;&c_t;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja;
  <ind>&c_ch;&c_a;&c_s;&c_t;&c_mol;</ind>
  </trd>
