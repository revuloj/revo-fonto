<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>part</rad>/o
</kap>
<drv mrk="part.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Io, kio povas esti facile apartigita de tuto a&ubreve; per penso,
      a&ubreve; per vido, a&ubreve; per efektiva disigo:
      <ekz>
        la kvin <tld/>oj de la mondo;
      </ekz>
      <ekz>
        la diversaj <tld/>oj de la korpo;
      </ekz>
      <ekz>
        distran&ccirc;i kukon en ses <tld/>ojn;
      </ekz>
      <ekz>
        lasta <tld/>o de la programo;
      </ekz>
      <ekz>
        &scirc;iru vin en du <tld/>ojn, la mondo trian
        postulos
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        por <tld/>on da homoj liberigi de unu facila regulo devigi
        multe pli grandan <tld/>on da aliaj homoj lerni multajn
        regulojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="er1.0o">ero</ref>,
      <ref cel="fragme.0o">fragmento</ref>,
      <ref cel="pec.0o">peco</ref>,
      <ref cel="pagx.0o">pa&gcirc;o</ref>,
      <ref cel="elemen.0o">elemento</ref>,
      <ref cel="divid">divido</ref>.
    </refgrp>
    <trd lng="be">&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>
    <trd lng="de">Teil</trd>
    <trd lng="hu">r&eacute;sz</trd>
    <trd lng="nl">deel</trd>
    <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Io, kion iu rajtas postuli el tuto, kiun oni dividas inter pluraj
      personoj:
      <ekz>
        disdoni al &ccirc;iu lian <tld/>on de kuko, al &ccirc;iu frato lian
        heredan <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="porci.0o">porcio</ref>,
      <ref cel="pren">partopreno</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_o;&c_l;&c_ja;</trd>,
      <trd>&c_p;&c_o;&c_r;&c_c;&c_y;&c_ja;</trd>
    </trdgrp>
    <trd lng="de">Anteil</trd>
  <trdgrp lng="hu">
    <trd>r&eacute;sz</trd>,
    <trd>r&eacute;szesed&eacute;s</trd>
  </trdgrp>
    <trd lng="nl">aandeel</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_o;&c_l;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>&ccaron;&aacute;st</trd>,
    <trd>d&iacute;l</trd>
  </trdgrp>
  <trd lng="en">part</trd>
  <trdgrp lng="fr">
    <trd>partie</trd>,
    <trd>morceau</trd>,
    <trd>part</trd>
  </trdgrp>
  <trd lng="pt">parte</trd>
</drv>

<drv mrk="part.0a">
  <kap><tld/>a</kap>
  <snc mrk="part.0a.KOMUNE">
    <dif>
      Estanta nur <tld/>o de tuto:
      <ekz>
        <ind><tld/>a pago</ind>, eklipso.
        <trd lng="hu">r&eacute;szfizet&eacute;s</trd>
      </ekz>
    </dif>
    <trd lng="de">teilweise</trd>
    <trd lng="fr">partiel</trd>
    <trdgrp lng="hu">
      <trd>r&eacute;sz-</trd>,
      <trd>r&eacute;szleges</trd></trdgrp>
    <trd lng="nl">deel-</trd>
  </snc>
  <snc mrk="part.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="part.0a.rilato">
      <dif>
        <klr>(p.p. <ref cel="rilat.ordo0o">ordo-rilato</ref>
        <frm>&#8804;</frm>)</klr>
        Tia, ke ekzistas du elementoj
        <frm><k>x</k></frm> kaj <frm><k>y</k></frm>, por kiuj veras
        nek <frm><k>x</k> &#8804; <k>y</k></frm>,
        nek <frm><k>y</k> &#8804; <k>x</k></frm>:
        <ekz>
          se la aro <frm><g>E</g></frm> enhavas pli ol unu elementon,
          la rilato <frm>&#8834;</frm> estas <tld/>a en
          <frm>2<sup><g>E</g></sup></frm>;
        </ekz>
        <ekz>
          la rilato &leftquot;<frm><k>a</k></frm> estas
          <ref cel="divid.0ebla.MAT">dividebla</ref> per
          <frm><k>b</k></frm>&rightquot; estas
          parta ordo-rilato ene de la aro de entjeroj.
        </ekz>
      </dif>
      <ref tip="ant" cel="tut.0eca.rilato">tuteca.</ref>
      <trd lng="de">Partial-<klr tip="amb">(ordnung)</klr></trd>
      <trd lng="en">partial<klr tip="ind"> (ordering)</klr></trd>
  <trdgrp lng="hu">
    <trd>r&eacute;szben-<klr tip="amb">(rendez&eacute;s)</klr></trd>,
    <trd>parci&aacute;lis</trd>
  </trdgrp>
      <trd lng="fr"><klr tip="ind">(relation d'ordre) </klr>partiel</trd>
      <trd lng="pl">cz&eogonek;&sacute;ciowy <klr tip="ind">
(porz&aogonek;dek)</klr></trd>
      <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;<klr
tip="ind"> (&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_k;)</klr></trd>
    </subsnc>
    <subsnc mrk="part.0a.diversaj">
      <refgrp tip="vid">
        <ref cel="deriv.parta0ajxo"><tld/>a deriva&jcirc;o</ref>,
        <ref cel="sum.parta0o"><tld/>a sumo</ref> de serio.
      </refgrp>
      <trd lng="hu">parci&aacute;lis</trd>
    </subsnc>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_s;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>
  <trd lng="en">partial</trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="part.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Disdoni al &ccirc;iu sian <tld/>on.
    </dif>
    <refgrp tip="vid">
      <ref cel="divid.0i">dividi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>partager</trd>,
    <trd>diviser<klr tip="ind"> (partager)</klr></trd>
  </trdgrp>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_l;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="hu">feloszt</trd>
  <trd lng="nl">verdelen</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol;
&c_n;&c_a; &c_ch;&c_a;&c_s;&c_t;&c_i;</trd>
</drv>

<drv mrk="part.0umo">
  <kap><tld/>umo</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 7</lok></fnt>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      Pozitiva <ref tip="super" cel="frakci.0o.MAT">frakcio</ref> pli
      malgranda ol unu:
      <ekz>
        <frm>&#8532;</frm>, <frm>&#8540;</frm>.
      </ekz>
    </dif>
    <adm>
      Vd adm sub frakcio pri vera/propra/norma frakcio, sinonimoj
      de ~umo. <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="hu">pozit&iacute;v t&ouml;rt</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja;
&c_d;&c_r;&c_o;&c_b;&c_mol;</trd>
</drv>

<drv mrk="part.0opreni">
  <kap><tld/>opreni</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Preni sian <tld/>on de io:
      <ekz>
        <tld/>opreni en festeno, kongreso, en la
        profitoj;
      </ekz>
      <ekz>
        <tld/>opreni je festeno, je la komuna &gcirc;ojo;
      </ekz>
      <ekz>
        <tld/>opreni festenon, la &gcirc;ojon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="divid.0i">dividi</ref>.
    </refgrp>
  <trd lng="hu">r&eacute;szt vesz</trd>
  </snc>
  <snc>
    <dif>
      Kunhelpi, kunpeni por plenumado de io:
      <ekz>
        <tld/>opreni laboron, en iu
        laboro, en komploto;
      </ekz>
      <ekz>
        la hazardo multe <tld/>oprenis en la sukceso.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxton.0o">&scirc;tono</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>r&eacute;szt vesz</trd>,
      <trd>hozz&aacute;j&aacute;rul</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Simpatii, kompati:
      <ekz>
        ni <tld/>oprenas vian funebron.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kondol.0o">kondolenco</ref>.
    </refgrp>
    <adm>
      &Ccirc;i tiu senco ne plu estas en PIV1 kaj aspektas
      ar&hcirc;aika.<aut>MB</aut>
    </adm>
    <trd lng="hu">osztozik</trd>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_z;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">participate</trd>
  <trdgrp lng="fr">
    <trd>participer<klr tip="ind"> (&agrave; un travail, une
    rencontre)</klr></trd>,
    <trd>partager<klr tip="ind"> (la peine de qn)</klr></trd>
  </trdgrp>
  <trd lng="nl">deelnemen</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;
&c_u;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="part.dis0igi">
  <kap>dis<tld/>igi</kap>
  <snc mrk="part.dis0igi.KOMUNE">
    <dif>
      Disigi en <tld/>ojn:
      <ekz>
        dis<tld/>igu al vi la landon per loto konforme al viaj familioj
        <fnt><bib>MT</bib>, <lok>&Nom; 33:54</lok></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="part.dis0igi.KOMP">
      <uzo tip="fak">KOMP</uzo>
      <dif>
        Apartigi logikajn pecojn de memoro a&ubreve; datumo, ebligante
        trakti ilin samrange kiel la tuton:
        <ekz>
          <ind>dis<tld/>igi</ind>
          <ref cel="disk.0o.fiksita">fiksitan diskon</ref> en
          <ref cel="disk.sub0o">subdiskojn</ref>
          <frm>C:</frm> kaj <frm>D:</frm>;
          <trd lng="ru">&c_r;&c_a;&c_z;&c_b;&c_i;&c_t;&c_mol;
&c_n;&c_a; <ind>&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_y;</ind></trd>
        </ekz>
        <ekz>
          <ind>dis<tld/>igi tabelon</ind> per sekcoj;
          <trd lng="ru">&c_r;&c_a;&c_z;&c_b;&c_i;&c_t;&c_mol;
&c_n;&c_a; <ind>&c_s;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_ja;</ind></trd>
        </ekz>
        <ekz>
          dis<tld/>igi <ref cel="datum.0bazo">datumbazon</ref>
          la&ubreve; valoro de &ccirc;ef&scirc;losilo.
        </ekz>
      </dif>
      <trd lng="en">to <ind>partition</ind></trd>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_z;&c_ja;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_ja;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>partager</trd>,
    <trd>diviser<klr tip="ind"> (partager)</klr></trd>,
    <trd>morceler</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;tszed</trd>,
    <trd>sz&eacute;tdarabol</trd>,
    <trd>sz&eacute;toszt</trd>
  </trdgrp>
  <trd lng="nl">verdelen</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>
  <adm>
    La senco-diferenco inter ~igi kaj dis~igi estas
    esplorinda.<aut>MB</aut>
  </adm>
</drv>

<drv mrk="part.dis0igo">
  <kap>dis<tld/>igo</kap>
  <snc mrk="part.dis0igo.KOMUNE">
    <dif>
      Ago dis<tld/>igi; rezulto a&ubreve; maniero de tiu ago.
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_z;&c_je;&c_l;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>partition</trd>,
      <trd>fractionnement</trd>,
      <trd>morcellement</trd>
    </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;tszed&eacute;s</trd>,
    <trd>sz&eacute;tdarabol&aacute;s</trd>
  </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <snc mrk="part.dis0igo.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>KompLeks</bib></fnt>
    <dif>
      <klr>(de <ref cel="ar.0o.MAT">aro</ref> <frm><g>E</g></frm>)</klr>
      Tia <ref cel="dis.0a.familio">disa</ref>
      <ref tip="super" cel="famili.0o.bildigo">familio</ref> de
      subaroj de <frm><g>E</g></frm>, ke la <ref
cel="kun.0ajxo.aro">kuna&jcirc;o</ref>
      de &gcirc;iaj termoj egalas al <frm><g>E</g></frm>:
      <ekz>
        &ccirc;iu dis<tld/>igo ebligas difini unikan ekvivalento-rilaton,
        kies ekvivalento-klasoj estas &gcirc;iaj termoj.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_b;&c_ib;&c_w;&c_k;&c_a;</trd>
    <trd lng="de">Zerlegung<klr tip="ind"> (einer Menge)</klr></trd>
    <trd lng="en">partition<klr tip="ind"> (of a set)</klr></trd>
    <trd lng="fr">partition<klr tip="ind"> (d'un ensemble)</klr></trd>
    <trdgrp lng="pl">
      <trd>podzia&lstroke;<klr tip="ind"> (zbioru)</klr></trd>,
      <trd>rozbicie<klr tip="ind"> (zbioru)</klr></trd></trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_b;&c_i;&c_je;&c_n;&c_i;&c_je;<klr
tip="ind"> (&c_m;&c_n;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_a;)</klr></trd>
  </snc>
</drv>

<drv mrk="part.oradis0igo">
  <kap>ora dis<tld/>igo</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>EKV</bib><lok>&para;307</lok></fnt>
  <snc>
    <dif>
      La nombro <frm>&Phi; = (1+&#8730;5)/2</frm>, egala al la valoro
      de ajna <ref cel="propor.0o.MAT">proporcio</ref> kun termoj
      <frm>(<k>a</k>,<k>b</k>,<k>a</k>+<k>b</k>,<k>a</k>)</frm>:
      <ekz>
        <uzo tip="fak">BELA</uzo>
        en arto la ora dis<tld/>igo estas konsiderata aparte harmonia
        proporcio.
      </ekz>
    </dif>
    <rim>
      Tiun nombron eblas ricevi kiel pozitivan solvon de la ekvacio
      <frm><k>x</k> = 1+<k>x</k><sup>-1</sup></frm> kaj anka&ubreve;
      kiel limeson de du sinsekvaj termoj de tia vico
      <frm>(<k>u</k><sub><k>n</k></sub>)</frm>, ke
      <frm><k>u</k><sub><k>n</k>+2</sub> =
      <k>u</k><sub><k>n</k>+1</sub>+<k>u</k><sub><k>n</k></sub></frm>.
    </rim>
    <rim>
      Troveblas pluraj sinonimoj, ekz-e <ctl>ora sekco</ctl>
      <fnt><bib>MatVort</bib></fnt> kaj <ctl>ora proporcio</ctl>
      (difinita en <fnt><bib>PIV2</bib></fnt>
      kiel <frm>&Phi;<sup>-1</sup></frm>).
    </rim>
  </snc>
  <trd lng="de">goldener <ind>Schnitt</ind></trd>
  <trd lng="en">golden <ind>section</ind></trd>
  <trd lng="fr">nombre d'or</trd>
  <trd lng="hu">aranymetsz&eacute;s</trd>
  <trdgrp lng="pl">
    <trd>z&lstroke;oty <ind>podzia&lstroke;</ind></trd>,
    <trd>liczba z&lstroke;ota</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_o;&c_l;&c_o;&c_t;&c_o;&c_je;
<ind>&c_s;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</ind></trd>
</drv>

<drv mrk="part.entjera0o">
  <kap>entjera <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 11</lok></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="reel.0o.MAT">reelo</ref>)</klr>
      La plej granda <ref cel="entjer.0o">entjero</ref>, kiu &gcirc;in
      ne superas:
      <ekz>
        la entjera <tld/>o de <frm>3,14</frm> estas <frm>3</frm>,
        kaj tiu de <frm>-6,758</frm> estas <frm>-7</frm>;
      </ekz>
      <ekz>
        la entjeran <tld/>on de reelo <frm><k>x</k></frm> oni kutime signas
        per <frm>E(<k>x</k>)</frm> a&ubreve; <frm>[<k>x</k>]</frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_e;&c_l;&c_a;&c_ja;
<ind>&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</ind></trd>
  <trd lng="de">ganzer <ind>Teil</ind></trd>
  <trdgrp lng="en">
    <trd>integral <ind>part</ind></trd>,
    <trd>integer <ind>part</ind></trd></trdgrp>
  <trd lng="fr">partie enti&egrave;re</trd>
  <trd lng="hu">eg&eacute;szr&eacute;sz</trd>
  <trdgrp lng="pl">
    <trd>ca&lstroke;o&sacute;&cacute;<klr tip="ind"> (liczby
rzeczywistej)</klr></trd>,
    <trd>cecha<klr tip="ind"> (liczby rzeczywistej)</klr></trd>,
    <trd>cz&eogonek;&sacute;&cacute; ca&lstroke;kowita<klr tip="ind">
(liczby rzeczywistej)</klr></trd>,
    <trd>entier</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_l;&c_a;&c_ja;
<ind>&c_ch;&c_a;&c_s;&c_t;&c_mol;</ind></trd>,
    <trd>&c_a;&c_n;&c_t;&c_mol;&c_je;</trd></trdgrp>
</drv>

<drv mrk="part.frakcia0o">
  <kap>frakcia <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV2</bib><lok>komo</lok></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="reel.0o.MAT">reelo</ref>)</klr>
      La diferenco inter &gcirc;i kaj &gcirc;ia entjera <tld/>o:
      <ekz>
