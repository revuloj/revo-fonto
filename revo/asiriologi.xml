<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>asiriologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="asiriologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="asiriologi.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="malprt" cel="histor.0o.SCI">Historia</ref> scienco pri
      la
      antikvaj mezopotamiaj kulturoj <ref tip="vid" 
      cel="asiri.A0o.GEOG">asiria</ref> kaj najbaraj:
      <ekz>
        <tld/>o estas fako de arkeologio, kiu studas civilizojn de 
        Babilonaj kaj Asiriaj popoloj
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;asiriologio&rightquot;,
        2006-05-14</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">assiriologia</trd>
  <trd lng="fr">assyriologie</trd>
  <trd lng="it">assirologia</trd>
  <trd lng="pl">asyriologia</trd>
</drv>

<drv mrk="asiriologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="asiriologi.0isto.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Fakulo pri <ref tip="vid" cel="asiriologi.0o.HIS"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="ca">assiri&ograve;leg</trd>
  <trd lng="fr">assyriologue</trd>
  <trd lng="it">assirologo</trd>
  <trd lng="pl">asyriolog</trd>
</drv>
</art>

