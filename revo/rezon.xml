<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>rezon</rad>/i
</kap>
<drv mrk="rezon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Logike kunmeti argumentojn kaj tiri el ili konkludon:
      <ekz>
        analogie <tld/>i;
      </ekz>
      <ekz>
        <tld/>ado kaj filozofado panon ne donas<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pruv.0i">pruvi</ref>,
      <ref cel="dialek1.0o">dialektiko</ref>.
    </refgrp>
  </snc>
  <trd lng="de">vern&uuml;nftig denken</trd>
  <trd lng="nl">redeneren</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_s;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
