<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vals.xml,v 1.28 2015/07/04 08:37:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vals</rad>/o
</kap>
<drv mrk="vals.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Para, turna <ref tip="lst" cel="danc.0o"
      lst="voko:dancoj">danco</ref> 
      la&ubreve; tritakta ritmo:
      <ekz>
        Angla <tld/>o estas nomata anka&ubreve;
      </ekz>
      <ekz>
        <ind>malrapida <tld/>o</ind>
        <trd lng="bg">&c_b;&c_a;&c_v;&c_je;&c_n; &c_v;&c_a;&c_l;&c_s;</trd>
        <trd lng="de">langsamer Walzer</trd>
        <trd lng="hu">lass&uacute; kering&odblac;</trd>
      </ekz>
      <ekz>
        dum vienan <tld/>on oni nomas
      </ekz>
      <ekz>
       <ind>rapida <tld/>o</ind>.
       <trd lng="bg">&c_v;&c_i;&c_je;&c_n;&c_s;&c_k;&c_i; &c_v;&c_a;&c_l;&c_s;</trd>
       <trd lng="de">Wiener <ind>Walzer</ind></trd>
      </ekz>
    </dif>
    <trd lng="de">Walzer</trd>
    <trd lng="pt">valsa<klr tip="ind"> (dan&ccedil;a)</klr></trd>
  </snc>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika&jcirc;o por tia danco:
      <ekz>
        Johann Strauss komponis sian unuan <tld/>on en a&gcirc;o de 6 jaroj.
        <fnt><aut>Evgeni GEORGIEV</aut>:
        <vrk><url ref="&Monato;/006644.html">Jubileo de
        valsre&gcirc;o</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 09/1999</lok>.
        </fnt>
      </ekz>
    </dif>
    <trd lng="pt">valsa<klr tip="ind"> (m&uacute;sica)</klr></trd>    
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_mol;&c_s;</trd>
  <trd lng="bg">&c_v;&c_a;&c_l;&c_s;</trd>
  <trd lng="br">valsenn</trd>
  <trd lng="ca">vals</trd>
  <trdgrp lng="cs">
    <trd>vals</trd>,
    <trd>valčík</trd>
  </trdgrp>
  <trd lng="el">&beta;&alfa_ton;&lambda;&sigma_fina;</trd>
  <trd lng="en">waltz</trd>
  <trd lng="fr">valse</trd>
  <trd lng="hu">kering&odblac;</trd>
  <trd lng="nl">wals</trd>
  <trd lng="pl">walc</trd>
  <trd lng="ru">&c_v;&c_a;&c_l;&c_mol;&c_s;</trd>
  <trd lng="sk">valčík</trd>
  <trd lng="sv">vals</trd>
</drv>

<drv mrk="vals.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Danci <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_mol;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_t;&c_a;&c_n;&c_c;&c_u;&c_v;&c_a;&c_m; &c_v;&c_a;&c_l;&c_s;</trd>
  <trd lng="br">valsi&ntilde;</trd>
  <trd lng="ca">valsar</trd>
  <trd lng="cs">tančit valčík</trd>
  <trd lng="de">Walzer <ind>tanzen</ind></trd>
  <trd lng="es">vals</trd>
  <trd lng="fr">valser</trd>
  <trd lng="hu">kering&odblac;zik</trd>
  <trd lng="nl">walsen</trd>
  <trd lng="pl">ta&nacute;czy&cacute; <ind>walca</ind></trd>
  <trd lng="pt">valsar</trd>
  <trd lng="ru">&c_v;&c_a;&c_l;&c_mol;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">tancovať valčík</trd>
  <trd lng="sv">valsa</trd>
</drv>
</art>
