<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vals</rad>/o
</kap>
<drv mrk="vals.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Para, turna <ref tip="super" cel="danc.0o">danco</ref> la&ubreve; tritakta
      ritmo:
      <ekz>
        Angla <tld/>o estas nomata anka&ubreve; malrapida <tld/>o dum vienan
        <tld/>on oni nomas rapida <tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Wiener <ind>Walzer</ind></trd>
  </snc>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika&jcirc;o por tia danco:
      <ekz>
        Johann Strauss komponis sian unuan <tld/>on en a&gcirc;o de 6 jaroj.
        <fnt><aut>Evgeni GEORGIEV</aut>:
        <vrk><url ref="&Monato;/006644.html">Jubileo de valsre&gcirc;o</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 09/1999</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="en">waltz</trd>
  <trdgrp lng="fr">
    <trd>valse</trd>
  </trdgrp>
  <trd lng="nl">wals</trd>
  <trd lng="ru">&c_v;&c_a;&c_l;&c_mol;&c_s;</trd>
  <trd lng="sv">vals</trd>
</drv>

<drv mrk="vals.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Danci <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Wiener <ind>Walzer</ind> <ind>tanzen</ind></trd>
  <trd lng="es">vals</trd>
  <trd lng="nl">walsen</trd>
  <trd lng="ru">&c_v;&c_a;&c_l;&c_mol;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">valsa</trd>
</drv>
</art>
