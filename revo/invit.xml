<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>invit</rad>/i
</kap>
<drv mrk="invit.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="invit.0i.ien">
    <dif>
      Afable peti iun, ke li ien venu:
      <ekz>
        <tld/>i iun al balo, al ia amuzo<fnt>Z</fnt>,
        koncerto, festeno, ceremonio;
      </ekz>
      <ekz>
        <tld/>i amikon, gaston en sian domon;
      </ekz>
      <ekz>
        mi <tld/>is vin al mi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        malafable akcepti la <tld/>itojn de sia edzo;
      </ekz>
      <ekz>
        anstata&ubreve; sur la tronon min <tld/>i, vi eble min
        elpelus el la regno<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">megh&iacute;v</trd>
    <trd lng="pt">convidar</trd>
  </snc>
  <snc mrk="invit.0i.instige">
    <dif>
      Afable petante, <ref cel="instig.0i.iun_al_io">instigi</ref> iun,
      ke li ion faru:
      <ekz>
        <tld/>i iun fari paroladon;
      </ekz>
      <ekz>
        oni <tld/>is lin rakonti siajn voja&gcirc;ojn;
      </ekz>
      <ekz>
        <tld/>i al danco, por danci;
      </ekz>
      <ekz>
        li <tld/>is ilin man&gcirc;i panon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        al mi li komisiis, ke mi <tld/>u vin alesti anka&ubreve; al la
        prezentado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la belvetero <tld/>as <klr>(logas)</klr> al promenado;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        bela tapi&scirc;o <tld/>as sidi<fnt>Z</fnt>.
      </ekz>
    </dif>
   <trdgrp lng="hu">
     <trd>h&iacute;v</trd>,
     <trd>h&iacute;vogat</trd>,
     <trd>k&eacute;r <klr tip="ind"> (vmire)</klr></trd>
   </trdgrp>
   <trd lng="pt">invitar</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>, 
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pozvat</trd>,
    <trd>zv&aacute;t</trd>
  </trdgrp>
  <trd lng="de">einladen</trd>
  <trd lng="en">invite</trd>
  <trd lng="es">invitar</trd>
  <trdgrp lng="fr">
    <trd>inviter</trd>,
    <trd>convier</trd>,
    <trd>engager<klr tip="ind"> (qn &agrave; faire qc)</klr></trd>,
    <trd>prier<klr tip="ind"> (qn de faire qc)</klr></trd>.
  </trdgrp>
  <trd lng="nl">uitnodigen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_g;&c_l;&c_a;&c_sh;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_g;&c_l;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="invit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago, vortoj de tiu, kiu <tld/>as:
      <ekz>
        fari, akcepti <tld/>on;
      </ekz>
      <ekz>
        <tld/>a letero;
      </ekz>
      <ekz>
        mizero faras viziton ne atendante <tld/>on
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <trd lng="de">Einladung</trd>
    <trd lng="es">invitaci&oacute;n</trd>
    <trdgrp lng="fr">
      <trd>invitation</trd>,
      <trd>invite<klr tip="ind"> (&agrave; faire qc)</klr></trd>
    </trdgrp>
    <trd lng="hu">megh&iacute;v&aacute;s</trd>
    <trd lng="nl">uitnodiging</trd>
    <trdgrp lng="pt">
     <trd>convite<klr tip="amb"> (a&ccedil;&atilde;o)</klr></trd>, 
     <trd>invitamento</trd>, 
     <trd>invita&ccedil;&atilde;o</trd> 
    </trdgrp>
  </snc>
  <snc mrk="invit.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En <ref tip="malprt" cel="dialog.0o.KOMP">dialogo</ref>,
      ago de la komputa sistemo per kiu &gcirc;i
      <tld/>as(<sncref ref="invit.0i.instige"/>) la uzanton enigi
      komandon a&ubreve; datumon.
    </dif>
    <rim>
      Kutime la sistemo eligas por tio tekston a&ubreve; piktogramon,
      eble akompanante ilin per sonsignalo &dash; kiujn oni povas
      nomi <ctl><tld/>ilo</ctl>.
    </rim>
    <trd lng="en">prompt</trd>
    <trd lng="fr">invite<klr tip="ind"> (info.)</klr></trd>
    <trdgrp lng="hu">
     <trd>fogad&oacute;jel <klr tip="ind">(prompt)</klr></trd>,
     <trd>felsz&oacute;l&iacute;t&oacute;jel <klr tip="ind">(prompt)</klr></trd>,
     <trd>prompt</trd>,
   </trdgrp>
    <trd lng="nl">prompt</trd>
    <trd lng="pl">zach&eogonek;ta</trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_r;&c_a;&c_sh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_g;&c_l;&c_a;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
