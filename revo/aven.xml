<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>aven</rad>/o
</kap>

<drv mrk="aven.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Avena</trd>), uzata kiel nutra&jcirc;o por
      &ccirc;evaloj kaj aliaj bestoj, por fari keksojn ktp:
      <ekz>
        tritiko, <tld/>o kaj betoj estas de plej frua epoko tie
        kultivataj
        en la grandbienoj<fnt><aut>Lode Van De Velde</aut>:
        <vrk>Bierlando
        Belgio</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 1a, p.
        18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_jo;&c_s;</trd>
  <trd lng="bg">&c_o;&c_v;&c_je;&c_s;</trd>
  <trd lng="br">kerc'h</trd>
  <trd lng="ca">civada</trd>
  <trd lng="de">Hafer</trd>
  <trd lng="en">oats</trd>
  <trd lng="es">avena</trd>
  <trdgrp lng="fa">
    <trd>&f_y;&a_w;&a_l;&a_A;&a_f;</trd>,
    <trd>&a_j;&a_w;&a_d;&a_w;&a_s;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">avoine</trd>
  <trd lng="hu">zab</trd>
  <trd lng="it">avena</trd>
  <trd lng="nl">haver</trd>
  <trd lng="ru">&c_o;&c_v;&c_jo;&c_s;</trd>
  <trd lng="sk">ovos</trd>
  <trd lng="sv">havre</trd>
</drv>
</art>
