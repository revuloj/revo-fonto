<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>aven</rad>/o
</kap>
<drv mrk="aven.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Avena</trd>), uzata kiel nutra&jcirc;o por
      &ccirc;evaloj kaj aliaj bestoj, por fari keksojn ktp.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_jo;&c_s;</trd>
  <trd lng="de">Hafer</trd>
  <trd lng="en">oats</trd>
  <trd lng="fr">avoine</trd>
  <trd lng="hu">zab</trd>
  <trd lng="nl">haver</trd>
  <trd lng="ru">&c_o;&c_v;&c_jo;&c_s;</trd>
  <trd lng="sv">havre</trd>
</drv>
</art>
