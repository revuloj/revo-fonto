<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aven.xml,v 1.37 2023/12/13 12:45:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>aven</rad>/o
</kap>

<drv mrk="aven.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">Genro</ref> el
      familio <ref tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Avena</trd>), uzata kiel nutra&jcirc;o por
      &ccirc;evaloj kaj aliaj bestoj, por fari keksojn ktp:
      <ekz>
        sur &ccirc;iuj kampoj &ccirc;irka&ubreve;e kreskis <klr>[&#8230;]</klr> sekalo, hordeo kaj <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Poligono</lok>
        </fnt>;
      </ekz>
      <ekz>
        li fal&ccirc;is sekalon kaj <tld/>on, partoprenis la dra&scirc;adon
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>ka&ccirc;o</ind> (a&ubreve; <ref cel="porigx.0o">pori&gcirc;o</ref>) estas ka&ccirc;o el <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 8-9 (106-107)</lok>
        </fnt>;
        <trd lng="de">Haferbrei</trd>
        <trdgrp lng="en">
          <trd>porridge</trd>,
          <trd>oatmeal</trd>
        </trdgrp>
      </ekz>
      <ekz>
        tritiko, <tld/>o kaj betoj estas de plej frua epoko tie
        kultivataj
        en la grandbienoj<fnt><aut>Lode Van De Velde</aut>:
        <vrk>Bierlando
        Belgio</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 1a, p.
        18a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      aven<ref cel="flok.0o.KUI">floko<sncref/></ref>j,
      <ref cel="musli.0o">muslio</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/a/a9/AvenaSativa2.jpg/320px-AvenaSativa2.jpg" prm="CC BY-SA 3.0">paniklo
    de <tld/>o (Avena sativa) balda&ubreve; rikoltota
        <fnt>
          <aut>Christian Fischer, propra verko</aut>
          <lok><url ref="&WCW;/File:AvenaSativa2.jpg">&WCW;/File:AvenaSativa2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_v;&c_jo;&c_s;</trd>
  <trd lng="bg">&c_o;&c_v;&c_je;&c_s;</trd>
  <trd lng="bo">&t_ma;&t_na;&t_-a;&t_tsheg;</trd>
  <trd lng="br">kerc'h</trd>
  <trd lng="ca">civada</trd>
  <trd lng="cs">aven</trd>
  <trd lng="de">Hafer</trd>
  <trd lng="en">oats</trd>
  <trd lng="es">avena</trd>
  <trdgrp lng="fa">
    <trd>&f_y;&a_w;&a_l;&a_A;&a_f;</trd>,
    <trd>&a_j;&a_w;&a_d;&a_w;&a_s;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">avoine</trd>
  <trd lng="he">&shin;&yod;&bet;&vav;&lamed;&tav; &shin;&vav;&ayin;&lamed;</trd>
  <trd lng="hu">zab</trd>
  <trd lng="it">avena</trd>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30E9;&#x30B9;&#x30E0;&#x30AE;</trd>,
    <trd>&#x30A8;&#x30F3;&#x30D0;&#x30AF;</trd>,
    <trd>&#x30AA;&#x30FC;&#x30C8;&#x30E0;&#x30AE;</trd>
  </trdgrp>
  <trd lng="nl">haver</trd>
  <trd lng="pt">aveia</trd>
  <trd lng="ru">&c_o;&c_v;&c_jo;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>aven</trd>,
    <trd>ovos</trd>
  </trdgrp>
  <trd lng="sv">havre</trd>
  <trd lng="uk">&c_o;&c_v;&c_je;&c_s;</trd>
</drv>

<drv mrk="aven.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      El <tld/>o:
      <ekz>
        <klr>[la &ccirc;evalo]</klr> ripozu man&gcirc;ante <tld/>a&jcirc;on
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        pia montanino scias kuiri franda&jcirc;ojn, uzante <klr>[&#8230;]</klr> abrikotojn,
        safranon, sova&gcirc;an zingibron kaj <tld/>an farunon
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>;
      </ekz>
      <ekz>
        doni al la malsanuloj <tld/>an supon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_s;&c_ja;&c_n;&c_y;</trd>
  <trd lng="de">Hafer-</trd>
</drv>

</art>
