<art mrk="$Id: lauxrenci.xml,v 1.20 2024/01/15 16:14:08 revo Exp $">
<kap>
  <rad>la&ubreve;renci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lauxrenci.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Lr</mlg>
  <snc mrk="lauxrenci.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="103">elemento</ref>,
      <ref tip="super" cel="urani.trans0o">transuranio</ref> kun
      atomnumero 103, atompezo 260,1053;
      kies &ccirc;iuj izotopoj estas radioaktivaj:
      <ekz>
        oni povas sintezi <tld/>on el kalifornio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=43560">La&ubreve;rencio (elemento)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_w;&c_r;&c_e;&c_n;&c_s;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_l;&c_o;&c_u;&c_r;&c_je;&c_n;&c_s;&c_i;&c_j;</trd>
  <trd lng="br">laorensiom</trd>
  <trd lng="de">Lawrencium</trd>
  <trd lng="en">lawrencium</trd>
  <trd lng="es">laurencio</trd>
  <trd lng="fr">lawrencium</trd>
  <trd lng="hu">laurencium</trd>
  <trd lng="id">lawrensium</trd>
  <trd lng="it">lawrencio</trd>
  <trd lng="nl">lawrencium</trd>
  <trd lng="pl">lorens</trd>
  <trd lng="pt">laur&ecirc;ncio <klr tip="amb">(qu&iacute;m.)</klr></trd>
  <trd lng="ru">&c_l;&c_o;&c_u;&c_r;&c_je;&c_n;&c_s;&c_i;&c_j;</trd>
  <trd lng="sv">lawrencium</trd>
  <trd lng="uk">&#x43B;&#x43E;&#x443;&#x440;&#x435;&#x43D;&#x441;&#x456;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x9412; <pr>l&#xE1;o</pr></trd>,
    <trd>&#x94F9; <pr>l&#xE1;o</pr></trd>
  </trdgrp>
</drv>
</art>
