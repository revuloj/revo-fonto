<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>arog</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="arog.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malhumile postuli, pretendi pri io; nejuste atribui a&ubreve; alproprigi
      al:
      <ekz>
        <tld/>i al si ian meriton;
      </ekz>
      <ekz>
        la sekretario <tld/>is al si la rajton modifi la decidon de la
        komitato;
      </ekz>
      <ekz>
        mi ne <tld/>as al mi kredi ke mi konvinkos vin;
      </ekz>
      <ekz>
        vi <tld/>as al vi esperi, ke vi kaptos viron per
        flatoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="uzurp.0i">uzurpi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>arrogar-se</trd>,
    <trd>usurpar</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anma&szlig;en</trd>.
    <trd>fordern</trd>
  </trdgrp>
  <trd lng="en">arrogate</trd>
  <trd lng="es">arrogar</trd>
  <trdgrp lng="hu">
    <trd>arrog&aacute;l</trd>,
    <trd>mag&aacute;nak <ind>k&ouml;vetel</ind></trd>,
    <trd>nem &aacute;tall<klr> (vmit tenni)</klr></trd>,
    <trd>mer&eacute;szel<klr> (vmit tenni)</klr></trd>
  </trdgrp>
  <trd lng="it">arrogare</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_r;&c_z;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_m;&c_je;&c_t;&c_mol; &c_d;&c_je;&c_r;&c_z;&c_o;&c_s;&c_t;&c_mol; <klr>(&c_ch;&c_t;&c_o;-&c_l;. &c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;)</klr></trd>,
    <trd>&c_m;&c_n;&c_o;&c_g;&c_o; &c_s;&c_je;&c_b;&c_je; &c_p;&c_o;&c_z;&c_v;&c_o;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
