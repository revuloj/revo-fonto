<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arog.xml,v 1.22 2023/10/13 16:46:48 revo Exp $">
<kap>
  <rad>arog</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="arog.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malhumile <ref tip="super" cel="postul.0i.iu">postuli</ref>, 
      <ref tip="super" cel="preten.0i.postuli">pretendi</ref> pri io; nejuste
      atribui a&ubreve; alproprigi
      al:
      <ekz>
        <tld/>i al si ian meriton;
      </ekz>
      <ekz>
        la sekretario <tld/>is al si la rajton modifi la decidon de la
        komitato;
      </ekz>
      <ekz>
        li e&ccirc; <tld/>os al si &scirc;an&gcirc;i la tempojn kaj le&gcirc;ojn
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 7:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        profeton, kiu <tld/>os diri en Mia nomo ion, kion Mi ne ordonis al li diri,
        <klr>[&#x2026;]</klr> oni mortigu
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 18:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne <tld/>as al mi kredi ke mi konvinkos vin;
      </ekz>
      <ekz>
        urbestro: mi ne <tld/>as al mi kredi, mi ne valoras tian honoron
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>as al vi esperi, ke vi kaptos viron per
        flatoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        knabo, kiu <tld/>is ludi kun la bastonego de Jupitero kaj kiu venkis pigmeojn,
        dum li devis frakasi titanojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="uzurp.0i">uzurpi</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="arogan.0a">aroganta</ref>,
      <ref cel="modest.mal0a">malmodesta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>arrogar-se</trd>,
    <trd>usurpar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chovat se zpupn&ecaron;</trd>,
    <trd>drze se do&zcaron;adovat</trd>,
    <trd>osobovat <klr>(si n&ecaron;co)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anma&szlig;en</trd>.
    <trd>fordern</trd>
  </trdgrp>
  <trd lng="en">arrogate</trd>
  <trd lng="es">arrogar</trd>
  <trd lng="fr">arroger</trd>
  <trd lng="he">&lamed;&tav;&bet;&vav;&ayin; &bet;&het;&vav;&tsadi;&pe;&he;</trd>
  <trdgrp lng="hu">
    <trd>arrog&aacute;l</trd>,
    <trd>mag&aacute;nak <ind>k&ouml;vetel</ind></trd>,
    <trd>nem &aacute;tall<klr> (vmit tenni)</klr></trd>,
    <trd>mer&eacute;szel<klr> (vmit tenni)</klr></trd>
  </trdgrp>
  <trd lng="it">arrogare</trd>
  <trdgrp lng="ja">
    <trd>&#x4E0D;&#x5F53;&#x306B;&#x8981;&#x6C42;&#x3059;&#x308B;  <pr>&#x3075;&#x3068;&#x3046;&#x306B;&#x3088;&#x3046;&#x304D;&#x3085;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x52DD;&#x624B;&#x306B;&#x308F;&#x304C;&#x7269;&#x3068;&#x3059;&#x308B;  <pr>&#x304B;&#x3063;&#x3066;&#x306B;&#x308F;&#x304C;&#x3076;&#x3064;&#x3068;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x601D;&#x3044;&#x4E0A;&#x304C;&#x3063;&#x3066;&#xFF5E;&#x3059;&#x308B;  <pr>&#x304A;&#x3082;&#x3044;&#x3042;&#x304C;&#x3063;&#x3066;&#xFF5E;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>arogowa&cacute;</trd>,
    <trd>przyw&lstroke;aszcza&cacute; sobie prawa</trd>,
    <trd>przypisywa&cacute; sobie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_r;&c_z;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_m;&c_je;&c_t;&c_mol; &c_d;&c_je;&c_r;&c_z;&c_o;&c_s;&c_t;&c_mol; <klr>(&c_ch;&c_t;&c_o;-&c_l;. &c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;)</klr></trd>,
    <trd>&c_m;&c_n;&c_o;&c_g;&c_o; &c_s;&c_je;&c_b;&c_je; &c_p;&c_o;&c_z;&c_v;&c_o;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>arogantne po&zcaron;adova&tcaron;</trd>,
    <trd>n&aacute;rokova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_z;&c_u;&c_h;&c_v;&c_a;&c_l;&c_i;&c_m;</trd>,
    <trd>&c_m;&c_a;&c_t;&c_i; &c_z;&c_u;&c_h;&c_v;&c_a;&c_l;&c_ib;&c_s;&c_t;&c_mol; <klr>(&c_shch;&c_o;-&c_n;. &c_z;&c_r;&c_o;&c_b;&c_i;&c_t;&c_i;)</klr></trd>,
    <trd>&c_b;&c_a;&c_g;&c_a;&c_t;&c_o; &c_s;&c_o;&c_b;&c_ib;&nbsp;&c_d;&c_o;&c_z;&c_v;&c_o;&c_l;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
