<drv mrk="jugx.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="asign.0i">Asigni</ref> al iu ion
      (per <tld/>o):
      <ekz>
        mi al<tld/>is la unuan premion al la leporo kaj la duan al la
        limako
        <fnt><bib>Fab4</bib>, <lok>Kuristoj</lok></fnt>;
      </ekz>
      <ekz>
        la kvar aliajn Nobel-premiojn <klr>[&#8230;]</klr> al<tld/>as svedaj akademioj
        <fnt>
          <bib>Monato</bib>
          <aut>Claude Rouget</aut>
          <vrk><url ref="https://www.monato.be/2008/009374.php"
              >&Ccirc;u Al Gore meritas pacpremion?</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[vi]</klr> devus almena&ubreve; al<tld/>i honoron al mi
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Internacia Mona Fonduso <klr>[&#8230;]</klr> alju&gcirc;is prunton je 4 miliardoj da
        e&ubreve;roj al la islanda &scirc;tato
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Vanden Bempt</aut>
          <vrk><url ref="https://www.monato.be/2008/009665.php"
              >La nuna monda financa krizo</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rezultato de l&#8217; proceso <klr>[&#8230;]</klr> dependas, grandaparte, de
        <klr>[&#8230;]</klr> neprecizaj dokumentoj a&ubreve;, pli&gcirc;uste, de la senco kaj signifo,
        kiujn la ju&gcirc;istaro al<tld/>os rilate al iuj geografiaj detaloj en ili
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        al la bretona re&gcirc;o Salomon oni al<tld/>as la fondon de gimnazio por kanonikoj
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="https://www.monato.be/2009/009817.php"
              >Fortika&jcirc;urbo Gerando</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="difin">aldifini</ref>,
      <ref cel="skrib">alskribi</ref>,
      <ref cel="atribu.0i">atribui</ref>,
      <ref cel="imput.0i">imputi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_s;&c_u;&c_d;&c_zh;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>zuerkennen</trd>,
    <trd>zusprechen</trd>,
    <trd>verleihen <klr>(Preis)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>adjudicar</trd>,
    <trd>otorgar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>adjuger</trd>,
    <trd>attribuer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>oda&iacute;t&eacute;l</trd>,
    <trd>neki <ind>&iacute;t&eacute;l</ind></trd>
  </trdgrp>
<trdgrp lng="it">
    <trd>aggiudicare</trd>, 
    <trd>attribuire<klr> (tramite giudizio)</klr></trd>, 
    <trd>assegnare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>toewijzen</trd>,
    <trd>toekennen</trd>
  </trdgrp>
  <trd lng="pt">adjudicar</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_s;&c_u;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">tilld&ouml;ma</trd>
  <trd lng="uk">&c_p;&c_r;&c_i;&c_s;&c_u;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

