<drv mrk="sonor.0ilaro">
  <kap><tld/>ilaro</kap>
  <snc mrk="sonor.0ilaro.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="sin" cel="kariljon.0o">Kariljono</ref>:
      <ekz>
        tiuj harmoniaj sonoj <klr>[&#8230;]</klr> anstata&ubreve;is plenan <tld/>ilaron
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kanontondrado kaj bruado de <tld/>ilaro, uniformoj kaj brilantaj armiloj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">zvonkohra</trd>
  <trd lng="de">Glockenspiel</trd>
  <trd lng="en">carillon</trd>
  <trd lng="fr">carillon</trd>
  <trdgrp lng="hu">
    <trd>harangj&aacute;t&eacute;k</trd>,
    <trd>campanelli</trd>
  </trdgrp>
  <trd lng="sk">zvonkohra</trd>
</drv>

<drv mrk="sonor.0ilego">
  <kap><tld/>ilego</kap>
  <snc mrk="sonor.0ilego.KOMUNE">
    <dif>
      Tre granda <tld/>ilo:
      <ekz>
        la <tld/>ilegoj komencis brue soni kaj ekestis 
        movi&gcirc;ado en la vila&gcirc;o
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>;
      </ekz>
      <ekz>
        de la pre&gcirc;ejturoj estas formetitaj la <tld/>ilegoj kaj de l&#8217; kupoloj la orumitaj
        krucoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        pri la <tld/>ilego &scirc;i kredis, ke &gcirc;i estas eksperimento por veki la mortinton
        <fnt>
          <bib>VaK</bib>
          <lok>Sepa &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Glocke</trd>,
    <trd>Kirchenglocke</trd>
  </trdgrp>
  <trd lng="en">big bell</trd>
  <trd lng="fr">tocsin</trd>
</drv>

<drv mrk="sonor.0ilejo">
  <kap><tld/>ilejo, <var><kap><tld/>ilturo</kap></var></kap>
  <snc mrk="sonor.0ilejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Turo enhavanta <tld/>ilojn:
      <ekz>
        vidinta <klr>[&#8230;]</klr> sangon de laboristoj, mortpafitaj el <tld/>ilejo de la
        obskura katedralo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[en]</klr> la antikva <tld/>ilejo <klr>[&#8230;]</klr> datumanta de la dekunua
        jarcento <klr>[&#8230;]</klr> oni lo&gcirc;igis <klr>[&#8230;]</klr> novajn <tld/>ilojn, kiuj
        vibrigis atmosferon per gaja <tld/>ilaro
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">zvonice</trd>
  <trd lng="de">Glockenturm</trd>
  <trdgrp lng="en">
    <trd>belfry</trd>,
    <trd>bell tower</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>campanile</trd>,
    <trd>clocher</trd>
  </trdgrp>
  <trd lng="hu">harangtorony</trd>
  <trd lng="id"><ind>menara</ind> lonceng</trd>
  <trd lng="nl">klokkentoren</trd>
  <trd lng="ru">&c_z;&c_v;&c_o;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">zvonica</trd>
  <trd lng="uk">&c_d;&c_z;&c_v;&c_ib;&c_n;&c_i;&c_c;&c_ja;</trd>
</drv>

<drv mrk="sonor.0ilo">
  <kap><ofc>*</ofc><tld/>ilo</kap>
  <snc mrk="sonor.0ilo.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.frap0o" lst="voko:frapinstrumentoj">Frapinstrumento</ref>
      estiganta <tld/>on, &gcirc;enerale konsistanta el metala kava duonsfero
      kun svingebla <ref tip="prt" cel="frap.0ilo">frapilo</ref>:
      <ekz>
        per <tld/>ilo ni <tld/>as, per fajfilo ni fajfas
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        la vila&gcirc;a <tld/>ilo malgaje eksonis noktomezon
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la forgesita
        pipo</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>ilon de la pordo li ne povis atingi
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio sonas tute kiel vitraj <tld/>iloj!
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ambra horlo&gcirc;o parolis pri politik' tik' tik', &gcirc;i sciis, kion sonis la
        <tld/>ilo, sed oni asertis, ke &gcirc;i iras mal&gcirc;uste
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;parmonujo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;ip<tld/>ilo jam vokis la va&ccirc;ojn
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 23</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ilo denove eksonoris por memorigi la lernantojn, ke la interleciona
        pa&ubreve;zo fini&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2005/008708.php">Patrino</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lifta alarm<tld/>ilo ne funkciis, kaj dum duonhoro neniu rimarkis ilian
        foreston
        <fnt>
          <bib>ChR</bib>
          <lok>Tri estas tro</lok>
        </fnt>;
      </ekz>
      <ekz>
        li premis la butonon de la <tld/>ilo precize la&ubreve; la instrukcio, palviza&gcirc;a juna
        kontoristino aperis
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ilo vokas al pre&gcirc;ejo kaj mem neniam eniras
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klosx.0o">klo&scirc;o</ref>
    </refgrp>
