<drv mrk="sonor.0a">
  <kap><tld/>a</kap>
  <snc mrk="sonor.0a.SON">
    <dif>
      Klare kaj da&ubreve;re sonanta, facile a&ubreve;debla:
      <ekz>
        malnobla vipero! respondis la Greko per <tld/>a vo&ccirc;o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        super la harpo de la Islanda kantisto glitadis la ru&gcirc;a <tld/>a beko de la birdo
        <fnt>
          <bib>Fab3</bib>
          <lok>Birdo Fenikso</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i eksplodis per la&ubreve;ta, longa, <tld/>a gamo de rido
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en kelka distanco eksonis la <tld/>aj tonoj de kukolo
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li a&ubreve;dis la bruon de du vo&ccirc;oj, tiun de la grafino kaj alian, viran, basan,
        <tld/>an, kiu povis aparteni nur al viro
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deksepa</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lauxt.0a">la&ubreve;ta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_v;&c_o;&c_n;&c_k;&c_ib;</trd>,
    <trd>&c_g;&c_u;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_n;&c_o;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">zvoniv&yacute;</trd>
  <trdgrp lng="de">
    <trd>sonor</trd>,
    <trd>voll <klr>(Stimme)</klr></trd>,
    <trd>vollt&ouml;nend</trd>,
    <trd>wohlklingend</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sonorous</trd>,
    <trd>ringing</trd>
  </trdgrp>
  <trd lng="fr">sonore</trd>
  <trdgrp lng="ja">
    <trd>&#x97FF;&#x304D;&#x306E;&#x3042;&#x308B;  <pr>&#x3072;&#x3073;&#x304D;&#x306E;&#x3042;&#x308B;</pr></trd>,
    <trd>&#x3088;&#x304F;&#x97FF;&#x304F;  <pr>&#x3088;&#x304F;&#x3072;&#x3073;&#x304F;</pr></trd>,
    <trd>&#x6717;&#x6717;&#x3068;&#x3057;&#x305F;  <pr>&#x308D;&#x3046;&#x308D;&#x3046;&#x3068;&#x3057;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_v;&c_o;&c_n;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_v;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zvoniv&yacute;</trd>,
    <trd>zvu&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_z;&c_v;&c_ib;&c_n;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_n;&c_o;&c_r;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x597D;&#x542C; <pr>h&#x1CE;ot&#x12B;ng</pr></trd>,
    <trd>&#x597D;&#x807D; <pr>h&#x1CE;ot&#x12B;ng</pr></trd>,
    <trd>&#x6709;&#x58F0; <pr>y&#x1D2;ush&#x113;ng</pr></trd>,
    <trd>&#x6709;&#x8072; <pr>y&#x1D2;ush&#x113;ng</pr></trd>,
    <trd>&#x6D2A;&#x4EAE; <pr>h&#xF3;ngli&#xE0;ng</pr></trd>,
