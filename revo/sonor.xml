<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sonor.xml,v 1.70 2021/06/18 18:20:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sonor</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sonor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sonor.0i.SON">
    <dif>
      <ref tip="super" cel="son.0i.SON">Soni</ref> kaj da&ubreve;re a&ubreve;di&gcirc;i post
      &ccirc;eso de la ka&ubreve;zo:
      <ekz>
        &ccirc;iuj <tld/>iloj de pre&gcirc;ejoj <tld/>is
        <fnt><bib>Fab1</bib>, <lok>la virineto de maro</lok></fnt>;
      </ekz>
      <ekz>
        en la marmoraj palacoj <tld/>as luksaj festenoj
        <fnt>
          <aut>E. Orzeszkowa, tr. Kabe</aut>: <vrk>Legendo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi faros <klr>...</klr> tian faron, ke al &ccirc;iu, kiu tion a&ubreve;dos,
        ek<tld/>os amba&ubreve; liaj oreloj
        <fnt><bib>MT</bib>, <lok>&Sa1; 3:11</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tint.0i">tinti</ref>,
      <ref cel="vibr.0i">vibri</ref>
    </refgrp>
  </snc>
  <trd lng="af">ring</trd>
  <trd lng="am">&#x1240;&#x1208;&#x1260;&#x1275;</trd>
  <trd lng="ar">&#x639;&#x635;&#x627;&#x628;&#x629;</trd>
  <trd lng="bn">&#x9B0;&#x9BF;&#x982;</trd>
  <trd lng="bo">&#xF66;&#xF90;&#xF51;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF42;&#xF0B;</trd>
  <trd lng="co">anellu</trd>
  <trdgrp lng="cs">
    <trd>drn&ccaron;et</trd>,
    <trd>zvonit</trd>,
    <trd>zvu&ccaron;et</trd>
  </trdgrp>
  <trd lng="da">ring</trd>
  <trdgrp lng="de">
    <trd>t&ouml;nen</trd>,
    <trd>klingen</trd>,
    <trd>l&auml;uten</trd>,
    <trd>schallen</trd>,
    <trd>hallen</trd>
  </trdgrp>
  <trd lng="en">ring</trd>
  <trd lng="et">ring</trd>
  <trdgrp lng="fr">
    <trd>r&eacute;sonner</trd>,
    <trd>sonner</trd>,
    <trd>tinter</trd>
  </trdgrp>
  <trd lng="gl">anel</trd>
  <trd lng="gu">&#xAB0;&#xABF;&#xA82;&#xA97;</trd>
  <trd lng="ha">zobe</trd>
  <trd lng="hi">&#x905;&#x902;&#x917;&#x942;&#x920;&#x940;</trd>
  <trd lng="hr">prsten</trd>
  <trdgrp lng="hu">
    <trd>zeng</trd>,
    <trd>cseng</trd>,
    <trd>bong</trd>,
    <trd>z&uacute;g</trd>,
    <trd>b&uacute;g</trd>
  </trdgrp>
  <trd lng="hy">&#x574;&#x561;&#x57F;&#x561;&#x576;&#x56B;</trd>
  <trdgrp lng="id">
    <trd>ber<ind>dering</ind></trd>,
    <trd>ber<ind>dentang</ind></trd>
  </trdgrp>
  <trd lng="is">hringur</trd>
  <trd lng="ja">&#x30EA;&#x30F3;&#x30B0;</trd>
  <trd lng="ka">&#x10D1;&#x10D4;&#x10ED;&#x10D4;&#x10D3;&#x10D8;</trd>
  <trd lng="km">&#x1785;&#x17B7;&#x1789;&#x17D2;&#x1785;&#x17C0;&#x1793;</trd>
  <trd lng="ko">&#xB9C1;</trd>
  <trd lng="lo">&#xEA7;&#xEBB;&#xE87;</trd>
  <trd lng="ml">&#xD35;&#xD33;&#xD2F;&#xD02;</trd>
  <trd lng="mr">&#x905;&#x902;&#x917;&#x920;&#x940;</trd>
  <trd lng="my">&#x101C;&#x1000;&#x103A;&#x1005;&#x103D;&#x1015;&#x103A;</trd>
  <trd lng="ne">&#x918;&#x928;&#x94D;&#x91F;&#x940;</trd>
  <trdgrp lng="nl">
    <trd>galmen</trd>,
    <trd>luiden</trd>,
    <trd>klinken</trd>
  </trdgrp>
  <trd lng="ny">mphete</trd>
  <trd lng="pa">&#xA30;&#xA3F;&#xA70;&#xA17;</trd>
  <trd lng="ps">&#x62D;&#x644;&#x642;&#x648;&#x64A;</trd>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sd">&#x645;&#x646;&#x68A;&#x64A;</trd>
  <trd lng="si">&#xDB8;&#xDD4;&#xDAF;&#xDCA;&#xDAF;</trd>
  <trdgrp lng="sk">
    <trd>znie&tcaron;</trd>,
    <trd>zvoni&tcaron;</trd>
  </trdgrp>
  <trd lng="sq">unaz&#xEB;</trd>
  <trd lng="ta">&#xBAE;&#xBCB;&#xBA4;&#xBBF;&#xBB0;&#xBA4;&#xBCD;&#xBA4;&#xBC8;</trd>
  <trd lng="te">&#xC30;&#xC3F;&#xC02;&#xC17;&#xC4D;</trd>
  <trd lng="th">&#xE41;&#xE2B;&#xE27;&#xE19;</trd>
  <trd lng="tl">singsing</trd>
  <trd lng="tp">kalama</trd>
  <trd lng="ur">&#x627;&#x646;&#x6AF;&#x648;&#x679;&#x6CC;</trd>
  <trd lng="vi">v&#xF2;ng</trd>
  <trd lng="yi">&#x5E8;&#x5D9;&#x5E0;&#x5D2;</trd>
  <trd lng="zu">nkeneza</trd>
</drv>

<drv mrk="sonor.0a">
  <kap><tld/>a</kap>
  <snc mrk="sonor.0a.SON">
    <dif>
      Klare kaj da&ubreve;re sonanta, facile a&ubreve;debla:
      <ekz>
        <tld/>a vo&ccirc;o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lauxt.0a">la&ubreve;ta</ref>
    </refgrp>
  </snc>
  <trd lng="cs">zvoniv&yacute;</trd>
  <trd lng="fr">sonore</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_v;&c_o;&c_n;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_v;&c_u;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>zvoniv&yacute;</trd>,
    <trd>zvu&ccaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="sonor.0o">
  <kap><tld/>o</kap>
  <snc mrk="sonor.0o.SON">
    <dif>
      <ref tip="lst" cel="son.0o.SON" lst="voko:sonoj">Sono</ref> da&ubreve;re
      sonanta post &ccirc;eso de &gcirc;ia ka&ubreve;zo:
      <ekz>
        la vitro dispeci&gcirc;is kun <tld/>o
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>&eacute;clat sonore</trd>,
      <trd>sonorit&eacute;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>zeng&eacute;s</trd>,
      <trd>cseng&eacute;s</trd>,
      <trd>z&uacute;g&aacute;s</trd>,
      <trd>b&uacute;g&aacute;s</trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_v;&c_o;&c_n;</trd>
  </snc>
  <snc mrk="sonor.0o.FON">
    <uzo tip="fak">FON</uzo>
    <fnt>PV</fnt>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="sonor.0anto.FON"><tld/>anto</ref>
  </snc>
  <trd lng="bo">&#xF66;&#xF92;&#xFB2;&#xF0B;</trd>
  <trd lng="cs">zvon&ecaron;n&iacute;</trd>
  <trd lng="sk">zvonenie</trd>
</drv>

<drv mrk="sonor.0anto">
  <kap><tld/>anto</kap>
  <snc mrk="sonor.0anto.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <ref cel="son.parol0o">Parolsono</ref>, estigata precipe per la
      vo&ccirc;o, sen bruo a&ubreve; kun minimuma bruo
      (<refgrp>
        <ref cel="vokal.0o">vokalo</ref>,
        <ref cel="vokal.kvazaux0o">duonvokalo</ref>,
        <ref cel="naz.0akonsonanto">naza konsonanto</ref>,
        <ref cel="flu.0akonsonanto">flua konsonanto</ref>
      </refgrp>):
      <ekz>
        esence tio signifas, ke sono estas <tld/>anto, se &gcirc;i
        produkteblas da&ubreve;re je la sama tonalto
        <fnt>
          <bib>Vivi</bib>, <lok><url ref="https://eo.wikipedia.org/wiki/Sonoranto">Sonoranto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="bru.0anto.FON">bruanto</ref>
    <trd lng="de">Sonorant</trd>
    <trd lng="en">sonorant</trd>
    <trd lng="hu">z&ouml;nge</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_o;&c_n;&c_a;&c_n;&c_t;</trd>,
      <trd>&c_s;&c_o;&c_n;&c_o;&c_r;&c_n;&c_y;&c_j; &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="sonor.0etigi">
  <kap><tld/>etigi</kap>
  <snc mrk="sonor.0etigi.toste">
    <uzo tip="stl">RAR</uzo>
    <dif>
      Interfrapi la glasojn anta&ubreve; ol trinki je honoro de iu a&ubreve; io.
    </dif>
    <rim>
      Ekzemplo ne troveblas.
      Prefere uzu
      <ref tip="vid" cel="tint.0igi">tintigi</ref>,
      <ref tip="vid" cel="tost.0i">tosti</ref>.      
    </rim>
  </snc>
  <trd lng="hu">koccint</trd>
  <trd lng="nl">klinken</trd>
  <trd lng="ru">&c_ch;&c_o;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja; <klr tip="amb">(&c_b;&c_o;&c_k;&c_a;&c_l;&c_a;&c_m;&c_i;)</klr></trd>
</drv>

<drv mrk="sonor.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="sonor.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on per tiucela ilo:
      <ekz>
        bonvolu <tld/>igi al la servisto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>rozezn&iacute;t</trd>,
    <trd>zazvonit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>t&ouml;nen lassen</trd>,
    <trd>klingen lassen</trd>,
    <trd>l&auml;uten lassen</trd>
  </trdgrp>
  <trd lng="fr">sonner<klr tip="amb"> (sonnette, cloche,
  etc.)</klr></trd>
  <trdgrp lng="hu">
    <trd>csenget</trd>,
    <trd>harangozik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bellen</trd>,
    <trd>aanbellen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_v;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zazvoni&tcaron;</trd>
</drv>

<drv mrk="sonor.0igisto">
  <kap><tld/>igisto</kap>
  <snc mrk="sonor.0igisto.kirka">
    <dif>
      Homo, metie <tld/>iganta la <tld/>ilojn de pre&gcirc;ejo.
    </dif>
  </snc>
  <trd lng="cs">zvon&iacute;k</trd>
  <trd lng="de">Gl&ouml;ckner</trd>
  <trd lng="fr">sonneur<klr tip="amb"> (de cloches)</klr></trd>
  <trd lng="hu">harangoz&oacute;</trd>
  <trd lng="nl">klokkenluider</trd>
  <trd lng="ru">&c_z;&c_v;&c_o;&c_n;&c_a;&c_r;&c_mol;</trd>
  <trd lng="sk">zvon&aacute;r <klr>(ktor&yacute; zvon&iacute;)</klr></trd>
</drv>

<drv mrk="sonor.0ilaro">
  <kap><tld/>ilaro</kap>
  <snc mrk="sonor.0ilaro.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <ref tip="sin" cel="kariljon.0o">kariljono</ref>
  </snc>
  <trd lng="cs">zvonkohra</trd>
  <trd lng="fr">carillon</trd>
  <trdgrp lng="hu">
    <trd>harangj&aacute;t&eacute;k</trd>,
    <trd>campanelli</trd>
  </trdgrp>
  <trd lng="sk">zvonkohra</trd>
</drv>

<drv mrk="sonor.0ilego">
  <kap><tld/>ilego</kap>
  <snc mrk="sonor.0ilego.KOMUNE">
    <dif>
      Tre granda <tld/>ilo:
      <ekz>
        la <tld/>ilegoj komencis brue soni kaj ekestis 
        movi&gcirc;ado en la vila&gcirc;o
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">tocsin</trd>
</drv>

<drv mrk="sonor.0ilejo">
  <kap><tld/>ilejo, <var><kap><tld/>ilturo</kap></var></kap>
  <snc mrk="sonor.0ilejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Turo enhavanta <tld/>ilojn.
    </dif>
  </snc>
  <trd lng="cs">zvonice</trd>
  <trd lng="de">Glockenturm</trd>
  <trdgrp lng="fr">
    <trd>campanile</trd>,
    <trd>clocher</trd>
  </trdgrp>
  <trd lng="hu">harangtorony</trd>
  <trd lng="id"><ind>menara</ind> lonceng</trd>
  <trd lng="nl">klokkentoren</trd>
  <trd lng="ru">&c_z;&c_v;&c_o;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">zvonica</trd>
</drv>

<drv mrk="sonor.0ilo">
  <kap><ofc>*</ofc><tld/>ilo</kap>
  <snc mrk="sonor.0ilo.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.frap0o" lst="voko:frapinstrumentoj">Frapinstrumento</ref>
      estiganta <tld/>on, &gcirc;enerale konsistanta el metala kava duonsfero
      kun svingebla frapilo:
      <ekz>
        per <tld/>ilo ni <tld/>as, per fajfilo ni fajfas
        <fnt><bib>F</bib>, <lok>&FE; 34</lok></fnt>;
      </ekz>
      <ekz>
        la vila&gcirc;a <tld/>ilo malgaje eksonis noktomezon
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la forgesita
        pipo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">klok</trd>
  <trd lng="am">&#x12F0;&#x12C8;&#x120D;</trd>
  <trd lng="ar">&#x62C;&#x631;&#x633;</trd>
  <trd lng="az">z&#x259;ng</trd>
  <trd lng="bn">&#x998;&#x9A3;&#x9CD;&#x99F;&#x9BE;</trd>
  <trd lng="bo">&#xF51;&#xFB2;&#xF72;&#xF63;&#xF0B;&#xF56;&#xF74;&#xF0B;</trd>
  <trd lng="co">ghjuva</trd>
  <trd lng="cs">zvonek u dve&rcaron;&iacute;</trd>
  <trd lng="da">klokke</trd>
  <trdgrp lng="de">
    <trd>Glocke</trd>,
    <trd>Klingel</trd>
  </trdgrp>
  <trd lng="et">bell</trd>
  <trd lng="eu">kanpai</trd>
  <trd lng="fr">cloche</trd>
  <trd lng="fy">bel</trd>
  <trd lng="gl">camp&#xE1;</trd>
  <trd lng="gu">&#xA98;&#xA82;&#xA9F;&#xAA1;&#xAC0;</trd>
  <trd lng="ha">kararrawa</trd>
  <trd lng="hi">&#x92C;&#x947;&#x932;</trd>
  <trd lng="hr">zvono</trd>
  <trdgrp lng="hu">
    <trd>cseng&odblac;</trd>,
    <trd>harang</trd>
  </trdgrp>
  <trd lng="hy">&#x566;&#x561;&#x576;&#x563;</trd>
  <trdgrp lng="id">
    <trd>bel</trd>,
    <trd>lonceng</trd>
  </trdgrp>
  <trd lng="is">bjalla</trd>
  <trd lng="ja">&#x9418;</trd>
  <trd lng="jw">lonceng</trd>
  <trd lng="ka">&#x10D1;&#x10D4;&#x10DA;&#x10D8;</trd>
  <trd lng="km">&#x1780;&#x178E;&#x17D2;&#x178A;&#x17B9;&#x1784;</trd>
  <trd lng="ko">&#xBCA8;</trd>
  <trd lng="lo">&#xEA5;&#xEB0;&#xE84;&#xEB1;&#xE87;</trd>
  <trd lng="ml">&#xD2E;&#xD23;&#xD3F;</trd>
  <trd lng="mr">&#x918;&#x902;&#x91F;&#x93E;</trd>
  <trd lng="my">&#x1001;&#x1031;&#x102B;&#x1004;&#x103A;&#x1038;&#x101C;&#x1031;&#x102C;&#x1004;&#x103A;&#x1038;</trd>
  <trdgrp lng="nl">
    <trd>bel</trd>,
    <trd>klok</trd>
  </trdgrp>
  <trd lng="ny">belu</trd>
  <trd lng="pa">&#xA18;&#xA70;&#xA1F;&#xA40;</trd>
  <trd lng="ps">&#x632;&#x646;&#x6AB;</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="sd">&#x628;&#x64A;&#x644;</trd>
  <trd lng="si">&#xDC3;&#xDD3;&#xDB1;&#xDD4;&#xDC0;</trd>
  <trd lng="sk">zvon&ccaron;ek <klr>(domov&yacute;)</klr></trd>
  <trd lng="sq">zile</trd>
  <trd lng="ta">&#xBAE;&#xBA3;&#xBBF;</trd>
  <trd lng="te">&#xC2C;&#xC46;&#xC32;&#xC4D;</trd>
  <trd lng="th">&#xE23;&#xE30;&#xE06;&#xE31;&#xE07;</trd>
  <trd lng="tl">kampanilya</trd>
  <trd lng="ur">&#x6AF;&#x6BE;&#x646;&#x679;&#x6CC;</trd>
  <trd lng="vi">chu&#xF4;ng</trd>
  <trd lng="yi">&#x5D2;&#x5DC;&#x5D0;&#x5B8;&#x5E7;</trd>
  <trd lng="zu">insimbi</trd>
</drv>
</art>
