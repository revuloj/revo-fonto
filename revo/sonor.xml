<drv mrk="sonor.0ilego">
  <kap><tld/>ilego</kap>
  <snc mrk="sonor.0ilego.KOMUNE">
    <dif>
      Tre granda <tld/>ilo:
      <ekz>
        la <tld/>ilegoj komencis brue soni kaj ekestis
        movi&gcirc;ado en la vila&gcirc;o
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep
        fratoj</vrk>, <lok>1947</lok></fnt>;
      </ekz>
      <ekz>
        de la pre&gcirc;ejturoj estas formetitaj la <tld/>ilegoj kaj de l&#x2019; kupoloj la orumitaj
        krucoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        pri la <tld/>ilego &scirc;i kredis, ke &gcirc;i estas eksperimento por veki la mortinton
        <fnt>
          <bib>VaK</bib>
          <lok>Sepa &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib; <ind>&c_z;&c_v;&c_o;&c_n;</ind></trd>
  <trdgrp lng="de">
    <trd>Glocke</trd>,
    <trd>Kirchenglocke</trd>
  </trdgrp>
  <trd lng="en">big bell</trd>
