    <trd>t&ouml;nen lassen</trd>,
    <trd>klingen lassen</trd>,
    <trd>l&auml;uten lassen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>give a ring</trd>,
    <trd>ring</trd>
  </trdgrp>
  <trd lng="fr">sonner<klr tip="amb"> (sonnette, cloche,
  etc.)</klr></trd>
  <trdgrp lng="hu">
    <trd>csenget</trd>,
    <trd>harangozik</trd>
  </trdgrp>
  <trd lng="ja">&#x9CF4;&#x3089;&#x3059;  <pr>&#x306A;&#x3089;&#x3059;</pr></trd>
  <trdgrp lng="nl">
    <trd>bellen</trd>,
    <trd>aanbellen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_v;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
