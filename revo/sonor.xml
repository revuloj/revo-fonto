<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sonor</rad>/i
</kap>
<drv mrk="sonor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <uzo tip="klr">(pp ilo)</uzo>
    <dif>
      Produkti sonon da&ubreve;re a&ubreve;di&gcirc;antan post &ccirc;eso de &gcirc;ia ka&ubreve;zo:
      <ekz>
        &ccirc;iuj <tld/>iloj de pre&gcirc;ejoj <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en la palacoj <tld/>as luksaj festenoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>a (facile &gcirc;is malproksime a&ubreve;debla) vo&ccirc;o;
      </ekz>
      <ekz>
        la entreprenon anta&ubreve; &ccirc;io pereigis <tld/>aj (f: monaj) ka&ubreve;zoj kaj
        la elseki&gcirc;o de materialaj fontoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi faros tian faron, ke al &ccirc;iu, kiu tion a&ubreve;dos, ek<tld/>os amba&ubreve;
        liaj oreloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="son.0i">soni</ref>,
      <ref cel="tint.0i">tinti</ref>,
      <ref cel="vibr.0i">vibri</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>t&ouml;nen</trd>,
    <trd>klingen</trd>,
    <trd>l&auml;uten</trd>,
    <trd>schallen</trd>,
    <trd>hallen</trd>
  </trdgrp>
  <trd lng="en">ring</trd>
  <trdgrp lng="hu">
    <trd>zeng</trd>,
    <trd>cseng</trd>,
    <trd>bong</trd>,
    <trd>z&uacute;g</trd>,
    <trd>b&uacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>galmen</trd>,
    <trd>luiden</trd>,
    <trd>klinken</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>
</drv>

<drv mrk="sonor.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sono da&ubreve;re sonanta post &ccirc;eso de &gcirc;ia ka&ubreve;zo.
    </dif>
    <trdgrp lng="hu">
      <trd>zeng&eacute;s</trd>,
      <trd>cseng&eacute;s</trd>,
      <trd>z&uacute;g&aacute;s</trd>,
      <trd>b&uacute;g&aacute;s</trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_v;&c_o;&c_n;</trd>
  </snc>
  <snc>
    <uzo tip="fak">FON</uzo>
    <dif>
      Regule vibranta sono, havanta diferencigeblan altecon
    </dif>
    <refgrp tip="sin">
      <ref cel="ton.0o">tono</ref>,
      <ref cel="bru.0ero">bruero</ref>,
      <ref cel="vokal.0o">vokalo</ref>.
    </refgrp>
    <trd lng="hu">z&ouml;nge</trd>    
  </snc>
</drv>

<drv mrk="sonor.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <uzo tip="klr">(pp homo estiganta <tld/>on per ilo)</uzo>
    <dif>
      Ka&ubreve;zi <tld/>on:
      <ekz>
        bonvolu <tld/>igi al la servisto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>t&ouml;nen lassen</trd>,
    <trd>klingen lassen</trd>,
    <trd>l&auml;uten lassen</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csenget</trd>,
    <trd>harangozik</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bellen</trd>,
    <trd>aanbellen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_v;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sonor.0igisto">
  <kap><tld/>igisto</kap>
  <snc>
    <dif>
      Homo, metie <tld/>iganta la <tld/>ilojn de pre&gcirc;ejo.
    </dif>
  </snc>
  <trd lng="de">Gl&ouml;ckner</trd>
  <trd lng="hu">harangoz&oacute;</trd>  
  <trd lng="nl">klokkenluider</trd>
  <trd lng="ru">&c_z;&c_v;&c_o;&c_n;&c_a;&c_r;&c_mol;</trd>
</drv>

<drv mrk="sonor.0ilaro">
  <kap><tld/>ilaro</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="sin" cel="kariljon.0o">Kariljono</ref>
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>harangj&aacute;t&eacute;k</trd>,
    <trd>campanelli</trd>
  </trdgrp>
</drv>

<drv mrk="sonor.0etigi">
  <kap><tld/>etigi</kap>
  <snc>
    <uzo tip="klr">(pp glasoj)</uzo>
    <dif>
      Interfrapi la glasojn anta&ubreve; ol trinki je honoro de iu a&ubreve; io.
    </dif>
  </snc>
  <trd lng="hu">koccint</trd>  
  <trd lng="nl">klinken</trd>
  <trd lng="ru">&c_ch;&c_o;&c_k;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja; <klr tip="amb">(&c_b;&c_o;&c_k;&c_a;&c_l;&c_a;&c_m;&c_i;)</klr></trd>
</drv>

<drv mrk="sonor.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Ilo por estigi <tld/>on, &gcirc;enerale konsistanta el metala kava
      duonsfero kun svingebla frapilo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Glocke</trd>,
    <trd>Klingel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>cseng&odblac;</trd>,
    <trd>harang</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bel</trd>,
    <trd>klok</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="sonor..0ilejo0ilturo">
  <kap><tld/>ilejo, <tld/>ilturo</kap>
  <snc>
    <dif>
      Turo enhavanta <tld/>ilojn.
    </dif>
  </snc>
  <trd lng="de">Glockenturm</trd>
  <trd lng="hu">harangtorony</trd>  
  <trd lng="nl">klokkentoren</trd>
  <trd lng="ru">&c_z;&c_v;&c_o;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
