<drv mrk="situac.0a">
  <kap><tld/>a</kap>
  <snc mrk="situac.0a.rilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la nun ofti&gcirc;anta <tld/>a instruado
        <fnt>
          <aut>Blazio Vaha</aut>:
          <vrk><url ref="http://www.ilei.info/ipr/el_niaj_metiejoj_95_3.htm">Fantazie kaj
          didakte</url></vrk>.
          <lok>Internacia Pedagogia Revuo, 1995:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a <klr>[&#x2026;]</klr> komiko
        <fnt>
          <bib>Monato</bib>
          <aut>Luiza Carol</aut>
          <vrk><url ref="&Monato;/2013/010892.php">Spegula mondo</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz><ekz>
        <ind><tld/>a komedio</ind>.
        <trd lng="ru">&c_k;&c_o;&c_m;&c_je;&c_d;&c_i;&c_ja; &c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_j;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_t;&c_u;&c_a;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
  <trd lng="cs">situativn&iacute;</trd>
  <trd lng="de">situativ</trd>
  <trd lng="pl">sytuacyjny</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_t;&c_u;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_i;&c_t;&c_u;&c_a;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>polohopisn&yacute;</trd>,
    <trd>polohov&yacute;</trd>,
    <trd>situa&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8FD9;&#x8981;&#x770B;&#x60C5;&#x51B5; <pr>zh&#xE8;y&#xE0;ok&#xE0;nq&#xED;ngku&#xE0;ng</pr></trd>,
    <trd>&#x9019;&#x8981;&#x770B;&#x60C5;&#x6CC1; <pr>zh&#xE8;y&#xE0;ok&#xE0;nq&#xED;ngku&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="situac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="situac.0i.esti">
    <dif>
      Esti en iu <tld/>o:
      <ekz>
        kiel ni tuj vidos, ne <tld/>as tiel
        <fnt>
          <vrk><url ref="https://jorgecice.blogspot.com/2013/09/vortoj-en-vortaroj.html">la blogo de
          Jorge: Vortoj en vortaroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="stat.0i">stati</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_ib;&c_sh;&c_ch;&c_y;</ind></trd>,
    <trd>&c_z;&c_n;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_c;&c_a; &c_w; <ind>&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_ib;&c_sh;&c_ch;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>situar-se</trd>,
    <trd>trobar-se</trd>
  </trdgrp>
  <trd lng="cs">b&yacute;t v postaven&iacute;</trd>
  <trd lng="de">stehen <klr>(Angelegenheit)</klr></trd>
  <trdgrp lng="hu">
    <trd><klr tip="amb">(vmilyen)</klr> helyzetben van</trd>,
    <trd><klr tip="amb">(vmilyen)</klr> poz&iacute;ci&oacute;ban van</trd>
    <trd>fekszik <klr tip="amb">(f&ouml;ldrajzilag)</klr> </trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>znale&zacute;&cacute; si&eogonek; w  <ind>po&lstroke;o&zdot;eniu</ind></trd>,
    <trd>znale&zacute;&cacute; si&eogonek; w  <ind>sytuacji</ind></trd>
  </trdgrp>
  <trd lng="sk">by&tcaron; v postaven&iacute;</trd>
</drv>
