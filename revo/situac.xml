<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>situaci</rad>/o
    </kap>
    <drv mrk="situac.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="situac.0o.cirkonstancaro">
        <dif>
          Tuto da kondi&ccirc;oj kaj cirkonstancoj, en kiuj iu trovi&gcirc;as:
          <ekz>
            brilanta <tld/>o<fnt>B</fnt>;
          </ekz><ekz>
            komika <tld/>o<fnt>B</fnt>;
          </ekz><ekz>
            veni en dan&gcirc;eran <tld/>on;
          </ekz><ekz>
            adapti&gcirc;i al nova <tld/>o;
          </ekz><ekz>
            fali en ankora&ubreve; pli malbonan <tld/>on
            (<refgrp tip="vid">
              <ref cel="pluv.0o">pluvo</ref>,
              <ref cel="flam.0o">flamo</ref>,
              <ref cel="sitel.0o">sitelo</ref>
            </refgrp>);
          </ekz><ekz>
            mastri, savi la <tld/>on.
          </ekz>
        </dif>
        <trdgrp lng="hu">
          <trd>helyzet</trd>,
          <trd>szitu&aacute;ci&oacute;</trd>
        </trdgrp>
        <trd lng="ru">&c_s;&c_i;&c_t;&c_u;&c_a;&c_c;&c_i;&c_ja;</trd>
      </snc>
      <snc mrk="situac.0o.rango">
        <dif>
          Ies pozicio en la socia skalo.
        </dif>
        <refgrp tip="sub">
          <ref cel="posten.0o">posteno</ref>,
          <ref cel="pozici.0o">pozicio</ref>,
          <ref cel="ofic.0o">ofico</ref>,
          <ref cel="rang.0o">rango</ref>,
          <ref cel="stat.0o.rango">stato</ref>
        </refgrp>
        <trd lng="hu"><klr>t&aacute;rsadalmi </klr>helyzet</trd>        
        <trd lng="ru">&c_o;&c_b;&c_shch;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je; <ind>&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</ind></trd>
      </snc>
      <snc mrk="situac.0o.situo">
        <dif>
          <ref tip="dif" cel="situ.0o">Situo</ref>..
        </dif>
        <trd lng="de">Lage</trd>
        <trd lng="hu">fekv&eacute;s</trd>        
        <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
      </snc>
      <trdgrp lng="be">
        <trd>&c_s;&c_y;&c_t;&c_u;&c_a;&c_c;&c_y;&c_ja;</trd>,
        <trd>&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_ib;&c_sh;&c_ch;&c_a;</trd>
      </trdgrp>
      <trd lng="de">Situation</trd>
      <trd lng="en">situation</trd>
      <trdgrp lng="nl">
        <trd>situatie</trd>,
        <trd>toestand</trd>
      </trdgrp>
    </drv>
    <drv mrk="situac.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Esti en iu <tld/>o.
        </dif>
      </snc>
      <trdgrp lng="hu">
        <trd><klr tip="amb">(vmilyen)</klr> helyzetben van</trd>,
        <trd><klr tip="amb">(vmilyen)</klr> poz&iacute;ci&oacute;ban van</trd>
        <trd>fekszik <klr tip="amb">(f&ouml;ldrajzilag)</klr> </trd>
      </trdgrp>
      <adm>
        &Ccirc;u temas pri &leftquot;situi&rightquot;?
        <aut>Sergio</aut>
      </adm>
    </drv>
  </art>
