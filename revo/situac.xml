    <trd>&#x60C5;&#x52E2;  <pr>&#x3058;&#x3087;&#x3046;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x4F4D;&#x7F6E;  <pr>&#x3044;&#x3061;</pr></trd>,
    <trd>&#x7ACB;&#x5834;  <pr>&#x305F;&#x3066;&#x3070;</pr></trd>,
    <trd>&#x72B6;&#x6CC1;  <pr>&#x3058;&#x3087;&#x3046;&#x304D;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x5834;&#x9762;  <pr>&#x3070;&#x3081;&#x3093;</pr></trd>,
    <trd>&#x30B7;&#x30C1;&#x30E5;&#x30A8;&#x30FC;&#x30B7;&#x30E7;&#x30F3;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>situatie</trd>,
    <trd>toestand</trd>
  </trdgrp>
  <trd lng="fr">situation</trd>
  <trdgrp lng="pl">
    <trd>po&lstroke;o&zdot;enie</trd>,
    <trd>sytuacja</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>poloha</trd>,
    <trd>polo&zcaron;enie</trd>,
    <trd>situ&aacute;cia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&c_t;&c_u;&c_a;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&c_s;&c_t;&c_a;&#x43D;&#x43E;&c_v;&c_i;&c_shch;&c_je;</trd>,
    <trd>&#x43C;&c_ib;&c_s;&c_c;&c_je;&#x43F;&#x43E;&#x43B;&#x43E;&c_zh;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43C;&c_ib;&c_s;&c_c;&c_je; &c_r;&#x43E;&c_z;&c_t;&c_a;&c_sh;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43E;&c_b;&c_s;&c_t;&c_a;&#x43D;&#x43E;&c_v;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5C40;&#x9762; <pr>j&#xFA;mi&#xE0;n</pr></trd>,
    <trd>&#x6001;&#x52BF; <pr>t&#xE0;ish&#xEC;</pr></trd>,
