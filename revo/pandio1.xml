<vortaro>
<art mrk="$Id: pandio1.xml,v 1.13 2023/04/19 06:32:24 revo Exp $">
<kap>
  <rad>pandioned</rad>/oj
</kap>
<drv mrk="pandio1.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="pandio1.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Familio el la ordo <ref tip="super"
      cel="bird.rabo0oj">rabobirdoj</ref>
      (<trd lng="la">Pandionidae</trd>):
      <ekz>
        <tld/>oj <klr>[&#8230;]</klr> havas nur unu genro<klr>[n]</klr> kaj nur unu specion,
        la <ref tip="prt" cel="agl.fisx0o">fi&scirc;aglon</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16258">Pandionedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_p;&c_ib;&c_n;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="cs">orlovcovit&iacute;</trd>
  <trd lng="de">Fischadler<klr tip="ind"> (Familie)</klr></trd>
  <trdgrp lng="fr">
    <trd>aigles p&ecirc;cheurs</trd>,
    <trd>balbuzards</trd>
  </trdgrp>
  <trd lng="hu">r&aacute;r&oacute;f&eacute;l&eacute;k</trd>
  <trd lng="nl">visarenden<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">rybo&lstroke;owy</trd>
  <trd lng="pt">pandion&iacute;deos</trd>
  <trd lng="ro">pescuitul</trd>
  <trd lng="sk">r&scaron;iakorod&eacute;</trd>
</drv>
</art>
