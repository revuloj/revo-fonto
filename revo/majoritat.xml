<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: majoritat.xml,v 1.17 2024/01/25 20:10:33 revo Exp $">
<kap>
  <rad>majoritat</rad>/o <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="&jcirc;">ma&jcirc;ori</rad>/o</kap></var>
</kap>
<drv mrk="majoritat.0o">
  <kap><tld/>o, <var><kap><tld var="&jcirc;"/>o</kap></var></kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="majoritat.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="mult.pli0o">Plimulto</ref>:
      <ekz>
        la <tld/>o de la familioj ne volas sendi siajn infanojn al 
        lernejo anta&ubreve; la sepa jaro
        <fnt>
          <aut>R. Pyh&auml;l&auml;</aut>,
          <vrk><url ref="http://ilei.info/ipr/eduksistemo_en_fi.htm">
          La Finnlanda Eduksistemo</url></vrk>,
          <lok>Internacia Pedagogia Revuo, 1995:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio, kion mi volas priparoli, estas ja &gcirc;uste tio, ke la amaso, 
        la <tld/>o, tiu damnita, kompakta plimulto &#x2010; ke estas tiu, mi 
        diras, kiu venenas niajn spiritajn vivofontojn
        <fnt>
          <aut>H. Ibsen, trad. O. Tangerud</aut>,
          <vrk><url ref="&Gutenberg;files/19803/19803-0.txt">
          Popolmalamiko</url></vrk>,
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a vo&ccirc;donado en lokaj konsilantaroj
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007075.php">Pa&scirc;oj al purigado</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 8</lok>
          </fnt>;
      </ekz>
      <ekz>
        la <tld var="&jcirc;"/>o parolis la rumanan
        <fnt>
          <vrk><url ref="https://groups.google.com/d/topic/soc.culture.esperanto/xJhGQmAciWU">Death
          of Esperanto - Google Groups</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &leftquot;eskimo&rightquot; la&ubreve; la hodia&ubreve;a <tld var="&jcirc;"/>a opinio de lingvistoj venas el la lingvo de
        najbaraj Kri- kaj Algonkin-indianoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29124">Eskimoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="ant" cel="minoritat.0o">minoritato</ref>
  <trd lng="be">&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="bg">
    <trd>&c_m;&c_n;&c_o;&c_z;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_sh;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="de">Mehrheit</trd>
  <trd lng="en">majority</trd>
  <trd lng="es">mayor&iacute;a</trd>
  <trd lng="fa">&a_A;&f_k;&a_th;&a_r;&f_y;&a_t;</trd>
  <trd lng="fr">majorit&eacute;</trd>
  <trd lng="he">&resh;&vav;&bet;</trd>
  <trd lng="hu">t&ouml;bbs&eacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x591A;&#x6570;&#x6D3E;  <pr>&#x305F;&#x3059;&#x3046;&#x306F;</pr></trd>,
    <trd>&#x30DE;&#x30B8;&#x30E7;&#x30EA;&#x30C6;&#x30A3;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">meerderheid<klr tip="ind"> (ontr.)</klr></trd>
  <trd lng="pl">wi&eogonek;kszo&sacute;&cacute;</trd>
  <trd lng="pt">muito<klr> (quantidade)</klr></trd>
  <trd lng="ru">&c_b;&c_o;&c_l;&c_mol;&c_sh;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>majorita</trd>,
    <trd>v&auml;&ccaron;&scaron;ina</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>majorita</trd>,
    <trd>v&ecaron;t&scaron;ina</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_v;&#x43D;&#x43E;&#x43B;&c_ib;&c_t;&c_t;&#x44F;</trd>,
    <trd>&c_b;&c_ib;&#x43B;&#x44C;&c_sh;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5927;&#x591A;&#x6570; <pr>d&#xE0;du&#x14D;sh&#xF9;</pr></trd>,
    <trd>&#x5927;&#x591A;&#x6578; <pr>d&#xE0;du&#x14D;sh&#xF9;</pr></trd>,
    <trd>&#x591A;&#x6570; <pr>du&#x14D;sh&#xF9;</pr></trd>,
    <trd>&#x591A;&#x6578; <pr>du&#x14D;sh&#xF9;</pr></trd>,
    <trd>&#x5927;&#x90E8;&#x4EFD; <pr>d&#xE0;b&#xF9;fen</pr></trd>,
    <trd>&#x5927;&#x90E8;&#x5206; <pr>d&#xE0;b&#xF9;fen</pr></trd>
  </trdgrp>
</drv>
</art>

