<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>jonkvil</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="jonkvi.0o">
  <kap><tld/>o</kap>
  <snc mrk="jonkvi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
     <ref tip="lst" cel="speci.0o.BIO" 
     lst="voko:botanikaj_specioj">Specio</ref> 
     de <ref tip="super" cel="narcis.0o">narciso</ref>
     (<trd lng="la">Narcissus jonquilla</trd>),
     <ref tip="lst" cel="plant.floro0o" 
     lst="voko:asparagoparencoj">floroplanto</ref> kun flava floro kaj
     granda korolo:
      <ekz>
        la <tld/>o estas svelta narciso, kiu povas atingi alton &gcirc;is 40 centimetroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=419399">Jonkvilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">&zcaron;onkyla</trd>
  <trd lng="de">Jonquille</trd>
  <trd lng="en">jonquil</trd>
  <trd lng="fr">jonquille</trd>
  <trd lng="hu">jonquilla n&aacute;rcisz</trd>
  <trd lng="it">giunchiglia</trd>
  <trd lng="nl">jonquilla-narcis</trd>
  <trd lng="pt">junquilho</trd>
  <trd lng="ru">&c_zh;&c_o;&c_n;&c_k;&c_i;&c_l;&c_mol;</trd>
  <trd lng="sk">druh narcisu</trd>
  <trd lng="sv">jonkvill</trd>
</drv>
</art>
