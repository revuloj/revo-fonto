<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jonkvi.xml,v 1.24 2023/12/03 07:09:18 revo Exp $">
<kap>
  <rad>jonkvil</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="jonkvi.0o">
  <kap><tld/>o</kap>
  <snc mrk="jonkvi.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
     <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref> 
     de <ref tip="super" cel="narcis.0o">narciso</ref>
     (<trd lng="la">Narcissus jonquilla</trd>),
     <ref tip="lst" cel="plant.floro0o" lst="voko:asparagoparencoj">floroplanto</ref> kun flava floro kaj
     granda korolo:
      <ekz>
        la <tld/>o estas svelta narciso, kiu povas atingi alton &gcirc;is 40 centimetroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=419399">Jonkvilo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        volontuloj disdonas al preterpasantoj paperajn <tld/>ojn &ndash; la florojn, kiujn
        <nom>Marek Edelman</nom> <klr>[&#x2026;]</klr> metis sur memorlokoj ligitaj kun ekstermo de
        judoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=571764">Muzeo pri Historio de Polaj Judoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/00/Narcissus_jonquilla_002_GotBot_2016.jpg/320px-Narcissus_jonquilla_002_GotBot_2016.jpg" prm="CC BY 4.0"><tld/>oj en la botanika &gcirc;ardeno de Goteburgo
        <fnt>
          <aut>Averater, propra verko</aut>
          <lok><url ref="&WCW;/File:Narcissus_jonquilla_002_GotBot_2016.jpg">&WCW;/File:Narcissus_jonquilla_002_GotBot_2016.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_zh;&c_o;&c_w;&c_t;&c_y; <ind>&c_n;&c_a;&c_r;&c_c;&c_y;&c_s;</ind></trd>
  <trd lng="cs">&zcaron;onkyla</trd>
  <trd lng="de">Jonquille</trd>
  <trd lng="en">jonquil</trd>
  <trd lng="fr">jonquille</trd>
  <trd lng="hu">jonquilla n&aacute;rcisz</trd>
  <trd lng="it">giunchiglia</trd>
  <trd lng="ja">&#x30AD;&#x30BA;&#x30A4;&#x30BB;&#x30F3;</trd>
  <trd lng="nl">jonquilla-narcis</trd>
  <trd lng="pt">junquilho</trd>
  <trd lng="ru">&c_zh;&c_o;&c_n;&c_k;&c_i;&c_l;&c_mol;</trd>
  <trd lng="sk">druh narcisu</trd>
  <trd lng="sv">jonkvill</trd>
  <trd lng="uk">&#x436;&#x43E;&#x43D;&#x43A;&#x456;&#x43B;&#x44C;</trd>
  <trd lng="zh">&#x6C34;&#x50CA; <pr>shu&#x1D0;xi&#x101;n</pr></trd>
</drv>
</art>
