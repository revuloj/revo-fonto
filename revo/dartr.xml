<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>dartr</rad>/o
  </kap>
  <drv mrk="dartr.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">MED</uzo>
    <snc>
      <dif>
        Nomo de kelkaj ha&ubreve;tmalsanoj, kiuj estas nekomunikeblaj,
        malrapide kuraceblaj, sencikatraj, facile reaperantaj,
        ofte heredaj kaj jukantaj.
      </dif>
    </snc>
      <trdgrp lng="de">
         <trd>Flechte</trd>,
         <trd>Schuppenflechte</trd>,
         <trd>Hautflechte</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>scurf</trd>,
        <trd>tetter</trd>
      </trdgrp>
      <trd lng="fr">dartre</trd>
      <trd lng="pt">dartro</trd>
      <trd lng="pl">liszaj</trd>
      <trd lng="ru">&c_l;&c_i;&c_sh;&c_a;&c_j;</trd>
  </drv>
</art>
