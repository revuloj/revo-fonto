<art mrk="$Id: dartr.xml,v 1.20 2022/03/15 15:50:54 revo Exp $">
<kap>
  <rad>dartr</rad>/o
</kap>
<drv mrk="dartr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Nomo de kelkaj ha&ubreve;t<ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>j, kiuj
      estas nekomunikeblaj,
      malrapide kuraceblaj, sencikatraj, facile reaperantaj,
      ofte heredaj kaj jukantaj:
      <ekz>
        unue &scirc;i suferis de koklu&scirc;o, kaj nun de <tld/>o, &ccirc;iam &scirc;i suferas
        de io
        <fnt>
          <aut>Mary Webb, trad. Grace Kirkwood</aut>
          <vrk><url ref="http://i-espero.info/files/elibroj/eo%20-%20webb,%20mary%20-%20veneno%20kara.pdf">Veneno
          Kara</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_h;&c_ib;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_u;&c_d;&c_y;</trd>,
    <trd>&c_k;&c_r;&c_u;&c_g;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_t;&c_a;</trd>,
    <trd>&c_je;&c_k;&c_z;&c_je;&c_m;&c_a;</trd>,
    <trd>&c_l;&c_i;&c_sh;&c_je;&c_i;</trd>
  </trdgrp>
  <trd lng="ca">borradura</trd>
  <trdgrp lng="cs">
    <trd>ekz&eacute;m</trd>,
    <trd>kop&rcaron;ivka</trd>,
    <trd>lichen</trd>,
    <trd>li&scaron;ej</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Schorf</trd>,
     <trd>Flechte</trd>,
     <trd>Schuppenflechte</trd>,
     <trd>Hautflechte</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>scurf</trd>,
    <trd>tetter</trd>
  </trdgrp><trd lng="en">schurft</trd>
  <trd lng="fr">dartre</trd>
  <trd lng="he">&#x5D7;&#x5D6;&#x5D6;&#x5D9;&#x5EA;</trd>
  <trd lng="hu">s&ouml;m&ouml;r</trd>
  <trdgrp lng="id">
    <trd>kudis</trd>,
    <trd>skabies</trd>
  </trdgrp>
  <trd lng="pl">liszaj</trd>
  <trd lng="pt">dartro</trd>
  <trd lng="ru">&c_l;&c_i;&c_sh;&c_a;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>ekz&eacute;m</trd>,
    <trd>li&scaron;aj</trd>
  </trdgrp>
  <trd lng="uk">&#x43B;&#x438;&#x448;&#x430;&#x439;</trd>
</drv>
</art>
