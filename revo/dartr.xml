<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>dartr</rad>/o
  </kap>
  <drv mrk="dartr.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">MED</uzo>
    <snc>
      <dif>
        Nomo de kelkaj ha&ubreve;t<ref tip="lst" cel="san.mal0o.MED"
        lst="voko:malsanoj">malsano</ref>j, kiuj estas nekomunikeblaj,
        malrapide kuraceblaj, sencikatraj, facile reaperantaj,
        ofte heredaj kaj jukantaj.
      </dif>
    </snc>
  <trdgrp lng="bg">
        <trd>&c_k;&c_r;&c_a;&c_s;&c_t;&c_a;</trd>,
        <trd>&c_je;&c_k;&c_z;&c_je;&c_m;&c_a;</trd>,
        <trd>&c_l;&c_i;&c_sh;&c_je;&c_i;</trd>
      </trdgrp>
  <trd lng="ca">borradura</trd>
  <trdgrp lng="cs">
    <trd>ekzém</trd>,
    <trd>kopřivka</trd>,
    <trd>lichen</trd>,
    <trd>lišej</trd>
  </trdgrp>
  <trdgrp lng="de">
         <trd>Schorf</trd>,
         <trd>Flechte</trd>,
         <trd>Schuppenflechte</trd>,
         <trd>Hautflechte</trd>
      </trdgrp>
  <trdgrp lng="en">
        <trd>scurf</trd>,
        <trd>tetter</trd>
      </trdgrp><trd lng="en">schurft</trd>
  <trd lng="fr">dartre</trd>
  <trdgrp lng="id">
        <trd>kudis</trd>,
        <trd>skabies</trd>
      </trdgrp>
  <trd lng="pl">liszaj</trd>
  <trd lng="pt">dartro</trd>
  <trd lng="ru">&c_l;&c_i;&c_sh;&c_a;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>ekzém</trd>,
    <trd>lišaj</trd>
  </trdgrp>
</drv>
</art>
