<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vultu.xml,v 1.7 2022/03/28 11:08:55 revo Exp $">
<kap>
  <rad>vultu</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vultu.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
  <uzo tip="fak">MED</uzo>
    <dif>
      Pufa, <ref tip="vid" cel="rugx.0igxi.vizagxe">ru&gcirc;i&gcirc;a</ref> pro
      <ref tip="vid" cel="konges.0o">kongesto</ref>, precipe en la viza&gcirc;o:
      <ekz>
        lia viza&gcirc;o, pufe <tld/>a, plenis de flamaj &scirc;vitgutoj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 57a, volumo 2a, p.
        258a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_ch;&c_y;&c_r;&c_v;&c_a;&c_n;&c_je;&c_l;&c_y; &c_ib; &c_a;&c_p;&c_u;&c_h;&c_l;&c_y;</trd>
  <trd lng="ca">vultu&oacute;s</trd>
  <trd lng="fr">vultueux</trd>
  <trdgrp lng="hu">
    <trd>v&eacute;rtolul&aacute;sos</trd>,
    <trd>puffadt</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nalany</trd>,
    <trd>napuchni&eogonek;ty</trd>,
    <trd>zapuchni&eogonek;ty</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>vultuoso</trd>
   </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x431;&#x430;&#x433;&#x440;&#x43E;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x43E;&#x43F;&#x443;&#x445;&#x43B;&#x438;&#x439; (&#x43F;&#x440;&#x43E; &#x43B;&#x438;&#x446;&#x435;)</trd>
  </trdgrp>
</drv>
</art>

