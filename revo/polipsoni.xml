<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
   <kap>
    <rad>polipsoni</rad>/o
   </kap>
   <drv mrk="polips.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">EKON</uzo>
    <snc>
    <dif>
      Merkata situacio, en kiu multaj klientoj regas la a&ccirc;etan
flankon.
    </dif>
    <refgrp tip="vid">
      <ref cel="polipol.0o">polipolo</ref>,
      <ref cel="monops.0o">monopsono</ref>,
      <ref cel="duops.0o">duopsonio</ref>,
      <ref cel="oligops.0o">oligopsonio</ref>.
    </refgrp>
    </snc>
    <trd lng="de">Polipson</trd>
    <trdgrp lng="en">
       <trd>polypsony</trd>,
       <trd>buyer's polypoly</trd>
    </trdgrp>
   </drv>
  </art>


