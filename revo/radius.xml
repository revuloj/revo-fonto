<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: radius.xml,v 1.30 2022/03/26 18:10:06 revo Exp $">
<kap>
  <rad>radius</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="radius.0o">
  <kap><tld/>o</kap>
  <snc mrk="radius.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      La pli mallonga osto de l' anta&ubreve;brako, kiu respondas al la dika
      fingro.
    </dif>
    <refgrp tip="vid">
      <ref cel="kubut.0o">kubuto</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_ja;&c_n;&c_jo;&c_v;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_k;&c_a;</trd>
    <trd lng="de">Speiche</trd>
    <trd lng="fr">radius</trd>
    <trd lng="hu">ors&oacute;csont</trd> 
    <trd lng="it">radio<klr> (anat.)</klr></trd>   
    <trd lng="nl">spaakbeen</trd>
    <trd lng="pl">ko&sacute;&cacute; promieniowa</trd>
    <trd lng="pt">r&aacute;dio</trd>
    <trd lng="ru">&c_l;&c_u;&c_ch;&c_je;&c_v;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="radius.0o.cirklo">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(elementa geometrio)</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <klr>(de <ref cel="cirkl.0o.linio">cirklo</ref> a&ubreve;
      <ref cel="sfer.0o.surfaco">sfero</ref>)</klr>
      &Ccirc;iu el la <ref tip="super" cel="strek.0o.segmento">strekoj</ref>,
      kunligantaj punkton de la koncerna figuro al &gcirc;ia
      <ref cel="centr.0o.cirklo">centro</ref>; la longo de tia streko:
      <ekz>
        areo de cirklo a&ubreve; sfero estas proporcia al la kvadrato de
        &gcirc;ia <tld/>o.
      </ekz>
    </dif>
    <ref tip="sin" cel="radi.0o.radiuso">radio.</ref>
    <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_ju;&c_s;</trd>
    <trd lng="bg">&c_r;&c_a;&c_d;&c_i;&c_u;&c_s;</trd>
    <trdgrp lng="de">
      <trd>Radius</trd>,
      <trd>Halbmesser</trd>
    </trdgrp>
    <trd lng="en">radius</trd>
    <trd lng="fr">rayon<klr tip="ind"> (d'un cercle ou d'une
    sph&egrave;re)</klr></trd>
    <trd lng="hu">sug&aacute;r<klr tip="ind"> (f&eacute;l &aacute;tm&eacute;r&odblac;)</klr></trd>    
    <trd lng="it">raggio</trd>
    <trd lng="nl">straal<klr tip="ind"> (v.e.cirkel)</klr></trd>
    <trd lng="pl">promie&nacute;<klr tip="ind"> (ko&lstroke;a lub sfery)</klr></trd>
    <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_u;&c_s;</trd>
  </snc>
  <trd lng="ca">radi</trd>
  <trdgrp lng="cs">
    <trd>polom&ecaron;r</trd>,
    <trd>r&aacute;dius</trd>
  </trdgrp>
  <trd lng="es">radio<klr tip="amb"> (hueso, geometr&iacute;a)</klr></trd>
  <trdgrp lng="sk">
    <trd>polomer</trd>,
    <trd>r&aacute;dius</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x430;&#x434;&#x456;&#x443;&#x441;</trd>,
    <trd>&#x43F;&#x440;&#x43E;&#x43C;&#x435;&#x43D;&#x435;&#x432;&#x430; &#x43A;&#x456;&#x441;&#x442;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="radius.konvergxo0o">
  <kap>konver&gcirc;o<tld/>o, <var><kap>konver&gcirc;<tld/>o</kap></var></kap>
  <snc mrk="radius.konvergxo0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(de <ref cel="seri.potenco0o">potencoserio</ref>
      <frm am="S(x)"><k>S</k>(<k>x</k>)</frm> 
      &ccirc;irka&ubreve; <frm am="x_0"><k>x</k><sub>0</sub></frm>)</klr>
      Tia reelo <frm am="rho">&rho;</frm>, ke la serio malkonver&gcirc;as, se 
      <frm am="|x-x_0| gt rho">|<k>x</k>-<k>x</k><sub>0</sub>| &gt;
      &rho;</frm>, 
      kaj konver&gcirc;as, se <frm am="|x-x_0| lt rho">|<k>x</k>-<k>x</k><sub>0</sub>| &lt; &rho;</frm>.
    </dif>
    <rim>
      Oni diras, ke la konver&gcirc;o<tld/>o estas nefinia, se la serio 
      konver&gcirc;as &ccirc;ie.
    </rim>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_ju;&c_s; &c_z;&c_mol;&c_b;&c_je;&c_zh;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;</trd>
  <trd lng="ca"><ind>radi</ind> de converg&egrave;ncia</trd>
  <trd lng="de">Konvergenzradius</trd>
  <trd lng="en">radius of convergence</trd>
  <trd lng="fr">rayon de convergence</trd>
  <trd lng="hu">konvergenciasug&aacute;r</trd>  
  <trd lng="it">raggio<klr> di convergenza</klr></trd>
  <trd lng="pl">promie&nacute; zbie&zdot;no&sacute;ci</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_u;&c_s; &c_s;&c_h;&c_o;&c_d;&c_i;&c_m;&c_o;&c_s;&c_t;&c_i;</trd>
</drv>

<drv mrk="radius.kurbeco0o">
  <kap>kurbeco<tld/>o, <var><kap>kurbec<tld/>o</kap></var></kap>
  <fnt><bib>MatTerm</bib><lok>p. 38,
  &leftquot;kurbecoradio&rightquot;</lok></fnt>
  <snc mrk="radius.kurbeco0o.de_ebena_kurbo">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <klr>(de ebena <ref cel="kurb.0o.MAT">kurbo</ref>, 
      &ccirc;e punkto <frm am="a"><k>a</k></frm> de &gcirc;i)</klr>
      La distanco inter <frm am="a"><k>a</k></frm> kaj la 
      <ref cel="centr.kurbeco0o.de_ebena_kurbo">kurbecocentro</ref> de
      la
      kurbo &ccirc;e tiu punkto:
      <ekz>
        la kurbeco<tld/>o de cirklo &ccirc;e ajna punkto de &gcirc;i egalas
        al &gcirc;ia <tld/>o.
      </ekz>
    </dif>
    <ref tip="vid" cel="kurb.0eco.de_ebena_kurbo">kurbeco.</ref>
    <rim>
      En <fnt><bib>MatVort</bib></fnt> trovi&gcirc;as la formo
      <ctl><tld/>o de kurbeco</ctl>.
    </rim>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_ju;&c_s; &c_k;&c_r;&c_y;&c_v;&c_ib;&c_n;&c_ib;</trd>
  <trd lng="ca"><ind>radi</ind> de curvatura</trd>
  <trd lng="cs">polom&ecaron;r k&rcaron;ivosti</trd>
  <trd lng="de">Kr&uuml;mmungsradius</trd>
  <trd lng="en">radius of curvature</trd>
  <trd lng="fr">rayon de courbure</trd>
  <trd lng="hu">g&ouml;rb&uuml;leti <ind>sug&aacute;r</ind></trd>
  <trd lng="it">raggio<klr> di curvatura</klr></trd>
  <trd lng="pl">promie&nacute; krzywizny</trd>
  <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_u;&c_s; &c_k;&c_r;&c_i;&c_v;&c_i;&c_z;&c_n;&c_y;</trd>
  <trd lng="sk">polomer krivosti</trd>
</drv>
</art>
