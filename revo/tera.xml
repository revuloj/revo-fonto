<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tera.xml,v 1.9 2023/09/04 18:15:46 revo Exp $">
<kap>
    <rad>tera</rad>- <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tera.0">
  <kap><tld/>-</kap>
  <mlg>T</mlg>
  <gra><vspec>prefikso</vspec></gra>
  <snc mrk="tera.0.MEZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      &leftquot;Biliono da&rightquot;, 10<sup>12</sup>
      en la Internacia Sistemo de Unuoj (SI):
      <ekz>
        grandegaj ciferecaj deponejoj de elsendoj de dekoj da stacioj 
        la&ubreve;longe de monatoj kaj e&ccirc; jaroj kun grandecoj de <tld/>bajtoj 
        (miloj da gigabajtoj)
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2003/008062.html">Spektantoj spektataj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        1986 en &Ccirc;ernobilo <klr>[&#x2026;]</klr> eksplodis nuklea reaktoro <klr>[&#x2026;]</klr>
        estis tiam eligitaj 5,2 milionoj da <tld/>bekereloj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011522.php">La
          akcidento de &Ccirc;ernobilo 30 jarojn poste</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_r;&c_a;-</trd>
  <trd lng="bg">&c_t;&c_je;&c_r;&c_a;-</trd>
  <trd lng="ca">tera-</trd>
  <trd lng="de">Tera-</trd>
  <trd lng="en">tera-</trd>
  <trd lng="es">tera-</trd>
  <trd lng="fa">&a_t;&a_r;&a_A;</trd>
  <trd lng="fr">t&eacute;ra-</trd>
  <trd lng="hu">tera-</trd>
  <trd lng="it">tera-</trd>
  <trd lng="ja">&#x30C6;&#x30E9;&#xFF5E;</trd>
  <trd lng="nl">tera-</trd>
  <trd lng="no">tera-</trd>
  <trd lng="pl">tera-</trd>
  <trd lng="pt">tera-</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_a;-</trd>
  <trd lng="sv">tera-</trd>
  <trd lng="uk">&#x442;&#x435;&#x440;&#x430;&#x2026;</trd>
</drv>
