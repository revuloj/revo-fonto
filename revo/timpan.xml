<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: timpan.xml,v 1.26 2023/10/21 13:52:03 revo Exp $">
<kap>
  <rad>timpan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="timpan.0o">
  <kap><tld/>o</kap>
  <snc mrk="timpan.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Tiu kava&jcirc;o de <ref tip="malprt" cel="orel.0o.ANA">orelo</ref>,
      sur kiu estas etendita la resona <ref cel="membra.orel0o">membrano</ref>:
      <ekz>
        kvaza&ubreve; mil tondroj senpere tamburas la <tld/>ojn
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        43a</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> al vi &scirc;iras la <tld/>on, vi devas prononci &#x201A;sinjoro doktor&#x2018;,
        sinjor&#x2019; doktoro&#x2019; kvaza&ubreve; vi poemus tuttage
        <fnt>
          <bib>ChC</bib>
          <lok>6</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Paukenh&ouml;hle</trd>
    <trdgrp lng="en">
      <trd>tympanium</trd>,
      <trd>middle ear</trd>
    </trdgrp>
    <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_r;&c_a;&c_p;&c_o;&c_n;&c_k;&c_a;</trd>
    <trd lng="ca">timp&agrave; (o&iuml;da)</trd>
    <trd lng="fr">tympan<klr tip="amb"> (ana.)</klr></trd>
    <trd lng="hu">dob&uuml;reg</trd>
    <trdgrp lng="id">
      <trd><ind>gendang</ind> telinga</trd>,
      <trd><ind>membran</ind> timpani</trd>,
      <trd>timpanum</trd>
    </trdgrp>
    <trd lng="nl">trommelholte</trd>
    <trd lng="pl">b&lstroke;ona b&eogonek;benkowa</trd>
  <trdgrp lng="pt">
    <trd>t&iacute;mpano</trd>
  </trdgrp>
    <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_r;&c_je;&c_p;&c_o;&c_n;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="timpan.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.frap0o" lst="voko:frapinstrumentoj">Frapinstrumento</ref> kun
      kaldronforma sonkesto kaj suretendita felo:
      <ekz>
        oni sonigu <tld/>ojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>!
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_u;&c_s;&c_mol;&c_l;&c_ib;</trd>,
      <trd>&c_t;&c_y;&c_m;&c_p;&c_a;&c_n;</trd>
    </trdgrp>
    <trd lng="ca">timbal</trd>
    <trd lng="de">Pauke</trd>
    <trd lng="en">timpani</trd>
    <trd lng="hu">&uuml;stdob</trd>
    <trd lng="id">timpani</trd>
    <trd lng="nl">pauk</trd>
    <trd lng="pl">kocio&lstroke; <klr>(instrument)</klr></trd>
  <trdgrp lng="pt">
    <trd>t&iacute;mpano</trd>
  </trdgrp>
    <trd lng="ru">&c_t;&c_i;&c_m;&c_p;&c_a;&c_n;</trd>
  </snc>
  <snc mrk="timpan.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Spaco enkadrigata per la kornicoj de <ref tip="malprt" cel="fronto.0o.ARKI">frontono</ref>:
      <ekz>
        el <tld/>oj elstaradis skulptitaj figuroj de dioj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_y;&c_m;&c_p;&c_a;&c_n;</trd>
    <trd lng="ca">timp&agrave; (front&oacute;)</trd>
    <trd lng="de">Tympanon <klr>(Architektur)</klr></trd>
    <trd lng="en">tympanium</trd>
    <trd lng="fr">tympan<klr tip="amb"> (archi.)</klr></trd>
    <trd lng="hu">timpanon</trd>
    <trd lng="nl">tympaan</trd>
    <trd lng="pl">tympanon</trd>
    <trd lng="ru">&c_t;&c_i;&c_m;&c_p;&c_a;&c_n;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>tymp&aacute;n</trd>,
    <trd>u&scaron;n&iacute; bl&aacute;na</trd>,
    <trd>u&scaron;n&iacute; bub&iacute;nek</trd>
  </trdgrp>
  <trd lng="it">timpano</trd>
  <trdgrp lng="ja">
    <trd>&#x9F13;&#x5BA4;  <pr>&#x3053;&#x3057;&#x3064;</pr></trd>,
    <trd>&#x4E2D;&#x8033;  <pr>&#x3061;&#x3085;&#x3046;&#x3058;</pr></trd>,
    <trd>&#x30C6;&#x30A3;&#x30F3;&#x30D1;&#x30CC;&#x30E0;</trd>,
    <trd>&#x4E09;&#x89D2;&#x5C0F;&#x9593;  <pr>&#x3055;&#x3093;&#x304B;&#x304F;&#x3057;&#x3087;&#x3046;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="sk">u&scaron;n&yacute; bubienok</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;&c_n;&c_a; &c_p;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_t;&c_i;&c_m;&c_p;&c_a;&c_n;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_o;&c_p;&c_ib;&c_d;&c_j;&c_o;&c_m;&c_n;&c_je; &c_k;&c_o;&c_l;&c_je;&c_s;&c_o;</trd>,
    <trd>&c_d;&c_je;&c_k;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
