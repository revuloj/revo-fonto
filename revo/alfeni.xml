<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alfeni.xml,v 1.18 2024/02/12 09:25:51 revo Exp $">
<kap>
  <rad>alfenid</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="alfeni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="aloj.0o">Alojo</ref> de <ref tip="prt" cel="kupr.0o.KEM">kupro</ref>,
      <ref tip="prt" cel="nikel.0o">nikelo</ref> kaj <ref tip="prt" cel="zink.0o">zinko</ref>,
      imitanta ar&gcirc;enton, ofte galvane kovrita de ar&gcirc;ento kaj uzata interalie por fabriki
      man&gcirc;ilarojn:
      <ekz>
        ar&gcirc;entano &dash; la&ubreve; iuj lingvoj &leftquot;novar&gcirc;ento&rightquot;, de <nom>L. L. Zamenhof</nom> nomita
        <klr>[&#8230;]</klr> <tld/>o <klr>[&#8230;]</klr> aspektas tre simile al ar&gcirc;ento, sed
        kemie tute ne rilatas al &gcirc;i, en la hispana kaj angla lingvoj oni anka&ubreve; nomas
        la metalon <klr>[&#8230;]</klr> &leftquot;germana ar&gcirc;ento&rightquot;, ar&gcirc;entano kovrita per maldika
        tavolo de ar&gcirc;ento estas nomata &leftquot;&ccirc;ina ar&gcirc;ento&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=215853">Ar&gcirc;entano</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="argxen1.0o">ar&gcirc;entano</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_g;&c_je;&c_n;&c_t;&c_a;&c_n;</trd>,
    <trd>&c_n;&c_e;&c_j;&c_z;&c_y;&c_l;&c_mol;&c_b;&c_e;&c_r;</trd>,
    <trd>&c_n;&c_o;&c_v;&c_a;&c_je; &c_s;&c_r;&c_e;&c_b;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Neusilber</trd>,
    <trd>Alfenid</trd>,
    <trd>Argentan</trd>,
    <trd>Alpaka <klr>(Neusilber)</klr></trd>,
    <trd>Maillechort</trd>
  </trdgrp>
  <trd lng="en">alfenide</trd>
  <trd lng="es">alfenide</trd>
  <trd lng="fr">alf&eacute;nide</trd>
  <trd lng="he">&#x5E1;&#x5D2;&#x5E1;&#x5D5;&#x5D2;&#x5EA;</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30EB;&#x30D5;&#x30A7;&#x30CB;&#x30C9;</trd>,
    <trd>&#x6D0B;&#37504;  <pr>&#12424;&#12358;&#x304E;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">alfenide</trd>
  <trd lng="pt">alf&ecirc;nide</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_j;&c_z;&c_i;&c_l;&c_mol;&c_b;&c_je;&c_r;</trd>,
    <trd>&c_m;&c_je;&c_l;&c_mol;&c_h;&c_i;&c_o;&c_r;</trd>,
    <trd>&c_a;&c_l;&c_mol;&c_f;&c_je;&c_n;&c_i;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>alpaka</trd>,
    <trd>argentan</trd>,
    <trd>pakf&oacute;n</trd>,
    <trd>&ccaron;&iacute;nske striebro</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>alpaka</trd>,
    <trd>nov&eacute; st&rcaron;&iacute;bro</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&#x43B;&#x44C;&c_f;&c_je;&#x43D;&c_ib;&c_d;</trd>
</drv>
</art>
