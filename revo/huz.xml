<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: huz.xml,v 1.25 2023/08/31 15:46:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>huz</rad>/o
</kap>
<drv mrk="huz.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">Fi&scirc;o</ref> parenca
      al 
      <ref cel="acipenser.0o">acipensero</ref>, sed kun lar&gcirc;a duoncirkla
      anta&ubreve;en malfermi&gcirc;anta bu&scirc;o; <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="acipen1.0oj">acipenseredoj</ref>
      (<trd lng="la">Huso</trd>), kies pli granda,
      &gcirc;is ses metrojn longa, specio vivas en Nigra kaj Kaspia Maroj kaj 
      alfluantaj riveroj kaj estas kaptata pro
      <ref tip="prt" cel="kaviar.0o">kaviaro</ref>:
      <ekz>
        la plej granda fi&scirc;o en Volgo estas <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=12593">Volgo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la beluga <tld/>o na&gcirc;is &gcirc;is Bratislavo en la Danubo, &gcirc;is oni konstruis la
        akvoenergian centralon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26042">Huzo</url></lok>
        </fnt>.
      </ekz> 
    </dif>
    <bld lok="&WCU;/thumb/0/04/&c_B;&c_ib;&c_l;&c_u;&c_g;&c_a;_(Huso_huso).jpg/640px-&c_B;&c_ib;&c_l;&c_u;&c_g;&c_a;_(Huso_huso).jpg" prm="CC BY-SA 4.0">Beluga <tld/>o (Huso huso)
        <fnt>
          <aut>&c_M;&c_a;&c_k;&c_s;&c_i;&c_m; &c_Ja;&c_k;&c_o;&c_v;&c_l;&c_jeu;&c_v;, propra verko</aut>
          <lok><url ref="&WCW;/File:%D0%91%D1%96%D0%BB%D1%83%D0%B3%D0%B0_(Huso_huso).jpg">&WCW;/File:&c_B;&c_ib;&c_l;&c_u;&c_g;&c_a;_(Huso_huso).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_u;&c_g;&c_a;</trd>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_u;&c_n;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Hausen<klr tip="amb"> (Fisch)</klr></trd>,
    <trd>Belugast&ouml;r</trd>
  </trdgrp>
  <trd lng="el">&omikron;&xi;&ypsilon_ton;&rho;&rho;&ypsilon;&gamma;&chi;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>beluga</trd>,
    <trd>European sturgeon</trd>,
    <trd>white sturgeon</trd>,
    <trd>hausen</trd>
  </trdgrp>
  <trd lng="es">esturi&oacute;n</trd>
  <trd lng="fr">esturgeon b&eacute;louga</trd>
  <trd lng="hu">viza</trd>
  <trd lng="it">storione<klr> (genere "huso")</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30ED;&#x30C1;&#x30E7;&#x30A6;&#x30B6;&#x30E1;</trd>,
    <trd>&#x30D9;&#x30EB;&#x30FC;&#x30AC;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>huso</trd>, <trd>steur</trd>
  </trdgrp>
  <trd lng="pl">bie&lstroke;uga</trd>
  <trd lng="pt">esturj&atilde;o</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_u;&c_g;&c_a;
<klr>(&c_r;&c_y;&c_b;&c_a;)</klr></trd>
  <trd lng="uk">&#x431;&#x456;&#x43B;&#x443;&#x433;&#x430;</trd>
</drv>
</art>
