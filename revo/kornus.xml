<vortaro>
<art mrk="$Id$">
<kap>
<rad>kornus</rad>/o
</kap>
<drv mrk="kornus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kornus.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Cornus</trd>).
    </dif>
  </snc>
  <trd lng="de">Hartriegel</trd>
</drv>
<drv mrk="kornus.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Cornaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Hartriegelgew&auml;chse</trd>
</drv>
</art>
