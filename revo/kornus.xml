<art mrk="$Id$">
<kap>
  <rad>kornus</rad>/o
</kap>
<drv mrk="kornus.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kornus1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Cornus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_z;&c_ib;&c_l;</trd>
  <trd lng="de">Hartriegel</trd>
  <trdgrp lng="en">
    <trd>cornel</trd>,
    <trd>dogwood</trd>
  </trdgrp>
  <trd lng="hu">som</trd>
  <trdgrp lng="pt">
    <trd>corniso</trd>,
    <trd>sangrinho</trd>,    
    <trd>sanguinho-leg&iacute;timo</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_i;&c_z;&c_i;&c_l;</trd>
</drv>
</art>
