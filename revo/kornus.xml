<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kornus.xml,v 1.22 2023/12/03 09:58:58 revo Exp $">
<kap>
  <rad>kornus</rad>/o
</kap>
<drv mrk="kornus.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="kornus.0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="kornus1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Cornus</trd>):
      <ekz>
        <tld/>o estas genro de plantoj <klr>[&#x2026;]</klr> kun integraj folioj &gcirc;enerale
        kontra&ubreve;e duopaj, kun malgrandaj, epiginaj, kvarnombraj floroj en cumoj umbel-
        a&ubreve; kap-formaj, kaj kun diverskoloraj drupoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30113">Kornuso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/65/Cornus_alternifolia_-_Krauss.jpg/444px-Cornus_alternifolia_-_Krauss.jpg" prm="PD">desegno de <tld/>o (Cornus alternifolia)
        <fnt>
          <aut>Johan Carl Krauss</aut>
          <lok><url ref="&WCW;/File:Cornus_alternifolia_-_Krauss.jpg">&WCW;/File:Cornus_alternifolia_-_Krauss.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_z;&c_ib;&c_l;</trd>
  <trd lng="de">Hartriegel</trd>
  <trdgrp lng="en">
    <trd>cornel</trd>,
    <trd>dogwood</trd>
  </trdgrp>
  <trd lng="fr">cornouiller</trd>
  <trd lng="hu">som</trd>
  <trdgrp lng="ja">
    <trd>&#x30DF;&#x30BA;&#x30AD;</trd>,
    <trd>&#x30B5;&#x30F3;&#x30B7;&#x30E5;&#x30E6;</trd>
  </trdgrp>
  <trd lng="nl">kornoelje</trd>
  <trd lng="pl">dere&nacute;</trd>
  <trdgrp lng="pt">
    <trd>corniso</trd>,
    <trd>sangrinho</trd>,    
    <trd>sanguinho-leg&iacute;timo</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_i;&c_z;&c_i;&c_l;</trd>
  <trd lng="zh">&#x5C71;&#x8331;&#x8438;&#x5C5E; <pr>sh&#x101;nzh&#x16B;y&#xFA;sh&#x1D4;</pr></trd>
</drv>
</art>
