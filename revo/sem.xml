  <trd lng="ka">&#x10D7;&#x10D4;&#x10E1;&#x10DA;&#x10D8;</trd>
  <trd lng="kek">aweel</trd>
  <trd lng="be">&c_s;&c_je;&c_m;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>semeno</trd>,
    <trd>sem&iacute;nko</trd>
  </trdgrp>
  <trd lng="de">Samen</trd>
  <trd lng="el">&sigma;&pi;&omikron_ton;&rho;&omikron;&sigma_fina;</trd>
  <trd lng="en">seed</trd>
  <trd lng="es">semilla</trd>
  <trd lng="fr">semence</trd>
