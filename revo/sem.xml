  <trd lng="ha">iri</trd>
  <trd lng="hi">&#x92C;&#x940;&#x91C;</trd>
  <trd lng="hr">sjeme</trd>
  <trd lng="hy">&#x57D;&#x565;&#x580;&#x574;</trd>
  <trd lng="is">fr&#xE6;</trd>
  <trd lng="ja">&#x30B7;&#x30FC;&#x30C9;</trd>
