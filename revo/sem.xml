<drv mrk="sem.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sem.0i.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Enmeti <tld/>ojn en la teron, por ke ili &gcirc;ermu kaj kresku:
      <ekz>
        nenie <tld/>ata, &ccirc;ie trovata
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="sem.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      En multajn unuopajn lokojn meti, konigi, kreskigi:
      <ekz>
        golfo <tld/>ita per multaj insuloj<fnt>B</fnt>;
      </ekz>
      <ekz>
        &scirc;oseo sur<tld/>ita per &scirc;tonrompa&jcirc;o;
      </ekz>
      <ekz>
        la epidemio <tld/>is la morton en la regiono;
      </ekz>
      <ekz>
        <tld/>i maltrankvilon, teruron en la urbo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i malkonfidon en la animon de iu<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>antoj de malpaco<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="nl">bezaaien</trd>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_s;&c_je;&c_ja;</trd>
  <trd lng="bo">&#xF60;&#xF51;&#xF7A;&#xF56;&#xF66; &#xF56;&#xF4F;&#xF56; &#xF42;&#xF51;&#xF56; &#xF50;&#xF7C;&#xF56;&#xF0B;</trd>
  <trd lng="cs">p&rcaron;edpona s v&yacute;znamem"<klr>(p&rcaron;ibli&zcaron;n&aacute;)</klr>
  polovina"</trd>
  <trd lng="de">s&auml;en</trd>
  <trd lng="en">sow</trd>
  <trd lng="es">sembrar</trd>
  <trdgrp lng="fr">
    <trd>parsemer</trd>,
    <trd>semer</trd>
  </trdgrp>
  <trd lng="hu">vet</trd>
  <trd lng="it">seminare</trd>
  <trd lng="nl">zaaien</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_je;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">predpona s v&yacute;znamom "<klr>(pribli&zcaron;n&aacute;)</klr>
  polovica"</trd>
  <trd lng="sv">s&aring;</trd>
</drv>

<drv mrk="sem.0o">
  <kap><ofc>*</ofc><tld/>o <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5194">sem'o</url></lok></fnt></kap>
  <snc mrk="sem.0o.KOMUNE">
    <dif>
      Malgranda fekunda&jcirc;o, el kiu kreskos io:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        scienco havas <tld/>on enuan, sed frukton bon&gcirc;uan
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <subsnc mrk="sem.0o.BOT">
      <uzo tip="fak">BOT</uzo>
      <dif>
        Tiu parto de <ref tip="malprt" cel="frukt.0o.BOT">frukto</ref>,
        kiu elfalas post la maturi&gcirc;o kaj servas por produkti
        novan vegeta&jcirc;on:
        <ekz>
          iuj <tld/>oj falis apud la vojo, kaj la birdoj venis kaj
          forman&gcirc;is
          ilin
          <fnt><bib>NT</bib>, <lok>&Mat; 13:4</lok></fnt>.
        </ekz>
      </dif>
      <rim>
        &Ccirc;e malfermfruktoj tio estas aro da <tld/>grajnoj (papavo,
        pizo),
        &ccirc;e fermfruktoj tio estas la tuta frukto (greno).
      </rim>
    </subsnc>
    <subsnc mrk="sem.0o.fekundiga">
      <dif>
        Fruktiga substanco produktata de virseksa homo,
        besto a&ubreve; vegeta&jcirc;o.
      </dif>
      <trd lng="sv">s&auml;d</trd>
    </subsnc>
  </snc>
  <snc mrk="sem.0o.GEN">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Idaro, posteulara:
      <ekz>
        la <tld/>o de David<fnt>Z</fnt>;
      </ekz>
      <ekz>
        el la re&gcirc;a <tld/>o <klr>(raso)</klr> de Edom<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ilia <tld/>o estos ekstermita el inter la homidoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sang.0o.GEN">sango<sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="sem.0o.disvastigilo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Disvastigilo.
    </dif>
  </snc>
  <trd lng="af">saad</trd>
  <trd lng="am">&#x12D8;&#x122D;</trd>
  <trd lng="ar">&#x628;&#x630;&#x631;&#x629;</trd>
