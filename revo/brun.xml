<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
  <rad>brun</rad>/a
</kap>

<drv mrk="brun.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta <ref tip="lst" cel="kolor.0o.propre"
      lst="voko:koloroj">koloron</ref> flave-ru&gcirc;e-nigran, kiel kafo a&ubreve;
      ka&scirc;tanoj:
      <ekz>
        malgrandaj <ind><tld/>aj okuloj</ind>
        <fnt><bib>Marta</bib></fnt>;
        <trd lng="ru">&c_k;&c_a;&c_r;&c_i;&c_je;
        &c_g;&c_l;&c_a;&c_z;&c_a;</trd>
        <trdgrp lng="pl">
          <trd>br&aogonek;zowe oczy</trd>,
          <trd>piwne oczy</trd>
        </trdgrp>
      </ekz>
      <ekz>
        homo en leda <tld/>a jako
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        rusto<tld/>a fero
        <fnt><bib>InfanTorent2</bib>,
        <lok>unua parto, &ccirc;apitro 2a</lok></fnt>;
      </ekz>
      <ekz>
        mola tapi&scirc;o el <tld/>aj pinpingloj
        <fnt><bib>InfanTorent2</bib>,
        <lok>dua parto, &ccirc;apitro 2a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>aj haroj;
      </ekz>
      <ekz>
        <ind><tld/>a &ccirc;evalo</ind>;
        <trdgrp lng="pl">
          <trd>gniadosz<klr> (ko&nacute;)</klr></trd>,
          <trd>kasztan<klr> (ko&nacute;)</klr></trd>
        </trdgrp>
        <trd lng="ru">&c_g;&c_n;&c_je;&c_d;&c_a;&c_ja;
        &c_l;&c_o;&c_sh;&c_a;&c_d;&c_mol;</trd>
      </ekz>
      <ekz>
        alta, <tld/>a <klr>(<tld/>hara, <tld/>ha&ubreve;ta)</klr> viro
        li estas
        <fnt><bib>SatirRak</bib>,
        <lok>la fluidumo de la &ccirc;iovido</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_je;&c_v;&c_y;</trd>
  <trd lng="bg">&c_k;&c_a;&c_f;&c_ja;&c_v;</trd>
  <trd lng="br">rous</trd>
  <trdgrp lng="cs">
    <trd>hn&ecaron;d&yacute;</trd>,
    <trd>hnědý</trd>,
    <trd>snědý</trd>
  </trdgrp>
  <trd lng="de">braun</trd>
  <trd lng="en">brown</trd>
  <trdgrp lng="es">
    <trd>marr&oacute;n</trd>,
    <trd>caf&eacute; <klr>(lat.am.)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>brun</trd>,
    <trd>marron</trd>
  </trdgrp>
  <trd lng="hu">barna</trd>
  <trd lng="id">(warna) <ind>cokelat</ind></trd>
  <trd lng="nl">bruin</trd>
  <trd lng="pl">br&aogonek;zowy</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_i;&c_ch;&c_n;&c_je;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">hned&yacute;</trd>
  <trd lng="sv">brun</trd>
</drv>

<drv mrk="brun.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="brun.0ulo.hare">
    <dif>
      Viro <tld/>hara:
      <ekz>
        la <tld/>uloj devas solidari, venki la blondulojn, kaj tio
        &ccirc;i alportos la eternan pacon
        <fnt><bib>SatirRak</bib>, <lok>Kain kaj Abel</lok></fnt>.
      </ekz>
    </dif>    
    <trdgrp lng="br">
      <trd>rouz<klr tip="ind"> (rouzard)</klr></trd>,
      <trd>rouzard</trd>
    </trdgrp>
    <trd lng="nl">man met <ind>bruin</ind> haar</trd>
    <trd lng="pl">brunet</trd>
    <trd lng="ru">&c_sh;&c_a;&c_t;&c_je;&c_n;</trd>
  </snc>
  <snc mrk="brun.0ulo.hauxte">
    <dif>
      Viro <tld/>ha&ubreve;ta:
      <ekz>
      </ekz>
    </dif>
    <trd lng="fr">brun<klr tip="amb"> (homme brun)</klr></trd>
    <trd lng="nl">man met <ind>bruine</ind> huid</trd>
    <trdgrp lng="pl">
      <trd>&sacute;niady<klr> cz&lstroke;owiek</klr></trd>,
      <trd>smag&lstroke;y<klr> cz&lstroke;owiek</klr></trd>,
      <trd>ogorza&lstroke;y<klr> cz&lstroke;owiek</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_m;&c_u;&c_g;&c_l;&c_y;&c_j;<klr>
    (&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>brunet</trd>,
    <trd>míšenec</trd>
  </trdgrp>
  <trd lng="es">moreno</trd>
  <trd lng="sk">brunet</trd>
</drv>

<drv mrk="brun.0ulino">
  <kap><tld/>ulino</kap>
  <snc mrk="brun.0ulino.hare">
    <dif>
      Ino <tld/>hara:
      <ekz>
        kiaj pli pla&ccirc;as al vi &#8208; <tld/>ulinoj a&ubreve;
        <ref tip="ant" cel="blond.0ulino">blondulinoj</ref>?
        <fnt><bib>Revizoro</bib>,
        <lok>akto 4a, sceno 5a</lok></fnt>.
      </ekz>
    </dif>        
    <trdgrp lng="br">
      <trd>rouzenn</trd>,
      <trd>rouzardez</trd>
    </trdgrp>
    <trd lng="nl">brunette</trd>
    <trd lng="pl">brunetka</trd>
  </snc>
  <snc mrk="brun.0ulino.hauxte">
    <dif>
      Ino <tld/>ha&ubreve;ta:
      <ekz>
      </ekz>
    </dif>
    <trd lng="nl">vrouw met <ind>bruine</ind> huid</trd>
    <trdgrp lng="pl">
      <trd>&sacute;niada<klr> kobieta</klr></trd>,
      <trd>smag&lstroke;a<klr> kobieta</klr></trd>,
      <trd>ogorza&lstroke;a<klr> kobieta</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_m;&c_u;&c_g;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bruneta</trd>,
    <trd>brunetka</trd>
  </trdgrp>
  <trd lng="es">morena</trd>
  <trd lng="fr">brune<klr tip="amb"> (femme brune)</klr></trd>
  <trd lng="sk">brunetka</trd>
</drv>

<drv mrk="brun.sun0o">
  <kap>sun<tld/>o</kap>
  <snc>
    <dif>
      Malhela koloro de la ha&ubreve;to, ka&ubreve;zita de elmetado al
      sunaj radioj:
      <ekz>
        li aspektis kvaza&ubreve; li pasigus la tagon eksterdome,
        lia ha&ubreve;to jam havis nuancon de sun<tld/>o
        <fnt><bib>Kiso</bib>, <lok>G&auml;rda</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_g;&c_a;&c_r;</trd>
  <trd lng="br">rouster</trd>
  <trd lng="cs">hnědé opálení kůže</trd>
  <trdgrp lng="de">
    <trd>Sonnenteint</trd>,
    <trd>Sonnenbr&auml;une</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>suntan</trd>
  </trdgrp>
  <trd lng="es">bronceado</trd>
  <trd lng="fr">h&acirc;le</trd>
  <trdgrp lng="hu">
    <trd>barnas&aacute;g</trd>,
    <trd>les&uuml;l&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">bruine kleur<klr tip="ind"> (door de zon)</klr></trd>
  <trd lng="pl">opalenizna</trd>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>farba pleti získaná pôsobením slnka</trd>,
    <trd>op&aacute;lenie</trd>
  </trdgrp>
  <trd lng="sv">solbr&auml;nna</trd>
</drv>

<drv mrk="brun.sun0igi">
  <kap>sun<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi, ke korpa parto <tld/>i&gcirc;u per elmetado al sunaj radioj:
      <ekz>
        la maldelikataj, sunbrunigitaj viza&gcirc;oj de la
        <klr>...</klr>
        rabistoj
        <fnt><bib>I&scirc;tar</bib>,
        <lok>&ccirc;apitro 9a, aventura voja&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="bronz.0i">bronzi</ref>
  </refgrp>                 
  <trd lng="br">rouza&ntilde;<klr tip="ind"> (kroc'hen -
  v.k.)</klr></trd>
  <trd lng="en">tan</trd>
  <trd lng="es">broncear</trd>
  <trdgrp lng="fr">
    <trd>bronzer <klr tip="ind">(h&acirc;ler)</klr></trd>,
    <trd>h&acirc;ler</trd>
  </trdgrp>
  <trd lng="hu">lebarn&iacute;t<klr tip="ind"> (nap)</klr></trd>
  <trd lng="nl">bruinen <klr tip="ind"> (in de zon gaan
  liggen)</klr></trd>
  <trd lng="pl">opali&cacute;<klr> (na
  s&lstroke;o&nacute;cu)</klr></trd>
  <trd lng="sk">op&aacute;li&tcaron; <klr>(na slnku)</klr></trd>
</drv>

<drv mrk="brun.sun0igxi">
  <kap>sun<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Akiri sun<tld/>on:
      <ekz>
        mi sun<tld/>i&gcirc;as sur la strando.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>sonnenbaden</trd>,
    <trd><klr tip="amb">in der Sonne</klr> <ind>br&auml;unen</ind></trd>
  </trdgrp>    
  <trd lng="br">rouza&ntilde;<klr tip="ind"> (kroc'hen -
  v.g.)</klr></trd>
  <trdgrp lng="nl">
    <trd>bruinen<klr tip="ind"> (bruin worden)</klr></trd>,
    <trd>bruin worden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>opali&cacute; si&eogonek;<klr> (na
    s&lstroke;o&nacute;cu)</klr></trd>
    <trd>ogorze&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
