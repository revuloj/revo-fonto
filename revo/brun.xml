<drv mrk="brun.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloron</ref>
      flave-ru&gcirc;e-nigran, kiel kafo a&ubreve;
      ka&scirc;tanoj:
      <ekz>
        malgrandaj <ind><tld/>aj okuloj</ind>
        <fnt><bib>Marta</bib></fnt>;
        <trd lng="ru">&c_k;&c_a;&c_r;&c_i;&c_je;
        &c_g;&c_l;&c_a;&c_z;&c_a;</trd>
        <trdgrp lng="pl">
          <trd>br&aogonek;zowe oczy</trd>,
          <trd>piwne oczy</trd>
        </trdgrp>
      </ekz>
      <ekz>
        homo en leda <tld/>a jako
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        rusto<tld/>a fero
        <fnt><bib>InfanTorent2</bib>,
        <lok>unua parto, &ccirc;apitro 2a</lok></fnt>;
      </ekz>
      <ekz>
        mola tapi&scirc;o el <tld/>aj pinpingloj
        <fnt><bib>InfanTorent2</bib>,
        <lok>dua parto, &ccirc;apitro 2a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>aj haroj;
      </ekz>
      <ekz>
        <ind><tld/>a &ccirc;evalo</ind>;
        <trdgrp lng="pl">
          <trd>gniadosz<klr> (ko&nacute;)</klr></trd>,
          <trd>kasztan<klr> (ko&nacute;)</klr></trd>
        </trdgrp>
        <trd lng="ru">&c_g;&c_n;&c_je;&c_d;&c_a;&c_ja;
        &c_l;&c_o;&c_sh;&c_a;&c_d;&c_mol;</trd>
      </ekz>
      <ekz>
        alta, <tld/>a <klr>(<tld/>hara, <tld/>ha&ubreve;ta)</klr> viro
        li estas
        <fnt><bib>SatirRak</bib>,
        <lok>la fluidumo de la &ccirc;iovido</lok></fnt>.
      </ekz>
    </dif>
  </snc>
