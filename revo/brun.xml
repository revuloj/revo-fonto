<drv mrk="brun.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="brun.0ulo.hare">
    <dif>
      Brunhara homo:
      <ekz>
        la <tld/>uloj devas solidari, venki la blondulojn, kaj tio &ccirc;i
        alportos la eternan pacon
        <fnt><bib>SatirRak</bib>, <lok>Kain kaj Abel</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_a;&c_t;&c_e;&c_n;</trd>
    <trdgrp lng="br">
      <trd>rouz<klr tip="ind"> (rouzard)</klr></trd>,
      <trd>rouzard</trd>
    </trdgrp>
    <trd lng="cs">brunet</trd>
    <trd lng="nl">persoon met <ind>bruin</ind> haar</trd>
    <trd lng="pl">brunet</trd>
    <trd lng="ru">&c_sh;&c_a;&c_t;&c_je;&c_n;</trd>
  </snc>
  <snc mrk="brun.0ulo.hauxte">
    <dif>
      Brunha&ubreve;ta homo.
    </dif>
    <trd lng="be">&c_s;&c_m;&c_u;&c_g;&c_l;&c_ja;&c_k;</trd>
    <trd lng="cs">m&iacute;&scaron;enec</trd>
    <trd lng="fr">brun<klr tip="amb"> (personne brune)</klr></trd>
    <trd lng="nl">persoon met <ind>bruine</ind> huid</trd>
    <trdgrp lng="pl">
      <trd>&sacute;niady<klr> cz&lstroke;owiek</klr></trd>,
      <trd>smag&lstroke;y<klr> cz&lstroke;owiek</klr></trd>,
      <trd>ogorza&lstroke;y<klr> cz&lstroke;owiek</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_m;&c_u;&c_g;&c_l;&c_y;&c_j;<klr> (&c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_k;)</klr></trd>
  </snc>
  <trd lng="es">moreno</trd>
  <trd lng="ja">&#33394;&#x306E;&#x6D45;&#x9ED2;&#12356;&#x4EBA;  <pr>&#12356;&#x308D;&#x306E;&#12354;&#12373;&#12368;&#x308D;&#12356;&#12376;&#12435;</pr></trd>
  <trd lng="sk">brunet</trd>
</drv>

<drv mrk="brun.0ulino">
  <kap><tld/>ulino</kap>
  <snc mrk="brun.0ulino.hare">
    <dif>
      Brunhara homino:
      <ekz>
        kiaj pli pla&ccirc;as al vi &#8208; <tld/>ulinoj a&ubreve; blondulinoj?
        <fnt><bib>Revizoro</bib>,
        <lok>akto 4a, sceno 5a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_a;&c_t;&c_e;&c_n;&c_k;&c_a;</trd>
    <trdgrp lng="br">
      <trd>rouzenn</trd>,
      <trd>rouzardez</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>bruneta</trd>,
      <trd>brunetka</trd>
    </trdgrp>
    <trd lng="nl">brunette</trd>
    <trd lng="pl">brunetka</trd>
  </snc>
  <snc mrk="brun.0ulino.hauxte">
    <dif>
      Brunha&ubreve;ta homino.
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_m;&c_u;&c_g;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_ch;&c_a;&c_r;&c_n;&c_ja;&c_w;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="cs">m&iacute;&scaron;enka</trd>
    <trd lng="nl">vrouw met <ind>bruine</ind> huid</trd>
    <trdgrp lng="pl">
      <trd>&sacute;niada<klr> kobieta</klr></trd>,
      <trd>smag&lstroke;a<klr> kobieta</klr></trd>,
      <trd>ogorza&lstroke;a<klr> kobieta</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_m;&c_u;&c_g;&c_l;&c_ja;&c_n;&c_k;&c_a;</trd>
  </snc>
  <trd lng="es">morena</trd>
  <trd lng="fr">brune<klr tip="amb"> (femme brune)</klr></trd>
  <trd lng="sk">brunetka</trd>
</drv>

<drv mrk="brun.sun0o">
  <kap>sun<tld/>o</kap>
  <snc>
    <dif>
      Malhela koloro de la ha&ubreve;to, ka&ubreve;zita de elmetado al sunaj radioj:
      <ekz>
        li aspektis kvaza&ubreve; li pasigus la tagon eksterdome,
        lia ha&ubreve;to jam havis nuancon de sun<tld/>o
        <fnt><bib>Kiso</bib>, <lok>G&auml;rda</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_g;&c_a;&c_r;</trd>
  <trd lng="br">rouster</trd>
  <trd lng="cs">hn&ecaron;d&eacute; op&aacute;len&iacute; k&uring;&zcaron;e</trd>
  <trdgrp lng="de">
    <trd>Sonnenteint</trd>,
    <trd>Sonnenbr&auml;une</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>suntan</trd>
  </trdgrp>
  <trd lng="es">bronceado</trd>
  <trd lng="fr">h&acirc;le</trd>
  <trdgrp lng="hu">
    <trd>barnas&aacute;g</trd>,
    <trd>les&uuml;l&eacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#x65E5;&#x713C;&#12369;&#33394;  <pr>&#12402;&#12420;&#12369;&#12356;&#x308D;</pr></trd>
  <trd lng="nl">bruine kleur<klr tip="ind"> (door de zon)</klr></trd>
  <trd lng="pl">opalenizna</trd>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>farba pleti z&iacute;skan&aacute; p&ocirc;soben&iacute;m slnka</trd>,
    <trd>op&aacute;lenie</trd>
  </trdgrp>
  <trd lng="sv">solbr&auml;nna</trd>
  <trd lng="uk">&c_z;&c_a;&c_s;&#x43C;&c_a;&c_g;&c_a;</trd>
</drv>

<drv mrk="brun.sun0igi">
  <kap>sun<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi, ke korpa parto <tld/>i&gcirc;u per elmetado al sunaj radioj:
      <ekz>
        la maldelikataj, sunbrunigitaj viza&gcirc;oj de la
        <klr>...</klr>
        rabistoj
        <fnt><bib>I&scirc;tar</bib>,
        <lok>&ccirc;apitro 9a, aventura voja&gcirc;o</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="bronz.0i">bronzi</ref>
  </refgrp>
  <trd lng="be">&c_p;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; &c_p;&c_a;&c_d; &c_s;&c_o;&c_n;&c_c;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_z;&c_a;&c_g;&c_a;&c_r;&c_u;)</klr></trd>
  <trd lng="br">rouza&ntilde;<klr tip="ind"> (kroc'hen -
  v.k.)</klr></trd>
  <trd lng="cs">op&aacute;lit<klr> (od slunce)</klr></trd>
  <trd lng="en">tan</trd>
  <trd lng="es">broncear</trd>
  <trdgrp lng="fr">
    <trd>bronzer <klr tip="ind">(h&acirc;ler)</klr></trd>,
    <trd>h&acirc;ler</trd>
  </trdgrp>
  <trd lng="hu">lebarn&iacute;t<klr tip="ind"> (nap)</klr></trd>
  <trd lng="nl">bruinen <klr tip="ind"> (in de zon gaan
  liggen)</klr></trd>
  <trd lng="pl">opali&cacute;<klr> (na
  s&lstroke;o&nacute;cu)</klr></trd>
  <trd lng="sk">op&aacute;li&tcaron; <klr>(na slnku)</klr></trd>
</drv>

<drv mrk="brun.sun0igxi">
  <kap>sun<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Akiri sun<tld/>on:
      <ekz>
        mi sun<tld/>i&gcirc;as sur la strando.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_g;&c_a;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_s;&c_o;&c_n;&c_c;&c_y;)</klr></trd>
  <trd lng="cs">op&aacute;lit se<klr> (od slunce)</klr></trd>
  <trdgrp lng="de">
    <trd>sonnenbaden</trd>,
    <trd><klr tip="amb">in der Sonne</klr> <ind>br&auml;unen</ind></trd>
  </trdgrp>
  <trd lng="br">rouza&ntilde;<klr tip="ind"> (kroc'hen -
  v.g.)</klr></trd>
  <trd lng="ja">&#x65E5;&#x713C;&#12369;&#12377;&#x308B;  <pr>&#12402;&#12420;&#12369;&#12377;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>bruinen<klr tip="ind"> (bruin worden)</klr></trd>,
    <trd>bruin worden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>opali&cacute; si&eogonek;<klr> (na
    s&lstroke;o&nacute;cu)</klr></trd>
    <trd>ogorze&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
