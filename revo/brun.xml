<drv mrk="brun.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Havanta <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloron</ref> flave-ru&gcirc;e-nigran, kiel kafo a&ubreve;
      ka&scirc;tanoj:
      <ekz>
        malgrandaj <ind><tld/>aj okuloj</ind>
        <fnt><bib>Marta</bib></fnt>;
        <trd lng="ru">&c_k;&c_a;&c_r;&c_i;&c_je;
        &c_g;&c_l;&c_a;&c_z;&c_a;</trd>
        <trdgrp lng="pl">
          <trd>br&aogonek;zowe oczy</trd>,
          <trd>piwne oczy</trd>
        </trdgrp>
      </ekz>
      <ekz>
        homo en leda <tld/>a jako
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        rusto<tld/>a fero
        <fnt><bib>InfanTorent2</bib>,
        <lok>unua parto, &ccirc;apitro 2a</lok></fnt>;
      </ekz>
      <ekz>
        mola tapi&scirc;o el <tld/>aj pinpingloj
        <fnt><bib>InfanTorent2</bib>,
        <lok>dua parto, &ccirc;apitro 2a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>aj haroj;
      </ekz>
      <ekz>
        <ind><tld/>a &ccirc;evalo</ind>;
        <trdgrp lng="pl">
          <trd>gniadosz<klr> (ko&nacute;)</klr></trd>,
          <trd>kasztan<klr> (ko&nacute;)</klr></trd>
        </trdgrp>
        <trd lng="ru">&c_g;&c_n;&c_je;&c_d;&c_a;&c_ja;
        &c_l;&c_o;&c_sh;&c_a;&c_d;&c_mol;</trd>
      </ekz>
      <ekz>
        alta, <tld/>a <klr>(<tld/>hara, <tld/>ha&ubreve;ta)</klr> viro
        li estas
        <fnt><bib>SatirRak</bib>,
        <lok>la fluidumo de la &ccirc;iovido</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">bruin</trd>
  <trd lng="am">&#x1265;&#x1293;&#x121B;</trd>
  <trd lng="ar">&#x623;&#x633;&#x645;&#x631;</trd>
  <trd lng="az">pal&#x131;d&#x131;</trd>
  <trd lng="be">&c_k;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_je;&c_v;&c_y;</trd>
  <trd lng="bg">&c_k;&c_a;&c_f;&c_ja;&c_v;</trd>
  <trd lng="bn">&#x9AC;&#x9BE;&#x9A6;&#x9BE;&#x9AE;&#x9C0;</trd>
  <trd lng="bo">&#xF62;&#xF92;&#xFB1;&#xF0B;&#xF66;&#xFA8;&#xFB1;&#xF74;&#xF42;&#xF0B;</trd>
  <trd lng="br">rous</trd>
  <trd lng="bs">sme&#x111;e</trd>
  <trd lng="co">marrone</trd>
  <trdgrp lng="cs">
    <trd>hn&ecaron;d&yacute;</trd>,
    <trd>sn&ecaron;d&yacute;</trd>
  </trdgrp>
  <trd lng="da">brun</trd>
  <trd lng="de">braun</trd>
  <trd lng="en">brown</trd>
  <trdgrp lng="es">
    <trd>marr&oacute;n</trd>,
    <trd>caf&eacute; <klr>(lat.am.)</klr></trd>
  </trdgrp>
  <trd lng="et">pruun</trd>
  <trd lng="eu">marroi</trd>
  <trdgrp lng="fr">
    <trd>brun</trd>,
    <trd>marron</trd>
  </trdgrp>
  <trd lng="fy">br&#xFA;n</trd>
  <trd lng="gl">marr&#xF3;n</trd>
  <trd lng="gu">&#xAAC;&#xAA6;&#xABE;&#xAAE;&#xAC0;</trd>
  <trd lng="ha">launin ruwan kasa</trd>
  <trd lng="he">&#x5D7;&#x5D5;&#x5DD;</trd>
  <trd lng="hi">&#x92C;&#x94D;&#x930;&#x93E;&#x909;&#x928;</trd>
