<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kokos</rad>/o
</kap>
<drv mrk="kokos.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Grandega man&gcirc;ebla nukso de la <tld/>arbo, kies interno entenas
      dol&ccirc;an sukon.
    </dif>
  </snc>
  <trd lng="de">Kokosnu&szlig;</trd>
  <trd lng="en">coconut</trd>
  <trd lng="nl">cocosnoot</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_o;&c_s;</trd>
</drv>
<drv mrk="kokos.0arbo">
  <kap><tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="palm.0oj">palmoj</ref>
      (<trd lng="la">Cocos</trd>).
    </dif>
  </snc>
  <trd lng="de">Kokospalme</trd>
  <trd lng="en">coconut palm</trd>
  <trd lng="nl">cocospalm</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_o;&c_s;&c_o;&c_v;&c_a;&c_ja; &c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>
</drv>
</art>
