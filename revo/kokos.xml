<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kokos</rad>/o
</kap>
<drv mrk="kokos.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Grandega man&gcirc;ebla nukso de la <tld/>arbo, kies interno entenas
      dol&ccirc;an sukon.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_o;&c_s;</trd>  
  <trd lng="bg">&c_k;&c_o;&c_k;&c_o;&c_s;&c_o;&c_v; &c_o;&c_r;&c_je;&c_h;</trd> 
  <trdgrp lng="br">
    <trd>kokozenn<klr tip="ind"> (krao&ntilde;enn)</klr></trd>,
    <trd>krao&ntilde;enn-gokoz</trd>
  </trdgrp>
  <trd lng="de">Kokosnuss</trd>
  <trd lng="en">coconut</trd>
  <trdgrp lng="fr">
    <trd>coco<klr tip="ind"> (noix)</klr></trd>,
    <trd>noix de coco</trd>
  </trdgrp>
  <trd lng="hu">k&oacute;kusz(di&oacute;)</trd>
  <trd lng="nl">kokosnoot</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_o;&c_s;</trd>
</drv>

<drv mrk="kokos.0arbo">
  <kap><tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="palm.0oj">palmoj</ref>
      (<trd lng="la">Cocos</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_o;&c_s;&c_a;&c_v;&c_a;&c_ja; &c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>  
  <trd lng="bg">&c_k;&c_o;&c_k;&c_o;&c_s;&c_o;&c_v;&c_a; &c_p;&c_a;&c_l;&c_m;&c_a;</trd>   
  <trdgrp lng="br">
    <trd>gwezenn-gokoz</trd>, 
    <trd>kokozenn<klr tip="ind"> (gwezenn)</klr></trd>
  </trdgrp>
  <trd lng="de">Kokospalme</trd>
  <trd lng="en">coconut palm</trd>
  <trd lng="fr">cocotier</trd>
  <trd lng="hu">k&oacute;kuszp&aacute;lma</trd>
  <trd lng="nl">kokospalm</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_o;&c_s;&c_o;&c_v;&c_a;&c_ja;
&c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>
</drv>
</art>
