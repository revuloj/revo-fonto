<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kokos.xml,v 1.30 2023/12/03 09:58:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kokos</rad>/o
</kap>
<drv mrk="kokos.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Grandega man&gcirc;ebla <ref tip="super" cel="nuks.0o">nukso</ref>
      de la <tld/>arbo, kies interno entenas dol&ccirc;an sukon:
      <ekz>
        mi deskuis <tld/>ojn
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        butero de <tld/>oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kovrita de &ccirc;okolado, eventuale kun sursternitaj <tld/>pecetoj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010973.php">Bombo-bankroto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>raspa&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010973.php">Bombo-bankroto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a suko
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/011068.php">Franda&jcirc;oj fratinaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_o;&c_s;</trd>
  <trd lng="bg">&c_k;&c_o;&c_k;&c_o;&c_s;&c_o;&c_v; &c_o;&c_r;&c_je;&c_h;</trd>
  <trdgrp lng="br">
    <trd>kokozenn<klr tip="ind"> (krao&ntilde;enn)</klr></trd>,
    <trd>krao&ntilde;enn-gokoz</trd>
  </trdgrp>
  <trd lng="ca">coco</trd>
  <trdgrp lng="cs">
    <trd>kokosovn&iacute;k</trd>,
    <trd>kokosovn&iacute;k o&rcaron;echoplod&yacute;</trd>
  </trdgrp>
  <trd lng="de">Kokosnuss</trd>
  <trd lng="en">coconut</trd>
  <trd lng="es">coco</trd>
  <trdgrp lng="fr">
    <trd>coco<klr tip="ind"> (noix)</klr></trd>,
    <trd>noix de coco</trd>
  </trdgrp>
  <trd lng="hu">k&oacute;kusz(di&oacute;)</trd>
  <trd lng="id">kelapa</trd>
  <trdgrp lng="ja">
    <trd>&#x30E4;&#x30B7;&#x306E;&#x5B9F;  <pr>&#x30E4;&#x30B7;&#x306E;&#x307F;</pr></trd>,
    <trd>&#x30B3;&#x30B3;&#x30CA;&#x30C3;&#x30C4;</trd>
  </trdgrp>
  <trd lng="nl">kokosnoot</trd>
  <trdgrp lng="pl">
    <trd>kokos</trd>,
    <trd>orzech kokosowy</trd>
  </trdgrp>
  <trd lng="ro">nuc&abreve; de cocos</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_o;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>kokos</trd>,
    <trd>kokosov&yacute; orech</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6930;&#x5B50; <pr>y&#x113;zi</pr></trd>,
    <trd>&#x6930;&#x5B50; <pr>y&#xE9;zi</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kokos.0arbo">
  <kap><tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="malprt" cel="palm.0oj">palmoj</ref>
      (<trd lng="la">Cocos</trd>):
      <ekz>
        &ccirc;efa koridoro de amuzlokoj en Fortalezo <klr>[&#x2026;]</klr> kun
        homplena promenejo,
        trinkejoj, tendoj, <tld/>arboj kaj imponaj edificoj la&ubreve; la bordo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1177">Fortalezo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/20/Coconuttree.JPG/640px-Coconuttree.JPG" prm="CC-BY-SA-3.0"><tld/>arbo
    (Cocos nucifera)
        <fnt>
          <lok><url ref="&WCW;/File:Coconuttree.JPG">&WCW;/File:Coconuttree.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_o;&c_s;&c_a;&c_v;&c_a;&c_ja; &c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>
  <trd lng="bg">&c_k;&c_o;&c_k;&c_o;&c_s;&c_o;&c_v;&c_a; &c_p;&c_a;&c_l;&c_m;&c_a;</trd>
  <trdgrp lng="br">
    <trd>gwezenn-gokoz</trd>,
    <trd>kokozenn<klr tip="ind"> (gwezenn)</klr></trd>
  </trdgrp>
  <trd lng="ca">cocoter</trd>
  <trd lng="cs">kokosov&aacute; palma</trd>
  <trd lng="de">Kokospalme</trd>
  <trd lng="en">coconut palm</trd>
  <trd lng="es">cocotero</trd>
  <trd lng="fr">cocotier</trd>
  <trd lng="hu">k&oacute;kuszp&aacute;lma</trd>
  <trd lng="id"><ind>pohon</ind> kelapa</trd>
  <trd lng="ja">&#x30B3;&#x30B3;&#x30E4;&#x30B7;</trd>
  <trd lng="nl">kokospalm</trd>
  <trd lng="pl">palma kokosowa</trd>
  <trd lng="ro">palmele nuc&abreve; de cocos </trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_o;&c_s;&c_o;&c_v;&c_a;&c_ja;
&c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>
  <trd lng="sk">kokosovn&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43A;&#x43E;&c_s;</trd>,
    <trd>&#x43A;&#x43E;&#x43A;&#x43E;&c_s;&#x43E;&c_v;&c_a; &#x43F;&c_a;&#x43B;&#x44C;&#x43C;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6930; <pr>y&#x113;</pr></trd>,
    <trd>&#x6930;&#x5B50; <pr>y&#x113;zi</pr></trd>,
    <trd>&#x6930; <pr>y&#xE9;</pr></trd>,
    <trd>&#x6930;&#x5B50; <pr>y&#xE9;zi</pr></trd>,
    <trd>&#x6930;&#x5B50;&#x6811; <pr>y&#x113;zish&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
