<drv mrk="kviet.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kviet.0igxi.KOMUNE">
    <dif>
      I&gcirc;i malpli vigla, malpli impeta, pelema a&ubreve; ekscitita:
      <ekz>
        la akvo <tld/>i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 8:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;os la furiozo de via frato
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 27:44</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia doloro ne <tld/>i&gcirc;os
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 16:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ventoj <tld/>i&gcirc;is, kvaza&ubreve; ili volus dormi
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>calmarse</trd>,
    <trd>aquietarse</trd>,
    <trd>quietarse</trd>
    <trd>sosegarse</trd>
  </trdgrp>
  <trd lng="fr">se <ind>calmer</ind></trd>
  <trdgrp lng="pl">
    <trd>och&lstroke;on&aogonek;&cacute;</trd>,
    <trd>wycisza&cacute; si&eogonek;</trd>,
    <trd>zel&zdot;e&cacute;</trd>,
    <trd>zes&lstroke;abn&aogonek;&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>restabili</trd>,
    <trd>mutul</trd>,
    <trd>lini&scedil;ti</trd>
  </trdgrp>
</drv>

<drv mrk="kviet.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="kviet.0ismo.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <ref tip="vid" cel="mistik.0ismo">Mistikismo</ref>,
      kiu celas neniigon de sia volo en Dion, kaj vivon de pasiva
      medito, precipe tia kristana movado de la 17a jarcento:
      <ekz>
        sinjorino Guyon estis franca mistikulino kaj &ccirc;efa priparolanto de <tld/>ismo
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Jeanne_Marie_Bouvier_de_la_Motte_Guyon">Jeanne Marie Bouvier de la Motte Guyon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kvietizmus</trd>
  <trd lng="de">Quietismus</trd>
  <trd lng="es">quietismo</trd>
  <trd lng="fr">qui&eacute;tisme</trd>
  <trd lng="hu">kvietizmus</trd>
  <trd lng="nl">quietisme</trd>
  <trd lng="pl">kwietyzm</trd>     
  <trd lng="ro">chietism</trd>
  <trd lng="ru">&c_k;&c_v;&c_i;&c_je;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">kvietizmus</trd>
</drv>

<drv mrk="kviet.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="kviet.mal0a.KOMUNE">
    <dif>
      Ekscitita, movi&gcirc;ema, malmodera, kolerema:
      <ekz>
        la malpiuloj estas kiel mal<tld/>a maro, kiu ne povas
        trankvili&gcirc;i
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 57:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a vento
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 1:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maro estas iom mal<tld/>a kaj la fi&scirc;kaptistoj direktas sin kun siaj &scirc;ipoj
        hejmen
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_s;&c_p;&c_a;&c_k;&c_o;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_t;&c_r;&c_y;&c_v;&c_o;&c_zh;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">neklidn&yacute;</trd>
  <trdgrp lng="de">
    <trd>unruhig</trd>,
    <trd>aufgeregt</trd>,
    <trd>aufgew&uuml;hlt</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>inquieto</trd>,
    <trd>excitado</trd>,
    <trd>intranquilo</trd>,
    <trd>perturbado</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>agit&eacute;</trd>,
    <trd>troubl&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyugtalan</trd>,
    <trd>izgatott</trd>,
    <trd>h&aacute;borg&oacute;</trd>
  </trdgrp>
  <trd lng="nl">onrustig</trd>
  <trd lng="pl">niespokojny</trd>
  <trdgrp lng="ro">
    <trd>agitat&abreve;</trd>,
    <trd>nelini&#x219;tit</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_v;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">nepokojn&yacute;</trd>
</drv>
