<drv mrk="kviet.0o">
  <kap><tld/>o,
  <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="kviet.0o.paco">
    <dif>
      Stato de io a&ubreve; iu <tld/>a, paca:
      <ekz>
        mi ne havas <tld/>on, mi ne havas ripozon, trafis min kolero
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 3:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        tia <tld/>o trankviligas kaj igas son&gcirc;i
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia tuta pasinteco estis <tld/>o, amo kaj amuzi&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>eco kaj fido estus via forto
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 30:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        meditiga <tld/>eco
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        veran animan <tld/>econ li ne atingis, &ccirc;ar nun venis la malnovaj rememoroj
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Ruhe</trd>,
      <trd>Gem&uuml;tsruhe</trd>,
      <trd>Gelassenheit</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>rest</trd>,
      <trd>restfulness</trd>
    </trdgrp>
    <trd lng="fr">calme<klr> (subst.)</klr></trd>
    <trdgrp lng="nl">
      <trd>rust</trd>,
      <trd>kalmte</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
      <trd>&c_p;&c_o;&c_k;&c_o;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>quietud</trd>,
      <trd>sosiego</trd>,
      <trd>tranquilidad</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>aman</baz>keamanan</trd>,
      <trd><baz>damai</baz>kedamaian</trd>,
      <trd><baz>karar</baz>kekararan</trd>,
      <trd><baz>tenang</baz>ketenangan</trd>,
      <trd><baz>tenteram</baz>ketenteraman</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#37806;&#38748; <pr>zh&egrave;nj&igrave;ng</pr></trd>,
    <trd>&#38215;&#38745; <pr>zh&egrave;nj&igrave;ng</pr></trd>,
    <trd>&#24179;&#24120;&#24515; <pr>p&iacute;ngch&aacute;ngx&imacron;n</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="kviet.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Stato de objekto, kiam &gcirc;i havas nulan rapidon rilate al iu
      bazo:
      <ekz>
        <ind><tld/>maso</ind> <klr>(maso de objekto en stato de
        <tld/>o)</klr>
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Diraka_ekvacio">Diraka ekvacio</url></vrk>
        </fnt>.
        <trd lng="de">Ruhemasse</trd>
        <trd lng="en">rest mass</trd>
        <trd lng="id"><baz>massa</baz>massa diam</trd>
        <trd lng="ru">&c_m;&c_a;&c_s;&c_s;&c_a;
        &c_p;&c_o;&c_k;&c_o;&c_ja;</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Ruhe</trd>,
      <trd>Bewegungslosigkeit</trd>
    </trdgrp>
    <trd lng="es">reposo <klr>(estado de)</klr></trd>
    <trd lng="en">rest</trd>
    <trd lng="fr">repos<klr> (immobilit&eacute;)</klr></trd>
    <trd lng="id">diam<klr tip="amb"> (keadaan materi)</klr></trd>
    <trdgrp lng="nl">
