<drv mrk="kviet.0o">
  <kap><tld/>o,
  <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="kviet.0o.paco">
    <dif>
      Stato de io a&ubreve; iu <tld/>a, paca:
      <ekz>
        mi ne havas <tld/>on, mi ne havas ripozon, trafis min kolero
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 3:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        tia <tld/>o trankviligas kaj igas son&gcirc;i
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia tuta pasinteco estis <tld/>o, amo kaj amuzi&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>eco kaj fido estus via forto
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 30:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        meditiga <tld/>eco
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        veran animan <tld/>econ li ne atingis, &ccirc;ar nun venis la malnovaj rememoroj
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Ruhe</trd>,
      <trd>Gem&uuml;tsruhe</trd>,
      <trd>Gelassenheit</trd>
    </trdgrp>
    <trd lng="fr">calme<klr> (subst.)</klr></trd>
    <trdgrp lng="nl">
      <trd>rust</trd>,
      <trd>kalmte</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_p;&c_o;&c_k;&c_o;&c_j;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
      <trd>&c_p;&c_o;&c_k;&c_o;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>quietud</trd>,
      <trd>sosiego</trd>,
      <trd>tranquilidad</trd>
    </trdgrp>
  </snc>
  <snc mrk="kviet.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Stato de objekto, kiam &gcirc;i havas nulan rapidon rilate al iu
      bazo:
      <ekz>
        <ind><tld/>maso</ind> <klr>(maso de objekto en stato de
        <tld/>o)</klr>
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Diraka_ekvacio">Diraka ekvacio</url></vrk>
        </fnt>.
        <trd lng="de">Ruhemasse</trd>
        <trd lng="en">rest mass</trd>
        <trd lng="ru">&c_m;&c_a;&c_s;&c_s;&c_a;
        &c_p;&c_o;&c_k;&c_o;&c_ja;</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Ruhe</trd>,
      <trd>Bewegungslosigkeit</trd>
    </trdgrp>
    <trd lng="es">reposo <klr>(estado de)</klr></trd>
    <trd lng="en">rest</trd>
    <trd lng="fr">repos<klr> (immobilit&eacute;)</klr></trd>
    <trdgrp lng="nl">
      <trd>rust</trd>,
      <trd>bewegingloosheid</trd>,
      <trd>roerloosheid</trd>,
      <trd>stilstand</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_k;&c_o;&c_j;</trd>,
      <trd>&c_s;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_n;&c_i;&c_je;
      &c_p;&c_o;&c_k;&c_o;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_k;&c_o;&c_j;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_a;&c_k;&c_o;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>klid</trd>,
    <trd>klidnost</trd>,
    <trd>poklid</trd>,
    <trd>usm&iacute;&rcaron;enost</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>nyugalom</trd>,
    <trd>nyugalmi &aacute;llapot</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bezruch</trd>,
    <trd>cicho&sacute;&cacute;</trd>,
    <trd>spok&oacute;j</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>imobilizat</trd>,
    <trd>lini&#x219;te</trd>,
    <trd>senin&abreve;tate</trd>
  </trdgrp>
  <trd lng="sk">pokoj <klr>(stav du&scaron;e)</klr></trd>
</drv>

<drv mrk="kviet.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kviet.0igi.KOMUNE">
    <dif>
      Igi iun a&ubreve; ion <tld/>a:
      <ekz>
        <tld/>igi la malsaton a&ubreve; soifon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        du gardistoj <klr>[&#x2026;]</klr> <tld/>igis la malkonsentojn inter la malri&ccirc;aj
        fremduloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        pacienculo <tld/>igas disputon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 15:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        milda respondo <tld/>igas koleron
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 15:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia spirito, <tld/>igita de trankvileco kaj vigligita
        de espero, fortigis la <klr>[&#x2026;]</klr> korpon
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sovagx.mal0igi.KOMUNE">malsova&gcirc;igi</ref>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_p;&c_a;&c_k;&c_o;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_t;&c_a;&c_l;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_d;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ja;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>bes&auml;nftigen</trd>,
      <trd>befrieden</trd>,
      <trd>beruhigen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>calmar</trd>,
      <trd>aquietar</trd>,
      <trd>quietar</trd>,
      <trd>sosegar</trd>
    </trdgrp>
    <trd lng="fr">calmer</trd>
    <trdgrp lng="hu">
      <trd>nyugtat</trd>,
      <trd>csillap&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kalmeren</trd>,
      <trd>stillen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_n;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_t;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_u;&c_s;&c_p;&c_o;&c_k;&c_o;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>konej&scaron;it</trd>,
    <trd>m&iacute;rnit <klr>(nap&ecaron;t&iacute;)</klr></trd>,
