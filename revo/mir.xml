<drv mrk="mir.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="mir.0ebla.KOMUNE">
    <dif>
      Tre <ref tip="vid" cel="surpri.0a">surpriza</ref>,
      pri kiu oni povas <tld/>i:
      <ekz>
        ne estas <tld/>eble, ke sur malbone flegita trunketo nepre
        aperis krea&jcirc;o parazita
        <fnt>
          <aut>Leono Zamenhof, trad. L. Dreher</aut>
          <vrk>Troabundo de Inteligentularo</vrk>
          <lok>Pola Esperantisto, 1934, p. 151a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>erstaunlich</trd>,
    <trd>verwunderlich</trd>
  </trdgrp>
  <trd lng="es">sorprendente</trd>
  <trd lng="fr">&eacute;tonnant</trd>
  <trdgrp lng="zh">
    <trd>&#x4EE4;&#x4EBA;&#x60CA;&#x8BB6; <pr>l&#xEC;ngr&#xE9;nj&#x12B;ngy&#xE0;</pr></trd>,
    <trd>&#x4EE4;&#x4EBA;&#x9A5A;&#x8A1D; <pr>l&#xEC;ngr&#xE9;nj&#x12B;ngy&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mir.0egi">
  <kap><tld/>egi</kap>
  <snc>
    <dif>
      Forte <tld/>i, senti da&ubreve;ran surprizi&gcirc;on:
      <ekz>
        &ccirc;iuj, kiuj a&ubreve;dis lin, <tld/>egis pro lia kompreno kaj liaj
        respondoj
        <fnt><bib>NT</bib>, <lok>&Luk; 2:47</lok></fnt>;
      </ekz>
      <ekz>
        pri &ccirc;i tiu domo, kiu estas tre alta, &ccirc;iu preteriranto <tld/>egos kaj fajfos
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 9:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        li povis <klr>[&#x2026;]</klr> rakonti al &scirc;i tiajn aferojn, ke &scirc;i forte <tld/>egis
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la infanoj <tld/>egis, vidante &hcirc;emiajn eksperimentojn, kiuj &scirc;ajnis al ili tiel
        nekompreneblaj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frap.0i.mirigi">frapi<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>velmi se divit</trd>,
    <trd>&zcaron;asnout</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>be stupefied</trd>
  </trdgrp>
  <trd lng="es">quedar <ind>asombrado</ind></trd>
  <trd lng="fr">s'<ind>&eacute;tonner</ind></trd>
  <trd lng="id"><baz>pesona</baz>terpesona</trd>
  <trdgrp lng="ja">
    <trd>&#x3073;&#x3063;&#x304F;&#x308A;&#x4EF0;&#x5929;&#x3059;&#x308B; 
    <pr>&#x3073;&#x3063;&#x304F;&#x308A;&#x304E;&#x3087;&#x3046;&#x3066;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5516;&#x7136;&#x3068;&#x3059;&#x308B;  <pr>&#x3042;&#x305C;&#x3093;&#x3068;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">by&cacute; <ind>zdumionym</ind></trd>
  <trd lng="ru">&c_i;&c_z;&c_u;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>ve&lcaron;mi obdivova&tcaron; nie&ccaron;o</trd>,
    <trd>vytre&scaron;ti&tcaron; o&ccaron;i</trd>,
    <trd>&zcaron;asn&uacute;&tcaron; nad nie&ccaron;&iacute;m</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5403;&#x60CA; <pr>ch&#x12B;j&#x12B;ng</pr></trd>,
    <trd>&#x5403;&#x9A5A; <pr>ch&#x12B;j&#x12B;ng</pr></trd>,
    <trd>&#x8A1D;&#x7570; <pr>y&#xE0;y&#xEC;</pr></trd>,
    <trd>&#x8BB6;&#x5F02; <pr>y&#xE0;y&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mir.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Tre forta <tld/>o:
      <ekz>
        ili sentis grandan <tld/>egon, &ccirc;ar ili ne komprenis pri <klr>[&#x2026;]</klr>
        <fnt>
          <bib>NT</bib>
          <lok>Marko 6:51</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun granda <tld/>ego li ekvidis, ke li trovi&gcirc;as jam alte super la nuboj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io tio &ccirc;i por ok dolaroj semajne! mi esprimis mian <tld/>egon
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la hejmo de la
        metiisto</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Dagon</nom> &scirc;toni&gcirc;is de <tld/>ego kaj rigardis &scirc;in per lar&gcirc;e malfermitaj okuloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la cirko ekregis <tld/>ego, neniu kura&gcirc;is krii a&ubreve; apla&ubreve;di
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>.
      </ekz>
