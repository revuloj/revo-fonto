  <trd lng="te">&#xC06;&#xC36;&#xC4D;&#xC1A;&#xC30;&#xC4D;&#xC2F;&#xC3E;&#xC28;&#xC3F;&#xC15;&#xC3F;</trd>
  <trd lng="th">&#xE44;&#xE21;&#xE48;&#xE19;&#xE48;&#xE32;&#xE41;&#xE1B;&#xE25;&#xE01;&#xE43;&#xE08;</trd>
  <trd lng="ur">&#x62D;&#x6CC;&#x631;&#x62A; &#x6C1;&#x6D2;</trd>
  <trd lng="vi">t&#x1EF1; h&#x1ECF;i</trd>
  <trd lng="yi">&#x5D5;&#x5D5;&#x5D5;&#x5E0;&#x5D3;&#x5E2;&#x5E8;</trd>
  <trd lng="zu">bayazibuza</trd>
</drv>

<drv mrk="mir.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="mir.0ebla.KOMUNE">
    <dif>
      Tre <ref tip="vid" cel="surpri.0a">surpriza</ref>,
      pri kiu oni povas <tld/>i:
      <ekz>
        ne estas <tld/>eble, ke sur malbone flegita trunketo nepre 
        aperis krea&jcirc;o parazita
        <fnt>
          <aut>Leono Zamenhof, trad. L. Dreher</aut>
          <vrk>Troabundo de Inteligentularo</vrk>
          <lok>Pola Esperantisto, 1934, p. 151a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">sorprendente</trd>
  <trd lng="fr">&eacute;tonnant</trd>
</drv>

<drv mrk="mir.0egi">
  <kap><tld/>egi</kap>
  <snc>
    <dif>
      Forte <tld/>i, senti da&ubreve;ran surprizi&gcirc;on:
      <ekz>
        &ccirc;iuj, kiuj a&ubreve;dis lin, <tld/>egis pro lia kompreno kaj liaj
        respondoj
        <fnt><bib>NT</bib>, <lok>&Luk; 2:47</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frap.0i.mirigi">frapi <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>velmi se divit</trd>,
    <trd>&zcaron;asnout</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>be stupefied</trd>
  </trdgrp>
  <trd lng="es">quedar <ind>asombrado</ind></trd>
  <trd lng="fr">s'<ind>&eacute;tonner</ind></trd>
  <trd lng="pl">by&cacute; <ind>zdumionym</ind></trd>
  <trd lng="ru">&c_i;&c_z;&c_u;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>ve&lcaron;mi obdivova&tcaron; nie&ccaron;o</trd>,
    <trd>vytre&scaron;ti&tcaron; o&ccaron;i</trd>,
    <trd>&zcaron;asn&uacute;&tcaron; nad nie&ccaron;&iacute;m</trd>
  </trdgrp>
</drv>

<drv mrk="mir.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Tre forta <tld/>o:
      <ekz>
        &ccirc;io tio &ccirc;i por ok dolaroj semajne! Mi esprimis mian <tld/>egon
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la hejmo de la
        metiisto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>&uacute;div</trd>,
    <trd>&uacute;&zcaron;as</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stupefaction</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>estupefacci&oacute;n</trd>,
    <trd>asombro</trd>
  </trdgrp>
  <trd lng="fr">&eacute;tonnement</trd>
  <trd lng="pl">zdumienie</trd>
  <trd lng="ru">&c_i;&c_z;&c_u;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">&uacute;&zcaron;as</trd>
</drv>

<drv mrk="mir.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on al iu:
      <ekz>
        &ccirc;iuj estis senfine <tld/>igitaj, la instruitulo e&ccirc; eksaltis
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ib;&c_v;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>budit podiv</trd>,
    <trd>udivit</trd>,
    <trd>udivovat</trd>
  </trdgrp>
  <trd lng="de">verwundern</trd>
  <trdgrp lng="en">
    <trd>amaze</trd>,
    <trd>astonish</trd>
  </trdgrp>
  <trd lng="es">maravillar</trd>
  <trd lng="fr">&eacute;merveiller</trd>
  <trdgrp lng="hu">
    <trd>csod&aacute;lkoz&aacute;sra k&eacute;sztet</trd>,
    <trd>&aacute;mulatba ejt</trd>
  </trdgrp>
  <trd lng="nl">verwonderen</trd>
  <trd lng="pl">zadziwia&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_i;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prekvapi&tcaron;</trd>,
    <trd>udivi&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="mir.0inda">
  <kap><tld/>inda</kap>
  <snc>
    <dif>
      Meritanta <tld/>on:
      <ekz>
        pluraj amatoraj lingvistoj posedas en siaj tirkestoj
        <tld/>indajn
        projektojn
        <fnt><bib>Lanti</bib>, <lok>nia pozicio</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>inda&jcirc;oj de la scienco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mirakl">mirakla</ref>
    </refgrp>
  </snc>
  <trd lng="bo">&#xF67;&#xF0B;&#xF63;&#xF66;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>obdivuhodn&yacute;</trd>,
    <trd>podivuhodn&yacute;</trd>,
    <trd>&uacute;&zcaron;asn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wundersam</trd>,
    <trd>wunderbar</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>wonderful</trd>,
    <trd>amazing</trd>,
    <trd>astonishing</trd>
  </trdgrp>
  <trd lng="es">maravilloso</trd>
  <trd lng="fr">merveilleux</trd>
  <trd lng="hu">csod&aacute;latos</trd>
  <trd lng="nl">wonderlijk</trd>
  <trd lng="pl">dziwny</trd>
  <trd lng="ru">&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">&uacute;&zcaron;asn&yacute;</trd>
  <trd lng="ms">menakjubkan</trd>
</drv>
</art>
