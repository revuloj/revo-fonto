<drv mrk="mir.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="impres.0o.gxenerale">Impreso</ref> sentita de tiu, kiu <tld/>as:
      <ekz>
        kion mi vidas! diris &scirc;ia patrino kun grandega <tld/>o
        <fnt>
          <bib>F</bib>
          <lok>&FE; 17</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun <tld/>o ili rigardas unu alian
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 13:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o kaptis lin
        <fnt>
          <bib>NT</bib>
          <lok>Luko 5:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ekvidis kun <tld/>o, ke la tombo estas ornamita
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili kriegis de &gcirc;ojo kaj <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur &scirc;ia viza&gcirc;o aperis <tld/>o kaj <ref cel="embara.0o.FIG">embaraso</ref>
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        senmovigita de <tld/>o kaj de <ref cel="admir.0o">admiro</ref>
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
