<drv mrk="mir.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="mir.0ebla.KOMUNE">
    <dif>
      Tre <ref tip="vid" cel="surpri.0a">surpriza</ref>,
      pri kiu oni povas <tld/>i:
      <ekz>
        ne estas <tld/>eble, ke sur malbone flegita trunketo nepre 
        aperis krea&jcirc;o parazita
        <fnt>
          <aut>Leono Zamenhof, trad. L. Dreher</aut>
          <vrk>Troabundo de Inteligentularo</vrk>
          <lok>Pola Esperantisto, 1934, p. 151a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>erstaunlich</trd>,
    <trd>verwunderlich</trd>
  </trdgrp>
  <trd lng="es">sorprendente</trd>
  <trd lng="fr">&eacute;tonnant</trd>
</drv>

<drv mrk="mir.0egi">
  <kap><tld/>egi</kap>
  <snc>
    <dif>
      Forte <tld/>i, senti da&ubreve;ran surprizi&gcirc;on:
      <ekz>
        &ccirc;iuj, kiuj a&ubreve;dis lin, <tld/>egis pro lia kompreno kaj liaj
        respondoj
        <fnt><bib>NT</bib>, <lok>&Luk; 2:47</lok></fnt>;
      </ekz>
      <ekz>
        pri &ccirc;i tiu domo, kiu estas tre alta, &ccirc;iu preteriranto <tld/>egos kaj fajfos
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 9:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        li povis <klr>[&#8230;]</klr> rakonti al &scirc;i tiajn aferojn, ke &scirc;i forte <tld/>egis
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la infanoj <tld/>egis, vidante &hcirc;emiajn eksperimentojn, kiuj &scirc;ajnis al ili tiel
        nekompreneblaj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frap.0i.mirigi">frapi<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>velmi se divit</trd>,
    <trd>&zcaron;asnout</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>be stupefied</trd>
  </trdgrp>
  <trd lng="es">quedar <ind>asombrado</ind></trd>
  <trd lng="fr">s'<ind>&eacute;tonner</ind></trd>
  <trd lng="pl">by&cacute; <ind>zdumionym</ind></trd>
  <trd lng="ru">&c_i;&c_z;&c_u;&c_m;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>ve&lcaron;mi obdivova&tcaron; nie&ccaron;o</trd>,
    <trd>vytre&scaron;ti&tcaron; o&ccaron;i</trd>,
    <trd>&zcaron;asn&uacute;&tcaron; nad nie&ccaron;&iacute;m</trd>
  </trdgrp>
</drv>

<drv mrk="mir.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Tre forta <tld/>o:
      <ekz>
        ili sentis grandan <tld/>egon, &ccirc;ar ili ne komprenis pri <klr>[&#8230;]</klr>
        <fnt>
          <bib>NT</bib>
          <lok>Marko 6:51</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun granda <tld/>ego li ekvidis, ke li trovi&gcirc;as jam alte super la nuboj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io tio &ccirc;i por ok dolaroj semajne! mi esprimis mian <tld/>egon
        <fnt><bib>FK</bib>, <lok>4, rakontoj, la hejmo de la
        metiisto</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Dagon</nom> &scirc;toni&gcirc;is de <tld/>ego kaj rigardis &scirc;in per lar&gcirc;e malfermitaj okuloj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la cirko ekregis <tld/>ego, neniu kura&gcirc;is krii a&ubreve; apla&ubreve;di
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>&uacute;div</trd>,
    <trd>&uacute;&zcaron;as</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stupefaction</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>estupefacci&oacute;n</trd>,
    <trd>asombro</trd>
  </trdgrp>
  <trd lng="fr">&eacute;tonnement</trd>
  <trd lng="pl">zdumienie</trd>
  <trd lng="ru">&c_i;&c_z;&c_u;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">&uacute;&zcaron;as</trd>
</drv>

<drv mrk="mir.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on al iu:
      <ekz>
        &ccirc;iuj estis senfine <tld/>igitaj, la instruitulo e&ccirc; eksaltis
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>igis ilin, <klr>[&#8230;]</klr> ke ili facile komprenis la lingvon
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ne komprenis liajn vortojn, sed la tono <tld/>igis ilin
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igas min via indiferenteco interrompis la plej alta ju&gcirc;isto
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la reguloj de la konveneco estis plenumitaj, sed la sciigo iom <tld/>igis &scirc;in
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la neatendita demando iom <tld/>igis
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konste1.0i">konsterni</ref>,
      <ref cel="surpri.0i.KOMUNE">surprizi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ib;&c_v;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>budit podiv</trd>,
    <trd>udivit</trd>,
    <trd>udivovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verwundern</trd>,
    <trd>verbl&uuml;ffen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amaze</trd>,
    <trd>astonish</trd>
  </trdgrp>
  <trd lng="es">maravillar</trd>
