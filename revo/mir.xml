<kap>
  <ofc>*</ofc>
  <rad>mir</rad>/i
</kap>
<drv mrk="mir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Senti impreson de surprizo pro io stranga, neatendita, nekutima,
      eksterordinara:
      <ekz>
        <tld/>i pri fremdaj kutimoj, pri akrobata&jcirc;o;
      </ekz>
      <ekz>
        <tld/>i ion;
      </ekz>
      <ekz>
        mi <tld/>as, ke li ankora&ubreve; ne respondis;
      </ekz>
      <ekz>
        mi <tld/>is scii&gcirc;ante, ke...
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="admir.0i">admiri</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ib;&c_v;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">divit se</trd>
  <trdgrp lng="de">
    <trd>sich <ind>wundern</ind></trd>,
    <trd>staunen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>marvel</trd>,
    <trd>be amazed</trd>,
    <trd>be astonished</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>admirar</ind>se</trd>,
    <trd><ind>asombrar</ind>se</trd>,
    <trd><ind>maravillar</ind>se</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;merveiller</ind></trd>,
    <trd>&ecirc;tre <ind>surpris</ind></trd>
  </trdgrp>
  <trd lng="hu">csod&aacute;lkozik</trd>
  <trd lng="nl">zich <ind>verwonderen</ind></trd>
  <trdgrp lng="pl">
    <trd>dziwi&cacute; si&eogonek;</trd>,
    <trd>by&cacute; <ind>zdziwionym</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_d;&c_i;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_d;&c_i;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>divi&tcaron; sa</trd>,
    <trd>&ccaron;udova&tcaron; sa</trd>
  </trdgrp>
</drv>

<drv mrk="mir.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Impreso sentita de tiu, kiu <tld/>as:
      <ekz>
        mutigita de <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">wonder</trd>
  <trd lng="am">&#x12ED;&#x1308;&#x122D;&#x121B;&#x120D;</trd>
  <trd lng="ar">&#x639;&#x62C;&#x628;</trd>
  <trd lng="az">g&#xF6;r&#x259;s&#x259;n</trd>
  <trd lng="be">&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bn">&#x986;&#x9B6;&#x9CD;&#x99A;&#x9B0;&#x9CD;&#x9AF;&#x9C7;&#x9B0;</trd>
  <trd lng="bs">pitam</trd>
  <trd lng="co">c&#x2019;avissi</trd>
  <trdgrp lng="cs">
    <trd>div</trd>,
    <trd>mirabilium</trd>,
    <trd>podivnost</trd>
  </trdgrp>
  <trd lng="da">spekulerer</trd>
  <trd lng="de">Verwunderung</trd>
  <trdgrp lng="en">
    <trd>wonder</trd>,
    <trd>amazement</trd>,
    <trd>astonishment</trd>
  </trdgrp>
  <trd lng="es">maravilla</trd>
  <trd lng="et">huvitav</trd>
  <trd lng="eu">galdetzen</trd>
  <trdgrp lng="fr">
    <trd>&eacute;merveillement</trd>,
    <trd>surprise</trd>
  </trdgrp>
  <trd lng="fy">w&#xFB;nder</trd>
  <trd lng="gl">pregunto</trd>
  <trd lng="gu">&#xA86;&#xAB6;&#xACD;&#xA9A;&#xAB0;&#xACD;&#xAAF;</trd>
  <trd lng="ha">mamaki</trd>
  <trd lng="hi">&#x906;&#x936;&#x94D;&#x91A;&#x930;&#x94D;&#x92F;</trd>
  <trd lng="hr">pitam</trd>
  <trd lng="hu">csod&aacute;lkoz&aacute;s</trd>
  <trd lng="hy">&#x566;&#x561;&#x580;&#x574;&#x561;&#x576;&#x578;&#x582;&#x574;</trd>
  <trd lng="is">fur&#xF0;a</trd>
  <trd lng="ja">&#x4E0D;&#x601D;&#x8B70;</trd>
