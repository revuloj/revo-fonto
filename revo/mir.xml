  <trd lng="km">&#x1786;&#x17D2;&#x1784;&#x179B;&#x17CB;</trd>
  <trd lng="ko">&#xAD81;&#xAE08;</trd>
  <trd lng="lo">&#xEAA;&#xEB4;&#xEC8;&#xE87;&#xEA1;&#xEB0;&#xEAB;&#xEB1;&#xE94;</trd>
  <trd lng="ml">&#xD35;&#xD3F;&#xD38;&#xD4D;&#xD2E;&#xD2F;&#xD02;</trd>
  <trd lng="mr">&#x906;&#x936;&#x94D;&#x91A;&#x930;&#x94D;&#x92F;</trd>
  <trd lng="my">&#x1021;&#x1036;&#x1037;&#x1018;&#x103D;&#x101A;&#x103A;&#x101E;&#x1031;&#x102C;&#x1021;&#x1019;&#x103E;&#x102F;</trd>
  <trd lng="ne">&#x906;&#x936;&#x94D;&#x91A;&#x930;&#x94D;&#x92F;</trd>
  <trd lng="nl">verwondering</trd>
  <trd lng="ny">ndikudabwa</trd>
  <trd lng="pa">&#xA39;&#xA48;&#xA30;&#xA3E;&#xA28;</trd>
  <trd lng="pl">zdziwienie</trd>
  <trd lng="ps">&#x62D;&#x64A;&#x631;&#x627;&#x646;</trd>
  <trd lng="ru">&c_u;&c_d;&c_i;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
