    <trd>&#x9A5A;&#x5606;&#x3059;&#x308B;  <pr>&#x304D;&#x3087;&#x3046;&#x305F;&#x3093;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">zich <ind>verwonderen</ind></trd>
  <trdgrp lng="pl">
