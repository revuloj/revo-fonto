<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc>
<rad>girland</rad>/o</kap>
<drv mrk="girlan.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ornama kunplektaĵo el floroj kaj folioj, en formo de kurba ĉeno:
      <ekz>
        <tld/>oj da artaj rozoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="feston.0o">festono</ref>.
    </refgrp>
  </snc>
  <trd lng="be">гірлянда</trd>
  <trdgrp lng="de">
    <trd>Girlande</trd>,
    <trd>Blumengewinde</trd>,
    <trd>Laubgewinde</trd>
  </trdgrp>
  <trd lng="en">garland</trd>
  <trd lng="fr">guirlande</trd>
  <trd lng="es">guirnalda</trd>
  <trdgrp lng="nl">
    <trd>guirlande</trd>,
    <trd>slinger</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>coroa<klr tip="amb"> (de folhas e flores)</klr></trd>,
   <trd>grinalda</trd>
  </trdgrp>
  <trd lng="ru">гирлянда</trd>
</drv>
</art>
