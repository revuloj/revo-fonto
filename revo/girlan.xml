<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc>
  <rad>girland</rad>/o
</kap>
<drv mrk="girlan.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ornama kunplekta&jcirc;o el floroj kaj folioj, en formo de kurba &ccirc;eno:
      <ekz>
        <tld/>oj da artaj rozoj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="feston.0o">festono</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Girlande</trd>,
    <trd>Blumengewinde</trd>,
    <trd>Laubgewinde</trd>
  </trdgrp>
  <trd lng="en">garland</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_l;&a_b;&a_n;&a_d;</trd>,
    <trd>&a_t;&a_A;&a_j; &f_g;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">guirlande</trd>
  <trd lng="es">guirnalda</trd>
  <trdgrp lng="nl">
    <trd>guirlande</trd>,
    <trd>slinger</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>coroa<klr tip="amb"> (de folhas e flores)</klr></trd>,
   <trd>grinalda</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_a;</trd>
  <trdgrp lng="tr">
    <trd>&ccedil;i&ccedil;ekten/yapraktan &ccedil;elenk</trd>,
    <trd>bezek kordonu</trd>
  </trdgrp>
</drv>
</art>
