<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: liut.xml,v 1.24 2022/08/24 07:29:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>liut</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="liut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="liut.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Iu el diversaj
      <ref tip="lst" cel="instru2.kord0o.MUZ" lst="voko:kordinstrumentoj">kordinstrumentoj</ref>
      kun sonkesto, unumane tenebla kolo kaj pluke, plektre
      a&ubreve; ar&ccirc;e sonigeblaj kordoj stre&ccirc;itaj
      la&ubreve;longe de la kolo.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ju;&c_t;&c_n;&c_ja;</trd>
  <trd lng="cs">loutna</trd>
  <trd lng="de">Laute</trd>
  <trd lng="el">&lambda;&alfa;&omikron;&ypsilon_ton;&tau;&omikron;</trd>
  <trd lng="en">lute</trd>
  <trdgrp lng="fr">
    <trd>luth</trd>
  </trdgrp>
  <trd lng="hu">lant</trd>
  <trd lng="nl">luit</trd>
  <trdgrp lng="pl">
    <trd>bandura</trd>,
    <trd>g&eogonek;&sacute;l</trd>,
    <trd>lutnia</trd>
  </trdgrp>
  <trd lng="pt">ala&uacute;de</trd>
  <trdgrp lng="ro">
    <trd>bandur&abreve;</trd>,
    <trd>l&abreve;ut&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_ju;&c_t;&c_n;&c_ja;</trd>
  <trd lng="sk">lutna</trd>
  <trd lng="uk">&#x43B;&#x44E;&c_t;&#x43D;&#x44F;</trd>
</drv>
<drv mrk="liut.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="liut.0isto.profesio">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzikisto kiu ludas liuton:
      <ekz>
la patro [...] venigis <tld/>iston, kiu devis ludi anta&ubreve; la infaneto
        <fnt>
          <aut>Paul Bennemann</aut>,
          <vrk>La Vendreda Klubo</vrk>,
          <lok><url ref="&Gutenberg;ebooks/47259">
        gutenberg.org</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_ju;&c_t;&c_n;&c_ib;&c_s;&c_t;</trd>
    <trd lng="de">Lautenist</trd>
    <trd lng="en">lutenist</trd>
    <trd lng="fr">luthiste</trd>
    <trd lng="pl">lutnik</trd>     
    <trd lng="ro">lutier</trd>    
  </snc>
</drv>
</art>
