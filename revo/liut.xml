<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>liut</rad>/o
</kap>
<drv mrk="liut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Iu el diversaj
      <ref tip="super" cel="instru2.kord0o">kordinstrumentoj</ref>
      kun sonkesto, unumane tenebla kolo kaj pluke, plektre
      a&ubreve; ar&ccirc;e sonigeblaj kordoj stre&ccirc;itaj
      trans la sonkesto &gcirc;is ekstreme de la kolo,
      tiel ke muzikanto povas ilin premi por &scirc;an&gcirc;i
      iliajn longojn kaj sekve son-altojn.
      Komparu kun
      <ref tip="vid" cel="citr.0o.MUZ">citroj</ref>,
      <ref tip="vid" cel="harp.0o">harpoj</ref>,
      <ref tip="vid" cel="lir.0o.MUZ">liroj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ju;&c_t;&c_n;&c_ja;</trd>
  <trd lng="de">Laute</trd>
  <trd lng="el">&lambda;&alfa;&omikron;&ypsilon_ton;&tau;&omikron;</trd>
  <trd lng="en">lute</trd>
  <trdgrp lng="fr">
    <trd>luth</trd>
  </trdgrp>
  <trd lng="hu">lant</trd>  
  <trd lng="nl">luit</trd>
  <trd lng="pt">ala&uacute;de</trd>
  <trd lng="ru">&c_l;&c_ju;&c_t;&c_n;&c_ja;</trd>
</drv>
</art>
