<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc> <ofc>7</ofc>
   <rad>to</rad>
</kap>

<drv mrk="t.0.konsonanto">
  <kap><tld/></kap>
  <uzo tip="fak">LIN</uzo>
  <uzo tip="fak">GRA</uzo>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      La dudekkvara <ref tip="super" cel="liter.0o.propra">litero</ref>
      de la Esperanta alfabeto 
      <ekz>
          nomoj de la literoj: <klr>[...]</klr> 
          po, ro, so, <tld/>, u, <klr>[...]</klr>
          <fnt>
            <bib>F</bib>
            <lok>&FE; 1</lok>
          </fnt>
      </ekz>, uzata i.a. kiel:
    </dif>
    <subsnc>
      <dif>
        (maj.) signo de formo: <ekz>T-fero</ekz>;
      </dif>
      <trd lng="fr">T<klr> (forme de)</klr></trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">FIZ</uzo>
      <dif>
        (maj.) simbolo de tera: <ekz>T</ekz>;
      </dif>
      <trd lng="fr">t&eacute;ra</trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">FIZ</uzo>
      <dif>
        (min.) simbolo de tuno: <ekz>t</ekz>.
      </dif>
      <trd lng="fr">tonne</trd>
    </subsnc> 
  </snc>
  <trd lng="cs">mluvit na n&ecaron;&ccaron;&iacute; obranu</trd>
  <trd lng="sk">hovori&tcaron; na nie&ccaron;iu obranu</trd>
</drv>

</art>

