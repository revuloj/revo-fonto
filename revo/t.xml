<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>t</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<subart>
<drv mrk="t.0.konsonanto">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      <ref tip="dif" cel="konson1.0o">Konsonanto</ref>, senvo&ccirc;a <ref
      tip="dif" cel="plosiv.0o">plosiva</ref> <ref tip="dif"
      cel="dental.0o">dentalo</ref> [t].
    </dif>
  <trd lng="fr">t<klr> (consonne)</klr></trd>  
  </snc>
  <snc>
     <dif>
      La dudekkvara <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto, uzata i.a. kiel:
     </dif>
  <trd lng="fr">t<klr> (lettre)</klr></trd>
  <subsnc>
  <dif>
  (maj.) signo de formo: <ekz><tld lit="T"/>-fero</ekz>;
  </dif>
  <trd lng="fr">T<klr> (forme de)</klr></trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (maj.) simbolo de tera: <ekz><tld lit="T"/></ekz>;
  </dif>
  <trd lng="fr">t&eacute;ra</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (min.) simbolo de tuno: <ekz><tld/></ekz>;
  </dif>
  <trd lng="fr">tonne</trd>
  </subsnc> 
  </snc>
  <trd lng="cs">mluvit na n&ecaron;&ccaron;&iacute; obranu</trd>
  <trd lng="sk">hovori&tcaron; na nie&ccaron;iu obranu</trd>
</drv>
</subart>

<subart>
<drv mrk="t.0.nomo">
  <kap><ofc>* 7</ofc><tld/>o</kap>
  <snc>   
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif>
  </snc>
  <trd lng="fr">t</trd>
</drv>
</subart>
</art>

