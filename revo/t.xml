<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: t.xml,v 1.14 2024/05/15 19:21:47 revo Exp $">
<kap>
   <ofc>*</ofc> <ofc>7</ofc>
   <rad>to</rad>
</kap>

<drv mrk="t.0">
  <kap><tld/></kap>
  <uzo tip="fak">LIN</uzo>
  <uzo tip="fak">GRA</uzo>
  <uzo tip="fak">FON</uzo>
  <snc mrk="t.0.konsonanto">
    <dif>
      La dudekkvara <ref tip="super" cel="liter.0o.propra">litero</ref>
      de la Esperanta alfabeto
      <ekz>
          nomoj de la literoj: <klr>[...]</klr>
          po, ro, so, <tld/>, u, <klr>[...]</klr>
          <fnt>
            <bib>F</bib>
            <lok>&FE; 1</lok>
          </fnt>,
      </ekz> uzata i.a. kiel:
    </dif>
    <subsnc>
      <dif>
        (maj.) signo de formo: <ekz>T-fero;</ekz>
      </dif>
      <trd lng="fr">T<klr> (forme de)</klr></trd>
      <trd lng="pl">T forma <klr>(np. teownik)</klr></trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">FIZ</uzo>
      <dif>
        (maj.) simbolo de tera: <ekz>T;</ekz>
      </dif>
      <trd lng="fr">t&eacute;ra</trd>
      <trd lng="pl">tera</trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">FIZ</uzo>
      <dif>
        (min.) simbolo de tuno: <ekz>t;</ekz>
      </dif>
      <trd lng="fr">tonne</trd>
      <trd lng="pl">tona</trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">MUZ</uzo>
      <dif>
        Neoficiala <ref tip="super" cel="sufiks.0o">sufikso</ref>
        por nomi <ref cel="interv.0o">intervalo</ref>jn:
        <ekz>
          <ref cel="unut.0o">unuto</ref>, <ref cel="dut.0o">duto</ref>,
          <ref cel="trit.0o">trito</ref>, <ref cel="kvart.0o">kvarto</ref> <klr>[&#8230;]</klr>.
        </ekz>
      </dif>
      <trd lng="fr">tonne</trd>
      <trd lng="pl">ton</trd>
    </subsnc>
  </snc>
  <trd lng="cs">mluvit na n&ecaron;&ccaron;&iacute; obranu</trd>
  <trd lng="pl">24 litera j&eogonek;z. esperanto</trd>
  <trd lng="sk">hovori&tcaron; na nie&ccaron;iu obranu</trd>
</drv>
</art>

