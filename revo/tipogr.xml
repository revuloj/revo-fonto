<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>tipografi</rad>/o
</kap>
<drv mrk="tipogr.0o">
  <kap><tld/>o</kap>
  <snc mrk="tipogr.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Procedo de <ref tip="super" cel="pres.0arto.TIP">presarto</ref>, en
      kiu tekstojn oni produktas per pretaj liter<ref tip="vid"
      cel="tip.0o.TIP">tipoj</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_u;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>,
    <trd>&c_d;&c_r;&c_u;&c_k;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja;
    <ind>&c_s;&c_p;&c_r;&c_a;&c_v;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="de">Typografie</trd>
  <trd lng="fr">typographie<klr tip="amb"> (technique
  d'impression)</klr></trd>
  <trdgrp lng="hu">
    <trd>nyomd&aacute;szat</trd>,
    <trd>k&ouml;nyvnyomtat&aacute;s</trd>,
    <trd>tipogr&aacute;fia</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>typografie</trd>,
    <trd>drukkunst</trd>,
    <trd>boekdrukkunst</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_i;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>,
    <trd>&c_t;&c_i;&c_p;&c_o;&c_g;&c_r;&c_a;&c_f;&c_s;&c_k;&c_o;&c_je; &c_d;&c_je;&c_l;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="tipogr.0a">
  <kap><tld/>a</kap>
  <snc mrk="tipogr.0a.TIP">
    <dif>
      Farata per <tld/>o, iel rilata al &gcirc;i:
      <ekz>
        135 literoj <tld/>aj
        <fnt><bib>FK</bib>, <lok>6, artikoloj pri esperanto, al la
        historio de la provoj de lingvoj tutmondaj de Leibnitz &gcirc;is la nuna
        tempo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">typographique</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_i;&c_p;&c_o;&c_g;&c_r;&c_a;&c_f;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_i;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
