<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skot</rad>/o
</kap>

<drv mrk="skot.0lando">
  <kap>
    <tld lit="S"/>lando
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="skot.0lando.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la norda parto de <ref tip="prt"
      cel="briti.B0o.insulo">Britio</ref>
      kun &ccirc;efurbo Edinburgo.
      Parto de <ref tip="malprt" cel="brit.B0ujo">Britujo</ref>.
    </dif>
    <trdgrp lng="pl">
      <trd>Szkocja</trd>
    </trdgrp>
  </snc>
</drv>

</art>

