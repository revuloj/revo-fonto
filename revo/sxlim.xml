<art mrk="$Id: sxlim.xml,v 1.33 2024/09/19 18:34:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;lim</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="sxlim.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxlim.0o.koto">
    <dif>
      Akvemoligita <ref cel="kot.0o.KOMUNE">kota</ref> <ref tip="super" cel="ter.0o.substanco">tero</ref>
      ku&scirc;anta sur fundo de <ref cel="marcx.0o.GEOG">mar&ccirc;o</ref>,
      lago, rivero a&ubreve; simila akvejo:
      <ekz>
        kelkaj specoj da fi&scirc;oj enfosas sin por la vintro en <tld/>on kaj restas en &gcirc;i en
        la stato de plena rigidi&gcirc;o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        eligu min el la <tld/>o, ke mi ne dronu
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 69:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la kavo ne estis akvo, sed nur <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 38:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        iri sur varma &scirc;anceli&gcirc;anta <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> rivero <klr>[&#8230;]</klr> eliras el siaj bordoj kaj kovras la tutan
        landon per <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Cikonioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ofte ni vadis tra la arbaro kun akvo kaj <tld/>o &gcirc;is la
        zono
        <fnt><aut>W. Schad</aut>, <vrk>Orinoko-delto: Indianoj,
        mar&ccirc;oj, arbarego</vrk>, <bib>Monato</bib>,
        <lok>2000:11, p. 20a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_l;&c_je;&c_j;</trd>,
      <trd>&c_m;&c_u;&c_l;</trd>,
      <trd>&c_c;&c_ib;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">tarquim</trd>
    <trdgrp lng="de">
      <trd>Schlamm</trd>,
      <trd>Schlick</trd>
    </trdgrp>
    <trd lng="es">cieno</trd>
    <trdgrp lng="fa">
      <trd>&f_g;&a_l; &a_w; &a_l;&a_A;&f_y;</trd>,
      <trd>&a_l;&a_j;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">vase<klr> (fange)</klr></trd>
    <trd lng="hu">iszap</trd>
    <trdgrp lng="it">
      <trd>melma</trd>,
      <trd>fanghiglia</trd>,
      <trd>limo</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>limo</trd>,
      <trd>lodo</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_l;</trd>,
      <trd>&c_t;&c_i;&c_n;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="th">
      <trd>&#3650;&#3588;&#3621;&#3609;</trd>,
      <trd>&#3604;&#3636;&#3609;&#3648;&#3621;&#3609;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxlim.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malglora, malnobla, malkomforta stato:
      <ekz>
        la popolo amas la militon <klr>[&#8230;]</klr>, sen la milito la mondo pereus a&ubreve;
        almena&ubreve; transformi&gcirc;us je <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;10</lok>
        </fnt>;
      </ekz>
      <ekz>
        ju pli multe mi konsciis la bonon kaj &ccirc;ion &leftquot;belegan kaj subliman&rightquot;, des pli
        profunde mi sinkis en mia <tld/>o kaj des pli forte impliki&gcirc;is en &gcirc;i
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;7</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fecx.0o.FIG">fe&ccirc;o<sncref/></ref>,
      <ref cel="kot.0o.FIG">koto<sncref/></ref>,
      <ref cel="marcx.0o.FIG">mar&ccirc;o<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_t;&c_v;&c_a;&c_n;&c_mol; <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>,
      <trd>&c_b;&c_a;&c_g;&c_n;&c_a; <klr>(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>
    </trdgrp>
    <trd lng="ca">deshonra</trd>
    <trd lng="hu">s&aacute;r</trd>
  </snc>
  <trd lng="bo">&t_-a;&t_da;&t_ga;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trd lng="ca">llot</trd>
  <trd lng="fr">fange</trd>
  <trdgrp lng="ja">
    <trd>&#27877;  <pr>&#12393;&#12429;</pr></trd>,
    <trd>&#36575;&#27877;  <pr>&#12394;&#12435;&#12391;&#12356;</pr></trd>,
    <trd>&#12408;&#12393;&#12429;</trd>,
    <trd>&#27738;&#27877;  <pr>&#12362;&#12391;&#12356;</pr></trd>,
    <trd>&#27877;&#27836;  <pr>&#12393;&#12429;&#12396;&#12414;</pr></trd>,
    <trd>&#27738;&#36785;  <pr>&#12362;&#12376;&#12423;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slib</trd>,
    <trd>slijk</trd>,
    <trd>modder</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>mud</trd>
    <trd>muck</trd>
    <trd>sludge</trd>
  </trdgrp>
  <trdgrp lng="pl">
     <trd>mu&lstroke;</trd>,
     <trd>szlam</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_u;&c_l;</trd>,
    <trd>&c_t;&c_v;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_g;&c_r;&c_ja;&c_z;&c_ib; (&c_l;&c_ib;&c_k;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_ib;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#27745;&#27877; <pr>w&umacron;n&iacute;</pr></trd>,
    <trd>&#27877; <pr>n&iacute;</pr></trd>,
    <trd>&#27877;&#27870; <pr>n&iacute;n&igrave;ng</pr></trd>,
    <trd>&#27877;&#28632; <pr>n&iacute;n&igrave;ng</pr></trd>,
    <trd>&#27877;&#28118; <pr>n&iacute;n&agrave;o</pr></trd>,
    <trd>&#22738; <pr>&agrave;i</pr></trd>,
    <trd>&#28132;&#27877; <pr>y&umacron;n&iacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxlim.0ejo">
  <kap><tld/>ejo,
  <var><kap><tld/>ujo</kap></var></kap>
  <snc mrk="sxlim.0ejo.GEOG">
    <dif>
      <ref tip="vid" cel="marcx.0o.GEOG">Mar&ccirc;o</ref>,
      loko, kie estas <tld/>o:
      <ekz>
        &gcirc;i flosas al ponto de <nom>Zlic</nom>, de la ponto sub deklivoj al
        <tld/>ujo
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K.
          Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/III.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        abomena konspirestro <klr>[&#8230;]</klr> mil legionojn da senkulpaj <klr>[&#8230;]</klr>
        trenis kun si malsupren en la profundan <tld/>ejon de la pereo de la animo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiel nia e&ubreve;ropa civilizacio <klr>[&#8230;]</klr> iom post iom sinkas en <tld/>ejon de
        importita malkulturo kaj malcivilizo
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007076.php">Ho, tempoj! Ho, moroj!</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 19</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_v;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_n;&c_a;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_n;&c_ib;&c_sh;&c_ch;&c_a;</trd>,
    <trd>&c_d;&c_r;&c_y;&c_g;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Morast</trd>,
    <trd>Sumpf</trd>,
    <trd>Schlickwatt</trd>
  </trdgrp>
  <trd lng="fr">bourbier</trd>
  <trdgrp lng="ja">
    <trd>&#27877;&#27836;  <pr>&#12393;&#12429;&#12396;&#12414;</pr></trd>,
    <trd>&#12396;&#12363;&#12427;&#12415;</trd>
  </trdgrp>
  <trd lng="nl">modderpoel</trd>
  <trdgrp lng="pl">
    <trd>mokrad&lstroke;o</trd>,
    <trd>bagno</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_r;&c_a;&c_g;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_ja;&c_s;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_ch;&c_a;&c_r;&c_ib;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_o;&c_t;&c_i;&c_s;&c_t;&c_a; &c_m;&c_ib;&c_s;&c_c;&c_je;&c_v;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_a;&c_g;&c_n;&c_o;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;. &c_k;&c_l;&c_o;&c_a;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_t;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#27877;&#28525; <pr>n&iacute;t&aacute;n</pr></trd>,
    <trd>&#20687;&#27877;&#27836;&#30340;&#22320;&#26041; <pr>xi&agrave;ngn&iacute;zh&#462;oded&igrave;fang</pr></trd>,
    <trd>&#27700;&#20179; <pr>shu&#464;c&amacron;ng</pr></trd>,
    <trd>&#27700;&#20489; <pr>shu&#464;c&amacron;ng</pr></trd>,
    <trd>&#31370; <pr>w&amacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxlim.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxlim.el0igxi.materie">
    <dif>
      Eli&gcirc;i el <tld/>o, el &gcirc;ia alsu&ccirc;a forto.
    </dif>
  <trd lng="de">sich aus dem Schlamm <ind>befreien</ind></trd>
  </snc>
  <snc mrk="sxlim.el0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Eli&gcirc;i el malfacilo, el embaraso:
      <ekz>
        en iuj afekcioj de la nerva sistemo la malsanulo
        <klr>[&#8230;]</klr> ne povas el<tld/>i&gcirc;is el ia nekapablo
        voli
        <fnt>
          <aut>M. Proust, trad. P. Combot</aut>,
          <vrk>Pri la legado</vrk>,
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_c;&c_a; <klr>(&c_z; &c_g;&c_l;&c_je;&c_ju;, &c_t;&c_v;&c_a;&c_n;&c_ib;)</klr></trd>
  <trd lng="de">sich <ind>losmachen</ind></trd>
  <trd lng="fr">se <ind>d&eacute;sembourber</ind></trd>
  <trd lng="pl">wydosta&cacute; si&eogonek;</trd>
</drv>

<drv mrk="sxlim.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxlim.en0igxi.materie">
    <dif>
      Parte a&ubreve; tute eni&gcirc;i en <tld/>on:
      <ekz>
        la radoj profunde en<tld/>i&gcirc;as en la koton
        <fnt>
          <aut>R. Condon</aut>,
          <vrk>La Elprovado</vrk>,
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">im Schlamm <ind>stecken</ind> bleiben</trd>
  </snc>
  <snc mrk="sxlim.en0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Embarasi&gcirc;i, eni&gcirc;i en malfacila&jcirc;ojn, en malbonan
      situacion:
      <ekz>
        vi en<tld/>i&gcirc;is en malamo kaj malrespekto
        <fnt>
          <aut>M. Markeyev</aut>,
          <vrk>Ukraina ribelo</vrk>,
          <lok>lernu.net, 2014-05-06</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_mol; <klr>(&c_u; &c_b;&c_a;&c_g;&c_n;&c_u;, &c_t;&c_v;&c_a;&c_n;&c_mol;)</klr></trd>
  <trd lng="de">sich <ind>festfahren</ind></trd>
  <trd lng="fr">s'<ind>embourber</ind></trd>
  <trd lng="ja">&#27877;&#27836;&#12395;&#12399;&#12414;&#12427;
  <pr>&#12393;&#12429;&#12396;&#12414;&#12395;&#12399;&#12414;&#12427;</pr></trd>
  <trdgrp lng="pl">
    <trd>ugrz&eogonek;zn&aogonek;&cacute;</trd>,
    <trd>utkn&aogonek;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20725; <pr>ji&amacron;ng</pr></trd>,
    <trd>&#27565; <pr>ji&amacron;ng</pr></trd>,
    <trd>&#21160;&#24377;&#19981;&#24471; <pr>d&ograve;ngtanb&ugrave;d&eacute;</pr></trd>,
    <trd>&#21205;&#24392;&#19981;&#24471; <pr>d&ograve;ngtanb&ugrave;d&eacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxlim.argil0o">
  <kap>
    argil<tld/>o, <var><kap><tld/>gluo</kap></var>
  </kap>
  <snc mrk="sxlim.argil0o.ceramiko">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="&Scirc;"/>o farita el argilo kaj akvo uzata &ccirc;e potfarado por gluado kaj ornamado:
      <ekz>
        <klr>[per]</klr> argil&scirc;limo <klr>[&#8230;]</klr> kovri la surfacon de ceramiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=39644"
              >Ceramiko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        por glui <klr>[&#8230;]</klr> oni uzas <tld/>gluon, kiu estas duon-likva miksa&jcirc;o de
        argilo kaj akvo
        <fnt>
          <vrk><url ref="http://www.eventoj.hu/steb/vortaroj/argilajxo.htm"
              >La Argilo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="da">begitning</trd>
  <trd lng="de">Schlicker</trd>
  <trd lng="en">slip <klr>(ceramics)</klr></trd>
  <trd lng="fr">barbotine <klr>(argile)</klr></trd>
  <trd lng="it">barbottina</trd>
  <trd lng="ru">&c_sh;&c_l;&c_i;&c_k;&c_je;&c_r;</trd>
</drv>

