<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;lim</rad>/o
</kap>
<drv mrk="sxlim.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxlim.0o.koto">
    <dif>
      Akvemoligita kota tero ku&scirc;anta sur fundo de mar&ccirc;o, lago, 
      rivero a&ubreve; simila akvejo:
      <ekz>
        ofte ni vadis tra la arbaro kun akvo kaj &scirc;limo &gcirc;is la
        zono<fnt><aut>Werner Schad</aut>: <vrk>Orinoko-delto: Indianoj,
        mar&ccirc;oj, arbarego</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero
        11a, p. 20a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_l;&c_je;&c_j;</trd>, 
      <trd>&c_m;&c_u;&c_l;</trd>, 
      <trd>&c_c;&c_ib;&c_n;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">tarquim</trd>
    <trdgrp lng="de">
      <trd>Schlamm</trd>,
      <trd>Schlick</trd>
    </trdgrp>
    <trd lng="es">cieno</trd>
    <trdgrp lng="fa">
      <trd>&f_g;&a_l; &a_w; &a_l;&a_A;&f_y;</trd>,
      <trd>&a_l;&a_j;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>vase<klr> (fange)</klr></trd>
    </trdgrp>
    <trd lng="hu">iszap</trd>
    <trdgrp lng="it">
      <trd>melma</trd>, 
      <trd>fanghiglia</trd>, 
      <trd>limo</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>limo</trd>,
      <trd>lodo</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_l;</trd>,
      <trd>&c_t;&c_i;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxlim.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malglora, malnobla stato.
    </dif>
    <refgrp tip="vid">
      <ref cel="kot.0o">koto</ref>,
      <ref cel="marcx.0o">mar&ccirc;o</ref>,
      <ref cel="fecx.0o">fe&ccirc;o</ref>.
    </refgrp>
    <trd lng="ca">deshonra</trd>
    <trd lng="hu">s&aacute;r</trd>
  </snc>
  <trd lng="ca">llot</trd>
  <trdgrp lng="fr">
    <trd>fange</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>modder</trd>,
    <trd>slijk</trd>
  </trdgrp>
  <trd lng="en">slime</trd>
</drv>
</art>
