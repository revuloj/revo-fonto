<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;lim</rad>/o
</kap>
<drv mrk="sxlim.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Akvemoligita kota tero ku&scirc;anta sur fundo de mar&ccirc;o, lago, rivero ks.
    </dif>
    <trdgrp lng="de">
      <trd>Schlamm</trd>,
      <trd>Schlick</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_l;</trd>,
      <trd>&c_t;&c_i;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malglora, malnobla stato.
    </dif>
    <refgrp tip="sin">
      <ref cel="kot.0o">koto</ref>,
      <ref cel="marcx.0o">mar&ccirc;o</ref>,
      <ref cel="fecx.0o">fe&ccirc;o</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>modder</trd>,
    <trd>slijk</trd>
  </trdgrp>
  <trd lng="en">slime</trd>
</drv>
</art>
