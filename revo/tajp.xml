<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>tajp</rad>/i
    </kap>

    <drv mrk="tajp.0i">
      <kap><tld/>i</kap>
      <uzo tip="fak">TEK</uzo>
      <uzo tip="fak">TIP</uzo>
      <fnt><bib>PIV1</bib></fnt>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          <ref tip="super" cel="skrib.0i.signojn">Skribi</ref> tekston
          <ref tip="sin" cel="klav.0i">klavante</ref>, per ma&scirc;ino:
          <ekz>
            kelkaj <ind><tld/>eraroj</ind> povas konfuzi la studanton,
            kio beda&ubreve;rindas.
            <trd lng="es">error tipogr&aacute;fico, de dedo</trd>
            <fnt><aut>Donald BROADRIBB</aut>:
              <vrk><url ref="&Monato;/rec/kegr.html">
              Vitraj &scirc;uoj de gramatiko</url></vrk>,
              <bib>Monato</bib>,
              <lok>n-ro 03/2001</lok>;
           </fnt>
        </ekz><ekz>
            Se oni scias la adreson, &ccirc;e kiu troveblas la dezirata
            informo, oni povas <tld/>i &gcirc;in en sian uzprogramon.
            <fnt><aut>Reinhard F&Ouml;SSMEIER</aut>:
              <vrk><url ref="&Monato;/004821.html">
              Teksa&jcirc;o &ccirc;irka&ubreve; la tero</url></vrk>,
              <bib>Monato</bib>,
              <lok>n-ro 12/1995</lok>.
            </fnt>
        </ekz>
        </dif>
      </snc>
      <trdgrp lng="de">
        <trd>tippen</trd>,
        <trd>eintippen</trd>
      </trdgrp>
      <trd lng="fa">&a_t;&a_A;&f_y;&f_p; &f_k;&a_r;&a_d;&a_n;</trd>
      <trd lng="hu">g&eacute;pel</trd>
      <trdgrp lng="nl"><trd>typen</trd>, <trd>tikken</trd>
      </trdgrp>
      <trdgrp lng="es">
       <trd>Escribir a m&aacute;quina</trd>,
       <trd>Mecanografiar</trd>,
      <trd>Tipear</trd>
     </trdgrp>
    <trdgrp lng="pl">
        <trd>pisa&cacute; na maszynie</trd>,
        <trd>wklepywa&cacute;<klr> (tekst) do komputera</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
        <trd>escrever &agrave; m&aacute;quina</trd>,
        <trd>datilografar</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol;
    &c_n;&c_a; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_k;&c_je;</trd>
    <trd lng="sv">skriva p&aring; maskin</trd>
    </drv>

    <drv mrk="tajp.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
        <dif>
          Teksto <tld/>ita.
        </dif>
      </snc>
      <trdgrp lng="de">
         <trd>Getipptes</trd>,
         <trd>Eingetipptes</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>g&eacute;pel&eacute;s</trd>,
        <trd>g&eacute;pelt sz&ouml;veg</trd>
      </trdgrp>
      <trd lng="pl">tekst maszynowy</trd>
      <trdgrp lng="ru">
        <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&c_o;&c_p;&c_i;&c_s;&c_mol;
        </trd>,
        <trd>
        &c_m;&c_a;&c_sh;&c_i;&c_n;&c_o;&c_p;&c_i;&c_s;&c_n;&c_y;&c_j;
        &c_t;&c_je;&c_k;&c_s;&c_t;</trd>
      </trdgrp>
      <trdgrp lng="es">
        <trd>texto mecanografiado</trd>,
        <trd>Escrito a m&aacute;quina</trd>
      </trdgrp>
    </drv>

    <drv mrk="tajp.0ilo">
      <kap><tld/>ilo</kap>
      <uzo tip="fak">TEK</uzo>
      <snc>
        <dif>
          Aparato por <tld/>i,
          <ref tip="sin" cel="skrib.0masxino">skribma&scirc;ino</ref>.
        </dif>
        <trdgrp lng="de">
          <trd>Schreibmaschine</trd>,
          <trd>Tatstatur</trd>
        </trdgrp>
        <trd lng="es">M&aacute;quina de escribir</trd>
        <trd lng="fa">&a_m;&a_A;&a_sh;&f_y;&a_n; &a_t;&a_H;&a_r;&f_y;&a_r;</trd>
        <trd lng="hu">&iacute;r&oacute;g&eacute;p</trd>
        <trd lng="pl">maszyna do pisania</trd>
        <trd lng="ru">&c_p;&c_i;&c_sh;&c_u;&c_shch;&c_a;&c_ja;
        &c_m;&c_a;&c_sh;&c_i;&c_n;&c_k;&c_a;</trd>
        <trd lng="sv">skrivmaskin</trd>
      </snc>
    </drv>

    <drv mrk="tajp.0isto">
      <kap><tld/>isto</kap>
      <snc>
        <dif>
          Homo kies metio estas <tld/>i.
        </dif>
      </snc>
      <trdgrp lng="de">
         <trd>Typist</trd>,
         <trd>Schreibmaschinensekret&auml;rin</trd>
      </trdgrp>
      <trdgrp lng="fa">
        <trd>&a_m;&a_A;&a_sh;&f_y;&a_n;&zwnj;&a_n;&a_w;&f_y;&a_s;</trd>,
        <trd>&a_t;&a_A;&f_y;&f_p;&f_y;&a_s;&a_t;</trd>
      </trdgrp>
      <trd lng="hu">g&eacute;p&iacute;r&oacute;</trd>
      <trd lng="es">Mecan&oacute;grafo</trd>
      <trdgrp lng="pl">
        <trd>maszynotypista</trd>,
        <trd>maszynotypistka</trd>
      </trdgrp>
      <trd lng="ru">&c_m;&c_a;&c_sh;&c_i;&c_n;&c_i;&c_s;&c_t;&c_k;&c_a;
      </trd>
    </drv>

  </art>

