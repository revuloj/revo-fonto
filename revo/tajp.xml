<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tajp.xml,v 1.28 2024/05/10 08:31:28 revo Exp $">
<kap>
  <ofc>8</ofc><rad>tajp</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="tajp.0i">
  <kap><ofc>8</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="tajp.0i.TIP">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="super" cel="skrib.0i.signojn">Skribi</ref> tekston
      <ref tip="sin" cel="klav.0i">klavante</ref>, per ma&scirc;ino:
      <ekz>
        kelkaj <ind><tld/>eraroj</ind> povas konfuzi la studanton,
        kio beda&ubreve;rindas
        <trd lng="es">error tipogr&aacute;fico, de dedo</trd>
        <fnt><aut>D. Broadribb</aut>,
          <vrk><url ref="&Monato;/2001/kegr.html">
          Vitraj &scirc;uoj de gramatiko</url></vrk>,
          <bib>Monato</bib>,
          <lok>2001/03, p. 30-31</lok>
       </fnt>;
      </ekz>
      <ekz>
        se oni scias la adreson, &ccirc;e kiu troveblas la dezirata
        informo, oni povas <tld/>i &gcirc;in en sian uzprogramon
        <fnt><aut>R. F&ouml;ssmeier</aut>,
          <vrk><url ref="&Monato;/1995/004821.php">
          Teksa&jcirc;o &ccirc;irka&ubreve; la tero</url></vrk>,
          <bib>Monato</bib>,
          <lok>1995/12, p. 14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_ib;&c_s;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_m;&c_a;&c_sh;&c_y;&c_n;&c_c;&c_y;)</klr></trd>,
    <trd>&c_d;&c_r;&c_u;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_n;&c_a; &c_m;&c_a;&c_sh;&c_y;&c_n;&c_c;&c_y;)</klr></trd>,
    <trd>&c_n;&c_a;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_t;&c_e;&c_k;&c_s;&c_t; &c_n;&c_a; &c_k;&c_l;&c_ja;&c_v;&c_ib;&c_ja;&c_t;&c_u;&c_r;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="cs">ps&aacute;t na stroji</trd>
  <trdgrp lng="de">
    <trd>tippen</trd>,
    <trd>eintippen</trd>
  </trdgrp>
  <trd lng="en">type</trd>
  <trdgrp lng="es">
    <trd>escribir a m&aacute;quina</trd>,
    <trd>mecanografiar</trd>,
    <trd>tipear</trd>
  </trdgrp>
  <trd lng="fa">&a_t;&a_A;&f_y;&f_p; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>dactylographier</trd>,
    <trd>taper &agrave; la machine</trd>
  </trdgrp>
  <trd lng="hu">g&eacute;pel</trd>
  <trd lng="ja">&#x30BF;&#x30A4;&#x30D7;&#x3059;&#x308B;  <pr>&#x305F;&#x3044;&#x3077;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>typen</trd>,
    <trd>tikken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pisa&cacute; na maszynie</trd>,
    <trd>wklepywa&cacute;<klr> (tekst) do komputera</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>escrever &agrave; m&aacute;quina</trd>,
    <trd>datilografar</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_ch;&c_a;&c_t;&c_a;&c_t;&c_mol; &c_n;&c_a; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_k;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>klepa&tcaron; na stroji</trd>,
    <trd>p&iacute;sa&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">skriva p&aring; maskin</trd>
  <trd lng="uk">&c_d;&c_r;&c_u;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i; &#x43D;&c_a; &#x43C;&c_a;&c_sh;&c_i;&#x43D;&c_c;&c_ib;</trd>
  <trd lng="zh">&#x6253;&#x5B57; <pr>d&#x1CE;z&#xEC;</pr></trd>
</drv>

<drv mrk="tajp.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="tajp.0ajxo.TIP">
    <dif>
      Teksto <tld/>ita.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_a;&c_p;&c_ib;&c_s;</trd>,
    <trd>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_a;&c_p;&c_ib;&c_s;&c_n;&c_y; &c_t;&c_e;&c_k;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Getipptes</trd>,
     <trd>Eingetipptes</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>texto mecanografiado</trd>,
    <trd>escrito a m&aacute;quina</trd>
  </trdgrp>
  <trd lng="fr">texte dactylographi&eacute;</trd>
  <trdgrp lng="hu">
    <trd>g&eacute;pel&eacute;s</trd>,
    <trd>g&eacute;pelt sz&ouml;veg</trd>
  </trdgrp>
  <trd lng="ja">&#x30BF;&#x30A4;&#x30D7;&#x6587;&#x66F8;  <pr>&#x305F;&#x3044;&#x3077;&#x3076;&#x3093;&#x3057;&#x3087;</pr></trd>
  <trd lng="nl">getypte tekst</trd>
  <trd lng="pl">tekst maszynowy</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&c_o;&c_p;&c_i;&c_s;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_sh;&c_i;&c_n;&c_o;&c_p;&c_i;&c_s;&c_n;&c_y;&c_j; &c_t;&c_je;&c_k;&c_s;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_sh;&c_i;&#x43D;&#x43E;&#x43F;&c_i;&c_s;</trd>,
    <trd>&#x43C;&c_a;&c_sh;&c_i;&#x43D;&#x43E;&#x43F;&c_i;&c_s;&#x43D;&c_i;&c_j; &c_t;&c_je;&#x43A;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="tajp.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="tajp.0ilo.TIP">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Aparato por <tld/>i,
      <ref tip="sin" cel="masxin.skrib0o.TIP">skribma&scirc;ino</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_u;&c_k;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; <ind>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_k;&c_a;</ind></trd>,
    <trd>&c_p;&c_ib;&c_sh;&c_u;&c_ch;&c_a;&c_ja; <ind>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_i;&c_sh;&c_je;&c_shch;&c_a; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_a;</trd>
  <trd lng="cs">psac&iacute; stroj</trd>
  <trdgrp lng="de">
    <trd>Schreibmaschine</trd>,
    <trd>Tatstatur</trd>
  </trdgrp>
  <trd lng="en">typewriter</trd>
  <trd lng="es">m&aacute;quina de escribir</trd>
  <trd lng="fa">&a_m;&a_A;&a_sh;&f_y;&a_n; &a_t;&a_H;&a_r;&f_y;&a_r;</trd>
  <trd lng="fr">machine &agrave; &eacute;crire</trd>
  <trd lng="hu">&iacute;r&oacute;g&eacute;p</trd>
  <trd lng="ja">&#x30BF;&#x30A4;&#x30D7;&#x30E9;&#x30A4;&#x30BF;&#x30FC;</trd>
  <trdgrp lng="nl">
    <trd>schrijfmachine</trd>,
    <trd>typemachine</trd>
  </trdgrp>
  <trd lng="pl">maszyna do pisania</trd>
  <trd lng="ru">&c_p;&c_i;&c_sh;&c_u;&c_shch;&c_a;&c_ja; &c_m;&c_a;&c_sh;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="sk">p&iacute;sac&iacute; stroj</trd>
  <trd lng="sv">skrivmaskin</trd>
  <trd lng="uk">&c_d;&c_r;&c_u;&#x43A;&c_a;&c_r;&c_s;&#x44C;&#x43A;&c_a; &#x43C;&c_a;&c_sh;&c_i;&#x43D;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x6253;&#x5B57;&#x673A; <pr>d&#x1CE;z&#xEC;j&#x12B;</pr></trd>,
    <trd>&#x6253;&#x5B57;&#x6A5F; <pr>d&#x1CE;z&#xEC;j&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tajp.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="tajp.0isto.TIP">
    <dif>
      Homo kies metio estas <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_sh;&c_y;&c_n;&c_ib;&c_s;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_n;&c_a;&c_b;&c_o;&c_r;&c_sh;&c_ch;&c_y;&c_k; <klr>(&c_t;&c_e;&c_k;&c_s;&c_t;&c_u; &c_n;&c_a; &c_k;&c_a;&c_m;&c_p;&#x2019;&c_ju;&c_t;&c_a;&c_r;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="cs">p&iacute;sa&rcaron; na stroji</trd>
  <trdgrp lng="de">
     <trd>Typist</trd>,
     <trd>Schreibmaschinensekret&auml;rin</trd>
  </trdgrp>
  <trd lng="en">typist</trd>
  <trd lng="es">mecan&oacute;grafo</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_A;&a_sh;&f_y;&a_n;&#x200C;&a_n;&a_w;&f_y;&a_s;</trd>,
    <trd>&a_t;&a_A;&f_y;&f_p;&f_y;&a_s;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">dactylographe</trd>
  <trd lng="hu">g&eacute;p&iacute;r&oacute;</trd>
  <trd lng="ja">&#x30BF;&#x30A4;&#x30D4;&#x30B9;&#x30C8;</trd>
  <trd lng="nl">typist</trd>
  <trdgrp lng="pl">
    <trd>maszynotypista</trd>,
    <trd>maszynotypistka</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_sh;&c_i;&c_n;&c_i;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="sk">pis&aacute;r na stroji</trd>
  <trdgrp lng="zh">
    <trd>&#x6253;&#x5B57;&#x5458; <pr>d&#x1CE;z&#xEC;yu&#xE1;n</pr></trd>,
    <trd>&#x6253;&#x5B57;&#x54E1; <pr>d&#x1CE;z&#xEC;yu&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>

