<drv mrk="multip.matrica0o">
  <kap>matrica <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="multip.matrica0o.MAT">
    <dif>
      <klr>(de <ref cel="matric.n_p0o">(n,p)-matrico</ref>
      <frm am="A">A</frm> per
      <frm am="(p,q)">(<k>p</k>,<k>q</k>)</frm>-matrico
      <frm am="B">B</frm>) </klr>
      <ref tip="super" cel="operac.0o.MAT">Operacio</ref>, kies rezulto
      estas <frm am="(n,q)">(<k>n</k>,<k>q</k>)</frm>-matrico
      <frm am="C">C</frm> kun &gcirc;enerala elemento
      <frm am="c_(ij)=sum_m a_(im)*b_(mj)">c<sub><k>ij</k></sub> = &#x2211;<sub><k>m</k></sub>
      a<sub><k>im</k></sub>&middot;b<sub><k>mj</k></sub></frm>,
      kie <frm am="m"><k>m</k></frm> varias inter <frm>1</frm> kaj
      <frm am="p"><k>p</k></frm>:
      <ekz>
        la matrica <tld/>o ne estas difinita por ajna paro de matricoj;
      </ekz>
      <ekz>
        provizite per matricaj adicio kaj <tld/>o, la aro de &ccirc;iuj
        <frm am="(n,n)">(<k>n</k>,<k>n</k>)</frm>-matricoj, ricevas la
        strukturon de ringo, nekomuteca se
        <frm am="n gt 1"><k>n</k> &gt; 1</frm>, kies unuo estas
        la <ref cel="matric.unuo0o">unuomatrico</ref>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Kontroli trd de/en. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_m;&c_a;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_je;
  <ind>&c_m;&c_n;&c_o;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  <trd lng="ca"><ind>multiplicaci&oacute;</ind> matricial</trd>
  <trd lng="de">Matrizenmultiplikation</trd>
  <trd lng="en">matrix <ind>multiplication</ind></trd>
  <trdgrp lng="es">
    <trd>multiplicaci&oacute;n de matrices</trd>,
    <trd>producto de matrices</trd>
  </trdgrp>
  <trd lng="fr">multiplication matricielle</trd>
  <trd lng="hu">m&aacute;trixszorz&aacute;s</trd>
  <trd lng="pl">mno&zdot;enie macierzy</trd>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_r;&c_i;&c_ch;&c_n;&c_o;&c_je;
<ind>&c_u;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  <trd lng="sk">maticov&eacute; n&aacute;sobenie</trd>
  <trd lng="cs">maticov&eacute; n&aacute;soben&iacute;</trd>
</drv>

<drv mrk="multip.polinoma0o">
  <kap>polinoma <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="multip.polinoma0o.operacio">
    <dif>
      Dua <ref tip="super" cel="operac.0o.MAT">operacio</ref> en
      <ref cel="ring.polinom0o">polinom-ringo</ref>:
      <ekz>
        la &gcirc;enerala termo <frm am="bb         S_n"><g>S</g><sub><k>n</k></sub></frm>
        de la rezulto de <tld/>o de <frm am="bb P"><g>P</g></frm> kun
        <frm am="bb Q"><g>Q</g></frm> estas
        <frm am="sum_(p+q=n)bb P_p*bb Q_q">&#x2211;<sub><k>p</k>+<k>q</k>=<k>n</k></sub>
        <g>P</g><sub><k>p</k></sub>&middot;<g>Q</g><sub><k>q</k></sub></frm>;
      </ekz>
      <ekz>
        la rezulto de <tld/>o de <frm am="bb         X^i"><g>X</g><sup><k>i</k></sup></frm>
        per <frm am="bb X^j"><g>X</g><sup><k>j</k></sup></frm> estas
        <frm am="bb X^(i+j)"><g>X</g><sup><k>i</k>+<k>j</k></sup></frm>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Kontroli trd de/en/ru. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_m;&c_n;&c_o;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;
  &c_m;&c_n;&c_a;&c_g;&c_a;&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_w;</trd>
  <trd lng="ca"><ind>multiplicaci&oacute;</ind> polin&ograve;mica</trd>
  <trd lng="de">Polynommultiplikation</trd>
  <trd lng="en">polynomial <ind>multiplication</ind></trd>
  <trd lng="fr">multiplication polynomiale</trd>
  <trd lng="hu">polinomszorz&aacute;s</trd>
  <trd lng="pl">mno&zdot;enie wielomian&oacute;w</trd>
  <trd lng="ru">&c_u;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;
&c_m;&c_n;&c_o;&c_g;&c_o;&c_ch;&c_l;&c_je;&c_n;&c_o;&c_v;</trd>
  <trd lng="sv">polynommultiplikation</trd>
</drv>
