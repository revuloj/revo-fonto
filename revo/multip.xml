<drv mrk="multip.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="multip.0o.elementa">
    <uzo tip="klr">(elementa aritmetiko)</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 15</lok></fnt>
    <dif>
      La operacio <ref cel="multip.0i">multipliki</ref>:
      <ekz>
        <frm am="3*7=21">3&#xD7;7 = 21</frm> <klr> (legu: trioble sep estas
        dudek unu, a&ubreve; tri multiplikite per sep estas dudek unu)</klr>;
      </ekz>
      <ekz>
        se via filo ne scipovas solvi la multiplikon <frm am="6&#xD7;8">6&#xD7;8</frm>, ne
        ripro&ccirc;u lin: tiu estas la plej komplika <tld/>o por la infanoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2014/011217.php">Nekovrilaj malkovroj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="multip.0ato">multiplikato</ref>,
      <ref cel="multip.0anto">multiplikanto</ref>.
    </refgrp>
  </snc>
  <snc mrk="multip.0o.neelementa">
    <dif>
      Simila operacio, kaj pli precize:
    </dif>
    <subsnc mrk="multip.0o.ringo">
      <fnt><bib>PIV2</bib><lok>ringo</lok></fnt>
      <dif>
        <klr>(en <ref cel="ring.0o.MAT">ringo</ref>)</klr> &Gcirc;ia dua
        <ref tip="super" cel="operac.0o.MAT">operacio</ref>.
      </dif>
    </subsnc>
    <subsnc mrk="multip.0o.modulo">
      <dif>
        <klr>(en <ref cel="modul.0o.strukt">modulo</ref> a&ubreve;
        <ref cel="spac.vektora0o">vektora spaco</ref>)</klr> &Gcirc;ia
        <ref tip="super" cel="operac.ekstera0o.aro">ekstera
        operacio</ref>.
      </dif>
    </subsnc>
    <subsnc mrk="multip.0o.de_matrico_per_skalaro">
      <dif>
        <klr>(de <ref cel="matric.n_p0o">(n,p)-matrico</ref>
        <frm am="A">A</frm> super <frm am="bb K"><g>K</g></frm> per 
        elemento <frm am="lambda">&lambda;</frm>
        de <frm am="bb K"><g>K</g></frm>) </klr>
        <ref tip="super" cel="operac.ekstera0o.aro">Ekstera
        operacio</ref>, kies rezulto estas
        <frm am="(n,p)">(<k>n</k>,<k>p</k>)</frm>-matrico kun &gcirc;enerala
        elemento
        <frm am="lambda a_(ij)">&lambda;a<sub><k>ij</k></sub></frm>:
        <ekz>
          <tld/>o de matrico de <frm am="A">A</frm> per skalaro
          <frm am="lambda">&lambda;</frm>
          havas saman rezulton kiel dekstra a&ubreve; maldekstra <tld/>o
          per diagonala matrico, kies &ccirc;iuj diagonalaj elementoj egalas
          al <frm am="lambda">&lambda;</frm>;
        </ekz>
        <ekz>
          provizite per <tld/>o per skalaro, la adicia grupo de
          <frm am="(n,p)">(<k>n</k>,<k>p</k>)</frm>-matricoj fari&gcirc;as
          <frm am="np"><k>np</k></frm>-dimensia <ref cel="spac.vektora0o">vektora spaco</ref>, kaj la ringo de
          <frm am="(n,p)">(<k>n</k>,<k>n</k>)</frm>-matricoj
          fari&gcirc;as <frm am="n^2"><k>n</k><sup>2</sup></frm>-dimensia
          <ref cel="algebr.lineara0o.strukturo">lineara algebro</ref>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="multip.0o.de_matricoj">
      <uzo tip="klr">(de matricoj)</uzo>
      <ref tip="dif" cel="multip.matrica0o">matrica <tld/>o</ref>
    </subsnc>
  </snc>
  <refgrp tip="vid">
    <ref cel="faktor.0o.multip">faktoro</ref>,
    <ref cel="produt.0o">produto</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_m;&c_n;&c_o;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_n;&c_a;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">multiplicaci&oacute;</trd>
  <trd lng="de">Multiplikation</trd>
  <trd lng="en">multiplication</trd>
  <trd lng="es">multiplicaci&oacute;n</trd>
  <trd lng="fr">multiplication</trd>
  <trd lng="hu">szorz&aacute;s</trd>  
  <trd lng="nl">vermenigvuldiging</trd>
  <trdgrp lng="pl">
    <trd>mno&zdot;enie</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sv">multiplikation</trd>
  <trd lng="sk">n&aacute;sobenie</trd>
  <trd lng="cs">n&aacute;soben&iacute;</trd>
</drv>

