<drv mrk="multip.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="multip.0i.elemente">
    <fnt><bib>PV</bib></fnt>
    <dif>
      <klr>(nombron <frm am="q"><k>q</k></frm> per entjero
      <frm am="n"><k>n</k></frm>) </klr>
      <ref cel="adici.0i">Adicii</ref> <frm am="n"><k>n</k></frm>
      ekzemplerojn
      de la nombro <frm am="q"><k>q</k></frm>:
      <ekz>
        <tld/>ante <frm>5</frm> per <frm>3</frm> oni ricevas
        <frm>15</frm>;
      </ekz>
      <ekz>
        se <tld/>i tiujn 40 cendojn per 3000 miliardoj, ni vidas, ke la kosto estas 1200
        miliardoj por la tuta generacio
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2013/010913.php">De la gepatroj lernu!</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        akumuli&gcirc;o de riskaj faktoroj havas ne suman, sed <tld/>an efikon
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/011124.php">Atentu la koron</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        klimata transformi&gcirc;o funkcios kiel &leftquot;<tld/>ilo&rightquot; a&ubreve; &leftquot;akcelilo&rightquot; de minacoj koncerne
        al konfliktoj kaj nestabileco
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2014/011272.php">Klimata &scirc;an&gcirc;i&gcirc;o kaj konfliktoj: kia
          interrilato?</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      En nefaka kunteksto oni povas diri <ctl>multobligi</ctl>
      anstata&ubreve; <ctl><tld/>i</ctl>.
    </rim>
    <refgrp tip="sin">
      <ref cel="obl.mult0igi.multipliki">multobligi</ref>,
      <ref cel="obl.0igi">obligi</ref>.
    </refgrp>
    <trd lng="de">malnehmen</trd>
  </snc>
  <snc mrk="multip.0i.neelemente">
    <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
    <uzo tip="klr"> (pli &gcirc;enerale)</uzo>
    <dif>
      Plenumi la operacion
      <ref cel="multip.0o.neelementa"><tld/>o <sncref/></ref>:
      <ekz>
        <tld/>i reelojn, polinomojn, matricojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">vermeerder</trd>
