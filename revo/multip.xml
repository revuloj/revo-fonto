<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: multip.xml,v 1.42 2020/02/15 18:10:33 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>multiplik</rad>/i <fnt><bib>PV</bib></fnt>
  <fnt>Z</fnt>
</kap>
<drv mrk="multip.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="multip.0i.elemente">
    <fnt><bib>PV</bib></fnt>
    <dif>
      <klr>(nombron <frm am="q"><k>q</k></frm> per entjero
      <frm am="n"><k>n</k></frm>) </klr>
      <ref cel="adici.0i">Adicii</ref> <frm am="n"><k>n</k></frm>
      ekzemplerojn
      de la nombro <frm am="q"><k>q</k></frm> :
      <ekz>
        <tld/>ante <frm>5</frm> per <frm>3</frm> oni ricevas
        <frm>15</frm>.
      </ekz>
    </dif>
    <rim>
      En nefaka kunteksto oni povas diri <ctl>multobligi</ctl>
anstata&ubreve;
      <ctl><tld/>i</ctl>.
    </rim>
    <refgrp tip="sin">
      <ref cel="obl.mult0igi.multipliki">multobligi</ref>,
      <ref cel="obl.0igi">obligi</ref>.
    </refgrp>
    <trd lng="de">malnehmen</trd>
  </snc>
  <snc mrk="multip.0i.neelemente">
    <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
    <uzo tip="klr">(pli &gcirc;enerale)</uzo>
    <dif>
      Plenumi la operacion
      <ref cel="multip.0o.neelementa"><tld/>o <sncref/></ref>:
      <ekz>
        <tld/>i reelojn, polinomojn, matricojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_n;&c_o;&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_n;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">multiplicar</trd>
  <trdgrp lng="cs">
    <trd>multiplikovat</trd>,
    <trd>n&aacute;sobit</trd>
  </trdgrp>
  <trd lng="de">multiplizieren</trd>
  <trd lng="en">multiply</trd>
  <trd lng="es">multiplicar</trd>
  <trd lng="fr">multiplier</trd>
  <trd lng="hu">szoroz</trd>
  <trd lng="nl">vermenigvuldigen</trd>
  <trd lng="pl">mno&zdot;y&cacute;</trd>
  <trd lng="pt">multiplicar <klr tip="amb">(mat.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_m;&c_n;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_m;&c_n;&c_o;&c_zh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">n&aacute;sobi&tcaron;</trd>
  <trd lng="sv">multiplicera</trd>
</drv>

<drv mrk="multip.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="multip.0o.elementa">
    <uzo tip="klr">(elementa aritmetiko)</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 15</lok></fnt>
    <dif>
      La operacio <ref cel="multip.0i">multipliki</ref>:
      <ekz>
        <frm am="3*7=21">3&#215;7 = 21</frm> <klr> (legu: trioble sep estas
        dudek
        unu, a&ubreve; tri multiplikite per sep estas dudek unu)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="multip.0ato">multiplikato</ref>,
      <ref cel="multip.0anto">multiplikanto</ref>.
    </refgrp>
  </snc>
  <snc mrk="multip.0o.neelementa">
    <dif>
      Simila operacio, kaj pli precize:
    </dif>
    <subsnc mrk="multip.0o.ringo">
      <fnt><bib>PIV2</bib><lok>ringo</lok></fnt>
      <dif>
        <klr>(en <ref cel="ring.0o.MAT">ringo</ref>)</klr> &Gcirc;ia dua
        <ref tip="super" cel="operac.0o.MAT">operacio</ref>.
      </dif>
    </subsnc>
    <subsnc mrk="multip.0o.modulo">
      <dif>
        <klr>(en <ref cel="modul.0o.strukt">modulo</ref> a&ubreve;
        <ref cel="spac.vektora0o">vektora spaco</ref>)</klr> &Gcirc;ia
        <ref tip="super" cel="operac.ekstera0o.aro">ekstera
        operacio</ref>.
      </dif>
    </subsnc>
    <subsnc mrk="multip.0o.de_matrico_per_skalaro">
      <dif>
        <klr>(de <ref cel="matric.n_p0o">(n,p)-matrico</ref>
        <frm am="A">A</frm> super <frm am="bb K"><g>K</g></frm> per 
        elemento <frm am="lambda">&lambda;</frm>
        de <frm am="bb K"><g>K</g></frm>) </klr>
        <ref tip="super" cel="operac.ekstera0o.aro">Ekstera
        operacio</ref>, kies rezulto estas
        <frm am="(n,p)">(<k>n</k>,<k>p</k>)</frm>-matrico kun &gcirc;enerala
        elemento
        <frm am="lambda a_(ij)">&lambda;a<sub><k>ij</k></sub></frm>:
        <ekz>
          <tld/>o de matrico de <frm am="A">A</frm> per skalaro
          <frm am="lambda">&lambda;</frm>
          havas saman rezulton kiel dekstra a&ubreve; maldekstra <tld/>o
          per diagonala matrico, kies &ccirc;iuj diagonalaj elementoj egalas
          al <frm am="lambda">&lambda;</frm>;
        </ekz>
        <ekz>
          provizite per <tld/>o per skalaro, la adicia grupo de
          <frm am="(n,p)">(<k>n</k>,<k>p</k>)</frm>-matricoj fari&gcirc;as
          <frm am="np"><k>np</k></frm>-dimensia <ref
          cel="spac.vektora0o">vektora spaco</ref>, kaj la ringo de
          <frm am="(n,p)">(<k>n</k>,<k>n</k>)</frm>-matricoj
          fari&gcirc;as <frm am="n^2"><k>n</k><sup>2</sup></frm>-dimensia
          <ref cel="algebr.lineara0o.strukturo">lineara algebro</ref>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="multip.0o.de_matricoj">
      <dif>
        <klr>(de matricoj)</klr>
      </dif>
      <ref tip="dif" cel="multip.matrica0o">matrica <tld/>o.</ref>
    </subsnc>
  </snc>
  <refgrp tip="vid">
    <ref cel="faktor.0o.multip">faktoro</ref>,
    <ref cel="produt.0o">produto</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_m;&c_n;&c_o;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_m;&c_n;&c_a;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">multiplicaci&oacute;</trd>
  <trd lng="de">Multiplikation</trd>
  <trd lng="en">multiplication</trd>
  <trd lng="es">multiplicaci&oacute;n</trd>
  <trd lng="fr">multiplication</trd>
  <trd lng="hu">szorz&aacute;s</trd>  
  <trd lng="nl">vermenigvuldiging</trd>
  <trdgrp lng="pl">
    <trd>mno&zdot;enie</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sv">multiplikation</trd>
  <trd lng="sk">n&aacute;sobenie</trd>
  <trd lng="cs">n&aacute;soben&iacute;</trd>
</drv>

<drv mrk="multip.0anto">
  <kap><tld/>anto</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Nombro, per kiu oni <ref cel="multip.0i"><tld/>as</ref> alian
      nombron.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_n;&c_o;&c_zh;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">multiplicador</trd>
  <trdgrp lng="cs">
    <trd>multiplik&aacute;tor</trd>,
    <trd>n&aacute;sobitel</trd>
  </trdgrp>
  <trd lng="de">Multiplikator</trd>
  <trd lng="en">multiplier</trd>
  <trd lng="es">multiplicador</trd>
  <trd lng="fr">multiplicateur</trd>
  <trd lng="hu">szorz&oacute;</trd>
  <trd lng="nl">vermenigvuldiger</trd>
  <trdgrp lng="pl">
    <trd>mno&zdot;nik</trd>,
    <trd>multiplikator</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_n;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">n&aacute;sobite&lcaron;</trd>
</drv>

<drv mrk="multip.0ato">
  <kap><tld/>ato</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Nombro, kiun oni <ref cel="multip.0i"><tld/>as</ref> per alia
      nombro.
    </dif>
    <ref tip="sin" cel="multip1.0o">multiplikendo.</ref>
    <rim>
      La&ubreve; la donitaj difinoj estas malfacile distingi <tld/>anton
      kaj
      <tld/>aton, &ccirc;ar &ccirc;iu el ili respondas al la difino de
      la
alia.
      Pli oportune estas paroli pri <ctl>dekstra</ctl> a&ubreve; 
      &leftquot;maldekstra <ref
cel="faktor.0o.multip">faktoro</ref>&rightquot;.
    </rim>
  </snc>
  <trd lng="be">&c_m;&c_n;&c_o;&c_zh;&c_y;&c_v;&c_a;</trd>
  <trd lng="ca">multiplicand</trd>
  <trdgrp lng="cs">
    <trd>multiplik&aacute;t</trd>,
    <trd>n&aacute;sobenec</trd>
  </trdgrp>
  <trd lng="de">Multiplikand</trd>
  <trd lng="en">multiplicand</trd>
  <trd lng="es">multiplicando</trd>
  <trd lng="fr">multiplicande</trd>
  <trd lng="hu">szorzand&oacute;</trd>
  <trd lng="nl">vermenigvuldigtal</trd>
  <trd lng="pl">mno&zdot;na</trd>
  <trd lng="ru">&c_m;&c_n;&c_o;&c_zh;&c_i;&c_m;&c_o;&c_je;</trd>
  <trd lng="sk">n&aacute;sobenec</trd>
</drv>

<drv mrk="multip.matrica0o">
  <kap>matrica <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(de <ref cel="matric.n_p0o">(n,p)-matrico</ref> 
      <frm am="A">A</frm> per
      <frm am="(p,q)">(<k>p</k>,<k>q</k>)</frm>-matrico 
      <frm am="B">B</frm>) </klr>
      <ref tip="super" cel="operac.0o.MAT">Operacio</ref>, kies rezulto
      estas <frm am="(n,q)">(<k>n</k>,<k>q</k>)</frm>-matrico 
      <frm am="C">C</frm> kun &gcirc;enerala elemento
      <frm am="c_(ij)=sum_m a_(im)*b_(mj)"
      >c<sub><k>ij</k></sub> = &#8721;<sub><k>m</k></sub>
      a<sub><k>im</k></sub>&middot;b<sub><k>mj</k></sub></frm>,
      kie <frm am="m"><k>m</k></frm> varias inter <frm>1</frm> kaj
      <frm am="p"><k>p</k></frm>:
      <ekz>
        la matrica <tld/>o ne estas difinita por ajna paro de matricoj;
      </ekz>
      <ekz>
        provizite per matricaj adicio kaj <tld/>o, la aro de &ccirc;iuj
        <frm am="(n,n)">(<k>n</k>,<k>n</k>)</frm>-matricoj, ricevas la 
        strukturon de ringo, nekomuteca se 
        <frm am="n gt 1"><k>n</k> &gt; 1</frm>, kies unuo estas
        la <ref cel="matric.unuo0o">unuomatrico</ref>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Kontroli trd de/en. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_m;&c_a;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_je;
  <ind>&c_m;&c_n;&c_o;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  <trd lng="ca"><ind>multiplicaci&oacute;</ind> matricial</trd>
  <trd lng="de">Matrizenmultiplikation</trd>
  <trd lng="en">matrix <ind>multiplication</ind></trd>
  <trdgrp lng="es">
    <trd>multiplicaci&oacute;n de matrices</trd>,
    <trd>producto de matrices</trd>
  </trdgrp>
  <trd lng="fr">multiplication matricielle</trd>
  <trd lng="hu">m&aacute;trixszorz&aacute;s</trd>  
  <trd lng="pl">mno&zdot;enie macierzy</trd>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_r;&c_i;&c_ch;&c_n;&c_o;&c_je;
<ind>&c_u;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  <trd lng="sk">maticov&eacute; n&aacute;sobenie</trd>
  <trd lng="cs">maticov&eacute; n&aacute;soben&iacute;</trd>
</drv>

<drv mrk="multip.polinoma0o">
  <kap>polinoma <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Dua <ref tip="super" cel="operac.0o.MAT">operacio</ref> en
      <ref cel="ring.polinom0o">polinom-ringo</ref>:
      <ekz>
        la &gcirc;enerala termo <frm am="bb
        S_n"><g>S</g><sub><k>n</k></sub></frm>
        de la rezulto de <tld/>o de <frm am="bb P"><g>P</g></frm> kun
        <frm am="bb Q"><g>Q</g></frm> estas
        <frm am="sum_(p+q=n)bb P_p*bb Q_q"
        >&#8721;<sub><k>p</k>+<k>q</k>=<k>n</k></sub>
        <g>P</g><sub><k>p</k></sub>&middot;<g>Q</g><sub><k>q</k></sub></frm>;
      </ekz>
      <ekz>
        la rezulto de <tld/>o de <frm am="bb
        X^i"><g>X</g><sup><k>i</k></sup></frm>
        per <frm am="bb X^j"><g>X</g><sup><k>j</k></sup></frm> estas
        <frm am="bb X^(i+j)"><g>X</g><sup><k>i</k>+<k>j</k></sup></frm>.
      </ekz>
    </dif>
  </snc>
  <adm>
    Kontroli trd de/en/ru. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_m;&c_n;&c_o;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;
  &c_m;&c_n;&c_a;&c_g;&c_a;&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_w;</trd>
  <trd lng="ca"><ind>multiplicaci&oacute;</ind> polin&ograve;mica</trd>
  <trd lng="de">Polynommultiplikation</trd>
  <trd lng="en">polynomial <ind>multiplication</ind></trd>
  <trd lng="fr">multiplication polynomiale</trd>
  <trd lng="hu">polinomszorz&aacute;s</trd>  
  <trd lng="pl">mno&zdot;enie wielomian&oacute;w</trd>
  <trd lng="ru">&c_u;&c_m;&c_n;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;
&c_m;&c_n;&c_o;&c_g;&c_o;&c_ch;&c_l;&c_je;&c_n;&c_o;&c_v;</trd>
  <trd lng="sv">polynommultiplikation</trd>
</drv>
</art>
