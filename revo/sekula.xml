<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sekula.xml,v 1.18 2023/08/03 21:23:25 revo Exp $">
<kap>
  <rad>sekulariz</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="sekula.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="dif" cel="sekular.0igi">Sekularigi</ref>:
      <ekz>
        Alberto en 1525 <tld/>is la monaksimilan &Scirc;taton de la te&ubreve;tonaj ordenanoj
        <nom>Insterburg</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=88545"
              >&Ccirc;ernja&hcirc;ovsk</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        a francoj oficiale transdonis la duklandon de Tirolo inkluzive de la <tld/>ita
        episkopejo de Trento al Bavario la 11-an de februaro 1806
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=462621"
              >Tirola ribelo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_k;&c_u;&c_l;&c_ja;&c_r;&c_y;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>sekularizovat</trd>,
    <trd>zesv&ecaron;t&scaron;tit</trd>
  </trdgrp>
  <trd lng="de">s&auml;kularisieren</trd>
  <trd lng="fr">s&eacute;culariser</trd>
  <trdgrp lng="hu">
    <trd>szekulariz&aacute;l</trd>,
    <trd>elvil&aacute;gias&iacute;t</trd>
  </trdgrp>
  <trd lng="it">laicizzare</trd>
  <trd lng="nl">seculariseren</trd>
  <trdgrp lng="pl">
    <trd>sekularyzowa&cacute;</trd>,
    <trd>ze&sacute;wieccza&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_je;&c_k;&c_u;&c_l;&c_ja;&c_r;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">zosvet&scaron;ti&tcaron;</trd>
</drv>
</art>
