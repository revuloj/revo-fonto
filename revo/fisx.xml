<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fi&scirc;</rad>/o
</kap>
<drv mrk="fisx.0o">
<kap><tld/>o</kap>
<snc>
<dif>
Vertebrulo, kun brankoj kaj na&gcirc;iloj, vivanta en akvo: <ekz>la
man&gcirc;ota <tld/>o estas ankora&ubreve; en la rivero<fnt>Z</fnt> </ekz>
</dif>
<refgrp tip="sin">
<ref cel="cert.mal0a">necerta</ref>.
</refgrp>
</snc>
<trd lng="de">Fisch</trd>
<trd lng="en">fish</trd>
</drv>
<drv mrk="fisx.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
  <dif>
    Klaso de <ref tip="super" cel="verteb.0uloj">vertebruloj</ref>
    (<trd lng="la">Pisces</trd>), kun brankoj kaj na&gcirc;iloj, vivantaj en
    akvo.
   </dif>
  </snc>
  <trd lng="de">Fische</trd>
</drv>
<drv mrk="fisx.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
    Tiu, kiu bredas, kaptas a&ubreve; vendas <tld/>ojn. 
    </dif>
  </snc>
  <trd lng="de">Fischer</trd>
</drv>
<drv>
  <kap><tld/>(kapt)i, <tld/>&ccirc;asi</kap> 
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provi kapti <tld/>ojn:
      <ekz><tld/>kaptisto. </ekz>
    </dif>
  </snc>
  <trdgrp lng="de"><trd>fischen</trd>, <trd>angeln</trd></trdgrp>
</drv>
<drv mrk="fisx.0hoko">
<kap><tld/>hoko</kap>
<snc>
<dif>
Hoko por <tld/>kapti.  
</dif>
</snc>
<trd lng="de">Angelhaken</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>