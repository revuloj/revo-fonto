<drv mrk="fisx.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="fisx.0isto.CXAS">
    <dif>
      Tiu, kiu okupi&gcirc;as pri kaptado de <tld/>oj, &ccirc;u profesie a&ubreve; libertempe;
      <ref tip="sin" cel="kapt.fisx0isto">fi&scirc;kaptisto</ref>:
      <ekz>
        <tld/>istoj kaptadis <tld/>ojn sur la maro &ccirc;e lumo de tor&ccirc;oj
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        nila <tld/>isto rapide eltiris la retojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la <tld/>isto eltiris la reton el la maro, li kriis tre
	kontente: &mdash; benita providenco!
        <fnt>
          <aut>Carlo Collodi, trad. Giuseppe Croatini</aut>
          <vrk>La Aventuroj de Pinokjo</vrk><lok>&ccirc;apitro
		28a</lok>
        </fnt>
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_y;&c_b;&c_a;&c_k;</trd>,
      <trd>&c_r;&c_y;&c_b;&c_a;&c_l;&c_o;&c_w;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>pescador</trd>,
      <trd>pescaire</trd>
    </trdgrp>
    <trd lng="de">Fischer</trd>
    <trd lng="fr">p&ecirc;cheur</trd>
    <trdgrp lng="es">
      <trd>pescador</trd>,
      <trd>piscicultor</trd>
    </trdgrp>
    <trd lng="he">&dalet;&yod;&yod;&#8503;</trd>
    <trdgrp lng="hu">
      <trd>hal&aacute;sz</trd>,
      <trd>halteny&eacute;szt&odblac;</trd>,
      <trd>hal&aacute;rus</trd>
    </trdgrp>
    <trd lng="id">nelayan</trd>
    <trd lng="nl">visser</trd>
    <trdgrp lng="pl">
      <trd>rybak</trd>,
      <trd>w&eogonek;dkarz</trd>
    </trdgrp>
    <trd lng="ro">pescar</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_y;&c_b;&c_a;&c_k;</trd>,
      <trd>&c_r;&c_y;&c_b;&c_o;&c_l;&c_o;&c_v;</trd>
    </trdgrp>
    <trd lng="sk">ryb&aacute;r</trd>
    <trd lng="tr">bal&inodot;k&ccedil;&inodot;</trd>
    <trd lng="zh">&#x9B5A;&#x592B; <pr>y&#xFA;f&#x16B;</pr></trd>
</snc>
  <snc mrk="fisx.0isto.komercisto">
    <dif>
      <ref cel="fisx.0bredisto">Fi&scirc;bredisto</ref>
      a&ubreve; <ref cel="fisx.0vendisto">fi&scirc;vendisto</ref>.
    </dif>
  <trdgrp lng="id">
    <trd><baz>jual</baz>penjual ikan</trd>,
    <trd><baz>ternak</baz>peternak ikan</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x6F01;&#x5E2B;  <pr>&#x308A;&#12423;&#12358;&#12375;</pr></trd>,
    <trd>&#x990A;&#x9B5A;&#x5BB6;  <pr>&#12424;&#12358;&#x304E;&#12423;&#x304B;</pr></trd>,
    <trd>&#x9B5A;&#x5C4B;  <pr>&#12373;&#x304B;&#x306A;&#12420;</pr></trd>
  </trdgrp>
</drv>

