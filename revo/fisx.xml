<drv mrk="fisx.F0oj">
  <kap><tld lit="F"/>oj</kap>
  <mlg>Psc</mlg>
  <mlg>&#9811;&#xFE0E;</mlg>
  <snc mrk="fisx.F0oj.ZOO">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="lst" cel="stel.0figuro" lst="voko:zodiakaj_signoj" val="12">Stelfiguro</ref> kaj
      signo de la
      <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Pisces</trd>):
      <ekz>Por <tld lit="F"/>oj la &ccirc;efaj momentoj dum la jaro
      estos
      demandoj pri la hejmo
        <fnt>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk>Nostalgie</vrk>
          <bib>Monato</bib>
          <lok>jaro 2001a, numero 5a, p. 34a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_R;&c_y;&c_b;&c_y; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="bg">&c_R;&c_i;&c_b;&c_i;</trd>
  <trd lng="ca">(els) Peixos</trd>
  <trd lng="de">Fische</trd>
  <trd lng="es">Piscis</trd>
  <trd lng="fr">Poissons<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="he">&mem;&zayin;&lamed; &dalet;&#8503;&yod;&fmem;</trd>
  <trd lng="hu">Halak</trd>
  <trdgrp lng="id">
    <trd>Hut <klr>(konstelasi)</klr></trd>,
    <trd>Mina <klr>(konstelasi)</klr></trd>,
    <trd>Pises <klr>(konstelasi)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x9B5A;&#x985E;  <pr>&#x304E;&#12423;&#x308B;&#12356;</pr></trd>
  <trd lng="nl">vissen</trd>
  <trd lng="pl">Ryby  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Pe&#537;tii  <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="ru">&c_R;&c_y;&c_b;&c_y;</trd>
  <trd lng="sk">Ryby</trd>
  <trd lng="th">&#xE01;&#xE25;&#xE38;&#xE48;&#xE21;&#xE14;&#xE32;&#xE27;&#xE1B;&#xE25;&#xE32;</trd>
  <trd lng="uk">&c_R;&c_i;&c_b;&c_i;</trd>
  <trd lng="zh">&#x53CC;&#x9C7C; <pr>shu&amacron;ngy&#xFA;</pr></trd>
