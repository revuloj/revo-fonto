<drv mrk="fisx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fisx.0i.CXAS">
    <dif>
      Provi kapti <tld/>ojn:
      <ekz>
        &ccirc;u vi permesas al mi kuniri, kiam vi fi&scirc;as
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>?
      </ekz>
      <ekz>
        la &scirc;ipestro <tld/>is pezan predon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 35</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kapt.fisx0i">fi&scirc;kapti</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_b;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_v;&c_ib;&c_c;&c_mol; &c_r;&c_y;&c_b;&c_u;</trd>
  </trdgrp>
  <trd
  lng="bo">&t_la;&t_cata;&t_ga;&t_sa;&t_tsheg;&t_ka;&t_yata;&t_shabchu;&t_tsheg;&t_da;&t_ba;&t_yata;&t_shabchu;&t_ga;&t_tsheg;</trd>
  <trd lng="ca">pescar</trd>
  <trdgrp lng="de">
    <trd>fischen</trd>,
    <trd>angeln</trd>
  </trdgrp>
  <trd lng="nl">vissen</trd>
  <trd lng="es">pescar</trd>
  <trd lng="fr">p&ecirc;cher</trd>
  <trd lng="he">&lamed;&dalet;&vav;&#8503;</trd>
  <trd lng="hu">hal&aacute;szik</trd>
  <trd lng="id"><baz>pancing</baz>memancing</trd>
  <trd lng="pl">&lstroke;owi&cacute; <ind>ryby</ind></trd>
  <trd lng="ro">pescuit cu undi&tcedil;a </trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_y;&c_b;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_i;&c_t;&c_mol; &c_r;&c_y;&c_b;&c_u;</trd>
  </trdgrp>
  <trd lng="th">&#3592;&#3633;&#3610;&#3611;&#3621;&#3634;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_i;&c_b;&c_a;&c_l;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_i;&c_t;&c_i; &c_r;&c_i;&c_b;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="fisx.F0oj">
  <kap><tld lit="F"/>oj</kap>
  <mlg>Psc</mlg>
  <mlg>&#9811;&#65038;</mlg>
  <snc mrk="fisx.F0oj.ZOO">
    <uzo tip="fak">AST</uzo>
    <dif>
      <ref tip="lst" cel="stel.0figuro" lst="voko:zodiakaj_signoj" val="12">Stelfiguro</ref> kaj
      signo de la 
      <ref tip="malprt" cel="zodiak.0o">zodiako</ref> 
      (<trd lng="la">Pisces</trd>):
      <ekz>Por <tld lit="F"/>oj la &ccirc;efaj momentoj dum la jaro
      estos
      demandoj pri la hejmo
        <fnt>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk>Nostalgie</vrk>
          <bib>Monato</bib>
          <lok>jaro 2001a, numero 5a, p. 34a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_R;&c_y;&c_b;&c_y; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="bg">&c_R;&c_i;&c_b;&c_i;</trd>
  <trd lng="ca">(els) Peixos</trd>
  <trd lng="de">Fische</trd>
  <trd lng="es">Piscis</trd>
  <trd lng="fr">Poissons<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="he">&mem;&zayin;&lamed; &dalet;&#8503;&yod;&fmem;</trd>
  <trd lng="hu">Halak</trd>
  <trdgrp lng="id">
    <trd>Hut</trd>,
    <trd>Mina</trd>,
    <trd>Pises</trd>
  </trdgrp>
  <trd lng="nl">vissen</trd>
  <trd lng="pl">Ryby  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Pe&#537;tii  <klr>(constela&#539;ie)</klr></trd>
  <trd lng="ru">&c_R;&c_y;&c_b;&c_y;</trd>
  <trd lng="sk">Ryby</trd>
  <trd lng="th">&#3585;&#3621;&#3640;&#3656;&#3617;&#3604;&#3634;&#3623;&#3611;&#3621;&#3634;</trd>
  <trd lng="uk">&c_R;&c_i;&c_b;&c_i;</trd>
</drv>

<drv mrk="fisx.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="fisx.0isto.CXAS">
    <dif>
      Tiu, kiu okupi&gcirc;as pri kaptado de <tld/>oj, &ccirc;u profesie a&ubreve; libertempe;
      <ref tip="sin" cel="kapt.fisx0isto">fi&scirc;kaptisto</ref>:
      <ekz>
        kiam la <tld/>isto eltiris la reton el la maro, li kriis tre
	kontente: &mdash;Benita providenco!
        <fnt>
          <aut>Carlo Collodi, trad. Giuseppe Croatini</aut>
          <vrk>La Aventuroj de Pinokjo</vrk><lok>&ccirc;apitro
		28a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_r;&c_y;&c_b;&c_a;&c_l;&c_o;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>pescador</trd>,
    <trd>pescaire</trd>
  </trdgrp>
  <trd lng="de">Fischer</trd>
  <trd lng="fr">p&ecirc;cheur</trd>
  <trdgrp lng="es">
    <trd>pescador</trd>,
    <trd>piscicultor</trd>
  </trdgrp>
  <trd lng="he">&dalet;&yod;&yod;&#8503;</trd>
  <trdgrp lng="hu">
    <trd>hal&aacute;sz</trd>,
    <trd>halteny&eacute;szt&odblac;</trd>,
    <trd>hal&aacute;rus</trd>
  </trdgrp>
  <trd lng="id">nelayan</trd>
  <trd lng="nl">visser</trd>
  <trdgrp lng="pl">
    <trd>rybak</trd>,
    <trd>w&eogonek;dkarz</trd>
  </trdgrp>
  <trd lng="ro">pescar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_y;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_r;&c_y;&c_b;&c_o;&c_l;&c_o;&c_v;</trd>
  </trdgrp>
  <trd lng="sk">ryb&aacute;r</trd>
  <trd lng="tr">bal&inodot;k&ccedil;&inodot;</trd>
</drv>

