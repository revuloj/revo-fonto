      cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Chondrichtyes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_a;&c_s;&c_t;&c_k;&c_o;&c_v;&c_y;&c_ja; &c_r;&c_y;&c_b;&c_y;</trd>
