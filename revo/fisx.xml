<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fi&scirc;</rad>/o
</kap>

<drv mrk="fisx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Vertebrulo, kun brankoj kaj na&gcirc;iloj, vivanta en akvo:
      <ekz>
        la man&gcirc;ota <tld/>o estas ankora&ubreve; en la rivero
        <klr>
        (<refgrp tip="vid">
          <ref cel="cert.mal0a">necerta</ref>
        </refgrp>)</klr>
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_b;&c_a;</trd>
  <trd lng="bg">&c_r;&c_i;&c_b;&c_a;</trd>
  <trd lng="ca">peix</trd>
  <trd lng="de">Fisch</trd>
  <trd lng="en">fish</trd>
  <trd lng="es">pez</trd>
  <trd lng="fr">poisson</trd>
  <trd lng="he">&dalet;&gimel;</trd>
  <trd lng="hu">hal</trd>
  <trd lng="nl">vis</trd>
  <trd lng="pl">ryba</trd>
  <trd lng="ru">&c_r;&c_y;&c_b;&c_a;</trd>
  <trd lng="sk">ryba</trd>
  <trd lng="tp">kala</trd>
  <trd lng="tr">bal&inodot;k</trd>
  <trd lng="vo">fit</trd>
</drv>

<drv mrk="fisx.F0oj">
  <kap><tld lit="F"/>oj</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako
      </ref> (<trd lng="la">Pisces</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_R;&c_y;&c_b;&c_y;</trd>
  <trd lng="bg">&c_R;&c_i;&c_b;&c_i;</trd>
  <trd lng="ca">peixos</trd>
  <trd lng="de">Fische</trd>
  <trd lng="es">Piscis</trd>
  <trd lng="fr">Poissons</trd>
  <trd lng="he">&mem;&zayin;&lamed; &dalet;&gimel;&yod;&fmem;</trd>
  <trd lng="hu">Halak</trd>
  <trd lng="nl">vissen</trd>
  <trd lng="pl">Ryby</trd>
  <trd lng="ru">&c_R;&c_y;&c_b;&c_y;</trd>
  <trd lng="sk">Ryby</trd>
</drv>

<drv mrk="fisx.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="fisx.0isto.KOMUNE">
    <dif>
      Tiu, kiu kaptas <tld/>ojn.
    </dif>
    <subsnc mrk="fisx.0isto.INDUSTRIA">
	    <dif>
	      Tiu, kiu kaptas <tld/>ojn profesie en grandaj kvantoj ekz
	      por industrio.
	    </dif>
	    <trd lng="pl">rybak</trd>
    </subsnc>
    <subsnc mrk="fisx.0isto.PLEZURO">
	    <dif>
	      Tiu, kiu kaptas <tld/>ojn kiel por plezuro, ofte per
	      fi&scirc;ostango.
	    </dif>
	    <trd lng="pl">w&eogonek;dkarz</trd>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_r;&c_y;&c_b;&c_a;&c_l;&c_o;&c_w;</trd>
  </trdgrp>
  <trd lng="ca">peixater</trd>
  <trd lng="de">Fischer</trd>
  <trdgrp lng="fr">
    <trd>p&ecirc;cheur</trd>,
    <trd>poissonnier</trd>,
    <trd>pisciculteur</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pescador</trd>,
    <trd>piscicultor</trd>
  </trdgrp>
  <trd lng="he">&dalet;&yod;&yod;&gimel;</trd>
  <trdgrp lng="hu">
    <trd>hal&aacute;sz</trd>,
    <trd>halteny&eacute;szt&odblac;</trd>,
    <trd>hal&aacute;rus</trd>
  </trdgrp>
  <trd lng="nl">visser</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_y;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_r;&c_y;&c_b;&c_o;&c_l;&c_o;&c_v;</trd>
  </trdgrp>
  <trd lng="sk">ryb&aacute;r</trd>
  <trd lng="tr">bal&inodot;k&ccedil;&inodot;</trd>
</drv>

<drv mrk="fisx.0bredisto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu bredas <tld/>ojn.
    </dif>
  </snc>
  <adm>
	  Enkondukita laux rimarko de MB el ~isto.
	  <aut>SC</aut>
  </adm> 
</drv>

<drv mrk="fisx.0vendisto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu vendas <tld/>ojn.
    </dif>
    <trdgrp lng="pl">
      <trd>sprzedawca <ind>ryb</ind></trd>,
      <trd>handlarz <ind>rybami</ind></trd>
    </trdgrp>
  </snc>
  <adm>
	  Enkondukita laux rimarko de MB el ~isto.
	  <aut>SC</aut>
  </adm> 
</drv>

<drv mrk="fisx.0kapti">
  <kap><tld/>(kapt)i, <tld/>&ccirc;asi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provi kapti <tld/>ojn:
      <ekz>
        <ind><tld/>kaptisto</ind>.
        <trd lng="fr">p&ecirc;cheur</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_b;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_v;&c_ib;&c_c;&c_mol; &c_r;&c_y;&c_b;&c_u;</trd>
  </trdgrp>
  <trd lng="ca">pescar</trd>
  <trdgrp lng="de">
    <trd>fischen</trd>,
    <trd>angeln</trd>
  </trdgrp>
  <trd lng="nl">vissen</trd>
  <trd lng="es">pescar</trd>
  <trd lng="fr">p&ecirc;cher</trd>
  <trd lng="he">&lamed;&dalet;&vav;&gimel;</trd>
  <trd lng="hu">hal&aacute;szik</trd>
  <trd lng="pl">&lstroke;owi&cacute; <ind>ryby</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_y;&c_b;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_i;&c_t;&c_mol; &c_r;&c_y;&c_b;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="fisx.0hoko">
  <kap><tld/>hoko</kap>
  <snc>
    <dif>
      Hoko por <tld/>kapti:
      <ekz>
        &ccirc;u vi povas eltiri levjatanon per <tld/>hoko, a&ubreve;
        ligi per &scirc;nuro &gcirc;ian langon?
        <fnt><bib>MT</bib>, <lok>&Ijo; 41:1</lok></fnt>;
      </ekz>
      <ekz>
        ili provizas la <tld/>hokojn per loga&jcirc;o
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_r;&c_y;&c_b;&c_n;&c_y; &c_k;&c_r;&c_u;&c_ch;&c_o;&c_k;</trd>
  <trd lng="ca">ham</trd>
  <trd lng="de">Angelhaken</trd>
  <trd lng="nl">vishaak</trd>
  <trd lng="es">anzuelo</trd>
  <trdgrp lng="he">
    <trd>&tsadi;&lamed;&tsadi;&lamed; &dalet;&gimel;&yod;&fmem;</trd>,
    <trd>&het;&kaf;&he;</trd>
  </trdgrp>
  <trd lng="fr">hame&ccedil;on</trd>
  <trd lng="hu">horog</trd>
  <trdgrp lng="pl">
    <trd>haczyk<klr> na ryby</klr></trd>,
    <trd>w&eogonek;dka<klr> (obrazowo - jako narz&eogonek;dzie z 
    haczykiem)</klr></trd>,
    <trd>sznur <ind>haczykowy</ind><klr> (obrazowo -
    jako narz&eogonek;dzie z haczykiem)</klr></trd>
  </trdgrp>
  <trd lng="ru">
  &c_r;&c_y;&c_b;&c_o;&c_l;&c_o;&c_v;&c_n;&c_y;&c_j;
  &c_k;&c_r;&c_ju;&c_ch;&c_o;&c_k;
  </trd>
  <trd lng="sk">h&aacute;&ccaron;ik <klr>(na ryby)</klr></trd>
  <adm>
  La&ubreve; Fundamento fi&scirc;'hok' estas
  hame&ccedil;on | fishing-hook | Fischangel |
  &c_u;&c_d;&c_a;, &c_u;&c_d;&c_o;&c_ch;&c_k;&c_a; | w&eogonek;dka
  </adm>
</drv>

<drv mrk="fisx.dorn0eto">
  <kap>dorn<tld/>eto</kap>
  <snc>
    <ref tip="dif" cel="gaster.0o">gasterosteo.</ref>
  </snc>
  <trd lng="ca">espina</trd>
  <trd lng="es">espina</trd>
  <trd lng="fr">&eacute;pinoche</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_ju;&c_sh;&c_k;&c_a;</trd>
</drv>

<drv mrk="fisx.kartilago0oj">
  <kap>kartilago<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Chondrichtyes</trd>), fi&scirc;oj kun kartilaga
      skeleto.
    </dif>
  </snc>
  <trd lng="de">Knorpelfische</trd>
  <trd lng="fr">chondrichtyens</trd>
  <trd lng="hu">porcoshalak</trd>
  <trd lng="nl">kraakbeenvissen</trd>
  <trd lng="pl">ryby chrz&eogonek;stnoszkieletowe</trd>
  <trd lng="ru">
  &c_h;&c_r;&c_ja;&c_shch;&c_je;&c_v;&c_y;&c_je; &c_r;&c_y;&c_b;&c_y;
  </trd>
</drv>

<drv mrk="fisx.osto0oj">
  <kap>osto<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Osteichtyes</trd>), fi&scirc;oj kun osta skeleto.
    </dif>
  </snc>
  <trd lng="de">Knochenfische</trd>
  <trd lng="hu">csontoshalak</trd>
  <trdgrp lng="fr">
    <trd>poissons osseux</trd>,
    <trd>ost&eacute;ichtyens</trd>
  </trdgrp>
  <trd lng="nl">beenvissen</trd>
  <trd lng="pl">ryby ko&sacute;ciste</trd>
  <trd lng="ru">
  &c_k;&c_o;&c_s;&c_t;&c_i;&c_s;&c_t;&c_y;&c_je; &c_r;&c_y;&c_b;&c_y;
  </trd>
</drv>
</art>
