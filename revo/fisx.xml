<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fi&scirc;</rad>/o
</kap>
<drv mrk="fisx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Vertebrulo, kun brankoj kaj na&gcirc;iloj, vivanta en akvo:
      <ekz>
        la man&gcirc;ota <tld/>o estas ankora&ubreve; en la rivero
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cert.mal0a">necerta</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Fisch</trd>
  <trd lng="en">fish</trd>
  <trd lng="fr">poisson</trd>
  <trd lng="nl">vis</trd>
  <trd lng="tr">bal&inodot;k</trd>
</drv>
<drv mrk="fisx.F0oj">
  <kap><tld lit="F"/>oj</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Pisces</trd>), tra kiu la suno pasas de la 20a de
      februaro &gcirc;is la 20a de marto.  
    </dif>
  </snc>
  <trd lng="de">Fische</trd>
</drv>
<drv mrk="fisx.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu bredas, kaptas a&ubreve; vendas <tld/>ojn. 
    </dif>
  </snc>
  <trd lng="de">Fischer</trd>
  <trd lng="tr">bal&inodot;k&ccedil;&inodot;</trd>
</drv>
<drv mrk="fisx.0kapti">
  <kap><tld/>(kapt)i, <tld/>&ccirc;asi</kap> 
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provi kapti <tld/>ojn:
      <ekz><tld/>kaptisto. </ekz>
    </dif>
  </snc>
  <trdgrp lng="de"><trd>fischen</trd>, <trd>angeln</trd></trdgrp>
  <trd lng="fr">p&ecirc;cher</trd>
</drv>
<drv mrk="fisx.0hoko">
  <kap><tld/>hoko</kap>
  <snc>
    <dif>
      Hoko por <tld/>kapti.  
    </dif>
  </snc>
  <trd lng="de">Angelhaken</trd>
  <trd lng="fr">hema&ccedil;on</trd>
</drv>
<drv mrk="fisx.kartilago0oj">
  <kap>kartilago<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Chondrichtyes</trd>), fi&scirc;oj kun kartilaga skeleto.
     </dif>
  </snc>
  <trd lng="de">Knorpelfische</trd>
</drv>
<drv mrk="fisx.osto0oj">
  <kap>osto<tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Osteichtyes</trd>), fi&scirc;oj kun osta skeleto.
    </dif>
  </snc>
  <trd lng="de">Knochenfische</trd>
</drv>
</art>
