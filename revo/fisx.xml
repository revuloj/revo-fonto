    <dif>
      <tld lit="F"/>o &ccirc;asanta aliajn fi&scirc;ojn kaj rapidajn 
      relative grandajn bestojn por nutri sin.
	  <ekz>la salamandroj estis armitaj pere de subakvaj pistoloj kun
	  dumdumaj &scirc;argoj por defendo kontra&ubreve;
	  rabo<tld/>oj<fnt><bib>Salamandroj</bib><lok>libro 2a, &ccirc;apitro
	  2a</lok></fnt></ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_p;&c_je;&c_zh;&c_n;&c_a;&c_ja; <ind>&c_r;&c_y;&c_b;&c_a;</ind></trd>
  <trd lng="de">Raubfisch</trd>
  <trd lng="pl">ryba drapie&zdot;na</trd>
  <trd lng="ro">pe&#537;te r&abreve;pitor</trd>
