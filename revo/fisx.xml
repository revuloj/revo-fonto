  <trd lng="id">todak</trd>
  <trd lng="ja">&#x30E1;&#x30AB;&#x30B8;&#x30AD;</trd>
  <trd lng="nl">zwaardvis</trd>
  <trd lng="pl">w&lstroke;&oacute;cznik  <klr>(ryba)</klr></trd>
  <trd lng="ro">pe&#537;te spad&abreve;</trd>
  <trd lng="ru">&c_m;&c_je;&c_ch;-&c_r;&c_y;&c_b;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_je;&c_ch;&c_o;&c_n;&c_ib;&c_s;</trd>,
    <trd>&c_m;&c_je;&c_ch;-&c_r;&c_i;&c_b;&c_a;</trd>
