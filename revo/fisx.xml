<drv mrk="fisx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fisx.0i.CXAS">
    <dif>
      Provi kapti <tld/>ojn:
      <ekz>
        &ccirc;u vi permesas al mi kuniri, kiam vi fi&scirc;as
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>?
      </ekz>
      <ekz>
        la &scirc;ipestro <tld/>is pezan predon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 35</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kapt.fisx0i">fi&scirc;kapti</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_b;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_v;&c_ib;&c_c;&c_mol; &c_r;&c_y;&c_b;&c_u;</trd>
  </trdgrp>
  <trd lng="bo">&t_la;&t_cata;&t_ga;&t_sa;&t_tsheg;&t_ka;&t_yata;&t_shabchu;&t_tsheg;&t_da;&t_ba;&t_yata;&t_shabchu;&t_ga;&t_tsheg;</trd>
  <trd lng="ca">pescar</trd>
  <trdgrp lng="de">
    <trd>fischen</trd>,
    <trd>angeln</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9B5A;&#12434;&#25429;&#x308B;  <pr>&#12373;&#x304B;&#x306A;&#12434;&#12392;&#x308B;</pr></trd>,
    <trd>&#x6F01;&#12434;&#12377;&#x308B;  <pr>&#x308A;&#12423;&#12358;&#12434;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">vissen</trd>
  <trd lng="es">pescar</trd>
  <trd lng="fr">p&ecirc;cher</trd>
  <trd lng="he">&lamed;&dalet;&vav;&#8503;</trd>
  <trd lng="hu">hal&aacute;szik</trd>
  <trd lng="id"><baz>pancing</baz>memancing</trd>
  <trd lng="pl">&lstroke;owi&cacute; <ind>ryby</ind></trd>
  <trd lng="ro">pescuit cu undi&#x21B;a </trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_y;&c_b;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_i;&c_t;&c_mol; &c_r;&c_y;&c_b;&c_u;</trd>
  </trdgrp>
  <trd lng="th">&#xE08;&#xE31;&#xE1A;&#xE1B;&#xE25;&#xE32;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_i;&c_b;&c_a;&c_l;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_l;&c_o;&c_v;&c_i;&c_t;&c_i; &c_r;&c_i;&c_b;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6E14; <pr>y&#xFA;</pr></trd>,
    <trd>&#25429;&#x9B5A; <pr>b&#x1D4;y&#xFA;</pr></trd>,
    <trd>&#x9B5A;&#x696D; <pr>y&#xFA;y&#xE8;</pr></trd>
  </trdgrp>
