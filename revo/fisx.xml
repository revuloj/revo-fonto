<drv mrk="fisx.0hoko">
  <kap><ofc>*</ofc> <tld/>hoko <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="&AdE;/fundamento/universala_vortaro.html#av-5048">fi&scirc;'hok'</url></lok></fnt></kap>
  <snc mrk="fisx.0hoko.CXAS">
    <dif>
      Hoko por <tld/>kapti:
      <ekz>
        &ccirc;u vi povas eltiri levjatanon per <tld/>hoko, a&ubreve;
        ligi per &scirc;nuro &gcirc;ian langon?
        <fnt><bib>MT</bib>, <lok>&Ijo; 41:1</lok></fnt>;
      </ekz>
      <ekz>
        ili provizas la <tld/>hokojn per loga&jcirc;o
        <fnt><bib>Fab4</bib>, <lok>Historio el la dunoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_b;&c_n;&c_y;
  &c_k;&c_r;&c_u;&c_ch;&c_o;&c_k;</trd>
  <trd lng="ca">ham</trd>
  <trd lng="de">Angelhaken</trd>
  <trd lng="nl">vishaak</trd>
  <trd lng="es">anzuelo</trd>
  <trdgrp lng="he">
    <trd>&tsadi;&lamed;&tsadi;&lamed; &dalet;&#8503;&yod;&fmem;</trd>,
    <trd>&het;&kaf;&he;</trd>
  </trdgrp>
  <trd lng="fr">hame&ccedil;on</trd>
  <trd lng="hu">horog</trd>
  <trdgrp lng="pl">
    <trd>haczyk na  <ind>ryby</ind></trd>,
    <trd>w&eogonek;dka</trd>,
    <trd>sznur <ind>haczykowy</ind></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>undi&tcedil;&abreve;</trd>,
    <trd>c&acirc;rlig</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_y;&c_b;&c_o;&c_l;&c_o;&c_v;&c_n;&c_y;&c_j;
  &c_k;&c_r;&c_ju;&c_ch;&c_o;&c_k;</trd>
  <trd lng="sk">h&aacute;&ccaron;ik <klr>(na ryby)</klr></trd>
  <trd lng="th">&#3648;&#3610;&#3655;&#3604;</trd>
  <trd lng="uk">&c_r;&c_i;&c_b;&c_o;&c_l;&c_o;&c_v;&c_n;&c_i;&c_j; &c_g;&c_a;&c_ch;&c_o;&c_k;</trd>
  <adm>
    La&ubreve; Fundamento fi&scirc;'hok' estas
    hame&ccedil;on | fishing-hook | Fischangel |
    &c_u;&c_d;&c_a;, &c_u;&c_d;&c_o;&c_ch;&c_k;&c_a; | w&eogonek;dka
  </adm>
</drv>

