<art mrk="$Id: centez.xml,v 1.22 2019/06/30 09:58:52 revo Exp $">
<kap>
  <rad>centezimal</rad>/a <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="centez.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="centez.0a.MAT">
    <dif>
      <ref tip="dif" cel="cent.0uma.MAT">Centuma</ref>:
      <ekz>
        &ccirc;irka&ubreve; 50 km de Tripolo <klr>[&#8230;]</klr> okazis la plej alta temperaturo oficiale
        registrita iam sur la terglobo: + 58 <tld/>aj gradoj, en la 13a de septembro
        1922
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010870.php"
              >Forna Kalifornio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni notis plii&gcirc;on de la temperaturo je 0,6 gradoj <tld/>aj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/011067.php"
              >Everesto mem blovospiras</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="celsi.0a">celsia</ref>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_o;&c_g;&c_r;&c_a;&c_d;&c_u;&c_s;&c_n;&c_y;</trd>
  <trd lng="bg">&c_s;&c_t;&c_o;&c_t;&c_n;&c_a;</trd>
  <trd lng="ca">centesimal</trd>
  <trdgrp lng="cs">
    <trd>centezim&aacute;ln&iacute;</trd>,
    <trd>setinn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>hundertteilig</trd>,
     <trd>zentesimal</trd>,
     <trd>Zentesimal-</trd>
  </trdgrp>
  <trd lng="el">&epsilon;&kappa;&alfa;&tau;&omikron;&ny;&tau;&alfa;&delta;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">centesimal</trd>
  <trd lng="es">centesimal</trd>
  <trd lng="fr">cent&eacute;simal</trd>
  <trdgrp lng="hu">
    <trd>sz&aacute;zad beoszt&aacute;s&uacute;</trd>,
    <trd>sz&aacute;z r&eacute;szre osztott</trd></trdgrp>
  <trd lng="it">centesimale</trd>
  <trdgrp lng="nl">
   <trd>honderddelig</trd>,
   <trd>centesimaal</trd>
  </trdgrp>
  <trd lng="pl">centezymalny</trd>
  <trd lng="pt">centesimal</trd>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_g;&c_r;&c_a;&c_d;&c_u;&c_s;&c_n;&c_y;&c_j; <klr tip="amb">(&c_o;
  &c_sh;&c_k;&c_a;&c_l;&c_je;)</klr></trd>
  <trd lng="sk">stotinn&yacute;</trd>
</drv>
</art>
