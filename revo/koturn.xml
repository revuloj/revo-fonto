<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: koturn.xml,v 1.19 2023/12/03 09:58:59 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>koturn</rad>/o
</kap>
<drv mrk="koturn.0o">
   <kap>
     <ofc>*</ofc><tld/>o
   </kap>
   <snc mrk="koturn.0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="fazane.0oj">fazanedoj</ref>
       (<trd lng="la">Coturnix</trd>), flavgriza kampobirdo:
      <ekz>
        kiam venis la vespero, alflugis <tld/>oj kaj kovris la tendaron
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 16:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kukolo kaj la najtingalo jam forflugis, kaj la <tld/>oj, kiel mi a&ubreve;dis,
        anta&ubreve;diras favoran venton
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
       <ekz>
         dudek bone rostitaj <tld/>oj sur rostita pano
         <fnt><bib>DOz</bib>, <lok>&leftquot;Ja&ccirc;jo la Kale&scirc;o-&Ccirc;evalo&rightquot;</lok></fnt>;
       </ekz>
      <ekz>
        man&gcirc;u ilin, la grasajn <tld/>ojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>.
      </ekz>
     </dif>
    <bld lok="&WCU;/thumb/a/a6/Brown_quail1_-_Christopher_Watson.jpg/401px-Brown_quail1_-_Christopher_Watson.jpg" prm="CC BY-SA 3.0"><tld/>o (Coturnix ypsilophora), Norda A&ubreve;stralio
        <fnt>
          <aut>Christopher Watson</aut>
          <lok><url ref="&WCW;/File:Brown_quail1_-_Christopher_Watson.jpg">&WCW;/File:Brown_quail1_-_Christopher_Watson.jpg</url></lok>
        </fnt>
    </bld>
   </snc>
   <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_p;&c_jo;&c_l;&c_k;&c_a;</trd>
   <trd lng="bg">&c_p;&c_malmol;&c_d;&c_p;&c_malmol;&c_d;&c_malmol;&c_k;</trd>
   <trd lng="cs">k&rcaron;epelka obecn&aacute;</trd>
   <trd lng="de">Wachtel</trd>
   <trd lng="en">quail</trd>
   <trd lng="fr">caille</trd>
   <trd lng="hu">f&uuml;rj</trd>
   <trd lng="ja">&#x30A6;&#x30BA;&#x30E9;</trd>
  <trd lng="nl">kwartel</trd>
   <trd lng="pl">przepi&oacute;rka</trd>
   <trdgrp lng="pt">
     <trd>codorna</trd>,
     <trd>codorna-caseira</trd>,    
     <trd>codorna-dom&eacute;stica</trd>,    
     <trd>codorniz</trd>
   </trdgrp>
   <trd lng="ro">prepeli&#x21B;&abreve;</trd>
   <trdgrp lng="ru">
     <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_jo;&c_l;&c_k;&c_a;</trd>,
     <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_je;&c_l;</trd>
   </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9D6A; <pr>&#x101;n</pr></trd>,
    <trd>&#x9D89; <pr>ch&#xFA;n</pr></trd>,
    <trd>&#x9D33; <pr>y&#xE0;n</pr></trd>,
    <trd>&#x9D6A;&#x9D89; <pr>&#x101;nch&#xFA;n</pr></trd>
  </trdgrp>
</drv>
</art>
