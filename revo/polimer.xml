<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: polimer.xml,v 1.19 2024/02/17 07:52:34 revo Exp $">
<kap>
  <rad>polimer</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="polimer.0o">
  <kap><tld/>o</kap>
  <snc mrk="polimer.0o.molekulo">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Granda <ref tip="vid" cel="moleku.0o">molekulo</ref> kunmetita el ofte samspecaj
      molekuloj (monomeroj) en formo de &ccirc;eno a&ubreve; bran&ccirc;i&gcirc;o:
      <ekz>
        mesa&gcirc;ista <ref tip="sub" cel="acid.ribonuklea0o">RNA</ref> estas <tld/>o, &ccirc;eno el
        nukleotidoj
        <fnt>
          <aut>Ste&ccirc;jo Norvell</aut>
          <vrk><url ref="&Monato;/2000/007157.html">Riboj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 16-17</lok>
          </fnt>;
      </ekz>
      <ekz>
        por malhelpi metalkorodon, oni uzas kromato-pigmenton, &scirc;ajnas bona ideo kombini
        tion kun memriparaj ecoj de <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2011/010455.php">Memsanigaj materialoj?</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni vizitis firmaon, kiu produktas <tld/>ajn pavim&scirc;tonojn
        <fnt>
          <bib>LOdE</bib>
          <aut>Osmo Buller</aut>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">Ekologio kaj
          folkloro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la botoj de atletoj <klr>[&#x2026;]</klr> estas kudritaj el <tld/>aj materialoj, kiuj
        elaste tegas la muskolojn kaj intensigas ilian laboron
        <fnt>
          <bib>Kon11</bib>
          <aut>Olga &Scirc;ilajeva</aut>
          <vrk>Olimpikoj: kolekti&gcirc;as pacaj batalantoj</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="polimer.0o.substanco">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Substanco, konsistanta el tiaj molekuloj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_m;&c_e;&c_r;</trd>
  <trd lng="ca">pol&iacute;mer</trd>
  <trd lng="cs">polymer</trd>
  <trd lng="de">Polymer</trd>
  <trd lng="en">polymer</trd>
  <trd lng="fa">&f_p;&a_l;&f_y;&a_m;&a_r;</trd>
  <trd lng="fr">polym&egrave;re</trd>
  <trd lng="hu">polimer</trd>
  <trdgrp lng="ja">
    <trd>&#x91CD;&#x5408;&#x4F53;  <pr>&#x3058;&#x3085;&#x3046;&#x3054;&#x3046;&#x305F;&#x3044;</pr></trd>,
    <trd>&#x30DD;&#x30EA;&#x30DE;&#x30FC;</trd>
  </trdgrp>
  <trd lng="nl">polymeer</trd>
  <trd lng="pl">polimer</trd>
  <trd lng="pt">pol&iacute;mero</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_m;&c_je;&c_r;</trd>
  <trd lng="sk">polym&eacute;r</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43B;&c_ib;&#x43C;&c_je;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x805A;&#x5408;&#x7269; <pr>j&#xF9;h&#xE9;w&#xF9;</pr></trd>,
    <trd>&#x805A;&#x5408;&#x4F53; <pr>j&#xF9;h&#xE9;t&#x1D0;</pr></trd>,
    <trd>&#x805A;&#x5408;&#x9AD4; <pr>j&#xF9;h&#xE9;t&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>
