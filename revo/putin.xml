<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: putin.xml,v 1.20 2023/03/10 08:35:42 revo Exp $">
<kap>
  <rad>putin</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="putin.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">VULG</uzo>
  <snc mrk="putin.0o.SEKS">
    <dif>
      <ref tip="dif" cel="amor.0istino">Amoristino</ref>:
      <ekz>
        ci, nenio alia ol &scirc;minkita <tld/>o
        <fnt><bib>RugxDom</bib>,
             <lok>&ccirc;apitro 60a, volumo 2a, p. 307a</lok></fnt>!
      </ekz>
      <ekz>
        dependas de la grado de amikeco, &ccirc;u oni rajtas diri al
        hispano, ke lia patrino estis gro&scirc;o<tld/>o
        <fnt>
          <bib>Mortul&Scirc;ip</bib>,
          <lok>dua libro, &ccirc;apitro 30a</lok>
        </fnt>;
      </ekz>
      <ekz>
        mankas &ccirc;apitro pri <ind><tld/>ejo</ind>, kvankam en Berlino tiaj establoj tiel normali&gcirc;is,
        ke unu el ili reklamas <klr>[&#8230;]</klr> rabaton por klientoj,
        kiuj alvenas per biciklo a&ubreve; publika transportilo
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="&Monato;/2012/010794.php">Distra kaj kleriga
          vortaro</url></vrk>
          <lok>2012</lok>
        </fnt>.
        <trd lng="de">Puff</trd>
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="putin1.0o">Putino (politikisto)</ref>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_y;&c_t;&c_u;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>meuca</trd>,
    <trd>bagassa</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&ecaron;hna</trd>,
    <trd>coura</trd>,
    <trd>d&ecaron;vka</trd>,
    <trd>flundra</trd>,
    <trd>kurva</trd>,
    <trd>nev&ecaron;stka</trd>,
    <trd>pob&ecaron;hlice</trd>,
    <trd>&scaron;lapka</trd>,
    <trd>&scaron;t&ecaron;tka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Prostitutierte</trd>,
    <trd>Hure</trd>,
    <trd>Nutte</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>harlot</trd>,
    <trd>hooker</trd>,
    <trd>strumpet</trd>,
    <trd>tart</trd>,
    <trd>whore</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>prostituta</trd>,
    <trd>puta</trd>, 
    <trd>meretriz</trd>
  </trdgrp>
  <trd lng="fr">putain</trd>
  <trd lng="nl">hoer</trd>
  <trd lng="pl">prostytutka</trd>
  <trdgrp lng="pt">
    <trd>puta</trd>,  
    <trd>prostituta</trd>,
    <trd>meretriz</trd>
  </trdgrp>
  <trd lng="ro">prostituat&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_l;&c_ju;&c_h;&c_a;</trd>,  
    <trd>&c_p;&c_o;&c_t;&c_a;&c_s;&c_k;&c_u;&c_h;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_u;&c_t;&c_a;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nehanebnica</trd>,
    <trd>pobehlica</trd>
  </trdgrp>
  <trd lng="th">&#3650;&#3626;&#3648;&#3616;&#3603;&#3637;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_v;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_u;&c_l;&c_mol;&c_gu;. &c_k;&c_u;&c_r;&c_v;&c_a;</trd>
  </trdgrp>
</drv>
</art>

