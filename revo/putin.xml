<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>putin</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="putin.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">VULG</uzo>
  <snc mrk="putin.0o.SEKS">
    <ref tip="dif" cel="amor.0istino">amoristino</ref>
    <ekz>
      ci, nenio alia ol &scirc;minkita <tld/>o
      <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 60a, volumo 2a, p.
      307a</lok></fnt>!
    </ekz>
    <ekz>
      dependas de la grado de amikeco, &ccirc;u oni rajtas diri al hispano, ke
      lia patrino estis gro&scirc;o<tld/>o
      <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro 30a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>Prostitutierte</trd>,
    <trd>Hure</trd>,
    <trd>Nutte</trd>
  </trdgrp>
  <trd lng="fr">putain</trd>
  <trdgrp lng="pt">
    <trd>puta</trd>,  
    <trd>prostituta</trd>,
    <trd>meretriz</trd>
  </trdgrp></drv>
</art>

