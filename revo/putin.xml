<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>putin</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="putin.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">VULG</uzo>
  <snc mrk="putin.0o.SEKS">
    <ref tip="dif" cel="amor.0istino">amoristino</ref>
    <ekz>
      ci, nenio alia ol &scirc;minkita <tld/>o!
      <fnt><bib>RugxDom</bib>,
           <lok>&ccirc;apitro 60a, volumo 2a, p. 307a</lok></fnt>;
    </ekz>
    <ekz>
      dependas de la grado de amikeco, &ccirc;u oni rajtas diri al hispano,
      ke lia patrino estis gro&scirc;o<tld/>o
      <fnt>
        <bib>Mortul&Scirc;ip</bib>,
        <lok>dua libro, &ccirc;apitro 30a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>meuca</trd>,
    <trd>bagassa</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Prostitutierte</trd>,
    <trd>Hure</trd>,
    <trd>Nutte</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>prostituta</trd>,
    <trd>puta</trd>, 
    <trd>meretriz</trd>
  </trdgrp> 
  <trd lng="fr">putain</trd>
  <trd lng="nl">hoer</trd>
  <trdgrp lng="pt">
    <trd>puta</trd>,  
    <trd>prostituta</trd>,
    <trd>meretriz</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_l;&c_ju;&c_h;&c_a;</trd>,  
    <trd>&c_p;&c_o;&c_t;&c_a;&c_s;&c_k;&c_u;&c_h;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_u;&c_t;&c_a;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
</art>

