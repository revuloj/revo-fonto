<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxies.xml,v 1.33 2025/11/02 18:35:31 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>7</ofc>
  <rad>&ccirc;ies</rad>
  <fnt> <bib>JED</bib></fnt>
</kap>

<drv mrk="cxies.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra>nevariebla <vspec>pronomo</vspec></gra>
  <snc>
    <dif>
      De &ccirc;iu, kiu apartenas al &ccirc;iu:
      <ekz>
        Esperanto estas <tld/> propra&jcirc;o
        <fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/> okuloj kun teruro deturni&gcirc;is
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        siajn havojn <klr>[ili]</klr> dividis <klr>[&#x2026;]</klr> la&ubreve; <tld/> aparta bezono
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 2:45</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj <klr>[&#x2026;]</klr> tute ne laboras, sed en <tld/> aferojn sin en&scirc;ovas
        <fnt>
          <bib>NT</bib>
          <lok>II. Tesalonikanoj 3:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi alkondukas &ccirc;i tien mian filinon, por klarigi la aferon en <tld/> alesto
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>surprize, la maljunulo iel strange ektremis
        <fnt>
          <bib>Kon11</bib>
          <lok>Novelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        publikaj transportoj (vere <tld/>aj!)
        <fnt>
          <bib>Kon11</bib>
          <lok>Socio</lok>
        </fnt>;
      </ekz>
      <ekz>
        enkonduki la afrikansan kiel <tld/>an instrulingvon
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 3 (101)</lok>
        </fnt>;
      </ekz>
      <ekz>
        klopodi pri <tld/> favoro estas pleje malsa&gcirc;a
        laboro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="neniu.0">neniu, Rim</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_ib;&c_h;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_a;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>jedes</trd>,
    <trd>jedermanns</trd>,
    <trd>von jedem</trd>
  </trdgrp>
  <trd lng="es">de <ind>todos</ind></trd>
  <trd lng="fr">de <ind>tous</ind></trd>
  <trd lng="he">&shin;&lamed; &kaf;&vav;&lamed;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>mindenkinek a</trd>,
    <trd>mindenki&eacute;</trd>
  </trdgrp>
  <trd lng="it">di tutti</trd>
  <trdgrp lng="ja">
    <trd>&#x5404;&#x4EBA;&#x306E;  <pr>&#x304B;&#x304F;&#x3058;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x307F;&#x3093;&#x306A;&#x306E;</trd>,
    <trd>&#x4E07;&#x4EBA;&#x306E;  <pr>&#x307E;&#x3093;&#x308A;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">ieders</trd>
  <trdgrp lng="pl">
    <trd>wszystkich</trd>,
    <trd>ka&zdot;dego</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>al fiec&abreve;ruia</trd>,
    <trd>al tuturor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_a;&c_d;&c_l;&c_je;&c_zh;&c_a;&c_shch;&c_i;&c_j; &c_k;&c_a;&c_zh;&c_d;&c_o;&c_m;&c_u;</trd>
  </trdgrp>
  <trd lng="sk">ka&zcaron;d&eacute;ho</trd>
  <trdgrp lng="sv">
    <trd><ind>var</ind> och ens</trd>,
    <trd>allas</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_i;&c_j; &c_k;&c_o;&c_zh;&c_n;&c_o;&c_m;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="cxies.0ulino">
  <kap><tld/>ulino</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="prosti.0itino">Prostituitino</ref>:
      <ekz>
        ekzistas tie en Torento kelkaj a&ccirc;ulinoj, kvaza&ubreve; <tld/>ulinoj
        <fnt>
          <bib>Tor</bib>
          <lok>II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u li prenas min por <tld/>ulino
        <fnt>
          <bib>ChM</bib>
          <lok>1</lok>
        </fnt>?
      </ekz>
      <ekz>
        la <tld/>ulino sakras pri lia avareco kaj foriras
        <fnt>
          <bib>KrB</bib>
          <lok>Mar&ccirc;andado a&ccirc;eti ne devigas</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_a;&c_l;&c_y;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_y;&c_t;&c_u;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_ga;&t_zha;&t_nga;&t_tsheg;&t_tsha;&t_naro;&t_nga;&t_tsheg;&t_ma;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>Prostituierte</trd>,
    <trd>Nutte</trd>,
    <trd>Dirne</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>prostituta</trd>,
    <trd>puta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd><ind>prostitu&eacute;</ind>e</trd>,
    <trd>fille publique</trd>,
    <trd>fille de <ind>joie</ind></trd>,
    <trd>grue</trd>,
    <trd>p&eacute;ripat&eacute;ticienne</trd>,
    <trd>putain</trd>,
    <trd>tapineuse</trd>,
  </trdgrp>
  <trd lng="he">&pe;&resh;&vav;&tsadi;&he;</trd>
  <trdgrp lng="it">
    <trd>prostituta</trd>,
    <trd>puttana</trd>,
    <trd>meretrice</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd/>,
    <trd>&#x5A3C;&#x5A66;  <pr>&#x3057;&#x3087;&#x3046;&#x3075;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wszetecznica</trd>,
    <trd>kobieta <ind>wszeteczna</ind></trd>
  </trdgrp>
  <trd lng="ro">prostituat&abreve;</trd>
  <trd lng="uk">&c_p;&c_o;&c_v;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x5A3C;&#x4F18; <pr>ch&amacron;ngy&omacron;u</pr></trd>,
    <trd>&#x5750;&#x53F0;&#x5C0F;&#x59D0; <pr>zu&ograve;t&aacute;ixi&#x1CE;oji&ecaron;</pr></trd>,
    <trd>&#x5993; <pr>j&igrave;</pr></trd>,
    <trd>&#x92D9; <pr>w&uacute;</pr></trd>,
    <trd>&#x70DF;&#x82B1; <pr>y&amacron;nhu&amacron;</pr></trd>,
    <trd>&#x4E09;&#x966A; <pr>s&amacron;np&eacute;i</pr></trd>,
    <trd>&#x91CE;&#x96DE; <pr>y&ecaron;j&imacron;</pr></trd>,
    <trd>&#x5A3C;&#x5973; <pr>ch&amacron;ngn&#x1DA;</pr></trd>,
    <trd>&#x5993;&#x5973; <pr>j&igrave;n&#x1DA;</pr></trd>,
    <trd>&#x5A3C; <pr>ch&amacron;ng</pr></trd>,
    <trd>&#x5A4A; <pr>bi&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

