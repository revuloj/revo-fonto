<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cxies.xml,v 1.26 2021/05/29 14:32:15 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>7</ofc>
  <rad>&ccirc;ies</rad> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="cxies.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <uzo tip="klr">(nevariebla pron)</uzo>
    <dif>
      De &ccirc;iu, kiu apartenas al &ccirc;iu:
      <ekz>
        Esperanto estas <tld/> propra&jcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        klopodi pri <tld/> favoro estas pleje malsa&gcirc;a
        laboro<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/> okuloj kun teruro deturni&gcirc;is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>vo&ccirc;a elekto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="neniu.0">neniu, Rim</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_ib;&c_h;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_a;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>jedes</trd>,
    <trd>jedermanns</trd>,
    <trd>von jedem</trd>
  </trdgrp>
  <trd lng="es">de <ind>todos</ind></trd>
  <trd lng="fr">de <ind>tous</ind></trd>
  <trd lng="he">&shin;&lamed; &kaf;&vav;&lamed;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>mindenkinek a</trd>,
    <trd>mindenki&eacute;</trd>
  </trdgrp>
  <trd lng="it">di tutti</trd>
  <trd lng="nl">ieders</trd>
  <trdgrp lng="pl">
    <trd>wszystkich</trd>,
    <trd>ka&zdot;dego</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>al fiec&abreve;ruia</trd>,
    <trd>al tuturor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_a;&c_d;&c_l;&c_je;&c_zh;&c_a;&c_shch;&c_i;&c_j; &c_k;&c_a;&c_zh;&c_d;&c_o;&c_m;&c_u;</trd>
  </trdgrp>
  <trd lng="sk">ka&zcaron;d&eacute;ho</trd>
  <trdgrp lng="sv">
    <trd><ind>var</ind> och ens</trd>,
    <trd>allas</trd>
  </trdgrp>
</drv>

<drv mrk="cxies.0ulino">
  <kap><tld/>ulino</kap>
  <snc>
    <ref tip="dif" cel="prosti.0itino">prostituitino</ref>
  </snc>
  <trd lng="bo">&#xF42;&#xF5E;&#xF44;&#xF0B;&#xF5A;&#xF7C;&#xF44;&#xF0B;&#xF58;&#xF0B;</trd>
  <trd lng="de">Prostituierte</trd>
  <trdgrp lng="es">
    <trd>prostituta</trd>,
    <trd>puta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd><ind>prostitu&eacute;</ind>e</trd>,
    <trd>fille publique</trd>,
    <trd>fille de <ind>joie</ind></trd>,
    <trd>grue</trd>,
    <trd>p&eacute;ripat&eacute;ticienne</trd>,
    <trd>putain</trd>,
    <trd>tapineuse</trd>,
  </trdgrp>
  <trd lng="he">&#x5E4;&#x5E8;&#x5D5;&#x5E6;&#x5D4;</trd>
  <trdgrp lng="it">
    <trd>prostituta</trd>,
    <trd>puttana</trd>,
    <trd>meretrice</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wszetecznica</trd>,
    <trd>kobieta <ind>wszeteczna</ind></trd>
  </trdgrp>
  <trd lng="ro">prostituat&abreve;</trd>
</drv>

</art>
