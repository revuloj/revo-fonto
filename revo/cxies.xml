<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;ies</rad></kap>
<drv mrk="cxies.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <uzo tip="klr">(nevariebla pron)</uzo>
    <dif>
      De &ccirc;iu, kiu apartenas al &ccirc;iu:
      <ekz>
        esperanto estas <tld/> propra&jcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        klopodi pri <tld/> favoro estas pleje malsa&gcirc;a laboro
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        <tld/> okuloj kun teruro deturni&gcirc;is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>vo&ccirc;a elekto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="neniu.0">neniu, Rim</ref>.
    </refgrp>
  </snc>
  <trd lng="de">jedes</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_a;&c_d;&c_l;&c_je;&c_zh;&c_a;&c_shch;&c_i;&c_j; &c_k;&c_a;&c_zh;&c_d;&c_o;&c_m;&c_u;</trd>
  </trdgrp>
</drv>
</art>
