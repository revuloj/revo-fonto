<art mrk="$Id: cxies.xml,v 1.31 2024/04/24 09:23:29 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>7</ofc>
  <rad>&ccirc;ies</rad> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="cxies.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <uzo tip="klr">(nevariebla pron)</uzo>
    <dif>
      De &ccirc;iu, kiu apartenas al &ccirc;iu:
      <ekz>
        Esperanto estas <tld/> propra&jcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        klopodi pri <tld/> favoro estas pleje malsa&gcirc;a
        laboro<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/> okuloj kun teruro deturni&gcirc;is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>vo&ccirc;a elekto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="neniu.0">neniu, Rim</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_ib;&c_h;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_g;&c_u;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_a;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>jedes</trd>,
    <trd>jedermanns</trd>,
    <trd>von jedem</trd>
  </trdgrp>
  <trd lng="es">de <ind>todos</ind></trd>
  <trd lng="fr">de <ind>tous</ind></trd>
  <trd lng="he">&shin;&lamed; &kaf;&vav;&lamed;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>mindenkinek a</trd>,
    <trd>mindenki&eacute;</trd>
  </trdgrp>
  <trd lng="it">di tutti</trd>
  <trdgrp lng="ja">
    <trd>&#21508;&#x4EBA;&#x306E;  <pr>&#x304B;&#x304F;&#12376;&#12435;&#x306E;</pr></trd>,
    <trd>&#x307F;&#12435;&#x306A;&#x306E;</trd>,
    <trd>&#x4E07;&#x4EBA;&#x306E;  <pr>&#x307E;&#12435;&#x308A;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">ieders</trd>
  <trdgrp lng="pl">
    <trd>wszystkich</trd>,
    <trd>ka&zdot;dego</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>al fiec&abreve;ruia</trd>,
    <trd>al tuturor</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_a;&c_d;&c_l;&c_je;&c_zh;&c_a;&c_shch;&c_i;&c_j; &c_k;&c_a;&c_zh;&c_d;&c_o;&c_m;&c_u;</trd>
  </trdgrp>
  <trd lng="sk">ka&zcaron;d&eacute;ho</trd>
  <trdgrp lng="sv">
    <trd><ind>var</ind> och ens</trd>,
    <trd>allas</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_g;&c_a;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&c_i;&#x43D;&c_a;&#x43B;&c_je;&c_zh;&#x43D;&c_i;&c_j; &#x43A;&#x43E;&c_zh;&#x43D;&#x43E;&#x43C;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="cxies.0ulino">
  <kap><tld/>ulino</kap>
  <snc>
    <ref tip="dif" cel="prosti.0itino">prostituitino</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_a;&c_l;&c_y;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_s;&c_t;&c_y;&c_t;&c_u;&c_t;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF42;&#xF5E;&#xF44;&#xF0B;&#xF5A;&#xF7C;&#xF44;&#xF0B;&#xF58;&#xF0B;</trd>
  <trd lng="de">Prostituierte</trd>
  <trdgrp lng="es">
    <trd>prostituta</trd>,
    <trd>puta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd><ind>prostitu&eacute;</ind>e</trd>,
    <trd>fille publique</trd>,
    <trd>fille de <ind>joie</ind></trd>,
    <trd>grue</trd>,
    <trd>p&eacute;ripat&eacute;ticienne</trd>,
    <trd>putain</trd>,
    <trd>tapineuse</trd>,
  </trdgrp>
  <trd lng="he">&#x5E4;&#x5E8;&#x5D5;&#x5E6;&#x5D4;</trd>
  <trdgrp lng="it">
    <trd>prostituta</trd>,
    <trd>puttana</trd>,
    <trd>meretrice</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd/>,
    <trd>&#x5A3C;&#x5A66;  <pr>&#12375;&#12423;&#12358;&#12405;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wszetecznica</trd>,
    <trd>kobieta <ind>wszeteczna</ind></trd>
  </trdgrp>
  <trd lng="ro">prostituat&abreve;</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_v;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x5A3C;&#x4F18; <pr>ch&amacron;ngy&#x14D;u</pr></trd>,
    <trd>&#22352;&#x53F0;&#x5C0F;&#x59D0; <pr>zu&#xF2;t&#xE1;ixi&#x1CE;oji&#x11B;</pr></trd>,
    <trd>&#22931; <pr>j&#xEC;</pr></trd>,
    <trd>&#x92D9; <pr>w&#xFA;</pr></trd>,
    <trd>&#x70DF;&#x82B1; <pr>y&amacron;nhu&amacron;</pr></trd>,
    <trd>&#x4E09;&#x966A; <pr>s&amacron;np&#xE9;i</pr></trd>,
    <trd>&#x91CE;&#x96DE; <pr>y&#x11B;j&#x12B;</pr></trd>,
    <trd>&#x5A3C;&#22899; <pr>ch&amacron;ngn&#x1DA;</pr></trd>,
    <trd>&#22931;&#22899; <pr>j&#xEC;n&#x1DA;</pr></trd>,
    <trd>&#x5A3C; <pr>ch&amacron;ng</pr></trd>,
    <trd>&#x5A4A; <pr>bi&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

