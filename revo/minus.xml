<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>8</ofc>
      <rad>minus</rad><fnt>Z</fnt>
    </kap>
    <drv mrk="minus.0">
      <kap><tld/></kap>
      <uzo tip="fak">FIZ</uzo>
      <uzo tip="fak">MAT</uzo>
      <snc>
        <dif>
          <ref cel="konjun.0o">Konjunkcio</ref> esprimanta
          <ref cel="subtra.0o">subtrahon</ref> a&ubreve; la negativan
          <ref tip="super" cel="sign.0umo">signumon</ref>:
          <ekz>
            <frm>10 - 4 = 6</frm> (dek <tld/> kvar estas ses);
          </ekz><ekz>
            temperaturo <frm>-25&ring;C</frm> (temperaturo
            <tld/> dudek kvin gradoj celsiaj);
          </ekz><ekz>
            <tld/>poluso, <tld/>drato.
          </ekz>
        </dif>
        <ref tip="ant" cel="plus.0">plus</ref>
        <ref tip="sin" cel="sen.0">sen</ref>
      </snc>
      <trd lng="cs">minus</trd>
      <trdgrp lng="de">
        <trd>minus</trd>,
        <trd>weniger</trd>,
        <trd>abz&uuml;glich</trd></trdgrp>
      <trdgrp lng="nl">
        <trd>min</trd>,
        <trd>minus</trd>
      </trdgrp>
      <trd lng="ru">&c_m;&c_i;&c_n;&c_u;&c_s;</trd>
      <trd lng="sv">minus</trd>
    </drv>
    <drv mrk="minus.0o">
      <kap><tld/>o</kap>
      <snc mrk="minus.0o.askia">
        <uzo tip="fak">KOMP</uzo>
        <uzo tip="fak">MAT</uzo>
        <uzo tip="fak">TIP</uzo>
        <dif>
          <ref tip="super" cel="sign.0o.KOMP">Signo</ref> <frm>-</frm> uzata
          kiel simbolo de <tld/> en formuloj kaj nombroprezentoj.
        </dif>
        <ref tip="vid" cel="plus.0o">pluso</ref>
      </snc>
      <trd lng="en">minus sign</trd>
      <trd lng="ru">&c_z;&c_n;&c_a;&c_k; <ind>&c_m;&c_i;&c_n;&c_u;&c_s;</ind></trd>
    </drv>
  </art>
