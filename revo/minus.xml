<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>minus</rad><fnt>Z</fnt>
</kap>
<drv mrk="minus.0">
  <kap><tld/></kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      <ref cel="konjun.0o">Konjunkcio</ref> esprimanta
      <ref cel="subtra.0o">subtrahon</ref> a&ubreve; la negativan
      <ref cel="sign.0o.MAT">signon</ref>:
      <ekz>
        <frm>10-4 = 6</frm> <klr>(legu: dek <tld/> kvar estas ses)</klr>;
      </ekz>
      <ekz>
        <frm>4-10 = -6</frm> <klr>(legu: kvar <tld/> dek estas <tld/>
        ses)</klr>;
      </ekz>
      <ekz>
        en adicie signata grupo, la
        <ref cel="neuxtr.0iganto.MAT">ne&ubreve;triganton</ref> de elemento
        <frm><k>a</k></frm> oni signas per <frm>-<k>a</k></frm> 
        <klr>(legu: <tld/> a)</klr>;
      </ekz>
      <ekz>
        temperaturo je <frm>-25&ring;C</frm> <klr>(legu: <tld/> dudek kvin
        gradoj celsiaj)</klr>;
      </ekz>
      <ekz>
        <tld/>poluso, <tld/>drato.
      </ekz>
    </dif>
    <ref tip="ant" cel="plus.0">plus;</ref>
    <ref tip="vid" cel="sen.0.prepozicio">sen.</ref>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_n;&c_u;&c_s;</trd>
  <trd lng="cs">minus</trd>
  <trdgrp lng="de">
    <trd>minus</trd>,
    <trd>weniger</trd>,
    <trd>abz&uuml;glich</trd>
  </trdgrp>
  <trd lng="en">minus</trd>
  <trdgrp lng="nl">
    <trd>min</trd>,
    <trd>minus</trd>
  </trdgrp>
  <trd lng="fr">moins</trd>
  <trd lng="pl">minus
  </trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_u;&c_s;</trd>
  <trd lng="sv">minus</trd>
</drv>

<drv mrk="minus.0o">
  <kap><tld/>o</kap>
  <snc mrk="minus.0o.signo">
    <uzo tip="fak">KOMP</uzo>
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>KompLeks</bib></fnt>
    <uzo tip="fak">TIP</uzo>
    <dif>
      La streketforma <ref tip="super" cel="sign.0o.KOMP">signo</ref>
      <frm>-</frm>, uzata kiel simbolo de <ref cel="minus.0"><tld/></ref>
      en formuloj kaj nombroprezentoj.
    </dif>
    <ref tip="vid" cel="plus.0o">pluso.</ref>
    <rim>
      Troveblas <ctl><tld/>signo</ctl> en <fnt><bib>MatVort</bib></fnt>.
    </rim>
    <trd lng="be">&c_z;&c_n;&c_a;&c_k; <ind>&c_m;&c_ib;&c_n;&c_u;&c_s;</ind></trd>
    <trd lng="de">Minuszeichen</trd>
    <trd lng="en">minus sign</trd>
    <trd lng="fr">signe moins</trd>
    <trd lng="pl">znak minus</trd>
    <trd lng="ru">&c_z;&c_n;&c_a;&c_k; <ind>&c_m;&c_i;&c_n;&c_u;&c_s;</ind></trd>
  </snc>
  <snc mrk="minus.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Negativa nombro, malavanta&gcirc;o, manko, deficito.
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_ib;&c_n;&c_u;&c_s;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_a;&c_h;&c_o;&c_p;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>manque</trd>,
      <trd>d&eacute;ficit</trd>,
      <trd>moins<klr tip="ind"> (manque)</klr></trd></trdgrp> 
  </snc>
</drv>
</art>
