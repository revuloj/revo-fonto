        <frm am="r(theta_i)">r(&theta;<sub>i</sub>)</frm>, 
        kaj centra <tld/>o <frm am="Delta theta">&Delta;&theta;</frm>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref='https://eo.wikipedia.org/?curid=164494'>Polusa
          koordinata sistemo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
