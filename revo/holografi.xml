<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: holografi.xml,v 1.10 2023/08/31 15:46:18 revo Exp $">
<kap>
  <rad>holografi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="holografi.0o">
  <kap><tld/>o</kap>
  <snc mrk="holografi.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Arto fari lumajn tridimensiajn bildojn per interferoj:
      <ekz>
        hungara sciencisto Dennis Gabor kreis <tld/>on en 1947
        <fnt><bib>Viki</bib>, <lok>artikolo
&leftquot;holografio&rightquot;, 
        2007-06</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_ja;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="ca">holografia</trd>
  <trd lng="fa">&a_h;&a_w;&a_l;&a_w;&f_g;&a_r;&a_A;&a_f;&f_y;</trd>
  <trd lng="fr">holographie</trd>
  <trd lng="en">holography</trd>
  <trd lng="it">olografia</trd>
  <trd lng="ja">&#x30DB;&#x30ED;&#x30B0;&#x30E9;&#x30D5;&#x30A3;&#x30FC;</trd>
  <trd lng="pl">holografia</trd>
  <trd lng="pt">holografia</trd>
  <trd lng="uk">&c_g;&#x43E;&#x43B;&#x43E;&c_g;&c_r;&c_a;&c_f;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="holografi.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="holografi.0ajxo.FOT">
    <dif>
      <tld lit="H"/>a bildo:
      <ekz>
        sur la anta&ubreve;a flanko estas anka&ubreve; <tld/>a&jcirc;o
montranta la
        nombron 5000
        <fnt>
          <aut>Birke</aut>,
          <vrk><url ref="http://bertilow.com/blogo">
          BBBlogo</url></vrk>,
          <lok>2006-03-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_ja;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="ca">holograma</trd>
  <trd lng="fa">&a_h;&a_w;&a_l;&a_w;&f_g;&a_r;&a_A;&a_m;</trd>
  <trd lng="fr">hologramme</trd>
  <trd lng="en">hologram</trd>
  <trd lng="it">ologramma</trd>
  <trd lng="pl">hologram</trd>
  <trd lng="pt">holograma</trd>
</drv>
</art>

