<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>holografi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="holografi.0o">
  <kap><tld/>o</kap>
  <snc mrk="holografi.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Arto fari lumajn tridimensiajn bildojn per interferoj:
      <ekz>
        hungara sciencisto Dennis Gabor kreis <tld/>on en 1947
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;holografio&rightquot;, 
        2007-06</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">holographie</trd>
  <trd lng="en">holography</trd>
</drv>

<drv mrk="holografi.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="holografi.0ajxo.FOT">
    <dif>
      <tld lit="H"/>a bildo:
      <ekz>
        sur la anta&ubreve;a flanko estas anka&ubreve; <tld/>a&jcirc;o montranta la
        nombron 5000
        <fnt>
          <aut>Birke</aut>,
          <vrk><url ref="http://bertilow.com/blogo">
          BBBlogo</url></vrk>,
          <lok>2006-03-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">hologramme</trd>
  <trd lng="en">hologram</trd>
</drv>
</art>

