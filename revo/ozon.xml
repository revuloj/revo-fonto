<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>2</ofc> <rad>ozon</rad>/o</kap>
<drv mrk="ozon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Triopa <ref tip="super" cel="oksige.0o">oksigeno</ref>,
      produktebla el la duopa ekz-e per efiko de elektro
      kaj diferenca de tiu gaso per karakteriza adoro kaj pli forta
      oksidiga kapablo.
    </dif>
  </snc>
  <trd lng="de">Ozon</trd>
  <trd lng="hu">&oacute;zon</trd>
  <trd lng="nl">ozon</trd>
  <trd lng="ru">&c_o;&c_z;&c_o;&c_n;</trd>
  <trd lng="sv">ozon</trd>
</drv>

<drv mrk="ozon.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Elektrizi la oksigenon de l' aero, por &gcirc;in aliigi en <tld/>on.
    </dif>
  </snc>
  <trd lng="nl">ozoniseren</trd>
  <trd lng="ru">&c_o;&c_z;&c_o;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
