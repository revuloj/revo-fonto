<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>cinam</rad>/o
</kap>

<drv mrk="cinam.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Aroma &scirc;elo de <ref tip="malprt"
      cel="cinam.0ujo"><tld/>ujo</ref>,
      uzata kiel <ref tip="super" cel="spic.0o">spico</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_y;&c_c;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_n;&c_je;&c_l;&c_a;</trd>
  <trd lng="br">kanell<klr tip="ind"> (keginerezh)</klr></trd>
  <trd lng="ca">canyella</trd>
  <trd lng="de">Zimt</trd>
  <trd lng="en">cinnamon</trd>
  <trd lng="es">canela</trd>
  <trd lng="fr">cannelle</trd>
  <trd lng="he">&qof;&yod;&nun;&mem;&vav;&fnun;</trd>
  <trd lng="hu">fah&eacute;j</trd>
  <trd lng="it">cannella</trd>
  <trd lng="nl">kaneel</trd>
  <trd lng="pl">cynamon</trd>
  <trd lng="pt">canela</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_i;&c_c;&c_a;</trd>
  <trd lng="tr">tar&ccedil;&inodot;n</trd>
</drv>

<drv mrk="cinam.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="lauxrac.0oj">la&ubreve;racoj</ref>
      (<trd lng="la">Cinnamomum</trd>).
    </dif>
    <ref tip="sub" cel="kamfor.0ujo">kamforujo</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_ib;&c_k;</trd>
  <trd lng="br">gwezenn-ganell</trd>
  <trdgrp lng="ca">
    <trd>canyeller</trd>,
    <trd>g&egrave;nere Cinnamomum</trd>
  </trdgrp>
  <trd lng="de">Zimtbaum</trd>
  <trd lng="en">cinnamon tree</trd>
  <trd lng="fr">cannelier</trd>
  <trd lng="hu">fah&eacute;jfa</trd>
  <trd lng="it">cinnamomo</trd>
  <trd lng="nl">kaneelboom</trd>
  <trd lng="pl">drzewo <ind>cynamonowe</ind></trd>
  <trd lng="pt">canela</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_i;&c_ch;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
