<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cinam.xml,v 1.34 2023/10/31 13:37:03 revo Exp $">
<kap>
  <ofc>*</ofc><rad>cinam</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="cinam.0o">
  <kap><tld/>o</kap>
  <snc mrk="cinam.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Aroma &scirc;elo de <ref tip="malprt" cel="cinam.0ujo"><tld/>ujo</ref>,
      uzata kiel <ref tip="lst" cel="spic.0o" lst="voko:spicoj">spico</ref>:
      <ekz>
        bruna miksa&jcirc;o el sekvinberoj, <tld/>o kaj nuksoj
        <fnt>
          <aut>H. Heine, trad. L. L. Zamenhof</aut>
          <vrk>La Rabeno de Ba&hcirc;ara&hcirc;</vrk>,
          <lok>1929</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_y;&c_c;&c_a;</trd>,
    <trd>&c_c;&c_y;&c_n;&c_a;&c_m;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_a;&c_n;&c_je;&c_l;&c_a;</trd>
  <trd lng="br">kanell<klr tip="ind"> (keginerezh)</klr></trd>
  <trd lng="ca">canyella</trd>
  <trd lng="cs">sko&rcaron;ice</trd>
  <trd lng="de">Zimt</trd>
  <trd lng="en">cinnamon</trd>
  <trd lng="es">canela</trd>
  <trd lng="fr">cannelle</trd>
  <trd lng="he">&qof;&yod;&nun;&mem;&vav;&fnun;</trd>
  <trd lng="hu">fah&eacute;j</trd>
  <trd lng="id"><ind>kayu</ind> manis</trd>
  <trd lng="it">cannella</trd>
  <trdgrp lng="ja">
    <trd>&#x6842;&#x76AE;  <pr>&#x3051;&#x3044;&#x3072;</pr></trd>,
    <trd>&#x30B7;&#x30CA;&#x30E2;&#x30F3;</trd>
  </trdgrp>
  <trd lng="nl">kaneel</trd>
  <trd lng="pl">cynamon</trd>
  <trd lng="pt">canela</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">&scaron;korica</trd>
  <trd lng="tr">tar&ccedil;&inodot;n</trd>
  <trd lng="uk">&#x43A;&#x43E;&c_r;&c_i;&c_c;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x8089;&#x6842; <pr>r&#xF2;ugu&#xEC;</pr></trd>,
    <trd>&#x8089;&#x6842;&#x7D05; <pr>r&#xF2;ugu&#xEC;h&#xF3;ng</pr></trd>,
    <trd>&#x8089;&#x6842;&#x8272; <pr>r&#xF2;ugu&#xEC;s&#xE8;</pr></trd>,
    <trd>&#x8089;&#x6842;&#x6811; <pr>r&#xF2;ugu&#xEC;sh&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="cinam.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="cinam.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="lauxrac.0oj">la&ubreve;racoj</ref>
      (<trd lng="la">Cinnamomum</trd>).
    </dif>
    <ref tip="sub" cel="kamfor.0ujo">kamforujo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>,
    <trd>&c_c;&c_y;&c_n;&c_a;&c_m;&c_o;&c_n;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="br">gwezenn-ganell</trd>
  <trdgrp lng="ca">
    <trd>canyeller</trd>,
    <trd>g&egrave;nere Cinnamomum</trd>
  </trdgrp>
  <trd lng="cs">sko&rcaron;icovn&iacute;k</trd>
  <trd lng="de">Zimtbaum</trd>
  <trd lng="en">cinnamon tree</trd>
  <trd lng="fr">cannelier</trd>
  <trd lng="he">&#x5E7;&#x5D9;&#x5E0;&#x5DE;&#x5D5;&#x5DF;</trd>
  <trd lng="hu">fah&eacute;jfa</trd>
  <trd lng="it">cinnamomo</trd>
  <trd lng="nl">kaneelboom</trd>
  <trd lng="pl">drzewo <ind>cynamonowe</ind></trd>
  <trd lng="pt">canela</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_i;&c_ch;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">&scaron;koricovn&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&c_r;&c_i;&c_ch;&#x43D;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;</trd>,
    <trd>&#x43A;&#x43E;&c_r;&c_i;&c_ch;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
