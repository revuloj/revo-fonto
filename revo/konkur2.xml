<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>konkurs</rad>/o
</kap>
<drv mrk="konkur2.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Anta&ubreve;aran&gcirc;ita konkuro, la&ubreve; difinitaj reguloj, kun apartaj
      ju&gcirc;antaro kaj premioj:
      <ekz>
        <tld/>o inter lernantoj;
      </ekz>
      <ekz>
        kur<tld/>o, rem<tld/>o;
      </ekz>
      <ekz>
        estigi, malfermi <tld/>on por tri vakantaj postenoj;
      </ekz>
      <ekz>
        <tld/>a ekzameno<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="turnir.0o">turniro</ref>.
    </refgrp>
    <rim>
      En ekzameno oni atentas nur la individuan valoron de la
      kandidatoj,
      en konkurso oni atentas pri ilia relativa valoro.
    </rim>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;</trd>
  <trd lng="bg">&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;</trd>
  <trd lng="ca">concurs</trd>
  <trdgrp lng="de">
     <trd>Wettbewerb</trd>,
     <trd>Wettkampf</trd>,
     <trd>Wettstreit</trd>,
     <trd>Wettlauf</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>competition</trd>,
    <trd>contest</trd>,
    <trd>match</trd>
  </trdgrp>
  <trd lng="es">concurso</trd>
  <trdgrp lng="nl">
    <trd>wedstrijd</trd>,
    <trd>prijskamp</trd>,
    <trd>wedkamp</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>concurso</trd>,
    <trd>certame</trd>,    
    <trd>gincana</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_ja;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="konkur2.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      <tld lit="K"/>e konkuri.
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_d;&c_z;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol; &c_u; <ind>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;&c_je;</ind></trd>
  <trdgrp lng="bg">
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_i;&c_r;&c_a;&c_m;</trd>,
    <trd>&c_s;&c_malmol;&c_s;&c_t;&c_je;&c_z;&c_a;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>wetteifern</trd>,
     <trd>um die Wette laufen</trd>,
     <trd>am Wettstreit teilnehmen</trd>
  </trdgrp>
  <trd lng="es">concursar</trd>
  <trdgrp lng="nl">
    <trd>aan een <ind>wedstrijd</ind> deelnemen</trd>,
    <trd>wedijveren</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_s;&c_t;&c_ja;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_v; &c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;&c_je;</trd>
  </trdgrp>
</drv>
</art>
