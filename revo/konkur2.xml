<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: konkur2.xml,v 1.24 2022/05/01 08:09:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>konkurs</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="konkur2.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="konkur2.0o.KOMUNE">
    <dif>
      Anta&ubreve;aran&gcirc;ita <ref tip="super" cel="konkur.0ado">konkuro</ref>, la&ubreve; difinitaj reguloj, kun
      apartaj
      ju&gcirc;antaro kaj premioj.
    </dif>
    <subsnc mrk="konkur2.0o.SPO">
      <uzo tip="fak">SPO</uzo>
      <dif>
        Aran&gcirc;ita konkuro inter individuoj a&ubreve; teamoj, eventuale en
        pluraj sinsekvaj ma&ccirc;oj, por distingo de unua, dua kaj
        ceteraj rangoj:
        <ekz>
          <tld/>o de saltado
          <fnt>
            <bib>Fab2</bib>
            <lok>Konkurso de saltado</lok>
          </fnt>;
        </ekz>
        <ekz>
          en la bilardejo gravaj <tld/>oj okazas inter pli kaj malpli lertaj ludantoj
          <fnt>
            <bib>KKl</bib>
            <lok>&Ccirc;apitro 8</lok>
          </fnt>;
        </ekz>
        <ekz>
          montri sian talenton en la internaciaj oratoraj <tld/>oj
          <fnt>
            <bib>Ret</bib>
            <lok>Dua Parto</lok>
          </fnt>;
        </ekz>
        <ekz>
          <klr>[oni]</klr> aran&gcirc;as por ili diversajn <tld/>ojn kun premioj: longdistancajn
          kurojn, cervo- a&ubreve; hundoveturadon, pafadojn
          <fnt>
            <bib>&Ccirc;uk&ccirc;oj</bib>
            <lok>Dua skizo:</lok>
          </fnt>;
        </ekz>
        <ekz>
          belec-<tld/>o de urbaj infanetoj
          <fnt>
            <bib>KrM</bib>
            <lok>40. Kaj Rapide Kreskas La Afero.</lok>
          </fnt>;
        </ekz>
        <ekz>
          kura <tld/>o
          <fnt>
            <bib>Fab4</bib>
            <lok>Kuristoj</lok>
          </fnt>;
        </ekz>
        <ekz>
          li estis premiita en la Belartaj <tld lit="K"/>oj
          <fnt>
            <bib>EeP</bib>
            <lok>Dua Parto</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="sub">
        <ref cel="turnir.0o">turniro</ref>
      </refgrp>
      <trd lng="es">concurso</trd>
    </subsnc>
    <subsnc mrk="konkur2.0o.PED">
      <uzo tip="fak">PED</uzo>
      <uzo tip="fak">POL</uzo>
      <dif>
        <ref tip="super" cel="ekzame.0o.PED">Ekzameno</ref>
        en kiu sukcesos nur la plej bonaj
        kandidatoj en limigita, anta&ubreve;difinita nombro
        <ekz>
          estigi, malfermi <tld/>on por tri vakantaj postenoj;
        </ekz>
        <ekz>
          <klr>[en]</klr> septembro okazis <tld/>-ekzamenoj &ccirc;e la Universitato de Tirano
          <fnt>
            <bib>Monato</bib>
            <aut>Bardhyl Selimi</aut>
            <vrk><url ref="&Monato;/2005/008786.php"
                >Ekzameno ekzamenata</url></vrk>
            <lok>2005</lok>
          </fnt>;
        </ekz>
        <ekz>
          danke al stipendio, kiun mi gajnis per <tld/>a ekzameno, mi atingis diploman
          gradon
          <fnt>
            <bib>Monato</bib>
            <aut>Albisturo Kvinke</aut>
            <vrk><url ref="&Monato;/2012/010361.php"
                >Lisuarte</url></vrk>
            <lok>2012</lok>
          </fnt>;
        </ekz>
        <ekz>
          finna arkitekto <klr>[&#8230;]</klr> gajnis la <tld/>on pri la plej bona projekto por
          registara konstrua&jcirc;o en Kuvajto, kaj oni invitis lin gvidi la konstruadon
          <fnt>
            <bib>KFl</bib>
            <lok>V. Pri unueco kaj lukto, kaj anka&ubreve; pri &rightquot;tipeco de l&#8217; disdivido&#8221;</lok>
          </fnt>.
        </ekz>
      </dif>
      <ref tip="sub" cel="adjudik.0o">adjudiko</ref>
      <rim>
        En ekzameno oni atentas nur la individuan valoron de la
        kandidatoj,
        en konkurso oni atentas pri ilia relativa valoro.
      </rim>
      <trd lng="es">oposici&oacute;n <klr>(examen)</klr></trd>
    </subsnc>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;</trd>
  <trd lng="bg">&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;</trd>
  <trd lng="ca">concurs</trd>
  <trdgrp lng="cs">
    <trd>konkurs</trd>,
    <trd>sout&ecaron;&zcaron;</trd>,
    <trd>z&aacute;vod</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Wettbewerb</trd>,
     <trd>Wettkampf</trd>,
     <trd>Wettstreit</trd>,
     <trd>Wettlauf</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>competition</trd>,
    <trd>contest</trd>,
    <trd>match</trd>
  </trdgrp>
  <trd lng="fr">concours</trd>
  <trdgrp lng="nl">
    <trd>wedstrijd</trd>,
    <trd>prijskamp</trd>,
    <trd>wedkamp</trd>
  </trdgrp>
  <trd lng="pl">konkurs</trd>    
  <trdgrp lng="pt">
    <trd>concurso</trd>,
    <trd>certame</trd>,    
    <trd>gincana</trd>
  </trdgrp>
  <trd lng="ro">concurs</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_ja;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>s&uacute;beh</trd>,
    <trd>s&uacute;&tcaron;a&zcaron;</trd>
  </trdgrp>
</drv>


<drv mrk="konkur2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="konkur2.0i.KOMUNE">
    <dif>
      <tld lit="K"/>e <ref tip="super" cel="konkur.0i">konkuri</ref>:
      <ekz>
        &ccirc;ion kronis la &leftquot;Lingva olimpiko&rightquot; por komencantoj, kiuj <tld/>is en vortsciado,
        deklamado, skriba tradukado kaj retoriko
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>is pli ol 4000 abiturientoj por nur 120 studlokoj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2005/008786.php"
              >Ekzameno ekzamenata</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu <ind><tld/>anto</ind> konsentas per sia partopreno <klr>[&#8230;]</klr>, ke la rajto pri unua
        presigo de premiita verko apartenas al UEA
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Wettkampfteilnehmer</trd>,
          <trd>Wettbewerber</trd>
        </trdgrp>
      </ekz>
      <ekz>
        12 kandidatoj <tld/>os pri 7 lokoj de komitatanoj B en la Komitato de UEA
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;4</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aspir.0i">aspiri</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_d;&c_z;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol; &c_u; <ind>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;&c_je;</ind></trd>,
    <trd>&c_s;&c_p;&c_a;&c_b;&c_o;&c_r;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_i;&c_r;&c_a;&c_m;</trd>,
    <trd>&c_s;&c_malmol;&c_s;&c_t;&c_je;&c_z;&c_a;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>soupe&rcaron;it</trd>,
    <trd>sout&ecaron;&zcaron;it</trd>,
    <trd>z&aacute;vodit</trd>,
    <trd>&uacute;&ccaron;astnit se konkursu</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>wetteifern</trd>,
     <trd>um die Wette laufen</trd>,
     <trd>am Wettstreit teilnehmen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>concursar</trd>,
    <trd>opositar <klr>(presntarse a examenes oposici&oacute;n)</klr></trd>
  </trdgrp>
  <trd lng="fr">concourir</trd>
  <trdgrp lng="nl">
    <trd>aan een <ind>wedstrijd</ind> deelnemen</trd>,
    <trd>wedijveren</trd>
  </trdgrp>
  <trd lng="pl">bra&cacute; udzia&lstroke; w <ind>konkursie</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_s;&c_t;&c_ja;&c_z;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_v; &c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>s&uacute;&tcaron;a&zcaron;i&tcaron;</trd>,
    <trd>z&uacute;&ccaron;astni&tcaron; sa s&uacute;&tcaron;a&zcaron;e</trd>
  </trdgrp>
</drv>


<drv mrk="konkur2.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="konkur2.0ajxo.SPO">
    <dif>
      Objekto, verko, kiun kandidato prezentas en
      <tld/>o:
      <ekz>
        la <tld/>a&jcirc;oj devas esti verkoj ankora&ubreve; nepresitaj
        <fnt>
          <bib>Plumamikoj</bib>
          <lok>Kelkaj indikoj de postpubereco</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iujn <tld/>a&jcirc;ojn en la bran&ccirc;oj Poezio, Prozo, 
        Teatra&jcirc;o kaj Eseo oni sendu rete al la sekretario
        <fnt>
          <aut>UEA</aut>,
          <vrk>Belartaj Konkursoj de UEA en 2015</vrk>,
          <lok><url
          ref="http://uea.org/dokumentoj/komunikoj/gk.php?no=556">
          Gazetaraj Komunikoj, 2014-10-14 (556)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la limdato de la alsendado de la <tld/>a&jcirc;oj: 15 apr
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 1 (51)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s;&c_n;&c_a;&c_ja; &c_p;&c_r;&c_a;&c_c;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Wettbewerbsbeitrag</trd>,
    <trd>Beitrag <klr>(Wettbewerb)</klr></trd>,
    <trd>Einreichung</trd>
  </trdgrp>
  <trd lng="fr">soumission<klr> (objet pr&eacute;sent&eacute; &agrave; un
  concours)</klr></trd>
  <trd lng="pl">praca <ind>konkursowa</ind></trd>
</drv>

<drv mrk="konkur2.acxet0o">
  <kap>a&ccirc;et<tld/>o</kap>
  <snc mrk="konkur2.acxet0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Publika <ref tip="super" cel="anonc.0o">anonco</ref> pri bezono de proviza&jcirc;oj a&ubreve; servoj, 
      al kiu kandidatoj-provizantoj
      povas respondi per <ref cel="propon.0o">propono</ref> &gcirc;is limdato, je kiu la anoncinta
      institucio
      elektos inter ili tiun, de kiu &gcirc;i a&ccirc;etos:
      <ekz>
        lokaj kompanioj <klr>[&#8230;]</klr> havas la&ubreve;le&gcirc;ajn avanta&gcirc;ojn
        en a&ccirc;et<tld/>oj kaj ofte simple proponas malpli altajn
        prezojn
        <fnt>
          <aut>S. Belov</aut>
          <vrk><url ref="https://stanobelov.blogspot.com/2018/09/teknikaj-mirakloj-de-cinio.html">
          Teknikaj mirakloj de &Ccirc;inio</url></vrk>
          <lok>2018-09-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="adjudik.0o">adjudiko</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_n;&c_k;&c_u;&c_r;&c_s; <klr>(&c_u; &c_g;&c_a;&c_n;&c_d;&c_l;&c_ib;)</klr></trd>,
    <trd>&c_t;&c_e;&c_n;&c_d;&c_a;&c_r; <klr>(&c_u; &c_g;&c_a;&c_n;&c_d;&c_l;&c_ib;)</klr></trd>
  </trdgrp>
  <trd lng="de">Ausschreibung</trd>
  <trd lng="en">call for bids</trd>
  <trd lng="fr">appel d'offre</trd>
</drv>
</art>
