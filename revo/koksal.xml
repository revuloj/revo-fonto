<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>koksalgi</rad>/o
</kap>
<drv mrk="koksal.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="koksal.0o.tuberkuloza">
    <fnt><bib>PV</bib></fnt>
    <dif>
      Tuberkuloza malsano de la koksa artiko:
      <ekz>
        malbona higieno ka&ubreve;zas la <tld/>on.
      </ekz>
    </dif>
    <trd lng="fr">coxalgie<klr tip="ind"> (tuberculeuse)</klr></trd>
  </snc>
  <snc mrk="koksal.0o.doloro">
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Doloro &ccirc;e la <ref cel="koks.0o">kokso</ref>.
    </dif>
    <trd lng="ca">cox&agrave;lgia</trd>
    <trd lng="es">coxalgia</trd>
    <trd lng="fr">coxalgie<klr tip="ind"> (simple douleur)</klr></trd>
  </snc>
  <trdgrp lng="de">
    <trd>H&uuml;ftschmerz</trd>,
    <trd>H&uuml;ftgelenkschmerz</trd>,
    <trd>Koxalgie</trd>
  </trdgrp>
</drv>
</art>
