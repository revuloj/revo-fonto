<drv mrk="akr.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akr.0igi.KOMUNE">
    <dif>
      Fari <tld/>a:
      <ekz>
        <tld/>igi sian plugilon
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 13:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>igas sian glavon
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 7:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[ili]</klr> <tld/>igis sian langon kiel glavon, direktis vortojn
        maldol&ccirc;ajn, kvaza&ubreve; siajn sagojn
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 64:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi malfacilan situacion;
      </ekz>
      <ekz>
        sperto sa&gcirc;on <tld/>igas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        lernado de Esperanto <tld/>igas la komprenon de la gepatra
        lingvo;
      </ekz>
      <ekz>
        tiu deklaro pli<tld/>igis la disputon.
      </ekz>
    </dif>
    <trdgrp lng="th">
      <trd>&#xE17;&#xE33;&#xE43;&#xE2B;&#xE49;&#xE04;&#xE21;</trd>,
      <trd>&#xE25;&#xE31;&#xE1A;</trd>
