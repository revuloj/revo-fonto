    <dif>
      &Scirc;tono a&ubreve; ilo por <tld/>igi klingojn:
      <ekz>
        oni bezonas por tio nur &scirc;tonan <tld/>igilon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li prenis tran&ccirc;ilegon, li &gcirc;in <tld/>igis sur longa
        <tld/>igilo
        <fnt>
          <aut>C. Perrault, trad. P. Sarpy</aut>,
          <vrk>La malgranda Fingreto</vrk>, 
          <lok>en:
          <url ref="http://eo.wikibooks.org/wiki/Rakontoj_pri_Feinoj">
          Rakontoj pri feinoj</url>, 1904</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#xE2B;&#xE34;&#xE19;&#xE25;&#xE31;&#xE1A;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_s;&c_t;&c_r;&c_y;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_ch;&c_y;&c_l;&c_a;</trd>
