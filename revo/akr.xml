    <trd>&c_p;&c_r;&c_a;&c_v;&c_ib;&c_c;&c_mol;
      <klr>(&c_b;&c_r;&c_y;&c_t;&c_v;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>lemma&ntilde;</trd>,
    <trd>bega&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>agusar</trd>,
    <trd>aguditzar</trd>,
    <trd>esmolar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>brousit</trd>,
    <trd>nabrousit</trd>,
    <trd>naost&rcaron;it</trd>,
    <trd>ost&rcaron;it</trd>,
    <trd>vybrousit</trd>,
    <trd>vyhrotit</trd>,
    <trd>zost&rcaron;it</trd>,
    <trd>zp&rcaron;&iacute;snit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sch&auml;rfen</trd>,
    <trd>versch&auml;rfen</trd>,
    <trd>schleifen</trd>
  </trdgrp>
  <trd lng="en">sharpen</trd>
  <trdgrp lng="es">
    <trd>aguzar</trd>,
    <trd>afilar</trd>
  </trdgrp>
  <trd lng="fa">&a_t;&f_y;&a_z; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>aiguiser</trd>,
    <trd>aff&ucirc;ter</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5B0;&#x5D7;&#x5B7;&#x5D3;&#x5B5;&#x5D3;</trd>
  <trdgrp lng="pl">
    <trd>ostrzy&cacute;</trd>,
    <trd>brusi&cacute;</trd>,
    <trd>zaostrza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>agu&ccedil;ar</trd>,
    <trd>afiar</trd>,
    <trd>acerar</trd>
  </trdgrp>
  <trdgrp lng="ru">
