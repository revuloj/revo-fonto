<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>akr</rad>/a
</kap>
<drv mrk="akr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="akr.0a.trancxa">
    <dif>
      Karakterizas ion kies rando estas tran&ccirc;a a&ubreve; pika:
      <ekz>
        tre <tld/>a razilo;
      </ekz>
      <ekz>
        <tld/>aj dentoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        malgranda birdeto, sed <tld/>a ungeto<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="fr">aiguis&eacute;</trd>
    <trd lng="sv">vass</trd>
    <trdgrp lng="pt">
      <trd>afiado</trd>,
      <trd>cortante</trd>
    </trdgrp>
  </snc>
  <snc mrk="akr.0a.akuta">
    <dif>
      Karakterizas ion, kies lateroj estigas angulon pli malgrandan ol
      ortangulo; pinta, akuta:
      <ekz>
        <tld/>a angulo;
      </ekz>
      <ekz>
        la bonodora fojno igis &ccirc;iun preterirantan &ccirc;evalon
        <tld/>igi la orelojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>aigu</trd>,
      <trd>pointu</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>agudo</trd>,
      <trd>pontudo</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>skarp</trd>,
      <trd>spetsig</trd>
    </trdgrp>
  </snc>
  <snc mrk="akr.0a.dolora">
    <dif>
      Impresanta forte kaj dolorete la sentumojn:
      <ekz>
        <tld/>a malvarmo,
        sono<fnt>Z</fnt>,
        kontrasto<fnt>Z</fnt>,
        brilo<fnt>Z</fnt>,
        vo&ccirc;o<fnt>Z</fnt>,
        bojado<fnt>B</fnt>,
        ru&gcirc;o, fajro<fnt>Z</fnt>,
        vento<fnt>Z</fnt>,
        doloro;
      </ekz>
      <ekz>
        la pipro estas <tld/>a.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>vif</trd>,
      <trd>fort</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>acre</trd>,
      <trd>picante</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>vass</trd>,
      <trd>skarp</trd>
    </trdgrp>
  </snc>
  <snc mrk="akr.0a.vigla">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Severega, malmilda:
      <ekz>
        <tld/>a batalo, krizo;
      </ekz>
      <ekz>
        <tld/>aj mokoj<fnt>Z</fnt>,
      </ekz>
      <ekz>
        ripro&ccirc;oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a fono<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la sinjoro <tld/>e atakos la pastrojn
        <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 12</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>&acirc;pre</trd>,
      <trd>acerbe</trd>
    </trdgrp>
    <trd lng="pt">r&iacute;spido</trd>
  </snc>
  <snc mrk="akr.0a.penetranta">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Sagaca, subtila, penetranta:
      <ekz>
        <tld/>aj okuloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i observis lin <tld/>e per &scirc;ajne mallevitaj
        okuloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="akut.0a.penetranta">akuta</ref>
    </refgrp>
    <trd lng="fr">per&ccedil;ant</trd>
    <trdgrp lng="pt">
      <trd>agu&ccedil;ado</trd>,
      <trd>penetrante</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_s;&c_t;&c_r;&c_y;</trd>
  <trdgrp lng="de">
    <trd>scharf</trd>,
    <trd>spitz</trd>,
    <trd>schneidend</trd>
  </trdgrp>
  <trd lng="en">sharp</trd>
  <trd lng="nl">scherp</trd>
  <trd lng="pl">ostry</trd>
  <trd lng="ru">&c_o;&c_s;&c_t;&c_r;&c_y;&c_j;</trd>
  <trd lng="sk">ostr&yacute;</trd>
</drv>

<drv mrk="akr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Tran&ccirc;rando.
    </dif>
    <trd lng="be">&c_l;&c_ja;&c_z;&c_o;</trd>
    <trdgrp lng="en">
      <trd>cutting egde</trd>,
      <trd>blade</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>tranchant</trd>,
      <trd>fil <klr>(du rasoir)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>corte</trd>,
      <trd>gume</trd>,
      <trd>fio</trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_je;&c_z;&c_v;&c_i;&c_je;</trd>
  </snc>
  <snc>
    <dif>
      Akuta rando; e&gcirc;o:
      <ekz>
        la <tld/>a&jcirc;oj de tegmentoj havas iafoje ornamitajn tegolojn.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_a;&c_s;&c_t;&c_r;&c_y;&c_jo;</trd>
    <trdgrp lng="en">
      <trd>peak</trd>,
      <trd>point</trd>
    </trdgrp>
    <trd lng="fr">ar&ecirc;te</trd>
    <trd lng="pt">aresta</trd>
    <trd lng="ru">&c_o;&c_s;&c_t;&c_r;&c_i;&c_jo;</trd>
  </snc>
</drv>

<drv mrk="akr.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari <tld/>a:
      <ekz>
        <tld/>igi malfacilan situacion;
      </ekz>
      <ekz>
        sperto sa&gcirc;on <tld/>igas<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        lernado de Esperanto <tld/>igas la komprenon de la gepatra
        lingvo;
      </ekz>
      <ekz>
        tiu deklaro pli<tld/>igis la disputon;
      </ekz>
      <ekz>
        la ekonomia krizo pli<tld/>i&gcirc;is;
      </ekz>
      <ekz>
        la apetito <tld/>i&gcirc;ados dum la
        man&gcirc;ado<fnt>Z</fnt>;
      </ekz>
      <ekz>
        maljuneco mal<tld/>igas la vidadon;
      </ekz>
      <ekz>
        post longa uzo mal<tld/>i&gcirc;as la tran&ccirc;iloj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_s;&c_t;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_t;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ja;&c_n;&c_c;&c_ib;&c_c;&c_mol;
    <klr>(&c_k;&c_a;&c_s;&c_u;)</klr></trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_ib;&c_c;&c_mol;
    <klr>(&c_b;&c_r;&c_y;&c_t;&c_v;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sch&auml;rfen</trd>,
    <trd>versch&auml;rfen</trd>
  </trdgrp>
  <trd lng="en">sharpen</trd>
  <trdgrp lng="fr">
    <trd>aiguiser</trd>,
    <trd>aff&ucirc;ter</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>agu&ccedil;ar</trd>,
    <trd>afiar</trd>,
    <trd>acerar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_o;&c_s;&c_t;&c_r;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_o;&c_s;&c_t;&c_r;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">v&auml;ssa</trd>
</drv>

<drv mrk="akr.0igilo">
  <kap><tld/>igilo</kap>
  <snc>
    <dif>
      &Scirc;tono a&ubreve; ilo por <tld/>igi klingojn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_s;&c_t;&c_r;&c_y;&c_l;&c_k;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_ch;&c_y;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="en">sharpener</trd>
  <trd lng="fr">aiguisoir</trd>
  <trdgrp lng="pt">
    <trd>afiador</trd>,
    <trd>pedra de amolar</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_i;&c_l;&c_o;</trd>
</drv>
</art>
