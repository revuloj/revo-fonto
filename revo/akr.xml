<drv mrk="akr.0igisto">
  <kap><tld/>igisto</kap>
  <snc mrk="akr.0igisto.KOMUNE">
    <dif>
      Homo, kiu profesie <tld/>igas klingojn, tran&ccirc;ilojn...:
      <ekz>
        li renkontis <tld/>igiston, kiu kantis turnante sian ilon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili trovis anta&ubreve; la mastra domo &ccirc;aron
        de tondil<tld/>igisto
        <fnt>
          <aut>T. Storm, trad. A. Bader</aut>,
          <vrk>Imenlago</vrk>,
          <lok>en: Du Amosentaj Noveloj, 1947</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Messerschleifer</trd>
  </snc>
  <trd lng="cs">brusi&ccaron;</trd>
  <trd lng="es">afilador</trd>
  <trd lng="fr">r&eacute;mouleur</trd>
  <trdgrp lng="pl">
    <trd>ostrzyciel</trd>,
    <trd>bru&sacute;nik</trd>,
    <trd>szlifierz</trd>
  </trdgrp>
  <trd lng="sk">brusi&ccaron;</trd>
</drv>

<drv mrk="akr.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="akr.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>a, tran&ccirc;ema, intensa:
      <ekz>
        fero <tld/>i&gcirc;as per fero
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 27:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ekonomia krizo pli<tld/>i&gcirc;is;
      </ekz>
      <ekz>
        la apetito <tld/>i&gcirc;ados dum la
        man&gcirc;ado.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd><ind>agudizar</ind>se</trd>,
    <trd>intensificarse</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>aiguiser</ind></trd>
  <trd lng="he">&#x5DC;&#x5B0;&#x5D7;&#x5B7;&#x5D3;&#x5B5;&#x5D3;</trd>
  <trd lng="pl">zaostrzy&cacute; si&eogonek;</trd>
</drv>

<drv mrk="akr.mal0a">
  <kap><ofc>*</ofc>mal<tld/>a</kap>
  <snc mrk="akr.mal0a.netrancxa">
    <dif>
      Malhavanta <tld/>econ sufi&ccirc;an por tran&ccirc;i a&ubreve;
      piki:
      <ekz>
        la tran&ccirc;ilo estis tiel mal<tld/>a, ke mi ne povis
        tran&ccirc;i
        per &gcirc;i la viandon
        <fnt><bib>F</bib>, <lok>&FE;34.</lok></fnt>;
      </ekz>
      <ekz>
        la tran&ccirc;ilo estis tiel mal<tld/>a, ke mi ne povis tran&ccirc;i per &gcirc;i la viandon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur la kampon venis maljuna virino, kaj per sia mal<tld/>a sentenila tran&ccirc;ilo &scirc;i
        ekfosis
        <fnt>
          <bib>Fab3</bib>
          <lok>Ekzistas diferenco</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">stumpf</trd>
    <trdgrp lng="th">
      <trd>&#xE17;&#xE37;&#xE48;&#xE2D;</trd>,
      <trd>&#xE44;&#xE21;&#xE48;&#xE04;&#xE21;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">tup&yacute;</trd>
  <trdgrp lng="es">
    <trd>desafilado</trd>,
    <trd>embotado</trd>,
    <trd>mellado</trd>
  </trdgrp>
  <trd lng="fr">&eacute;mouss&eacute;</trd>
  <trdgrp lng="pl">
    <trd>t&eogonek;py</trd>,
    <trd>g&lstroke;uchy<klr> (d&zacute;wi&eogonek;k)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_u;&c_p;&c_o;&c_j;</trd>
  <trd lng="sk">tup&yacute;</trd>
</drv>

<drv mrk="akr.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="akr.mal0igi.KOMUNE">
    <dif>
      Igi mal<tld/>a, obtuzigi:
      <ekz>
        per tio &ccirc;i vi neniam mal<tld/>igos la sagojn de la doloro
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi devus &gcirc;emi, se vi volus mal<tld/>igi mian pinton
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">abstumpfen</trd>
    <trdgrp lng="th">
      <trd>&#xE17;&#xE33;&#xE43;&#xE2B;&#xE49;&#xE17;&#xE37;&#xE48;&#xE2D;</trd>,
      <trd>&#xE17;&#xE33;&#xE43;&#xE2B;&#xE49;&#xE44;&#xE21;&#xE48;&#xE04;&#xE21;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>otupit</trd>,
    <trd>ztupit</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>desafilar</trd>,
    <trd>embotar</trd>,
    <trd>mellar</trd>,
    <trd>enromar</trd>
  </trdgrp>
  <trd lng="fr">&eacute;mousser</trd>
