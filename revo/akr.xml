<drv mrk="akr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="akr.0ajxo.trancxrando">
    <dif>
      Tran&ccirc;rando:
      <ekz>
        per la <tld/>a&jcirc;o de la glavo
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 22:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi elektas al mi tri<tld/>a&jcirc;an spadon kaj mi kuras kun &gcirc;i rapidege al la domo de
        la ministro
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a&jcirc;oj de l' sorto
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_l;&c_ja;&c_z;&c_o;</trd>
    <trdgrp lng="br">
      <trd>barvenn<klr tip="ind"> (ul lavnenn)</klr></trd>,
      <trd>dremm<klr tip="ind"> (ul lavnenn)</klr></trd>
    </trdgrp>
    <trd lng="ca">fulla<klr> (d'eina)</klr></trd>
    <trd lng="de">Schneide</trd>
    <trdgrp lng="en">
      <trd>cutting egde</trd>,
