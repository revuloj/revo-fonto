<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kren</rad>/o
</kap>
<drv mrk="kren.0o">
  <kap>
  <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="kren.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Armoracia</trd>)
      kun dika karna akre odoranta kaj gustanta radiko.
    </dif>
  </snc>
  <snc mrk="kren.0o.mangxo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="kren.0ajxo">Krena&jcirc;o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_e;&c_n; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>Meerrettich</trd>,
    <trd>Kren</trd>
  </trdgrp>
  <trd lng="en">horseradish</trd>
  <trd lng="fr">raifort</trd>
  <trd lng="he">&het;&zayin;&resh;&tav;</trd>
  <trd lng="nl">mierikswortel</trd>
  <trd lng="pl">chrzan</trd>
  <trdgrp lng="pt">
    <trd>raiz-forte</trd>,
    <trd>r&aacute;bano-bastardo</trd>,
    <trd>r&aacute;bano-de-cavalo</trd>,
    <trd>r&aacute;bano-picante</trd>,    
    <trd>r&aacute;bano-r&uacute;stico</trd>,    
    <trd>r&aacute;bano-silvestre</trd>,    
    <trd>r&aacute;bano-silvestre-maior</trd>,    
    <trd>r&aacute;b&atilde;o-silvestre</trd>,    
    <trd>rabi&ccedil;a-brava</trd>,    
    <trd>rabo-de-cavalo <klr tip="amb">(bot.)</klr></trd>,
    <trd>saramago-maior</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_r;&c_je;&c_n;</trd>
  <trd lng="yi">&kaf;&resh;&yod;&yod;&fnun;</trd>
</drv>
<drv mrk="kren.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="spic.0ajxo">Spica&jcirc;o</ref>
      el raspita radiko de 
      <ref tip="malprt" cel="kren.kampara0o">kampara <tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_e;&c_n; <klr>(&c_s;&c_p;&c_e;&c_c;&c_y;&c_ja;)</klr></trd>
</drv>
<drv mrk="kren.kampara0o">
  <kap>kampara <tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="kren.0o.genro"><tld/>o</ref> 
      (<trd lng="la">Armoracia rusticana</trd>) kun man&gcirc;ebla radiko.
    </dif>
  </snc>
</drv>
</art>
