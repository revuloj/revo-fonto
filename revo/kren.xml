<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kren</rad>/o
</kap>
<drv mrk="kren.0o">
  <kap>
  <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="kren.0o.mangxo">
    <uzo tip="fak">KUI</uzo> 
    <dif>
      <ref tip="super" cel="spic.0oajxo">Spica&jcirc;o</ref> el raspita 
      radiko de specio el la genro <ref tip="vid" cel="kren.0o"><tld/>o</ref>
      (<trd lng="la">Armoracia rusticana</trd>).
    </dif>
  </snc>
  <snc mrk="kren.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Armoracia</trd>)
      kun dika karna akre odoranta kaj gustanta radiko.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Meerrettich</trd>,
    <trd>Kren</trd>
  </trdgrp>
  <trd lng="en">horseradish</trd>
  <trd lng="fr">raifort</trd>
  <trd lng="nl">mierikswortel</trd>
  <trd lng="pl">chrzan</trd>
  <trd lng="ru">&c_h;&c_r;&c_je;&c_n;</trd>
</drv>
</art>
