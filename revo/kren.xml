<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kren.xml,v 1.29 2024/10/19 07:26:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kren</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="kren.0o">
  <kap>
  <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="kren.0o.specio">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="herb.0o.BOT" lst="voko:brasikoparencoj">Herbo</ref>
      kun longa dika karna akre odoranta kaj gustanta radiko;
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">specio</ref>
      de <ref tip="malprt" cel="armoraci.0o">armoracio</ref>
      (<trd lng="la">Armoracia rusticana</trd>) anka&ubreve; nomata kampara armoracio.
    </dif>
    <trd lng="ca">g&egrave;nere Armoracia</trd>
  </snc>
  <snc mrk="kren.0o.radiko">
    <uzo tip="fak">KUI</uzo>
    <dif>
      &Gcirc;ia <ref tip="super" cel="radik.0o.BOT">radiko</ref>
      uzata kiel man&gcirc;a&jcirc;o:
      <ekz>
        <tld/>o ne estas pli dol&ccirc;a, ol rafano
        <fnt>
          <bib>Metrop</bib>
        </fnt>!
      </ekz>
      <ekz>
        onklo Jozefo posedis azenon, &parsl;
        al &gcirc;i man&gcirc;igis karoton kaj <tld/>on
        <fnt>
          <aut>R. Rossetti</aut>,
          <vrk><url ref="&DonH;Revuoj/nlr/nlr11/infanrimajoj.html">
          Infanrima&jcirc;oj</url></vrk>,
          <lok>La Nica Literatura Revuo, 1955-09 &gcirc;is 10 (1/1), p.
          19a</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/ca/Kren_Verkauf.jpg/320px-Kren_Verkauf.jpg" prm="CC BY-SA 3.0"><tld/>o, bazaro en Vieno
        <fnt>
          <aut>Anna reg, propra verko</aut>
          <lok><url ref="&WCW;/File:Kren_Verkauf.jpg">&WCW;/File:Kren_Verkauf.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <snc mrk="kren.0o.mangxo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="kren.0ajxo">Krena&jcirc;o</ref>:
      <ekz>
        Manjo &ccirc;irka&ubreve;portis <klr>[&#8230;]</klr> malvarman porkidon kun <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni rakontas <klr>[&#8230;]</klr>, ke vi estis farita inter la viando kaj <tld/>o, kaj
        ke via patro neniam povis ekrigardi vin sen <klr>[&#8230;]</klr> Dio indulgu min
        pekulon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>!
      </ekz>
    </dif>
    <ref tip="lst" cel="spic.0o.KUI" lst="voko:spicoj">spico</ref>
    <trd lng="ca">rave rustic&agrave;</trd>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_e;&c_n; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="bg">&c_h;&c_r;&c_ja;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Meerrettich</trd>,
    <trd>Kren</trd>
  </trdgrp>
  <trd lng="en">horseradish</trd>
  <trd lng="es">r&aacute;bano<klr tip="amb"> (picante)</klr></trd>
  <trd lng="fr">raifort</trd>
  <trd lng="he">&het;&zayin;&resh;&tav;</trd>
  <trdgrp lng="ja">
    <trd>&#12475;&#12452;&#12520;&#12454;&#12527;&#12469;&#12499;</trd>,
    <trd>&#12527;&#12469;&#12499;&#12480;&#12452;&#12467;&#12531;</trd>,
    <trd>&#12431;&#12373;&#12403;&#22823;&#26681;  <pr>&#12431;&#12373;&#12403;&#12384;&#12356;&#12371;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">mierik</trd>
  <trd lng="pl">chrzan</trd>
  <trdgrp lng="pt">
    <trd>raiz-forte</trd>,
    <trd>r&aacute;bano-bastardo</trd>,
    <trd>r&aacute;bano-de-cavalo</trd>,
    <trd>r&aacute;bano-picante</trd>,
    <trd>r&aacute;bano-r&uacute;stico</trd>,
    <trd>r&aacute;bano-silvestre</trd>,
    <trd>r&aacute;bano-silvestre-maior</trd>,
    <trd>r&aacute;b&atilde;o-silvestre</trd>,
    <trd>rabi&ccedil;a-brava</trd>,
    <trd>rabo-de-cavalo <klr tip="amb">(bot.)</klr></trd>,
    <trd>saramago-maior</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_r;&c_je;&c_n;</trd>
  <trd lng="uk">&c_h;&c_r;&c_ib;&c_n;</trd>
  <trd lng="yi">&kaf;&resh;&yod;&yod;&fnun;</trd>
  <trd lng="zh">&#36771;&#26681; <pr>l&agrave;g&emacron;n</pr></trd>
</drv>

<drv mrk="kren.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="kren.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="spic.0ajxo">Spica&jcirc;o</ref>
      el raspita radiko de <tld/>o:
      <ekz>
        restas sa&ubreve;co kun <tld/>a&jcirc;o
        <fnt>
          <vrk><url ref="https://vk.com/wall-8143994_748">&c_S;&c_t;&c_je;&c_n;&c_a; | &c_V;&c_K;&c_o;&c_n;&c_t;&c_a;&c_k;&c_t;&c_je;</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_e;&c_n; <klr>(&c_s;&c_p;&c_e;&c_c;&c_y;&c_ja;)</klr></trd>
  <trd lng="ca">rave rustic&agrave; <klr>(pasta)</klr></trd>
  <trd lng="de">Meerrettich <klr>(Zubereitung)</klr></trd>
  <trd lng="nl">mierikswortelsaus</trd>
</drv>

</art>
