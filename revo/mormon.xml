<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mormon.xml,v 1.12 2022/07/24 16:44:25 revo Exp $">
<kap>
  <rad>mormon</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="mormon.0o">
  <kap><tld/>o</kap>
  <snc mrk="mormon.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Ano de <ref tip="vid" cel="mormon.0ismo.KRI"><tld/>ismo</ref>:
      <ekz>
        <nom>Ruby</nom> estas eleganta kaj potenca programlingvo. Inventis
        &gcirc;in 
        Japana <tld/>o 
        <fnt>
          <aut>S. E. Kisa</aut>,
          <vrk><url ref="http://esperanto.kisa.ca/ruby-por-esperantistoj-1">
          Ruby por Esperantistoj 1</url></vrk>,
          <lok>2010-01-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_m;&c_o;&c_n;</trd>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_m;&c_o;&c_n;</trd>
  <trd lng="ca">morm&oacute;</trd>
  <trd lng="cs">mormon</trd>
  <trd lng="de">Mormone</trd>
  <trd lng="en">Mormon</trd>
  <trd lng="fr">mormon<klr> (subst.)</klr></trd>
  <trd lng="pl">mormon</trd>
  <trd lng="sk">mormon</trd>
  <trd lng="sv">mormon</trd>
  <trd lng="uk">&#x43C;&#x43E;&c_r;&#x43C;&#x43E;&#x43D;</trd>
</drv>

<drv mrk="mormon.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="mormon.0ismo.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <ref tip="super" cel="doktri.0o">Doktrino</ref> kaj praktiko de      
      kristandevena grupo fondita 1830 de Jozefo <nom>Smith</nom> en Usono,
      alkalkulanta al la Biblio modernajn sanktajn librojn;
      oficiale <ctl>Eklezio de Jesuo Kristo de la Sanktuloj de la 
      Lastaj Tagoj</ctl>:
      <ekz>
        esence, <tld lit="M"/>ismo pretendas esti moderna revelacio de 
        la antikvaj principoj die deklaritaj kun nova emfazo kaj 
        kompleteco en nia epoko
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.esperokatolika.org/ek19861990/ek1987_11.htm">
          Mormonoj Prezentas sin mem</url></vrk>,
          <lok>Esperanta Mormonaro, 1987-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_m;&c_o;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="bg">&c_m;&c_o;&c_r;&c_m;&c_o;&c_n;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="ca">mormonisme</trd>
  <trd lng="de">Mormonentum</trd>
  <trd lng="en">Mormonism</trd>
  <trd lng="fr">mormonisme</trd>
  <trd lng="pl">mormonizm</trd>
  <trd lng="sv">mormonism</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x43E;&c_r;&#x43C;&#x43E;&#x43D;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd>&#x43C;&#x43E;&c_r;&#x43C;&#x43E;&#x43D;&c_ib;&c_z;&#x43C;</trd>
  </trdgrp>
</drv>
</art>

