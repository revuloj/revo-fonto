<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>flag</rad>/o
</kap>
<drv mrk="flag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Peco de kolorita tolo, adaptita al stango, kiu servas
      kiel ornamo en festoj a&ubreve; por fari signalojn:
      <ekz>la Esperanta <tld/>o;</ekz>
      <ekz>la &scirc;ipoj elmetis la <tld/>ojn.</ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="standa.0o">standardo</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">vlajka</trd>
  <trd lng="de">Flagge</trd>
  <trd lng="en">flag</trd>
  <trdgrp lng="fr">
    <trd>drapeau</trd>,
    <trd>pavillon<klr tip="ind"> (drapeau)</klr></trd></trdgrp> 
  <trd lng="nl">vlag</trd>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_g;</trd>
</drv>
<drv mrk="flag.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Ornami per <tld/>oj: 
      <ekz>diverskolore <tld/>itaj &scirc;ipoj<fnt>Z</fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="de">flaggen</trd>
  <trd lng="fr">pavoiser<klr tip="ind"> (orner de drapeaux)</klr></trd>
  <trd lng="nl">met <ind>vlaggen</ind> versieren</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_c;&c_v;&c_je;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol; &c_f;&c_l;&c_a;&c_g;&c_a;&c_m;&c_i;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_a;&c_sh;&c_a;&c_t;&c_mol; &c_f;&c_l;&c_a;&c_g;&c_a;&c_m;&c_i;</trd>
  </trdgrp>
</drv>
</art>
