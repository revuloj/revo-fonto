<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kanvas</rad>/o
</kap>
<drv mrk="kanvas.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Lina a&ubreve; kanaba tolo, tre lar&gcirc;ma&scirc;e kaj retece teksita, sur kiu oni
      faras broda&jcirc;on a&ubreve; punton. 
    </dif>
    <trd lng="nl">canvas</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_l;&c_o;&c_t;&c_n;&c_o;</trd>,
      <trd>&c_h;&c_o;&c_l;&c_s;&c_t;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Skizo, &gcirc;eneralaj trajtoj:
      <ekz>jen la <tld/>o la&ubreve; kiu ni povus labori.</ekz>
    </dif>
    <trd lng="nl">stramien</trd>
  </snc>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_v;&c_a;</trd>
</drv>
</art>
