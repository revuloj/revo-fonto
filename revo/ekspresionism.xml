<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekspresionism.xml,v 1.15 2023/11/07 19:22:36 revo Exp $">
<kap>
  <rad>ekspresionism</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ekspresionism.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekspresionism.0o.BELA">
    <dif>
      <ref tip="dif" cel="esprim.0ismo.BELA">Esprimismo</ref>:
      <ekz>
        lia fantoma trajno rekte rekondukas nin en la grandan epokon de 
        l' <tld/>o
        <fnt>
          <aut/>,
          <vrk><url ref="http://www.senlime.be/Kino/UnSoir.htm">
          Iun vesperon... trajno</url></vrk>,
          <lok>[vidita en 2007-01]</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efaj verkoj de <nom>Schiele</nom> kaj <nom>Klimt</nom> pa&scirc;on post pa&scirc;o montras la
        evoluon de
        junstilo al <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2014/011324.php">Tamen arto!</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        taksante tiun arton tra &gcirc;iaj kriterioj, e&ubreve;ropaj kritikistoj emas paroli pri
        realismo a&ubreve; kubismo a&ubreve; <tld/>o a&ubreve; surrealismo
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2006/008835.php">500 &ccirc;efverkoj en Monako</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_r;&c_e;&c_s;&c_ib;&c_ja;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_je;&c_k;&c_s;&c_p;&c_r;&c_je;&c_s;&c_i;&c_o;&c_n;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="ca">expressionisme</trd>
  <trd lng="cs">expresionizmus</trd>
  <trd lng="de">Expressionismus</trd>
  <trd lng="en">expressionism</trd>
  <trd lng="fr">expressionnisme</trd>
  <trd lng="pl">ekspresjonizm</trd>
  <trd lng="pt">expressionismo</trd>
  <trd lng="sk">expresionizmus</trd>
  <trd lng="uk">&c_je;&c_k;&c_s;&c_p;&c_r;&c_je;&c_s;&c_ib;&c_o;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trd lng="zh">&#x8868;&#x73B0;&#x4E3B;&#x4E49; <pr>bi&#x1CE;oxi&#xE0;nzh&#x1D4;y&#xEC;</pr></trd>
</drv>

<drv mrk="ekspresionism.0a">
  <kap><tld/>a</kap>
  <snc mrk="ekspresionism.0a.BELA">
    <dif>
      <ref tip="dif" cel="esprim.0isma.BELA">Esprimisma</ref>:
      <ekz>
        <nom>Trakl</nom> forte influis la <tld/>an lirikon kaj la germanan lirikon 
        post 1945
        <fnt>
          <aut>R. Haupenthal</aut>,
          <vrk>Georg Trakl</vrk>,
          <bib>LOdE</bib>,
          <lok>2006:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la enmar&scirc;on de la venkinta princo en la <tld/>an &leftquot;orientalan&#x201D; urbon kartonan,
        oni filmis per 2000 personoj kaj 500 &ccirc;evaloj
        <fnt>
          <bib>Monato</bib>
          <aut>Bernhard Pabst</aut>
          <vrk><url ref="&Monato;/2005/008759.php">Internacia festivalo de la muta
          filmo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_p;&c_r;&c_e;&c_s;&c_ib;&c_ja;&c_n;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">expressionista</trd>
  <trd lng="de">expressionistisch</trd>
  <trd lng="en">expressionistic</trd>
  <trd lng="fr">expressionniste<klr> (adj.)</klr></trd>
  <trd lng="pl">ekspresjonistyczny</trd>
  <trd lng="pt">expressionista</trd>
  <trdgrp lng="uk">
    <trd>&c_je;&c_k;&c_s;&c_p;&c_r;&c_je;&c_s;&c_ib;&c_o;&c_n;&c_ib;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_je;&c_k;&c_s;&c_p;&c_r;&c_je;&c_s;&c_ib;&c_o;&c_n;&c_ib;&c_s;&c_t;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

