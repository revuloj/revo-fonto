  </trdgrp>
  <trd lng="de">Judenviertel</trd>
  <trd lng="en">Jewish quarter</trd>
  <trdgrp lng="fr">
    <trd>juiverie<klr tip="ind"> (quartier juif)</klr></trd>
    <trd>ghetto<klr tip="ind"> (quartier juif)</klr></trd>
  </trdgrp>
  <trd lng="he">&resh;&vav;&bet;&ayin; &yod;&he;&vav;&dalet;&yod;</trd>
  <trd lng="hu">zsid&oacute;negyed</trd>
  <trd lng="it">ghetto<klr tip="ind"> (ebraico)</klr></trd>
  <trd lng="nl">jodenwijk</trd>
  <trdgrp lng="pl">
    <trd>dzielnica <ind>&zdot;ydowska</ind></trd>,
    <trd>getto <ind>&zdot;ydowskie</ind></trd>
  </trdgrp>
  <trd lng="pt">judiaria</trd>
  <trd lng="ru">&c_g;&c_je;&c_t;&c_t;&c_o;</trd>
  <trd lng="sk">&zcaron;idovsk&eacute; geto</trd>
</drv>

<drv mrk="jud.kontraux0ismo">
  <kap>kontra&ubreve;<tld/>ismo</kap>
  <snc mrk="jud.kontraux0ismo.MOR">
    <ref tip="dif" cel="antise.0ismo">antisemitismo</ref>
    <ekz>
      pri kelkaj radikoj de la kristana kontra&ubreve;<tld/>ismo
      <fnt>
        <aut>Gerrit Berveling</aut>,
        <vrk>Pri la dokumento "Dominus Iesus" de Vatikano</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2000a, numero 11a, p. 18a</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd
  lng="be">&c_a;&c_n;&c_t;&c_y;&c_s;&c_e;&c_m;&c_ib;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="br">enepyuzevouriezh</trd>
  <trd lng="ca">antisemitisme</trd>
  <trd lng="cs">antisemitismus</trd>
  <trdgrp lng="de">
    <trd>Antisemitismus</trd>,
    <trd>Judenhass</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>antisemitism</trd>,
    <trd>anti-Semitism</trd>
    <trd>anti-semitism</trd>
  </trdgrp>
  <trd lng="es">antisemitismo</trd>
  <trd
  lng="fa">&f_y;&a_h;&a_w;&a_d;&f_y;&zwnj;&a_s;&a_t;&f_y;&a_z;&f_y;</trd>
  <trd lng="fr">anti-s&eacute;mitisme</trd>
  <trd lng="it">antisemitismo</trd>
  <trd lng="he">&alef;&nun;&tet;&yod;&shin;&mem;&yod;&vav;&tav;</trd>
  <trdgrp lng="nl">
    <trd>anti-semitisme</trd>,
    <trd>jodenhaat</trd>
  </trdgrp>
  <trd lng="pl">antysemityzm</trd>
  <trd
  lng="ru">&c_a;&c_n;&c_t;&c_i;&c_s;&c_je;&c_m;&c_i;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">antisemitizmus</trd>
  <trd lng="sv">antisemitism</trd>
  <trd
  lng="yi">&alef;&#1463;&nun;&tet;&yod;&samekh;&ayin;&mem;&yod;&tet;&yod;&zayin;&fmem;</trd>
</drv>
