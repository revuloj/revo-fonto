<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kupol</rad>/o
</kap>
<drv mrk="kupol.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Duonsfera konstruo en formo de renversita kaliko, konsistiganta la
      supran parton de monumento.
    </dif>
    <trd lng="de">Kuppel</trd>
    <trdgrp lng="en">
      <trd>cupola</trd>,
      <trd>dome</trd>
    </trdgrp>
  </snc>
  <trd lng="nl">koepel</trd>
  <trd lng="ru">&c_k;&c_u;&c_p;&c_o;&c_l;</trd>
  <trd lng="sv">kupol</trd>
</drv>
</art>
