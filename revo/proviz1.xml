<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: proviz1.xml,v 1.21 2018/09/05 17:10:19 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>provizor</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="proviz1.0a">
  <kap><tld/>a</kap>
  <snc mrk="proviz1.0a.KOMUNE">
    <dif>
      Konsiderata kiel ne longe da&ubreve;ronta, sed
      anstata&ubreve;igota per definitiva;
      atenda:
      <ekz>
        <tld/>a komitato, regularo, ponto;
      </ekz>
      <ekz>
        <tld/>e fiksi la konsiston de l' komisiono;
      </ekz>
      <ekz>
        nur por <tld/>a laboro kaj kun negranda salajro
        <fnt>
          <aut>H. Weinhengst</aut>
          <vrk><url
          ref="http://www.fadedpage.com/showbook.php?pid=20131216">
          Tur-Strato 4</url></vrk>
          <lok>1934</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_s;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>do&ccaron;asn&yacute;</trd>,
    <trd>provizorn&iacute;</trd>,
    <trd>prozat&iacute;mn&iacute;</trd>,
    <trd>p&rcaron;echodn&yacute;</trd>
  </trdgrp>
  <trd lng="de">provisorisch</trd>
  <trdgrp lng="en">
    <trd>provisional</trd>,
    <trd>temporary</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>provisional</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_m;&a_w;&a_q;&a_t;</trd>,
    <trd>&a_m;&a_w;&a_q;&a_t;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">provisoire</trd>
  <trdgrp lng="hu">
    <trd>ideiglenes</trd>,
    <trd>&aacute;tmeneti</trd>,
    <trd>id&odblac;leges</trd>
    <trd>proviz&oacute;rikus</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>provisional</trd>,
    <trd>sementara</trd>,
    <trd>temporer</trd>,
    <trd>tentatif</trd>
  </trdgrp>
  <trd lng="nl">voorlopig</trd>
  <trdgrp lng="pl">
    <trd>prowizoryczny</trd>,
    <trd>przej&sacute;ciowy</trd>,
    <trd>tymczasowy</trd>
  </trdgrp>
  <trd lng="ro">temporar</trd>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>do&ccaron;asn&yacute;</trd>,
    <trd>predbe&zcaron;n&yacute;</trd>,
    <trd>proviz&oacute;rny</trd>
  </trdgrp>
  <trd lng="sv">provisorisk</trd>
</drv>

<drv mrk="proviz1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="proviz1.0i.KOMUNE">
    <dif>
      Esti <tld/>a, funkcii portempe, atende por pli da&ubreve;ra stato:
      <ekz>
        drinko semas kura&gcirc;on en la plej sterila grundo kaj feli&ccirc;on, sed 
        beda&ubreve;rinde amba&ubreve; <tld/>as
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&ecirc;tre <ind>provisoire</ind></trd>
</drv>

<drv mrk="proviz1.0o">
  <kap><tld/>o</kap>
  <snc mrk="proviz1.0o.KOMUNE">
    <dif>
      <tld lit="P"/>a situacio, <tld/>a aferstato:
      <ekz>
        <tld/>o da&ubreve;ras jam de 38 jaroj!
        <fnt>
          <aut>D. Durand</aut>,
          <vrk><url ref="http://www.recim.org/far/daniel-eo.htm">
          Pro plena libero</url></vrk>,
          <lok>[vidita en 2018]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">situation <ind>provisoire</ind></trd>
</drv>
</art>
