<drv mrk="minut.0e">
  <kap><tld/>e</kap>
  <snc mrk="minut.0e.KAL">
    <dif>
      Dum unu <tld/>o:
      <ekz>
        7200 turnoj <tld/>e
        <fnt>
          <bib>Monato</bib>
          <aut>Franck Arnaud</aut>
          <vrk><url ref="&Monato;/2004/008261.html">Kvietigu tiun komputilon!</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la malkopiilo]</klr> purigas <tld/>e po tri A4-foliojn, kaj eblos reuzi la
        paperon plurfoje
        <fnt>
          <aut>div.</aut>
          <vrk><url ref="&Monato;/1993/A931002.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/10, p. 4</lok>
         </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>min&uuml;tlich</trd>,
      <trd>in der <ind>Minute</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>b&ecaron;hem kr&aacute;tk&eacute; chv&iacute;le</trd>,
    <trd>v minut&ecaron;</trd>
  </trdgrp>

  <trd lng="es">en un <ind>minuto</ind></trd>
  <trd lng="id">per <ind>menit</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x5206;&#x9593;&#x306B;  <pr>&#x3072;&#x3068;&#x3075;&#x3093;&#x304B;&#x3093;&#x306B;</pr></trd>,
    <trd>&#x3061;&#x3087;&#x3063;&#x3068;&#x306E;&#x9593;&#x306B;  <pr>&#x3061;&#x3087;&#x3063;&#x3068;&#x306E;&#x3042;&#x3044;&#x3060;&#x306B;</pr></trd>
  </trdgrp>
  <trd lng="nl">per <ind>minuut</ind></trd>
  <trd lng="pl">w ci&aogonek;gu <ind>minuty</ind></trd>
  <trd lng="sk">v min&uacute;te</trd>
</drv>

<drv mrk="minut.cxiu0e">
  <kap>
    &ccirc;iu<tld/>e
  </kap>
  <snc mrk="minut.cxiu0e.tuttempe">
    <dif>
      Tuttempe, <ref tip="sin" cel="konsta.0e">konstante</ref>, en &ccirc;iu <tld/>o:
      <ekz>
        ili sidi&gcirc;os sur la tero, tremos &ccirc;iu<tld/>e, kaj eksentos teruron
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 26:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plua interparolado iris ne tiel bone, &ccirc;iu<tld/>e oni penis korekti unu la
        alian
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#x2026;]</klr> legis kaj legis kaj balancadis &ccirc;iu<tld/>e la kapon, &ccirc;ar estis
        al li agrable scii&gcirc;i pri tiuj belegaj priskriboj de la urbo
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        via vivo estos senripoza &ccirc;iu<tld/>a singardo kontra&ubreve; atencoj
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2009/009725.php">Nu, karaj fratoj ...</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>die ganze <ind>Zeit</ind></trd>,
    <trd>dauernd</trd>,
    <trd>min&uuml;tlich</trd>
  </trdgrp>
<trd lng="id">setiap <ind>menit</ind></trd>
  <trdgrp lng="zh">
    <trd>&#x4E00;&#x5411; <pr>y&#x12B;xi&#xE0;ng</pr></trd>,
    <trd>&#x65F6;&#x65F6;&#x523B;&#x523B; <pr>sh&#xED;sh&#xED;k&#xE8;k&#xE8;</pr></trd>,
    <trd>&#x6642;&#x6642;&#x523B;&#x523B; <pr>sh&#xED;sh&#xED;k&#xE8;k&#xE8;</pr></trd>,
    <trd>&#x59CB;&#x7D42; <pr>sh&#x1D0;zh&#x14D;ng</pr></trd>,
    <trd>&#x59CB;&#x7EC8; <pr>sh&#x1D0;zh&#x14D;ng</pr></trd>,
    <trd>&#x6BCF;&#x65F6;&#x6BCF;&#x523B; <pr>m&#x11B;ish&#xED;m&#x11B;ik&#xE8;</pr></trd>,
    <trd>&#x6BCF;&#x6642;&#x6BCF;&#x523B; <pr>m&#x11B;ish&#xED;m&#x11B;ik&#xE8;</pr></trd>
  </trdgrp>
