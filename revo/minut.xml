<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>minut</rad>/o
</kap>
<drv mrk="minut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <fnt><bib>VdE</bib></fnt>
  <snc mrk="minut.0o.tempo">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de tempo,
      egala al sesdek <ref tip="prt" cel="sekund.0o.tempo">sekundoj</ref>;
      simbole &leftquot;mn&rightquot;:
      <ekz>
        <tld/>o estas sesdekona parto de horo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="minut.0o.angulo">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="angul.0o.MAT">angulo</ref>, egala al sesdekono
      de <ref tip="malprt" cel="grad.0o">grado</ref>.
    </dif>
    <trd lng="pl">minuta</trd>
  </snc>
  <trd lng="be">&c_h;&c_v;&c_ib;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trdgrp lng="br">
    <trd>munut</trd>,
    <trd>munutenn</trd>
  </trdgrp>
  <trd lng="cs">minuta</trd>
  <trd lng="de">Minute</trd>
  <trd lng="en">minute</trd>
  <trd lng="fr">minute<klr tip="ind"> (unit&eacute; de temps, d'angle)</klr></trd>
  <trd lng="hu">perc</trd>
  <trd lng="nl">minuut</trd>
  <trd lng="pl">minuta</trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_u;&c_t;&c_a;</trd>
  <trd lng="sk">min&uacute;ta</trd>
</drv>

<drv mrk="minut.0a">
  <kap><tld/>a</kap>
  <snc mrk="minut.0a.KAL">
    <dif>
      Da&ubreve;ranta unu <tld/>on:
      <ekz>
        estingi&gcirc;i kiel la lumo de <tld/>a meteoro<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="moment.0a">momenta</ref>
    </refgrp>
  </snc>
  <trd lng="hu">percnyi</trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_u;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="minut.0e">
  <kap><tld/>e</kap>
  <snc mrk="minut.0e.KAL">
    <dif>
      Dum unu <tld/>o:
      <ekz>
        pri &ccirc;iuj <tld/>e kreskantaj projektoj mi absolute
        silentados<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
