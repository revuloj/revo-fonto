<drv mrk="minut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <fnt><bib>VdE</bib></fnt>
  <snc mrk="minut.0o.tempo">
    <uzo tip="fak">KAL</uzo>
    <mlg>min</mlg>
    <dif>
      Komunuza <ref tip="lst" cel="unu.mezur0o" lst="voko:da&ubreve;roj">mezurunuo</ref> de
      tempo, egala al sesdek <ref tip="prt" cel="sekund.0o.tempo">sekundoj</ref>:
      <ekz>
        sesdek <tld/>oj faras unu horon, kaj unu <tld/>o konsistas el sesdek sekundoj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        post kelkaj <tld/>oj levi&gcirc;os la suno
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        post du <tld/>oj li sidis en dro&scirc;ko, kiu veturigis lin al la Kristiana haveno
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ok <tld/>ojn kaj kelke da sekundoj bezonas la sunlumo por
        <klr>[atingi la teron]</klr>
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        kara estas dono en <tld/>o de bezono
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kion jaroj ne donis, ofte <tld/>o alportas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sidas gasto <tld/>on, sed vidas la tuton
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#xE19;&#xE32;&#xE17;&#xE35;</trd>
    <trd lng="zh">&#x5206;&#x949F; <pr>f&emacron;n zh&#x14D;ng</pr></trd>
  </snc>
  <snc mrk="minut.0o.angulo">
    <uzo tip="fak">MAT</uzo><uzo tip="fak">GEOD</uzo>
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">Mezurunuo</ref> de
      <ref cel="angul.0o.MAT">angulo</ref>, egala al sesdekono
      de <ref tip="malprt" cel="grad.0o">grado</ref>:
      <ekz>
        desegna&jcirc;o de sep &ccirc;ielringoj kun gradoj, <tld/>oj, sekundoj kaj sekunddekonoj
        <fnt>
          <bib>Monato</bib>
          <aut>Vitalij kaj Anatolij Ionesov</aut>
          <vrk><url ref="&Monato;/2010/009996.php">Mirzo Ulugbek: granda re&gcirc;o de
          astronomio</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        la komunumo situas je 20 gradoj kaj 32 <tld/>oj norda latitudo kaj 98 gradoj kaj
        29 <tld/>oj okcidenta longitudo
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/Huayacocotla">Huayacocotla</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">minuta</trd>
  </snc>
