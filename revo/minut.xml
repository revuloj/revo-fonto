<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>minut</rad>/o
</kap>

<drv mrk="minut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <fnt><bib>VdE</bib></fnt>
  <snc mrk="minut.0o.tempo">
    <uzo tip="fak">KAL</uzo>
    <mlg>min</mlg>
    <dif>
      Komunuza <ref tip="lst" cel="unu.mezur0o"
      lst="voko:da&ubreve;roj">mezurunuo</ref> de
      tempo,
      egala al sesdek <ref tip="prt"
      cel="sekund.0o.tempo">sekundoj</ref>:
      <ekz>
        <tld/>o estas sesdekona parto de horo.
      </ekz>
    </dif>
    <trd lng="th">&#3609;&#3634;&#3607;&#3637;</trd>
    <trd lng="zh">&#20998;&#38047; [<ind>f&#275;n zh&#333;ng</ind>]</trd>
  </snc>
  <snc mrk="minut.0o.angulo">
    <uzo tip="fak">MAT</uzo>
    <dif>
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">Mezurunuo</ref> de
      <ref cel="angul.0o.MAT">angulo</ref>, egala al sesdekono
      de <ref tip="malprt" cel="grad.0o">grado</ref>.
    </dif>
    <trd lng="pl">minuta</trd>
  </snc>
  <trd lng="be">&c_h;&c_v;&c_ib;&c_l;&c_ib;&c_n;&c_a;</trd>
  <trd lng="bg">&c_m;&c_i;&c_n;&c_u;&c_t;&c_a;</trd>
  <trdgrp lng="br">
    <trd>munut</trd>,
    <trd>munutenn</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chv&iacute;le</trd>,
    <trd>minuta</trd>
  </trdgrp>
  <trd lng="de">Minute</trd>
  <trd lng="en">minute</trd>
  <trd lng="es">minuto</trd>
  <trd lng="fr">minute<klr tip="ind"> (unit&eacute; de temps, 
d'angle)</klr></trd>
  <trd lng="he">&dalet;&qof;&he;</trd>
  <trd lng="hu">perc</trd>
  <trd lng="id">menit</trd>
  <trd lng="nl">minuut</trd>
  <trd lng="pt">minuto</trd>
  <trd lng="pl">minuta</trd>
  <trd lng="ro">minut</trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_u;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>chv&iacute;&lcaron;a</trd>,
    <trd>min&uacute;ta</trd>
  </trdgrp>
  <trd lng="zh">&#20998; [<ind>f&#275;n</ind>]</trd>
</drv>

<drv mrk="minut.0a">
  <kap><tld/>a</kap>
  <snc mrk="minut.0a.KAL">
    <dif>
      Da&ubreve;ranta unu <tld/>on:
      <ekz>
        estingi&gcirc;i kiel la lumo de <tld/>a meteoro<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="moment.0a">momenta</ref>
    </refgrp>
  </snc>
  <trd lng="es">moment&aacute;neo</trd>
  <trd lng="hu">percnyi</trd>
  <trd lng="pl">minutowy</trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_u;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="minut.0e">
  <kap><tld/>e</kap>
  <snc mrk="minut.0e.KAL">
    <dif>
      Dum unu <tld/>o:
      <ekz>
        pri &ccirc;iuj <tld/>e kreskantaj projektoj mi absolute
        silentados<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>b&#65533;&#8250;hem kr&aacute;tk&eacute; chv&iacute;le</trd>,
    <trd>v minut&#65533;&#8250;</trd>
  </trdgrp>
  <trd lng="es">en un <ind>minuto</ind></trd>
  <trd lng="pl">w ci&aogonek;gu <ind>minuty</ind></trd>
  <trd lng="sk">v min&uacute;te</trd>
</drv>
</art>
