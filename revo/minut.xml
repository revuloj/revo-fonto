<drv mrk="minut.0a">
  <kap><tld/>a</kap>
  <snc mrk="minut.0a.KAL">
    <dif>
      Da&ubreve;ranta <tld/>on:
      <ekz>
        li rigardis atente la viza&gcirc;on de la juna virino kaj respondis post <tld/>a medito
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni devis sen ia e&ccirc; <tld/>a &scirc;anceli&gcirc;o akcepti <klr>[&#x2026;]</klr>
        <fnt>
          <bib>LR</bib>
          <lok>4. Fonetiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <klr>[&#x2026;]</klr> luis taksion por la dudek-<tld/>a voja&gcirc;o
        <fnt>
       <aut>Thomas Eccardt</aut>
          <vrk><url ref="&Monato;/2000/006859.php">Urbeto a&ubreve; sendependa lando?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili tuj &gcirc;uis kelk<tld/>an navigadon &ccirc;irka&ubreve; la atlantika ja&hcirc;thaveno
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2003/007992.html">Persistemo donis plezuron!</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="moment.0a">momenta</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>min&uuml;tig</trd>,
    </trdgrp>
  </snc>
  <snc mrk="minut.0a.rilata">
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        la <tld/>a montrilo longas 1,5&#x202F;m
        <fnt>
          <bib>Monato</bib>
          <aut>La denaska &hcirc;arkovano Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2004/008012.html">Katedralo renaski&gcirc;anta</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> esperas pri last<tld/>a savanto
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2005/008487.php">Fina fakturo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        Karolino! murmuretis Marta kun unua<tld/>a miri&gcirc;o
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">moment&aacute;neo</trd>
  <trd lng="hu">percnyi</trd>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x5206;&#x9593;&#x306E;  <pr>&#x3072;&#x3068;&#x3075;&#x3093;&#x304B;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x3061;&#x3087;&#x3063;&#x3068;&#x306E;&#x9593;&#x306E;  <pr>&#x3061;&#x3087;&#x3063;&#x3068;&#x306E;&#x3042;&#x3044;&#x3060;&#x306E;</pr></trd>
  </trdgrp>
