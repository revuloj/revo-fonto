  <art mrk="$Id: gordi.xml,v 1.3 2022/04/30 08:42:40 afido Exp $">
    <kap>
      <rad>Gordi</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="gordi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIT</uzo>
  <snc mrk="gordi.0o.mita_regho">
    <dif>
      Mita frigia <ref tip="super" cel="regx.0o">re&gcirc;o</ref>, 
      patro de <ref cel="midas.0o">Midaso</ref> kaj fondinto de 
      samnoma <ref cel="frigi.0o">frigia</ref> &ccirc;efurbo:
      <ekz>
        pluraj re&gcirc;oj de Frigio havis la nomojn <tld/>o kaj Midaso, sed ne estas klare,
        kiom, unu el <klr>[ili]</klr> estis fama pro la ege komplika nodo, kiu ligis
        lian &ccirc;aron al la jugo
        <fnt>
          <vrk><url ref="&Viki;/Frigio">Frigio - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="nod.gordia0o">gordia nodo</ref>
    <trd lng="de">Gordios</trd>
    <trd lng="en">Gordias</trd>
    <trd lng="fr">Gordias</trd>
    <trd lng="grc">&Gamma;&omikron;&rho;&delta;&jota_ton;&alfa;&sigma_fina;</trd>
  </snc>
  <trd lng="uk">&#x413;&#x43E;&#x440;&#x434;&#x456;&#x439;</trd>
</drv>

  </art>

