<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>aktual</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="aktual.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ekzistanta en nuntempo.
    </dif>
    <trdgrp lng="pt">
      <trd>atual</trd>,
      <trd>vigente</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Interesanta pro nuneca karaktero:
      <ekz>
        la demando pri internacia lingvo
        estas &ccirc;iam <tld/>a.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sensac">sensacia</ref>,
      <ref cel="furor.0o">furoro</ref>.
    </refgrp>
  </snc>
  <trd lng="de">aktuell</trd>
  <trd lng="fr">actuel</trd>
  <trd lng="nl">actueel</trd>
  <trd lng="it">attuale</trd>
  <trd lng="pt">atual</trd>
  <trd lng="ru">&c_a;&c_k;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">aktuell</trd>
</drv>
</art>
