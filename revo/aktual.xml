<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>aktual</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="aktual.0a">
  <kap><tld/>a</kap>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      Ekzistanta en nuntempo.
    </dif>
    <trd lng="be">&c_s;&c_u;&c_ch;&c_a;&c_s;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>momentan</trd>,
      <trd>gegenw&auml;rtig</trd>,
      <trd>derzeitig</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>current</trd>,
      <trd>present</trd>,
      <trd>up-to-the-minute</trd>,
      <trd>live</trd>
    </trdgrp>
    <trd lng="es">actual</trd>
    <trdgrp lng="pt">
      <trd>atual</trd>,
      <trd>vigente</trd>
    </trdgrp>
    <trd lng="sk">s&uacute;&ccaron;asn&yacute;</trd>
  </snc>
  <snc>
    <dif>
      Interesanta pro nuneca karaktero:
      <ekz>
        la demando pri internacia lingvo
        estas &ccirc;iam <tld/>a.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sensac">sensacia</ref>,
      <ref cel="furor.0o">furoro</ref>.
    </refgrp>
    <trd lng="be">&c_a;&c_k;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trd lng="de">aktuell</trd>
    <trd lng="es">vigente</trd>
    <trd lng="sk">aktu&aacute;lny</trd>
  </snc>
  <trd lng="fr">actuel</trd>
  <trd lng="nl">actueel</trd>
  <trd lng="it">attuale</trd>
  <trd lng="pt">atual</trd>
  <trd lng="ru">&c_a;&c_k;&c_t;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">aktuell</trd>
</drv>
</art>
