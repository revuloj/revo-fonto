  <trdgrp lng="be">
    <trd>&c_U;&c_l;&c_a;&c_f;</trd>,
    <trd>&c_O;&c_l;&c_a;&c_f; <klr>(&c_ib;&c_m;&c_ja;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Olaf</trd>,
    <trd>Olav</trd>
  </trdgrp>
  <trd lng="pl">Olaf</trd>
  <trd lng="ro">Olav</trd>
</drv>

