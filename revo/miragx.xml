<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mira&gcirc;</rad>/o
</kap>
<drv mrk="miragx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Optika fenomeno okazanta en varmaj landoj kaj konsistanta en tio, ke
      tre malproksimaj objektoj aperas kiel proksimaj, sed renversitaj,
      kvaza&ubreve; spegulataj en akvo.
    </dif>
  <trdgrp lng="de">
    <trd>Luftspiegelung</trd>,
    <trd>Fatamorgana</trd>
  </trdgrp>
  <trd lng="hu">d&eacute;lib&aacute;b</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Erariga alloga iluzio.
    </dif>
    <trdgrp lng="de">
      <trd>Trugbild</trd>,
      <trd>Wahngebilde</trd>,
      <trd>T&auml;uschung</trd>
    </trdgrp>
  </snc>
  <trd lng="en">mirage</trd>
  <trdgrp lng="hu">
    <trd>d&eacute;lib&aacute;b</trd>,
    <trd>&aacute;br&aacute;ndk&eacute;p</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>luchtspiegeling</trd>,
    <trd>fatamorgana</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_r;&c_a;&c_zh;</trd>
</drv>
</art>
