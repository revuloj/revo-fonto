<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: hekat.xml,v 1.6 2025/10/06 20:32:44 afido Exp $">
    <kap>
      <rad>Hekat</rad>/o
    </kap>

<drv mrk="hekat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIT</uzo>
  <snc>
    <dif>
      Helena <ref tip="super" cel="di.0ino">diino</ref>
      de magio, rito kaj komuniko kun mortintoj
      <klr>(<ref cel="nekromanci.0o">nekromancio</ref>)</klr>,
      bildigita ekde la 4a jc a.K. triforma:
      <ekz>
        Vi, kiu sur vi portas la malbenon / Trifojan de <tld/>o, &ndash; la venenon
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        je <tld/>o Triforma
        <fnt>
          <aut>Henryk Sienkiewicz, trad. Lidia Zamenhof </aut>
          <vrk><url ref="&Tekstaro;?nomo=quo-vadis">Quo Vadis</url></vrk>
        </fnt>!
      </ekz>
    </dif>
    <trd lng="be">&c_G;&c_je;&c_k;&c_a;&c_t;&c_a;</trd>
    <trd lng="de">Hekate</trd>
    <trd lng="en">Hecate</trd>
    <trd lng="fr">H&eacute;cate</trd>
    <trd lng="grc">&Epsilon_densa;&kappa;&alfa_ton;&tau;&eta;</trd>
    <trd lng="pl">Hekate</trd>
    <trd lng="ru">&c_G;&c_je;&c_k;&c_a;&c_t;&c_a;</trd>
  </snc>
  <trd lng="uk">&c_G;&c_je;&#x43A;&c_a;&c_t;&c_a;</trd>
  <trd lng="zh">&#x8D6B;&#21345;&#x5FD2; <pr>H&#xE8;k&#x1CE;t&#xE8;</pr></trd>
</drv>
</art>

