<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: farad.xml,v 1.22 2023/11/09 19:13:15 revo Exp $">
<kap>
<rad>Farad</rad>/o
</kap>
<drv mrk="farad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Anglalingve: <trd lng="en">Faraday <klr tip="amb">(Michael)</klr></trd>,
      1791-1867, angla fizikisto.
    </dif>
  </snc>
  <trd lng="be">&c_F;&c_a;&c_r;&c_a;&c_d;&c_e;&c_j;</trd>
  <trd lng="bg">&c_M;&c_a;&c_j;&c_k;&c_malmol;&c_l; <ind>&c_F;&c_a;&c_r;&c_a;&c_d;&c_je;&c_j;</ind></trd>
  <trd lng="ca">Faraday</trd>
  <trd lng="cs">Faraday M.</trd>
  <trd lng="de">Faraday</trd>
  <trd lng="fr">Faraday</trd>
  <trd lng="hu">Faraday</trd>
  <trd lng="id">Faraday</trd>
  <trd lng="nl">Michael <ind>Faraday</ind></trd>
  <trd lng="pt">Faraday</trd>
  <trd lng="ru">&c_F;&c_a;&c_r;&c_a;&c_d;&c_je;&c_j;</trd>
  <trd lng="sk">Faraday M.</trd>
  <trd lng="uk">&c_M;&c_a;&c_j;&c_k;&c_l;&c_a; <ind>&c_F;&c_a;&c_r;&c_a;&c_d;&c_je;&c_ja;</ind></trd>
  <trd lng="zh">&#x9EA5;&#x53EF;&#x6CD5;&#x62C9;&#x7B2C; <pr>m&#xE0;ik&#x11B;f&#x1CE;l&#x101;d&#xEC;</pr></trd>
</drv>

<drv mrk="farad.f0o">
  <kap><tld lit="f"/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PV</bib></fnt>
  <mlg kod="SI">F</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">Mezurunuo</ref> de
      <ref cel="kapaci1.0o">kapacitanco</ref> en la Internacia Sistemo
      de unuoj:
      <ekz>
        unu <tld lit="f"/>o egalas al la kapacitanco de kondensatoro,
        kiu
        &scirc;argite per unu <ref cel="kulomb.k0o">kulombo</ref> aperigas
        tension je unu <ref cel="volt.v0o">volto</ref> inter siaj
        armaturoj.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="hom">
    Ne konfuzu kun:
    <ref cel="far.0ado">farado</ref>
  </refgrp>
  <trd lng="be">&c_f;&c_a;&c_r;&c_a;&c_d;&c_a;</trd>
  <trd lng="bg">&c_f;&c_a;&c_r;&c_a;&c_d;</trd>
  <trd lng="ca">farad</trd>
  <trd lng="de">Farad</trd>
  <trd lng="fr">farad</trd>
  <trd lng="hu">farad</trd>
  <trd lng="id">farad</trd>
  <trd lng="ja">&#x30D5;&#x30A1;&#x30E9;&#x30C7;&#x30FC;</trd>
  <trd lng="nl">farad</trd>
  <trd lng="ru">&c_f;&c_a;&c_r;&c_a;&c_d;&c_a;</trd>
  <trd lng="uk">&c_f;&c_a;&c_r;&c_a;&c_d;</trd>
  <trd lng="sk">Faraday M.</trd>
  <trd lng="cs">Faraday M.</trd>
</drv>
</art>

