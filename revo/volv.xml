    <trd>&#x6ED1;&#x8F2A; <pr>hu&#xE1;l&#xFA;n</pr></trd>,
    <trd>&#x6ED1;&#x8F6E; <pr>hu&#xE1;l&#xFA;n</pr></trd>,
