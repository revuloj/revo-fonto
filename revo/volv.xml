<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>volv</rad>/i
</kap>
<drv mrk="volv.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Plurfoje rondmeti ion flekseblan &ccirc;irka&ubreve; io alia:
      <ekz>
        <tld/>i &scirc;nuron &ccirc;irka&ubreve; bastono, kolskarpon sur sian
        kolon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        banda&gcirc;on &ccirc;irka&ubreve; brako;
      </ekz>
      <ekz>
        vinberbran&ccirc;oj <tld/>is sin &ccirc;irka&ubreve; la kolonoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        hederoj <tld/>i&gcirc;is &ccirc;irka&ubreve; la arboj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>ajo da fadeno, de lupolo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        granda pergamena rul<tld/>a&jcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_m;&c_o;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>umwickeln</trd>,
      <trd>umbinden</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_v;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Rondirigi objekton &ccirc;irka&ubreve; &gcirc;i mem:
      <ekz>
        <tld/>i paperfolion;
      </ekz>
      <ekz>
        <tld/>ita rubando, petalo de rozo;
      </ekz>
      <ekz>
        kun manikoj supren<tld/>itaj &gcirc;is la kubutoj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">w&auml;lzen</trd>
    <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="cs">vinout</trd>
  <trd lng="en">roll</trd>
  <trd lng="es">enrollar</trd>
  <trdgrp lng="nl">
    <trd>wikkelen</trd>,
    <trd>oprollen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>enrolar</trd>,
    <trd>envolver</trd>
  </trdgrp>
</drv>

<drv mrk="volv.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;igi objekton per io, kio <tld/>i&gcirc;as &ccirc;irka&ubreve;e:
      <ekz>
        &ccirc;irka&ubreve;<tld/>i la kolon per rubando<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &scirc;tato &ccirc;irka&ubreve;<tld/>as la modernan individuon per tro multaj
        ligiloj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vind.0i">vindi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_o;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="nl">omwikkelen</trd>
  <trd lng="pt">enrolar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Etendi ion <tld/>itan &ccirc;irka&ubreve; si mem:
      <ekz>
        dis<tld/>i tolrula&jcirc;on, standardon;
      </ekz>
      <ekz>
        plene dis<tld/>ita floro.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plivastigi, pliampleksigi; realigi &ccirc;ion, kio estas farebla:
      <ekz>
        dis<tld/>i &ccirc;iujn siajn kapablojn, viglan propagandon, sian
        programon, la popolan instruadon;
      </ekz>
      <ekz>
        la uzado de Esperanto dis<tld/>as &ccirc;e la infanoj la intereson al
        fremdaj popoloj kaj guston al geografio;
      </ekz>
      <ekz>
        la turka registaro entreprenis dis<tld/>i sian milit&scirc;iparon;
      </ekz>
      <ekz>
        la industrio pli kaj pli dis<tld/>i&gcirc;as;
      </ekz>
      <ekz>
        la programo glate dis<tld/>i&gcirc;is.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="efekti.0igi">efektivigi</ref>,
      <ref cel="evolu.0i">evolui</ref>,
      <ref cel="plen.0umi">plenumi</ref>,
      <ref cel="grand">pligrandigi</ref>,
      <ref cel="fort">plifortigi</ref>,
      <ref cel="akcel.0i">akceli</ref>,
      <ref cel="kresk.0igi">kreskigi</ref>,
      <ref cel="progre.0igi">progresigi</ref>.
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>ontplooien</trd>,
    <trd>ontwikkelen<klr tip="ind"> (ontplooien)</klr></trd>
  </trdgrp>
  <trd lng="pt">desenrolar</trd>
</drv>

<drv mrk="volv.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Dis<tld/>i, disfaldi:
      <ekz>
        la bur&gcirc;ono el<tld/>i&gcirc;as;
      </ekz>
      <ekz>
        tiu literaturo rapide el<tld/>i&gcirc;is.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Klare kaj detale prezenti temon por &gcirc;in konigi kaj komprenigi:
      <ekz>
        el<tld/>i argumenton, planon.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="volv.el0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 13</lok></fnt>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      Esprimi ion kiel <ref cel="sum.0o">sumon</ref> de multaj
      <ref cel="term.0o.sumo">termoj</ref>, <ref
      cel="produt.0o">produton</ref>
      de multaj <ref cel="faktor.0o.multip">faktoroj</ref>, ofte e&ccirc; kiel
      <ref cel="seri.0o.MAT">serion</ref> a&ubreve; nefinian produton:
      <ekz>
        el<tld/>i funkcion en potencoserion, la&ubreve; potencoj de la
        variablo;
      </ekz>
      <ekz>
        <ind>el<tld/>a&jcirc;o</ind> de
        <ref cel="ekspon1.0o">eksponencialo</ref> &gcirc;is la dua ordo estas
        <frm>1+<k>x</k>+<k>x</k><sup>2</sup>/2</frm>.
        <trd lng="de">Entwicklung</trd>
        <trd lng="en">expansion</trd>
        <trd lng="fr">d&eacute;veloppement<klr tip="ind"> (math.)</klr></trd>
        <trd lng="pl">rozwini&eogonek;cie</trd>
        <trd lng="ru">&c_r;&c_a;&c_z;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
    </dif>
    <trd lng="de">entwickeln</trd>
    <trd lng="en">expand</trd>
    <trd lng="fr">d&eacute;velopper</trd>
    <trd lng="pl">rozwija&cacute;</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="nl">ontvouwen</trd>
  <trd lng="pt">desenvolver</trd>
</drv>

<drv mrk="volv.el0anto">
  <kap>el<tld/>anto</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
  <snc>
    <dif>
      <klr>(de ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>)</klr>
      La kurbo, kies <ref cel="volv.el0ato">el<tld/>ato</ref> &gcirc;i
      estas:
      <ekz>
        <ind>el<tld/>anto de cirklo</ind>.
        <trd lng="de">Kreisevolvente</trd>
        <trd lng="en">evolvent of circle</trd>
        <trd lng="fr">d&eacute;veloppante du cercle</trd>
        <trd lng="pl">ewolwenta okr&eogonek;gu</trd>
        <trd lng="ru">&c_e;&c_v;&c_o;&c_l;&c_mol;&c_v;&c_je;&c_n;&c_t;&c_a; &c_o;&c_k;&c_r;&c_u;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="evolve.0o">evolvento.</ref>
  </snc>
  <bld tip="svg" lok="../bld/k_elvolvanto_de_cirklo.svg" alt="300"
    lrg="300">
    el<tld/>anto de cirklo
  </bld>
  <trd lng="be">&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">Evolvente</trd>
  <trdgrp lng="en">
    <trd>evolvent</trd>,
    <trd>involute</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;veloppante</trd>
  <trd lng="pl">ewolwenta</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_r;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_i;&c_n;&c_v;&c_o;&c_l;&c_ju;&c_t;&c_a;</trd>,
    <trd>&c_e;&c_v;&c_o;&c_l;&c_mol;&c_v;&c_je;&c_n;&c_t;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.el0ato">
  <kap>el<tld/>ato</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
  <snc>
    <dif>
      <klr>(de ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>)</klr>
      La aro de &ccirc;iuj &gcirc;iaj
      <ref cel="centr.kurbeco0o.de_ebena_kurbo">kurbecocentroj</ref>:
      <ekz>
        la el<tld/>ato de <ref cel="cikloi.0o">cikloido</ref> estas mem
        cikloido.
      </ekz>
    </dif>
    <ref tip="sin" cel="evolut.0o">evoluto.</ref>
  </snc>
  <trd lng="be">&c_e;&c_v;&c_a;&c_l;&c_ju;&c_t;&c_a;</trd>
  <trd lng="de">Evolute</trd>
  <trd lng="en">evolute</trd>
  <trd lng="fr">d&eacute;velopp&eacute;e<klr tip="ind"> (d'une courbe)</klr></trd>
  <trd lng="pl">ewoluta</trd>
  <trd lng="ru">&c_e;&c_v;&c_o;&c_l;&c_ju;&c_t;&c_a;</trd>
</drv>

<drv mrk="volv.en0i0ekovri">
  <kap>en<tld/>i, <tld/>ekovri</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tute kovri objekton per io, kiu <tld/>i&gcirc;as &ccirc;irka&ubreve; &gcirc;i:
      <ekz>
        en<tld/>i en tuko la vunditan fingron, beston en reto, sin mem en
        varma mantelo, paka&jcirc;on en paperon<fnt>B</fnt>;
      </ekz>
      <ekz>
        esti en<tld/>ita en, per mantelo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute kovri:
      <ekz>
        la nokto en<tld/>is la teron per nigra vualo;
      </ekz>
      <ekz>
        la ne&gcirc;o kovras la straton en<tld/>itan per la krepusko;
      </ekz>
      <ekz>
        la valo estis en<tld/>ita en blanka nebulo<fnt>B</fnt>;
      </ekz>
      <ekz>
        la nebulo <tld/>ekovranta nin;
      </ekz>
      <ekz>
        ni estis en<tld/>itaj en labirintaj komplika&jcirc;oj;
      </ekz>
      <ekz>
        la internacia vivo en<tld/>as nun la tutan terglobon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hav.en0i">enhavi</ref>,
      <ref cel="ten.en0i">enteni</ref>,
      <ref cel="amplek.0i">ampleksi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_h;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_t;&c_u;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="nl">inwikkelen</trd>
  <trd lng="pt">envolver</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.en0ajxo">
  <kap>en<tld/>a&jcirc;o</kap>
  <snc mrk="volv.en0ajxo.KOMUNE">
    <dif>
      Io, kio en<tld/>as.
    </dif>
    <trd lng="ru">&c_o;&c_b;&c_jo;&c_r;&c_t;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="volv.en0ajxo.de_kurboj">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
    <dif>
      <klr>(de familio de
      ebenaj <ref tip="super" cel="kurb.0o.MAT">kurboj</ref>)</klr>
      Tia ebena kurbo, ke &ccirc;e &ccirc;iu &gcirc;ia punkto tan&gcirc;as
      &gcirc;in iu kurbo de la familio, kaj &ccirc;iu kurbo el la familio
      tan&gcirc;as &gcirc;in en almena&ubreve; unu punkto:
      <ekz>
        la <ref cel="volv.el0ato">el<tld/>ato</ref> de kurbo estas
        la en<tld/>a&jcirc;o de &gcirc;iaj ortantoj.
      </ekz>
    </dif>
    <ref tip="sin" cel="envelo.0o">envelopo.</ref>
    <trd lng="fr">enveloppe<klr tip="ind"> (d'une famille de courbes)</klr></trd>
    <trd lng="pl">obwiednia<klr tip="ind"> (rodziny krzywych)</klr></trd>
    <trd lng="ru">&c_o;&c_g;&c_i;&c_b;&c_a;&c_ju;&c_shch;&c_a;&c_ja;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_a; &c_k;&c_r;&c_i;&c_v;&c_y;&c_h;)</klr></trd>
  </snc>
  <trdgrp lng="de">
    <trd>Einh&uuml;llende</trd>,
    <trd>Umh&uuml;llungskurve</trd>,
    <trd>Envolvente</trd>
  </trdgrp>
  <trd lng="en">envelope</trd>
</drv>

<drv mrk="volv.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kune <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_r;&c_a;&c_z;&c_a;&c_m;)</klr></trd>
  <trd lng="nl">samenwikkelen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_i;&c_t;&c_mol;<klr> (&c_ch;&c_t;&c_o;-&c_l;. &c_v;&c_m;&c_je;&c_s;&c_t;&c_je;)</klr></trd>,
    <trd>&c_s;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Demeti ion <tld/>itan:
      <ekz>
