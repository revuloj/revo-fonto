      <klr>(de ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>)</klr>
      La aro de &ccirc;iuj &gcirc;iaj
      <ref cel="centr.kurbeco0o.de_ebena_kurbo">kurbecocentroj</ref>:
      <ekz>
        la el<tld/>ato de <ref cel="cikloi.0o">cikloido</ref> estas
        mem
        cikloido.
      </ekz>
    </dif>
    <ref tip="sin" cel="evolut.0o">evoluto.</ref>
  </snc>
  <trd lng="be">&c_e;&c_v;&c_a;&c_l;&c_ju;&c_t;&c_a;</trd>
  <trd lng="de">Evolute</trd>
  <trd lng="en">evolute</trd>
  <trd lng="fr">d&eacute;velopp&eacute;e<klr tip="ind"> (d'une
  courbe)</klr></trd>
  <trd lng="hu">evol&uacute;ta</trd>
  <trd lng="pl">ewoluta</trd>
  <trd lng="ru">&c_e;&c_v;&c_o;&c_l;&c_ju;&c_t;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x6E10;&#x5C48;&#x7EBF; <pr>ji&#xE0;nq&#x16B;xi&#xE0;n</pr></trd>,
    <trd>&#x6F38;&#x5C48;&#x7DDA; <pr>ji&#xE0;nq&#x16B;xi&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="volv.en0i">
  <kap>en<tld/>i, <var><kap><tld/>ekovri</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volv.en0i.tute">
    <dif>
      Tute kovri objekton per io, kiu <tld/>i&gcirc;as
      &ccirc;irka&ubreve; &gcirc;i:
      <ekz>
        la malgranda knabo en<tld/>is ion en pecon da papero
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la glavo de <nom>Goljat</nom> <klr>[&#8230;]</klr>, en<tld/>ita en
        vesto
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 21:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili en<tld/>i&gcirc;u en sian honton kiel en veston
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 109:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis tute nuda, kaj tial &scirc;i en<tld/>is sin en siajn densajn
        longajn harojn
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni &scirc;in metis en la nigran &ccirc;erkon, &scirc;i ku&scirc;is en<tld/>ita en
        blanka tola&jcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Avineto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="volv.en0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute kovri:
      <ekz>
        la nokto en<tld/>is la teron per nigra vualo;
      </ekz>
      <ekz>
        tondris la kanonoj, <klr>[&#8230;]</klr> la &scirc;ipoj staris en<tld/>itaj
        en fumo de pulvo
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia tero ku&scirc;is en<tld/>ita en flaveta nebulo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        fre&scirc;a aero en<tld/>is lin
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hav.en0i">enhavi</ref>,
      <ref cel="ten.en0i">enteni</ref>,
      <ref cel="amplek.0i">ampleksi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_h;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_t;&c_u;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einh&uuml;llen</trd>,
    <trd>einwickeln</trd>,
    <trd>umh&uuml;llen</trd>,
    <trd>verbinden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>envelop</trd>,
    <trd>wrap</trd>
  </trdgrp>
  <trd lng="es">envolver</trd>
  <trdgrp lng="fr">
    <trd>entourer</trd>,
    <trd>envelopper</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>beburkol</trd>,
    <trd>bebugyol&aacute;l</trd>
    <trd>betakar</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304F;&#x308B;&#12416;</trd>,
    <trd>&#21253;&#x307F;&#x8FBC;&#12416;  <pr>&#12388;&#12388;&#x307F;&#12371;&#12416;</pr></trd>,
    <trd>&#x5DFB;&#x304D;&#x8FBC;&#12416;  <pr>&#x307E;&#x304D;&#12371;&#12416;</pr></trd>
  </trdgrp>
  <trd lng="nl">inwikkelen</trd>
  <trdgrp lng="pl">
    <trd>otula&cacute;</trd>,
    <trd>owija&cacute;</trd>,
    <trd>zapakowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">envolver</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&#x43C;&#x43E;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_g;&#x43E;&c_r;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_u;&#x43A;&c_u;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_u; &c_shch;&#x43E;-&#x43D;.)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21253;&#x4F4F; <pr>b&amacron;ozh&#xF9;</pr></trd>,
    <trd>&#x7B3C;&#x7F69; <pr>l&#x1D2;ngzh&#xE0;o</pr></trd>,
    <trd>&#x7C60;&#x7F69; <pr>l&#x1D2;ngzh&#xE0;o</pr></trd>,
    <trd>&#28952; <pr>d&#xE0;o</pr></trd>,
    <trd>&#x71FE; <pr>d&#xE0;o</pr></trd>,
    <trd>&#21253;&#x88F9; <pr>b&amacron;ogu&#x1D2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="volv.en0ajxo">
  <kap>en<tld/>a&jcirc;o</kap>
  <snc mrk="volv.en0ajxo.KOMUNE">
    <dif>
      Io, kio en<tld/>as:
      <ekz>
        liberigita el la en<tld/>a&jcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>g&ouml;ngy&ouml;leg</trd>,
      <trd>burkolat</trd>
    </trdgrp>
    <trd lng="pl">otulina</trd>
    <trd lng="ru">&c_o;&c_b;&c_jo;&c_r;&c_t;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="volv.en0ajxo.de_kurboj">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
    <dif>
      <klr>(de familio de
        ebenaj <ref tip="super" cel="kurb.0o.MAT">kurboj</ref>)</klr>
      Tia ebena kurbo, ke &ccirc;e &ccirc;iu &gcirc;ia punkto
      tan&gcirc;as
      &gcirc;in iu kurbo de la familio, kaj &ccirc;iu kurbo el la
      familio
      tan&gcirc;as &gcirc;in en almena&ubreve; unu punkto:
      <ekz>
        la <ref cel="volv.el0ato">el<tld/>ato</ref> de kurbo estas
        la en<tld/>a&jcirc;o de &gcirc;iaj ortantoj.
      </ekz>
    </dif>
    <ref tip="sin" cel="envelo.0o">envelopo</ref>
    <trd lng="fr">enveloppe<klr tip="ind"> (d'une famille de
        courbes)</klr></trd>
    <trd lng="pl">obwiednia<klr tip="ind"> (rodziny
        krzywych)</klr></trd>
    <trd lng="ru">&c_o;&c_g;&c_i;&c_b;&c_a;&c_ju;&c_shch;&c_a;&c_ja;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_a;
        &c_k;&c_r;&c_i;&c_v;&c_y;&c_h;)</klr></trd>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Einh&uuml;llende</trd>,
    <trd>Umh&uuml;llungskurve</trd>,
    <trd>Envolvente</trd>
  </trdgrp>
  <trd lng="en">envelope</trd>
  <trd lng="es">envoltorio</trd>
  <trd lng="hu">burkol&oacute;g&ouml;rbe</trd>
</drv>

<drv mrk="volv.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volv.kun0i.kune">
    <dif>
      Kune <tld/>i:
      <ekz>
        <nom>Elija</nom> prenis sian mantelon kaj kun<tld/>is &gcirc;in
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 2:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun<tld/>inte la libron, kaj redoninte &gcirc;in al la subulo, li
        sidi&gcirc;is
        <fnt>
          <bib>NT</bib>
          <lok>Luko 4:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        la floro kun<tld/>as siajn foliojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i rapide demetis la anta&ubreve;tukon, kun<tld/>is &gcirc;in kaj metis en
        tirkeston
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        globe kun<tld/>ita tuketo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        haroj, tre supra&jcirc;e kombitaj, kun<tld/>itaj sur la malanta&ubreve;o de
        la kapo en formo
        de bulo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj nigraj haroj estis kun<tld/>itaj en pezaj plektoj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;
    <klr>(&c_r;&c_a;&c_z;&c_a;&c_m;)</klr></trd>
  <trdgrp lng="cs">
    <trd>smotat</trd>,
    <trd>zabalit</trd>,
    <trd>zavinout</trd>
  </trdgrp>
  <trd lng="hu">&ouml;sszeg&ouml;ngy&ouml;l</trd>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x7DD2;&#x306B;&#x5DFB;&#x304F;  <pr>&#12356;&#12387;&#12375;&#12423;&#x306B;&#x307E;&#x304F;</pr></trd>,
    <trd>&#x675F;&#x306D;&#x308B;  <pr>&#x305F;&#12400;&#x306D;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">samenwikkelen</trd>
  <trd lng="pl">zwija&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_i;&c_t;&c_mol;<klr> (&c_ch;&c_t;&c_o;-&c_l;.
    &c_v;&c_m;&c_je;&c_s;&c_t;&c_je;)</klr></trd>,
    <trd>&c_s;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sto&ccaron;i&tcaron;</trd>,
    <trd>zvin&uacute;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&#x43C;&#x43E;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&#x43A;&c_r;&c_u;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_d;&#x43E;&#x43A;&c_u;&#x43F;&c_i;)</trd>
  </trdgrp>
</drv>

<drv mrk="volv.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volv.mal0i.KOMUNE">
    <dif>
      Dis<tld/>i:
      <ekz>
        la kripla rozarbeto levis siajn bran&ccirc;ojn, mal<tld/>is siajn
        foliojn kaj aperigis
        plej &ccirc;arman rozon
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> mal<tld/>is la tukon kaj montris la juvelojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fidela Johano</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>abwickeln</trd>,
      <trd>abnehmen</trd>
    </trdgrp>
    <trd lng="en">unroll</trd>
  </snc>
  <snc mrk="volv.mal0i.FIG">
    <uzo tip="stl">RAR</uzo>
    <dif>
      <ref tip="dif" cel="volv.el0i.FIG">El<tld/>i<sncref/></ref>:
      <ekz>
        mal<tld/>i&gcirc;as ege stranga kaj interesa rakonto
        <fnt>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2001/007110.php">Atentokapta sed ru&gcirc;krajoninda</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 31</lok>
          </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro tamen mal<tld/>as sian rakontofadenon ne linie
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2013/011040.php">Toter Lehrer, guter Lehrer</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>rozmotat</trd>,
    <trd>rozmot&aacute;vat</trd>,
    <trd>rozvinout</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;rouler</trd>
  <trdgrp lng="hu">
    <trd>kibont</trd>,
    <trd>kig&ouml;ngy&ouml;l</trd>,
    <trd>lefejt</trd>,
    <trd>leteker</trd>
  </trdgrp>
  <trd lng="nl">afwikkelen</trd>
  <trdgrp lng="pl">
    <trd>rozmota&cacute;</trd>,
    <trd>odwija&cacute;</trd>,
    <trd>odwik&lstroke;a&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odmota&tcaron;</trd>,
    <trd>odvi&tcaron;</trd>,
    <trd>rozmota&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43C;&#x43E;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_g;&#x43E;&c_r;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43A;&c_u;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
