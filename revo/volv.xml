<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>volv</rad>/i
</kap>
<drv mrk="volv.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Plurfoje rondmeti ion flekseblan &ccirc;irka&ubreve; io alia:
      <ekz>
        <tld/>i &scirc;nuron &ccirc;irka&ubreve; bastono, kolskarpon sur sian
        kolon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        banda&gcirc;on &ccirc;irka&ubreve; brako;
      </ekz>
      <ekz>
        vinberbran&ccirc;oj <tld/>is sin &ccirc;irka&ubreve; la kolonoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        hederoj <tld/>i&gcirc;is &ccirc;irka&ubreve; la arboj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>ajo da fadeno, de lupolo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        granda pergamena rul<tld/>a&jcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>umwickeln</trd>,
      <trd>umbinden</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_v;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Rondirigi objekton &ccirc;irka&ubreve; &gcirc;i mem:
      <ekz>
        <tld/>i paperfolion;
      </ekz>
      <ekz>
        <tld/>ita rubando, petalo de rozo;
      </ekz>
      <ekz>
        kun manikoj supren<tld/>itaj &gcirc;is la kubutoj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="de">w&auml;lzen</trd>
    <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="cs">vinout</trd>
  <trd lng="en">roll</trd>
  <trdgrp lng="nl">
    <trd>wikkelen</trd>,
    <trd>oprollen</trd>
  </trdgrp>
</drv>

<drv mrk="volv.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;igi objekton per io, kio <tld/>i&gcirc;as &ccirc;irka&ubreve;e:
      <ekz>
        &ccirc;irka&ubreve;<tld/>i la kolon per rubando<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &scirc;tato &ccirc;irka&ubreve;<tld/>as la modernan individuon per tro multaj
        ligiloj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vind.0i">vindi</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">omwikkelen</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Etendi ion <tld/>itan &ccirc;irka&ubreve; si mem:
      <ekz>
        dis<tld/>i tolrula&jcirc;on, standardon;
      </ekz>
      <ekz>
        plene dis<tld/>ita floro.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plivastigi, pliampleksigi; realigi &ccirc;ion, kio estas farebla:
      <ekz>
        dis<tld/>i &ccirc;iujn siajn kapablojn, viglan propagandon, sian
        programon, la popolan instruadon;
      </ekz>
      <ekz>
        la uzado de Esperanto dis<tld/>as &ccirc;e la infanoj la intereson al
        fremdaj popoloj kaj guston al geografio;
      </ekz>
      <ekz>
        la turka registaro entreprenis dis<tld/>i sian milit&scirc;iparon;
      </ekz>
      <ekz>
        la industrio pli kaj pli dis<tld/>i&gcirc;as;
      </ekz>
      <ekz>
        la programo glate dis<tld/>i&gcirc;is.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="efekti.0igi">efektivigi</ref>,
      <ref cel="evolu.0i">evolui</ref>,
      <ref cel="plen.0umi">plenumi</ref>,
      <ref cel="grand">pligrandigi</ref>,
      <ref cel="fort">plifortigi</ref>,
      <ref cel="akcel.0i">akceli</ref>,
      <ref cel="kresk.0igi">kreskigi</ref>,
      <ref cel="progre.0igi">progresigi</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="nl">
    <trd>ontplooien</trd>,
    <trd>ontwikkelen<klr tip="ind"> (ontplooien)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="volv.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Dis<tld/>i, disfaldi:
      <ekz>
        la bur&gcirc;ono el<tld/>i&gcirc;as;
      </ekz>
      <ekz>
        tiu literaturo rapide el<tld/>i&gcirc;is.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Klare kaj detale prezenti temon por &gcirc;in konigi kaj komprenigi:
      <ekz>
        el<tld/>i argumenton, planon.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="volv.el0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 13</lok></fnt>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      Esprimi ion kiel <ref cel="sum.0o">sumon</ref> de multaj
      <ref cel="term.0o.sumo">termoj</ref>, <ref
      cel="produt.0o">produton</ref>
      de multaj <ref cel="faktor.0o.multip">faktoroj</ref>, ofte e&ccirc; kiel
      <ref cel="seri.0o.MAT">serion</ref> a&ubreve; nefinian produton:
      <ekz>
        el<tld/>i funkcion en potencoserion, la&ubreve; potencoj de la
        variablo;
      </ekz>
      <ekz>
        <ind>el<tld/>a&jcirc;o</ind> de
        <ref cel="ekspon1.0o">eksponencialo</ref> &gcirc;is la dua ordo estas
        1+x+x<sup>2</sup>/2.
        <trd lng="de">Entwicklung</trd>
        <trd lng="en">expansion</trd>
        <trd lng="fr">d&eacute;veloppement<klr tip="ind"> (math.)</klr></trd>
        <trd lng="ru">&c_r;&c_a;&c_z;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">ontvouwen</trd>
</drv>

<drv mrk="volv.en0i0ekovri">
  <kap>en<tld/>i, <tld/>ekovri</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tute kovri objekton per io, kiu <tld/>i&gcirc;as &ccirc;irka&ubreve; &gcirc;i:
      <ekz>
        en<tld/>i en tuko la vunditan fingron, beston en reto, sin mem en
        varma mantelo, paka&jcirc;on en paperon<fnt>B</fnt>;
      </ekz>
      <ekz>
        esti en<tld/>ita en, per mantelo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute kovri:
      <ekz>
        la nokto en<tld/>is la teron per nigra vualo;
      </ekz>
      <ekz>
        la ne&gcirc;o kovras la straton en<tld/>itan per la krepusko;
      </ekz>
      <ekz>
        la valo estis en<tld/>ita en blanka nebulo<fnt>B</fnt>;
      </ekz>
      <ekz>
        la nebulo <tld/>ekovranta nin;
      </ekz>
      <ekz>
        ni estis en<tld/>itaj en labirintaj komplika&jcirc;oj;
      </ekz>
      <ekz>
        la internacia vivo en<tld/>as nun la tutan terglobon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hav.en0i">enhavi</ref>,
      <ref cel="ten.en0i">enteni</ref>,
      <ref cel="amplek.0i">ampleksi</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">inwikkelen</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kune <tld/>i.
    </dif>
  </snc>
  <trd lng="nl">samenwikkelen</trd>
</drv>

<drv mrk="volv.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Demeti ion <tld/>itan:
      <ekz>
        mal<tld/>i tukon de la kapo<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      El<tld/>i 2.
    </dif>
  </snc>
  <trd lng="nl">afwikkelen</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
