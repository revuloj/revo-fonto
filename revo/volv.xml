  </trdgrp>
  <trd lng="uk">&c_r;&#x43E;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x62D3; <pr>tu&#xF2;</pr></trd>,
    <trd>&#x884D; <pr>y&#x1CE;n</pr></trd>,
    <trd>&#x5F00;&#x5C55; <pr>k&amacron;izh&#x1CE;n</pr></trd>,
    <trd>&#x958B;&#x5C55; <pr>k&amacron;izh&#x1CE;n</pr></trd>
