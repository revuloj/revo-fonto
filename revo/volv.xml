<drv mrk="volv.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volv.cxirkaux0i.KOMUNE">
    <dif>
      &Ccirc;irka&ubreve;igi objekton per io, kio <tld/>i&gcirc;as
      &ccirc;irka&ubreve;e:
      <ekz>
        sian bierkru&ccirc;on li &ccirc;irka&ubreve;<tld/>is per brune-verdaj
        <tld/>otrunkoj de lupolo
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kama ku&scirc;i&gcirc;is sur la lito kaj
        &ccirc;irka&ubreve;<tld/>is
        la kapon per tuko
        <fnt><bib>Far2</bib><lok>&ccirc;ap. 16</lok></fnt>;
      </ekz><ekz>
        &ccirc;ion, kion <klr>[la polipoj]</klr> povis en la maro
        kapti,
        ili &ccirc;irka&ubreve;<tld/>adis fortike, por jam neniam
        &gcirc;in ellasi
        <fnt><bib>Fab1</bib><lok>La virineto de maro</lok></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        la &scirc;tato &ccirc;irka&ubreve;<tld/>as la modernan
        individuon per tro multaj ligiloj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vind.0i">vindi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_o;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>umwickeln</trd>,
    <trd>umklammern</trd>,
    <trd>umfangen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enlacer</trd>,
    <trd>envelopper</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;rbecsavar</trd>,
    <trd>k&ouml;rbeteker</trd>,
    <trd>k&ouml;rbefon</trd>
  </trdgrp>
  <trd lng="nl">omwikkelen</trd>
  <trdgrp lng="pl">
    <trd>zawija&cacute;</trd>,
    <trd>nawija&cacute;</trd>,
    <trd>p&eogonek;ta&cacute;</trd>,
    <trd>omota&cacute;</trd>
  </trdgrp>
  <trd lng="pt">enrolar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21253;&#x4F4F; <pr>b&amacron;ozh&#xF9;</pr></trd>,
    <trd>&#21253;&#x88F9; <pr>b&amacron;ogu&#x1D2;</pr></trd>,
    <trd>&#25171;&#21253; <pr>d&#x1CE;b&amacron;o</pr></trd>,
    <trd>&#28952; <pr>d&#xE0;o</pr></trd>,
    <trd>&#x71FE; <pr>d&#xE0;o</pr></trd>,
    <trd>&#x7B3C;&#x7F69; <pr>l&#x1D2;ngzh&#xE0;o</pr></trd>,
    <trd>&#x7C60;&#x7F69; <pr>l&#x1D2;ngzh&#xE0;o</pr></trd>
  </trdgrp>
</drv>
<drv mrk="volv.dis0igxi">
  <kap>
    dis<tld/>i&gcirc;i
  </kap>
  <snc mrk="volv.dis0igxi.etendigxi">
    <dif>
      <ref tip="super" cel="etend.0igxi">Etendi&gcirc;i</ref> de volvita al
      malvolvita stato:
      <ekz>
        dis<tld/>i&gcirc;inta lilio
        <fnt>
          <bib>MT</bib>
          <lok>I. re&gcirc;oj 7:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgrandaj blankaj bur&gcirc;onoj dis<tld/>i&gcirc;is en florojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Elfo de la rozo</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite dis<tld/>i&gcirc;is &gcirc;iaj flugiloj, ili movi&gcirc;is pli forte ol anta&ubreve;e kaj portis
        &gcirc;in vigle anta&ubreve;en
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        la raketoj flugis al la &ccirc;ielo, dis<tld/>i&gcirc;is kvaza&ubreve; floroj kaj falis teren kiel
        pluvo
        <fnt>
          <bib>IKr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="burgxo.0i">bur&gcirc;oni</ref>,
      <ref cel="flor.0i.disvolvigxi">flori<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="volv.0i.evolui">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="evolu.0i">Evolui</ref>:
      <ekz>
        la pasio fumi tabakon dis<tld/>i&gcirc;is &ccirc;iam pli multe
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        potence dis<tld/>i&gcirc;anta, vigla urbo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malgranda Tuk</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun &ccirc;iu monato la infano pli dis<tld/>i&gcirc;is, kaj en la da&ubreve;ro de kelke da jaroj
        <klr>[&#8230;]</klr> fari&gcirc;is granda knabino
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        supre, kie &ccirc;io bela dis<tld/>i&gcirc;as kaj alti&gcirc;as, &scirc;ia animo eble eksonos per tiel
        plena forto, ke &gcirc;i supersonos la mian
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tia]</klr> lingvo restos &ccirc;iam rigida kaj neniam dis<tld/>i&gcirc;os
        <fnt>
          <bib>F</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="progre.0i">progresi</ref>,
      <ref cel="prospe1.0i">prosperi</ref>
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </snc>
  <trd lng="de">sich <ind>entwickeln</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x5E83;&#x304C;&#x308B;  <pr>&#12402;&#x308D;&#x304C;&#x308B;</pr></trd>,
    <trd>&#x5C55;&#x958B;&#12377;&#x308B;  <pr>&#12390;&#12435;&#x304B;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x958B;&#x82B1;&#12377;&#x308B;  <pr>&#x304B;&#12356;&#x304B;&#12377;&#x308B;</pr></trd>,
    <trd>&#x767A;&#x5C55;&#12377;&#x308B;  <pr>&#x306F;&#12387;&#12390;&#12435;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozwija&cacute; si&eogonek;</trd>,
    <trd>wywija&cacute; si&eogonek;</trd>,
    <trd>ewoluowa&cacute;</trd>
