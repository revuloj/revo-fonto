<drv mrk="volv.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volv.0i.plurfoje">
    <dif>
      Plurfoje rondmeti ion flekseblan &ccirc;irka&ubreve; io alia:
      <ekz>
        vinberbran&ccirc;oj <tld/>is sin &ccirc;irka&ubreve; altaj kolonoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        densa vinko <tld/>i&gcirc;as &ccirc;irka&ubreve; la malnovaj ru&gcirc;aj muroj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la bariloj <tld/>i&gcirc;adis supren sova&gcirc;a lupolo kaj florantaj
        konvolvuloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>otrunkoj de rubuso <tld/>i&gcirc;is super la oferaltaro
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is tabakon en strion da papero kaj fumis tiamaniere
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        li trovis sova&gcirc;an <tld/>okreska&jcirc;on kaj kolektis de &gcirc;i
        kolocintojn
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 4:39</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        karbonigra nokto <tld/>is sin &ccirc;irka&ubreve; kampoj
        <fnt>
          <bib>Tor</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_m;&c_o;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>umwickeln</trd>,
      <trd>umbinden</trd>,
      <trd>einwickeln</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>becsavar</trd>,
      <trd>beg&ouml;ngy&ouml;l</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_v;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="volv.0i.rondirigi">
    <dif>
      Rondirigi objekton &ccirc;irka&ubreve; &gcirc;i mem:
      <ekz>
        <klr>[la]</klr> skribisto <tld/>is la forpu&scirc;itan dokumenton kaj prezentis alian
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is la tuber&scirc;nuron kaj metis la ringegon tiel formitan &ccirc;irka&ubreve; sian
        bruston
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        nigraj bukloj <tld/>i&gcirc;is sur la klinita nuko
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz><ekz>
        kun manikoj supren<tld/>itaj &gcirc;is la
        kubutoj
        <fnt>
          <aut>Eliza Orzeszko, trad. Karl Bein</aut>
          <vrk><url
          ref="http://i-espero.info/files/elibroj/eo%20-%20orzeszko,%20eliza%20-%20bona%20sinjorino.pdf">Bona
          sinjorino</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        grandaj grasaj maraj serpentoj sin <tld/>adis kaj montradis sian
        abomenan blankflavan ventron
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ita kuko
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2005/008488.php">Kuk-urbo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="etend.0i">etendi</ref>
    <refgrp tip="vid">
      <ref cel="rul.0i.KOMUNE">ruli</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">w&auml;lzen</trd>
    <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>motat</trd>,
    <trd>navinout</trd>,
    <trd>nav&iacute;jet</trd>,
    <trd>srolovat</trd>,
    <trd>sto&ccaron;it</trd>,
    <trd>vinout</trd>,
    <trd>v&iacute;t</trd>,
    <trd>zato&ccaron;it</trd>,
    <trd>zavinout</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wickeln</trd>,
    <trd>binden</trd>,
    <trd>umkrempeln</trd>,
    <trd>umschlagen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>roll</trd>,
    <trd>wrap</trd>,
    <trd>wind</trd>
  </trdgrp>
  <trd lng="es">enrollar</trd>
  <trd lng="fr">enrouler</trd>
  <trdgrp lng="hu">
    <trd>felcsavar</trd>,
    <trd>felteker</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5DFB;&#x304F;  <pr>&#x307E;&#x304F;</pr></trd>,
    <trd>&#x5DFB;&#x304D;&#12388;&#12369;&#x308B;  <pr>&#x307E;&#x304D;&#12388;&#12369;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wikkelen</trd>,
    <trd>oprollen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wi&cacute;</trd>,
    <trd>zwija&cacute;</trd>,
    <trd>nawija&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>enrolar</trd>,
    <trd>envolver</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mota&tcaron;</trd>,
    <trd>nav&iacute;ja&tcaron;</trd>,
    <trd>sto&ccaron;i&tcaron;</trd>,
    <trd>zavin&uacute;&tcaron;</trd>,
    <trd>zrolova&tcaron;</trd>,
    <trd>zvin&uacute;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43E;&c_b;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43E;&c_b;&#x43C;&#x43E;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_g;&#x43E;&c_r;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_g;&#x43E;&c_r;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&#x43A;&c_r;&c_u;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25171;&#x6EDA; <pr>d&#x1CE;g&#x1D4;n</pr></trd>,
    <trd>&#25171;&#x6EFE; <pr>d&#x1CE;g&#x1D4;n</pr></trd>,
