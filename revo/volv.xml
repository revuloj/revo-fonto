  <trd lng="fr">bobine</trd>
  <trd lng="ja">&#x5DFB;&#29289;  <pr>&#x307E;&#x304D;&#12418;&#x306E;</pr></trd>
  <trd lng="pl">zw&oacute;j pisma</trd>
  <trdgrp lng="uk">
