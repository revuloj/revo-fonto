<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: volv.xml,v 1.44 2020/02/22 20:10:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>volv</rad>/i
</kap>

<drv mrk="volv.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volv.0i.plurfoje">
    <dif>
      Plurfoje rondmeti ion flekseblan &ccirc;irka&ubreve; io alia:
      <ekz>
        vinberbran&ccirc;oj <tld/>is sin &ccirc;irka&ubreve; altaj kolonoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        densa vinko <tld/>i&gcirc;as &ccirc;irka&ubreve; la malnovaj ru&gcirc;aj muroj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la bariloj <tld/>i&gcirc;adis supren sova&gcirc;a lupolo kaj florantaj
        konvolvuloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>otrunkoj de rubuso <tld/>i&gcirc;is super la oferaltaro
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is tabakon en strion da papero kaj fumis tiamaniere
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        li trovis sova&gcirc;an <tld/>okreska&jcirc;on kaj kolektis de &gcirc;i
        kolocintojn
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 4:39</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_m;&c_o;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>umwickeln</trd>,
      <trd>umbinden</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>becsavar</trd>,
      <trd>beg&ouml;ngy&ouml;l</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_v;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="volv.0i.rondirigi">
    <dif>
      Rondirigi objekton &ccirc;irka&ubreve; &gcirc;i mem:
      <ekz>
        <tld/>i paperfolion;
      </ekz><ekz>
        <tld/>ita rubando, petalo de rozo;
      </ekz>
      <ekz>
        nigraj bukloj <tld/>i&gcirc;is sur la klinita nuko
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz><ekz>
        kun manikoj supren<tld/>itaj &gcirc;is la
        kubutoj
        <fnt>
          <aut>Eliza Orzeszko, trad. Karl Bein</aut>
          <vrk><url
          ref="http://i-espero.info/files/elibroj/eo%20-%20orzeszko,%20eliza%20-%20bona%20sinjorino.pdf"
              >Bona sinjorino</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        grandaj grasaj maraj serpentoj sin <tld/>adis kaj montradis sian
        abomenan
        blankflavan ventron
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">w&auml;lzen</trd>
    <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>motat</trd>,
    <trd>navinout</trd>,
    <trd>nav&iacute;jet</trd>,
    <trd>srolovat</trd>,
    <trd>sto&ccaron;it</trd>,
    <trd>vinout</trd>,
    <trd>v&iacute;t</trd>,
    <trd>zato&ccaron;it</trd>,
    <trd>zavinout</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wickeln</trd>,
    <trd>binden</trd>,
    <trd>umkrempeln</trd>,
    <trd>umschlagen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>roll</trd>,
    <trd>wrap</trd>,
    <trd>wind</trd>
  </trdgrp>
  <trd lng="es">enrollar</trd>
  <trd lng="fr">enrouler</trd>
  <trdgrp lng="hu">
    <trd>felcsavar</trd>,
    <trd>felteker</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>wikkelen</trd>,
    <trd>oprollen</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>enrolar</trd>,
    <trd>envolver</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mota&tcaron;</trd>,
    <trd>nav&iacute;ja&tcaron;</trd>,
    <trd>sto&ccaron;i&tcaron;</trd>,
    <trd>zavin&uacute;&tcaron;</trd>,
    <trd>zrolova&tcaron;</trd>,
    <trd>zvin&uacute;&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="volv.0ajxo.io">
    <dif>
      Io volvita:
      <ekz>
        <tld/>a&jcirc;o da fadeno;
      </ekz>
      <ekz>
        trikiloj kaj <tld/>a&jcirc;o
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;oj &ccirc;e lupolo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;oj de hedero
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj pensoj estis kiel interkonfuzita <tld/>a&jcirc;o, nur unu fadeno
        tiri&gcirc;is tra &ccirc;iuj
        &scirc;iaj pensoj kaj sentoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkdeko da papirusaj <tld/>a&jcirc;oj, kovritaj de amba&ubreve; flankoj per
        skriba&jcirc;oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Rolle</trd>,
    <trd>Wickel</trd>,
    <trd>Spule</trd>,
    <trd>Ranke</trd>
  </trdgrp>
  <trd lng="en">wrap</trd>
  <trd lng="es">rollo</trd>
  <trd lng="fr">enroulement</trd>
  <trd lng="ru">&c_s;&c_v;&c_jo;&c_r;&c_t;&c_o;&c_k;</trd>
</drv>

<drv mrk="volv.rul0ajxo">
  <kap>rul<tld/>a&jcirc;o</kap>
  <snc mrk="volv.rul0ajxo.rulajxo">
    <dif>
      <ref tip="dif" cel="rul.0ajxo">Rula&jcirc;o</ref>:
      <ekz>
        granda pergamena rul<tld/>a&jcirc;o
        <fnt>
          <aut>Henri Heine, trad. &Z;</aut>
          <vrk><url
          ref="https://tekstaro.com/t?nomo=la-rabeno-de-bahharahh"
              >La Rabeno de Ba&hcirc;ara&hcirc;</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="es">
    <trd>madeja</trd>,
    <trd>bobina</trd>,
    <trd>carrete</trd>
  </trdgrp>
  <trd lng="de">Rolle</trd>
  <trd lng="fr">bobine</trd>
</drv>

<drv mrk="volv.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volv.cxirkaux0i.KOMUNE">
    <dif>
      &Ccirc;irka&ubreve;igi objekton per io, kio <tld/>i&gcirc;as
      &ccirc;irka&ubreve;e:
      <ekz>
        sian bierkru&ccirc;on li &ccirc;irka&ubreve;<tld/>is per brune-verdaj
        <tld/>otrunkoj de lupolo
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kama ku&scirc;i&gcirc;is sur la lito kaj
        &ccirc;irka&ubreve;<tld/>is
        la kapon per tuko
        <fnt><bib>Far2</bib><lok>&ccirc;ap. 16</lok></fnt>;
      </ekz><ekz>
        &ccirc;ion, kion <klr>[la polipoj]</klr> povis en la maro
        kapti,
        ili &ccirc;irka&ubreve;<tld/>adis fortike, por jam neniam
        &gcirc;in ellasi
        <fnt><bib>Fab1</bib><lok>La virineto de maro</lok></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        la &scirc;tato &ccirc;irka&ubreve;<tld/>as la modernan
        individuon per tro multaj ligiloj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vind.0i">vindi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_o;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>umwickeln</trd>,
    <trd>umklammern</trd>,
    <trd>umfangen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enlacer</trd>,
    <trd>envelopper</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;rbecsavar</trd>,
    <trd>k&ouml;rbeteker</trd>,
    <trd>k&ouml;rbefon</trd>
  </trdgrp>
  <trd lng="nl">omwikkelen</trd>
  <trd lng="pt">enrolar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volv.dis0i.etendi">
    <dif>
      Etendi ion <tld/>itan &ccirc;irka&ubreve; si mem:
      <ekz>
        dis<tld/>i tolrula&jcirc;on, standardon;
      </ekz>
      <ekz>
        dis<tld/>i&gcirc;inta lilio
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        dis<tld/>i&gcirc;inta floro
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen mano estas etendita al mi, kaj en &gcirc;i estas skribrula&jcirc;o kaj
        li dis<tld/>is
        &gcirc;in anta&ubreve; mi, kaj &gcirc;i estis skribkovrita interne kaj ekstere
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 2:10</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="es">desenrollar</trd>
    <trdgrp lng="hu">
      <trd>kitereget</trd>,
      <trd>kibont</trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="volv.dis0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Plivastigi, pliampleksigi; realigi &ccirc;ion, kio estas
      farebla:
      <ekz>
        dis<tld/>i &ccirc;iujn siajn kapablojn, viglan propagandon,
        sian programon, la popolan instruadon;
      </ekz><ekz>
        la uzado de Esperanto dis<tld/>as &ccirc;e la infanoj la
        intereson al fremdaj popoloj kaj guston al geografio;
      </ekz>
      <ekz>
        konservi por la interna ideo de la Esperantismo la eblon tute
        libere dis<tld/>i&gcirc;i
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en
          la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la malgranda pizo dis<tld/>i&gcirc;as bonege
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>;
      </ekz><ekz>
        la industrio pli kaj pli dis<tld/>i&gcirc;as;
      </ekz><ekz>
        la programo glate dis<tld/>i&gcirc;is.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="efekti.0igi">efektivigi</ref>,
      <ref cel="evolu.0i">evolui</ref>,
      <ref cel="plen.0umi">plenumi</ref>,
      <ref cel="grand">pligrandigi</ref>,
      <ref cel="fort">plifortigi</ref>,
      <ref cel="akcel.0i">akceli</ref>,
      <ref cel="kresk.0igi">kreskigi</ref>,
      <ref cel="progre.0igi">progresigi</ref>.
    </refgrp>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="hu">
      <trd>kibontakoztat</trd>,
      <trd>fejleszt</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>rozkulit</trd>,
    <trd>rozmotat</trd>,
    <trd>rozmot&aacute;vat</trd>,
    <trd>rozvinout</trd>,
    <trd>rozv&iacute;jet</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>entwickeln</trd>,
    <trd>auswickeln</trd>,
    <trd>entfalten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>unwind</trd>,
    <trd>unfold</trd>,
    <trd>to develop</trd>
  </trdgrp>
  <trd lng="es">desarrollar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;rouler</trd>,
    <trd>d&eacute;velopper</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ontplooien</trd>,
    <trd>ontwikkelen<klr tip="ind"> (ontplooien)</klr></trd>
  </trdgrp>
  <trd lng="pt">desenrolar</trd>
  <trdgrp lng="sk">
    <trd>odmota&tcaron;</trd>,
    <trd>odvi&tcaron;</trd>,
    <trd>rozmota&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volv.el0i.KOMUNE">
    <dif>
      Dis<tld/>i, disfaldi:
      <ekz>
        oni povis vidi blankru&gcirc;an pizan flora&jcirc;on, plene el<tld/>i&gcirc;intan
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>;
      </ekz>
      <ekz>
        el &ccirc;i tiu penso el<tld/>i&gcirc;is penso alia
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        lernantoj plene el<tld/>is siajn talentojn
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="https://www.monato.be/2009/009886.php"
              >Ekzameno ekzamenata</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">entfalten</trd>
    <trdgrp lng="hu">
      <trd>kibont</trd>,
      <trd>kiteker</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="volv.el0i.FIG">
    <dif>
      Klare kaj detale prezenti temon
      por &gcirc;in konigi kaj komprenigi:
      <ekz>
        el<tld/>i argumenton, planon.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>entwickeln</trd>,
      <trd>entfalten</trd>
    </trdgrp>
    <trd lng="hu">kifejt</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="volv.el0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 13</lok></fnt>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      Esprimi ion kiel <ref cel="sum.0o">sumon</ref> de multaj
      <ref cel="term.0o.sumo">termoj</ref>, <ref
      cel="produt.0o">produton</ref>
      de multaj <ref cel="faktor.0o.multip">faktoroj</ref>, ofte
      e&ccirc;
      kiel
      <ref cel="seri.0o.MAT">serion</ref> a&ubreve; nefinian
      produton:
      <ekz>
        el<tld/>i funkcion en potencoserion, la&ubreve; potencoj de
        la
        variablo;
      </ekz><ekz>
        <ind>el<tld/>a&jcirc;o</ind> de
        <ref cel="ekspon1.0o">eksponencialo</ref> &gcirc;is la dua
        ordo
        estas
        <frm am="1+x+x^2//2">1+<k>x</k>+<k>x</k><sup>2</sup>/2</frm>.
        <trd lng="de">Entwicklung</trd>
        <trd lng="en">expansion</trd>
        <trd lng="fr">d&eacute;veloppement<klr tip="ind">
        (math.)</klr></trd>
        <trd lng="hu">kifejt&eacute;s</trd>
        <trd lng="pl">rozwini&eogonek;cie</trd>
        <trd lng="ru">&c_r;&c_a;&c_z;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>entwickeln</trd>,
      <trd>ausmultiplizieren</trd>
    </trdgrp>
    <trd lng="en">expand</trd>
    <trd lng="fr">d&eacute;velopper</trd>
    <trd lng="hu">kifejt</trd>
    <trd lng="pl">rozwija&cacute;</trd>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>rozvinout</trd>,
    <trd>rozv&eacute;st <klr>(t&eacute;ma, my&scaron;lenku)</klr></trd>
  </trdgrp>
  <trd lng="fr">d&eacute;velopper</trd>
  <trd lng="nl">ontvouwen</trd>
  <trd lng="pt">desenvolver</trd>
  <trdgrp lng="sk">
    <trd>rozanalyzova&tcaron;</trd>,
    <trd>rozobra&tcaron;</trd>,
    <trd>rozvin&uacute;&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.el0anto">
  <kap>el<tld/>anto</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
  <snc mrk="volv.el0anto.MAT">
    <dif>
      <klr>(de ebena <ref tip="super"
      cel="kurb.0o.MAT">kurbo</ref>)</klr>
      La kurbo, kies <ref cel="volv.el0ato">el<tld/>ato</ref>
      &gcirc;i
      estas:
      <ekz>
        <ind>el<tld/>anto de cirklo</ind>.
        <trd lng="de">Kreisevolvente</trd>
        <trd lng="en">evolvent of circle</trd>
        <trd lng="fr">d&eacute;veloppante du cercle</trd>
        <trd lng="hu">k&ouml;r <ind>evolvense</ind></trd>
        <trd lng="pl">ewolwenta okr&eogonek;gu</trd>
        <trd lng="ru">&c_e;&c_v;&c_o;&c_l;&c_mol;&c_v;&c_je;&c_n;&c_t;&c_a;
        &c_o;&c_k;&c_r;&c_u;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="evolve.0o">evolvento.</ref>
  </snc>
  <bld lok="../bld/k_elvolvanto_de_cirklo.svg" tip="svg" alt="400" lrg="400">
    el<tld/>anto de cirklo
  </bld>
  <trd lng="be">&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">Evolvente</trd>
  <trdgrp lng="en">
    <trd>evolvent</trd>,
    <trd>involute</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;veloppante</trd>
  <trd lng="hu">evolvens</trd>  
  <trd lng="pl">ewolwenta</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_r;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_i;&c_n;&c_v;&c_o;&c_l;&c_ju;&c_t;&c_a;</trd>,
    <trd>&c_e;&c_v;&c_o;&c_l;&c_mol;&c_v;&c_je;&c_n;&c_t;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.el0ato">
  <kap>el<tld/>ato</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
  <snc mrk="volv.el0ato.MAT">
    <dif>
      <klr>(de ebena <ref tip="super"
      cel="kurb.0o.MAT">kurbo</ref>)</klr>
      La aro de &ccirc;iuj &gcirc;iaj
      <ref cel="centr.kurbeco0o.de_ebena_kurbo">kurbecocentroj</ref>:
      <ekz>
        la el<tld/>ato de <ref cel="cikloi.0o">cikloido</ref> estas
        mem
        cikloido.
      </ekz>
    </dif>
    <ref tip="sin" cel="evolut.0o">evoluto.</ref>
  </snc>
  <trd lng="be">&c_e;&c_v;&c_a;&c_l;&c_ju;&c_t;&c_a;</trd>
  <trd lng="de">Evolute</trd>
  <trd lng="en">evolute</trd>
  <trd lng="fr">d&eacute;velopp&eacute;e<klr tip="ind"> (d'une
  courbe)</klr></trd>
  <trd lng="hu">evol&uacute;ta</trd>  
  <trd lng="pl">ewoluta</trd>
  <trd lng="ru">&c_e;&c_v;&c_o;&c_l;&c_ju;&c_t;&c_a;</trd>
</drv>

<drv mrk="volv.en0i">
  <kap>en<tld/>i, <var><kap><tld/>ekovri</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volv.en0i.tute">
    <dif>
      Tute kovri objekton per io, kiu <tld/>i&gcirc;as
      &ccirc;irka&ubreve; &gcirc;i:
      <ekz>
        la malgranda knabo en<tld/>is ion en pecon da papero
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la glavo de <nom>Goljat</nom> <klr>[&#8230;]</klr>, en<tld/>ita en
        vesto
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 21:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili en<tld/>i&gcirc;u en sian honton kiel en veston
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 109:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis tute nuda, kaj tial &scirc;i en<tld/>is sin en siajn densajn
        longajn harojn
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni &scirc;in metis en la nigran &ccirc;erkon, &scirc;i ku&scirc;is en<tld/>ita en
        blanka tola&jcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Avineto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="volv.en0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute kovri:
      <ekz>
        la nokto en<tld/>is la teron per nigra vualo;
      </ekz>
      <ekz>
        tondris la kanonoj, <klr>[&#8230;]</klr> la &scirc;ipoj staris en<tld/>itaj
        en fumo de pulvo
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia tero ku&scirc;is en<tld/>ita en flaveta nebulo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        fre&scirc;a aero en<tld/>is lin
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hav.en0i">enhavi</ref>,
      <ref cel="ten.en0i">enteni</ref>,
      <ref cel="amplek.0i">ampleksi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_h;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_t;&c_u;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>einh&uuml;llen</trd>,
    <trd>einwickeln</trd>,
    <trd>umh&uuml;llen</trd>,
    <trd>verbinden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>envelop</trd>,
    <trd>wrap</trd>
  </trdgrp>
  <trd lng="es">envolver</trd>
  <trdgrp lng="fr">
    <trd>entourer</trd>,
    <trd>envelopper</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>beburkol</trd>,
    <trd>bebugyol&aacute;l</trd>
    <trd>betakar</trd>
  </trdgrp>
  <trd lng="nl">inwikkelen</trd>
  <trd lng="pt">envolver</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.en0ajxo">
  <kap>en<tld/>a&jcirc;o</kap>
  <snc mrk="volv.en0ajxo.KOMUNE">
    <dif>
      Io, kio en<tld/>as:
      <ekz>
        liberigita el la en<tld/>a&jcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>g&ouml;ngy&ouml;leg</trd>,
      <trd>burkolat</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_b;&c_jo;&c_r;&c_t;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="volv.en0ajxo.de_kurboj">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
    <dif>
      <klr>(de familio de
        ebenaj <ref tip="super" cel="kurb.0o.MAT">kurboj</ref>)</klr>
      Tia ebena kurbo, ke &ccirc;e &ccirc;iu &gcirc;ia punkto
      tan&gcirc;as
      &gcirc;in iu kurbo de la familio, kaj &ccirc;iu kurbo el la
      familio
      tan&gcirc;as &gcirc;in en almena&ubreve; unu punkto:
      <ekz>
        la <ref cel="volv.el0ato">el<tld/>ato</ref> de kurbo estas
        la en<tld/>a&jcirc;o de &gcirc;iaj ortantoj.
      </ekz>
    </dif>
    <ref tip="sin" cel="envelo.0o">envelopo</ref>
    <trd lng="fr">enveloppe<klr tip="ind"> (d'une famille de
        courbes)</klr></trd>
    <trd lng="pl">obwiednia<klr tip="ind"> (rodziny
        krzywych)</klr></trd>
    <trd lng="ru">&c_o;&c_g;&c_i;&c_b;&c_a;&c_ju;&c_shch;&c_a;&c_ja;<klr tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_a;
        &c_k;&c_r;&c_i;&c_v;&c_y;&c_h;)</klr></trd>
  </snc>
  <trdgrp lng="de">
    <trd>Einh&uuml;llende</trd>,
    <trd>Umh&uuml;llungskurve</trd>,
    <trd>Envolvente</trd>
  </trdgrp>
  <trd lng="en">envelope</trd>
  <trd lng="es">envoltorio</trd>
  <trd lng="hu">burkol&oacute;g&ouml;rbe</trd>  
</drv>

<drv mrk="volv.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volv.kun0i.kune">
    <dif>
      Kune <tld/>i:
      <ekz>
        <nom>Elija</nom> prenis sian mantelon kaj kun<tld/>is &gcirc;in
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 2:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun<tld/>inte la libron, kaj redoninte &gcirc;in al la subulo, li
        sidi&gcirc;is
        <fnt>
          <bib>NT</bib>
          <lok>Luko 4:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        la floro kun<tld/>as siajn foliojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i rapide demetis la anta&ubreve;tukon, kun<tld/>is &gcirc;in kaj metis en
        tirkeston
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        globe kun<tld/>ita tuketo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        haroj, tre supra&jcirc;e kombitaj, kun<tld/>itaj sur la malanta&ubreve;o de
        la kapo en formo
        de bulo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj nigraj haroj estis kun<tld/>itaj en pezaj plektoj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;
    <klr>(&c_r;&c_a;&c_z;&c_a;&c_m;)</klr></trd>
  <trdgrp lng="cs">
    <trd>smotat</trd>,
    <trd>zabalit</trd>,
    <trd>zavinout</trd>
  </trdgrp>
  <trd lng="hu">&ouml;sszeg&ouml;ngy&ouml;l</trd>
  <trd lng="nl">samenwikkelen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_i;&c_t;&c_mol;<klr> (&c_ch;&c_t;&c_o;-&c_l;.
    &c_v;&c_m;&c_je;&c_s;&c_t;&c_je;)</klr></trd>,
    <trd>&c_s;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>sto&ccaron;i&tcaron;</trd>,
    <trd>zvin&uacute;&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="volv.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="volv.mal0i.KOMUNE">
    <dif>
      Dis<tld/>i:
      <ekz>
        la kripla rozarbeto levis siajn bran&ccirc;ojn, mal<tld/>is siajn
        foliojn kaj aperigis
        plej &ccirc;arman rozon
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> mal<tld/>is la tukon kaj montris la juvelojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fidela Johano</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_k;&c_r;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>abwickeln</trd>,
      <trd>abnehmen</trd>
    </trdgrp>
    <trd lng="en">unroll</trd>
  </snc>
  <snc mrk="volv.mal0i.FIG">
    <uzo tip="stl">RAR</uzo>
    <dif>
      <ref tip="dif" cel="volv.el0i.FIG">El<tld/>i<sncref/></ref>:
      <ekz>
        mal<tld/>i&gcirc;as ege stranga kaj interesa rakonto
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007110.html"
              >Atentokapta sed ru&gcirc;krajoninda</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toro tamen mal<tld/>as sian rakontofadenon ne linie
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="https://www.monato.be/2013/011040.php"
              >Toter Lehrer, guter Lehrer</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>rozmotat</trd>,
    <trd>rozmot&aacute;vat</trd>,
    <trd>rozvinout</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;rouler</trd>
  <trdgrp lng="hu">
    <trd>kibont</trd>,
    <trd>kig&ouml;ngy&ouml;l</trd>,
    <trd>lefejt</trd>,
    <trd>leteker</trd>
  </trdgrp>
  <trd lng="nl">afwikkelen</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_o;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odmota&tcaron;</trd>,
    <trd>odvi&tcaron;</trd>,
    <trd>rozmota&tcaron;</trd>
  </trdgrp>
</drv>
