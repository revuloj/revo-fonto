<drv mrk="volv.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="volv.0ajxo.io">
    <dif>
      Io volvita:
      <ekz>
        <tld/>a&jcirc;o da fadeno;
      </ekz>
      <ekz>
        trikiloj kaj <tld/>a&jcirc;o
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;oj &ccirc;e lupolo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;o de la pizo
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&jcirc;oj de hedero
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ne&gcirc;o enpenetris en &ccirc;iujn <tld/>a&jcirc;ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj pensoj estis kiel interkonfuzita <tld/>a&jcirc;o, nur unu fadeno
        tiri&gcirc;is tra &ccirc;iuj &scirc;iaj pensoj kaj sentoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkdeko da papirusaj <tld/>a&jcirc;oj, kovritaj de amba&ubreve; flankoj per
        skriba&jcirc;oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        oceano sub la nigraj <tld/>a&jcirc;oj de la nebuloj
        <fnt>
          <bib>Lasta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="boben.0o">bobeno</ref>,
      <ref cel="rul.0ajxo">rula&jcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_r;&c_u;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_u;&c_n;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rolle</trd>,
    <trd>Wickel</trd>,
    <trd>Spule</trd>,
    <trd>Ranke</trd>
  </trdgrp>
  <trd lng="en">wrap</trd>
  <trd lng="es">rollo</trd>
  <trd lng="fr">enroulement</trd>
  <trdgrp lng="ja">
    <trd>&#x5DFB;&#12356;&#x305F;&#29289;  <pr>&#x307E;&#12356;&#x305F;&#12406;&#12388;</pr></trd>,
    <trd>&#x5DFB;&#x7DDA;  <pr>&#x307E;&#x304D;&#x305B;&#12435;</pr></trd>,
    <trd>&#x30B3;&#x30A4;&#x30EB;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>k&lstroke;&aogonek;b</trd>,
    <trd>rulon</trd>,
    <trd>zwitek</trd>,
    <trd>zw&oacute;j</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_jo;&c_r;&c_t;&c_o;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_g;&#x43E;&c_r;&c_t;&#x43E;&#x43A;</trd>,
    <trd>&c_r;&c_u;&#x43B;&#x43E;&#x43D;</trd>,
    <trd>&#x43E;&c_b;&#x43C;&#x43E;&c_t;&#x43A;&c_a;</trd>,
    <trd>&#x43A;&#x43B;&c_u;&c_b;&#x43E;&#x43A; (&#x43D;&c_i;&c_t;&#x43E;&#x43A;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21367; <pr>ju&#x1CE;n</pr></trd>,
    <trd>&#25458; <pr>ju&#x1CE;n</pr></trd>,
    <trd>&#x7E8F; <pr>ch&#xE1;n</pr></trd>,
    <trd>&#x7F20; <pr>ch&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="volv.rul0ajxo">
  <kap>rul<tld/>a&jcirc;o</kap>
