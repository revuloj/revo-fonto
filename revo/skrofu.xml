<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>skrofulari</rad>/o
</kap>
<drv mrk="skrofu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="skrofu.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Scrophularia</trd>).
    </dif>
  </snc>
  <trd lng="de">Braunwurz</trd>
</drv>
<drv mrk="skrofu.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Scrophulariaceae</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Rachenbl&uuml;ter</trd>,
    <trd>Braunwurzgew&auml;chse</trd>
  </trdgrp>
</drv>
</art>
