<art mrk="$Id: skrofu.xml,v 1.19 2022/05/05 05:09:57 revo Exp $">
<kap>
  <rad>skrofulari</rad>/o
</kap>
<drv mrk="skrofu.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="skrofu1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Scrophularia</trd>):
      <ekz>
        <klr>[la vorto]</klr> <ref cel="skrofo.0o">skrofolo</ref>
        malbone koheras kun <tld/>o, planto kuracanta
        &gcirc;uste la &leftquot;skroful(oz)on&rightquot;
        <fnt>
          <aut>Josef Kavka</aut>
          <vrk><url ref="&STEB;gxenerala_naturscienco/terminologio/homonim.htm">Homonimoj en Esperanto,  konsiderataj nomenklature</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/71/Scrophularia_umbrosa1.jpg/640px-Scrophularia_umbrosa1.jpg" prm="CC-BY-SA-3.0">floroj de <tld/>o (Scrophularia umbrosa)
        <fnt>
          <aut>Kurt St&uuml;ber</aut>
          <lok><url ref="&WCW;/File:Scrophularia_umbrosa1.jpg">&WCW;/File:Scrophularia_umbrosa1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_l;&c_o;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="de">Braunwurz</trd>
  <trdgrp lng="en">
    <trd>Scrophularia</trd>,
    <trd>figwort</trd>
  </trdgrp>
  <trd lng="fr">scrofulaire</trd>
  <trd lng="hu">g&ouml;rv&eacute;lyf&udblac;</trd>
  <trd lng="nl">helmkruid</trd>
  <trd lng="pl">tr&eogonek;downik</trd>
  <trd lng="ru">&c_n;&c_o;&c_r;&c_i;&c_ch;&c_n;&c_i;&c_k;</trd>
  <trd lng="uk">&#x440;&#x430;&#x43D;&#x43D;&#x438;&#x43A;</trd>
</drv>
