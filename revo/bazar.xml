<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>bazar</rad>/o
</kap>
<drv mrk="bazar.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<snc mrk="bazar.0o.placo">
<dif>
Loko, kovrita a&ubreve; ne, kie estas multaj diversaj vendobudoj.
</dif>
<trdgrp lng="fr">
  <trd>march&eacute;<klr tip="ind"> (bazar)</klr></trd>,
  <trd>bazar<klr tip="ind"> (march&eacute;)</klr></trd></trdgrp> 
<trdgrp lng="ru">
   <trd>&c_b;&c_a;&c_z;&c_a;&c_r;</trd>,
   <trd>&c_r;&c_y;&c_n;&c_o;&c_k;</trd>
</trdgrp>
</snc>
<snc mrk="bazar.0o.magazeno">
<dif>
<ref tip="super" cel="magaze.0o">Magazeno</ref>, en kiu oni vendas plej 
diversajn komerca&jcirc;ojn, ordinare je malkara prezo.
</dif>
<ref tip="sin" cel="vend.cxio0ejo">&ccirc;iovendejo.</ref>
<trdgrp lng="fr">
   <trd>&eacute;picerie</trd>,
   <trd>bazar<klr tip="ind"> (magasin)</klr></trd></trdgrp>
<trdgrp lng="ru">
   <trd>&c_u;&c_n;&c_i;&c_v;&c_je;&c_r;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; 
&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;</trd>,
   <trd>&c_u;&c_n;&c_i;&c_v;&c_je;&c_r;&c_m;&c_a;&c_g;</trd>,
   <trd>&c_s;&c_u;&c_p;&c_je;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
</trdgrp>
</snc>
<refgrp tip="vid">
   <ref cel="foir.0o">foiro</ref>,
   <ref cel="foir.0ejo">foirejo</ref>,
   <ref cel="merkat.0o">merkato</ref>.
</refgrp>
</drv>

     <drv mrk="bazar.pul0o">
       <kap>pul<tld/>o</kap>
       <snc>
         <dif>
           Granda <tld/>o, kie estas a&ccirc;eteblaj plej diversaj
           <ref cel="brokan.0ajxo">brokanta&jcirc;oj</ref>.
         </dif>
         <trd lng="fr">march&eacute; aux puces</trd>
       </snc>
     </drv>

     <drv mrk="bazar.super0o">
       <kap>super<tld/>o</kap>
       <snc>
         <dif>
           Granda, memserva <ref tip="super" cel="magaze.0o">magazeno</ref>, 
           kie estas a&ccirc;eteblaj la varoj plej bezonataj en la 
           &ccirc;iutaga vivo.
         </dif>
         <trd lng="fr">supermarch&eacute;</trd>
       </snc>
     </drv>

</art>
