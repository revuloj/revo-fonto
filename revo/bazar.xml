<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bazar.xml,v 1.28 2021/05/28 12:52:40 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bazar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bazar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bazar.0o.placo">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Granda, subtegmenta a&ubreve; sub&ccirc;iela
      <ref tip="super" cel="vend.0ejo">vendejo</ref>,
      kie kunestas diversaj sendependaj vendobudoj:
      <ekz>
        mi sendus al la <tld/>o, por a&ccirc;eti almena&ubreve; paneton
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        virinoj en la <tld/>o interbati&gcirc;is, sed la polico venis tro malfrue
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de malnova&jcirc;oj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa <tld/>o, kien oni alportis la komerca&jcirc;ojn el la oazoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        brua, abunda <tld/>o
        <fnt>
          <aut>B. Selimi</aut>:
          <vrk>Voja&gcirc;impresoj, somero 2000</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 1a, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="foir.0ejo">foirejo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_b;&c_a;&c_z;&c_a;&c_r;</trd>,
      <trd>&c_k;&c_ib;&c_r;&c_m;&c_a;&c_sh;</trd>,
      <trd>&c_r;&c_y;&c_n;&c_a;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Markt</trd>,
      <trd>Basar</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>mall</trd>,
      <trd>market</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>bazar</trd>,
      <trd>mercado</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>march&eacute;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>piac</trd>,
      <trd>baz&aacute;r</trd>
    </trdgrp>
    <trd lng="nl">markt</trd>
    <trdgrp lng="pl">
      <trd>bazar</trd>,
      <trd>targ</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>bazar</trd>,
      <trd>mercado</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_z;&c_a;&c_r;</trd>,
      <trd>&c_r;&c_y;&c_n;&c_o;&c_k;<klr tip="ind">
      (&c_b;&c_a;&c_z;&c_a;&c_r;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="bazar.0o.magazeno">
    <uzo tip="fak">EKON</uzo>
    <ref tip="dif" cel="vend.cxio0ejo">&ccirc;iovendejo</ref>
    <ref tip="sin" cel="bazar.super0o">superbazaro</ref>
    <trdgrp lng="be">
      <trd>&c_u;&c_n;&c_ib;&c_v;&c_e;&c_r;&c_m;&c_a;&c_g;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_e;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>&eacute;picerie</trd>,
      <trd>bazar<klr tip="ind"> (magasin)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>department <ind>store</ind></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>tienda por departamento</trd>,
      <trd>almac&eacute;n por departamento</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Supermarkt</trd>,
      <trd>Kaufhaus</trd>
    </trdgrp>
    <trd lng="hu">ABC-bolt</trd>
    <trd lng="nl">minimarkt</trd>
    <trdgrp lng="pl">
      <trd>market</trd>,
      <trd>hala<klr> (region.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_n;&c_i;&c_v;&c_je;&c_r;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;
      &c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;</trd>,
      <trd>&c_u;&c_n;&c_i;&c_v;&c_je;&c_r;&c_m;&c_a;&c_g;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_je;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
    </trdgrp>
  </snc>
  <trd lng="bo">&#xF41;&#xFB2;&#xF7C;&#xF58;</trd>
  <trdgrp lng="cs">
    <trd>bazar</trd>,
    <trd>tr&zcaron;nice</trd>
  </trdgrp>
  <trd lng="he">&#x5E9;&#x5C1;&#x5D5;&#x5BC;&#x5E7;</trd>
  <trdgrp lng="sk">
    <trd>baz&aacute;r</trd>,
    <trd>starin&aacute;rstvo</trd>,
    <trd>trh <klr>(orient&aacute;lny)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="bazar.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="bazar.0ejo.EKON">
    <dif>
      Loko, plejofte placo, sur kiu okazas <tld/>o:
      <ekz>
        La vila&gcirc;o havas nur unu gravan straton kaj la malgrandan
        bazarejon
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="https://eo.wikipedia.org/wiki/Hallstatt_%28urbo%29">
          Hallstatt (urbo)</url>, 2015-11-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">place du march&eacute;</trd>
</drv>

<drv mrk="bazar.flos0o">
  <kap>flos<tld/>o, <var><kap>flosa <tld/>o</kap></var></kap>
  <snc mrk="bazar.flos0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Surakva <ref tip="super" cel="bazar.0o.placo">bazaro</ref>,
      kie oni komercas de sur boatoj:
      <ekz>
        Koda&gcirc;aso estas <ctl>lando de asaio</ctl>, tie estas flosa 
        <tld/>o
        <fnt>
          <aut>R. da Silva Ribeiro</aut>,
          <vrk>Voja&gcirc;taglibro al Amazonio</vrk>,
          <lok><url ref="http://joaojosesantos.net/pdf/LK_018_RV.pdf">
          La Karavelo, 2009-09 (18)</url>, p. 15a</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni piediras al malfama flos<tld/>o de Bangkoko
        <fnt>
          <aut>C. Pinardon. R. Sanz</aut>,
          <vrk>Du Bicikloj je la Horizonto</vrk>,
          <lok><url ref="http://horizonto.free.fr/rakontoj/15-tajlando.html">
          Tajlando</url>, 2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">floating market</trd>
  <trd lng="fr">march&eacute; flottant</trd>
</drv>

<drv mrk="bazar.pul0o">
  <kap>pul<tld/>o</kap>
  <snc mrk="bazar.pul0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="bazar.0o.placo"><tld lit="B"/>o</ref>,
      kie estas a&ccirc;eteblaj <ref tip="vid" cel="brokan.0ajxo">brokanta&jcirc;oj</ref>:
      <ekz>
        en la pul<tld/>o de Parizo, krimulo invitas nekonaton al sia lo&gcirc;ejo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9175">Angoroj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas unika mediteranea pul<tld/>o, kiu havas preska&ubreve; &ccirc;ion, oni neniam scias kiun trezoron oni povas trovi tie
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34093">Tel-Avivo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Flohmarkt</trd>,
      <trd>Antikmarkt</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>flea <ind>market</ind></trd>
    </trdgrp>
    <trd lng="fr">march&eacute; aux puces</trd>
    <trdgrp lng="hu">
      <trd>&oacute;cskapiac</trd>,
      <trd>bolhapiac</trd>
    </trdgrp>
    <trd lng="nl">vlooienmarkt</trd>
    <trdgrp lng="pl">
      <trd>pchli <ind>targ</ind></trd>,
      <trd>targ staroci</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_b;&c_a;&c_r;&c_a;&c_h;&c_o;&c_l;&c_k;&c_a;</trd>,
      <trd>&c_t;&c_o;&c_l;&c_k;&c_u;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_b;&c_l;&c_o;&c_sh;&c_i;&c_n;&c_y;&c_j; &c_r;&c_y;&c_n;&c_o;&c_k;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">ble&scaron;&iacute; trh</trd>
  <trd lng="sk">bl&scaron;&iacute; trh</trd>
</drv>

<drv mrk="bazar.super0o">
  <kap>super<tld/>o</kap>
  <snc mrk="bazar.super0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="vend.cxio0ejo">&Ccirc;iovendejo</ref>:
      <ekz>
        super<tld/>o estas granda vendejo, kiu vendas ampleksan varion de varoj inkluzive de man&gcirc;a&jcirc;oj, 
        trinka&jcirc;oj, drogoj, vesta&jcirc;oj, nutra&jcirc;oj bestaj, ludiloj, aliaj domaj produktoj kaj 
        sezonaj artikloj kiuj estas konsumitaj kutime
        <fnt>
          <vrk><url ref="https://eo.m.wikipedia.org/wiki/Superbazaro">Superbazaro - Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        vendistino en via a&ubreve; mia loka super<tld/>o
        <fnt>
          <aut>P. Gubbins</aut>,
          <vrk>Diana... &ccirc;u diino?</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1997a, numero 9a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_u;&c_p;&c_e;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
    <trdgrp lng="de">
      <trd>Supermarkt</trd>,
      <trd>Kaufhaus</trd>
    </trdgrp>
    <trd lng="en">supermarket</trd>
    <trd lng="es">supermercado</trd>
    <trd lng="fr">supermarch&eacute;</trd>
    <trd lng="hu">szupermarket</trd>
    <trd lng="nl">supermarkt</trd>
    <trdgrp lng="pl">
      <trd>supermarket</trd>,
      <trd>supersam<klr> (przest.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_u;&c_p;&c_je;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>obchodn&iacute; d&uring;m se samoobsluhou</trd>,
    <trd>supermarket</trd>
  </trdgrp>
  <trd lng="sk">supermarket</trd>
</drv>
</art>
