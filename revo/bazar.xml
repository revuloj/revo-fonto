    <trd>&c_b;&c_l;&c_o;&c_sh;&c_i;&c_n;&c_i;&c_j; &c_r;&c_i;&c_n;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="bazar.super0o">
  <kap>super<tld/>o</kap>
  <snc mrk="bazar.super0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="vend.cxio0ejo">&Ccirc;iovendejo</ref>:
      <ekz>
        super<tld/>o estas granda vendejo, kiu vendas ampleksan varion de varoj inkluzive de
        man&gcirc;a&jcirc;oj,
        trinka&jcirc;oj, drogoj, vesta&jcirc;oj, nutra&jcirc;oj bestaj, ludiloj, aliaj domaj produktoj kaj
        sezonaj artikloj kiuj estas konsumitaj kutime
        <fnt>
          <vrk><url ref="https://eo.m.wikipedia.org/wiki/Superbazaro">Superbazaro -
          Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        vendistino en via a&ubreve; mia loka super<tld/>o
        <fnt>
          <aut>P. Gubbins</aut>,
          <vrk>Diana... &ccirc;u diino?</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1997a, numero 9a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_u;&c_p;&c_e;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
    <trdgrp lng="de">
      <trd>Supermarkt</trd>,
      <trd>Kaufhaus</trd>
    </trdgrp>
    <trd lng="en">supermarket</trd>
    <trd lng="es">supermercado</trd>
    <trd lng="fr">supermarch&eacute;</trd>
    <trd lng="hu">szupermarket</trd>
    <trd lng="nl">supermarkt</trd>
    <trdgrp lng="pl">
      <trd>supermarket</trd>,
      <trd>supersam<klr> (przest.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_u;&c_p;&c_je;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
    <trdgrp lng="zh">
    <trd>&#36229;&#24066; <pr>ch&amacron;osh&igrave;</pr></trd>,
    <trd>&#36229;&#32026;&#24066;&#22580; <pr>ch&amacron;oj&iacute;sh&igrave;ch&#462;ng</pr></trd>,
    <trd>&#33258;&#36873;&#21830;&#22330; <pr>z&igrave;xu&#462;nsh&amacron;ngch&#462;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ca">supermercat</trd>
  <trdgrp lng="cs">
    <trd>obchodn&iacute; d&uring;m se samoobsluhou</trd>,
    <trd>supermarket</trd>
  </trdgrp>
  <trd lng="ja">&#12473;&#12540;&#12497;&#12540;&#12510;&#12540;&#12465;&#12483;&#12488;</trd>
  <trd lng="sk">supermarket</trd>
  <trd lng="uk">&c_s;&c_u;&c_p;&c_je;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
</drv>
