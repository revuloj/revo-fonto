<drv mrk="sxangx.inter0i">
  <kap>inter<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxangx.inter0i.KOMUNE">
    <dif>
      Doni ion por ricevi ion alian:
      <ekz>
        la junulo inter<tld/>is kun li la vestojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter<tld/>o de opinioj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili inter<tld/>is vortojn kaj sen&ccirc;ese rigardis sin reciproke
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la pli junaj laboristinoj ka&scirc;e inter<tld/>is rigardon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i aliris al la skribtablo <klr>[&#x2026;]</klr> inter<tld/>inte saluton kun la libristo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        vendu la &ccirc;evalon a&ubreve; faru bonan inter<tld/>on
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi volas inter<tld/>i? pro via ansero mi donos al vi la &scirc;afon
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> en Esperanto vidas nur rimedon por inter<tld/>i ilustritajn
        po&scirc;tajn kartojn
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni inter<tld/>is la vizitkartojn kaj interkonsentis kunveni je la oka horo
        <fnt>
          <bib>FdO</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_m;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_m;&c_je;&c_n;&c_ja;&c_m;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_je;&c_n;&c_ja;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>austauschen</trd>,
    <trd>auswechseln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>austauschen</trd>,
  </trdgrp>
  <trd lng="es">intercambiar</trd>
  <trdgrp lng="fr">
    <trd>&eacute;changer</trd>,
    <trd>troquer</trd>
  </trdgrp>
  <trd lng="hu">cser&eacute;l <klr tip="ind">(vmit
  vmire)</klr></trd>
  <trd lng="hr">razmijeniti</trd>
  <trdgrp lng="it">
    <trd>scambiare<klr> (interscambiare)</klr></trd>,
    <trd>interscambiare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ruilen</trd>,
    <trd>omwisselen</trd>
  </trdgrp>
  <trd lng="pl">wymienia&cacute;</trd>
  <trdgrp lng="pt">
    <trd>trocar</trd>,
    <trd>intercambiar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_m;&c_je;&c_n;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sxangx.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="sxangx.ne0ebla.KOMUNE">
    <dif>
      Kiun neniu, nenio povas <tld/>i;
      fiksita:
      <ekz>
        ordono, konfirmita de la re&gcirc;o, devas resti ne<tld/>ebla
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 6:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estas ne<tld/>eble decidita
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 28:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne<tld/>ebla sorto
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        super ni re&gcirc;as ne<tld/>ebla fatalo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili konsideras la Koranon kiel finan,
        ne<tld/>eblan vorton de Dio 
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.internacia-festivalo.de/temouni.html">
          Religioj kaj konfliktoj</url></vrk>,
          <lok>[2002?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_n;&c_je;&c_p;&c_r;&c_o;&c_m;&c_je;&c_n;&c_i;&c_m;</trd>
  
  <trdgrp lng="de">
    <trd>unab&auml;nderlich</trd>,
    <trd>unver&auml;nderbar</trd>,
    <trd>unverr&uuml;ckbar</trd>,
    <trd>unantastbar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fixe<klr> (immuable)</klr></trd>,
    <trd>immuable</trd>,
    <trd>obstin&eacute;<klr> (immuable)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sxangx.sen0a">
  <kap><ofc>*</ofc> sen<tld/>a</kap>
  <snc mrk="sxangx.sen0a.KOMUNE">
    <dif>
      Ne <tld/>i&gcirc;anta,
      <ref tip="vid" cel="firm.0a">firma</ref>,
      <ref tip="vid" cel="mov.sen0a">senmova</ref>:
      <ekz>
        <klr>[vi]</klr> vivan temperamenton, &scirc;i diris kun sen<tld/>a dol&ccirc;eco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>aj restis nur tiuj silaboj, kiujn oni bone a&ubreve;dis
        <fnt>
          <bib>LR</bib>
          <lok>4. Fonetiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fundamento de Esperanto devas plej severe resti absolute sen<tld/>a
        <fnt>
          <bib>F</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malnova Wartburga kastelo &ccirc;iam ankora&ubreve; staris sen<tld/>e alte supre sur sia
        roka alta&jcirc;o
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kerno de la karaktero restas sen<tld/>a, oni kredas
        <fnt>
          <bib>Monato</bib>
          <aut>Lenke Sz&aacute;sz</aut>
          <vrk><url ref="https://www.monato.be/2011/010302.php">Renkonti&gcirc;o</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ja;&c_z;&c_mol;&c_m;&c_je;&c_n;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>unver&auml;ndert</trd>,
    <trd>unver&auml;nderlich</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>immuable</trd>,
    <trd>inalt&eacute;r&eacute;</trd>,
    <trd>permanent</trd>
  </trdgrp>
  <trd lng="hu">v&aacute;ltozatlan</trd>
  <trdgrp lng="it">
    <trd>immutabile</trd>,
    <trd>immutato</trd>, 
    <trd>inalterato</trd>,
    <trd>invariato</trd>, 
    <trd>invariabile</trd>,
    <trd>costante</trd>
  </trdgrp>
  <trd lng="hr">neizmjenjen</trd>
  <trd lng="nl">onveranderlijk</trd>
  <trd lng="pl">niezmienny</trd>
  <trd lng="ru">&c_n;&c_je;&c_i;&c_z;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>
