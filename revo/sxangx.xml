<drv mrk="sxangx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxangx.0i.anstatauxigi">
    <dif>
      <klr>(<tld/>i ion je, al, kontra&ubreve; alio:)</klr>
      Anstata&ubreve;igi ion per io alia:
      <ekz>
        sed Mia popolo <tld/>is sian gloron je idoloj
        <fnt><bib>MT</bib>, <lok>&Jer; 2:11</lok></fnt>;
      </ekz>
      <ekz>
        kaj oni ne povas <tld/>i &gcirc;in kontra&ubreve; vazoj el
        pura oro
        <fnt><bib>MT</bib>, <lok>&Ijo; 28:17</lok></fnt>;
      </ekz>
      <ekz>
        li sin razis kaj <tld/>is siajn vestojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 41:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        via patro trompis min kaj dekfoje <tld/>is mian laborpagon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu konstante lokon <tld/>as, neniam sin aran&gcirc;as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        vulpo mienon <tld/>as, sed plue kokidojn man&gcirc;as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la longa da&ubreve;rado de la kataro oni ofte devas <tld/>i la rimedojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        unu fojon mi decidis, kaj mian vorton mi ne <tld/>os, kolere respondis la re&gcirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tial mi <tld/>is la temon de la parolado
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>os la monon en la magazeno
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne malvolonte <tld/>us lin je nova amanto
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ali.0igi">aliigi</ref>,
      <ref cel="korekt.0i">korekti</ref>,
      <ref cel="tusx.0i">tu&scirc;i</ref>.
    </refgrp>
    <trdgrp lng="bg">
      <trd>&c_p;&c_r;&c_o;&c_m;&c_je;&c_n;&c_ja;&c_m;</trd>,
