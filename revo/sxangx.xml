<drv mrk="sxangx.0a">
  <kap><tld/>a</kap>
  <snc mrk="sxangx.0a.sxangxanta">
    <dif>
      Kiu <tld/>as; de <tld/>o:
      <ekz>
        <tld/>aj proponoj, kiuj efektive povus havi ian praktikan valoron
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        nom<tld/>a partikunveno
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008502.php">La Blok blokita ... a&ubreve; &ccirc;u?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        <ind>re&gcirc;im<tld/>a klavo</ind>
        <fnt>
          <aut>E. Grimley Evans</aut>,
          <vrk><url ref="&Monato;/2000/006801.html">
          Interreto: uniksulo ekspertas Unikodon</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/02, p. 20</lok>
        </fnt>.
        <trd lng="de">Umschalttaste</trd>
        <trd lng="pl">klawisz zmiany trybu</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="sxangx.0a.sxangxigxa">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <tld lit="&Scirc;"/>i&gcirc;anta:
      <ekz>
        bonodora kiel la lilio, facila kiel la vento, <tld/>a kiel la maro
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
