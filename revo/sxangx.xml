<drv mrk="sxangx.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="sxangx.ne0ebla.KOMUNE">
    <dif>
      Kiun neniu, nenio povas <tld/>i;
      fiksita:
      <ekz>
        ordono, konfirmita de la re&gcirc;o, devas resti ne<tld/>ebla
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 6:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estas ne<tld/>eble decidita
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 28:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne<tld/>ebla sorto
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        super ni re&gcirc;as ne<tld/>ebla fatalo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili konsideras la Koranon kiel finan,
        ne<tld/>eblan vorton de Dio
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.internacia-festivalo.de/temouni.html">
          Religioj kaj konfliktoj</url></vrk>,
