<drv mrk="sxangx.inter0i">
  <kap>inter<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxangx.inter0i.KOMUNE">
    <dif>
      Doni ion por ricevi ion alian:
      <ekz>
        la junulo inter<tld/>is kun li la vestojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter<tld/>o de opinioj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili inter<tld/>is vortojn kaj sen&ccirc;ese rigardis sin reciproke
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la pli junaj laboristinoj ka&scirc;e inter<tld/>is rigardon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i aliris al la skribtablo <klr>[&#8230;]</klr> inter<tld/>inte saluton kun la libristo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        vendu la &ccirc;evalon a&ubreve; faru bonan inter<tld/>on
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi volas inter<tld/>i? pro via ansero mi donos al vi la &scirc;afon
        <fnt>
          <bib>Fab4</bib>
          <lok>Kion la patro faras, estas &ccirc;iam &gcirc;usta</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> en Esperanto vidas nur rimedon por inter<tld/>i ilustritajn
        po&scirc;tajn kartojn
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni inter<tld/>is la vizitkartojn kaj interkonsentis kunveni je la oka horo
        <fnt>
          <bib>FdO</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="th">
      <trd>&#xE41;&#xE25;&#xE01;&#xE40;&#xE1B;&#xE25;&#xE35;&#xE48;&#xE22;&#xE19;</trd>,
      <trd>&#xE41;&#xE25;&#xE01;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_m;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
