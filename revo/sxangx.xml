<kap>
  <ofc>*</ofc>
  <rad>&scirc;an&gcirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sxangx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxangx.0i.anstatauxigi">
    <dif>
      <klr>(<tld/>i ion je, al, kontra&ubreve; alio:)</klr>
      Anstata&ubreve;igi ion per io alia:
      <ekz>
        sed Mia popolo <tld/>is sian gloron je idoloj
        <fnt><bib>MT</bib>, <lok>&Jer; 2:11</lok></fnt>;
      </ekz>
      <ekz>
        kaj oni ne povas <tld/>i &gcirc;in kontra&ubreve; vazoj el
        pura oro
        <fnt><bib>MT</bib>, <lok>&Ijo; 28:17</lok></fnt>;
      </ekz>
      <ekz>
        li sin razis kaj <tld/>is siajn vestojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 41:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        via patro trompis min kaj dekfoje <tld/>is mian laborpagon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiu konstante lokon <tld/>as, neniam sin aran&gcirc;as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        vulpo mienon <tld/>as, sed plue kokidojn man&gcirc;as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e la longa da&ubreve;rado de la kataro oni ofte devas <tld/>i la rimedojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        unu fojon mi decidis, kaj mian vorton mi ne <tld/>os, kolere respondis la re&gcirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tial mi <tld/>is la temon de la parolado
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>os la monon en la magazeno
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne malvolonte <tld/>us lin je nova amanto
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ali.0igi">aliigi</ref>,
      <ref cel="korekt.0i">korekti</ref>,
      <ref cel="tusx.0i">tu&scirc;i</ref>.
    </refgrp>
    <trdgrp lng="bg">
      <trd>&c_p;&c_r;&c_o;&c_m;&c_je;&c_n;&c_ja;&c_m;</trd>,
      <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_ja;&c_m;</trd>
    </trdgrp>
    <trd lng="ms">menukar</trd>
    <trdgrp lng="de">
      <trd>tauschen</trd>,
      <trd>ersetzen</trd>,
      <trd>wechseln</trd>,
      <trd>auswechseln</trd>,
      <trd>&auml;ndern</trd>,
      <trd>ab&auml;ndern</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cser&eacute;l</trd>,
      <trd>v&aacute;lt</trd>
    </trdgrp>
    <trd lng="hr">promjeniti</trd>
    <trdgrp lng="nl">
      <trd>veranderen</trd>,
      <trd>wisselen</trd>,
    </trdgrp>
    <trdgrp lng="pt">
      <trd>mudar</trd>,
      <trd>trocar</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sxangx.0i.modifi">
    <dif>
      <klr>(<tld/>i ion en alion, al alio:)</klr>
      Aliigi la staton de io, transformi ion:
      <ekz>
        Li <tld/>is en sangon iliajn riverojn kaj torentojn
        <fnt><bib>MT</bib>, <lok>&Psa; 78:44</lok></fnt>;
      </ekz>
      <ekz>
        Li <tld/>as riverojn en dezerton kaj fontojn de akvo en seka&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 107:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        la persekuto kaj teruraj batoj kontra&ubreve; la Hebreoj dum la milito ne <tld/>is tiun
        &ccirc;i vidpunkton
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        la tuta grupo, subite <tld/>inte la situacion, restas kvaza&ubreve; &scirc;tonigita
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i la lingvon &gcirc;is nerekonebleco
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la verbo ne estas <tld/>ata la&ubreve; personoj nek nombroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        tago aran&gcirc;as, tago <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sufi&ccirc;e <tld/>ita, sed <tld/>ita al bono
        <fnt>
          <aut>C. Dickens, trad. L. Zamenhof</aut>,
          <vrk>La batalo de l' vivo</vrk>,
          <lok>1891</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="form.ali0i.KOMUNE">aliformi</ref>,
      <ref cel="metamo1.0i">metamorfozi</ref>,
      <ref cel="modif.0i">modifi</ref>
    </refgrp>
    <trdgrp lng="bg">
      <trd>&c_p;&c_r;&c_o;&c_m;&c_je;&c_n;&c_ja;&c_m;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_v;&c_r;&c_malmol;&c_shch;&c_a;&c_m;</trd>
    </trdgrp>
    <trd lng="de">&auml;ndern</trd>
    <trd lng="hu">megv&aacute;ltoztat</trd>  
    <trdgrp lng="nl">
      <trd>veranderen</trd>,
      <trd>wijzigen</trd>
    </trdgrp>
    <trd lng="pt">mudar</trd>
    <trdgrp lng="ru">
      <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_je;&c_v;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">canviar</trd>
  <trd lng="cs">m&ecaron;nit</trd>
  <trd lng="en">change</trd>
  <trd lng="es">cambiar</trd>
  <trd lng="fr">changer<klr> (tr.)</klr></trd>
  <trd lng="hr">promjeniti</trd>
  <trd lng="it">cambiare</trd>
  <trd lng="oc">cambiar</trd>
  <trd lng="pl">zmienia&cacute;</trd>
  <trd lng="ro">a schimba</trd>
  <trd lng="ru">&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="tp">ante e ijo</trd>
</drv>

<drv mrk="sxangx.0a">
  <kap><tld/>a</kap>
  <snc mrk="sxangx.0a.sxangxanta">
    <dif>
      Kiu <tld/>as; de <tld/>o:
      <ekz>
        <tld/>aj proponoj, kiuj efektive povus havi ian praktikan valoron
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto 1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        nom<tld/>a partikunveno
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="https://www.monato.be/2005/008502.php"
              >La Blok blokita ... a&ubreve; &ccirc;u?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        <ind>re&gcirc;im<tld/>a klavo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006801.html"
              >Interreto: Uniksulo ekspertas Unikodon</url></vrk>
        </fnt>.
        <trd lng="de">Umschalttaste</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="sxangx.0a.sxangxigxa">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <tld lit="&Scirc;"/>i&gcirc;anta:
      <ekz>
        bonodora kiel la lilio, facila kiel la vento, <tld/>a kiel la maro
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
<!-- ne traduku EVI:
  <trd lng="de"><klr>sich </klr>&auml;ndernd</trd>
  <trd lng="es">cambiante</trd>
  <trd lng="hu">v&aacute;ltoz&oacute;</trd>
  <trdgrp lng="it">
    <trd>cangiante</trd>,
    <trd>mutevole</trd>
  </trdgrp>
  <trd lng="hr">promjenjiv</trd>
  <trd lng="nl">veranderend<klr tip="ind"> (ontr.)</klr></trd>
  <trd lng="pl">zmienny</trd> -->
  </snc>
</drv>

<drv mrk="sxangx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxangx.0o.anstatauxigo">
    <dif>
     Anstata&ubreve;igo: 
     <ekz>
        mi atendus, &gcirc;is venos mia for<tld/>o<fnt>Z</fnt>.
     </ekz>
    </dif>
  </snc>
  <snc mrk="sxangx.0o.modifo">
    <dif>
      Modifo, variigo:
      <ekz>
      &ccirc;iujn deziritajn &scirc;an&gcirc;ojn, se tiaj &scirc;an&gcirc;oj montri&gcirc;os necesaj
      <fnt><bib>F</bib>, <lok>Anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        vidi rimarkindajn <tld/>ojn de la koloro de la
        lando<fnt>B</fnt>;
      </ekz>
      <ekz>
      se ni volas proponi a&ubreve; fari <tld/>on ni devas nin demandi,
      kian
      utilon la <tld/>o alportus al la lingvo mem, kaj &ccirc;u la utilo
      de la
      <tld/>o kovrus la malutilon<fnt><bib>LR</bib>, <lok>La
      Esperantisto, 1891, p.49-50</lok></fnt>.
      </ekz>
    </dif>
    <rim>
    Z. klarigis en sia Aldono al la Dua Libro, ke "la sola <tld/>o, kiun
    mi trovas
    necesa fari mem,
    estas: anstata&ubreve; <ekz>"ian", "&ccirc;ian", "kian", "nenian",
    "tian"</ekz> -
    devas esti: <ekz>"iam", "&ccirc;iam", "kiam", "neniam", "tiam"</ekz>
    (por
    malegaligi la vortojn <ekz>"ian" ktp. </ekz> kaj <ekz>"ia/n"</ekz>).
    </rim>
  </snc>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_m;&c_ja;&c_n;&c_a;</trd>
  <trd lng="fr">changement</trd>
</drv>

<drv mrk="sxangx.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc mrk="sxangx.0igxema.KOMUNE">
    <dif>
      Facile kaj ofte <tld/>i&gcirc;anta.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_m;&c_je;&c_n;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_m;&c_je;&c_n;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_m;&c_je;&c_n;&c_l;&c_i;&c_v;</trd>
  <trd lng="de">ver&auml;nderlich</trd>
  <trd lng="es">cambiante</trd>
  <trdgrp lng="fr">
    <trd>inconstant</trd>,
    <trd>fluctuant</trd>,
    <trd>variable</trd>
  </trdgrp>
  <trd lng="hu">v&aacute;ltoz&eacute;kony</trd>
 <trdgrp lng="it">
    <trd>mutevole</trd>, 
    <trd>variabile</trd>, 
    <trd>volubile</trd>, 
    <trd>inconstante</trd>
  </trdgrp>
  <trd lng="nl">veranderlijk</trd>
  <trd lng="pl">zmieniaj&aogonek;cy si&eogonek;</trd>
  <trd lng="pt">mut&aacute;vel</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_je;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="sxangx.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sxangx.0igxi.KOMUNE">
    <dif>
      Pasi de unu stato al alia,
      <ref tip="vid" cel="vari.0i">varii</ref>:
      <ekz>
        la cirkonstancoj <tld/>i&gcirc;is al pli
        malbono<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>i&gcirc;oj<fnt>Z</fnt>,
      </ekz>
      <ekz>
        la <tld/>i&gcirc;eco de la sorto<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_m;&c_ja;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_m;&c_je;&c_n;&c_ja;&c_m; &c_s;&c_je;</trd>
  <trd lng="ca">canviar</trd>
  <trdgrp lng="de">
    <trd>sich <ind>&auml;ndern</ind></trd>,
    <trd>sich <ind>ver&auml;ndern</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>change</trd>,
    <trd>be changed</trd>,
    <trd>alter</trd>,
    <trd>convert</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cambiar<klr> (interiormente)</klr></trd>,
    <trd><ind>alterar</ind>se</trd>,
    <trd>ser <ind>transformado</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>changer<klr> (intr.)</klr></trd>,
    <trd>s&#8217;alt&eacute;rer</trd>,
    <trd>muter</trd>,
    <trd>se transformer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>v&aacute;ltozik</trd>,
    <trd>cser&eacute;l&odblac;dik</trd>
  </trdgrp>
  <trd lng="hr">mijenjati se</trd>
  <trdgrp lng="it">
    <trd>cambiare<klr> (diventare diverso)</klr></trd>,
    <trd>mutare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zich <ind>veranderen</ind></trd>
    <trd>veranderen</trd>,
    <trd>kenteren</trd>,
    <trd>zich <ind>aanpassen</ind></trd>,
  </trdgrp>
  <trd lng="oc">cambiar</trd>
  <trd lng="pl">zmienia&cacute; si&eogonek;</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_i;&c_z;&c_m;&c_je;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="tp">kama ante</trd>
</drv>

<drv mrk="sxangx.inter0i">
  <kap>inter<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sxangx.inter0i.KOMUNE">
    <dif>
      Doni ion por ricevi ion alian:
      <ekz>
        inter<tld/>i varojn, pentra&jcirc;ojn,
        saluton, vortojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        inter<tld/>i ideojn;
      </ekz>
      <ekz>
        inter<tld/>i ion por<fnt>Z</fnt>,
      </ekz>
      <ekz>
        kontra&ubreve;<fnt>Z</fnt>,
      </ekz>
      <ekz>
        anstata&ubreve; io.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_a;&c_b;&c_m;&c_je;&c_n;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_r;&c_a;&c_z;&c_m;&c_je;&c_n;&c_ja;&c_m;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_je;&c_n;&c_ja;&c_m;</trd>
  </trdgrp>
  <trd lng="de">austauschen</trd>
  <trd lng="es">intercambiar</trd>
  <trdgrp lng="fr">
    <trd>&eacute;changer</trd>,
    <trd>troquer</trd>
  </trdgrp>
  <trd lng="hu">cser&eacute;l <klr tip="ind">(vmit
  vmire)</klr></trd>
  <trd lng="hr">razmijeniti</trd>
  <trdgrp lng="it">
    <trd>scambiare<klr> (interscambiare)</klr></trd>,
    <trd>interscambiare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ruilen</trd>,
    <trd>omwisselen</trd>
  </trdgrp>
  <trd lng="pl">wymienia&cacute;</trd>
  <trdgrp lng="pt">
    <trd>trocar</trd>,
    <trd>intercambiar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_m;&c_je;&c_n;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_je;&c_n;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="sxangx.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="sxangx.ne0ebla.KOMUNE">
    <dif>
      Kiun neniu, nenio povas <tld/>i;
      fiksita:
      <ekz>
        ili konsideras la Koranon kiel finan,
        ne<tld/>eblan vorton de Dio 
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://www.internacia-festivalo.de/temouni.html">
          Religioj kaj konfliktoj</url></vrk>,
          <lok>[2002?]</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_n;&c_je;&c_p;&c_r;&c_o;&c_m;&c_je;&c_n;&c_i;&c_m;</trd>
  <trdgrp lng="fr">
    <trd>fixe<klr> (immuable)</klr></trd>,
    <trd>immuable</trd>,
    <trd>obstin&eacute;<klr> (immuable)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sxangx.sen0a">
  <kap><ofc>*</ofc> sen<tld/>a</kap>
  <snc mrk="sxangx.sen0a.KOMUNE">
    <dif>
      Ne <tld/>i&gcirc;anta,
      <ref tip="vid" cel="firm.0a">firma</ref>,
