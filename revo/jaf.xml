<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: jaf.xml,v 1.4 2022/05/05 07:30:38 afido Exp $">
    <kap>
      <rad>Jaf</rad>/o <fnt><bib>MT</bib></fnt>
    </kap>

<drv mrk="jaf.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">HIS</uzo><uzo tip="fak">BIB</uzo>
  <snc>
    <dif>
      Antikva haven<ref tip="lst" lst="voko:urboj" cel="urb.0o">urbo</ref>
      en <ref tip="malprt" cel="israel.I0o">Israelo</ref>, nun parto de Tel-Avivo:
      <ekz>
        ni hakos lignon de <nom>Lebanon</nom>, kiom vi bezonas, kaj ni sendos &gcirc;in al vi per flosoj
        sur la maro &gcirc;is <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 2:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum multe da tagoj li lo&gcirc;is en <tld/>o &ccirc;e <klr>[&#8230;]</klr> tanisto
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 9:43</lok>
        </fnt>;
      </ekz>
      <ekz>
        la batalo de <tld/>o okazis komence de a&ubreve;gusto 1192 dum la tria krucmilito
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=301175">Batalo de Jafo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ar">&a_y;&a_fatha;&a_A;&a_f;&a_fatha;&a_A;</trd>
    <trd lng="he">&lrm;&yod;&#x5B8;&pe;&vav;&#x5B9;</trd>
    <trd lng="de">Jaffa</trd>
    <trd lng="en">Jaffa</trd>
    <trd lng="fr">Jaffa</trd>
  </snc>
  <trd lng="be">&c_Ja;&c_f;&c_a;</trd>
  <trd lng="uk">&#x42F;&c_f;&c_f;&c_a;</trd>
</drv>

  </art>

