<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dielektrik</rad>/o
</kap>
<drv mrk="dielek.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Substanco, kiu ne kondukas elektran fluon. 
    </dif>
  </snc>
  <trd
  lng="be">&c_d;&c_y;&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>dielektrikum</trd>,
    <trd>izolátor</trd>,
    <trd>nevodivé prostředí</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Dielektrikum</trd>,
     <trd>Nichtleiter</trd>
  </trdgrp>
  <trd lng="es">diel&eacute;ctrico</trd>
  <trd lng="fr">di&eacute;lectrique<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">dielektrikum</trd>
  <trdgrp lng="pl">
    <trd>dielektryk</trd>,
    <trd>izolator<klr> (elektr.)</klr></trd>
  </trdgrp>
  <trd lng="pt">diel&eacute;trico</trd>
  <trd
  lng="ru">&c_d;&c_i;&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>dielektrikum</trd>,
    <trd>izolátor</trd>
  </trdgrp>
</drv>
<drv mrk="dielek.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta econ de <tld/>o.  
    </dif>
  </snc>
  <trd
  lng="be">&c_d;&c_y;&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="de">
     <trd>dielektrisch</trd>,
     <trd>nichtleitend</trd>
  </trdgrp>
  <trd lng="es">diel&eacute;ctrico</trd>
  <trd lng="fr">di&eacute;lectrique<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="hu">dielektrikus</trd>
  <trdgrp lng="pl">
    <trd>dielektryczny</trd>,
    <trd>izolacyjny<klr> (elektr.)</klr></trd>
  </trdgrp>
  <trd
  lng="ru">&c_d;&c_i;&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
