<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>polariz</rad>/i
</kap>
<drv mrk="polari.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Modifi lumajn radiojn per reflekto a&ubreve; refrakto kaj tiel akiri por
      ili novajn proprecojn.
    </dif>
  </snc>
  <trd lng="de">polarisieren</trd>
  <trd lng="hu">polariz&aacute;l</trd>
  <trd lng="nl">polariseren</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_ja;&c_r;&c_i;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="polari.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="P"/>anta.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>polariz&aacute;ci&oacute;s</trd>,
    <trd>polariz&aacute;l&oacute;</trd>
  </trdgrp>
</drv>

<drv mrk="polari.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fenomeno <tld/>i.
    </dif>
  </snc>
  <trd lng="hu">polariz&aacute;ci&oacute;</trd>
  <trd lng="nl">polarisatie</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_ja;&c_r;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="polari.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Aparato, por <tld/>i la lumon.
    </dif>
  </snc>
  <trd lng="de">Polarisator</trd>
  <trd lng="hu">polariz&aacute;tor</trd>
  <trd lng="nl">polarisator</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_ja;&c_r;&c_i;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>
</drv>
</art>
