<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>koncert</rad>/o
</kap>
<drv mrk="koncer1.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzika kunveno, dum kiu oni ludas plurajn kanta&jcirc;ojn a&ubreve; instrumentajn
      kompona&jcirc;ojn:
      <ekz>
        ludi, doni <tld/>on
        <klr>(f)</klr>;
      </ekz>
      <ekz>
        e&ccirc; la senprudentaj birdoj muzikas sian &ccirc;ielan <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Akordo de opinioj a&ubreve; ideoj:
      <ekz>
        oni a&ubreve;das nur <tld/>on de la&ubreve;doj pri li;
      </ekz>
      <ekz>
        la E&ubreve;ropa <tld/>o havas kiel bazon la akordi&gcirc;on de Germanujo kaj
        Francujo;
      </ekz>
      <ekz>
        a&ubreve;digi sian vo&ccirc;on en la E&ubreve;ropa <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">koncert</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;</trd>
</drv>
<drv mrk="koncer1.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Fari <tld/>on.
    </dif>
  </snc>
  <trd lng="ru">&c_d;&c_a;&c_t;&c_mol; &c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;</trd>
</drv>
<drv mrk="koncer1.0kafejo">
  <kap>
    <tld/>kafejo
  </kap>
  <snc>
    <dif>
      Ejo, kie oni povas trinki, a&ubreve;skultante <tld/>on
    </dif>
    <refgrp tip="sin">
      <ref cel="variet.0o">varieteo</ref>.
    </refgrp>
  </snc>
</drv>
</art>
