<vortaro>
<art mrk="$Id: koncer1.xml,v 1.34 2023/12/03 09:58:54 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>koncert</rad>/o
</kap>
<drv mrk="koncer1.0o">
  <kap><tld/>o</kap>
  <snc mrk="koncer1.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika kunveno, dum kiu oni ludas plurajn kanta&jcirc;ojn
      a&ubreve; instrumentajn kompona&jcirc;ojn:
      <ekz>
        ludi, doni <tld/>on;
      </ekz>
      <ekz>
        kvaropa <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>La bremenaj muzikistoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda salono estis plena de homoj, <klr>[&#x2026;]</klr> granda <tld/>o devis esti
        donata
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni iam vizitas la teatron a&ubreve; <tld/>on
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vendreda <tld/>o de la Filharmonio
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond</aut>
          <vrk><url ref="https://www.monato.be/2011/010147.php">Georgo</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Romeska&ubreve;</nom> studadis kompozicion, kion li devus solludi dum granda <tld/>o, en kiu
        partoprenus aliaj famaj artistoj
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        akridoj ludis sur bu&scirc;harmoniko kaj strigo frapadis sin sur la ventron,
        <klr>[&#x2026;]</klr> tio estis amuza <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        e&ccirc; la senprudentaj birdoj <klr>[&#x2026;]</klr> muzikas sian &ccirc;ielan <tld/>on
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Ne konfuzu kun <ref tip="hom" cel="koncxert.0o">kon&ccirc;erto</ref>.
    </rim>
    <trd lng="bg">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;</trd>
    <trd lng="de">Konzert</trd>
    <trd lng="es">concierto</trd>
    <trd lng="nl">concert</trd>
    <trd lng="pl">koncert</trd>
    <trd lng="pt">concerto <klr tip="amb">(audi&ccedil;&atilde;o)</klr></trd>
    <trd lng="ro">concert</trd>
    <trd lng="vo">konzed</trd>
  </snc>
  <snc mrk="koncer1.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="akord.0o.samopinieco">Akordo</ref> de opinioj a&ubreve; ideoj:
      <ekz>
        estis alveninta la momento plenumi la projekton <tld/>e elpensitan inter &scirc;i kaj
        &scirc;ia amikino
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>;
      </ekz>
      <ekz>
        kion la katolika flanko respondos al &ccirc;i lasta akuzo, en kiu historio,
        psikologio, antropologio ktp postuli&gcirc;as en koncerto kun cent aliaj sciencoj
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="https://www.monato.be/2006/008871.php">&Scirc;tato, eklezio kaj ideologia laikeco</url></vrk>
          <lok>2006</lok>
        </fnt>?
      </ekz>

      <ekz>
        oni a&ubreve;das nur <tld/>on de la&ubreve;doj pri li;
      </ekz>
      <ekz>
        la E&ubreve;ropa <tld/>o havas kiel bazon la akordi&gcirc;on
        de Germanujo kaj Francujo;
      </ekz>
      <ekz>
        a&ubreve;digi sian vo&ccirc;on en la E&ubreve;ropa <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
    <ref cel="akord.0o.samopinieco">akordo <sncref/></ref>,
    <ref cel="harmon1.0o.paco">harmonio <sncref/></ref>,
    <ref cel="konsen.inter0o">interkonsento</ref>
    </refgrp>
    <trd lng="bg">&c_s;&c_malmol;&c_g;&c_l;&c_a;&c_s;&c_i;&c_je;</trd>
    <trdgrp lng="de">
      <trd>Zusammenspiel</trd>,
      <trd>Einverst&auml;ndnis</trd>,
      <trd>Einklang</trd>,
      <trd>Konzert</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>accord</trd>,
      <trd>agreement</trd>,
      <trd>harmony</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>samenspel</trd>,
      <trd>harmonie</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_c;&c_e;&c_r;&c_t;</trd>
  <trd lng="ca">concert</trd>
  <trd lng="cs">koncert</trd>
  <trd lng="en">concert</trd>
  <trdgrp lng="fr">
    <trd>concert<klr tip="ind"> (spectacle de musique)</klr></trd>,
    <trd>r&eacute;cital</trd>,
    <trd>ch&oelig;ur<klr tip="ind"> (fig.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hangverseny</trd>,
    <trd>koncert</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B3;&#x30F3;&#x30B5;&#x30FC;&#x30C8;  <pr>&#x3053;&#x3093;&#x3055;&#x30FC;&#x3068;</pr></trd>,
    <trd>&#x6F14;&#x594F;&#x4F1A;  <pr>&#x3048;&#x3093;&#x305D;&#x3046;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x97F3;&#x697D;&#x4F1A;  <pr>&#x304A;&#x3093;&#x304C;&#x304F;&#x304B;&#x3044;</pr></trd>,
    <trd>&#x5354;&#x8ABF;  <pr>&#x304D;&#x3087;&#x3046;&#x3061;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x5354;&#x529B;  <pr>&#x304D;&#x3087;&#x3046;&#x308A;&#x3087;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="pl">koncert</trd>
  <trd lng="ro">concert</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;</trd>
  <trd lng="sk">koncert</trd>
  <trdgrp lng="zh">
    <trd>&#x6F14;&#x51FA; <pr>y&#x1CE;nch&#x16B;</pr></trd>,
    <trd>&#x97F3;&#x4E50;&#x4F1A; <pr>y&#x12B;nyu&#xE8;hu&#xEC;</pr></trd>,
    <trd>&#x6F14;&#x5531;&#x4F1A; <pr>y&#x1CE;nch&#xE0;nghu&#xEC;</pr></trd>,
    <trd>&#x534F;&#x594F;&#x66F2; <pr>xi&#xE9;z&#xF2;uq&#x1D4;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="koncer1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="muzik.0i">Muziki</ref> prezentante <tld/>on:
      <ekz>
        &ccirc;epiane <nom>Liszt</nom> <klr>[&#x2026;]</klr> fari&gcirc;is vaste konata stelulo, kiu <tld/>is en la
        tuta E&ubreve;ropo
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="https://www.monato.be/2012/010588.php">Geniulo mondcivitana</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum 10 horoj <tld/>is muzikistoj en Litovio por tiel <klr>[&#x2026;]</klr> adia&ubreve;i la
        jaron
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <lok><url ref="http://steloj.de/esperanto/monato2/006840.html">http://steloj.de/esperanto/monato2/006840.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la bando fari&gcirc;is pli konata sur la muzika scenejo de sia propra lando, kie &gcirc;i
        plurfoje <tld/>is
        <fnt>
          <bib>Monato</bib>
          <aut>Reinaldo Marcelo Ferreira</aut>
          <vrk><url ref="https://www.monato.be/2005/008664.php">Ree bela sono</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        por la festivalo venis el Francio kaj <tld/>is <nom>JoMo</nom>
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 4 (66)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_n;&c_c;&c_e;&c_r;&c_t;</ind></trd>
  <trd lng="bg">&c_i;&c_z;&c_n;&c_je;&c_s;&c_je; <ind>&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;</ind></trd>
  <trd lng="ca">fer un concert</trd>
  <trd lng="cs">koncertovat</trd>
  <trdgrp lng="de">
    <trd>konzertieren</trd>,
    <trd>ein <ind>Konzert geben</ind></trd>,
    <trd>in einem <ind>Konzert mitspielen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>give a concert</trd>,
    <trd>play a concert</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>donner un <ind>concert</ind></trd>,
    <trd>concerter<klr tip="ind"> (donner un concert)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hangversenyez</trd>,
    <trd>koncertezik</trd>
  </trdgrp>
  <trd lng="ja">&#x30B3;&#x30F3;&#x30B5;&#x30FC;&#x30C8;&#x3092;&#x958B;&#x304F;  <pr>&#x3053;&#x3093;&#x3055;&#x30FC;&#x3068;&#x3092;&#x3042;&#x304F;</pr></trd>
  <trdgrp lng="nl">
    <trd>een <ind>concert</ind> geven</trd>,
    <trd>concerteren</trd>
  </trdgrp>
  <trd lng="pl">koncertowa&cacute;</trd>
  <trd lng="ro">concerta</trd>
  <trd lng="ru">&c_d;&c_a;&c_t;&c_mol;
<ind>&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;</ind></trd>
  <trd lng="sk">koncertova&tcaron;</trd>
  <trd lng="zh">&#x6F14;&#x51FA; <pr>y&#x1CE;nch&#x16B;</pr></trd>
</drv>

<drv mrk="koncer1.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Kompona&jcirc;o aran&gcirc;ita por prezento en <tld/>o:
      <ekz>
        li dedi&ccirc;is sin al muziko kaj komponis kelkajn <tld/>a&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>W. M. A. de Smet</aut>
          <vrk><url ref="https://www.monato.be/2004/008418.html">Edward Croegaert, la forgesita lingvokreanto</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        alportu vian violonon post la teman&gcirc;o, ni ludos malgrandan <tld/>a&jcirc;on
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Konzertst&uuml;ck</trd>
  <trd lng="en">concert piece</trd>
  <trd lng="fr">pi&egrave;ce de concert</trd>
  <trd lng="ja">&#x6F14;&#x594F;&#x66F2;&#x76EE;  <pr>&#x3048;&#x3093;&#x305D;&#x3046;&#x304D;&#x3087;&#x304F;&#x3082;&#x304F;</pr></trd>
</drv>

<drv mrk="koncer1.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">ARKI</uzo>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Loko por <tld/>oj, ordinare <ref tip="super" cel="konstr.0ajxo">konstrua&jcirc;o</ref> kun akustike
      ta&ubreve;ga salono a&ubreve; amfiteatro:
      <ekz>
        mi solludos anta&ubreve; la publiko en la granda <tld/>ejo
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li studadis la muzikon kaj iafoje, post laciga laboro, vizitis grandan
        <tld/>ejon de la tiea filharmonio
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la granda halo funkcias kiel <tld/>ejo
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2013/010891.php">De praavo al arto</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[lia]</klr> solena <tld/>o plenigis la presti&gcirc;an zagreban <tld/>ejon
        <nom>Glazbeni zavod</nom>
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 7 (105)</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="kaf.koncert0ejo"><tld/>kafejo</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_c;&c_e;&c_r;&c_t;&c_n;&c_a;&c_ja; &c_z;&c_a;&c_l;&c_ja;</trd>
  <trd lng="bg">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;&c_n;&c_a; &c_z;&c_a;&c_l;&c_a;</trd>
  <trd lng="ca">sala de concerts</trd>
  <trdgrp lng="cs">
    <trd>koncertn&iacute; m&iacute;stnost</trd>,
    <trd>koncertn&iacute; s&aacute;l</trd>
  </trdgrp>
  <trd lng="en">concert hall</trd>
  <trd lng="fr">salle de <ind>concert</ind></trd>
  <trd lng="hu">hangversenyterem</trd>
  <trdgrp lng="ja">
    <trd>&#x30B3;&#x30F3;&#x30B5;&#x30FC;&#x30C8;&#x4F1A;&#x5834;  <pr>&#x3053;&#x3093;&#x3055;&#x30FC;&#x3068;&#x304B;&#x3044;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x6F14;&#x594F;&#x4F1A;&#x5834;  <pr>&#x3048;&#x3093;&#x305D;&#x3046;&#x304B;&#x3044;&#x3058;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>concertgebouw</trd>,
    <trd>concertzaal</trd>
  </trdgrp>
  <trd lng="pl">sala koncertowa</trd>
  <trd lng="ro">sala de concert</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;&c_n;&c_y;&c_j;
&c_z;&c_a;&c_l;</trd>
  <trd lng="sk">koncertn&aacute; s&aacute;la</trd>
  <trd lng="zh">&#x97F3;&#x4E50;&#x5385; <pr>y&#x12B;nyu&#xE8;t&#x12B;ng</pr></trd>
</drv>
</art>
