<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>koncert</rad>/o
</kap>
<drv mrk="koncer1.0o">
  <kap><tld/>o</kap>
  <snc mrk="koncer1.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika kunveno, dum kiu oni ludas plurajn kanta&jcirc;ojn a&ubreve; instrumentajn
      kompona&jcirc;ojn:
      <ekz>
        ludi, doni <tld/>on;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        e&ccirc; la senprudentaj birdoj muzikas sian &ccirc;ielan <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Konzert</trd>
    <trd lng="es">concierto</trd>
    <trd lng="vo">konzed</trd>
  </snc>
  <snc mrk="koncer1.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Akordo de opinioj a&ubreve; ideoj:
      <ekz>
        oni a&ubreve;das nur <tld/>on de la&ubreve;doj pri li;
      </ekz>
      <ekz>
        la E&ubreve;ropa <tld/>o havas kiel bazon la akordi&gcirc;on de Germanujo kaj
        Francujo;
      </ekz>
      <ekz>
        a&ubreve;digi sian vo&ccirc;on en la E&ubreve;ropa <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Einverst&auml;ndnis</trd>,
      <trd>Einklang</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_c;&c_e;&c_r;&c_t;</trd>
  <trd lng="cs">koncert</trd>
  <trd lng="fr">concert</trd>
  <trd lng="nl">concert</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;</trd>
</drv>

<drv mrk="koncer1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="muzik.0i">Muziki</ref> prezentante <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_n;&c_c;&c_e;&c_r;&c_t;</ind></trd>
  <trdgrp lng="de">
    <trd>konzertieren</trd>,
    <trd>ein <ind>Konzert geben</ind></trd>,
    <trd>in einem <ind>Konzert mitspielen</ind></trd>
  </trdgrp>
  <trd lng="fr">donner un <ind>concert</ind></trd>
  <trd lng="nl">concerteren</trd>
  <trd lng="ru">&c_d;&c_a;&c_t;&c_mol; &c_k;&c_o;&c_n;&c_c;&c_je;&c_r;&c_t;</trd>
</drv>

<drv mrk="koncert.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">ARKI</uzo>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Loko por <tld/>oj, ordinare <ref tip="super"
      cel="konstr.0ajxo">konstrua&jcirc;o</ref> kun akustike ta&ubreve;ga salono a&ubreve;
      amfiteatro.
    </dif>
  </snc>
  <trd lng="fr">salle de concert</trd>
</drv>

<drv mrk="koncer1.0kafejo">
  <kap><tld/>kafejo</kap>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <ref tip="dif" cel="variet.0o">varieteo</ref>
  </snc>
  <trd lng="de">Konzertcaf&eacute;</trd>
  <trdgrp lng="fr">
    <trd>caf&eacute;-chantant</trd>,
    <trd>caf&eacute;-concert</trd>
  </trdgrp>
  <trd lng="nl">caf&eacute;-chantant</trd>
</drv>
</art>
