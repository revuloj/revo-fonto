<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>celadon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="celadon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Hele bluverda <ref tip="super" cel="glazur.0o.TEK">glazuro</ref>
      tipa de iuj &ccirc;inaj kaj koreaj ceramika&jcirc;oj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_l;&c_ja;&c_d;&c_o;&c_n; <klr>(&c_v;&c_ib;&c_d; &c_g;&c_l;&c_a;&c_z;&c_u;&c_r;&c_y; &c_n;&c_a; &c_k;&c_ib;&c_t;&c_a;&c_j;&c_s;&c_k;&c_a;&c_j; &c_a;&c_l;&c_mol;&c_b;&c_o; &c_k;&c_a;&c_r;&c_e;&c_j;&c_s;&c_k;&c_a;&c_j; &c_k;&c_je;&c_r;&c_a;&c_m;&c_ib;&c_c;&c_y;)</klr></trd>
  <trdgrp lng="fr">
    <trd>c&eacute;ladon<klr> (subst., gla&ccedil;ure)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szeladonm&aacute;z</trd>,
    <trd>tengerz&ouml;ld m&aacute;z</trd>
  </trdgrp>
</drv>

<drv mrk="celadon.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Kun aspekto, koloro de <tld/>o:
      <ekz>
        tiaj <tld/>verdaj porcelana&jcirc;oj karakterizi&gcirc;as per delikateco,
        maldikeco kaj kristala glazuro
        <fnt><bib>EncCxi</bib> <lok>&ccirc;ap. &leftquot;porcelana&jcirc;oj&rightquot;, p.
        212a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_a;&c_g;&c_a; &c_s;&c_ib;&c_n;&c_ja;-&c_z;&c_ja;&c_l;&c_jo;&c_n;&c_a;&c_g;&c_a; &c_k;&c_o;&c_l;&c_je;&c_r;&c_u; <klr>(&c_k;&c_o;&c_l;&c_je;&c_r;&c_u; &c_s;&c_e;&c_l;&c_ja;&c_d;&c_o;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="fr">
    <trd>c&eacute;ladon<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tengerz&ouml;ld</trd>,
    <trd>halv&aacute;nyz&ouml;ld</trd>
  </trdgrp>
</drv>
</art>

