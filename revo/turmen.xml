<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: turmen.xml,v 1.26 2024/02/17 01:22:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>turment</rad>/i
</kap>
<drv mrk="turmen.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="turmen.0i.korpe">
    <dif>
      Suferigi iun per kruelaj korpaj suferoj:
      <ekz>
        malbonaj infanoj amas <tld/>i bestojn
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        se malsato <tld/>as, lupo timon ne sentas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &scirc;in terure <tld/>is la frosto
        <fnt><bib>Fab1</bib>, <lok>Elinjo-fingreto</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_u;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>martelen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dokucza&cacute;</trd>,
      <trd>dr&eogonek;czy&cacute;</trd>,
      <trd>gn&eogonek;bi&cacute;</trd>,
      <trd>katowa&cacute;</trd>,
      <trd>m&eogonek;czy&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="turmen.0i.anime">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Suferigi iun per moralaj suferoj:
      <ekz>
        maltrankvilo <tld/>is lin<fnt>B</fnt>;
      </ekz>
      <ekz>
        kiun mal&gcirc;ojo ne <tld/>is, tiun &gcirc;ojo ne
        sentas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>ita per konstantaj duboj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj estis <tld/>ataj de la timo, ke...
      </ekz>
      <ekz>
        tro da bono ne <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i iun per postuloj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxagre">&ccirc;agreni</ref>,
      <ref cel="cxikan.0i">&ccirc;ikani</ref>,
      <ref cel="dolor.0igi">dolorigi</ref>,
      <ref cel="trankv.mal0igi">maltrankviligi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_u;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_je;&c_p;&c_a;&c_k;&c_o;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_r;&c_o;&c_ib;&c_c;&c_mol;
      <klr>(&c_s;&c_e;&c_r;&c_c;&c_a;,
      &c_d;&c_u;&c_sh;&c_u;)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kwellen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dokucza&cacute;</trd>,
      <trd>dr&eogonek;czy&cacute;</trd>,
      <trd>gn&eogonek;bi&cacute;</trd>,
      <trd>m&eogonek;czy&cacute;</trd>,
      <trd>n&eogonek;ka&cacute;</trd>,
      <trd>trapi&cacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>mo&rcaron;it</trd>,
    <trd>mu&ccaron;it</trd>,
    <trd>sou&zcaron;it</trd>,
    <trd>su&zcaron;ovat</trd>,
    <trd>tr&aacute;pit</trd>
  </trdgrp>
  <trd lng="de">qu&auml;len</trd>
  <trd lng="en">torment</trd>
  <trdgrp lng="es">
    <trd>atormentar</trd>,
    <trd>mortificar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire souffrir</trd>,
    <trd>tenailler</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&ouml;t&ouml;r</trd>,
    <trd>k&iacute;noz</trd>,
    <trd>sanyargat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6FC0;&#12375;&#12356;&#x82E6;&#x75DB;&#12434;&#x4E0E;&#12360;&#x308B; 
    <pr>&#x306F;&#12370;&#12375;&#12356;&#x304F;&#12388;&#12358;&#12434;&#12354;&#x305F;&#12360;&#x308B;</pr></trd>,
    <trd>&#x82E6;&#12375;&#12417;&#x308B;  <pr>&#x304F;&#x308B;&#12375;&#12417;&#x308B;</pr></trd>,
    <trd>&#12356;&#12376;&#12417;&#x308B;</trd>,
    <trd>&#12373;&#12356;&#x306A;&#12416;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>atormentar</trd>,
    <trd>torturar</trd>,
    <trd>supliciar</trd>,
    <trd>mortificar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_u;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_s;&c_t;&c_ja;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_z;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tr&aacute;pi&tcaron;</trd>,
    <trd>tr&yacute;zni&tcaron;</trd>,
    <trd>t&yacute;ra&tcaron; <klr>(du&scaron;evne i&nbsp;telesne)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_u;&c_ch;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43A;&c_a;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43C;&#x43E;&c_r;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="turmen.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Da&ubreve;re suferiga:
      <ekz>
        &scirc;i forpu&scirc;adis ilin de si kiel ion <klr>...</klr> tro
        pezan por
        &scirc;iaj fortoj, tro <tld/>an por &scirc;ia koro
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        en aliaj okazoj ekzistas <tld/>a sento de sekeco kaj la okuloj
        povas esti malfermataj nur kun malfacileco
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj, kronika
        katara konjunktivito, (el la libro de profesoro E.
        Fuchs)</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_u;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_u;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_d;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">mu&ccaron;iv&yacute;</trd>
  <trd lng="es">tormentoso</trd>
  <trd lng="fr">tourmentant</trd>
  <trdgrp lng="hu">
    <trd>gy&ouml;tr&odblac;</trd>,
    <trd>k&iacute;nz&oacute;</trd>,
    <trd>gy&ouml;trelmes</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x82E6;&#12375;&#12417;&#x308B;  <pr>&#x304F;&#x308B;&#12375;&#12417;&#x308B;</pr></trd>,
    <trd>&#x60A9;&#x307E;&#12377;  <pr>&#x306A;&#12420;&#x307E;&#12377;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
     <trd>dokuczliwy</trd>,
     <trd>dr&eogonek;cz&aogonek;cy</trd>,
     <trd>m&eogonek;cz&aogonek;cy</trd>,
     <trd>uci&aogonek;&zdot;liwy</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_u;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">mu&ccaron;iv&yacute;</trd>
  <trd lng="uk">&c_b;&#x43E;&#x43B;&c_ib;&c_s;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="turmen.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Suferoj ricevataj de iu a&ubreve; io:
      <ekz>
        <tld/>oplenaj formala&jcirc;oj;
      </ekz>
      <ekz>
        la bramano ne povis elporti la <tld/>ojn, ricevatajn de sia
        edzino
        <fnt><bib>FK</bib>, <lok>2, fabeloj kaj legendoj, Karagara,
        hinda
        fabelo, rakontis F. V. L.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_u;&c_t;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>muka</trd>,
    <trd>sou&zcaron;en&iacute;</trd>,
    <trd>tr&aacute;pen&iacute;</trd>,
    <trd>tr&yacute;ze&ncaron;</trd>
  </trdgrp>
  <trd lng="es">tormento</trd>
  <trdgrp lng="fr">
    <trd>affres</trd>,
    <trd>tourment</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&ouml;trelem</trd>,
    <trd>k&iacute;n</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8CAC;&#x82E6;  <pr>&#x305B;&#12417;&#x304F;</pr></trd>,
    <trd>&#x82E6;&#x75DB;  <pr>&#x304F;&#12388;&#12358;</pr></trd>,
    <trd>&#x82E6;&#x60A9;  <pr>&#x304F;&#x306E;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dolegliwo&sacute;&cacute;</trd>,
    <trd>udr&eogonek;ka</trd>,
    <trd>katusze</trd>,
    <trd>m&eogonek;ka</trd>,
    <trd>utrapienie</trd>,
    <trd>zgryzota</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_u;&#769;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_u;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tr&aacute;penie</trd>,
    <trd>tr&yacute;znenie</trd>,
    <trd>t&yacute;ranie</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&c_u;&#x43A;&c_a;</trd>
</drv>

<drv mrk="turmen.0egi">
  <kap><tld/>egi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi grandajn dolorojn:
      <ekz>
        per kapricoj &scirc;i &ccirc;iujn <tld/>egis kaj fine
        for&jcirc;etis, a&ubreve; ili mem
        fu&gcirc;is de &scirc;i
        <fnt><bib>SatirRak</bib>, <lok>tria pre&gcirc;o de
        Pygmalion</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="martir.0o">martiro</ref>,
      <ref cel="tortur.0i">torturi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_a;&c_r;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>mo&rcaron;it</trd>,
    <trd>mu&ccaron;it</trd>,
    <trd>sou&zcaron;it</trd>,
    <trd>su&zcaron;ovat</trd>,
    <trd>tr&aacute;pit</trd>
  </trdgrp>
  <trd lng="fr">tourmenter</trd>
  <trd lng="hu">hal&aacute;lra <ind>gy&ouml;t&ouml;r</ind></trd>
  <trdgrp lng="pl">
    <trd>pastwi&cacute; si&eogonek;</trd>,
    <trd>torturowa&cacute;</trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_t;&c_ja;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_y;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tr&aacute;pi&tcaron;</trd>,
    <trd>tr&yacute;zni&tcaron;</trd>,
    <trd>t&yacute;ra&tcaron; <klr>(du&scaron;evne i&nbsp;telesne)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="turmen.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti trafata de suferoj:
      <ekz>
        kiel gravedulino <klr>...</klr> <tld/>i&gcirc;as kaj krias en
        siaj
        doloroj
        <fnt><bib>MT</bib>, <lok>&Jes; 26:17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_u;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_u;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">souffrir</trd>
  <trdgrp lng="hu">
    <trd>gy&ouml;tr&odblac;dik</trd>,
    <trd>k&iacute;nl&oacute;dik</trd>,
    <trd>senyved</trd>
  </trdgrp>
  <trd lng="es">atormentarse</trd>
  <trdgrp lng="ja">
    <trd>&#x82E6;&#x60A9;&#12377;&#x308B;  <pr>&#x304F;&#x306E;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x82E6;&#12375;&#12416;  <pr>&#x304F;&#x308B;&#12375;&#12416;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dr&eogonek;czy&cacute; si&eogonek;</trd>,
    <trd>zamartwia&cacute; si&eogonek;</trd>,
    <trd>m&eogonek;czy&cacute; si&eogonek;</trd>,
    <trd>trapi&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="uk">&#x43C;&c_u;&c_ch;&c_i;&c_t;&c_i;&c_s;&#x44F;</trd>
</drv>

<drv mrk="turmen.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="turmen.el0i.plene">
    <dif>
      Plene <tld/>i &gcirc;is senrezisto a&ubreve; senespero:
      <ekz>
        per sen&ccirc;esa enlo&gcirc;igado de soldatoj li tute nin
        el<tld/>is, ni ne
        scias jam kiel nin savi
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 10a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_m;&c_u;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_m;&c_u;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="fr">&eacute;puiser par la souffrance</trd>
    <trd lng="hu">v&eacute;gs&odblac;kig
    <ind>elgy&ouml;t&ouml;r</ind></trd>
    <trd lng="pl">wym&eogonek;czy&cacute;</trd>
    <trd lng="ru">&c_i;&c_z;&c_m;&c_u;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x82E6;&#12375;&#12417;&#x629C;&#x304F;  <pr>&#x304F;&#x308B;&#12375;&#12417;&#x306C;&#x304F;</pr></trd>,
    <trd>&#12356;&#12376;&#12417;&#x629C;&#x304F;  <pr>&#12356;&#12376;&#12417;&#x306C;&#x304F;</pr></trd>,
    <trd>&#12422;&#12377;&#x308A;&#x53D6;&#x308B;  <pr>&#12422;&#12377;&#x308A;&#12392;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_i;&#x43C;&c_u;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>
</art>
