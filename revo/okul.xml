<drv mrk="okul.akr0a">
  <kap>
    akr<tld/>a,
    <var><kap>akra<tld/>a</kap></var>
  </kap>
  <snc mrk="okul.akr0a.KOMUNE">
    <dif>
      Havanta akrajn <tld/>ojn, tio estas tre bonan vidpovon:
      <ekz>
        pafarkon ili sciis bone utiligi, &ccirc;ar ili estis akr<tld/>aj
        <fnt>
          <aut>J. R. R. Tolkien, trad. C. Gledhill</aut>
          <vrk>La Hobito</vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi aspektas kiel vera husaroficiro! kaj tion rimarkas anka&ubreve; akra<tld/>a tajloro
        <fnt>
          <aut>A. Salga, trad. J. Horvath</aut>
          <vrk><url ref="http://mek.oszk.hu/21100/21145/21145.pdf">Esperiganto</url></vrk>
          <lok>2020</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ayant un <ind>&oelig;il de lynx</ind></trd>
</drv>

<drv mrk="okul.propr0e">
  <kap>propr<tld/>e, <var><kap>propra<tld/>e</kap></var></kap>
  <snc mrk="okul.propr0e.ateste">
    <dif>
      Vidante mem, &ccirc;eestante mem:
      <ekz>
        mi mem iris tien kaj propr<tld/>e vidis
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://esperanto.org.uk/eldonoj/piron/tekstoj/Tien.html">
          Tien</url></vrk>,
          <lok>1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidis propra<tld/>e la deputiton <nom>Lukavsky</nom>
        <fnt>
          <aut>K. &Ccirc;apek, trad. M. Malovec</aut>,
          <vrk>La&ubreve;do de &jcirc;urnaloj</vrk>,
          <bib>LOdE</bib>,
          <lok>2000:6 (68)</lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_s;&c_malmol;&c_s; &c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_je;&c_n;&c_i; &c_o;&c_ch;&c_i;</trd>
  <trd lng="es">con los propios ojos</trd>
  <trd lng="fr">de ses propres yeux</trd>
  <trd lng="hu">saj&aacute;t <ind>szem&eacute;vel</ind></trd>
  <trdgrp lng="it">
    <trd>con i propri <ind>occhi</ind></trd>,
    <trd>di persona</trd>
  </trdgrp>
  <trd lng="nl"><ind>oog</ind>: met eigen ogen</trd>
</drv>

