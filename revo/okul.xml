<drv mrk="okul.0a">
  <kap><tld/>a</kap>
  <snc mrk="okul.0a.ANA">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a malsano, kuracisto, medikamento.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_o;&c_ch;&c_je;&c_n;</trd>
  <trd lng="br">-<ind>lagad</ind></trd>
  <trdgrp lng="cs">
    <trd>optick&yacute;</trd>,
    <trd>o&ccaron;it&yacute;</trd>,
    <trd>o&ccaron;n&iacute;</trd>,
    <trd>zrakov&yacute;</trd>
  </trdgrp>
  <trd lng="de">Augen-</trd>
  <trd lng="el">&omikron;&phi;&theta;&alfa;&lambda;&my;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
