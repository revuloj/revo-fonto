<drv mrk="okul.0i">
  <kap><tld/>i, <var><kap><tld/>umi</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="okul.0i.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="sin" cel="rigard.0i.okule">Rigardi</ref> iel videble
      a&ubreve; 
          
      insiste, ekzemple pro deziro:
      <ekz>
        &scirc;i ekfiksis la rigardon kaj malvarme <tld/>is la knabon
        <fnt>
          <aut>I. Asztalos</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr35/malfacileu.html">
          Kiel malfacile estas sekreti la amon</url></vrk>,
          <lok>Nica Literatura Revuo, 1958-05 &gcirc;is 06, numero 3/5,
          p.
          180-186a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="palpeb.0umi">palpebrumi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_l;&c_u;&c_p;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_a;&c_r;&c_e;&c_sh;&c_ch;&c_v;&c_a;&c_c;&c_c;&c_a;
  </trd>
  </trdgrp>
  <trd lng="br">ober <ind>lagadenn</ind></trd>
  <trdgrp lng="cs">
    <trd>koukat</trd>,
    <trd>vyvalovat o&ccaron;i</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>&auml;ugeln</trd>,
     <trd><klr>(jmd.) </klr>Augen machen</trd>,
     <trd><klr>(jmd.) </klr>sch&ouml;ne <ind>Augen machen</ind></trd>
  </trdgrp>
  <trd lng="en">ogle</trd>
  <trdgrp lng="es">
    <trd>ojear</trd>,
    <trd>echar un vistazo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lorgner</trd>,
    <trd>loucher sur</trd>,
    <trd>zyeuter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kacsint</trd>,
    <trd>szemez</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fissare<klr> (guardare)</klr></trd>,
    <trd>guardare<klr> (con espressione o fissamente)</klr></trd>,
    <trd>occhieggiare</trd>,
    <trd>addocchiare</trd>
   </trdgrp>
  <trd lng="nl">lonken</trd>
  <trd lng="pl">zerka&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_a;&c_z;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_a;&c_r;&c_a;&c_shch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>civie&tcaron;</trd>,
    <trd>ok&aacute;li&tcaron;</trd>,
    <trd>vyva&lcaron;ova&tcaron; o&ccaron;i</trd>,
    <trd>z&iacute;za&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="okul.0egi">
  <kap><tld/>egi</kap>
  <snc mrk="okul.0egi.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Rigardadi per insistaj, grandigitaj okuloj:
      <ekz>
        li sidis sur verando el onda lado, tenis en la dikaj fingroj
        glason 
        el dika vitro kaj trinkis kaj kra&ccirc;is kaj <tld/>egis el sub
        la
        tufaj 
        brovoj la flavajn magrajn kokinojn    
        <fnt>
          <bib>Salamandroj</bib>,
          <lok>libro 1a, &ccirc;apitro 1a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">civ&ecaron;t</trd>
  <trd lng="fr">zyeuter</trd>
  <trd lng="sk">civie&tcaron;</trd>
</drv>

