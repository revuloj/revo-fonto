<drv mrk="okul.0a">
  <kap><tld/>a</kap>
  <snc mrk="okul.0a.ANA">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a malsano, kuracisto, medikamento;
      </ekz>
      <ekz>
        blanka&jcirc;o <tld/>a vidi&gcirc;as
        <fnt>
          <bib>SrT</bib>
          <lok>Libro XI.</lok>
        </fnt>;
      </ekz>
      <ekz>
        en momento, en <tld/>a ekmovo
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 15:52</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a iriso, kovranta la pupilon
        <fnt>
          <bib>Monato</bib>
          <aut>J&aacute;nos S&aacute;rk&ouml;zi</aut>
          <vrk><url ref="&Monato;/2005/008401.php">La fenomeno de ru&gcirc;aj okuloj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        niaj bu&scirc;-al-bu&scirc;aj scioj kaj <tld/>aj atestoj estas tiom fidindaj <klr>[&#x2026;ke
        apena&ubreve;]</klr> vi esprimas e&ccirc; la plej malgrandan dubon pri ies ajn aserto
        <fnt>
          <bib>Kon11</bib>
          <lok>Scienco</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_o;&c_ch;&c_je;&c_n;</trd>
  <trd lng="br">-<ind>lagad</ind></trd>
  <trdgrp lng="cs">
    <trd>optick&yacute;</trd>,
    <trd>o&ccaron;it&yacute;</trd>,
    <trd>o&ccaron;n&iacute;</trd>,
    <trd>zrakov&yacute;</trd>
  </trdgrp>
  <trd lng="de">Augen-</trd>
  <trd lng="el">&omikron;&phi;&theta;&alfa;&lambda;&my;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">ocular</trd>
  <trd lng="es">ocular</trd>
