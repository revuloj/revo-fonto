<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>okul</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="okul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="okul.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Organo de la vidkapablo:
      <ekz>
        bluaj, migdalformaj, lacaj, blue kadritaj, ru&gcirc;randaj <tld/>oj;
      </ekz>
      <ekz>
        sablero falis en mian <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        doni, fari signojn per la <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        grimaci per la <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        postuli <tld/>on pro <tld/>o, denton pro dento<fnt>Z</fnt>
        <klr>(egalan repagon)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kiu tro alten rigardon direktas
        tiu tre balda&ubreve; <tld/>ojn difektas
        <klr>(ambiciulo malsukcesos)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="en">eye</trd>
  </snc>
  <snc mrk="okul.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tiu sama organo, konsiderata kiel ilo de atento kaj intereso:
      <ekz>
        for de l' <tld/>oj, for de la koro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        teni la <tld/>ojn en stre&ccirc;o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        direkti siajn <tld/>ojn al iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        sekvi iun per la <tld/>o;
      </ekz>
      <ekz>
        <tld/>o al <tld/>o
        <klr>(viza&gcirc;o al viza&gcirc;o, vidalvide)</klr>;
      </ekz>
      <ekz>
        anta&ubreve;, sub la <tld/>oj de iu;
      </ekz>
      <ekz>
        verkante la regularon, mi havis anta&ubreve; la <tld/>oj (priatentis)
        la
        specialan situacion de la afero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        foriru de anta&ubreve; miaj <tld/>oj;
      </ekz>
      <ekz>
        &ccirc;ies <tld/>oj kun teruro deturni&gcirc;is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kovri al si la <tld/>ojn kontra&ubreve; (por ne vidi) la ekzistanta
        dan&gcirc;ero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        malfermi la <tld/>ojn al iu
        <klr>(forigi la iluziojn kaj montri la efektivan reala&jcirc;on,
        <ref tip="vid" cel="vual.0o">vualo</ref>)</klr>;
      </ekz>
      <ekz>
        tio estas evidenta kaj falas al &ccirc;iu en la
        <tld/>ojn<fnt>Z</fnt>
        (<refgrp tip="vid">
          <ref cel="fal.0i">fali 4</ref>,
          <ref cel="frap.0i">frapi</ref>).
        </refgrp>
      </ekz>
      <ekz>
        subskribi ion kun fermitaj <tld/>oj
        <klr>(ne kontrolante)</klr>;
      </ekz>
      <ekz>
        &scirc;uti polvon en la <tld/>ojn
        (<ref tip="vid" cel="polv.0o">polvo</ref>)
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        iafoje oni devas <tld/>on fermeti
        <klr>(ne esti tro severa)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        fermu la <tld/>ojn kontra&ubreve; negravaj plendoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne kredu al orelo, kredu al <tld/>o
        <klr>(ne onidiron, sed propran rimarkon)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        en ies <tld/>oj<fnt>Z</fnt>
        <klr>(la&ubreve; lia vidmaniero, lia opinio)</klr>;
      </ekz>
      <ekz>
        <tld/>o de mastro <klr>(kontrolo de interesato)</klr>
        pli efikas ol beno de pastro<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="okul.0o.rigardo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tiu organo, konsiderata kiel esprimanta diversajn animajn sentojn:
      <ekz>
        rigardi iun per enviema, malama, voluptema <tld/>o;
      </ekz>
      <ekz>
        avidege &jcirc;eti <tld/>on al<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tute jam sata sed <tld/>o malsata<fnt>Z</fnt>;
      </ekz>
      <ekz>
        timo havas grandajn <tld/>ojn
        <klr>(gravigas &ccirc;ion)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        havi <tld/>ojn pli grandajn ol la ventro<fnt>Z</fnt>
        <klr>(preni el plado pli, ol kiom oni povas man&gcirc;i)</klr>;
      </ekz>
      <ekz>
        <ind>de l' koro spegulo estas la <tld/>o</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">a <ind>szem a l&eacute;lek t&uuml;kre</ind></trd>        
      </ekz>
      <ekz>
        ne iru <tld/>oj super la frunto<fnt>Z</fnt>
        <klr>(ne fieru)</klr>;
      </ekz>
      <ekz>
        &scirc;i pla&ccirc;is al liaj <tld/>oj<fnt>Z</fnt>
        <klr>(li ekamis &scirc;in)</klr>;
      </ekz>
      <ekz>
        Noa akiris pla&ccirc;on en la <tld/>oj de la
        Eternulo<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>                    
  <trd lng="be">&c_v;&c_o;&c_k;&c_a;</trd>
  <trd lng="bg">&c_o;&c_k;&c_o;</trd>
  <trd lng="br">lagad</trd>
  <trd lng="cs">oko</trd>
  <trd lng="de">Auge</trd>
  <trd lng="el">&my;&alfa_ton;&tau;&jota;</trd>
  <trd lng="es">ojo</trd>
  <trd lng="fr">&oelig;il</trd>
  <trd lng="hu">szem</trd>  
  <trd lng="hr">oko</trd>
  <trd lng="nl">oog</trd>
  <trd lng="pl">oko</trd>
  <trd lng="pt">olho</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_a;&c_z;</trd>,
    <trd>&c_o;&c_k;&c_o;</trd>
  </trdgrp>
  <trd lng="sv">&ouml;ga</trd>
  <trd lng="tr">g&ouml;z</trd>
  <trd lng="vo">log</trd>
</drv>

<drv mrk="okul.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a malsano, kuracisto, medikamento.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_ch;&c_n;&c_y;</trd>   
  <trd lng="br">-<ind>lagad</ind></trd>
  <trd lng="de">Augen-</trd>
  <trd lng="el">&omikron;&phi;&theta;&alfa;&lambda;&my;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">ocular</trd>
  <trd lng="es">ocular</trd>
  <trd lng="fr">oculaire</trd>
  <trd lng="hu">szem-</trd>
  <trd lng="pl">oczny</trd>
  <trd lng="pt">ocular</trd>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_z;&c_n;&c_o;&c_j;</trd>
</drv>

<drv mrk="okul.0i">
  <kap><tld/>i, <var><kap><tld/>umi</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="okul.0i.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="sin" cel="rigard.0i.okule">Rigardi</ref> iel videble a&ubreve;
      
      insiste, ekzemple pro deziro:
  <ekz>
    &scirc;i ekfiksis la rigardon kaj malvarme <tld/>is la knabon
    <fnt>
      <aut>I. Asztalos</aut>,
      <vrk><url
      ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr35/malfacileu.html">Kiel
      malfacile estas sekreti la amon</url></vrk>,
      <lok>Nica Literatura Revuo, 1958-05 &gcirc;is 06, numero 3/5, p.
      180-186a</lok>
    </fnt>.
  </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="palpeb.0umi">palpebrumi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_l;&c_u;&c_p;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_t;&c_a;&c_r;&c_e;&c_sh;&c_ch;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>   
  <trd lng="br">ober <ind>lagadenn</ind></trd>
  <trdgrp lng="de">
     <trd>&auml;ugeln</trd>,
     <trd><klr>(jmd.) </klr>Augen machen</trd>,
     <trd><klr>(jmd.) </klr>sch&ouml;ne <ind>Augen machen</ind></trd>
  </trdgrp>
  <trd lng="en">ogle</trd>
  <trdgrp lng="fr">
    <trd>lorgner</trd>,
    <trd>loucher sur</trd>,
    <trd>zyeuter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kacsint</trd>,
    <trd>szemez</trd>
  </trdgrp>  
  <trd lng="nl">lonken</trd>
  <trd lng="pl">zerka&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_l;&c_a;&c_z;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_a;&c_r;&c_a;&c_shch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="okul.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      <tld/>kuracisto
    </dif>
    <trdgrp lng="en">
      <trd>oculist</trd>,
      <trd>ophthamologist</trd>
  </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="optik.0isto">optikisto</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_a;&c_k;&c_u;&c_l;&c_ib;&c_s;&c_t;</trd>,
    <trd>&c_a;&c_f;&c_t;&c_a;&c_l;&c_mol;&c_m;&c_o;&c_l;&c_ja;&c_g;</trd>
  </trdgrp>   
  <trd lng="br">lagadour</trd>
  <trd lng="de">Augenarzt</trd>
  <trd lng="el">&omikron;&phi;&theta;&alfa;&lambda;&my;&jota_ton;&alfa;&tau;&rho;&omikron;&sigma_fina;</trd>
  <trd lng="es">oculista</trd>
  <trdgrp lng="fr">
    <trd>oculiste</trd>,
    <trd>ophtalmologiste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szem&eacute;sz</trd>,
    <trd>szemorvos</trd>
  </trdgrp>  
  <trd lng="nl">oogarts</trd>
  <trd lng="pl">okulista</trd>
  <trdgrp lng="pt">
    <trd>oculista</trd>,
    <trd>oftalmologista</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_k;&c_u;&c_l;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_o;&c_f;&c_t;&c_a;&c_l;&c_mol;&c_m;&c_o;&c_l;&c_o;&c_g;</trd>
  </trdgrp>
</drv>

<drv mrk="okul.propra0e">
  <kap>propra<tld/>e</kap>
  <snc mrk="okul.propra0e.ateste">
    <dif>
      Vidante mem, &ccirc;eestante mem:
      <ekz>
        mi vidis propra<tld/>e la deputiton Lukavsky
        <fnt>
          <aut>K. &Ccirc;apek, trad. M. Malovec</aut>,
          <vrk>La&ubreve;do de &jcirc;urnaloj</vrk>,
          <bib>LOdE</bib>,
          <lok>jaro 2000a, numero 6a (68)</lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">de ses propres yeux</trd>
</drv>

<drv mrk="okul.unu0a">
  <kap>unu<tld/>a</kap>
  <snc>
    <dif>
      Perdinta la kapablon uzi unu el siaj <tld/>oj.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="blind.0a">blinda</ref>,
    <ref cel="strab">straba</ref>.
  </refgrp>
  <trd lng="be">&c_a;&c_d;&c_n;&c_a;&c_v;&c_o;&c_k;&c_ib;</trd>  
  <trd lng="br">born<klr tip="ind"> (ag.)</klr></trd>
  <trd lng="de">ein&auml;ugig</trd>
  <trd lng="en">one-eyed</trd>
  <trd lng="es">tuerto</trd>
  <trd lng="fr">borgne</trd>
  <trd lng="hu">f&eacute;lszem&udblac;</trd>  
  <trd lng="nl">eenogig</trd>
  <trd lng="pl">jednooki</trd>
  <trd lng="ru">&c_o;&c_d;&c_n;&c_o;&c_g;&c_l;&c_a;&c_z;&c_y;&c_j;</trd>
  <trd lng="sv">en&ouml;gd</trd>
</drv>

<drv mrk="okul.0globo">
  <kap><tld/>globo</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Globo formita de la <tld/>o kaj enprofundigita en la <tld/>kavo.
    </dif>
    <trdgrp lng="en">
      <trd>eye</trd>,
      <trd>eyeball</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Augapfel</trd>
  <trd lng="es">globo <ind>ocular</ind></trd>
  <trd lng="fr">globe oculaire</trd>
  <trd lng="hu">szemgoly&oacute;</trd>  
  <trd lng="nl">oogbal</trd>
  <trd lng="pl">ga&lstroke;ka oczna</trd>
  <trd lng="pt">globo ocular</trd>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_z;&c_n;&c_o;&c_je; &c_ja;&c_b;&c_l;&c_o;&c_k;&c_o;</trd>
</drv>

<drv mrk="okul.0lenso">
  <kap><tld/>lenso</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Natura lenso, lokita en <tld/>globo kaj destinata por formi
      optikan
      bildon sur la retino.
    </dif>
    <trd lng="en">lens</trd>
  </snc>         
  <trd lng="br">gwerenn al lagad</trd>
  <trd lng="de">Augenlinse</trd>
  <trd lng="es">cristalino</trd>   
  <trd lng="fr">cristallin</trd>
  <trd lng="hu">szemlencse</trd>  
  <trd lng="nl">ooglens</trd>
  <trd lng="pt">cristalino</trd>
  <trd lng="ru">&c_h;&c_r;&c_u;&c_s;&c_t;&c_a;&c_l;&c_i;&c_k;</trd>
</drv>

<drv mrk="okul.0mezure">
  <kap><tld/>mezure</kap>
  <snc>
    <dif>
      Kiel oni povas mezuri sen aparatoj.
    </dif>
  </snc>                  
  <trd lng="br">a-wel-dremm</trd>
  <trd lng="de"><klr>nach </klr>Augenma&szlig;</trd>
  <trd lng="fr">&agrave; vue d'&oelig;il</trd>
  <trd lng="hu">szemm&eacute;rt&eacute;kkel</trd>
  <trd lng="pl">na oko</trd>
  <trd lng="ru">&c_n;&c_a; &c_g;&c_l;&c_a;&c_z;</trd>
  <trd lng="sv">med <ind>&ouml;gonm&aring;tt</ind></trd>
</drv>
</art>
