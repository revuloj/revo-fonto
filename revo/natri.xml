<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>4</ofc>
   <rad>natri</rad>/o
   <fnt>Z</fnt>
</kap>
<drv mrk="natri.0o">
   <kap><tld/>o</kap>
   <mlg kod="IUPAC">Na</mlg>
   <uzo tip="fak">KEM</uzo>
   <snc>
     <dif>
       &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
       lst="voko:&hcirc;emiaj_elementoj" val="11">elemento</ref> kun
       atomnumero 11, atompezo 22,9898;
       <ref tip="super" cel="alkal.0eca">alkala</ref> blanka 
       <ref tip="super" cel="metal.0o.KEM">metalo</ref>, 
       kiu malkomponas akvon kaj kies kombino kun
       <ref tip="super" cel="klor.0o">kloro</ref> estigas la vulgaran
       salon uzatan en man&gcirc;a&jcirc;oj.
     </dif>
   </snc>
  <trd lng="be">&c_n;&c_a;&c_t;&c_r;&c_y;&c_j;</trd>
  <trd lng="bg">&c_n;&c_a;&c_t;&c_r;&c_i;&c_j;</trd>
  <trdgrp lng="br">
     <trd>sodiom</trd>,
     <trd>natriom</trd>
   </trdgrp>
  <trd lng="ca">sodi</trd>
  <trdgrp lng="cs">
    <trd>Na</trd>,
    <trd>natrium</trd>,
    <trd>sodík</trd>
  </trdgrp>
  <trd lng="de">Natrium</trd>
  <trd lng="el">&sigma;&omikron_ton;&delta;&jota;&omikron;</trd>
  <trd lng="en">sodium</trd>
  <trd lng="es">sodio</trd>
  <trd lng="fr">sodium</trd>
  <trd lng="hu">n&aacute;trium</trd>
  <trd lng="id">natrium</trd>
  <trd lng="it">sodio</trd>
  <trd lng="la">Natrium</trd>
  <trd lng="nl">natrium</trd>
  <trd lng="pl">s&oacute;d</trd>
  <trd lng="pt">s&oacute;dio</trd>
  <trd lng="ru">&c_n;&c_a;&c_t;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">sodík</trd>
  <trd lng="sv">natrium</trd>
</drv>
</art>
