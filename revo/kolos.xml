<art mrk="$Id: kolos.xml,v 1.25 2023/12/03 09:58:53 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kolos</rad>/o
</kap>
<drv mrk="kolos.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Grandega homo, statuo a&ubreve; objekto:
      <ekz>
        la <ind>Rodesa <tld/>o</ind>;
        <trd lng="fr">Colosse de Rhodes</trd>
      </ekz>
      <ekz>
        familio el arboj-<tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gigant.0o">giganto</ref>,
      <ref cel="titan.0o">titano</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_jo;&c_s;</trd>,
    <trd>&c_g;&c_ib;&c_g;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">col&oacute;s</trd>
  <trdgrp lng="cs">
    <trd>kolos</trd>,
    <trd>obr</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Riese</trd>,
    <trd>Gigant</trd>,
    <trd>Koloss</trd>
  </trdgrp>
  <trd lng="en">colossus</trd>
  <trd lng="es">coloso</trd>
  <trd lng="fr">colosse</trd>
  <trdgrp lng="hu">
    <trd>kolosszus</trd>,
    <trd>&oacute;ri&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5DE8;&#x50CF;  <pr>&#x304D;&#x3087;&#x305E;&#x3046;</pr></trd>,
    <trd>&#x5DE8;&#x4EBA;  <pr>&#x304D;&#x3087;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x5927;&#x56FD;  <pr>&#x305F;&#x3044;&#x3053;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kolos</trd>,
    <trd>kolossus</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kolos</trd>,
    <trd>wielkolud</trd>
  </trdgrp>
  <trd lng="pt">colosso</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_o;&c_s;&c_s;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kolos</trd>,
    <trd>obor</trd>,
    <trd>ozruta</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x43B;&#x43E;&#x441;</trd>,
    <trd>&#x432;&#x435;&#x43B;&#x435;&#x442;&#x435;&#x43D;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5E9E;&#x7136;&#x5927;&#x7269; <pr>p&#xE1;ngr&#xE1;nd&#xE0;w&#xF9;</pr></trd>,
    <trd>&#x5DE8;&#x5BA2; <pr>j&#xF9;k&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kolos.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta eksterordinarajn proporciojn a&ubreve; ecojn:
      <ekz>
        <tld/>a statuo, viro, urbo, entrepreno;
      </ekz>
      <ekz>
        ankora&ubreve; anta&ubreve; 50 jaroj inter la japanoj kaj
        e&ubreve;ropanoj
        ekzistis &scirc;ajne <tld/>a malsameco<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mamut">mamuta</ref>,
      <ref cel="elefan">elefanta</ref>,
      <ref cel="gigant.0a">giganta</ref>,
      <ref cel="titan">titana</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_ja;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_g;&c_ib;&c_g;&c_a;&c_n;&c_c;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="ca">colossal</trd>
  <trdgrp lng="cs">
    <trd>gigantick&#xFD;</trd>,
    <trd>kolos&#xE1;ln&#xED;</trd>,
    <trd>obrovsk&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>riesig</trd>,
    <trd>gigantisch</trd>,
    <trd>kolossal</trd>
  </trdgrp>
  <trd lng="en">colossal</trd>
  <trd lng="es">colosal</trd>
  <trdgrp lng="fr">
    <trd>colossal</trd>,
    <trd>d&eacute;mesur&eacute;<klr tip="ind"> (gigantesque)</klr>
    </trd>,
    <trd>g&eacute;ant<klr tip="ind"> (adj., gigantesque)</klr></trd>,
    <trd>gigantesque</trd>,
    <trd>hercul&eacute;en</trd>,
    <trd>ph&eacute;nom&eacute;nal<klr tip="ind"> (gigantesque)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kolossz&aacute;lis</trd>,
    <trd>&oacute;ri&aacute;si</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5DE8;&#x5927;&#x306A;  <pr>&#x304D;&#x3087;&#x3060;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x5927;&#x898F;&#x6A21;&#x306A;  <pr>&#x3060;&#x3044;&#x304D;&#x307C;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">kolossaal</trd>
  <trd lng="pl">kolosalny</trd>
  <trd lng="ru">
  &c_k;&c_o;&c_l;&c_o;&c_s;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>kolos&#xE1;lny</trd>,
    <trd>obrovsk&#xFD;</trd>,
    <trd>ohromn&#xFD;</trd>,
    <trd>ozrutn&#xFD;</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x43E;&#x43B;&#x43E;&#x441;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>
  <trdgrp lng="zh">
    <trd>&#x8EAB;&#x9AD8;&#x99AC;&#x5927; <pr>sh&#x113;ng&#x101;om&#x1CE;d&#xE0;</pr></trd>,
    <trd>&#x9AD8;&#x5927; <pr>g&#x101;od&#xE0;</pr></trd>,
    <trd>&#x5DE8;&#x5927; <pr>j&#xF9;d&#xE0;</pr></trd>,
    <trd>&#x5DE8; <pr>j&#xF9;</pr></trd>,
    <trd>&#x5E9E; <pr>p&#xE1;ng</pr></trd>,
    <trd>&#x5E9E;&#x5927; <pr>p&#xE1;ngd&#xE0;</pr></trd>,
    <trd>&#x5DE8;&#x578B; <pr>j&#xF9;x&#xED;ng</pr></trd>,
    <trd>&#x9B41;&#x68A7; <pr>ku&#xED;w&#xFA;</pr></trd>
  </trdgrp>
</drv>
