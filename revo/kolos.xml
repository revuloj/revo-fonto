<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kolos.xml,v 1.21 2015/07/04 08:37:37 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kolos</rad>/o
</kap>
<drv mrk="kolos.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Grandega homo, statuo a&ubreve; objekto:
      <ekz>
        la <ind>Rodesa <tld/>o</ind>;
        <trd lng="fr">Colosse de Rhodes</trd>
      </ekz>
      <ekz>
        familio el arboj-<tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gigant.0o">giganto</ref>,
      <ref cel="titan.0o">titano</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_jo;&c_s;</trd>,
    <trd>&c_g;&c_ib;&c_g;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="ca">col&oacute;s</trd>
  <trdgrp lng="cs">
    <trd>kolos</trd>,
    <trd>obr</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Riese</trd>,
    <trd>Gigant</trd>,
    <trd>Koloss</trd>
  </trdgrp>
  <trd lng="en">colossus</trd>
  <trd lng="es">coloso</trd>
  <trd lng="fr">colosse</trd>
  <trdgrp lng="hu">
    <trd>kolosszus</trd>,
    <trd>&oacute;ri&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kolos</trd>,
    <trd>kolossus</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kolos</trd>,
    <trd>wielkolud</trd>
  </trdgrp>
  <trd lng="pt">colosso</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_o;&c_s;&c_s;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_a;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kolos</trd>,
    <trd>obor</trd>,
    <trd>ozruta</trd>
  </trdgrp>
</drv>

<drv mrk="kolos.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta eksterordinarajn proporciojn a&ubreve; ecojn:
      <ekz>
        <tld/>a statuo, viro, urbo, entrepreno;
      </ekz>
      <ekz>
        ankora&ubreve; anta&ubreve; 50 jaroj inter la japanoj kaj
        e&ubreve;ropanoj
        ekzistis &scirc;ajne <tld/>a malsameco<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mamut">mamuta</ref>,
      <ref cel="elefan">elefanta</ref>,
      <ref cel="gigant.0a">giganta</ref>,
      <ref cel="titan">titana</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_ja;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_g;&c_ib;&c_g;&c_a;&c_n;&c_c;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="ca">colossal</trd>
  <trdgrp lng="cs">
    <trd>gigantický</trd>,
    <trd>kolosální</trd>,
    <trd>obrovský</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>riesig</trd>,
    <trd>gigantisch</trd>,
    <trd>kolossal</trd>
  </trdgrp>
  <trd lng="en">colossal</trd>
  <trd lng="es">colosal</trd>
  <trdgrp lng="fr">
    <trd>colossal</trd>,
    <trd>d&eacute;mesur&eacute;<klr tip="ind"> (gigantesque)</klr>
    </trd>,
    <trd>g&eacute;ant<klr tip="ind"> (adj., gigantesque)</klr></trd>,
    <trd>gigantesque</trd>,
    <trd>hercul&eacute;en</trd>,
    <trd>ph&eacute;nom&eacute;nal<klr tip="ind"> (gigantesque)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kolossz&aacute;lis</trd>,
    <trd>&oacute;ri&aacute;si</trd>
  </trdgrp>
  <trd lng="nl">kolossaal</trd>
  <trd lng="pl">kolosalny</trd>
  <trd lng="ru">
  &c_k;&c_o;&c_l;&c_o;&c_s;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>kolosálny</trd>,
    <trd>obrovský</trd>,
    <trd>ohromný</trd>,
    <trd>ozrutný</trd>
  </trdgrp>
</drv>
</art>
