<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kolos</rad>/o
</kap>
<drv mrk="kolos.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Grandega homo, statuo a&ubreve; objekto:
      <ekz>la Rodesa <tld/>o;</ekz>
      <ekz>familio el arboj-<tld/>oj<fnt>Z</fnt>.</ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="gigant.0o">giganto</ref>,
      <ref cel="titan.0o">titano</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Rie&szlig;e</trd>,
    <trd>Gigant</trd>,
    <trd>Koloss</trd>
  </trdgrp>
  <trd lng="en">colossus</trd>
  <trdgrp lng="nl">
    <trd>kolos</trd>,
    <trd>kolossus</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_l;&c_o;&c_s;&c_s;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_i;&c_k;&c_a;&c_n;</trd>
  </trdgrp>
</drv>
<drv mrk="kolos.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta eksterordinarajn proporciojn a&ubreve; ecojn:
      <ekz><tld/>a statuo, viro, urbo, entrepreno;</ekz>
      <ekz>
        ankora&ubreve; anta&ubreve; 50 jaroj inter la japanoj kaj e&ubreve;ropanoj
        ekzistis &scirc;ajne <tld/>a malsameco<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mamut">mamuta</ref>,
      <ref cel="elefan">elefanta</ref>,
      <ref cel="gigant.0a">giganta</ref>,
      <ref cel="titan">titana</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>rie&szlig;ig</trd>,
    <trd>gigantisch</trd>,
    <trd>kolossal</trd>
  </trdgrp>
  <trd lng="en">colossal</trd>
  <trd lng="nl">kolossaal</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_s;&c_s;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
