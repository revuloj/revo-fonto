<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ni&ccirc;</rad>/o
</kap>
<drv mrk="nicx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kava&jcirc;o en muro a&ubreve; konstruo destinita por loki statuon, fornon a&ubreve;
      alian objekton.
    </dif>
    <refgrp tip="sin">
      <ref cel="alkov.0o">alkovo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Nische</trd>
  <trdgrp lng="en">
    <trd>niche</trd>,
    <trd>nook</trd>
  </trdgrp>
  <trd lng="nl">nis</trd>
  <trd lng="pt">nicho</trd>
  <trd lng="ru">&c_n;&c_i;&c_sh;&c_a;</trd>
  <trd lng="sv">nisch</trd>
</drv>
</art>
