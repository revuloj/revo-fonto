<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: triumf.xml,v 1.22 2024/05/10 10:43:47 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>triumf</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="triumf.0o">
  <kap><tld/>o</kap>
  <snc mrk="triumf.0o.MOR">
    <dif>
      Granda venko, granda sukceso montri&gcirc;anta per eksteraj signoj:
      <ekz>
        la lasta kongreso estis vera <tld/>o;
      </ekz>
      <ekz>
        <ind><tld/>a</ind>j kantoj sonos<fnt>Z</fnt>.
        <trd lng="be">&c_t;&c_r;&c_y;&c_ju;&c_m;&c_f;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
        <trdgrp lng="hu">
          <trd>gy&odblac;zedelmi</trd>,
          <trd>diadal-</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_t;&c_r;&c_i;&c_u;&c_m;&c_f;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trd lng="be">&c_u;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trdgrp lng="hu">
      <trd>gy&odblac;zelem</trd>,
      <trd>diadal</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </snc>
  <snc mrk="triumf.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      &Ccirc;e la antikvaj Romanoj, pompa kaj solena eniro de militestro post
      granda venko.
    </dif>
    <trd lng="hu">diadalmenet</trd>
  </snc>
  <trd lng="bo">&#xF62;&#xF92;&#xFB1;&#xF63;&#xF0B;&#xF41;&#xF0B;</trd>
  <trdgrp lng="ja">
    <trd>&#x51F1;&#x65CB;  <pr>&#x304C;&#x3044;&#x305B;&#x3093;</pr></trd>,
    <trd>&#x5927;&#x52DD;&#x5229;  <pr>&#x3060;&#x3044;&#x3057;&#x3087;&#x3046;&#x308A;</pr></trd>,
    <trd>&#x5927;&#x6210;&#x529F;  <pr>&#x3060;&#x3044;&#x305B;&#x3044;&#x3053;&#x3046;</pr></trd>,
    <trd>&#x52DD;&#x5229;&#x611F;  <pr>&#x3057;&#x3087;&#x3046;&#x308A;&#x304B;&#x3093;</pr></trd>,
    <trd>&#x52DD;&#x5229;&#x306E;&#x559C;&#x3073;  <pr>&#x3057;&#x3087;&#x3046;&#x308A;&#x306E;&#x3088;&#x308D;&#x3053;&#x3073;</pr></trd>
  </trdgrp>
  <trd lng="lat">triumphus, i</trd>
  <trd lng="be">&c_t;&c_r;&c_y;&c_ju;&c_m;&c_f;</trd>
  <trdgrp lng="cs">
    <trd>oslava v&iacute;t&ecaron;zstv&iacute;</trd>,
    <trd>triumf</trd>
  </trdgrp>
  <trd lng="de">Triumph</trd>
  <trd lng="en">triumph</trd>
  <trd lng="fr">triomphe</trd>
  <trd lng="nl">triomf</trd>
  <trd lng="pl">triumf</trd>
  <trd lng="pt">tiunfo</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_u;&c_m;&c_f;</trd>
  <trd lng="sk">triumf</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_ib;&c_u;&#x43C;&c_f;</trd>,
    <trd>&c_t;&#x43E;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&#x43C;&#x43E;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6377; <pr>ji&#xE9;</pr></trd>,
    <trd>&#x51EF;&#x65CB; <pr>k&#x1CE;ixu&#xE1;n</pr></trd>,
    <trd>&#x51F1;&#x65CB; <pr>k&#x1CE;ixu&#xE1;n</pr></trd>,
    <trd>&#x52DD;&#x5229; <pr>sh&#xE8;ngl&#xEC;</pr></trd>,
    <trd>&#x80DC;&#x5229; <pr>sh&#xE8;ngl&#xEC;</pr></trd>,
    <trd>&#x5927;&#x52DD; <pr>d&#xE0;sh&#xE8;ng</pr></trd>,
    <trd>&#x5927;&#x80DC; <pr>d&#xE0;sh&#xE8;ng</pr></trd>,
    <trd>&#x4F1F;&#x5927;&#x7684;&#x6210;&#x5C31; <pr>w&#x11B;id&#xE0;dech&#xE9;ngji&#xF9;</pr></trd>,
    <trd>&#x5049;&#x5927;&#x7684;&#x6210;&#x5C31; <pr>w&#x11B;id&#xE0;dech&#xE9;ngji&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="triumf.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Plene venki, brile sukcesi:
      <ekz>
        <tld/>i super iu<fnt>Z</fnt>,
      </ekz>
      <ekz>
        super &ccirc;iuj malhelpoj.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_o;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_t;&c_mol; &c_v;&c_je;&c_r;&c_h;</trd>,
      <trd>&c_v;&c_o;&c_s;&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Fieri pro venko, pro sukceso
    </dif>
    <refgrp tip="vid">
      <ref cel="fru">tro frue</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_p;&c_o;&c_b;&c_je;&c_d;&c_u;</trd>,
      <trd>&c_l;&c_i;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_ju;&c_m;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">triumphieren</trd>
  <trd lng="fr">triompher</trd>
  <trdgrp lng="hu">
    <trd>gy&odblac;zedelmeskedik</trd>,
    <trd>diadalmaskodik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x51F1;&#x65CB;&#x3059;&#x308B;  <pr>&#x304C;&#x3044;&#x305B;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5927;&#x52DD;&#x5229;&#x3059;&#x308B;  <pr>&#x305F;&#x3044;&#x3057;&#x3087;&#x3046;&#x308A;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6253;&#x3061;&#x8CA0;&#x304B;&#x3059;  <pr>&#x3046;&#x3061;&#x307E;&#x304B;&#x3059;</pr></trd>,
    <trd>&#x5927;&#x6210;&#x529F;&#x3092;&#x53CE;&#x3081;&#x308B;  <pr>&#x3060;&#x3044;&#x305B;&#x3044;&#x3053;&#x3046;&#x3092;&#x304A;&#x3055;&#x3081;&#x308B;</pr></trd>,
    <trd>&#x52DD;&#x3061;&#x8A87;&#x308B;  <pr>&#x304B;&#x3061;&#x307B;&#x3053;&#x308B;</pr></trd>,
    <trd>&#x5F97;&#x610F;&#x306B;&#x306A;&#x308B;  <pr>&#x3068;&#x304F;&#x3044;&#x306B;&#x306A;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">triomferen</trd>
  <trd lng="pl">triumfowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>triunfar</trd>,
    <trd>alcan&ccedil;ar vit&oacute;ria</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_ib;&c_u;&#x43C;&c_f;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_v;&#x44F;&c_t;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i; &#x43F;&c_je;&c_r;&c_je;&#x43C;&#x43E;&c_g;&c_u;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_i; &c_v;&c_je;&c_r;&c_h;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&#x43C;&c_a;&c_g;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F97;&#x52DD; <pr>d&#xE9;sh&#xE8;ng</pr></trd>,
    <trd>&#x5F97;&#x80DC; <pr>d&#xE9;sh&#xE8;ng</pr></trd>,
    <trd>&#x51EF;&#x65CB; <pr>k&#x1CE;ixu&#xE1;n</pr></trd>,
    <trd>&#x51F1;&#x65CB; <pr>k&#x1CE;ixu&#xE1;n</pr></trd>,
    <trd>&#x52DD;&#x5229; <pr>sh&#xE8;ngl&#xEC;</pr></trd>,
    <trd>&#x80DC;&#x5229; <pr>sh&#xE8;ngl&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
