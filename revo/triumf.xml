<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: triumf.xml,v 1.24 2025/04/12 22:26:42 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>triumf</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="triumf.0o">
  <kap><tld/>o</kap>
  <snc mrk="triumf.0o.MOR">
    <dif>
      Granda venko, granda sukceso montri&gcirc;anta per eksteraj signoj:
      <ekz>
        la lasta kongreso estis vera <tld/>o;
      </ekz>
      <ekz>
        <ind><tld/>a</ind>j kantoj sonos<fnt>Z</fnt>.
        <trd lng="be">&c_t;&c_r;&c_y;&c_ju;&c_m;&c_f;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
        <trdgrp lng="hu">
          <trd>gy&odblac;zedelmi</trd>,
          <trd>diadal-</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_t;&c_r;&c_i;&c_u;&c_m;&c_f;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </ekz>
    </dif>
    <trd lng="be">&c_u;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trdgrp lng="hu">
      <trd>gy&odblac;zelem</trd>,
      <trd>diadal</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  </snc>
  <snc mrk="triumf.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      &Ccirc;e la antikvaj Romanoj, pompa kaj solena eniro de militestro post
      granda venko.
    </dif>
    <trd lng="hu">diadalmenet</trd>
  </snc>
  <trd lng="bo">&t_ra;&t_gata;&t_yata;&t_la;&t_tsheg;&t_kha;&t_tsheg;</trd>
  <trdgrp lng="ja">
    <trd>&#20977;&#26059;  <pr>&#12364;&#12356;&#12379;&#12435;</pr></trd>,
    <trd>&#22823;&#21213;&#21033;  <pr>&#12384;&#12356;&#12375;&#12423;&#12358;&#12426;</pr></trd>,
    <trd>&#22823;&#25104;&#21151;  <pr>&#12384;&#12356;&#12379;&#12356;&#12371;&#12358;</pr></trd>,
    <trd>&#21213;&#21033;&#24863;  <pr>&#12375;&#12423;&#12358;&#12426;&#12363;&#12435;</pr></trd>,
    <trd>&#21213;&#21033;&#12398;&#21916;&#12403;  <pr>&#12375;&#12423;&#12358;&#12426;&#12398;&#12424;&#12429;&#12371;&#12403;</pr></trd>
  </trdgrp>
  <trd lng="lat">triumphus, i</trd>
  <trd lng="be">&c_t;&c_r;&c_y;&c_ju;&c_m;&c_f;</trd>
  <trdgrp lng="cs">
    <trd>oslava v&iacute;t&ecaron;zstv&iacute;</trd>,
    <trd>triumf</trd>
  </trdgrp>
  <trd lng="de">Triumph</trd>
  <trd lng="en">triumph</trd>
  <trd lng="fr">triomphe</trd>
  <trd lng="nl">triomf</trd>
  <trd lng="pl">triumf</trd>
  <trd lng="pt">tiunfo</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_u;&c_m;&c_f;</trd>
  <trd lng="sk">triumf</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_ib;&c_u;&c_m;&c_f;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25463; <pr>ji&eacute;</pr></trd>,
    <trd>&#20975;&#26059; <pr>k&#462;ixu&aacute;n</pr></trd>,
    <trd>&#20977;&#26059; <pr>k&#462;ixu&aacute;n</pr></trd>,
    <trd>&#21213;&#21033; <pr>sh&egrave;ngl&igrave;</pr></trd>,
    <trd>&#32988;&#21033; <pr>sh&egrave;ngl&igrave;</pr></trd>,
    <trd>&#22823;&#21213; <pr>d&agrave;sh&egrave;ng</pr></trd>,
    <trd>&#22823;&#32988; <pr>d&agrave;sh&egrave;ng</pr></trd>,
    <trd>&#20255;&#22823;&#30340;&#25104;&#23601; <pr>w&ecaron;id&agrave;dech&eacute;ngji&ugrave;</pr></trd>,
    <trd>&#20553;&#22823;&#30340;&#25104;&#23601; <pr>w&ecaron;id&agrave;dech&eacute;ngji&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="triumf.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Plene venki, brile sukcesi:
      <ekz>
        <tld/>i super iu<fnt>Z</fnt>,
      </ekz>
      <ekz>
        super &ccirc;iuj malhelpoj.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_o;&c_d;&c_je;&c_r;&c_zh;&c_a;&c_t;&c_mol; &c_v;&c_je;&c_r;&c_h;</trd>,
      <trd>&c_v;&c_o;&c_s;&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Fieri pro venko, pro sukceso
    </dif>
    <refgrp tip="vid">
      <ref cel="fru">tro frue</ref>.
    </refgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_p;&c_o;&c_b;&c_je;&c_d;&c_u;</trd>,
      <trd>&c_l;&c_i;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_ju;&c_m;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">triumphieren</trd>
  <trd lng="en">triumph</trd>
  <trd lng="fr">triompher</trd>
  <trdgrp lng="hu">
    <trd>gy&odblac;zedelmeskedik</trd>,
    <trd>diadalmaskodik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20977;&#26059;&#12377;&#12427;  <pr>&#12364;&#12356;&#12379;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#22823;&#21213;&#21033;&#12377;&#12427;  <pr>&#12383;&#12356;&#12375;&#12423;&#12358;&#12426;&#12377;&#12427;</pr></trd>,
    <trd>&#25171;&#12385;&#36000;&#12363;&#12377;  <pr>&#12358;&#12385;&#12414;&#12363;&#12377;</pr></trd>,
    <trd>&#22823;&#25104;&#21151;&#12434;&#21454;&#12417;&#12427;  <pr>&#12384;&#12356;&#12379;&#12356;&#12371;&#12358;&#12434;&#12362;&#12373;&#12417;&#12427;</pr></trd>,
    <trd>&#21213;&#12385;&#35463;&#12427;  <pr>&#12363;&#12385;&#12411;&#12371;&#12427;</pr></trd>,
    <trd>&#24471;&#24847;&#12395;&#12394;&#12427;  <pr>&#12392;&#12367;&#12356;&#12395;&#12394;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">triomferen</trd>
  <trd lng="pl">triumfowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>triunfar</trd>,
    <trd>alcan&ccedil;ar vit&oacute;ria</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_ib;&c_u;&c_m;&c_f;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_v;&c_ja;&c_t;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i; &c_p;&c_je;&c_r;&c_je;&c_m;&c_o;&c_g;&c_u;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_i; &c_v;&c_je;&c_r;&c_h;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_m;&c_a;&c_g;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#24471;&#21213; <pr>d&eacute;sh&egrave;ng</pr></trd>,
    <trd>&#24471;&#32988; <pr>d&eacute;sh&egrave;ng</pr></trd>,
    <trd>&#20975;&#26059; <pr>k&#462;ixu&aacute;n</pr></trd>,
    <trd>&#20977;&#26059; <pr>k&#462;ixu&aacute;n</pr></trd>,
    <trd>&#21213;&#21033; <pr>sh&egrave;ngl&igrave;</pr></trd>,
    <trd>&#32988;&#21033; <pr>sh&egrave;ngl&igrave;</pr></trd>
  </trdgrp>
</drv>
