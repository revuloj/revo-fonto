<drv mrk="kest.leter0o">
  <kap>leter<tld/>o</kap>
  <snc mrk="kest.leter0o.POSX">
    <uzo tip="fak">POSX</uzo>
    <dif>
      Kesto por enmeti ricevatajn leterojn, paka&jcirc;ojn,
      mesa&gcirc;ojn:
      <ekz>
        redakcia leter<tld/>o
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007091.php">Du patroj interparolas</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 26</lok>
          </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        ofte kiam asocio uzas retpo&scirc;ton por organizi kunvenon, la duono
        de la korespondantoj
        konfesas <klr>[&#x2026;]</klr>, ke ilia leter<tld/>o <klr>(virtuala
        fako por retpo&scirc;toj)</klr> 
        estis plen&scirc;topita a&ubreve; ke &jcirc;us estis problemeto pri la komputilo
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-S&eacute;bastien Brilleaud</aut>
          <vrk><url ref="&Monato;/2006/008987.php">Modernaj
          komunikiloj, &ccirc;u fideblaj?</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kest.posxt0o">po&scirc;t<tld/>o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_sh;&c_t;&c_o;&c_v;&c_a;&c_ja; &c_s;&c_k;&c_r;&c_y;&c_n;&c_k;&c_a;</trd>
  <trd lng="bg">&c_p;&c_o;&c_shch;&c_je;&c_n;&c_s;&c_k;&c_a; &c_k;&c_u;&c_t;&c_i;&c_ja;</trd>
