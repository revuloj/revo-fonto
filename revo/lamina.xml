<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lamina.xml,v 1.19 2023/09/01 19:10:58 revo Exp $">
<kap>
  <rad>laminari</rad>/o
</kap>
<drv mrk="lamina.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      de maraj <ref tip="malprt" cel="alg.0oj">algoj</ref>,
      kies kelkaj specoj estas man&gcirc;eblaj (<trd lng="la">Laminaria</trd>):
      <ekz>
        super 90 procentoj de japana <tld/>o estas kultivata, plejparte
        en Hokajdo, sed
        anka&ubreve; en la fora sudo kiel la Enlanda Maro de Seto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=377818">Laminario</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d9/Laminaria_digitata_hyperborea_Helgoland.JPG/640px-Laminaria_digitata_hyperborea_Helgoland.JPG" prm="CC BY-SA 3.0"><tld/>o (Laminaria digitata a&ubreve; Laminaria hyperborea), insulo
      Helgoland, Germanujo
        <fnt>
          <aut>Gabriele Kothe-Heinrich, propra verko</aut>
          <lok><url ref="&WCW;/File:Laminaria_digitata_hyperborea_Helgoland.JPG">&WCW;/File:Laminaria_digitata_hyperborea_Helgoland.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_m;&c_ib;&c_n;&c_a;&c_r;&c_y;&c_ja;</trd>
  <trd lng="ca">lamin&agrave;ria</trd>
  <trdgrp lng="de">
     <trd>Blatttang</trd>,
     <trd>Riementang</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>brown <ind>seaweeds</ind></trd>,
    <trd>brown algae</trd>,
    <trd>kelp</trd>
  </trdgrp>
  <trd lng="fr">laminaire</trd>
  <trdgrp lng="hu">
    <trd>lemezmoszat</trd>,
    <trd>sz&iacute;jmoszat</trd>,
    <trd>mannalev&eacute;l</trd></trdgrp>
  <trd lng="ja">&#x30B3;&#x30F3;&#x30D6;</trd>
  <trd lng="nl">zeewier</trd>
  <trd lng="pl">listownica  <klr>(glon)</klr></trd> 
  <trd lng="pt">lamin&aacute;ria</trd>
  <trd lng="ro">laminaria</trd>
  <trd lng="ru">&c_l;&c_a;&c_m;&c_i;&c_n;&c_a;&c_r;&c_i;&c_ja;</trd>
  <trd lng="uk">&#x43B;&#x430;&#x43C;&#x456;&#x43D;&#x430;&#x440;&#x456;&#x44F;</trd>
</drv>
</art>
