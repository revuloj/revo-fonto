<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>Tife</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tife.0o.MIT">
  <kap><tld/>o</kap>
  <snc mrk="tife.0o.SNC">
    <uzo tip="fak">MIT</uzo>
    <dif>
      <ref tip="sin" cel="tifonon.0o.MET">Tifono</ref>. La&ubreve; helena mito,
      monstra esta&jcirc;o, iel rilatata al ventegoj kaj vulkanoj.
    </dif>
  </snc>
  <trdgrp lng="it">
    <trd>Tifone<klr> (mit)</klr></trd>,
    <trd>Tifeo</trd>
  </trdgrp>
</drv>
</art>

