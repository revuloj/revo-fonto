<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>Tife</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tife.0o">
  <kap><tld/>o</kap>
  <snc mrk="tife.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      La&ubreve; helena mito, monstra esta&jcirc;o iel rilata al ventegoj
      kaj vulkanoj;
      <ref tip="sin" cel="tifon.0o.MIT">Tifono</ref>.
    </dif>
  </snc>
  <trd lng="en">Typhon</trd>
  <trdgrp lng="es">
    <trd>Tif&oacute;n<klr> (mitolog&iacute;a)</klr></trd>,
    <trd>Tifeo</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Typhon</trd>,
    <trd>Typh&eacute;e</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>Tifone<klr> (mit.)</klr></trd>,
    <trd>Tifeo</trd>
  </trdgrp>
</drv>
</art>

