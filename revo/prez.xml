<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prez.xml,v 1.23 2024/02/17 07:52:36 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>prez</rad>/o
</kap>
<drv mrk="prez.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Vendvaloro de objekto:
      <ekz>
        por tiu ma&scirc;ino kies kosto estis mil markoj, la fabrikanto
        postulas
        <tld/>on de du mil dum &gcirc;ia valoro ne atingas e&ccirc; kvincent
        markojn;
      </ekz>
      <ekz>
        <ind><tld/>aro</ind>
        <klr>(tabelo de varoj kun <tld/>o)</klr>;
        <trdgrp lng="be">
          <trd>&c_p;&c_r;&c_a;&c_j;&c_s;-&c_l;&c_ib;&c_s;&c_t;</trd>,
          <trd>&c_p;&c_r;&c_e;&c_j;&c_s;&c_k;&c_u;&c_r;&c_a;&c_n;&c_t;</trd>
        </trdgrp>
        <trd lng="de">Preisliste</trd>
        <trd lng="hu">&aacute;rjegyz&eacute;k</trd>        
        <trdgrp lng="ru">
          <trd>&c_p;&c_r;&c_je;&c_j;&c_s;&c_k;&c_u;&c_r;&c_a;&c_n;&c_t;</trd>,
          <trd>&c_p;&c_r;&c_a;&c_j;&c_s;-&c_l;&c_i;&c_s;&c_t;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        anonci <tld/>on por la kapo de krimulo
        (<ref tip="sin" cel="premi.0o">premio</ref>);
      </ekz>
      <ekz>malgranda pezo, sed granda <tld/>o
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>ju pli da a&ccirc;etantoj, des pli alta la <tld/>o
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        la <tld/>oj falis tre malalten;
      </ekz>
      <ekz>
        granda<tld/>a.
      </ekz>
    </dif>
    <ref tip="sin" cel="kost.0o">kosto</ref>
    <ref tip="vid" cel="lu.0prezo">luprezo</ref>
  </snc>
  <trd lng="be">&c_c;&c_a;&c_n;&c_a;</trd>
  <trd lng="bg">&c_c;&c_je;&c_n;&c_a;</trd>
  <trd lng="bo">&#xF42;&#xF7C;&#xF44;&#xF0B;</trd>
  <trd lng="de">Preis</trd>
  <trd lng="en">price</trd>
  <trd lng="es">precio</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_h;&a_A;</trd>
    <trd>&a_q;&f_y;&a_m;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">prix</trd>
  <trd lng="hu">&aacute;r</trd>  
  <trd lng="it">prezzo</trd>
  <trdgrp lng="ja">
    <trd>&#x4FA1;&#x683C;  <pr>&#x304B;&#x304B;&#x304F;</pr></trd>,
    <trd>&#x5024;&#x6BB5;  <pr>&#x306D;&#x3060;&#x3093;</pr></trd>,
    <trd>&#x5B9A;&#x4FA1;  <pr>&#x3066;&#x3044;&#x304B;</pr></trd>
  </trdgrp>
  <trd lng="nl">prijs</trd>
  <trd lng="pl">cena</trd>
  <trd lng="pt">pre&ccedil;o</trd>
  <trd lng="ru">&c_c;&c_je;&c_n;&c_a;</trd>
  <trd lng="uk">&#x446;&#x456;&#x43D;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x4EF7;&#x683C; <pr>ji&#xE0;g&#xE9;</pr></trd>,
    <trd>&#x50F9;&#x683C; <pr>ji&#xE0;g&#xE9;</pr></trd>,
    <trd>&#x4EE3;&#x4EF7; <pr>d&#xE0;iji&#xE0;</pr></trd>,
    <trd>&#x4EE3;&#x50F9; <pr>d&#xE0;iji&#xE0;</pr></trd>,
    <trd>&#x4EF7;&#x94B1; <pr>ji&#xE0;qian</pr></trd>,
    <trd>&#x50F9;&#x9322; <pr>ji&#xE0;qian</pr></trd>
  </trdgrp>
</drv>
</art>
