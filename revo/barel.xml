<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>barel</rad>/o
</kap>
<drv mrk="barel.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
     <dif>
       Ronda ventroforma ujo el lignaj kurbaj tabuletoj kunfirmigitaj per
       feraj a&ubreve; lignaj ringoj kun du plataj fundoj, uzata por enteni
       vinon, akvon a&ubreve; aliajn flua&jcirc;ojn:
       <ekz><tld/>o malplena sonas plej la&ubreve;te
         <klr>(plej fanfaronas senvaloruloj kaj senmerituloj.
         <refgrp tip="vid">
           <ref cel="glor.0o">gloro</ref>,
           <ref cel="rad.0o">rado</ref>
         </refgrp>)</klr><fnt><bib>PrV</bib></fnt></ekz>
     </dif>
     <adm>
       La referencoj al "gloro" kaj "rado" aperas malbele. La lasta
       estas komprenebla pro samsignifa proverbo, sed la unua restas
       mistera. <aut>MB</aut>
     </adm>
     <refgrp tip="vid">
       <ref cel="kuv.0o">kuvo</ref>,
       <ref cel="sitel.0o">sitelo</ref>.
     </refgrp>
   </snc>
   <trd lng="cs">sud</trd>
   <trd lng="de">Fass</trd>
   <trd lng="en">barrel</trd>
   <trd lng="es">barril</trd>
   <trdgrp lng="fr">
     <trd>tonneau</trd>,
     <trd>barrique</trd></trdgrp>
   <trd lng="hu">hord&oacute;</trd>
   <trd lng="ru">&c_b;&c_o;&c_ch;&c_k;&c_a;</trd>
   <trd lng="sk">sud</trd>
</drv>
<drv mrk="barel.0ejo">
   <kap><tld/>ejo</kap>
   <snc>
     <dif>
       <tld lit="B"/>fabriko.
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>B&ouml;ttcherei</trd>,
     <trd>Fassmacherei</trd>,
     <trd>K&uuml;ferei</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>cooper's shop</trd>,
     <trd>barrel factory</trd></trdgrp>
   <trd lng="fr">tonnellerie<klr tip="ind"> (fabrique)</klr></trd>
   <trd lng="hu">k&aacute;d&aacute;rm&udblac;hely</trd>
</drv>
<drv mrk="barel.0eto">
   <kap><tld/>eto</kap>
   <snc num="1">
     <dif>
       Malgranda <tld/>o.
     </dif>
     <trd lng="de">F&auml;sschen</trd>
     <trd lng="en">barrel</trd>
     <trd lng="fr">baril</trd>
     <trd lng="hu">csoboly&oacute;</trd>
     <trd lng="ru">&c_b;&c_o;&c_ch;&c_o;&c_n;&c_o;&c_k;</trd>
     <trd lng="sk">s&uacute;dok</trd>
   </snc>
   <snc num="2">
     <uzo tip="fak">TEK</uzo>
     <dif>
       Cilindra ujo, en kiu trovi&gcirc;as la risorto de horlo&gcirc;o;
       turni&gcirc;a peco de revolvero, entenanta la karto&ccirc;ojn.
     </dif>
     <trdgrp lng="de">
       <trd>Kammer</trd>,
       <trd>Trommel</trd>,
       <trd>Geh&auml;use</trd>
     </trdgrp>
     <trd lng="en">chamber</trd>
     <trd lng="fr">barillet</trd>
     <trdgrp lng="en">
       <trd>rug&oacute;tok</trd>,
       <trd>forg&oacute;t&aacute;r<klr tip="ind"> (revolveren)</klr></trd></trdgrp>
     <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n; 
<klr>(&c_r;&c_je;&c_v;&c_o;&c_l;&c_mol;&c_v;&c_je;&c_r;&c_a;, 
&c_ch;&c_a;&c_s;&c_o;&c_v;)</klr></trd>
   </snc>
</drv>
<drv mrk="barel.0isto">
   <kap><tld/>isto</kap>
   <snc>
     <dif>
       <tld lit="B"/>faristo.
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>B&ouml;ttcher</trd>,
     <trd>Fassmacher</trd>,
     <trd>K&uuml;fer</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>cooper</trd>,
     <trd>coopersmith</trd></trdgrp>
   <trd lng="fr">tonnelier</trd>
   <trdgrp lng="hu">
     <trd>k&aacute;d&aacute;r</trd>,
     <trd>bogn&aacute;r</trd></trdgrp>
   <trd lng="ru">&c_b;&c_o;&c_ch;&c_a;&c_r;</trd>
</drv>
<drv mrk="barel.0igxi">
   <kap><tld/>i&gcirc;i<fnt>Z</fnt></kap>
   <snc>
     <uzo tip="stl">FIG</uzo>
     <dif>
       Ventrodiki&gcirc;i kiel <tld/>o.
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>zunehmen</trd>,
     <trd>Fett ansetzen</trd>,
     <trd>kugelrund werden</trd>
   </trdgrp>
   <trd lng="en">put on <ind>weight</ind></trd>
   <trd lng="fr">enfler<klr tip="ind"> (comme un tonneau)</klr></trd>
   <trd lng="hu">pocakosodik</trd>
</drv>
<drv mrk="barel.en0igi">
   <kap>en<tld/>igi</kap>
   <snc>
     <uzo tip="klr">(p.p. precipe pri likva&jcirc;o)</uzo>
     <dif>
       Meti en <tld/>on.
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>eindosen</trd>,
     <trd>einfassen</trd>
   </trdgrp>
   <trd lng="en">load into a <ind>barrel</ind></trd>
   <trd lng="fr">mettre en <ind>tonneau</ind></trd>
   <trd lng="hu">hord&oacute;ba t&ouml;lt</trd>
</drv>
<drv mrk="barel.el0igi">
   <kap>en<tld/>igi</kap>
   <snc>
     <uzo tip="klr">(p.p. precipe pri likva&jcirc;o)</uzo>
     <dif>
       Eligi el <tld/>o.
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>aus dem Fass <ind>holen</ind></trd>,
     <trd>absch&uuml;tten</trd>
   </trdgrp>
   <trd lng="en">decant</trd>
   <trd lng="fr">tirer<klr tip="ind"> (du tonneau)</klr></trd>
   <trd lng="hu">hord&oacute;b&oacute;l kit&ouml;lt</trd>
</drv>

</art>
