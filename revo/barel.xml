  <trd lng="ja">&#x6A3D;&#x5DE5;&#x5834;  <pr>&#x305F;&#x308B;&#x3053;&#x3046;&#x3058;&#x3087;&#x3046;</pr></trd>
  <trd lng="ru">&c_b;&c_o;&c_n;&c_d;&c_a;&c_r;&c_n;&c_ja;</trd>
  <trd lng="sk">v&yacute;rob&ncaron;a sudov</trd>
  <trd lng="uk">&c_b;&c_o;&c_n;&c_d;&c_a;&c_r;&c_n;&c_ja;</trd>
</drv>

<drv mrk="barel.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="barel.0eto.malgranda">
    <dif>
      Malgranda <tld/>o:
      <ekz>
        jen estas <tld/>eto da tritiko
        <fnt><bib>FK</bib>, <lok>4, la hejmo de la metiisto</lok></fnt>;
      </ekz>
      <ekz>
        li povas trinki du <tld/>etojn da juli-mielo
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IX.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> mem iradis kun la bier<tld/>eto al la homoj sur la vendejon
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havas lad<tld/>eton kun du litroj da brando
        <fnt>
          <bib>VRA</bib>
          <lok>Moa Martinson</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        jen fine guto de optimismo en tiun <tld/>eton de &ccirc;agreno
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 10 (108)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_a;&c_ch;&c_o;&c_n;&c_a;&c_k;</trd>,
      <trd>&c_b;&c_o;&c_ch;&c_a;&c_ch;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>barazhig</trd>,
      <trd>barilh</trd>
    </trdgrp>
    <trd lng="de">F&auml;sschen</trd>
    <trd lng="en">barrel</trd>
    <trd lng="hu">csoboly&oacute;</trd>
    <trdgrp lng="it">
      <trd>barilotto</trd>,
      <trd>botticella</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>beczu&lstroke;ka</trd>,
      <trd>anta&lstroke;ek</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_o;&c_ch;&c_o;&c_n;&c_o;&c_k;</trd>
    <trd lng="sk">s&uacute;dok</trd>
  </snc>
  <snc mrk="barel.0eto.formo">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="B"/>forma ujeto de ma&scirc;ino.
    </dif>
    <subsnc mrk="barel.0eto.KAL">
      <uzo tip="fak">KAL</uzo>
      <dif>
        Ujo, en kiu trovi&gcirc;as la risorto de <ref tip="malprt" cel="horlog.0o.KAL">horlo&gcirc;o</ref>.
      </dif>
      <trdgrp lng="it">
        <trd>bariletto<klr> (dell'orologio)</klr></trd>,
        <trd>tamburo<klr> (dell'orologio)</klr></trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="barel.0eto.ARM">
      <dif>
        Turni&gcirc;a peco de revolvero, entenanta la karto&ccirc;ojn.
      </dif>
      <trd lng="it">tamburo<klr> (pistola)</klr></trd>
      <trd lng="pl">b&eogonek;benek</trd>
    </subsnc>
    <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;
    <klr>(&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_mol;&c_n;&c_ib;&c_k;&c_a;,
    &c_r;&c_e;&c_v;&c_a;&c_l;&c_mol;&c_v;&c_e;&c_r;&c_a;)</klr></trd>
    <trd lng="br">taboulin<klr> (un arm, un horolaj)</klr></trd>
    <trdgrp lng="de">
      <trd>Kammer</trd>,
      <trd>Trommel</trd>,
      <trd>Geh&auml;use</trd>
    </trdgrp>
    <trd lng="en">chamber</trd>
    <trdgrp lng="hu">
      <trd>rug&oacute;tok</trd>,
      <trd>forg&oacute;t&aacute;r<klr tip="ind">
      (revolveren)</klr></trd>
    </trdgrp>
    <trd lng="it">tamburo<klr> (mec.)</klr></trd>
    <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n;
    <klr>(&c_r;&c_je;&c_v;&c_o;&c_l;&c_mol;&c_v;&c_je;&c_r;&c_a;,
    &c_ch;&c_a;&c_s;&c_o;&c_v;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bub&iacute;nek revolveru</trd>,
    <trd>soudek</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>barillet</trd>,
    <trd>tonnelet</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5C0F;&#x6A3D;  <pr>&#x304A;&#x305F;&#x308B;</pr></trd>,
    <trd>&#x5F3E;&#x5009;  <pr>&#x3060;&#x3093;&#x305D;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_a;&c_r;&c_i;&c_l;&c_o;</trd>
</drv>

<drv mrk="barel.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="barel.0isto.TEK">
    <dif>
      <tld lit="B"/>faristo:
      <ekz>
        la kolosa ligna <tld/>o de la teleskopo estis farita de urbaj bier<tld/>istoj
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2010/009731.php">Tajg kaj Tomas</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5236;&#x6728;&#x6876;&#x5DE5;&#x4EBA; <pr>zh&#xEC;m&#xF9;t&#x1D2;ngg&#x14D;ngr&#xE9;n</pr></trd>,
    <trd>&#x5236;&#x6876;&#x5DE5; <pr>zh&#xEC;t&#x1D2;ngg&#x14D;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_b;&c_o;&c_n;&c_d;&c_a;&c_r;</trd>
  <trdgrp lng="br">
    <trd>barazher</trd>,
    <trd>toneller</trd>
  </trdgrp>
  <trd lng="cs">bedn&aacute;&rcaron;</trd>
  <trdgrp lng="de">
    <trd>B&ouml;ttcher</trd>,
    <trd>Fassmacher</trd>,
    <trd>K&uuml;fer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cooper</trd>,
    <trd>coopersmith</trd>
  </trdgrp>
  <trd lng="fr">tonnelier</trd>
  <trd lng="he">&yod;&tsadi;&resh;&nun;&yod;&tav; &het;&bet;&yod;&vav;&tav;</trd>
  <trdgrp lng="hu">
    <trd>k&aacute;d&aacute;r</trd>,
    <trd>bogn&aacute;r</trd>
  </trdgrp>
  <trd lng="it">bottaio</trd>
  <trd lng="ja">&#x6A3D;&#x8077;&#x4EBA;  <pr>&#x305F;&#x308B;&#x3057;&#x3087;&#x304F;&#x306B;&#x3093;</pr></trd>
  <trd lng="pl">bednarz</trd>
  <trd lng="ru">&c_b;&c_o;&c_ch;&c_a;&c_r;</trd>
 <trd lng="sk">v&yacute;robca sudov</trd>
  <trd lng="uk">&c_b;&c_o;&c_n;&c_d;&c_a;&c_r;</trd>
</drv>

<drv mrk="barel.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="barel.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ventrodiki&gcirc;i kiel <tld/>o:
      <ekz>
        kiam via ventro <tld/>i&gcirc;as kaj vi plene &scirc;topas al vi
        la po&scirc;ojn, tiam vi levas la nazon
        <klr>(tiam vi fieri&gcirc;as)</klr>
        <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno 2a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_s;&c_t;&c_a;&c_w;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol; &c_ja;&c_k; <ind>&c_b;&c_o;&c_ch;&c_k;&c_a;</ind></trd>
    <trd lng="br">dont da veza&ntilde; <ind>tev</ind> evel ur
    varrikenn</trd>
    <trdgrp lng="de">
      <trd>zunehmen</trd>,
      <trd>Fett ansetzen</trd>,
      <trd>kugelrund werden</trd>
    </trdgrp>
    <trd lng="en">put on <ind>weight</ind></trd>
    <trdgrp lng="fr">
      <trd>enfler<klr tip="ind"> (comme un tonneau)</klr></trd>,
      <trd>prendre de la <ind>bedaine</ind></trd>,
      <trd>prendre de la <ind>brioche</ind></trd>
    </trdgrp>
    <trd lng="hu">pocakosodik</trd>
    <trd lng="it">diventare (come una) <ind>botte</ind></trd>
    <trdgrp lng="pl">
      <trd>uty&cacute; jak <ind>beka</ind></trd>,
      <trd>spa&sacute;&cacute; si&eogonek; jak <ind>beka</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_o;&c_l;&c_s;&c_t;&c_je;&c_t;&c_mol;
    &c_k;&c_a;&c_k; <ind>&c_b;&c_o;&c_ch;&c_k;&c_a;</ind></trd>
    <trd lng="zh">&#x53D1;&#x80D6; <pr>f&#x101;p&#xE0;ng</pr></trd>
</snc>
  <trd lng="ja">&#x592A;&#x9F13;&#x8179;&#x306B;&#x592A;&#x308B;  <pr>&#x305F;&#x3044;&#x3053;&#x306F;&#x3089;&#x306B;&#x3075;&#x3068;&#x308B;</pr></trd>
  <trd lng="uk">&c_n;&c_a;&c_ji;&c_s;&c_t;&c_i; &c_zh;&c_i;&c_v;&c_ib;&c_t; &c_ja;&c_k; &c_b;&c_o;&c_ch;&c_k;&c_a;/&c_b;&c_o;&c_d;&c_n;&c_ja;</trd>
</drv>

<drv mrk="barel.en0igi">
  <kap>en<tld/>igi</kap>
  <snc mrk="barel.en0igi.TEK">
    <uzo tip="klr">(precipe pri likva&jcirc;o)</uzo>
    <dif>
      Meti en <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>nal&iacute;t do sudu</trd>,
    <trd>napustit do sudu</trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_l;&c_ib;&c_c;&c_mol; &c_u;
    <ind>&c_b;&c_o;&c_ch;&c_k;&c_u;</ind></trd>,
    <trd>&c_n;&c_a;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol; &c_u;
    <ind>&c_b;&c_o;&c_ch;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eindosen</trd>,
    <trd>einfassen</trd>
  </trdgrp>
  <trd lng="en">load into a <ind>barrel</ind></trd>
  <trd lng="fr">mettre en <ind>tonneau</ind></trd>
  <trd lng="hu">hord&oacute;ba t&ouml;lt</trd>
  <trdgrp lng="it">
    <trd>imbarilare</trd>,
    <trd>imbottare</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>w&lstroke;o&zdot;y&cacute; do <ind>beczki</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_m;&c_je;&c_s;&c_t;&c_i;&c_t;&c_mol; &c_v;
    <ind>&c_b;&c_o;&c_ch;&c_k;&c_u;</ind></trd>,
    <trd>&c_z;&c_a;&c_l;&c_i;&c_t;&c_mol; &c_v;
    <ind>&c_b;&c_o;&c_ch;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="sk">napusti&tcaron; do sudu</trd>
</drv>

<drv mrk="barel.el0igi">
  <kap>el<tld/>igi</kap>
  <snc mrk="barel.el0igi.TEK">
    <uzo tip="klr">(precipe pri likva&jcirc;o)</uzo>
    <dif>
      Eligi el <tld/>o.
    </dif>
