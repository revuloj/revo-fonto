<?xml version="1.0"?>

<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>barel</rad>/o
</kap>
<drv mrk="barel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="barel.0o.TEK">
    <dif>
      Ronda ventroforma ujo el lignaj kurbaj tabuletoj kunfirmigitaj per
      feraj a&ubreve; lignaj ringoj kun du plataj fundoj, uzata por enteni
      vinon, akvon a&ubreve; aliajn flua&jcirc;ojn:
      <ekz>
        la kelestro <klr>...</klr> iris al alia <tld/>o, el kiu la mar&scirc;alo
        trinkis
        <fnt><bib>ElektFab</bib>, <lok>du fratoj</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o malplena sonas plej la&ubreve;te
        <klr>(plej fanfaronas senvaloruloj, senmerituloj)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kuv.0o">kuvo</ref>,
      <ref cel="sitel.0o">sitelo</ref>
    </refgrp>
  <trd lng="be">&c_b;&c_o;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="br">
    <trd>barrikenn</trd>,
    <trd>tonell</trd>
  </trdgrp>
  <trd lng="cs">sud</trd>
  <trd lng="de">Fass</trd>
  <trd lng="en">barrel</trd>
  <trd lng="es">barril</trd>
  <trdgrp lng="fr">
    <trd>barrique</trd>,
    <trd>fut<klr> (tonneau)</klr></trd>,
    <trd>tonneau</trd>
  </trdgrp>
  <trd lng="hu">hord&oacute;</trd>
  <trdgrp lng="pt">
    <trd>barril</trd>,
    <trd>pipa</trd>,
    <trd>barrica</trd>,
    <trd>casco <klr tip="amb">(vasilha)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_o;&c_ch;&c_k;&c_a;</trd>
  <trd lng="sk">sud</trd>
  </snc>
  <snc mrk="barel.0o.MEZ">
    <dif>
      <ref tip="super" cel="unu.mezur0o.SCI">Mezurunuo</ref> por nafto
      kaj similaj substancoj, valoranta 159 litrojn:
      <ekz>
        en 1992 Occidental ricevis permeson espluati terenon kiu 
        enhavis 2 bilionojn da <tld/>oj de nafto
        <fnt>
        <aut>S. Oceransky, trad. R. Torres</aut>,
        <vrk><url ref="http://www.geocities.com/andajondoj/okt99/Uua.htm">Vivo au morto por la indi&gcirc;ena popolo Uua</url></vrk>,
        <lok>[vidita je 2006-04-21]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="barel.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="barel.0ejo.TEK">
    <dif>
      <tld lit="B"/>fabriko.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_n;&c_d;&c_a;&c_r;&c_n;&c_ja;</trd>
  <trdgrp lng="br">
    <trd>barazherezh</trd>,
    <trd>tonellerezh</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>B&ouml;ttcherei</trd>,
    <trd>Fassmacherei</trd>,
    <trd>K&uuml;ferei</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cooper's shop</trd>,
    <trd>barrel factory</trd>
  </trdgrp>
  <trd lng="fr">tonnellerie<klr tip="ind"> (fabrique)</klr></trd>
  <trd lng="hu">k&aacute;d&aacute;rm&udblac;hely</trd>
  <trd lng="ru">&c_b;&c_o;&c_n;&c_d;&c_a;&c_r;&c_n;&c_ja;</trd>
</drv>

<drv mrk="barel.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="barel.0eto.malgranda">
    <dif>
      Malgranda <tld/>o:
      <ekz>
        jen estas <tld/>eto da tritiko
        <fnt><bib>FK</bib>, <lok>4, la hejmo de la metiisto</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_a;&c_ch;&c_o;&c_n;&c_a;&c_k;</trd>,
      <trd>&c_b;&c_o;&c_ch;&c_a;&c_ch;&c_k;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>barazhig</trd>,
      <trd>barilh</trd>
    </trdgrp>
    <trd lng="de">F&auml;sschen</trd>
    <trd lng="en">barrel</trd>
    <trd lng="fr">baril</trd>
    <trd lng="hu">csoboly&oacute;</trd>
    <trd lng="ru">&c_b;&c_o;&c_ch;&c_o;&c_n;&c_o;&c_k;</trd>
    <trd lng="sk">s&uacute;dok</trd>
  </snc>
  <snc mrk="barel.0eto.formo">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <tld lit="B"/>forma ujeto de ma&scirc;ino.
    </dif>
    <subsnc mrk="barel.0eto.KAL">
      <uzo tip="fak">KAL</uzo>
      <dif>
        Ujo, en kiu trovi&gcirc;as la risorto de <ref tip="malprt"
        cel="horlog.0o.KAL">horlo&gcirc;o</ref>.
      </dif>
    </subsnc>
    <subsnc mrk="barel.0eto.ARM">
      <dif>
        Turni&gcirc;a peco de revolvero, entenanta la karto&ccirc;ojn.
      </dif>
    </subsnc>
    <trd lng="be">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n; <klr>(&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_mol;&c_n;&c_ib;&c_k;&c_a;,
    &c_r;&c_e;&c_v;&c_a;&c_l;&c_mol;&c_v;&c_e;&c_r;&c_a;)</klr></trd>
    <trd lng="br">taboulin<klr> (un arm, un horolaj)</klr></trd>
    <trdgrp lng="de">
      <trd>Kammer</trd>,
      <trd>Trommel</trd>,
      <trd>Geh&auml;use</trd>
    </trdgrp>
    <trd lng="en">chamber</trd>
    <trd lng="fr">barillet</trd>
    <trdgrp lng="hu">
      <trd>rug&oacute;tok</trd>,
      <trd>forg&oacute;t&aacute;r<klr tip="ind"> (revolveren)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_a;&c_r;&c_a;&c_b;&c_a;&c_n; <klr>(&c_r;&c_je;&c_v;&c_o;&c_l;&c_mol;&c_v;&c_je;&c_r;&c_a;, &c_ch;&c_a;&c_s;&c_o;&c_v;)</klr></trd>
  </snc>
</drv>

<drv mrk="barel.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="barel.0isto.TEK">
    <dif>
      <tld lit="B"/>faristo.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_o;&c_n;&c_d;&c_a;&c_r;</trd>
  <trdgrp lng="br">
    <trd>barazher</trd>,
    <trd>toneller</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>B&ouml;ttcher</trd>,
    <trd>Fassmacher</trd>,
    <trd>K&uuml;fer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cooper</trd>,
    <trd>coopersmith</trd>
  </trdgrp>
  <trd lng="fr">tonnelier</trd>
  <trdgrp lng="hu">
    <trd>k&aacute;d&aacute;r</trd>,
    <trd>bogn&aacute;r</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_o;&c_ch;&c_a;&c_r;</trd>
</drv>

<drv mrk="barel.0igxi">
  <kap><tld/>i&gcirc;i<fnt>Z</fnt></kap>
  <snc mrk="barel.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ventrodiki&gcirc;i kiel <tld/>o:
      <ekz>
        kiam via ventro <tld/>i&gcirc;as kaj vi plene &scirc;topas al vi la po&scirc;ojn,
        tiam vi levas la nazon
        <klr>(tiam vi fieri&gcirc;as)</klr>
        <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno 2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_t;&c_a;&c_w;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol; &c_ja;&c_k; <ind>&c_b;&c_o;&c_ch;&c_k;&c_a;</ind></trd>
  <trd lng="br">dont da veza&ntilde; <ind>tev</ind> evel ur varrikenn</trd>
  <trdgrp lng="de">
    <trd>zunehmen</trd>,
    <trd>Fett ansetzen</trd>,
    <trd>kugelrund werden</trd>
  </trdgrp>
  <trd lng="en">put on <ind>weight</ind></trd>
  <trdgrp lng="fr">
    <trd>enfler<klr tip="ind"> (comme un tonneau)</klr></trd>,
    <trd>prendre de la <ind>bedaine</ind></trd>,
    <trd>prendre de la <ind>brioche</ind></trd>
  </trdgrp>
  <trd lng="hu">pocakosodik</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_o;&c_l;&c_s;&c_t;&c_je;&c_t;&c_mol; &c_k;&c_a;&c_k; <ind>&c_b;&c_o;&c_ch;&c_k;&c_a;</ind></trd>
</drv>

<drv mrk="barel.en0igi">
  <kap>en<tld/>igi</kap>
  <snc mrk="barel.en0igi.TEK">
    <uzo tip="klr">(precipe pri likva&jcirc;o)</uzo>
    <dif>
      Meti en <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_l;&c_ib;&c_c;&c_mol; &c_u; <ind>&c_b;&c_o;&c_ch;&c_k;&c_u;</ind></trd>,
    <trd>&c_n;&c_a;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol; &c_u; <ind>&c_b;&c_o;&c_ch;&c_k;&c_u;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eindosen</trd>,
    <trd>einfassen</trd>
  </trdgrp>
  <trd lng="en">load into a <ind>barrel</ind></trd>
  <trd lng="fr">mettre en <ind>tonneau</ind></trd>
  <trd lng="hu">hord&oacute;ba t&ouml;lt</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_m;&c_je;&c_s;&c_t;&c_i;&c_t;&c_mol; &c_v; <ind>&c_b;&c_o;&c_ch;&c_k;&c_u;</ind></trd>,
    <trd>&c_z;&c_a;&c_l;&c_i;&c_t;&c_mol; &c_v; <ind>&c_b;&c_o;&c_ch;&c_k;&c_u;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="barel.el0igi">
  <kap>el<tld/>igi</kap>
  <snc mrk="barel.el0igi.TEK">
    <uzo tip="klr">(precipe pri likva&jcirc;o)</uzo>
    <dif>
      Eligi el <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_l;&c_ib;&c_c;&c_mol; &c_z; <ind>&c_b;&c_o;&c_ch;&c_k;&c_ib;</ind></trd>,
    <trd>&c_v;&c_y;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol; &c_z; <ind>&c_b;&c_o;&c_ch;&c_k;&c_ib;</ind></trd>,
    <trd>&c_v;&c_y;&c_n;&c_u;&c_c;&c_mol; &c_z; <ind>&c_b;&c_o;&c_ch;&c_k;&c_ib;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aus dem Fass <ind>holen</ind></trd>,
    <trd>absch&uuml;tten</trd>
  </trdgrp>
  <trd lng="en">decant</trd>
  <trd lng="fr">tirer<klr tip="ind"> (du tonneau)</klr></trd>
  <trd lng="hu">hord&oacute;b&oacute;l kit&ouml;lt</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_n;&c_u;&c_t;&c_mol; &c_i;&c_z; <ind>&c_b;&c_o;&c_ch;&c_k;&c_i;</ind></trd>,
    <trd>&c_v;&c_y;&c_l;&c_i;&c_t;&c_mol; &c_i;&c_z; <ind>&c_b;&c_o;&c_ch;&c_k;&c_i;</ind></trd>
  </trdgrp>
</drv>
</art>
