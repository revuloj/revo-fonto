<kap>
  <rad>trole</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="trole.0o">
  <kap><tld/>o</kap>
  <snc mrk="trole.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Elektropera aparato el radeto ruli&gcirc;anta sur
      elektra kablo kaj stangoj, kiuj kondukas la
      kurenton &gcirc;is la suba veturilo:
      <ekz>
        <tld/>busoj uzas ordinarajn vojojn sed havas <tld/>ojn supre
        <fnt>
          <aut>E. Tikhonov</aut>,
          <vrk><url ref="http://sxakludanto.narod.ru/transportiloj.htm">
          Pri publika transportiloj en Novosibirsko</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pantograf.0o">pantografo</ref>
    <bld
      lok="&WCU;/thumb/f/f2/Trolley_Wheel.jpg/320px-Trolley_Wheel.jpg"
      prm="CC BY-SA 4.0"
      ><tld/>rado
        <fnt>
          <aut>Ben Franske, propra verko</aut>
          <lok><url ref="&WCW;/File:Trolley_Wheel.jpg"
              >https://commons.wikimedia.org/wiki/File:Trolley_Wheel.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_l;&c_je;&c_j;</trd>
  <trd lng="ca">tr&ograve;lei</trd>
  <trdgrp lng="cs">
    <trd>sb&ecaron;ra&ccaron;</trd>,
    <trd>trolej</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stromabnehmer <klr>(mit Rolle)</klr></trd>,
    <trd>Rollenstromabnehmer</trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>trolley pole</trd>
     <trd>trolley</trd>
  </trdgrp>
  <trd lng="fr">trolley</trd>
  <trdgrp lng="ja">
    <trd>&#12488;&#12525;&#12522;&#12540;</trd>,
    <trd>&#35302;&#36650;  <pr>&#12373;&#12431;&#12426;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>odbierak pr&aogonek;du</trd>,
    <trd>trolej</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>trolej</trd>,
    <trd>zbern&aacute; ty&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_r;&c_o;&c_l;&c_je;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_t;&c_a;&c_k;&c_t;&c_n;&c_i;&c_j; &c_r;&c_o;&c_l;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_u;&c_m;&c_o;&c_p;&c_r;&c_i;&c_j;&c_m;&c_a;&c_ch;</trd>,
    <trd>&c_r;&c_o;&c_l;&c_i;&c_k;&c_o;&c_v;&c_i;&c_j; &c_s;&c_t;&c_r;&c_u;&c_m;&c_o;&c_z;&c_n;&c_ib;&c_m;&c_a;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38598;&#30005;&#26438; <pr>j&iacute;di&agrave;ng&#462;n</pr></trd>,
    <trd>&#38598;&#38651;&#26751; <pr>j&iacute;di&agrave;ng&#462;n</pr></trd>
  </trdgrp>
