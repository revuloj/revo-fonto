<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>telegraf</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="telegr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="telegr.0o.TEL">
    <uzo tip="fak">TEL</uzo>
    <uzo tip="fak">ELET</uzo>
    <dif>
      Aparato optika a&ubreve; elektra por transsendi per videblaj
      a&ubreve; skribitaj signoj sciigojn &gcirc;is granda distanco:
      <ekz>
        Aureliano Buend&iacute;a redaktis <klr>...</klr> <tld/>on por la
        prezidanto de la respubliko, sed la <tld/>isto rifuzis &gcirc;in
        transsendi
        <fnt>
        <aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent  Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="ca">tel&egrave;graf</trd>
  <trd lng="de">Telegraf</trd>
  <trd lng="en">telegraph</trd>
  <trd lng="es">tel&eacute;grafo</trd>
  <trd lng="fr">t&eacute;l&eacute;graphe</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;v&iacute;r&oacute;(k&eacute;sz&uuml;l&eacute;k)</trd>,
    <trd>telegr&aacute;f</trd></trdgrp>
  <trd lng="nl">telegraaf</trd>
  <trd lng="pl">telegraf</trd>
  <trdgrp lng="pt">
    <trd>tel&eacute;grafo</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_l;&c_je;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="sk">telegraf</trd>
</drv>

<drv mrk="telegr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="telegr.0i.TEL">
    <dif>
      Sciigi per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_g;&c_r;&c_a;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">telegrafiar</trd>
  <trd lng="de">telegrafieren</trd>
  <trd lng="en">telegraph</trd>
  <trd lng="fr">t&eacute;l&eacute;graphier</trd>
  <trd lng="hu">t&aacute;viratozik</trd>
  <trd lng="nl">telegraferen</trd>
  <trd lng="pl">telegrafowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>telegrafar</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_l;&c_je;&c_g;&c_r;&c_a;&c_f;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">telegrafova&tcaron;</trd>
</drv>
</art>
