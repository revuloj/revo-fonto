<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: litin.xml,v 1.18 2024/01/15 19:02:15 revo Exp $">
  <kap>
    <rad>litin</rad>/o
    <fnt>Z</fnt>
  
  </kap>
  <drv mrk="litin.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      <frm>LiOH</frm>, hidroksido de <ref tip="prt" cel="liti.0o">litio</ref>:
      <ekz>
        teknike oni produktas <tld/>on el litiokarbonato kaj kalcia
        hidroksido:
        <frm>Li<sub>2</sub>CO<sub>3</sub> + Ca(OH)<sub>2</sub> &#x27F6; 2 LiOH
        + CaCO<sub>3</sub></frm>,
        alternative oni povas produkti &gcirc;in per elektrolizo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_s;&c_ib;&c_d; <ind>&c_l;&c_ib;&c_t;&c_y;&c_ju;</ind></trd>
  <trd lng="cs">lithin</trd>
  <trd lng="de">Lithiumhydroxid</trd>
  <trd lng="el">&lambda;&jota;&theta;&jota_ton;&ny;&eta;</trd>
  <trd lng="es">litina</trd>
  <trd lng="fr">lithine</trd>
  <trd lng="hu">l&iacute;tium-oxid</trd>
  <trd lng="ja">&#x6C34;&#x9178;&#x5316;&#x30EA;&#x30C1;&#x30A6;&#x30E0;  <pr>&#x3059;&#x3044;&#x3055;&#x3093;&#x304B;&#x30EA;&#x30C1;&#x30A6;&#x30E0;</pr></trd>
  <trd lng="nl">lithiumoxide</trd>
  <trd lng="pl">tlenek litu</trd>
  <trd lng="pt">litina</trd>
  <trd lng="ro">oxid de litiu</trd>
  <trd lng="sk">lith&iacute;n</trd>
  <trd lng="uk">&#x43B;&#x456;&#x442;&#x438;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x6C22;&#x6C27;&#x5316;&#x9502; <pr>q&#x12B;ngy&#x1CE;nghu&#xE0;l&#x1D0;</pr></trd>,
    <trd>&#x6C2B;&#x6C27;&#x5316;&#x92F0; <pr>q&#x12B;ngy&#x1CE;nghu&#xE0;l&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>
