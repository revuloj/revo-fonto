<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>privat</rad>/a
    </kap>

    <drv mrk="privat.0a">
      <kap><ofc>*</ofc><tld/>a</kap>
      <snc mrk="privat.0a.neoficiala">
        <dif>
          Plenumanta nenian oficialan rolon:
          <ekz>
            <tld/>a homo;
          </ekz><ekz>
            <tld/>a oficisto;
          </ekz><ekz>
            tia homo ne povos resti simpla <tld/>ulo.
          </ekz>
        </dif>
      </snc>
      <snc mrk="privat.0a.individua">
        <dif>
          Kiu koncernas nur la personan a&ubreve; familian vivon
          de individuo:
          <ekz>
            <tld/>a domo;
          </ekz><ekz>vivon <tld/>an ka&scirc;u la muroj
<fnt><bib>PrV</bib></fnt>;</ekz><ekz>
            tiu kontrakto estas mia afero pure <tld/>a kaj
            persona<fnt>Z</fnt>.
          </ekz><ekz>
            <tld/>a letero;
          </ekz><ekz>
            &ccirc;u <tld/>a a&ubreve;tomobilo a&ubreve; publika transportilo?
          </ekz>  
        </dif>
      </snc>
      <snc mrk="privat.0a.malpublika">
        <dif>
          Rezervita al limigita rondo da personoj, ne
          <ref tip="ant" cel="publik.0a.malprivata">publika</ref>:
          <ekz>
            <tld/>a enirejo;
          </ekz><ekz>
            <tld/>a proprieto.
          </ekz>  
        </dif>
      </snc>
      <trdgrp lng="de">
        <trd>privat</trd>,
        <trd>Privat-</trd></trdgrp>
      <trdgrp lng="nl">
	<trd>particulier</trd>,
	<trd>priv&eacute;</trd>
      </trdgrp>
      <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="privat.0ulo">
      <kap><tld/>ulo</kap>
      <snc>
        <dif>
	  Homo kiel individuo,
	  natura <ref tip="super" cel="person.0o.homo">persono</ref>
	  &dash; kontraste al organiza&jcirc;o, partio, firmao,
	  &scirc;tato, <ref cel="person.jura0o">jura persono</ref>,
	  <ref cel="funkci.0ulo">funkciulo</ref>, oficialulo ktp:
	  <ekz>
	    la prezo de tiu komputilo estas tro alta por <tld/>ulo.
	  </ekz>
	</dif>
      </snc>
      <trd lng="en">private person</trd>
      <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_n;&c_o;&c_je; &c_l;&c_i;&c_c;&c_o;</trd>
    </drv>

  </art>
