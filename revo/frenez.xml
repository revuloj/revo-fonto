<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: frenez.xml,v 1.28 2021/05/28 12:14:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>frenez</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="frenez.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="frenez.0a.malsana">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Perdinta la prudenton pro mensa malsano.
    </dif>
    <trdgrp lng="de">
      <trd>verr&uuml;ckt</trd>,
      <trd>wahnsinnig</trd>,
      <trd>irr</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>gek</trd>,
      <trd>waanzinnig</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wariat</trd>,
      <trd>szaleniec</trd>
    </trdgrp>
  </snc>
  <snc mrk="frenez.0a.pasieMalsagxega">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Perdinta la sinregadon pro pasio; malsa&gcirc;ega:
      <ekz>
        li estis <tld/>a pro &jcirc;aluzo, &gcirc;ojo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="furioz.0a">furioza</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>verr&uuml;ckt</trd>,
      <trd>wahnsinnig</trd>,
      <trd>durchgedreht</trd>,
      <trd>durchgeknallt</trd>
    </trdgrp>
    <trd lng="nl">gek</trd>
    <trd lng="pl">szaleniec</trd>
  </snc>
  <snc mrk="frenez.0a.sensenca">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Malsa&gcirc;ega, sensenca:
      <ekz>
        <tld/>a amo, deziro, elspezo<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="delir.0o">deliro</ref>,
      <ref cel="ekstra1.0o">ekstravaganco</ref>,
      <ref cel="mani.0o">manio</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="musx.0o">mu&scirc;o</ref>,
      <ref cel="zum.0i">zumi</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>verr&uuml;ckt</trd>,
      <trd>abartig</trd>
    </trdgrp>
    <trd lng="nl">waanzinnig</trd>
    <trd lng="pl">szalony</trd>
  </snc>
  <trd lng="bg">&c_l;&c_u;&c_d;</trd>
  <trd lng="bo">&#xF66;&#xFA8;&#xFB1;&#xF7C;&#xF53;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>bl&aacute;zniv&yacute;</trd>,
    <trd>frenetick&yacute;</trd>,
    <trd>&scaron;&iacute;len&yacute;</trd>
  </trdgrp>
  <trd lng="en">insane</trd>
  <trd lng="es">loco</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&f_y;&a_w;&a_A;&a_n;&a_h;</trd>,
    <trd>&a_m;&a_j;&a_n;&a_w;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fou</trd>,
    <trd>d&eacute;ment</trd>,
    <trd>d&eacute;raisonnable</trd>,
    <trd>insens&eacute;</trd>
  </trdgrp>
  <trd lng="id">gila</trd>
  <trdgrp lng="it">
    <trd>pazzo<klr> (agg.)</klr></trd>,
    <trd>folle<klr> (agg.)</klr></trd>,
    <trd>matto<klr> (agg.)</klr></trd>,
    <trd>insensato<klr> (folle, agg.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ku">
    <trd>d&icirc;n</trd>,
    <trd>&scedil;&ecirc;t</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>louco</trd>,
    <trd>doido</trd>,
    <trd>maluco</trd>,
    <trd>demente</trd>,
    <trd>insano</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_z;&c_u;&c_m;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_a;&c_s;&c_sh;&c_je;&c_d;&c_sh;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bl&aacute;zniv&yacute;</trd>,
    <trd>posadnut&yacute;</trd>,
    <trd>&scaron;ialen&yacute;</trd>
  </trdgrp>
  <trd lng="sv">galen</trd>
  <trd lng="tp">nasa</trd>
</drv>

<drv mrk="frenez.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="frenez.0i.KOMUNE">
    <dif>
      Esti <tld/>a:
      <ekz>
        &ccirc;u vi <tld/>as? &ccirc;u vi deziras ka&ubreve;zi skandalon?
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>bl&aacute;znit</trd>,
    <trd>&scaron;&iacute;let</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verr&uuml;ckt sein</trd>,
    <trd>wahnsinnig sein</trd>,
    <trd>durchgedreht sein</trd>
  </trdgrp>
  <trd lng="en">rave</trd>
  <trd lng="es">estar loco</trd>
  <trd lng="fa">&a_d;&f_y;&a_w;&a_A;&a_n;&a_h;
  &a_b;&a_w;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;raisonner</trd>,
    <trd>&ecirc;tre <ind>fou</ind></trd>,
    <trd>d&eacute;railler<klr tip="ind">
    (d&eacute;raisonner)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>sragionare</trd>,
    <trd>essere <ind>pazo</ind></trd>,
    <trd>essere <ind>folle</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>gek</ind> zijn</trd>,
    <trd><ind>waanzinnig</ind> zijn</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>by&cacute; <ind>szalonym</ind></trd>,
    <trd>by&cacute; <ind>szale&nacute;cem</ind></trd>,
    <trd>szale&cacute;</trd>,
    <trd>by&cacute; <ind>wariatem</ind></trd>,
    <trd>wariowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_u;&c_m;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">bl&aacute;zni&tcaron; sa</trd>
  <trd lng="sv">vara <ind>galen</ind></trd>
</drv>

<drv mrk="frenez.0o">
  <kap><tld/>o,
  <var><kap><tld/>eco</kap></var></kap>
  <snc mrk="frenez.0o.KOMUNE">
    <dif>
      Stato, simptomaro, sinteno de tiu, kiu <tld/>as:
      <ekz>
        tiu &ccirc;i malsano konsistas en tio, ke homoj,
        kiuj &gcirc;is nun estis tute prudentaj, ricevas
        atakon de <tld/>o 
        <fnt>
          <aut>-</aut>,
          <vrk>Rimedo Kontra&ubreve; Esperantismo</vrk>,
          <lok>La Revuo Orienta, 1930-09, p. 265a</lok>
        </fnt>;
      </ekz>
      <ekz>
        li atingis tian gradon de <tld/>eco, ke li volis sin
        mortigi.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>folie</trd>,
    <trd>d&eacute;mence</trd>,
    <trd>d&eacute;raison</trd>,
    <trd>fr&eacute;n&eacute;sie</trd>
  </trdgrp>
  <trd lng="id">ke<ind>gila</ind>an</trd>
  <trdgrp lng="it">
    <trd>pazzia</trd>,
    <trd>follia</trd>
  </trdgrp>
  <trd lng="nl">dementie</trd>
</drv>

<drv mrk="frenez.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="frenez.0ajxo.KOMUNE">
    <dif>
      Ago, diro <tld/>a a&ubreve;
      ekstravaganca:
      <ekz>
        kial vi faras tiajn <tld/>a&jcirc;ojn?
        &ccirc;u ne venis al vi en la kapo, ke eble iu vidos vin?
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">folie<klr tip="ind"> (chose
  d&eacute;raisonnable)</klr></trd>
  <trd lng="it">follia<klr> (atto)</klr></trd>
  <trd lng="nl">folie</trd>
</drv>

<drv mrk="frenez.0iga">
  <kap><tld/>iga</kap>
  <snc mrk="frenez.0iga.KOMUNE">
    <dif>
      Kiu igas iun <tld/>a, malprudenta, kondukas al
      malraciaj agoj:
      <ekz>
        la plej fantaziaj priskriboj pri &scirc;ipopereoj, kiujn mi iam
        legis, restas malproksime de la <tld/>iga realeco de tiu 
        katastrofo
        <fnt>
          <aut>H. A. Luyken</aut>,
          <vrk>Rakonto de Maljuna Klubano</vrk>,
          <lok>Verda Mondo, 1927-10, p. 32a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">affolant</trd>
</drv>

<drv mrk="frenez.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="frenez.0igi.KOMUNE">
    <dif>
      Igi iun <tld/>a:
      <ekz>
        la kolero <tld/>igis lin.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>pobl&aacute;znit</trd>,
    <trd>p&rcaron;ipravit o rozum</trd>,
    <trd>p&rcaron;iv&aacute;d&ecaron;t k &scaron;&iacute;lenstv&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verr&uuml;ckt machen</trd>,
    <trd>wahnsinnig machen</trd>,
    <trd>irr machen</trd>
  </trdgrp>
  <trd lng="en">madden</trd>
  <trd lng="es">enloquecer</trd>
  <trd lng="fa">&a_d;&f_y;&a_w;&a_A;&a_n;&a_h;
  &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">rendre <ind>fou</ind></trd>
  <trdgrp lng="it">
    <trd>far <ind>sragionare</ind></trd>,
    <trd>rendere <ind>pazo</ind></trd>,
    <trd>rendere <ind>folle</ind></trd>,
    <trd>far <ind>impazzire</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>gek</ind> maken</trd>,
    <trd><ind>waanzinnig</ind> maken</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>doprowadza&cacute; do <ind>szale&nacute;stwa</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_s;&c_t;&c_i; &c_s;
  &c_u;&c_m;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>pobl&aacute;zni&tcaron;</trd>,
    <trd>pripravi&tcaron; o rozum</trd>
  </trdgrp>
  <trd lng="sv">g&ouml;ra <ind>galen</ind></trd>
  <trd lng="tp">nasa</trd>
</drv>

<drv mrk="frenez.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="frenez.0igxi.KOMUNE">
    <dif>
      Fari&gcirc;i <tld/>a:
      <ekz>
        <tld/>i&gcirc;i de malamo, pro envio.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">go crazy</trd>
  <trd lng="fa">&a_d;&f_y;&a_w;&a_A;&a_n;&a_h; &a_sh;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>devenir <ind>fou</ind></trd>,
    <trd>perdre la <ind>raison</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verr&uuml;ckt werden</trd>,
    <trd>wahnsinnig werden</trd>,
    <trd>durchdrehen</trd>
  </trdgrp>
  <trd lng="es">volver se loco</trd>
  <trd lng="id">meng<ind>gila</ind></trd>
  <trdgrp lng="it">
    <trd>impazzire</trd>,
    <trd>perdere la <ind>ragione</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>gek</ind> worden</trd>,
    <trd><ind>waanzinnig</ind> worden</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zwariowa&cacute;</trd>,
    <trd>oszale&cacute;</trd>,
    <trd>oczadzie&cacute; <klr>(pot.)</klr></trd>,
    <trd>ocipie&cacute; <klr>(pot., wulg.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_j;&c_t;&c_i; &c_s; &c_u;&c_m;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_je;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_z;&c_u;&c_m;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">bli <ind>galen</ind></trd>
</drv>

<drv mrk="frenez.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="frenez.0ulo.MED">
    <dif>
      Tiu, kiu estas <tld/>a a&ubreve; tia &scirc;ajnas;
      <ref tip="vid" cel="san.mensmal0ulo">mensmalsanulo</ref>:
      <ekz>
        kelkaj <tld/>uloj turnis sin tra la &ccirc;ambro, mirige, ke ili ne 
        sentis la kapturnon
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. J. Patera</aut>,
          <vrk>Pri malsa&gcirc;a Han&ccirc;jo</vrk>,
          <lok>Bulteno de EK Praha, 2003:2, p. 4a-7a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>fou<klr tip="ind"> (subst.)</klr></trd>,
    <trd>ali&eacute;n&eacute;<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="id"><ind>orang</ind> gila</trd>
  <trdgrp lng="it">
    <trd>pazzo</trd>,
    <trd>matto</trd>,
    <trd>folle</trd>,
    <trd>malato psichiatrico</trd>,
    <trd>sbroccato<klr> (pop.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="frenez.0ulejo">
  <kap><tld/>ulejo</kap>
  <snc mrk="frenez.0ulejo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="san.mal0ulejo.MED">Malsanulejo</ref>
      por <tld/>uloj:
    </dif>
    <ekz>
      <ctl>en la <tld/>ulejo</ctl>
      <fnt>
        <aut>E. Matkowski</aut>,
        <vrk><url ref="http://literaturo.org/HARLOW-Don/Esperanto/Literaturo/Revuoj/np/np5602/frenezulejou.html">
        En la frenezulejo</url></vrk>,
        <lok>Norda Prismo, 1956:2, p. 84a</lok>
      </fnt>;
    </ekz>
    <ekz>
      la mondo estas <tld/>ulejo
      <fnt>
        <vrk><url ref="http://tatoeba.org/eng/sentences/show/454691">
          Frazo 454691a</url>
        </vrk>,
        <lok>tatoeba.org</lok>
      </fnt>.
    </ekz>
    <trd lng="fr">h&ocirc;pital psychiatrique</trd>
    <trd lng="id"><ind>rumah sakit</ind> jiwa</trd>
    <trdgrp lng="pl">
      <trd>zak&lstroke;ad <ind>psychiatryczny</ind></trd>,
      <trd>szpital <ind>psychiatryczny</ind></trd>,
      <trd>dom <ind>wariat&oacute;w</ind> <klr>(pot.)</klr></trd>,
      <trd>wariatkowo <klr>(pot.)</klr></trd>
    </trdgrp>
  </snc>
</drv>

</art>
