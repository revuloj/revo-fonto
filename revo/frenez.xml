<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>frenez</rad>/a
</kap>
<drv mrk="frenez.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc num="1">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Perdinta la prudenton pro mensa malsano:
      <ekz>
        hospitalo por <ind><tld/>ulo</ind>j.
        <trdgrp lng="fr">
          <trd>fou<klr tip="ind"> (subst.)</klr></trd>,
          <trd>ali&eacute;n&eacute;<klr tip="ind"> (subst.)</klr></trd></trdgrp> 
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>verr&uuml;ckt</trd>,
       <trd>wahnsinnig</trd>,
       <trd>irr</trd></trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Perdinta la sinregadon pro pasio; malsa&gcirc;ega:
      <ekz>li estis <tld/>a pro &jcirc;aluzo, &gcirc;ojo;</ekz>
      <ekz>
        li atingis tian gradon de <ind><tld/>eco</ind>, ke li volis sin mortigi.
        <trdgrp lng="fr">
          <trd>folie</trd>,
          <trd>d&eacute;mence</trd>,
          <trd>d&eacute;raison</trd>,
          <trd>fr&eacute;n&eacute;sie</trd></trdgrp> 
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="furioz.0a">furioza</ref>.
    </refgrp>
    <trdgrp lng="de">
       <trd>verr&uuml;ckt</trd>,
       <trd>wahnsinnig</trd>,
       <trd>durchgedreht</trd>,
       <trd>durchgeknallt</trd></trdgrp>
  </snc>
  <snc num="3">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Malsa&gcirc;ega, sensenca:
      <ekz><tld/>a amo, deziro, elspezo<fnt>B</fnt>;</ekz>
      <ekz>
        tio estas nur multekosta <ind><tld/>a&jcirc;o</ind>.
        <trd lng="fr">folie<klr tip="ind"> (chose d&eacute;raisonnable)</klr></trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mani.0o">manio</ref>,
      <ref cel="ekstra1.0o">ekstravaganco</ref>,
      <ref cel="delir.0o">deliro</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="zum.0i">zumi</ref>,
      <ref cel="musx.0o">mu&scirc;o</ref>.
    </refgrp>
    <trdgrp lng="de">
       <trd>verr&uuml;ckt</trd>,
       <trd>abartig</trd></trdgrp>
  </snc>
  <trd lng="en">insane</trd>
  <trdgrp lng="fr">
    <trd>fou</trd>,
    <trd>d&eacute;ment</trd>,
    <trd>d&eacute;raisonnable</trd>,
    <trd>insens&eacute;</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_z;&c_u;&c_m;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_a;&c_s;&c_sh;&c_je;&c_d;&c_sh;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">galen</trd>
</drv>
<drv mrk="frenez.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>verr&uuml;ckt sein</trd>,
     <trd>wahnsinnig sein</trd>,
     <trd>durchgedreht sein</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;raisonner</trd>,
    <trd>&ecirc;tre <ind>fou</ind></trd>,
    <trd>d&eacute;railler<klr tip="ind"> (d&eacute;raisonner)</klr></trd></trdgrp> 
  <trd lng="ru">&c_b;&c_je;&c_z;&c_u;&c_m;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">vara <ind>galen</ind></trd>
</drv>
<drv mrk="frenez.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi iun <tld/>a: <ekz>la kolero <tld/>igis lin.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>verr&uuml;ckt machen</trd>,
     <trd>wahnsinnig machen</trd>,
     <trd>irr machen</trd></trdgrp>
  <trd lng="fr">rendre <ind>fou</ind></trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_s;&c_t;&c_i; &c_s; &c_u;&c_m;&c_a;</trd>
  <trd lng="sv">g&ouml;ra <ind>galen</ind></trd>
</drv>
<drv mrk="frenez.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Fari&gcirc;i <tld/>a: <ekz><tld/>i&gcirc;i de malamo, pro envio.</ekz>
    </dif>
  </snc>
    <trdgrp lng="de">
       <trd>verr&uuml;ckt werden</trd>,
       <trd>wahnsinnig werden</trd>,
       <trd>durchdrehen</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_j;&c_t;&c_i; &c_s; &c_u;&c_m;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_m;&c_je;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_z;&c_u;&c_m;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>devenir <ind>fou</ind></trd>,
    <trd>perdre la <ind>raison</ind></trd></trdgrp> 
  <trd lng="sv">bli <ind>galen</ind></trd>
</drv>
</art>
