<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: panegi.xml,v 1.20 2024/02/16 19:24:10 revo Exp $">
<kap>
  <rad>panegir</rad>/a
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="panegi.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="panegi.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Solena <ref tip="dif" cel="parol.lauxd0o.BELE">la&ubreve;dparolo</ref>:
      <ekz>
        en multaj paroladoj &ndash; ekzemple en &ccirc;iuj tostoj, <tld/>oj, oracioj kaj en aliaj
        okazaj paroladoj &ndash; mankos tute certe la anonco kaj tre ver&scirc;ajne la refuto
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Vi kronis Vin majstra, genia, unika, princa, verkis abundon da plumpaj
        Mem-<tld/>oj el dikta (ebur)tureto Via-provinca
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        post &ccirc;i tiu duopa <tld/>o <klr>[&#x2026;]</klr> mi nun transirus al ia manlibreca
        listigo de nia nuntempa Esperanta poetaro, <klr>[&#x2026;]</klr> disdonante bonajn
        kritikistajn poentojn jen al &ccirc;i tiu jen al alia
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#x2116;6</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ditira.0o">ditirambo</ref>,
      <ref cel="lauxdaci.0o">la&ubreve;dacio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_e;&c_g;&c_ib;&c_r;&c_y;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>chvalo&rcaron;e&ccaron;</trd>,
    <trd>panegyrik</trd>,
    <trd>panegyrika</trd>,
    <trd>panegyrikon</trd>,
    <trd>slavnostn&iacute; &rcaron;e&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Panegyrikos</trd>,
    <trd>Festrede</trd>
  </trdgrp>
  <trd lng="en">panegyric</trd>
  <trd lng="fr">pan&eacute;gyrique<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>panegirisz</trd>,
    <trd>dicshimnusz</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x79F0;&#x8CDB;&#x306E;&#x6F14;&#x8AAC;  <pr>&#x3057;&#x3087;&#x3046;&#x3055;&#x3093;&#x306E;&#x3048;&#x3093;&#x305C;&#x3064;</pr></trd>,
    <trd>&#x8CDB;&#x8F9E;  <pr>&#x3055;&#x3093;&#x3058;</pr></trd>
  </trdgrp>
  <trd lng="nl">lofrede</trd>
  <trd lng="pl">panegiryk</trd>
  <trd lng="pt">paneg&iacute;rico</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_n;&c_je;&c_g;&c_i;&c_r;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_h;&c_v;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">chv&aacute;lore&ccaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_n;&c_je;&c_g;&c_ib;&c_r;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="panegi.0a">
  <kap><tld/>a</kap>
  <snc mrk="panegi.0a.BELE">
    <dif>
      Solene la&ubreve;da:
      <ekz>
        ne estas mia intenco polemiki pri tiu entute <tld/>a komuniko
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 10 (108)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_e;&c_g;&c_ib;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>panegyrisch</trd>,
    <trd>lobrednerisch</trd>
  </trdgrp>
  <trd lng="en">panegyrical</trd>
  <trdgrp lng="fr">
    <trd>louangeur<klr> (adj.)</klr></trd>,
    <trd>pan&eacute;gyrique<klr> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>panegirikus</trd>,
    <trd>magasztal&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x79F0;&#x8CDB;&#x306E;  <pr>&#x3057;&#x3087;&#x3046;&#x3055;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x8CDB;&#x8F9E;&#x306E;  <pr>&#x3055;&#x3093;&#x3058;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">prijzend</trd>
  <trd lng="pl">panegiryczny</trd>
  <trd lng="pt">paneg&iacute;rico</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_n;&c_je;&c_g;&c_i;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_je;&c_b;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_n;&c_je;&c_g;&c_ib;&c_r;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_je;&c_b;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="panegi.0i">
  <kap><tld/>i</kap>
  <snc mrk="panegi.0i.BELE">
    <dif>
      Emfaze, bombaste <ref tip="super" cel="lauxd.0i">la&ubreve;di</ref>:
      <ekz>
        li <tld/>is la firmajn aran&gcirc;ojn por volontuloj
        <fnt><aut>J. Francis</aut>,
        <vrk>La Granda Kaldrono</vrk>, 
        <lok>1978</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>lobpreisen</trd>,
    <trd>beweihr&auml;uchern</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>encenser<klr> (fig.)</klr></trd>,
    <trd>louanger</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x79F0;&#x8CDB;&#x306E;&#x6F14;&#x8AAC;&#x3092;&#x3059;&#x308B;  <pr>&#x3057;&#x3087;&#x3046;&#x3055;&#x3093;&#x306E;&#x3048;&#x3093;&#x305C;&#x3064;&#x3092;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x8CDB;&#x8F9E;&#x3092;&#x8AAD;&#x307F;&#x4E0A;&#x3052;&#x308B;  <pr>&#x3055;&#x3093;&#x3058;&#x3092;&#x3088;&#x307F;&#x3042;&#x3052;&#x308B;</pr></trd>,
    <trd>&#x8CDB;&#x8F9E;&#x3092;&#x66F8;&#x304F;  <pr>&#x3055;&#x3093;&#x3058;&#x3092;&#x304B;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_g;&c_o;&c_l;&c_o;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i; &c_p;&c_a;&c_n;&c_je;&c_g;&c_ib;&c_r;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6B4C;&#x980C; <pr>g&#x113;s&#xF2;ng</pr></trd>,
    <trd>&#x6B4C;&#x9882; <pr>g&#x113;s&#xF2;ng</pr></trd>,
    <trd>&#x548F;&#x8D5E; <pr>y&#x1D2;ngz&#xE0;n</pr></trd>,
    <trd>&#x8A60;&#x8D0A; <pr>y&#x1D2;ngz&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
