<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>panegir</rad>/a<fnt>Z</fnt></kap>
<drv mrk="panegi.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Solene la&ubreve;da.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>panegyrisch</trd>,
    <trd>lobrednerisch</trd>
  </trdgrp>
  <trd lng="nl">prijzend</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_n;&c_je;&c_g;&c_i;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_l;&c_je;&c_b;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="panegi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Longa solena parola a&ubreve; skriba la&ubreve;do.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Panegyrikos</trd>,
    <trd>Festrede</trd>
  </trdgrp>
  <trd lng="nl">lofrede</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_n;&c_je;&c_g;&c_i;&c_r;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_h;&c_v;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
