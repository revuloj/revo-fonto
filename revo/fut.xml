<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fut.xml,v 1.23 2023/08/31 13:11:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fut</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fut.0o.MEZ">
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:longoj">Mezurunuo</ref>
      de longo proksimuma de piedo, la&ubreve; la landoj varianta 
      inter 26,5&#x202F;kaj&#x202F;33 cm (&ccirc;. triono de metro a&ubreve; 12 <ref tip="prt" cel="col.0o">coloj</ref>)
<!-- tro detala, ta&ubreve;ga por leksikono sed ne &gcirc;enerala vortaro!
      futo Argentina = 28,9 cm,
      futo Babilona = 26,45 cm,
      futo Badena (Germanujo) = 30 cm,
      futo Bavara (Germanujo) = 29,18 cm,
      futoj Belgaj = 28,6 cm, 29,5 cm kaj 32,49 cm,
      futo Berlina (Germanujo) = 39 cm,
      futo Brita = 30,48 cm,
      futo &Ccirc;ilia = 28 cm,
      futo Dana = 31,4 cm,
      futo de Frankfurto &ccirc;e Majno (Germanujo) = 28,5 cm,
      futo Franca = 32,5 cm [pied du roi],
      futo Hamburga (Germanujo) = 28,66 cm,
      futo Hispana = 27,86 cm,
      futo Hungara = 31,6 cm,
      futo Itala = 30 cm,
      futo Japana = 30,3 cm,
      futo Nederlanda = 28,3 cm,
      futo Norvega = 31,4 cm,
      futo Olimpa = 30,24 cm,
      futo Pariza = 32,5 cm,
      futo Pola = 28,8 cm,
      futo Portugala = 33 cm,
      futoj Prusaj = 31,4 cm, 37,6 cm,
      futo Romia = 29,6 cm,
      futo Rusa = 28,3 cm [fut: 30,5 cm],
      futo Sveda = 29,7 cm,
      futo Svisa = 30 cm,
      futo Usona = 30,48 cm,
      futo Viena = 28,8 cm,
      futo Virtemberga (Germanujo) = 28,6 cm
-->
      <ekz>
        la flankaj galerio <klr>[&#x2026;]</klr> havis la longon de tridek tri <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 41:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        alta je kvin <tld/>oj kaj kvin coloj
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi flugas dek <tld/>ojn malsupren, &mdash; jen mi ku&scirc;as
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la maro profundi&gcirc;is unu <tld/>o plu, mi pu&scirc;egis la floson sur ebenan pecon
        da tero, por &gcirc;in alligi
        <fnt>
          <bib>Rob</bib>
          <lok>Parto II</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili nun flugis je 6&#x202F;000 <tld/>oj super la kanada arbaro
        <fnt>
          <aut>J. Francis</aut>
          <vrk>La Granda Kaldrono</vrk>
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>foot</trd>,
    <trd>stopa <klr>(m&iacute;ra)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Fu&szlig; <klr>(Ma&szlig;einheit)</klr></trd>,
     <trd>Schuh <klr>(Ma&szlig;einheit)</klr></trd></trdgrp>
  <trd lng="en">foot</trd>
  <trd lng="es">pie<klr> (medida de longitud)</klr></trd>
  <trd lng="fr">pied <klr tip="ind">(mesure)</klr></trd>
  <trd lng="hu">l&aacute;b<klr tip="ind"> (hosszm&eacute;rt&eacute;k)</klr></trd>
  <trd lng="ja">&#x30D5;&#x30A3;&#x30FC;&#x30C8;</trd>
  <trd lng="nl">voet<klr tip="ind"> (lengtemaat)</klr></trd>
  <trd lng="pl">stopa<klr tip="amb"> (jednostka d&lstroke;ugo&sacute;ci)</klr></trd>
  <trd lng="pt">p&eacute; <klr tip="amb">(medida de comprimento)</klr></trd>
  <trd lng="ru">&c_f;&c_u;&c_t;</trd>
  <trd lng="sk">stopa <klr>(d&lacute;&zcaron;kov&aacute; miera)</klr></trd>
  <trd lng="uk">&c_f;&c_u;&c_t; (&c_m;&c_ib;&c_r;&c_a; &c_d;&c_o;&c_v;&c_zh;&c_i;&c_n;&c_i;)</trd>
</drv>
</art>
