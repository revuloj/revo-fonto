<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fut</rad>/o
</kap>
<drv mrk="fut.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de longo,
      diversa la&ubreve; la landoj, valoranta proksimume trionon de metro.
    </dif>
  </snc>
  <trd lng="fr">pied <klr tip="ind">(mesure)</klr></trd>
  <trd lng="ru">&c_f;&c_u;&c_t;</trd>
</drv>
</art>
