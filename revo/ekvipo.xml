<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekvipo.xml,v 1.22 2025/03/02 22:30:32 revo Exp $">
<kap>
<rad>ekvipolent</rad>/a
<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="ekvipo.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="ekvipo.0a.strekoj">
    <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
    <dif>
      <klr>(p.p. du <ref cel="strek.0o.segmento">strekoj</ref>)</klr>
      <ref cel="egal.0a.izometria">Egalaj</ref> kaj paralelaj.
    </dif>
  </snc>
  <snc mrk="ekvipo.0a.punktoparoj">
    <uzo tip="klr">(afina geometrio)</uzo>
    <dif>
      <klr>(p.p. du <ref cel="par.punkto0o">punktoparoj</ref>
      <frm am="(A,B)">(A,B)</frm> kaj <frm am="(C,D)">(C,D)</frm>)</klr>
      Tiaj, ke la <ref cel="mez.0o.punkto.afina">mezo</ref> de
      <frm am="(A,D)">(A,D)</frm> egalas al tiu de <frm am="(B,C)">(B,C)</frm>:
      <ekz>
        <tld/>aj punktoparoj formas
        <ref cel="parale2.0o">paralelogramon</ref>;
      </ekz>
      <ekz>
        <ind><tld/>eco</ind> estas
        <ref cel="rilat.ekvivalento0o">ekvivalento-rilato</ref>;
        <trd lng="ca">equipol&middot;l&egrave;ncia</trd>
        <trd lng="fr">&eacute;quipollence</trd>
        <trd lng="nl">equipollentie</trd>
        <trd lng="pl">przestrze&nacute; wektorowa</trd>
      </ekz>
      <ekz>
        la <ref cel="ar.kvocienta0o">kvocienta aro</ref> de <tld/>eco
        estas
        <ref cel="spac.vektora0o">vektora spaco</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_v;&c_ib;&c_p;&c_a;&c_l;&c_je;&c_n;&c_t;&c_n;&c_y;</trd>
  <trd lng="ca">equipol&middot;lent</trd>
  <trd lng="cs">ekvipolentn&iacute;</trd>
  <trdgrp lng="de">
     <trd>&auml;quipollent</trd>
  </trdgrp>
  <trd lng="en">equipollent</trd>
  <trd lng="fr">&eacute;quipollent</trd>
  <trdgrp lng="ja">
    <trd>&#x540C;&#20516;&#x306E;  <pr>&#12393;&#12358;&#12354;&#x305F;&#12356;&#x306E;</pr></trd>,
    <trd>&#x7B49;&#20516;&#x306E;  <pr>&#12392;&#12358;&#12354;&#x305F;&#12356;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">equipollent</trd>
  <trdgrp lng="pl">
    <trd>r&oacute;wnoznaczny</trd>,
    <trd>o tej samej d&lstroke;ugo&sacute;ci i kierunku <klr>(wektor)</klr></trd>
  </trdgrp>
  <trd lng="pt">eq&uuml;ipolente</trd>
  <trd lng="ru">&c_e;&c_k;&c_v;&c_i;&c_p;&c_o;&c_l;&c_l;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">ekvipolentn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_ib;&c_v;&#x43D;&#x43E;&c_z;&#x43D;&c_a;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_je;&#x43A;&c_v;&c_ib;&#x43F;&#x43E;&#x43B;&c_je;&#x43D;&c_t;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

