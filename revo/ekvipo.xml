<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ekvipolent</rad>/a
</kap>
<drv mrk="ekvipo.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="ekvipo.0a.strekoj">
    <fnt><bib>MatTerm</bib><lok>p. 27</lok></fnt>
    <dif>
      <klr>(p.p. du <ref cel="strek.0o.segmento">strekoj</ref>)</klr>
      <ref cel="egal.0a.izometria">Egalaj</ref> kaj paralelaj.
    </dif>
  </snc>
  <snc mrk="ekvipo.0a.punktoparoj">
    <uzo tip="klr">(afina geometrio)</uzo>
    <dif>
      <klr>(p.p. du <ref cel="par.punkto0o">punktoparoj</ref>
      <frm>(A,B)</frm> kaj <frm>(C,D)</frm>)</klr>
      Tiaj, ke la <ref cel="mez.0o.punkto.afina">mezo</ref> de
      <frm>(A,D)</frm> egalas al tiu de <frm>(B,C)</frm>:
      <ekz>
        <tld/>aj punktoparoj formas 
        <ref cel="parale2.0o">paralelogramon</ref>;
      </ekz>
      <ekz>
        <ind><tld/>eco</ind> estas
        <ref cel="rilat.ekvivalento0o">ekvivalento-rilato</ref>;
        <trd lng="fr">&eacute;quipollence</trd>
        <trd lng="nl">equipollentie</trd>
      </ekz>
      <ekz>
        la <ref cel="ar.kvocienta0o">kvocienta aro</ref> de <tld/>eco
        estas
        <ref cel="spac.vektora0o">vektora spaco</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_v;&c_ib;&c_p;&c_a;&c_l;&c_je;&c_n;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="de">
     <trd>&auml;quipollent</trd>
  </trdgrp>
  <trd lng="en">equipollent</trd>
  <trd lng="fr">&eacute;quipollent</trd>
  <trd lng="nl">equipollent</trd>
  <trd lng="pt">eq&uuml;ipolente</trd>
  <trd lng="ru">&c_e;&c_k;&c_v;&c_i;&c_p;&c_o;&c_l;&c_l;&c_je;&c_n;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>
</art>

