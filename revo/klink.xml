<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klink</rad>/o
  <fnt>V</fnt>
</kap>
<drv mrk="klink.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Peco de pordofermilo, konsistanta el metala dika lameno,
      pivotfiksita &ccirc;e unu ekstrema&jcirc;o, kaj kiun oni povas levante
      a&ubreve; mallevante enigi en hokon fiksitan &ccirc;e la pordokadro. 
    </dif>
    <refgrp tip="sin">
      <ref cel="ans.0o">anso 1</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Klinke</trd>
  <trdgrp lng="en">
    <trd>latch</trd>,
    <trd>spring bolt</trd>
  </trdgrp>
  <trd lng="nl">klink</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_v;&c_je;&c_r;&c_n;&c_o;&c_j; &c_k;&c_r;&c_ju;&c_k;</trd>,
    <trd>&c_shch;&c_je;&c_k;&c_o;&c_l;&c_d;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_v;&c_i;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
