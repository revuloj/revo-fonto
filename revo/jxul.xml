<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&Jcirc;ul</rad>/o
</kap>
<drv mrk="jxul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Anglalingve: James <trd lng="en">Joule</trd>, 1818-1889. Angla
      fizikisto.
    </dif>
  </snc>
  <trd lng="be">&c_D;&c_zh;&c_o;&c_w;&c_l;&c_mol;
  <klr>(&c_a;&c_n;&c_g;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_ib;
  &c_f;&c_ib;&c_z;&c_ib;&c_k;)</klr></trd>
  <trd lng="de">Joule</trd>
  <trd lng="fr">Joule</trd>
  <trd lng="hu">Joule</trd>
  <trd lng="nl">James <ind>Joule</ind></trd>
  <trd lng="pt">James <ind>Joule</ind></trd>
  <trd lng="ru">&c_D;&c_zh;&c_o;&c_u;&c_l;&c_mol;</trd>
  <trd lng="it">Joule</trd>
</drv>

<drv mrk="jxul.jx0o">
  <kap><tld lit="&jcirc;"/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="energi.0o.FIZ">energio</ref> en la Internacia Sistemo de
      unuoj, egala al la <ref cel="labor.0o.FIZ">laboro</ref> de
      me&hcirc;anika forto je unu <ref cel="neuxto.0o">ne&ubreve;tono
      </ref>, kies aplikopunkto movi&gcirc;as je unu
      <ref cel="metr.0o.unuo">metro</ref>
      la&ubreve; la direkto de la forto; simb. J:
      <ekz>
        en elektrodinamiko unu <tld lit="&jcirc;"/>o estas laboro
        efektivigita de elektra kvanto je unu kulombo fluanta sub tensio
        je unu volto.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_zh;&c_o;&c_w;&c_l;&c_mol;
  <klr>(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;
  &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="de">Joule</trd>
  <trd lng="en">joule</trd>
  <trd lng="es">julio</trd>
  <trd lng="fr">joule<klr tip="ind"> (unit&eacute; de mesure)</klr>
  </trd>
  <trd lng="hu">joule</trd>
  <trd lng="nl">joule</trd>
  <trd lng="pl">d&zdot;ul</trd>
  <trd lng="ru">&c_d;&c_zh;&c_o;&c_u;&c_l;&c_mol;</trd>
  <trd lng="it">joule</trd>
</drv>
</art>
