<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&Jcirc;ul</rad>/o
</kap>
<drv mrk="jxul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Anglalingve: James <trd lng="en">Joule</trd>, 1818-1889. Angla fizikisto.
    </dif>
  </snc>
  <trd lng="fr">Joule</trd>
  <trd lng="ru">&c_D;&c_zh;&c_o;&c_u;&c_l;&c_mol;</trd>
</drv>
<drv mrk="jxul.jx0o">
  <kap><tld lit="&jcirc;"/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="energi.0o.FIZ">energio</ref> en la Internacia Sistemo de
      unuoj, egala al la <ref cel="labor.0o.FIZ">laboro</ref> de me&hcirc;anika
      forto je unu <ref cel="neuxto.0o">ne&ubreve;tono</ref>, kies 
      aplikopunkto movi&gcirc;as je unu <ref cel="metr.0o.unuo">metro</ref>
      la&ubreve; la direkto de la forto; simb. J:
      <ekz>
        en elektrodinamiko unu <tld lit="&jcirc;"/>o estas laboro efektivigita 
        de elektra kvanto je unu kulombo fluanta sub tensio je unu volto.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Joule</trd>
  <trd lng="en">joule</trd>
  <trd lng="es">julio</trd>
  <trd lng="fr">joule<klr tip="ind"> (unit&eacute; de mesure)</klr></trd>
  <trd lng="nl">joule</trd>
  <trd lng="ru">&c_d;&c_zh;&c_o;&c_u;&c_l;&c_mol;</trd>
</drv>
</art>
