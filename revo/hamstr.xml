<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hamstr.xml,v 1.41 2023/11/15 16:45:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hamstr</rad>/o
</kap>

<drv mrk="hamstr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hamstr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="rongx.0ulo" lst="voko:ron&gcirc;uloj">Ron&gcirc;ulo</ref> 
      vivanta en grenkampoj kaj stepoj, povanta porti 
      <ref cel="gren.0o">grenon</ref> en siaj 
      <ref cel="vang.0o">vango</ref>sakoj; 
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> 
      el familio
      <ref tip="malprt" cel="hamstr1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cricetus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_m;&c_ja;&c_k;</trd>
  <trdgrp lng="br">
    <trd>hamster</trd>,
    <trd>razh-segal</trd>
  </trdgrp>
  <trd lng="cs">k&rcaron;e&#x10D;ek</trd>
  <trd lng="de">Hamster</trd>
  <trd lng="en">hamster</trd>
  <trd lng="es">h&aacute;mster</trd>
  <trd lng="fr">hamster</trd>
  <trd lng="hu">h&ouml;rcs&ouml;g</trd>
  <trd lng="id">hamster</trd>
  <trd lng="it">criceto</trd>
  <trdgrp lng="ja">
    <trd>&#x30CF;&#x30E0;&#x30B9;&#x30BF;&#x30FC;</trd>,
    <trd>&#x30AD;&#x30CC;&#x30B2;&#x30CD;&#x30BA;&#x30DF;</trd>
  </trdgrp>
  <trd lng="nl">hamster</trd>
  <trd lng="pl">chomik</trd>
  <trd lng="pt">hamster</trd>
  <trd lng="ru">&c_h;&c_o;&c_m;&c_ja;&c_k;</trd>
  <trd lng="sk">&scaron;kre&#x10D;ok</trd>
  <trd lng="sv">hamster</trd>
  <trd lng="uk">&c_h;&#x43E;&#x43C;&#x2019;&#x44F;&#x43A;</trd>
  <trd lng="zh">&#x4ED3;&#x9F20; <pr>c&#x101;ngsh&#x1D4;</pr></trd>
</drv>

<drv mrk="hamstr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hamstr.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tro <ref tip="super" cel="kolekt.0i.kunmeti">kolekti</ref> kaj 
      <ref tip="super" cel="konser.0i.pluhavi">konservi</ref> provize, 
      kiel faras <tld/>oj:
      <ekz>
        ne malmultaj e&ubreve;ropanoj <tld/>is medikamentojn
        <fnt>
        <aut>S. Maul</aut>,
        <vrk><url ref="&Monato;/2006/008913.php">Paniko</url></vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2006a, numero 4a, p. 7a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_z;&c_a;&c_p;&c_a;&c_sh;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>hromadit</trd>,
    <trd>k&rcaron;e&#x10D;kovat</trd>,
    <trd>st&rcaron;&#xE1;dat</trd>,
    <trd>&scaron;kudlit</trd>
  </trdgrp>
  <trd lng="de">hamstern</trd>
  <trd lng="en">hoard</trd>
  <trdgrp lng="fr">
    <trd>emmagasiner</trd>,
    <trd>th&eacute;sauriser</trd>
  </trdgrp>
  <trdgrp lng="it"><trd>accaparrare</trd>, 
  <trd>accumulare<klr> (raccogliere)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x305F;&#x3081;&#x8FBC;&#x3080;  <pr>&#x305F;&#x3081;&#x3053;&#x3080;</pr></trd>,
    <trd>&#x8CB7;&#x3044;&#x3060;&#x3081;&#x3059;&#x308B;  <pr>&#x304B;&#x3044;&#x3060;&#x3081;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">hamsteren</trd>
  <trd lng="pl">chomikowa&cacute;</trd>
  <trdgrp lng="sk">
    <trd>zhroma&#x17E;&#x10F;ova&tcaron;</trd>,
    <trd>&scaron;meli&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#x43F;&c_a;&c_s;&c_a;&c_t;&c_i;&c_s;&#x44F; (&#x43F;&c_r;&#x43E;&c_d;&c_u;&#x43A;&c_t;&c_a;&#x43C;&c_i;)</trd>,
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; (&#x43D;&c_je;&#x43F;&#x43E;&c_t;&c_r;&c_ib;&c_b;&#x43D;&c_ib;) &c_z;&c_a;&#x43F;&c_a;&c_s;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x56E4;&#x79EF; <pr>t&#xFA;nj&#x12B;</pr></trd>
</drv>
</art>
