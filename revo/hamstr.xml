<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hamstr</rad>/o
</kap>
<drv mrk="hamstr.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="hamstr1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cricetus</trd>), rodulo vivanta en grenkampoj
      kaj stepoj, povanta porti grenon en siaj vangosakoj.
    </dif>
  </snc>                      
  <trd lng="be">&c_h;&c_a;&c_m;&c_ja;&c_k;</trd>
  <trdgrp lng="br">  
    <trd>hamster</trd>
    <trd>razh-segal</trd>
  </trdgrp>
  <trd lng="de">Hamster</trd>
  <trd lng="es">h&aacute;mster</trd>
  <trd lng="fr">hamster</trd>
  <trd lng="hu">h&ouml;rcs&ouml;g</trd>
  <trd lng="nl">hamster</trd>
  <trd lng="pt">hamster</trd>
  <trd lng="ru">&c_h;&c_o;&c_m;&c_ja;&c_k;</trd>
  <trd lng="sv">hamster</trd>
</drv>
</art>
