<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>hamstr</rad>/o
</kap>
<drv mrk="hamstr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="hamstr.edoj"><tld/>edoj</ref>
      (<trd lng="la">Cricetus</trd>), rodulo vivanta en grenkampoj
      kaj stepoj, povanta porti grenon en siaj vangosakoj.
    </dif>
  </snc>
  <trd lng="de">Hamster</trd>
</drv>
<drv mrk="hamstr.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="rod1.uloj">roduloj</ref>
      (<trd lng="la">Cricetidae</trd>).
    </dif>
  </snc>
  <trd lng="de">W&uuml;hler</trd>
</drv>
</art>
