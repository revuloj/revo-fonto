<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hamstr.xml,v 1.43 2025/07/21 20:00:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>hamstr</rad>/o
<fnt><bib>UV</bib></fnt></kap>

<drv mrk="hamstr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hamstr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="rongx.0ulo" lst="voko:ron&gcirc;uloj">Ron&gcirc;ulo</ref> 
      vivanta en grenkampoj kaj stepoj, povanta porti 
      <ref cel="gren.0o">grenon</ref> en siaj 
      <ref cel="vang.0o">vango</ref>sakoj; 
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> 
      el familio
      <ref tip="malprt" cel="hamstr1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cricetus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_m;&c_ja;&c_k;</trd>
  <trdgrp lng="br">
    <trd>hamster</trd>,
    <trd>razh-segal</trd>
  </trdgrp>
  <trd lng="ca">h&agrave;mster</trd>
  <trd lng="cs">k&rcaron;e&ccaron;ek</trd>
  <trd lng="cv">&c_a;&c_r;&c_l;&c_a;&c_n;</trd>
  <trd lng="de">Hamster</trd>
  <trd lng="en">hamster</trd>
  <trd lng="es">h&aacute;mster</trd>
  <trd lng="fr">hamster</trd>
  <trd lng="hu">h&ouml;rcs&ouml;g</trd>
  <trd lng="id">hamster</trd>
  <trd lng="it">criceto</trd>
  <trdgrp lng="ja">
    <trd>&#12495;&#12512;&#12473;&#12479;&#12540;</trd>,
    <trd>&#12461;&#12492;&#12466;&#12493;&#12474;&#12511;</trd>
  </trdgrp>
  <trd lng="nl">hamster</trd>
  <trd lng="pl">chomik</trd>
  <trd lng="pt">hamster</trd>
  <trd lng="ru">&c_h;&c_o;&c_m;&c_ja;&c_k;</trd>
  <trd lng="sk">&scaron;kre&ccaron;ok</trd>
  <trd lng="sv">hamster</trd>
  <trd lng="uk">&c_h;&c_o;&c_m;&#8217;&c_ja;&c_k;</trd>
  <trd lng="zh">&#20179;&#40736; <pr>c&#257;ngsh&#468;</pr></trd>
</drv>

<drv mrk="hamstr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hamstr.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tro <ref tip="super" cel="kolekt.0i.kunmeti">kolekti</ref> kaj 
      <ref tip="super" cel="konser.0i.pluhavi">konservi</ref> provize, 
      kiel faras <tld/>oj:
      <ekz>
        ne malmultaj e&ubreve;ropanoj <tld/>is medikamentojn
        <fnt>
        <aut>S. Maul</aut>,
        <vrk><url ref="&Monato;/2006/008913.php">Paniko</url></vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2006a, numero 4a, p. 7a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_z;&c_a;&c_p;&c_a;&c_sh;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>hromadit</trd>,
    <trd>k&rcaron;e&ccaron;kovat</trd>,
    <trd>st&rcaron;&aacute;dat</trd>,
    <trd>&scaron;kudlit</trd>
  </trdgrp>
  <trd lng="de">hamstern</trd>
  <trd lng="en">hoard</trd>
  <trdgrp lng="fr">
    <trd>emmagasiner</trd>,
    <trd>th&eacute;sauriser</trd>
  </trdgrp>
  <trdgrp lng="it"><trd>accaparrare</trd>, 
  <trd>accumulare<klr> (raccogliere)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12383;&#12417;&#36796;&#12416;  <pr>&#12383;&#12417;&#12371;&#12416;</pr></trd>,
    <trd>&#36023;&#12356;&#12384;&#12417;&#12377;&#12427;  <pr>&#12363;&#12356;&#12384;&#12417;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">hamsteren</trd>
  <trd lng="pl">chomikowa&cacute;</trd>
  <trdgrp lng="sk">
    <trd>zhroma&zcaron;&dcaron;ova&tcaron;</trd>,
    <trd>&scaron;meli&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_p;&c_a;&c_s;&c_a;&c_t;&c_i;&c_s;&c_ja; (&c_p;&c_r;&c_o;&c_d;&c_u;&c_k;&c_t;&c_a;&c_m;&c_i;)</trd>,
    <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; (&c_n;&c_je;&c_p;&c_o;&c_t;&c_r;&c_ib;&c_b;&c_n;&c_ib;)
    &c_z;&c_a;&c_p;&c_a;&c_s;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#22244;&#31215; <pr>t&uacute;nj&#299;</pr></trd>
</drv>
</art>
