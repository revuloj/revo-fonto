<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hamstr</rad>/o
</kap>

<drv mrk="hamstr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hamstr.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ron&gcirc;ulo vivanta en grenkampoj kaj stepoj, povanta porti grenon en
      siaj vangosakoj, genro el familio <ref tip="super"
      cel="hamstr1.0oj"><tld/>edoj</ref> (<trd lng="la">Cricetus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_m;&c_ja;&c_k;</trd>
  <trdgrp lng="br">
    <trd>hamster</trd>,
    <trd>razh-segal</trd>
  </trdgrp>
  <trd lng="de">Hamster</trd>
  <trd lng="es">h&aacute;mster</trd>
  <trd lng="fr">hamster</trd>
  <trd lng="hu">h&ouml;rcs&ouml;g</trd>
  <trd lng="nl">hamster</trd>
  <trd lng="pt">hamster</trd>
  <trd lng="ru">&c_h;&c_o;&c_m;&c_ja;&c_k;</trd>
  <trd lng="sv">hamster</trd>
</drv>

<drv mrk="hamstr.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="hamstr.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tro kolekti kaj konservi provize, kiel faras <tld/>oj:
      <ekz>
        ne malmultaj e&ubreve;ropanoj <tld/>is medikamentojn
        <fnt>
        <aut>S. Maul</aut>,
        <vrk>Paniko</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2006a, numero 4a, p. 7a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">hamstern</trd>
  <trdgrp lng="fr">
    <trd>emmagasiner</trd>,
    <trd>th&eacute;sauriser</trd>
  </trdgrp>
</drv>
</art>
