<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>banal</rad>/a
</kap>
<drv mrk="banal.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ordinare, ofte uzata de &ccirc;iu ajn; seninteresa pro senoriginaleco:
      <ekz>
        <tld/>aj la&ubreve;doj, <tld/>a esprimo, vera&jcirc;o;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;oj de la &ccirc;iutaga vivo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vulgar.0a">vulgara</ref>,
      <ref cel="trivia.0a">triviala</ref>,
      <ref cel="stereo3.0a">stereotipa</ref>,
      <ref cel="gurd.0i">gurdita</ref>,
      <ref cel="sxablo">&scirc;ablona</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">banal</trd>
  <trd lng="nl">banaal</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_t;&c_r;&c_i;&c_v;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_z;&c_b;&c_i;&c_t;&c_y;&c_j;<klr tip="ind"> (&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;)</klr></trd>,
    <trd>&c_z;&c_a;&c_t;&c_a;&c_s;&c_k;&c_a;&c_n;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_b;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;)</klr></trd>
  </trdgrp>
</drv>
</art>
