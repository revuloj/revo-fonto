<drv mrk="acor.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="insul.0aro">Insularo</ref> en Atlantiko,
      apartenanta al <ref tip="malprt" cel="portugal.P0ujo">Portugalujo</ref>,
      &ccirc;. 1500&#8239;km okcidente de Lisbono:
      <ekz>
        <nom>Pico</nom>, vulkano 2351 metrojn alta <klr>[&#8230;]</klr>
        estas la plej alta en la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=287"
              >Acoroj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ant&oacute;nio Dacosta</nom> <klr>[&#8230;]</klr> naski&gcirc;is sur la
        Insulo <nom>Terceira</nom> en la <tld/>oj,
        kie li komencis siajn unuajn artistajn studojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=162983"
              >Ant&oacute;nio Dacosta</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Azoren</trd>
    <trd lng="en">Azores</trd>
    <trd lng="fr">A&ccedil;ores</trd>
  </snc>
</drv>

