<vortaro>
  <art mrk="$Id: acor.xml,v 1.3 2023/08/27 11:37:48 afido Exp $">
    <kap>
      <rad>Acor</rad>/oj
    </kap>

<drv mrk="acor.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="insul.0aro">Insularo</ref> en Atlantiko,
      apartenanta al <ref tip="malprt" cel="portugal.P0ujo">Portugalujo</ref>,
      &ccirc;. 1500&#x202F;km okcidente de Lisbono:
      <ekz>
        <nom>Pico</nom>, vulkano 2351 metrojn alta <klr>[&#x2026;]</klr>
        estas la plej alta en la <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=287">Acoroj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ant&oacute;nio Dacosta</nom> <klr>[&#x2026;]</klr> naski&gcirc;is sur la
        Insulo <nom>Terceira</nom> en la <tld/>oj,
        kie li komencis siajn unuajn artistajn studojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=162983">Ant&oacute;nio Dacosta</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Azoren</trd>
    <trd lng="en">Azores</trd>
    <trd lng="fr">A&ccedil;ores</trd>
    <trd lng="nl">Azoren</trd>
  </snc>
  <trd lng="ja">&#x30A2;&#x30BE;&#x30EC;&#x30B9;&#x8AF8;&#x5CF6;  [<ind>&#x3042;&#x305E;&#x308C;&#x3059;&#x3057;&#x3087;&#x3068;&#x3046;</ind>]</trd>
</drv>

  </art>

