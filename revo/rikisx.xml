<art mrk="$Id$">
<kap>
  <rad>riki&scirc;</rad>/o
</kap>
<drv mrk="rikisx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Orienta veturilo tirata per homa forto.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_k;&c_sh;&c_a;</trd>
  <trd lng="bg">&c_r;&c_i;&c_k;&c_sh;&c_a;</trd>
  <trd lng="de">Rikscha</trd>
  <trdgrp lng="en">
    <trd>ricksha</trd>,
    <trd>rickshaw</trd>,
    <trd>jinricksha</trd>,
    <trd>jinrikisha</trd>
  </trdgrp>
  <trd lng="fa">&a_r;&f_y;&f_k;&a_sh;&a_A;</trd>
  <trd lng="hu">riksa</trd>
  <trd lng="nl">riksja</trd>
  <trdgrp lng="pt">
    <trd>jinriquix&aacute;</trd>,
    <trd>riquix&aacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_i;&c_k;&c_sh;&c_a;</trd>
</drv>
</art>
