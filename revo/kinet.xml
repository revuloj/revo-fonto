<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kinet</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kinet.0a">
  <kap><tld/>a</kap>
  <snc mrk="kinet.0a.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Rilata la movon.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_k;&c_i;&c_n;&c_je;&c_t;&c_i;&c_ch;&c_je;&c_n;</trd>
  <trd lng="de">kinetisch</trd>
  <trd lng="fa">&a_j;&a_n;&a_b;&a_sh;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>cin&eacute;tique<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kinetikus</trd>,
    <trd>mozg&aacute;s-</trd>
  </trdgrp>
  <trd lng="nl">kinetisch</trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_je;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">kinetický</trd>
</drv>

<drv mrk="kinet.0aenergio">
  <kap><tld/>a energio</kap>
  <snc mrk="kinet.0aenergio.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Duona produto de la maso per la kvadrato de la rapido de
      movi&gcirc;anta korpo:
      <ekz>
        la <tld/>a energio, kiu estas proporcia kun la kvadrato de la
        rapido
        <fnt>
        <aut>OH</aut>,
        <vrk>&Ccirc;u akvo fluas supren ie ajn en la mondo?</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 3a, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="energi.0o.FIZ">energio</ref>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_e;&c_n;&c_e;&c_r;&c_g;&c_ib;&c_ja;</ind></trd>
  <trd lng="bg">&c_k;&c_i;&c_n;&c_je;&c_t;&c_i;&c_ch;&c_n;&c_a; &c_je;&c_n;&c_je;&c_r;&c_g;&c_i;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>kinetische Energie</trd>,
    <trd>Bewegungsenergie</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_n;&a_r;&f_zh;&f_y; &a_j;&a_n;&a_b;&a_sh;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;nergie cin&eacute;tique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kinetikus <ind>energia</ind></trd>,
    <trd>mozg&aacute;senergia</trd>
  </trdgrp>
  <trd lng="nl">kinetische <ind>energie</ind></trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_je;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; &c_e;&c_n;&c_je;&c_r;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">kinetická energia</trd>
</drv>
</art>
