<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kinet.xml,v 1.21 2023/12/03 09:58:49 revo Exp $">
<kap>
  <rad>kinet</rad>/a <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kinet.0a">
  <kap><tld/>a</kap>
  <snc mrk="kinet.0a.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Rilata la movon:
      <ekz>
        la kanono donas ankora&ubreve; plian <tld/>an energion
        <fnt>
          <aut>C. Spinola</aut>
          <vrk><url ref="https://www.youtube.com/watch?v=cS0ASrvrn8Y">VK 2020, veturado tra la interplaneda spaco</url></vrk>
          <lok>2020-12-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">kinetic</trd>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_k;&c_i;&c_n;&c_je;&c_t;&c_i;&c_ch;&c_je;&c_n;</trd>
  <trd lng="cs">kinetick&yacute;</trd>
  <trd lng="de">kinetisch</trd>
  <trd lng="fa">&a_j;&a_n;&a_b;&a_sh;&f_y;</trd>
  <trd lng="fr">cin&eacute;tique<klr tip="ind"> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>kinetikus</trd>,
    <trd>mozg&aacute;s-</trd>
  </trdgrp>
  <trd lng="ja">&#x904B;&#x52D5;&#x306E;  <pr>&#x3046;&#x3093;&#x3069;&#x3046;&#x306E;</pr></trd>
  <trd lng="nl">kinetisch</trd>
  <trd lng="pl">kinetyczny</trd>
  <trd lng="pt">cin&eacute;tico</trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_je;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">kinetick&yacute;</trd>
  <trd lng="uk">&#x43A;&#x456;&#x43D;&#x435;&#x442;&#x438;&#x447;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="kinet.0aenergio">
  <kap><tld/>a energio</kap>
  <snc mrk="kinet.0aenergio.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="super" cel="energi.0o.FIZ">Energio</ref>
      de movi&gcirc;anta korpo egala al duona produto de &gcirc;ia maso
      per la kvadrato de &gcirc;ia rapido:
      <ekz>
        la <tld/>a energio <klr>[&#x2026;]</klr> estas proporcia kun la kvadrato de la
        rapido
        <fnt>
        <aut>OH</aut>
        <vrk>&Ccirc;u akvo fluas supren ie ajn en la mondo?</vrk>
        <bib>Monato</bib>
        <lok>2001:3, p. 22a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">kinetic energy</trd>
  <trd lng="be">&c_k;&c_ib;&c_n;&c_e;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_e;&c_n;&c_e;&c_r;&c_g;&c_ib;&c_ja;</ind></trd>
  <trd lng="bg">&c_k;&c_i;&c_n;&c_je;&c_t;&c_i;&c_ch;&c_n;&c_a; &c_je;&c_n;&c_je;&c_r;&c_g;&c_i;&c_ja;</trd>
  <trd lng="cs">kinetick&aacute; energie</trd>
  <trdgrp lng="de">
    <trd>kinetische Energie</trd>,
    <trd>Bewegungsenergie</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_n;&a_r;&f_zh;&f_y; &a_j;&a_n;&a_b;&a_sh;&f_y;</trd>
  <trd lng="fr">&eacute;nergie cin&eacute;tique</trd>
  <trdgrp lng="hu">
    <trd>kinetikus <ind>energia</ind></trd>,
    <trd>mozg&aacute;senergia</trd>
  </trdgrp>
  <trd lng="ja">&#x904B;&#x52D5;&#x30A8;&#x30CD;&#x30EB;&#x30AE;&#x30FC;  <pr>&#x3046;&#x3093;&#x3069;&#x3046;&#x30A8;&#x30CD;&#x30EB;&#x30AE;&#x30FC;</pr></trd>
  <trd lng="nl">kinetische <ind>energie</ind></trd>
  <trd lng="pl">energia <ind>kinetyczna</ind></trd>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_je;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; &c_e;&c_n;&c_je;&c_r;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">kinetick&aacute; energia</trd>
  <trd lng="uk">&#x43A;&#x456;&#x43D;&#x435;&#x442;&#x438;&#x447;&#x43D;&#x430; &#x435;&#x43D;&#x435;&#x440;&#x433;&#x456;&#x44F;</trd>
  <trd lng="zh">&#x52A8;&#x80FD; <pr>d&#xF2;ngn&#xE9;ng</pr></trd>
</drv>
</art>
