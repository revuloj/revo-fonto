<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: kauxlun.xml,v 1.3 2024/05/17 07:24:09 afido Exp $">
    <kap>
      <rad>Ka&ubreve;lun</rad>/o
    </kap>

<drv mrk="kauxlun.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="kauxlun.0o.duoninsulo">
    <dif>
      <ref tip="super" cel="insul.duon0o">Duoninsulo</ref>, sur kiu lo&gcirc;as
      preska&ubreve; triono de la enlo&gcirc;antoj de <ref tip="malprt" cel="honkong.0o">Honkongo</ref>:
      <ekz>
        Honkongo konsistas el la malgranda duoninsulo <tld/>o kaj 236 insuloj en la
        Sud&ccirc;ina Maro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1708">Honkongo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la plimulto de la 1.200 studentoj lo&gcirc;as en Novaj Teritorioj kaj Norda <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=567787">Kolegio &Scirc;a Tin</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kowloon</trd>
    <trd lng="en">Kowloon</trd>
    <trd lng="fr">Kowloon</trd>
    <trd lng="zh">&#x4E5D;&#x9F8D;&#x534A;&#x5CF6;
    <pr>ji&#x1D4;l&#xF3;ngb&#xE0;nd&#x1CE;o</pr></trd>
</snc>
<trdgrp lng="be">
  <trd>&c_K;&c_a;&c_w;&c_l;&c_u;&c_n;</trd>,
  <trd>&c_D;&c_z;&c_ju;&c_l;&c_u;&c_n;</trd>
</trdgrp>
</drv>

  </art>

