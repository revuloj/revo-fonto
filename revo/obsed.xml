  </trdgrp>
  <trd lng="it">ossessione</trd>
  <trdgrp lng="ja">
    <trd>&#x5984;&#x57F7;  <pr>&#x3082;&#x3046;&#x3057;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x56FA;&#x5B9A;&#x89B3;&#x5FF5;  <pr>&#x3053;&#x3066;&#x3044;&#x304B;&#x3093;&#x306D;&#x3093;</pr></trd>,
    <trd>&#x5F37;&#x8FEB;&#x89B3;&#x5FF5;  <pr>&#x304D;&#x3087;&#x3046;&#x306F;&#x304F;&#x304B;&#x3093;&#x306D;&#x3093;</pr></trd>
