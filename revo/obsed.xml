<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>obsed</rad>/i
</kap>
<drv mrk="obsed.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Preme okupi la spiriton; trude ekkapti la atenton (pp fiksa ideo):
      <ekz>
        <tld/>anta halucinacio.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="siegx.0i">sie&gcirc;i</ref>,
      <ref cel="persek.0i">persekuti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>heimsuchen</trd>,
    <trd>verfolgen</trd>,
    <trd>qu&auml;len</trd>,
    <trd>plagen</trd>,
    <trd>befallen</trd>
  </trdgrp>
  <trd lng="fr">obs&eacute;der</trd>
  <trd lng="nl">obsederen</trd>
  <trdgrp lng="pt">
    <trd>obsedar</trd>,
    <trd>obcecar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_o;&c_t;&c_v;&c_ja;&c_z;&c_n;&c_o; &c_p;&c_r;&c_je;&c_s;&c_l;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_m;&c_u;&c_ch;&c_i;&c_t;&c_mol; <klr>(&c_o; &c_n;&c_a;&c_v;&c_ja;&c_z;&c_ch;&c_i;&c_v;&c_o;&c_j; &c_i;&c_d;&c_je;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="obsed.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Tio, kio <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Besessenheit</trd>,
    <trd>Obsession</trd>
  </trdgrp>
  <trd lng="fr">obsession</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_v;&c_ja;&c_z;&c_ch;&c_i;&c_v;&c_a;&c_ja; &c_i;&c_d;&c_je;&c_ja;</trd>,
    <trd>&c_o;&c_d;&c_je;&c_r;&c_zh;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="obsed.0ato">
  <kap><tld/>ato</kap>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <snc>
    <dif>
      Persojno, kion io <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Besessener</trd>
  </trdgrp>
</drv>

</art>
