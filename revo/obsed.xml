    <trd>&c_m;&c_u;&c_ch;&c_i;&c_t;&c_mol; <klr>(&c_o;
    &c_n;&c_a;&c_v;&c_ja;&c_z;&c_ch;&c_i;&c_v;&c_o;&c_j; &c_i;&c_d;&c_je;&c_je;
    &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ovl&aacute;dnu&tcaron;</trd>,
    <trd>posadn&uacute;&tcaron;</trd>,
    <trd>zachv&aacute;ti&tcaron;</trd>,
