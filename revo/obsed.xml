<drv mrk="obsed.0ato">
  <kap><tld/>ato, <var><kap><tld/>ito</kap></var></kap>
  <snc mrk="obsed.0ato.PSI">
    <dif>
      Persono, kiun io <tld/>as:
      <ekz>
        <tld/>atoj de son&gcirc;a kalifujo, kiuj revas paradi en Mosulo
        <fnt>
          <bib>MD14</bib>
          <vrk>Kalkuleraro</vrk>
        </fnt>;
      </ekz>
      <ekz>
        bando de <tld/>atoj rasistaj
        <fnt>
          <bib>MD17</bib>
          <vrk>Politika kaj socia &scirc;an&gcirc;o &dash; Klasbatalo en Francujo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ajnis utopia a&ubreve; paranoja deliro, frukto el la pli malpli halucina imago de la
        <tld/>itoj
        <fnt>
          <bib>MD17</bib>
          <vrk>Ni vivas sub la dominado de ia imperio de spionado</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="fanati.0ulo">fanatikulo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_p;&c_a;&c_n;&c_t;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_t;&c_r;&c_a;&c_p;&c_jo;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">posedl&yacute; &ccaron;lov&ecaron;k</trd>
  <trdgrp lng="de">
    <trd>Besessener</trd>
  </trdgrp>
  <trd lng="en">obsessed</trd>
