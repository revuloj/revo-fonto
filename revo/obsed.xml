  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x435;&#x432;&#x456;&#x434;&#x441;&#x442;&#x443;&#x43F;&#x43D;&#x43E; &#x43F;&#x435;&#x440;&#x435;&#x441;&#x43B;&#x456;&#x434;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43C;&#x443;&#x447;&#x438;&#x442;&#x438; (&#x43F;&#x440;&#x43E; &#x43D;&#x430;&#x432;&#x2019;&#x44F;&#x437;&#x43B;&#x438;&#x432;&#x443; &#x456;&#x434;&#x435;&#x44E;</trd>,
    <trd>&#x441;&#x442;&#x440;&#x430;&#x445; &#x456; &#x442;.&#x43F;.)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E7E;&#x8457;&#x6025; <pr>g&#x101;nzh&#xE1;oj&#xED;</pr></trd>,
    <trd>&#x5E72;&#x7740;&#x6025; <pr>g&#x101;nzh&#xE1;oj&#xED;</pr></trd>,
    <trd>&#x6781;&#x75DB;&#x82E6; <pr>j&#xED;t&#xF2;ngk&#x1D4;</pr></trd>,
    <trd>&#x6975;&#x75DB;&#x82E6; <pr>j&#xED;t&#xF2;ngk&#x1D4;</pr></trd>,
    <trd>&#x8FF7;&#x4F4F; <pr>m&#xED;zhu</pr></trd>
  </trdgrp>
</drv>

<drv mrk="obsed.0o">
  <kap><tld/>o</kap>
  <snc mrk="obsed.0o.PSI">
    <dif>
      Tio, kio <tld/>as,
      <ref tip="vid" cel="ide.fiksa0o.PSI">fiksa ideo</ref>:
      <ekz>
        sur l' ekrano / tricentfoje &ccirc;iutage la tirano / sin altrudis kvaza&ubreve; son&gcirc;-<tld/>'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Angkor'a&ubreve;!</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia <tld/>o pri viroj, &scirc;iaj &scirc;ablonaj diroj pri la vira sekso impresis malnature
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        kial &ccirc;iuj acerbaj kaj vitriolaj frazoj pri morto, forpaso kaj pereo? &ndash; kredeble
        temas pri persona <tld/>o
        <fnt>
          <bib>MMa</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi menciis la francan <tld/>on pri la &leftquot;&gcirc;usta termino&rightquot;
        <fnt>
          <bib>BonaLingvo</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="fanati.0eco">fanatikeco</ref>,
       <ref cel="ide.fiksa0o">fiksa ideo</ref>,
       <ref cel="mani.0o">manio</ref>,
       <ref cel="monoma.0o">monomanio</ref>,
       <ref cel="marot.0o">maroto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_a;&c_d;&c_ch;&c_e;&c_p;&c_n;&c_a;&c_ja;
    &c_ib;&c_d;&c_e;&c_ja;</trd>,
    <trd>&c_ib;&c_d;&c_e;&c_f;&c_ib;&c_k;&c_s;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_n;&c_t;&c_a;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">posedlost</trd>
  <trdgrp lng="de">
    <trd>Besessenheit</trd>,
    <trd>Obsession</trd>
  </trdgrp>
  <trd lng="en">obsession</trd>
