<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>obsed</rad>/i
</kap>
<drv mrk="obsed.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Preme okupi la spiriton; trude ekkapti la atenton (pp fiksa ideo):
      <ekz>
        <tld/>anta halucinacio.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="siegx.0i">sie&gcirc;i</ref>,
      <ref cel="persek.0i">persekuti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>heimsuchen</trd>,
     <trd>verfolgen</trd>
  </trdgrp>
  <trd lng="fr">obs&eacute;der</trd>
  <trd lng="nl">obsederen</trd>
</drv>
<drv mrk="obsed.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tio, kio <tld/>as.
    </dif>
  </snc>
  <trd lng="fr">obsession</trd>
</drv>

</art>
