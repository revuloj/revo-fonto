<drv mrk="obsed.0o">
  <kap><tld/>o</kap>
  <snc mrk="obsed.0o.PSI">
    <dif>
      Tio, kio <tld/>as,
      <ref tip="vid" cel="ide.fiksa0o.PSI">fiksa ideo</ref>:
      <ekz>
        sur l' ekrano / tricentfoje &ccirc;iutage la tirano / sin altrudis kvaza&ubreve; son&gcirc;-<tld/>'
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Angkor'a&ubreve;!</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia <tld/>o pri viroj, &scirc;iaj &scirc;ablonaj diroj pri la vira sekso impresis malnature
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        kial &ccirc;iuj acerbaj kaj vitriolaj frazoj pri morto, forpaso kaj pereo? &ndash; kredeble
        temas pri persona <tld/>o
        <fnt>
          <bib>MMa</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi menciis la francan <tld/>on pri la &leftquot;&gcirc;usta termino&rightquot;
        <fnt>
          <bib>BonaLingvo</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="fanati.0eco">fanatikeco</ref>,
       <ref cel="ide.fiksa0o">fiksa ideo</ref>,
       <ref cel="mani.0o">manio</ref>,
       <ref cel="monoma.0o">monomanio</ref>,
       <ref cel="marot.0o">maroto</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>korpremo</ke>, <ke>premzorgo</ke>, <ke>inkub(son&#285;)o</ke>, <ke>manio</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_a;&c_d;&c_ch;&c_e;&c_p;&c_n;&c_a;&c_ja;
    &c_ib;&c_d;&c_e;&c_ja;</trd>,
