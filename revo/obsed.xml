  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1085;&#1077;&#1074;&#1110;&#1076;&#1089;&#1090;&#1091;&#1087;&#1085;&#1086; &#1087;&#1077;&#1088;&#1077;&#1089;&#1083;&#1110;&#1076;&#1091;&#1074;&#1072;&#1090;&#1080;</trd>,
    <trd>&#1084;&#1091;&#1095;&#1080;&#1090;&#1080; (&#1087;&#1088;&#1086; &#1085;&#1072;&#1074;&#8217;&#1103;&#1079;&#1083;&#1080;&#1074;&#1091; &#1110;&#1076;&#1077;&#1102;</trd>,
    <trd>&#1089;&#1090;&#1088;&#1072;&#1093; &#1110; &#1090;.&#1087;.)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20094;&#33879;&#24613; <pr>g&#257;nzh&#225;oj&#237;</pr></trd>,
    <trd>&#24178;&#30528;&#24613; <pr>g&#257;nzh&#225;oj&#237;</pr></trd>,
    <trd>&#26497;&#30171;&#33510; <pr>j&#237;t&#242;ngk&#468;</pr></trd>,
    <trd>&#26997;&#30171;&#33510; <pr>j&#237;t&#242;ngk&#468;</pr></trd>,
    <trd>&#36855;&#20303; <pr>m&#237;zhu</pr></trd>
  </trdgrp>
