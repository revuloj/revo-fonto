  <trd lng="be">&c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>
  <trd lng="bo">&t_pha;&t_tsheg;&t_ra;&t_na;&t_tsheg;&t_sa;&t_gigu;&t_-a;&t_gigu;&t_tsheg;&t_sa;&t_kata;&t_da;</trd>
  <trd lng="ca">llengua francesa</trd>
  <trd lng="de">Franz&ouml;sisch</trd>
  <trd lng="en">French</trd>
  <trd lng="hu">francia nyelv</trd>
  <trdgrp lng="io">
    <trd>Franciana linguo</trd>,
    <trd>Franca linguo</trd>
  </trdgrp>
  <trd lng="ja">&#12501;&#12521;&#12531;&#12473;&#35486;  <pr>&#12501;&#12521;&#12531;&#12473;&#12372;</pr></trd>
  <trd lng="nl">Frans</trd>
  <trd lng="no">fransk</trd>
  <trd lng="pl"><klr tip="amb">j&eogonek;zyk </klr>francuski</trd>
  <trd lng="pt">franc&ecirc;s <klr tip="amb">(l&iacute;ngua)</klr></trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_z;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
  <trd lng="tok">toki Kanse</trd>
  <trd lng="vo">Frans&auml;nap&uuml;k</trd>
  <trd lng="zh">&#27861;&#25991;<klr> [F&#462;w&eacute;n]</klr></trd>
</drv>

