<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: franc.xml,v 1.45 2023/08/31 13:11:31 revo Exp $">
<kap>
<ofc>*</ofc><ofc>7</ofc>
  <rad>franc</rad>/o
</kap>

<drv mrk="franc.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="franc.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="franc.F0ujo"><tld lit="F"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="franc.0o.naciano">
    <dif>
      Homo kun franca nacieco:
      <ekz>
        Germanoj kaj francoj, kiuj lo&gcirc;as en Rusujo, estas Rusujanoj,
        kvankam ili ne estas rusoj.
        <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>.
      </ekz>
      <ekz>
        se ekzemple iu <tld/>o a&ubreve; hispano ne komprenus la principon de
        la sengenteco de la tero mi ne mirus, ili tiel kutimi&gcirc;is vidi
        &ccirc;irka&ubreve; si nur samgentanojn, a&ubreve;di nur unu lingvon, ke estus tute
        pardoneble se ili profunde kredus ke lando kaj lingvo kaj
        religio estas identaj
        <fnt><bib>Homaranismo</bib></fnt>;
      </ekz>
      <ekz>
        tutajn jarojn devas uzi <tld/>o a&ubreve; anglo por tio, ke li ekpovu
        regule skribi en sia patra lingvo
        <fnt><bib>EE</bib>, <lok>&ccirc;ap. 5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_z;</trd>
  <trd lng="bg">&c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_z;&c_i;&c_n;</trd>
  <trd lng="bo">&t_pha;&t_tsheg;&t_ra;&t_na;&t_tsheg;&t_sa;&t_gigu;&t_-a;&t_gigu;&t_tsheg;&t_ma;&t_gigu;</trd>
  <trd lng="ca">franc&eacute;s<klr> (subst.)</klr></trd>
  <trd lng="cs">Francouz</trd>
  <trd lng="de">Franzose</trd>
  <trdgrp lng="en">
    <trd>French person</trd>,
    <trd>Frenchman</trd>,
    <trd>Frenchwoman</trd>
  </trdgrp>
  <trd lng="es">franc&eacute;s</trd>
  <trd lng="fr">Fran&ccedil;ais</trd>
  <trd lng="hu">francia</trd>
  <trdgrp lng="io">
    <trd>Franciano</trd>,
    <trd>Franco</trd>
  </trdgrp>
  <trd lng="ja">&#x30D5;&#x30E9;&#x30F3;&#x30B9;&#x4EBA;  [<ind>&#x30D5;&#x30E9;&#x30F3;&#x30B9;&#x3058;&#x3093;</ind>]</trd>
  <trd lng="nl">Fransman</trd>
  <trd lng="no">franskmann</trd>
  <trdgrp lng="pl">
    <trd>Francuz<klr> {m.}</klr></trd>,
    <trd>Francuzka<klr> {f.}</klr></trd>
  </trdgrp>
  <trd lng="pt">franc&ecirc;s <klr tip="amb">(subst.)</klr></trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_z;</trd>
  <trd lng="tok">jan pi ma Kanse</trd>
  <trd lng="uk">&#x444;&#x440;&#x430;&#x43D;&#x446;&#x443;&#x437;</trd>
  <trd lng="vo">Frans&auml;nan</trd>
</drv>

<drv mrk="franc.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al francoj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        en A&ubreve;gusto 1905 en la <tld/>a urbo Boulogne sur Mer
        havis lokon la unua tutmonda kongreso de esperantistoj,
        kies sukceso superis &ccirc;iujn atendojn
        <fnt><bib>Homaranismo</bib></fnt>;
      </ekz>
      <ekz>
        li ne estis amanto de la <tld/>aj romanoj kiuj estas
        preparitaj el vento kaj sekvinberaj trunketoj
        <fnt><bib>Fab4</bib>, <lok>Turgardisto Ole</lok></fnt>;
      </ekz>
      <ekz>
        ekzemple en la lingvo germana kapo havas viran sekson,
        en la <ref tip="lst" cel="lingv.0o.homa" lst="voko:lingvoj">lingvo</ref>
        <tld/>a virinan kaj en la lingvo latina
        ne&ubreve;tralan
        <fnt><bib>EE</bib>, <lok>&ccirc;ap. 5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      La <ref tip="dif" cel="franc.0alingvo">franca lingvo</ref>.
    </dif>
  </snc>  
  <trd lng="be">&c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_f;&c_r;&c_je;&c_n;&c_s;&c_k;&c_i;</trd>
  <trd lng="bo">&#xF55;&#xF0B;&#xF62;&#xF53;&#xF0B;&#xF66;&#xF72;&#xF60;&#xF72;&#xF0B;&#xF66;&#xF90;&#xF51;&#xF0B;</trd>
  <trd lng="ca">franc&eacute;s<klr> (adj.)</klr></trd>
  <trdgrp lng="cs">
    <trd>francouzsk&yacute;</trd>,
    <trd>francouz&scaron;tina</trd>,
    <trd>fran&scaron;tina</trd>
  </trdgrp>
  <trd lng="de">franz&ouml;sisch</trd>
  <trd lng="en">French</trd>
  <trd lng="es">franc&eacute;s</trd>
  <trd lng="fr">fran&ccedil;ais</trd>
  <trd lng="hu">francia</trd>
  <trdgrp lng="io">
    <trd>Franciana</trd>,
    <trd>Franca</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30E9;&#x30F3;&#x30B9;&#x306E;  [<ind>&#x3075;&#x3089;&#x3093;&#x3059;&#x306E;</ind>]</trd>,
    <trd>&#x30D5;&#x30E9;&#x30F3;&#x30B9;&#x8A9E;&#x306E;  [<ind>&#x3075;&#x3089;&#x3093;&#x3059;&#x3054;&#x306E;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">Frans</trd>
  <trd lng="no">fransk</trd>
  <trd lng="pl">francuski</trd>
  <trd lng="pt">franc&ecirc;s <klr tip="amb">(adj.)</klr></trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_z;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">franc&uacute;z&scaron;tina</trd>
  <trd lng="tok">pi ma Kanse</trd>
  <trd lng="uk">&#x444;&#x440;&#x430;&#x43D;&#x446;&#x443;&#x437;&#x44C;&#x43A;&#x438;&#x439;</trd>
  <trd lng="vo">Frans&auml;nik</trd>
</drv>

<drv mrk="franc.0alingvo">
  <kap><tld/>a lingvo</kap>
  <snc>
    <dif>Oficiala lingvo de la franca &scirc;tato.</dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_s;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>
  <trd lng="bo">&t_pha;&t_tsheg;&t_ra;&t_na;&t_tsheg;&t_sa;&t_gigu;&t_-a;&t_gigu;&t_tsheg;&t_sa;&t_kata;&t_da;</trd>
  <trd lng="ca">llengua francesa</trd>
  <trd lng="de">Franz&ouml;sisch</trd>
  <trd lng="en">French</trd>
  <trd lng="hu">francia nyelv</trd>  
  <trdgrp lng="io">
    <trd>Franciana linguo</trd>,
    <trd>Franca linguo</trd>
  </trdgrp>
  <trd lng="ja">&#x30D5;&#x30E9;&#x30F3;&#x30B9;&#x8A9E;  [<ind>&#x30D5;&#x30E9;&#x30F3;&#x30B9;&#x3054;</ind>]</trd>
  <trd lng="nl">Frans</trd>
  <trd lng="no">fransk</trd>
  <trd lng="pl"><klr tip="amb">j&eogonek;zyk </klr>francuski</trd>
  <trd lng="pt">franc&ecirc;s <klr tip="amb">(l&iacute;ngua)</klr></trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_z;&c_s;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
  <trd lng="tok">toki Kanse</trd>
  <trd lng="vo">Frans&auml;nap&uuml;k</trd>
  <trd lng="zh">&#x6CD5;&#x6587;<klr> [F&#x1CE;w&eacute;n]</klr></trd>
</drv>

<drv mrk="franc.0lingvano">
  <kap><tld/>lingvano</kap>
  <snc>
    <dif>Parolanto de la franca lingvo.</dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_r;&c_a;&c_n;&c_k;&c_a;&c_m;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_r;&c_a;&c_n;&c_c;&c_u;&c_s;&c_k;&c_a;&c_m;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">franc&ograve;fon</trd>
  <trd lng="de">Franz&ouml;sischsprecher</trd>
  <trdgrp lng="es">
    <trd>francohablante</trd>
    <trd>frencoparlante</trd>
  </trdgrp>
  <trd lng="en">French speaker</trd>
  <trd lng="fr">francophone</trd>
  <trd lng="hu">francia nyelv&udblac;</trd>
  <trd lng="io">Francaparolanto</trd>  
  <trd lng="ja">&#x30D5;&#x30E9;&#x30F3;&#x30B9;&#x8A9E;&#x8A71;&#x8005;  [<ind>&#x3075;&#x3089;&#x3093;&#x3059;&#x3054;&#x308F;&#x3057;&#x3083;</ind>]</trd>
  <trd lng="nl">Franstalige</trd>
  <trd lng="no">fransktalende</trd>
  <trd lng="pt">franc&oacute;fono</trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_n;&c_k;&c_o;&c_g;&c_o;&c_v;&c_o;&c_r;&c_ja;&c_shch;&c_i;&c_j;<klr tip="amb"> (&c_v; &c_z;&c_n;&c_a;&c_ch;.
  &c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="vo">Frans&auml;nap&uuml;kan</trd>
</drv>

<drv mrk="franc.F0ujo">
  <kap><tld lit="F"/>ujo</kap>
  <mlg kod="ISO-3166">FR</mlg>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta en
      okcidenta <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>
      kaj limanta okcidente al Atlantiko, norde al Belgujo, oriente
      al Svislando, sude al Hispanujo k. a.:
      <ekz>
        Anglujo, Germanujo kaj <tld lit="F"/>ujo alsendis
        reprezentantojn
        <fnt><bib>Fab2</bib>, <lok>Najbaraj familioj</lok></fnt>;
      </ekz>
      <ekz>
        de la bordoj de <tld lit="F"/>ujo eksonis krio de timego
        anta&ubreve;
        la sangaj cignoj kiuj aperis el la nordo kun fajro sub la
        flugiloj kaj la popolo pre&gcirc;is
        <fnt><bib>Fab3</bib>, <lok>Cigna nesto</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="franci1.0o">Francio</ref>
  </snc>
  <bld lok="../bld/Francujo.png">mapo</bld>
  <trd lng="af">Frankryk</trd>
  <trd lng="be">&c_F;&c_r;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>  
  <trd lng="bg">&c_F;&c_r;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trd lng="bo">&t_pha;&t_tsheg;&t_ra;&t_na;&t_tsheg;&t_sa;&t_gigu;</trd>
  <trd lng="ca">Fran&ccedil;a</trd>
  <trd lng="cs">Francie</trd>
  <trd lng="da">Frankrig</trd>
  <trd lng="de">Frankreich</trd>
  <trd lng="el">&Gamma;&alfa;&lambda;&lambda;&jota_ton;&alfa;</trd>
  <trd lng="en">France</trd>
  <trd lng="es">Francia</trd>
  <trd lng="eu">Frantzia</trd>
  <trd lng="fa">&a_f;&a_r;&a_A;&a_n;&a_s;&a_h;</trd>
  <trd lng="fi">Ranska</trd>
  <trdgrp lng="fr">
    <trd>France</trd>,
    <trd>R&eacute;publique fran&ccedil;aise</trd>
  </trdgrp>
  <trd lng="fy">Frankryk</trd>
  <trd lng="gl">Francia</trd>
  <trd lng="hr">Francuska</trd>
  <trd lng="hu">Franciaorsz&aacute;g</trd>
  <trd lng="ia">Francia</trd>
  <trdgrp lng="id">
    <trd>Francia</trd>,
    <trd>Perancis</trd>,
    <trd>Prancis</trd>
  </trdgrp>
  <trd lng="io">Francia</trd>
  <trd lng="it">Francia</trd>
  <trd lng="la">Francia</trd>
  <trd lng="nl">Frankrijk</trd>
  <trd lng="no">Frankrike</trd>
  <trd lng="oc">Fran&ccedil;a</trd>
  <trd lng="pl">Francja</trd>
  <trd lng="pt">Fran&ccedil;a</trd>
  <trd lng="ru">&c_F;&c_r;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">Franc&uacute;zsko</trd>
  <trd lng="sv">Frankrike</trd>
  <trd lng="sw">Ufaransa</trd>
  <trd lng="tok">ma Kanse</trd>
  <trd lng="tr">Fransa</trd>
  <trd lng="vo">Frans&auml;n</trd>
  <trdgrp lng="zh">
    <trd>&#x6CD5;&#x56FD;</trd>,
    <trd><klr>tr. </klr>&#x6CD5;&#x570B;<klr> [F&#x1CE;gu&oacute;]</klr></trd>,
    <trd>&#x6CD5;&#x5170;&#x897F;</trd>,
    <trd><klr>tr. </klr>&#x6CD5;&#x862D;&#x897F;<klr> [F&#x1CE;l&aacute;nx&imacron;]</klr></trd>
  </trdgrp>
</drv>
</art>
