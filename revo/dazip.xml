<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>dazip</rad>/o
</kap>
<drv mrk="dazip.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="dazip.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Dasypus</trd>).
    </dif>
  </snc>
  <trd lng="de">G&uuml;rteltier</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_je;&c_n;&c_o;&c_s;&c_je;&c_c;</trd>
</drv>
<drv mrk="dazip.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="artik.krom0uloj">kromartikuloj</ref>
      (<trd lng="la">Dasypodidae</trd>),
      kies dorso kaj vosto estas kovritaj de skvamoj ofte en formo de zonoj.
    </dif>
  </snc>
  <trd lng="de">G&uuml;rteltiere <klr tip="ind">Familie)</klr></trd>
</drv>
</art>
