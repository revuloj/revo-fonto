<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dazip.xml,v 1.27 2023/11/04 13:16:26 revo Exp $">
<kap>
<rad>dazip</rad>/o
</kap>
<drv mrk="dazip.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="dazipe.0oj"><tld/>edoj</ref>
      (<trd lng="la">Dasypus</trd>):
      <ekz>
        la armadelo a&ubreve; na&ubreve;zona <tld/>o estas specio de <tld/>o kun na&ubreve; vicoj de ostopecoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=59704">Armadelo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la giganta pratatuo a&ubreve; gliptodonto estis praa, giganta tatuo a&ubreve; <tld/>o, kiu
        vivis origine en Sudameriko kaj dum la glaciepoko disvasti&gcirc;is anka&ubreve; en
        Nordameriko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=59578">Giganta pratatuo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="zon.0o">zono</ref>
    <bld lok="&WCU;/thumb/7/77/Dasypus_novemcinctus.jpg/640px-Dasypus_novemcinctus.jpg" prm="PD">na&ubreve;zona <tld/>o (Dasypus novemcinctus), Sekvoja Nacia Sova&gcirc;parko, Usono
        <fnt>
          <aut>Hollingsworth, John and Karen</aut>
          <lok><url ref="&WCW;/File:Dasypus_novemcinctus.jpg">&WCW;/File:Dasypus_novemcinctus.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_n;&c_ja;&c_n;&c_o;&c_s;&c_je;&c_c;</trd>
  <trd lng="cs">p&aacute;sovec</trd>
  <trd lng="de">G&uuml;rteltier</trd>
  <trd lng="en">armadillo</trd>
  <trd lng="es">armadillo</trd>
  <trd lng="fr">tatou</trd>
  <trd lng="he">&#x5D8;&#x5D8;&#x5D5;</trd>
  <trdgrp lng="hu">
    <trd>&ouml;rv&ouml;s &aacute;llat</trd>,
    <trd>tatu</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30B3;&#x30B3;&#x30CE;&#x30AA;&#x30D3;&#x30A2;&#x30EB;&#x30DE;&#x30B8;&#x30ED;</trd>,
    <trd>&#x30A2;&#x30EB;&#x30DE;&#x30B8;&#x30ED;</trd>
  </trdgrp>
  <trd lng="nl">gordeldier</trd>
  <trd lng="pl">pancernik</trd>
  <trd lng="pt">tatu</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_n;&c_je;&c_n;&c_o;&c_s;&c_je;&c_c;</trd>
  <trd lng="sk">p&aacute;sovec</trd>
  <trd lng="uk">&#x431;&#x440;&#x43E;&#x43D;&#x435;&#x43D;&#x43E;&#x441;&#x435;&#x446;&#x44C;</trd>
  <trdgrp lng="zh">
    <trd>&#x72B0;&#x72F3; <pr>qi&#xFA;y&#xFA;</pr></trd>,
    <trd>&#x72B0; <pr>qi&#xFA;</pr></trd>,
    <trd>&#x72F3; <pr>y&#xFA;</pr></trd>
  </trdgrp>
</drv>

</art>
