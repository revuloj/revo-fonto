<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sekret1.xml,v 1.21 2022/05/05 05:09:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sekretari</rad>/o
</kap>
<drv mrk="sekret1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="hom.0o">Homo</ref>
      komisiita por verki kaj skribi leterojn, raportojn,
      protokolojn, dokumentojn:
      <ekz>
        grandaj sinjoroj havas <tld/>ojn kaj rajtigitojn
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        sciigoj devas esti sendataj al la <tld/>o
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>El la mondo homarana (hilelista).</vrk>
        </fnt>;
      </ekz>
      <ekz>
        sciigi pri sia deziro per letero la <tld/>on de la societo, kun aldono de 50
        centimoj en signoj de po&scirc;to
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la raporto de l' <tld/>o <nom>Henry Philipps</nom> estis publikigita
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        al &ccirc;iu membro la <tld/>o de la universitato sendas &ccirc;iujare krom la katalogo de
        komuna kurso anka&ubreve; la katalogojn de specialaj verkoj, senpagan gazeton &leftquot;Alma
        mater&rightquot; kaj multon da diversaj gravaj kaj interesaj katalogoj, bro&scirc;uroj kaj
        dokumentoj
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_k;&c_r;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">secretari</trd>
  <trdgrp lng="cs">
    <trd>jednatel</trd>,
    <trd>sekret&aacute;&rcaron;</trd>,
    <trd>tajemn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sekret&auml;r</trd>,
    <trd>Sekret&auml;rin</trd>
  </trdgrp>
  <trd lng="en">secretary</trd>
  <trd lng="es">secretario</trd>
  <trd lng="fr">secr&eacute;taire</trd>
  <trd lng="hu">titk&aacute;r</trd>
  <trd lng="it">segretario</trd>
  <trd lng="nl">secretaris</trd>
  <trd lng="ru">&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_a;&c_r;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>sekret&aacute;r</trd>,
    <trd>tajomn&iacute;k</trd>
  </trdgrp>
  <trd lng="uk">&#x441;&#x435;&#x43A;&#x440;&#x435;&#x442;&#x430;&#x440;</trd>
</drv>

<drv mrk="sekret1.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      <ref tip="super" cel="lok.0o.spaca">Loko</ref>, kie funkcias <tld/>o:
      <ekz>
        havebla kontra&ubreve; 25 e&ubreve;roj en la <tld/>ejo
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2003/007945.html">Bukedo de popolarto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &gcirc;enerala <tld/>ejo &gcirc;is junio 2008 trovi&gcirc;is en Hamburgo
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2012/010831.php">&Ccirc;u estos pli bona mondo en 2052?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kancel.0o">kancelario</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_k;&c_r;&c_a;&c_t;&c_a;&c_r;&c_y;&c_ja;&c_t;</trd>
  <trd lng="ca">secretaria</trd>
  <trd lng="cs">sekretari&aacute;t <klr>(&uacute;&rcaron;adovna)</klr></trd>
  <trd lng="de">Sekretariat</trd>
  <trd lng="es">secretar&iacute;a</trd>
  <trd lng="fr">secr&eacute;tariat</trd>
  <trd lng="hu">titk&aacute;rs&aacute;g</trd>
  <trd lng="it">segreteria</trd>
  <trd lng="nl">secretariaat</trd>
  <trd lng="ru">&c_s;&c_je;&c_k;&c_r;&c_je;&c_t;&c_a;&c_r;&c_i;&c_a;&c_t;</trd>
  <trd lng="sk">sekretari&aacute;t <klr>(budova)</klr></trd>
  <trd lng="uk">&#x441;&#x435;&#x43A;&#x440;&#x435;&#x442;&#x430;&#x440;&#x456;&#x430;&#x442; (&#x43F;&#x440;&#x438;&#x43C;&#x456;&#x449;&#x435;&#x43D;&#x43D;&#x44F;)</trd>
</drv>
</art>
