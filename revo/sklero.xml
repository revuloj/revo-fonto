<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skleroz</rad>/o
</kap>
<drv mrk="sklero.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malmoli&gcirc;o de histoj:
      <ekz>
        en Germanio &ccirc;irka&ubreve; 120 000 personoj malsanas de multobla
        <tld/>o,
        kiu kondukas 85 procentojn de la pacientoj al
        rulse&gcirc;o<fnt><aut>Martin Cziesla</aut>: <vrk>kio kulpas pri
        multobla sklerozo?</vrk>, <bib>Monato</bib><lok>jaro 1999a,
        numero
        3a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_je;&c_r;&c_o;&c_z;</trd>
  <trd lng="bg">&c_s;&c_k;&c_l;&c_je;&c_r;&c_o;&c_z;&c_a;</trd>
  <trd lng="de">Sklerose</trd>
  <trd lng="en">sclerosis</trd>
  <trdgrp lng="fr">
    <trd>scl&eacute;rose</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szkler&oacute;zis</trd>
    <trd>elmeszesed&eacute;s</trd>,
    <trd>kem&eacute;nyed&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">sclerose</trd>
  <trd lng="ru">&c_s;&c_k;&c_l;&c_je;&c_r;&c_o;&c_z;</trd>
</drv>
</art>
