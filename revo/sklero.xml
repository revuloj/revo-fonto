<art mrk="$Id: sklero.xml,v 1.21 2023/10/21 07:46:47 revo Exp $">
<kap>
  <rad>skleroz</rad>/o
</kap>
<drv mrk="sklero.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malmoli&gcirc;o de histoj:
      <ekz>
        en Germanio &ccirc;irka&ubreve; 120 000 personoj malsanas de
        multobla
        <tld/>o,
        kiu kondukas 85 procentojn de la pacientoj al
        rulse&gcirc;o<fnt><aut>Martin Cziesla</aut>: <vrk>kio kulpas pri
        multobla sklerozo?</vrk>, <bib>Monato</bib><lok>jaro 1999a,
        numero
        3a, p. 24a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_je;&c_r;&c_o;&c_z;</trd>
  <trd lng="bg">&c_s;&c_k;&c_l;&c_je;&c_r;&c_o;&c_z;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>kornat&ecaron;n&iacute;</trd>,
    <trd>skler&oacute;za</trd>
  </trdgrp>
  <trd lng="de">Sklerose</trd>
  <trd lng="en">sclerosis</trd>
  <trd lng="es">esclerosis</trd>
  <trdgrp lng="fr">
    <trd>scl&eacute;rose</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szkler&oacute;zis</trd>
    <trd>elmeszesed&eacute;s</trd>,
    <trd>kem&eacute;nyed&eacute;s</trd>
  </trdgrp>
  <trd lng="id">sklerosis</trd>
  <trd lng="ja">&#x786C;&#21270;&#x75C7;  <pr>&#12371;&#12358;&#x304B;&#12375;&#12423;&#12358;</pr></trd>
  <trd lng="nl">sclerose</trd>
  <trdgrp lng="pl">
    <trd>skleroza</trd>,
    <trd>zwapnienie</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_l;&c_je;&c_r;&c_o;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>k&ocirc;rnatenie</trd>,
    <trd>skler&oacute;za</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&#x43A;&#x43B;&c_je;&c_r;&#x43E;&c_z;</trd>
</drv>
</art>
