<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fig</rad>/o
</kap>
<drv mrk="fig.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sukerdol&ccirc;a mola frukto de <tld/>arbo.
    </dif>
  </snc>
  <trd lng="de">Feige</trd>
  <trd lng="fr">figue</trd>
  <trd lng="hu">f&uuml;ge</trd>
  <trd lng="nl">vijg</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_zh;&c_i;&c_r;</trd>,
    <trd>&c_s;&c_m;&c_o;&c_k;&c_v;&c_a;</trd>,
    <trd>&c_f;&c_i;&c_g;&c_a;</trd>
  </trdgrp>
  <trd lng="tr">incir</trd>
</drv>
<drv mrk="fig.0arbo">
  <kap><tld/>arbo, <tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="morusa.0oj">morusacoj</ref>
      (<trd lng="la">Ficus</trd>).
    </dif>
  </snc>
  <trd lng="de">Feige</trd>
  <trd lng="fr">figuier</trd>
  <trd lng="hu">f&uuml;gefa</trd>
  <trd lng="nl">vijgenboom</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_o;&c_k;&c_o;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_f;&c_i;&c_g;&c_o;&c_v;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_i;&c_n;&c_zh;&c_i;&c_r;</trd>
  </trdgrp>
</drv>
</art>
