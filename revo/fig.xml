<drv mrk="fig.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="morusa.0oj">morusacoj</ref>
      (<trd lng="la">Ficus</trd>), kies unu specion
      <trd lng="la">Ficus carica</trd> oni kulturas pro la fruktoj:
      <ekz>
        ne restos beroj sur la vinberbran&ccirc;o nek <tld/>oj sur la
        <tld/>arbo, kaj la
        folioj forvelkos
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 8:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi estas kiel homo, kiu, ser&ccirc;ante kukurbon sur <tld/>arbo, ne
        vidas la <tld/>on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie kreskis palmoj, vinberujoj, olivarboj, <tld/>ujoj kun
        radikoj en la aero
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter palmoj kaj sova&gcirc;aj <tld/>ujoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/3/30/Ficus_carica3.jpg/640px-Ficus_carica3.jpg" prm="CC-BY-SA-3.0"><tld/>oj &ccirc;e bran&ccirc;o de <tld/>ujo (Ficus carica)
        <fnt>
          <lok><url ref="&WCW;/File:Ficus_carica3.jpg">https://commons.wikimedia.org/wiki/File:Ficus_carica3.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="ca">
    <trd>figuera</trd>,
    <trd>ficus</trd>
  </trdgrp>
  <trd lng="de">Feigenbaum</trd>
  <trd lng="en">fig</trd>
  <trd lng="es">higuera</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t; <ind>&a_A;&a_n;&a_j;&f_y;&a_r;</ind></trd>
  <trd lng="fr">figuier</trd>
  <trd lng="hu">f&uuml;gefa</trd>
  <trd lng="nl">vijgenboom</trd>
  <trd lng="pl">figowiec<klr> pospolity</klr></trd>
  <trd lng="pt">figueira</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_o;&c_k;&c_o;&c_v;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_f;&c_i;&c_g;&c_o;&c_v;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_i;&c_n;&c_zh;&c_i;&c_r;</trd>
  </trdgrp>
</drv>
