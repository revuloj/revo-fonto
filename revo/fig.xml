  <trdgrp lng="id">
    <trd>ara</trd>,
    <trd>tin</trd>
  </trdgrp>
  <trd lng="is">mynd</trd>
  <trd lng="ja">&#x30A4;&#x30C1;&#x30B8;&#x30AF;</trd>
  <trd lng="jw">anjir</trd>
  <trd lng="ka">&#x10DA;&#x10D4;&#x10E6;&#x10D5;&#x10D8;&#x10E1;</trd>
