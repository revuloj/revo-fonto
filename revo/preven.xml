<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: preven.xml,v 1.19 2023/10/20 16:38:17 revo Exp $">
<kap>
  <rad>prevent</rad>/i
</kap>
<drv mrk="preven.0i">
  <kap><tld/>i</kap>
  <snc mrk="preven.0i.antauxmalhelpi">
    <dif>
      <ref tip="dif" cel="help.antauxmal0i">Anta&ubreve;malhelpi</ref>:
      <ekz>
        <ind><tld/>ado</ind> per nura kondomkampanjo estas, miaopinie,
        kretena&jcirc;o
        <fnt><bib>UrdHadda</bib><lok>p. 26</lok></fnt>;
        <trdgrp lng="be">
          <trd>&c_p;&c_r;&c_a;&c_d;&c_u;&c_h;&c_ib;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
          <trd>&c_p;&c_r;&c_a;&c_f;&c_ib;&c_l;&c_ja;&c_k;&c_t;&c_y;&c_k;&c_a;</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Pr&auml;vention</trd>,
          <trd>Vorbeugung</trd>
        </trdgrp>
        <trd lng="en">prevention</trd>
        <trd lng="fr">pr&eacute;vention<klr tip="ind"> (maladie, accidents,
        etc.)</klr></trd>
        <trdgrp lng="hu">
          <trd>megel&odblac;z&eacute;s</trd>,
          <trd>prevenci&oacute;</trd>
        </trdgrp>
        <trd lng="nl">preventie</trd>
      </ekz>
      <ekz>
        la poeto per oblikva rigardo observis la nekonaton por <tld/>i
        ties fu&gcirc;on
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 1</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i marbordajn dama&gcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="&Monato;/2003/008085.html">&Ccirc;u revo iam fari&gcirc;os realo?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kondomo plej oftas por la unua sperto kiel <tld/>ilo, poste plej gravas la
        pilolo
         <fnt>
          <aut>Guido van Damme</aut>
          <vrk><url ref="&Monato;/1995/A950102.php">Gejunula seksumado</url></vrk>
          <bib>Monato</bib>
          <lok>1995/01, p. 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        konflikt<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008516.php">&Ccirc;u E&ubreve;ropa Unio fari&gcirc;as militema?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de teroristaj atencoj 
        <fnt>
          <aut>jpa</aut>
            <vrk><url ref="&Monato;/2000/007093.php">Internacia kunsido de financistoj en
            Prago</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 9</lok>
          </fnt>;
      </ekz>
      <ekz>
        <tld/>a milito
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008000.html">Jur-renverso</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="preven.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Profilakti:
      <ekz>
        prefere <tld/>i, ol penti kaj lamenti, &ccirc;ar kuraci ne eblas
        <fnt><bib>UrdHadda</bib><lok>p. 26</lok></fnt>;
      </ekz>
      <ekz>
        oni aplikas por <tld/>ado kaj kuracado de gripo interferonojn
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2003/008099.html">Birda gripo, &ccirc;u dan&gcirc;era por la homo?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        raportoj pri teo kiel <tld/>ilo de kanceroj aperis fojfoje en amaskomunikiloj
        <fnt>
          <bib>Monato</bib>
          <aut>Peng Jianjun</aut>
          <vrk><url ref="&Monato;/2005/008444.php">Teo por preventi kancerojn</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        por la <tld/>o de la malsano necesas rigoraj kvarantenaj kaj profilaktikaj
        rimedoj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_p;&c_ja;&c_r;&c_e;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_d;&c_u;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF66;&#xF94;&#xF7C;&#xF53;&#xF0B;&#xF60;&#xF42;&#xF7C;&#xF42;&#xF0B;&#xF56;&#xFB1;&#xF7A;&#xF51;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>odvracet</trd>,
    <trd>p&rcaron;edch&aacute;zet</trd>,
    <trd>zabra&ncaron;ovat</trd>,
    <trd>za&zcaron;ehnat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>vorbeugen</trd>,
    <trd>verhindern</trd>
  </trdgrp>
  <trd lng="en">prevent</trd>
  <trdgrp lng="fr">
    <trd>pr&eacute;venir<klr tip="ind"> (emp&ecirc;cher la survenue)</klr></trd>,
    <trd>&eacute;viter<klr tip="ind"> (la survenue de)</klr></trd>,
    <trd>parer<klr tip="ind"> (&eacute;viter)</klr></trd>
  </trdgrp>
  <trd lng="hu">megel&odblac;z <klr tip="ind">(bajt)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x4E88;&#x9632;&#x3059;&#x308B;  <pr>&#x3088;&#x307C;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x672A;&#x7136;&#x306B;&#x9632;&#x3050;  <pr>&#x307F;&#x305C;&#x3093;&#x306B;&#x3075;&#x305B;&#x3050;</pr></trd>
  </trdgrp>
  <trd lng="nl">voorkomen</trd>
  <trdgrp lng="pl">
    <trd>zapobiega&cacute;</trd>,
    <trd>zaradzi&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>prevenir</trd>,
    <trd>evitar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>preveni</trd>,
    <trd>remediu</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_t;&c_v;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pred&iacute;s&tcaron;</trd>,
    <trd>zabra&ncaron;ova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43F;&c_je;&c_r;&c_je;&c_d;&c_zh;&c_a;&c_t;&c_i; (&c_h;&c_v;&#x43E;&c_r;&#x43E;&c_b;&c_u; &c_ib; &c_t;.&#x43F;.)</trd>,
    <trd>&c_z;&c_a;&#x43F;&#x43E;&c_b;&c_ib;&c_g;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>



