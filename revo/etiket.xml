<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>etiket</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="etiket.0o">
  <kap><tld/>o</kap>
  <snc mrk="etiket.0o.ceremoniaro">
    <dif>
      <ref tip="vid" cel="ceremo.0o.solena">Ceremoni</ref>aro
      &ccirc;e solena&jcirc;oj.
    </dif>
    <trd lng="be">&c_e;&c_t;&c_y;&c_k;&c_je;&c_t;</trd>
    <trd lng="bg">&c_je;&c_t;&c_i;&c_k;&c_je;&c_t;</trd>
    <trd lng="de">Etikette</trd>
    <trdgrp lng="en">
      <trd>etiquette</trd>,
      <trd>code of conduct</trd>,
      <trd>rules of behavior</trd>,
      <trd>protocol</trd>
    </trdgrp>
    <trd lng="es">etiqueta<klr tip="ind">
    (ceremonial)</klr></trd>
    <trd lng="fr">&eacute;tiquette<klr tip="ind">
    (c&eacute;r&eacute;monial)</klr></trd>
    <trd lng="hu">etikett</trd>
    <trdgrp lng="nl">
      <trd>etiquette</trd>,
      <trd>protocol</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>etykieta</trd>,
      <trd>protok&oacute;&lstroke;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>cerimonial</trd>,
      <trd>etiqueta</trd>,
      <trd>pragm&aacute;tica</trd>
    </trdgrp>
    <trd lng="ru">&c_e;&c_t;&c_i;&c_k;&c_je;&c_t;</trd>
  </snc>
  <snc mrk="etiket.0o.konduto">
    <dif>
      Reguloj de sinteno en mondumo;
      <ref tip="vid" cel="manier.bonaj0oj.MOR">bonaj manieroj</ref>.
    </dif>    
    <trd lng="be">&c_e;&c_t;&c_y;&c_k;&c_je;&c_t;</trd>
    <trd lng="bg">&c_je;&c_t;&c_i;&c_k;&c_je;&c_t;</trd>
    <trd lng="de">Umgangsformen</trd>
    <trdgrp lng="en">
      <trd>etiquette</trd>,
      <trd>code of conduct</trd>,
      <trd>rules of behavior</trd>,
      <trd>protocol</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>protocolo</trd>,
      <trd>reglas de conducta</trd>
    </trdgrp>
    <trd lng="fr">&eacute;tiquette<klr tip="ind">
    (biens&eacute;ance)</klr></trd>
    <trd lng="hu">illemszab&aacute;lyok</trd>
    <trd lng="nl">etiquette</trd>
    <trdgrp lng="pl">
      <trd>etykieta</trd>,
      <trd>bon ton</trd>,
      <trd>savoir vivre</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>cerimonial</trd>,
      <trd>etiqueta</trd>,
      <trd>pragm&aacute;tica</trd>
    </trdgrp>
    <trd lng="ru">&c_e;&c_t;&c_i;&c_k;&c_je;&c_t;</trd>
  </snc>
  <snc mrk="etiket.0o.karteto">
    <ref tip="dif" cel="etiked.0o.karteto">etikedo<sncref/></ref>
    <trd lng="be">&c_e;&c_t;&c_y;&c_k;&c_je;&c_t;&c_k;&c_a;</trd>
    <trd lng="bg">&c_je;&c_t;&c_i;&c_k;&c_je;&c_t;</trd>
    <trd lng="de">Etikett</trd>
    <trdgrp lng="en">
      <trd>label</trd>,
      <trd>tag</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>r&oacute;tulo</trd>,
      <trd>marca</trd>
    </trdgrp>
    <trd lng="fr">&eacute;tiquette<klr tip="ind"> (fiche)</klr></trd>
    <trd lng="hu">c&iacute;mke</trd>
    <trd lng="nl">etiket</trd>
    <trdgrp lng="pl">
      <trd>etykietka</trd>,
      <trd>naklejka</trd>,
      <trd>nalepka</trd>,
      <trd>fiszka</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>etiqueta</trd>,
      <trd>r&oacute;tulo</trd>,
      <trd>d&iacute;stico</trd>,
      <trd>marca</trd>
    </trdgrp>
    <trd lng="ru">&c_e;&c_t;&c_i;&c_k;&c_je;&c_t;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>etiketa</trd>,
    <trd>návěští</trd>,
    <trd>označení</trd>,
    <trd>viněta</trd>,
    <trd>značka</trd>,
    <trd>štítek</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>etiketa</trd>,
    <trd>nálepka</trd>,
    <trd>vineta</trd>,
    <trd>značka</trd>,
    <trd>štítok</trd>
  </trdgrp>
</drv>
</art>
