<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: etiket.xml,v 1.26 2023/11/07 20:34:12 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>etiket</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="etiket.0o">
  <kap><tld/>o</kap>
  <snc mrk="etiket.0o.MOR">
    <dif>
      Reguloj de sinteno en formalaj cirkonstancoj,
      &ccirc;e oficiala a&ubreve; solena ceremonio, en mondumo:
      <ekz>
        don Pedro Miguel <klr>...</klr> enkondukadis sinjoron Lel&iacute;&ccaron;ek
        en la misterojn de riverencoj kaj moroj de la hispana kortega
        <tld/>o, kiu estas el &ccirc;iuj <tld/>oj la plej <tld/>eca
        <fnt>
          <aut>H. Vavris, trad. E. Kh&uuml;nl kaj K. Proch&aacute;zka,
          red. M. Malovec kaj Z. Hr&scaron;el</aut>
          <vrk><url ref="http://www.esperanto.cz/soubory/Vavris-Sherlock%20Holmes%20en%20la%20servo%20de%20la%20hiaspana%20trono.rtf">
          Sherlock Holmes en la Servo de la Hispana Trono</url></vrk>
          <lok>1995</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="manier.bonaj0oj.MOR">bonaj manieroj</ref>
    </refgrp>
    <trd lng="be">&c_e;&c_t;&c_y;&c_k;&c_je;&c_t;</trd>
    <trd lng="bg">&c_je;&c_t;&c_i;&c_k;&c_je;&c_t;</trd>
    <trdgrp lng="de">
      <trd>Etikette</trd>,
      <trd>Umgangsformen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>protocolo</trd>,
      <trd>reglas de conducta</trd>
      <trd>etiqueta<klr tip="ind"> (ceremonial)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>etiquette</trd>,
      <trd>code of conduct</trd>,
      <trd>rules of behavior</trd>,
      <trd>protocol</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>c&eacute;r&eacute;monial</trd>,
      <trd>&eacute;tiquette</trd>,
      <trd>protocole</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>etikett</trd>,
      <trd>illemszab&aacute;lyok</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>etiquette</trd>,
      <trd>protocol</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bon ton</trd>,
      <trd>etykieta</trd>,
      <trd>protok&oacute;&lstroke;</trd>,
      <trd>savoir vivre</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>cerimonial</trd>,
      <trd>etiqueta</trd>,
      <trd>pragm&aacute;tica</trd>
    </trdgrp>
    <trd lng="ru">&c_e;&c_t;&c_i;&c_k;&c_je;&c_t;</trd>
    <trdgrp lng="zh">
    <trd>&#x724C;&#x5B50; <pr>p&#xE1;izi</pr></trd>,
    <trd>&#x793C;&#x8282; <pr>l&#x1D0;ji&#xE9;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="etiket.0o.karteto">
    <ref tip="dif" cel="etiked.0o.karteto">etikedo<sncref/></ref>
    <trd lng="be">&c_e;&c_t;&c_y;&c_k;&c_je;&c_t;&c_k;&c_a;</trd>
    <trd lng="bg">&c_je;&c_t;&c_i;&c_k;&c_je;&c_t;</trd>
    <trd lng="de">Etikett</trd>
    <trdgrp lng="en">
      <trd>label</trd>,
      <trd>tag</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>r&oacute;tulo</trd>,
      <trd>marca</trd>
    </trdgrp>
    <trd lng="fr">&eacute;tiquette<klr tip="ind"> (fiche)</klr></trd>
    <trd lng="hu">c&iacute;mke</trd>
    <trd lng="nl">etiket</trd>
    <trdgrp lng="pl">
      <trd>etykietka</trd>,
      <trd>naklejka</trd>,
      <trd>nalepka</trd>,
      <trd>fiszka</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>etiqueta</trd>,
      <trd>r&oacute;tulo</trd>,
      <trd>d&iacute;stico</trd>,
      <trd>marca</trd>
    </trdgrp>
    <trd lng="ru">&c_e;&c_t;&c_i;&c_k;&c_je;&c_t;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>etiketa</trd>,
    <trd>n&aacute;v&ecaron;&scaron;t&iacute;</trd>,
    <trd>ozna&ccaron;en&iacute;</trd>,
    <trd>vin&ecaron;ta</trd>,
    <trd>zna&ccaron;ka</trd>,
    <trd>&scaron;t&iacute;tek</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5100;&#x793C;  <pr>&#x304E;&#x308C;&#x3044;</pr></trd>,
    <trd>&#x793C;&#x5100;&#x4F5C;&#x6CD5;  <pr>&#x308C;&#x3044;&#x304E;&#x3055;&#x307B;&#x3046;</pr></trd>,
    <trd>&#x30A8;&#x30C1;&#x30B1;&#x30C3;&#x30C8;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>etiketa</trd>,
    <trd>n&aacute;lepka</trd>,
    <trd>vineta</trd>,
    <trd>zna&ccaron;ka</trd>,
    <trd>&scaron;t&iacute;tok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x435;&#x442;&#x438;&#x43A;&#x435;&#x442;</trd>,
    <trd>&#x446;&#x435;&#x440;&#x435;&#x43C;&#x43E;&#x43D;&#x456;&#x430;&#x43B;</trd>,
    <trd>&#x435;&#x442;&#x438;&#x43A;&#x435;&#x442;&#x43A;&#x430;</trd>,
    <trd>&#x44F;&#x440;&#x43B;&#x438;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="etiket.ret0o">
  <kap>ret<tld/>o</kap>
  <snc mrk="etiket.ret0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Reguloj pri akcepteblaj sinteno kaj parolo
      en retforumoj, en retbabilejoj:
      <ekz>
        diskutado, e&ccirc; akra, estas akceptebla la&ubreve; ret-<tld/>o
        <fnt>
          <aut>M. Weichert</aut>
          <vrk><url ref="http://www.esperanto.net/veb/sce.html#ret-etiketo">
          Fakdosiero pri soc.culture.esperanto ("s.c.e")</url></vrk>
          <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">netiquette</trd>
  <trd lng="fr">n&eacute;tiquette</trd>
</drv>
</art>
