<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: rami.xml,v 1.2 2024/01/11 14:30:37 afido Exp $">
    <kap>
      <rad>rami</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="rami.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref> el familio
      <ref tip="malprt" cel="urtika.0oj">urtikacoj</ref> (<trd lng="la">Boehmeria</trd>), kies
      konata specio
      (<trd lng="la">Boehmeria nivea</trd>), uzi&gcirc;as por produktado de fibroj:
      <ekz>
        <tld/>o estas plurjara orientazia herbo <klr>[&#8230;]</klr> kun 1-2 m altaj tigoj
        <klr>[&#8230;]</klr>, la bastfibroj el la trunko&scirc;elo estas utiligitaj por
        multspecaj teksa&jcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29628">Urtikacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/b/bf/Boehmeria_nivea_kz1.jpg/342px-Boehmeria_nivea_kz1.jpg"
      prm="CC BY-SA 4.0"
      ><tld/>o (Boehmeria nivea), Garfield Park Conservatory, Illinois
        <fnt>
          <aut>Krzysztof Ziarnek, Kenraiz, propra verko</aut>
          <lok><url ref="&WCW;/File:Boehmeria_nivea_kz1.jpg"
              >https://commons.wikimedia.org/wiki/File:Boehmeria_nivea_kz1.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Ramie</trd>
    <trd lng="en">Ramie</trd>
    <trd lng="fr">Ramie</trd>
  </snc>
<trd lng="be">&c_r;&c_a;&c_m;&c_ib; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
</drv>

  </art>

