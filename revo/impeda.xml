<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: impeda.xml,v 1.22 2023/11/19 10:22:12 revo Exp $">
<kap>
  <rad>impedanc</rad>/o
</kap>
<drv mrk="impeda.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>, egala
      al la &scirc;ajna elektra <ref cel="rezist1.0o">rezistanco</ref>
      de cirkvito, kiun trafluas alterna kurento:
      <ekz>
        elektra <tld/>o a&ubreve; pli simple <tld/>o estas mezuro de la kontra&ubreve;o al sinusoida
        elektra kurento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=56386">Elektra impedanco</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        per disvolvi&gcirc;o de tiu ekvacio, oni konstatas, ke la efiko de <tld/>o tra la
        transformilo varias la&ubreve; la dua potenco de la volvo-faktoro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=27289">Transformatoro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="admita.0o">admitanco</ref>,
      <ref cel="om.o0o">omo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_m;&c_p;&c_e;&c_d;&c_a;&c_n;&c_s;</trd>,
    <trd>&c_p;&c_o;&c_w;&c_n;&c_a;&c_je; <ind>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_ib;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
  </trdgrp>
  <trd lng="cs">impedance</trd>
  <trdgrp lng="de">
    <trd>Impedanz</trd>,
    <trd>Scheinwiderstand</trd>
  </trdgrp>
  <trd lng="en">impedance</trd>
  <trd lng="es">impedancia</trd>
  <trd lng="fr">imp&eacute;dance</trd>
  <trdgrp lng="hu">
    <trd>impedancia</trd>,
    <trd>v&aacute;lt&oacute;&aacute;ram&uacute;
    <ind>ellen&aacute;ll&aacute;s</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x30A4;&#x30F3;&#x30D4;&#x30FC;&#x30C0;&#x30F3;&#x30B9;</trd>
  <trd lng="nl">impedantie</trd>
  <trd lng="pl">impedancja</trd>
  <trd lng="pt">imped&acirc;ncia<klr tip="amb"> (f&iacute;sica)</klr>
  </trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_m;&c_p;&c_je;&c_d;&c_a;&c_n;&c_s;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_n;&c_o;&c_je; &c_s;&c_o;&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">impedancia</trd>
  <trdgrp lng="uk">
    <trd>&#x456;&#x43C;&#x43F;&#x435;&#x434;&#x430;&#x43D;&#x441;</trd>,
    <trd>&#x43F;&#x43E;&#x432;&#x43D;&#x438;&#x439; &#x43E;&#x43F;&#x456;&#x440;</trd>
  </trdgrp>
  <trd lng="zh">&#x963B;&#x6297; <pr>z&#x1D4;k&#xE0;ng</pr></trd>
</drv>
</art>
