<kap>
  <rad>Mahara&scirc;tr</rad>/o <fnt><bib>NePiv</bib></fnt>
  <fnt><bib>LanLin</bib></fnt>
</kap>
<drv mrk="maharasxtr.0o">
  <kap><tld/>o</kap>
  <snc mrk="maharasxtr.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Gubernio en la okcidenta parto de
      <ref tip="malprt" cel="hind.H0ujo.POL">Hindujo</ref>:
      <ekz>
        ni distenu la maratojn kaj ilian &scirc;taton <tld/>o, 
        a&ubreve; la teluguojn kaj ilian &scirc;taton Andraprade&scirc;o
        <fnt>
          <aut>P. Da&scirc;gupto</aut>,
          <vrk>La Esperantaj Nomoj por la &Scirc;tatoj en la 
          Respubliko de Barato</vrk>,
          <lok><url ref="http://barato.50webs.org/fenestro/2002/auxgusto.htm">
          Fenestro, 2002-08</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_M;&c_a;&c_g;&c_a;&c_r;&c_a;&c_sh;&c_t;&c_r;&c_a;</trd>
  <trd lng="fr">Maharashtra</trd>
  <trd lng="ja">&#x30DE;&#x30CF;&#x30E9;&#x30B7;&#x30E5;&#x30C8;&#x30E9;&#x5DDE;  <pr>&#x30DE;&#x30CF;&#x30E9;&#x30B7;&#x30E5;&#x30C8;&#x30E9;&#x3057;&#x3085;&#x3046;</pr></trd>
  <trd lng="pl">Maharasztra</trd>     
  <trd lng="ro">Maharashtra</trd>
  <trd lng="uk">&#x41C;&c_a;&c_g;&c_a;&c_r;&c_a;&c_sh;&c_t;&c_r;&c_a;</trd>
</drv>
</art>

