<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fald</rad>/i
</kap>
<drv mrk="fald.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Duoble a&ubreve; pluroble fleksi tukon, paperon kaj &gcirc;enerale ion
      ajn maldikan kaj iom vastan almetante unu sur alian la diversajn
      fleksa&jcirc;ojn:
      <ekz>
        <tld/>i leteron, tukojn pur ilin loki en &scirc;rankon;
      </ekz>
      <ekz>
        <tld/>i paperon en formo de akordiono;
      </ekz>
      <ekz>
        zorge <tld/>ita vesto;
      </ekz>
      <ekz>
        <tld/>i la frunton;
      </ekz>
      <ekz>
        duige <klr>(a&ubreve; duoble)</klr> <tld/>i;
      </ekz>
      <ekz>
        kvaroble <tld/>i paperfolion.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sulk">sulkigi</ref>,
      <ref cel="krisp.0igi">krispigi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">falten</trd>
  <trd lng="en">fold</trd>
  <trd lng="es">doblar</trd>
  <trd lng="fr">plier<klr tip="ind"> (qc de pliable)</klr></trd>
  <trdgrp lng="hu">
    <trd>behajt<klr tip="ind"> (hajtogat)</klr></trd>,
    <trd>hajtogat</trd>
  </trdgrp>
  <trd lng="nl">vouwen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol; <klr>(&c_p;&c_o;&c_p;&c_o;&c_l;&c_a;&c_m;, &c_v;&c_ch;&c_je;&c_t;&c_v;&c_je;&c_r;&c_o; &c_i; &c_t;.&c_p;.)</klr></trd>,
    <trd>&c_s;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="fald.0o0ajxo">
  <kap><tld/>o, <tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      La <tld/>ita parto:
      <ekz>
        robo kun multaj <tld/>oj;
      </ekz>
      <ekz>
        gladi &scirc;tofon por forigi la <tld/>ojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni povas rigardi en la plej ka&scirc;itajn <tld/>ojn de lia
        koro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o de la kutimo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Falte</trd>
  <trd lng="es">doblez</trd>
  <trdgrp lng="fr">
    <trd>pli<klr tip="ind"> (pliure)</klr></trd>,
    <trd>pliure</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hajt&aacute;s<klr tip="ind"> (red&odblac;)</klr></trd>,
    <trd>berak&aacute;s</trd>,
    <trd>red&odblac;</trd>
  </trdgrp>
  <trd lng="nl">vouw</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_g;&c_i;&c_b;</trd>
  </trdgrp>
</drv>

<drv mrk="fald.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Kiun oni povas facile <tld/>i:
      <ekz>
        <tld/>ebla se&gcirc;o;
      </ekz>
      <ekz>
        <tld/>ebla aparato.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>faltbar</trd>,
    <trd>klappbar</trd>
  </trdgrp>
  <trd lng="fr">pliable</trd>
  <trdgrp lng="hu">
    <trd>&ouml;sszecsukhat&oacute;</trd>,
    <trd>&ouml;sszehajthat&oacute;</trd>
  </trdgrp>
  <trd lng="nl">plooibaar</trd>
  <trd lng="ru">&c_s;&c_k;&c_l;&c_a;&c_d;&c_n;&c_o;&c_j;</trd>
</drv>

<drv mrk="fald.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="F"/>i teksa&jcirc;on per da&ubreve;raj paralelaj regulaj
      <tld/>oj:
      <ekz>
        <ind><tld/>umita jupo</ind>
        <trd lng="fr">jupe pliss&eacute;e</trd>
        <trd lng="hu">rakott <ind>szoknya</ind></trd>
        <trd lng="ru">&c_p;&c_l;&c_i;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_a;&c_ja; &c_ju;&c_b;&c_k;&c_a;</trd>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">plisser<klr tip="ind"> (tr. marquer de plis)</klr></trd>
  <trdgrp lng="hu">
    <trd>berak<klr tip="ind"> (szokny&aacute;t)</klr></trd>,
    <trd>plisz&iacute;roz</trd>
  </trdgrp>
  <trd lng="nl">plooien</trd>
  <trd lng="ru">&c_p;&c_l;&c_i;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fald.dis0imal0i">
  <kap>dis<tld/>i, mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Dismeti, etendi la <tld/>itajn partojn, malvolvi, disvolvi:
      <ekz>
        mal<tld/>i la gazeton por tralegi &gcirc;in;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la lingvo ankora&ubreve; ne dis<tld/>is &ccirc;iujn siajn
        ebla&jcirc;ojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dis<tld/>i&gcirc;as la luksego de la somero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>entfalten</trd>,
    <trd>auseinander <ind>falten</ind></trd>
  </trdgrp>
  <trd lng="es">desdoblar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;plier</trd>,
    <trd>d&eacute;ployer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;thajtogat</trd>,
    <trd>kiter&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ontplooien</trd>,
    <trd>openvouwen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol; <klr>(&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_i;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="fald.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Supren <tld/>i:
      <ekz>
        re<tld/>i la manikon &gcirc;is la kubuto.
      </ekz>
    </dif>
    <trd lng="de">hochkrempeln</trd>
    <trdgrp lng="hu">
      <trd>visszahajt<klr tip="ind"> (hajtogat&aacute;ssal)</klr></trd>,
      <trd>felt&udblac;r</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>opstropen</trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_a;&c_k;&c_a;&c_t;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_r;&c_u;&c_k;&c_a;&c_v;&c_a;)</klr></trd>
  </snc>
  <snc>
    <dif>
      Pluroble <tld/>i, zigzage fleksi.
    </dif>
    <refgrp tip="vid">
      <ref cel="akordi.0o">akordiono</ref>,
      <ref cel="balg.0o">balgo</ref>.
    </refgrp>
    <trd lng="hu">red&odblac;z</trd>
    <trdgrp lng="nl">
      <trd>opvouwen</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">replier</trd>
</drv>

<drv mrk="fald.lini0o">
  <kap>lini<tld/>o</kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Dum tekstenigo, a&ubreve;tomata transporto de tajpata vorto
      transiranta la dekstran mar&gcirc;enon en la komencon de la sekva
      linio:
      <ekz>
        &scirc;alti, mal&scirc;alti la lini<tld/>an re&gcirc;imon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Textumlauf</trd>, <trd>Zeilenumbruch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>line wrap</trd>, <trd>word <ind>wrap</ind></trd>
  </trdgrp>
  <trd lng="hu">sort&ouml;r&eacute;s</trd>
  <trd lng="nl">afbreking<klr> (v.e. lijn)</klr></trd>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_o;&c_r;&c_o;&c_t; &c_s;&c_t;&c_r;&c_o;&c_k;</trd>
</drv>
</art>
