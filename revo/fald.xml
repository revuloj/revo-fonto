<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fald.xml,v 1.28 2020/03/14 10:59:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fald</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fald.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fald.0i.MOV">
    <dif>
      Fleksi tukon, paperon kaj &gcirc;enerale
      ion ajn maldikan kaj iom vastan almetante unu sur
      alian la diversajn fleksa&jcirc;ojn:
      <ekz>
        <tld/>i leteron, tukojn pur ilin loki en &scirc;rankon;
      </ekz>
      <ekz>
        <tld/>i paperon en formo de akordiono;
      </ekz>
      <ekz>
        zorge <tld/>ita vesto;
      </ekz>
      <ekz>
        doloro <tld/>is &scirc;ian blankan frunton
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sesan tapi&scirc;on <tld/>u duige
        <fnt>
          <bib>MT</bib>,
          <lok>&Eli; 26:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvaroble <tld/>i paperfolion.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sulk">sulkigi</ref>,
      <ref cel="krisp.0igi">krispigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>faldovat</trd>,
    <trd>ohrnout</trd>,
    <trd>ohrnovat <klr>(l&aacute;tku)</klr></trd>,
    <trd>p&rcaron;ekl&aacute;dat p&rcaron;eh&yacute;bat</trd>,
    <trd>skl&aacute;dat do z&aacute;hyb&uring;</trd>
  </trdgrp>
  <trd lng="de">falten</trd>
  <trd lng="en">fold</trd>
  <trd lng="es">doblar</trd>
  <trd lng="fr">plier<klr tip="ind"> (qc de pliable)</klr></trd>
  <trdgrp lng="hu">
    <trd>behajt<klr tip="ind"> (hajtogat)</klr></trd>,
    <trd>hajtogat</trd>
  </trdgrp>
  <trd lng="nl">vouwen</trd>
  <trdgrp lng="pl">
    <trd>sk&lstroke;ada&cacute;</trd>,
    <trd>zak&lstroke;ada&cacute;</trd>,
    <trd>fa&lstroke;dowa&cacute;</trd>,
    <trd>karbowa&cacute;</trd>,
    <trd>marszczy&cacute;</trd>,
    <trd>plisowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>dobrar</trd>,
    <trd>fazer <ind>pregas</ind></trd>,
    <trd>ferrar <klr tip="amb">(velas)</klr></trd>,
    <trd>vincar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;
    <klr>(&c_p;&c_o;&c_p;&c_o;&c_l;&c_a;&c_m;,
    &c_v;&c_ch;&c_je;&c_t;&c_v;&c_je;&c_r;&c_o; &c_i;
    &c_t;.&c_p;.)</klr></trd>,
    <trd>&c_s;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>posklada&tcaron; <klr>(&scaron;aty, papier)</klr></trd>,
    <trd>posklada&tcaron; <klr>(&scaron;aty| papier)</klr></trd>
  </trdgrp>
  <trd lng="sv">vika</trd>
  <trd lng="tr">katlamak</trd>
</drv>

<drv mrk="fald.0o">
  <kap><tld/>o, <var><kap><tld/>a&jcirc;o</kap></var></kap>
  <snc mrk="fald.0o.MOV">
    <dif>
      La <tld/>ita parto:
      <ekz>
        robo kun multaj <tld/>oj;
      </ekz>
      <ekz>
        gladi &scirc;tofon por forigi la <tld/>ojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni povas rigardi en la plej ka&scirc;itajn <tld/>ojn de ilia
        koro
        <fnt>
          <bib>Fab3</bib>
          <vrk>Historio</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o de la kutimo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Falte</trd>
  <trd lng="es">doblez</trd>
  <trdgrp lng="fr">
    <trd>pli<klr tip="ind"> (pliure)</klr></trd>,
    <trd>pliure</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hajt&aacute;s<klr tip="ind"> (red&odblac;)</klr></trd>,
    <trd>berak&aacute;s</trd>,
    <trd>red&odblac;</trd>
  </trdgrp>
  <trd lng="nl">vouw</trd>
  <trdgrp lng="pl">
    <trd>fa&lstroke;da</trd>,
    <trd>zak&lstroke;adka</trd>,
    <trd>marszczenie</trd>,
    <trd>plisa</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_g;&c_i;&c_b;</trd>
  </trdgrp>
  <trd lng="sv">veck</trd>
  <trdgrp lng="tr">
    <trd>k&inodot;vr&inodot;m</trd>,
    <trd>k&inodot;r&inodot;&scedil;&inodot;k</trd>
  </trdgrp>
</drv>

<drv mrk="fald.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="fald.0ebla.MOV">
    <dif>
      Kiun oni povas facile <tld/>i:
      <ekz>
        <tld/>ebla se&gcirc;o;
      </ekz>
      <ekz>
        <tld/>ebla aparato.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">skl&aacute;dac&iacute;</trd>
  <trdgrp lng="de">
    <trd>faltbar</trd>,
    <trd>klappbar</trd>
  </trdgrp>
  <trd lng="fr">pliable</trd>
  <trdgrp lng="hu">
    <trd>&ouml;sszecsukhat&oacute;</trd>,
    <trd>&ouml;sszehajthat&oacute;</trd>
  </trdgrp>
  <trd lng="nl">plooibaar</trd>
  <trd lng="pl">sk&lstroke;adany</trd>
  <trd lng="ru">&c_s;&c_k;&c_l;&c_a;&c_d;&c_n;&c_o;&c_j;</trd>
  <trd lng="sk">skladac&iacute;</trd>
  <trd lng="sv">vikbar</trd>
  <trd lng="tr">katlanabilen<klr>(e&scedil;ya)</klr></trd>
</drv>

<drv mrk="fald.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fald.0umi.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="F"/>i teksa&jcirc;on per da&ubreve;raj paralelaj regulaj
      <tld/>oj:
      <ekz>
        <ind><tld/>umita jupo</ind>
        <trd lng="fr">jupe pliss&eacute;e</trd>
        <trd lng="hu">rakott <ind>szoknya</ind></trd>
        <trd lng="ru">&c_p;&c_l;&c_i;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_a;&c_ja;
        &c_ju;&c_b;&c_k;&c_a;</trd>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">plisovat</trd>
  <trd lng="fr">plisser<klr tip="ind"> (tr. marquer de plis)</klr></trd>
  <trdgrp lng="hu">
    <trd>berak<klr tip="ind"> (szokny&aacute;t)</klr></trd>,
    <trd>plisz&iacute;roz</trd>
  </trdgrp>
  <trd lng="nl">plooien</trd>
  <trdgrp lng="pl">
    <trd>plisowa&cacute;</trd>,
    <trd>marszczy&cacute;</trd>
  </trdgrp>
  <trd
  lng="ru">&c_p;&c_l;&c_i;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">plisova&tcaron;</trd>
</drv>

<drv mrk="fald.dis0i">
  <kap>dis<tld/>i, <var><kap>mal<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fald.dis0i.MOV">
    <dif>
      Dismeti, etendi la <tld/>itajn partojn, malvolvi, disvolvi:
      <ekz>
        mal<tld/>i la gazeton por tralegi &gcirc;in;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la lingvo ankora&ubreve; ne dis<tld/>is &ccirc;iujn siajn
        ebla&jcirc;ojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>odstranit z&aacute;hyby</trd>,
    <trd>rozkl&aacute;dat</trd>,
    <trd>rozlo&zcaron;it</trd>,
    <trd>vyhladit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>entfalten</trd>,
    <trd>auseinander <ind>falten</ind></trd>
  </trdgrp>
  <trd lng="es">desdoblar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;plier</trd>,
    <trd>d&eacute;ployer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&eacute;thajtogat</trd>,
    <trd>kiter&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ontplooien</trd>,
    <trd>openvouwen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozk&lstroke;ada&cacute;</trd>,
    <trd>rozprostowywa&cacute;</trd>,
    <trd>rozwija&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_je;&c_r;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;
    <klr>(&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_i;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozprestrie&tcaron;</trd>,
    <trd>rozvin&uacute;&tcaron;</trd>
  </trdgrp>
  <trd lng="sv"><ind>vika</ind> upp</trd>
</drv>

<drv mrk="fald.dis0igxi">
  <kap>dis<tld/>i&gcirc;i,
  <var><kap>mal<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fald.dis0igxi.MOV">
    <dif>
      Dismeti siajn anta&ubreve;e kun<tld/>itajn partojn:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dis<tld/>i&gcirc;as la luksego de la somero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;plier</ind></trd>,
    <trd>se <ind>d&eacute;ployer</ind></trd>
  </trdgrp>
</drv>

<drv mrk="fald.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fald.kun0i.MOV">
    <dif>
      Malvastigi, pluroble <tld/>ante:
      <ekz>
        &scirc;i <klr>...</klr> kun<tld/>is la mantelon kaj metis &gcirc;in en sian 
        keston
        <fnt>
          <aut>-, trad. B. Ragnarsson</aut>,
          <vrk>Sagao de Njal</vrk>,
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">replier</trd>
</drv>

<drv mrk="fald.kun0igxi">
  <kap>kun<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fald.kun0igxi.MOV">
    <dif>
      Sin malgrandigi per <tld/>ado de sia formo:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        iu kun<tld/>i&gcirc;is pro ridego, rigardinte vortaron, &scirc;ajne 
        kompreninte kiel simpla estis tiu vorto
        <fnt>
          <aut>Maksim</aut>,
          <vrk>en: <url ref="https://vk.com/topic-57966658_29221920">
          Impresoj pri Alte-1</url></vrk>,
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">se <ind>replier</ind></trd>
</drv>

<drv mrk="fald.re0i">
  <kap>re<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fald.re0i.MOV_supren">
    <dif>
      Supren <tld/>i:
      <ekz>
        re<tld/>i la manikon &gcirc;is la kubuto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kusp.0i">kuspi</ref>
    </refgrp>
    <trd lng="de">hochkrempeln</trd>
    <trdgrp lng="hu">
      <trd>visszahajt<klr tip="ind"> (hajtogat&aacute;ssal)</klr></trd>,
      <trd>felt&udblac;r</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>opstropen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>podwija&cacute;<klr> (nogawki)</klr></trd>,
      <trd>zak&lstroke;ada&cacute;</trd>,
      <trd>zakasywa&cacute;<klr> (r&eogonek;kawy)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_a;&c_k;&c_a;&c_t;&c_a;&c_t;&c_mol;<klr
    tip="amb">
    (&c_r;&c_u;&c_k;&c_a;&c_v;&c_a;)</klr></trd>
  </snc>
  <snc mrk="fald.re0i.MOV_pluroble">
    <dif>
      Pluroble <tld/>i, zigzage fleksi.
    </dif>
    <refgrp tip="vid">
      <ref cel="akordi.0o">akordiono</ref>,
      <ref cel="balg.0o">balgo</ref>.
    </refgrp>
    <trd lng="hu">red&odblac;z</trd>
    <trdgrp lng="nl">
      <trd>opvouwen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>sk&lstroke;ada&cacute; w <ind>harmonijk&eogonek;</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>ohrnout</trd>,
    <trd>vyhrnout <klr>(ruk&aacute;vy)</klr></trd>
  </trdgrp>
  <trd lng="fr">replier</trd>
  <trdgrp lng="sk">
    <trd>ohrn&uacute;&tcaron;</trd>,
    <trd>vyhrn&uacute;&tcaron; <klr>(ruk&aacute;vy)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="fald.re0o">
  <kap>re<tld/>o</kap>
  <snc mrk="fald.re0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Parto de vesto returnita eksteren:
      <ekz>
        li &ccirc;iuminute rigardis la roversojn kaj manikajn
        re<tld/>ojn, kaj sin tutan en la spegulo
        <fnt>
          <aut>F. Dostojevskij, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="http://esperanto-ondo.ru/Rn-dost2.htm">La
          centjarulino</url></vrk>,
          <bib>LOdE</bib>,
          <lok>1999:7 (57)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">revers<klr> (de v&ecirc;tement)</klr></trd>
    <trdgrp lng="pl">
      <trd>wy&lstroke;&oacute;g</trd>,
      <trd>zak&lstroke;adka</trd>
    </trdgrp>
  </snc>
  <trd lng="sk">man&zcaron;eta <klr>(nohav&iacute;c)</klr></trd>
</drv>

<drv mrk="fald.lini0o">
  <kap>lini<tld/>o</kap>
  <snc mrk="fald.lini0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Dum tekstenigo, a&ubreve;tomata transporto de tajpata vorto
      transiranta la dekstran mar&gcirc;enon en la komencon de la sekva
      linio:
      <ekz>
        &scirc;alti, mal&scirc;alti la lini<tld/>an re&gcirc;imon.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">&rcaron;&aacute;dkov&aacute;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>Textumlauf</trd>,
    <trd>Zeilenumbruch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>line wrap</trd>,
    <trd>word <ind>wrap</ind></trd>
  </trdgrp>
  <trd lng="fr">retour &agrave; la ligne automatique</trd>
  <trd lng="hu">sort&ouml;r&eacute;s</trd>
  <trd lng="nl">afbreking<klr> (v.e. lijn)</klr></trd>
  <trdgrp lng="pl">
    <trd><klr>(automatyczne)</klr>&lstroke;amanie wierszy</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_o;&c_r;&c_o;&c_t; &c_s;&c_t;&c_r;&c_o;&c_k;</trd>
  <trd lng="sk">riadkovanie</trd>
</drv>
</art>
