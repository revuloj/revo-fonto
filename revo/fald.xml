<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fald.xml,v 1.43 2025/11/28 14:32:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fald</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="fald.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fald.0i.MOV">
    <dif>
      <ref tip="super" cel="fleks.0i.kurbigi">Fleksi</ref> tukon, paperon kaj &gcirc;enerale
      ion ajn maldikan kaj iom vastan almetante unu sur
      alian la diversajn fleksa&jcirc;ojn:
      <ekz>
        <klr>[&scirc;i]</klr> <tld/>is liajn &ccirc;emizojn
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> faldas la leteron kaj surskribas <klr>[adreson]</klr>
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni ricevas etan <tld/>itan ventumilon
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        doloro <tld/>is &scirc;ian blankan frunton
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        dense <tld/>itaj kurtenoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sesan tapi&scirc;on <tld/>u duige
        <fnt>
          <bib>MT</bib>,
          <lok>&Eli; 26:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        Iva&ccirc;jo kvaroble <klr>(prefere kvaronige a&ubreve; kvaropige, t.e. duoble)</klr>
        faldis la skribitan folion kaj metis &gcirc;in en la koverton
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 12 (62)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sulk">sulkigi</ref>,
      <ref cel="krisp.0igi">krispigi</ref>.
      <ref cel="plis.0i">plisi</ref>
    </refgrp>
  </snc>
  <snc mrk="fald.0i.fleksi">
    <dif>
      Kunmeti du surfacojn:
      <ekz>
        <tld/>u la manojn kaj klinu la kapojn
        <klr>(por pre&gcirc;i)</klr>
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li kvaza&ubreve; pre&gcirc;e <tld/>is siajn manojn &ccirc;irka&ubreve; la violonon
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_u; &c_n;&c_je;&c_k;&c_a;&c_l;&c_mol;&c_k;&c_ib; &c_r;&c_a;&c_z;&c_o;&c_w;)</klr></trd>,
    <trd>&c_z;&c_g;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>faldovat</trd>,
    <trd>ohrnout</trd>,
    <trd>ohrnovat <klr>(l&aacute;tku)</klr></trd>,
    <trd>p&rcaron;ekl&aacute;dat p&rcaron;eh&yacute;bat</trd>,
    <trd>skl&aacute;dat do z&aacute;hyb&uring;</trd>
  </trdgrp>
  <trd lng="de">falten</trd>
  <trd lng="en">fold</trd>
  <trd lng="es">doblar</trd>
  <trd lng="fr">plier<klr tip="ind"> (qc de pliable)</klr></trd>
  <trdgrp lng="hu">
    <trd>behajt<klr tip="ind"> (hajtogat)</klr></trd>,
    <trd>hajtogat</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>lipat</trd>,
    <trd><baz>lipat</baz>melipat</trd>
  </trdgrp>
  <trd lng="ja">&#x6298;&#x308A;&#x305F;&#x305F;&#x3080;  <pr>&#x304A;&#x308A;&#x305F;&#x305F;&#x3080;</pr></trd>
  <trd lng="nl">vouwen</trd>
  <trdgrp lng="pl">
    <trd>sk&lstroke;ada&cacute;</trd>,
    <trd>zak&lstroke;ada&cacute;</trd>,
    <trd>fa&lstroke;dowa&cacute;</trd>,
    <trd>karbowa&cacute;</trd>,
    <trd>marszczy&cacute;</trd>,
    <trd>plisowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>dobrar</trd>,
    <trd>fazer <ind>pregas</ind></trd>,
    <trd>ferrar <klr tip="amb">(velas)</klr></trd>,
    <trd>vincar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;
    <klr>(&c_p;&c_o;&c_p;&c_o;&c_l;&c_a;&c_m;,
    &c_v;&c_ch;&c_je;&c_t;&c_v;&c_je;&c_r;&c_o; &c_i;
    &c_t;.&c_p;.)</klr></trd>,
    <trd>&c_s;&c_g;&c_i;&c_b;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_g;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>posklada&tcaron; <klr>(&scaron;aty, papier)</klr></trd>,
    <trd>posklada&tcaron; <klr>(&scaron;aty| papier)</klr></trd>
  </trdgrp>
  <trd lng="sv">vika</trd>
  <trd lng="tr">katlamak</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_g;&c_i;&c_n;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_g;&c_o;&c_r;&c_t;&c_a;&c_t;&c_i; (&c_u; &c_k;&c_ib;&c_l;&c_mol;&c_k;&c_a; &c_r;&c_a;&c_z;&c_ib;&c_v;)</trd>,
    <trd>&c_z;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i; &c_v; &c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_i;</trd>,
    <trd>&c_p;&c_l;&c_ib;&c_s;&c_i;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_c;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6298;&#x53E0; <pr>zh&eacute;di&eacute;</pr></trd>,
    <trd>&#x5BF9;&#x6298; <pr>du&igrave;zh&eacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fald.0o">
  <kap><tld/>o</kap>
  <snc mrk="fald.0o.MOV">
    <dif>
      La <tld/>ita parto:
      <ekz>
        &scirc;i portis longan veston kun <tld/>oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la <tld/>oj de la grandaj veluraj kurtenoj &ccirc;irka&ubreve;e rigardis strangaj kapoj
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o en la &scirc;tofo nebone ebenigita
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[subite]</klr> rajdistoj <klr>[&#x2026;]</klr> ne ekviditaj de ni pro <tld/>o de
        l&#x2019; ebena&jcirc;o, rapidis al ni
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni povas rigardi en la plej ka&scirc;itajn <tld/>ojn de ilia
        koro
        <fnt>
          <bib>Fab3</bib>
          <vrk>Historio</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la plej intimaj <tld/>oj de <klr>[&scirc;ia]</klr> konscienco
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_je;&c_r;&c_a;&c_g;&c_ib;&c_b;</trd>
    </trdgrp>
    <trd lng="de">Falte</trd>
    <trd lng="en">fold</trd>
    <trd lng="es">doblez</trd>
    <trdgrp lng="fr">
      <trd>pli<klr tip="ind"> (pliure)</klr></trd>,
      <trd>pliure</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>hajt&aacute;s<klr tip="ind"> (red&odblac;)</klr></trd>,
      <trd>berak&aacute;s</trd>,
      <trd>red&odblac;</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>lipatan</trd>,
      <trd>rimpel</trd>,
      <trd>wiru</trd>
    </trdgrp>
    <trdgrp lng="ja">
      <trd>&#x6298;&#x308A;&#x76EE;  <pr>&#x304A;&#x308A;&#x3081;</pr></trd>,
      <trd>&#x3072;&#x3060;</trd>
    </trdgrp>
    <trd lng="ne">&#x915;&#x94D;&#x930;&#x940;&#x91C;</trd>
    <trd lng="nl">vouw</trd>
    <trdgrp lng="pl">
      <trd>fa&lstroke;da</trd>,
      <trd>zak&lstroke;adka</trd>,
      <trd>marszczenie</trd>,
      <trd>plisa</trd>
    </trdgrp>
    <trd lng="ro">rid</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_g;&c_i;&c_b;</trd>
    </trdgrp>
    <trd lng="sv">veck</trd>
    <trdgrp lng="tr">
      <trd>k&inodot;vr&inodot;m</trd>,
      <trd>k&inodot;r&inodot;&scedil;&inodot;k</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#x8936; <pr>zh&ecaron;</pr></trd>,
      <trd>&#x7E10; <pr>zh&ograve;u</pr></trd>,
      <trd>&#x88E5; <pr>ji&#x1CE;n</pr></trd>,
      <trd>&#x8936; <pr>zh&ecaron;</pr></trd>,
      <trd>&#x7E10; <pr>zh&ograve;u</pr></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="fald.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Io faldita:
      <ekz>
        eldonita <tld/>a&jcirc;o <klr>(faldfolio)</klr> kun lia portreto kaj biografieto
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 6 (104)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Faltblatt</trd>
</drv>

<drv mrk="fald.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="fald.0ebla.MOV">
    <dif>
      Kiun oni povas facile <tld/>i:
      <ekz>
        <tld/>ebla se&gcirc;o;
      </ekz>
      <ekz>
        <tld/>ebla aparato.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_y;</trd>
  <trd lng="cs">skl&aacute;dac&iacute;</trd>
  <trdgrp lng="de">
    <trd>faltbar</trd>,
    <trd>klappbar</trd>
  </trdgrp>
  <trd lng="en">foldable</trd>
  <trd lng="fr">pliable</trd>
  <trdgrp lng="hu">
    <trd>&ouml;sszecsukhat&oacute;</trd>,
    <trd>&ouml;sszehajthat&oacute;</trd>
  </trdgrp>
  <trd lng="id">dapat dilipat</trd>
  <trd lng="ja">&#x6298;&#x308A;&#x305F;&#x305F;&#x3081;&#x308B;
  <pr>&#x304A;&#x308A;&#x305F;&#x305F;&#x3081;&#x308B;</pr></trd>
  <trd lng="nl">plooibaar</trd>
  <trd lng="pl">sk&lstroke;adany</trd>
  <trd lng="ru">&c_s;&c_k;&c_l;&c_a;&c_d;&c_n;&c_o;&c_j;</trd>
  <trd lng="sk">skladac&iacute;</trd>
  <trd lng="sv">vikbar</trd>
  <trd lng="tr">katlanabilen<klr>(e&scedil;ya)</klr></trd>
  <trd lng="zh">&#x6298;&#x53E0;&#x5F0F; <pr>sh&eacute;di&eacute;sh&igrave;</pr></trd>
</drv>

<drv mrk="fald.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fald.0umi.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="F"/>i teksa&jcirc;on per da&ubreve;raj paralelaj regulaj
      <tld/>oj:
      <ekz>
        <ind><tld/>umita jupo</ind>
        <trd lng="fr">jupe pliss&eacute;e</trd>
        <trd lng="hu">rakott <ind>szoknya</ind></trd>
        <trdgrp lng="id">
          <trd>rok rimpel</trd>,
          <trd>rok wiru</trd>
        </trdgrp>
        <trd lng="ru">&c_p;&c_l;&c_i;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_a;&c_ja;
        &c_ju;&c_b;&c_k;&c_a;</trd>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ib;&c_s;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">plisovat</trd>
  <trd lng="en">pleat</trd>
  <trd lng="fr">plisser<klr tip="ind"> (tr. marquer de plis)</klr></trd>
  <trdgrp lng="hu">
    <trd>berak<klr tip="ind"> (szokny&aacute;t)</klr></trd>,
    <trd>plisz&iacute;roz</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>rimpel</baz>merimpel</trd>,
    <trd><baz>wiru</baz>mewiru</trd>
  </trdgrp>
  <trd lng="nl">plooien</trd>
  <trdgrp lng="pl">
    <trd>plisowa&cacute;</trd>,
    <trd>marszczy&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_i;&c_s;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">plisova&tcaron;</trd>
</drv>

<drv mrk="fald.dis0igxi">
  <kap>dis<tld/>i&gcirc;i,
  <var><kap>mal<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fald.dis0igxi.MOV">
    <dif>
      Dismeti siajn anta&ubreve;e kun<tld/>itajn partojn:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dis<tld/>i&gcirc;as la luksego de la somero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_k;&c_l;&c_a;&c_d;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_o;&c_r;&c_t;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="en">unfold</trd>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;plier</ind></trd>,
    <trd>se <ind>d&eacute;ployer</ind></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>buka</baz>terbuka<klr tip="amb"> (lipatannya)</klr></trd>,
    <trd><baz>urai</baz>terurai</trd>,
    <trd><baz>orak</baz>terorak</trd>
  </trdgrp>
  <trd lng="pl">rozwin&aogonek;&cacute; si&eogonek;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_k;&c_l;&c_a;&c_d;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_g;&c_o;&c_r;&c_t;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="fald.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fald.kun0i.MOV">
    <dif>
      Malvastigi, pluroble <tld/>ante:
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> kun<tld/>is la mantelon kaj metis &gcirc;in en sian
        keston
        <fnt>
          <aut>-, trad. B. Ragnarsson</aut>,
          <vrk>Sagao de Njal</vrk>,
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_k;&c_a;&c_m;&c_p;&c_a;&c_k;&c_t;&c_n;&c_a;)</klr></trd>
  <trd lng="en">fold up</trd>
  <trd lng="fr">replier</trd>
  <trd lng="id"><baz>lipat</baz>melipat-lipat</trd>
  <trd lng="pl">dokonywa&cacute; <ind>splotu</ind></trd>
</drv>

<drv mrk="fald.kun0igxi">
  <kap>kun<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="fald.kun0igxi.MOV">
    <dif>
      Sin malgrandigi per <tld/>ado de sia formo:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        iu kun<tld/>i&gcirc;is pro ridego, rigardinte vortaron, &scirc;ajne
        kompreninte kiel simpla estis tiu vorto
        <fnt>
          <aut>Maksim</aut>,
          <vrk>en: <url ref="https://vk.com/topic-57966658_29221920">
          Impresoj pri Alte-1</url></vrk>,
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_d;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_k;&c_a;&c_m;&c_p;&c_a;&c_k;&c_t;&c_n;&c_a;)</klr></trd>
  <trd lng="en">fold up</trd>
  <trd lng="fr">se <ind>replier</ind></trd>
  <trd lng="id"><baz>lipat</baz>terlipat-lipat</trd>
  <trd lng="pl">zwija&cacute; si&eogonek;</trd>
</drv>

<drv mrk="fald.mal0i">
  <kap>
    mal<tld/>i, <var><kap>dis<tld/>i</kap></var>
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disigi la <tld/>itajn partojn de io:
      <ekz>
        Vilfrido <tld/>is kaj mal<tld/>is la programon
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i mal<tld/>is la duopige <tld/>itan folieton
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi dis<tld/>is la vesta&jcirc;on kaj trovis en &gcirc;i anka&ubreve; &ccirc;emizon
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
