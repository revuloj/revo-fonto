  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_n;&c_o;&c_s; &c_r;&c_a;&c_d;&c_k;&c_o;&c_w; <klr>(&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;)</klr></trd>
