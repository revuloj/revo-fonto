  <trd lng="it">lasciare</trd>
  <trd lng="kk">&c_m;&#x4AF;&c_m;&c_k;&c_ib;&c_n;&c_d;&c_ib;&c_k;</trd>
  <trd lng="km">&#x17A2;&#x1793;&#x17BB;&#x1789;&#x17D2;&#x1789;&#x17B6;&#x178F;&#x17B1;&#x17D2;&#x1799;</trd>
  <trd lng="kn">&#xC85;&#xCB5;&#xC95;&#xCBE;&#xCB6;</trd>
  <trd lng="ko">&#xD558;&#xC790;</trd>
  <trd lng="ku">berdan</trd>
  <trd lng="ky">&c_zh;&c_o;&c_l;</trd>
  <trd lng="lat">dimitte</trd>
  <trd lng="lo">&#xEC3;&#xEAB;&#xEC9;</trd>
  <trd lng="lt">tegul</trd>
  <trd lng="lv">&#x160C;aujiet</trd>
  <trd lng="mg">aoka</trd>
