</drv>

<drv mrk="las.preter0i">
  <kap>preter<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="L"/>i preterpasante, neatentante, ne kaptante:
      <ekz>
        ni preter<tld/>as multajn aliajn <klr>[&#x2026;]</klr> anta&ubreve;dirojn kaj transiras al
        pririgardo de la teorioj, sur kiuj liaj anta&ubreve;diroj estis fondataj
        <fnt><bib>FK</bib></fnt>;
      </ekz><ekz>
        mi esperas, ke li ne preter<tld/>is 
        <klr>(<ref cel="traf.mal0i">maltrafis</ref>)</klr> la vagonaron
        <fnt><bib>KPr</bib></fnt>;
      </ekz><ekz>
        <klr>[li]</klr> divenis, tra la maldol&ccirc;eco de tiuj paroloj, la konfeson, kiun
        preter<tld/>is la junulino
        <fnt><bib>KPr</bib></fnt>;
      </ekz><ekz>
        dank&#x2019; al kelkaj vortoj, kiujn preter<tld/>is tiu viro, mi e&ccirc; kredis kompreni
        <fnt><bib>ChL</bib></fnt>;
      </ekz><ekz>
        vi estas homo sa&gcirc;a kaj vi ne amas preter<tld/>i tion, kio na&gcirc;as al la manoj
        <fnt><bib>Revizoro</bib></fnt>;
      </ekz><ekz>
        li preter<tld/>is neniun okazon
        <fnt><bib>ChL</bib></fnt>;
      </ekz><ekz>
        &gcirc;is nun &ccirc;iuj iliaj ama&jcirc;oj ne preter<tld/>is la limojn de negrava amindumeto
        <fnt><bib>ChL</bib></fnt>;
      </ekz><ekz>
        oni malsupre solene ekkantis la dek-ok-pre&gcirc;on, kiun neniu devas preter<tld/>i
        <fnt><bib>RBa</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="neglek.0i">neglekti</ref>,
      <ref cel="salt.0i">salti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
