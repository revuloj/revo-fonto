<drv mrk="las.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="L"/>i trairi:
      <ekz>
        pro la malvasteco de la eniroj, kiuj apena&ubreve; sufi&ccirc;as por tra<tld/>i la plej
        malgrandajn regatojn, la gere&gcirc;oj neniel povas eliri
        <fnt><bib>FK</bib></fnt>;
      </ekz><ekz>
        &scirc;ipetoj <klr>[&#x2026;]</klr> diskuradis en &ccirc;iujn direktojn por tra<tld/>i grandegan
        floson
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz><ekz>
        la dua kohorto tra<tld/>is supren nur tiujn, kiuj havis rolon en la ekzekuto
        <fnt><bib>MkM</bib></fnt>;
      </ekz><ekz>
        la maldika &scirc;tofo tra<tld/>is neklaran lumon
        <fnt><bib>Metrop</bib></fnt>;
      </ekz><ekz>
        en la alto &scirc;vebis densa, senkontura nuba&jcirc;o, kiu sen&ccirc;ese ver&scirc;is akvon kaj ne
        tra<tld/>is e&ccirc; imageton pri suno kaj lumo
        <fnt><bib>InfanTorent2</bib></fnt>;
      </ekz><ekz>
        interne de ili estas &scirc;tone malmola muro, kiu <klr>[&#x2026;]</klr> ne tra<tld/>as ion
        el tio, kio estas ka&scirc;ita en ilia propra interno
        <fnt><bib>HsT</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_r;&c_a;&c_z; &c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
