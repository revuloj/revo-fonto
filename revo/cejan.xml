<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>cejan</rad>/o
</kap>
<drv mrk="cejan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="cejan.0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="centau1.0o">centa&ubreve;reo</ref>
      (<trd lng="la">Centaurea cyanus</trd>), kun blua floraro:
      <ekz>&scirc;i eniris en la altan densan sekalon, en kiu abunde kreskis 
<tld/>o
      <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 8a, p. 41a</lok></fnt>.
</ekz>
    </dif>
  <trd lng="be">&c_v;&c_a;&c_l;&c_o;&c_sh;&c_k;&c_a;</trd>
  <trd lng="bg">&c_s;&c_i;&c_n;&c_ja; <ind>&c_m;&c_je;&c_t;&c_l;&c_i;&c_ch;&c_i;&c_n;&c_a;</ind></trd>
  <trd lng="br">glizinenn</trd>
  <trdgrp lng="ca">
    <trd>blauet</trd>,
    <trd>angelet</trd>,
    <trd>capblau</trd>
  </trdgrp>
  <trd lng="de">Kornblume</trd>
  <trdgrp lng="en">
     <trd>cornflower</trd>
     <trd>bluebottle</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aciano</trd>,
    <trd>azulejo</trd>
  </trdgrp>
  <trd lng="fr">bleuet <klr tip="ind">(fleur)</klr></trd>
  <trd lng="hu">b&uacute;zavir&aacute;g</trd>
  <trd lng="it">fiordaliso</trd>
  <trd lng="nl">korenbloem</trd>
  <trd lng="pl">chaber b&lstroke;awatek</trd>
  <trd lng="ru">&c_v;&c_a;&c_s;&c_i;&c_l;&c_jo;&c_k; <klr>(&c_g;&c_o;&c_l;&c_u;&c_b;&c_o;&c_j;)</klr></trd>
  </snc>
  <snc mrk="cejan.0o.koloro">
    <ref tip="dif" cel="cejan.0a.koloro">cejana</ref>
    <trdgrp lng="es">
      <trd>aciano</trd>,
      <trd>azul aciano</trd>,
      <trd>cian</trd>
     </trdgrp>
    <trd lng="pl">cyjan</trd>
    <trd lng="en">cyan</trd>
  </snc>
  <adm>
  Koloro de floro de cejano neniel samas al la cejana kolor uzata en
  presado.
  <aut>SC</aut>
  </adm>
</drv>

<drv mrk="cejan.0a">
  <kap><tld/>a</kap>
  <snc mrk="cejan.0a.koloro">
    <dif>
Koloro inter blua kaj verda, grava en subtrahaj kolormodeloj por inkaj 
presiloj kaj fotokompostado.
    </dif>
    <trd lng="bg">&c_c;&c_i;&c_a;&c_n;</trd>
    <trd lng="ca">cian</trd>
    <trd lng="de">Cyan</trd>
    <trd lng="en">cyan</trd>
    <trdgrp lng="es">
     <trd>aciano</trd>,
     <trd>cian</trd>
    </trdgrp>
    <trd lng="it">ciano</trd>
    <trd lng="nl">cyaan</trd>
    <trd lng="pl">cyjan <klr>(b&lstroke;&eogonek;kitny)</klr></trd>
  </snc>
 <snc mrk="cejan.0a.floro">
    <dif>
      Rilata al <tld/>o.
    </dif>
  </snc>
</drv>

</art>
