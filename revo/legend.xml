<art mrk="$Id: legend.xml,v 1.29 2024/01/15 16:14:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>legend</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="legend.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="legend.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Rakonto pri malnovaj okazinta&jcirc;oj, en kiu intermiksi&gcirc;as historiaj
      kaj
      fabelaj tradiciaj faktoj:
      <ekz>
        lian fantazion jam frue stimulis la <tld/>o pri la Babelturo
        <fnt><bib>Dan&gcirc;Lng</bib>, <lok>1.1.</lok></fnt>;
      </ekz>
      <ekz>
        popolaj <tld/>oj;
      </ekz>
      <ekz>
        la ora <tld/>o <klr>(pri sanktuloj)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_g;&c_je;&c_n;&c_d;&c_a;</trd>
  <trd lng="br">mojenn</trd>
  <trdgrp lng="cs">
    <trd>b&aacute;je</trd>,
    <trd>legenda</trd>,
    <trd>pov&ecaron;st</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sage</trd>,
    <trd>Legende</trd>
  </trdgrp>
  <trd lng="el">&my;&ypsilon_ton;&theta;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>legend</trd>,
    <trd>myth</trd>
  </trdgrp>
  <trd lng="es">leyenda</trd>
  <trd lng="fr">l&eacute;gende</trd>
  <trd lng="he">&alef;&gimel;&dalet;&he;</trd>
  <trd lng="hu">legenda</trd>
  <trdgrp lng="ja">
    <trd>&#x4F1D;&#x8AAC;  <pr>&#x3067;&#x3093;&#x305B;&#x3064;</pr></trd>,
    <trd>&#x8A00;&#x3044;&#x4F1D;&#x3048;  <pr>&#x3044;&#x3044;&#x3064;&#x305F;&#x3048;</pr></trd>
  </trdgrp>
  <trd lng="nl">legende</trd>
  <trd lng="pl">legenda</trd>
  <trdgrp lng="pt">
    <trd>lenda</trd>,
    <trd>legenda <klr>(lenda)</klr></trd>
  </trdgrp>
  <trd lng="ro">legend&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_g;&c_je;&c_n;&c_d;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>b&aacute;j</trd>,
    <trd>legenda</trd>,
    <trd>poves&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">legend</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_je;&c_gu;&c_je;&#x43D;&c_d;&c_a;</trd>,
    <trd>&c_s;&#x43A;&c_a;&c_z;&c_a;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x795E;&#x8A71; <pr>sh&#xE9;nhu&#xE0;</pr></trd>,
    <trd>&#x795E;&#x8BDD; <pr>sh&#xE9;nhu&#xE0;</pr></trd>,
    <trd>&#x4F20;&#x8BF4; <pr>chu&#xE1;nshu&#x14D;</pr></trd>,
    <trd>&#x50B3;&#x8AAA; <pr>chu&#xE1;nshu&#x14D;</pr></trd>,
    <trd>&#x4F20;&#x5947; <pr>chu&#xE1;nq&#xED;</pr></trd>,
    <trd>&#x50B3;&#x5947; <pr>chu&#xE1;nq&#xED;</pr></trd>,
    <trd>&#x4F73;&#x8A71; <pr>ji&#x101;hu&#xE0;</pr></trd>,
    <trd>&#x4F73;&#x8BDD; <pr>ji&#x101;hu&#xE0;</pr></trd>,
    <trd>&#x56FE;&#x4F8B; <pr>t&#xFA;l&#xEC;</pr></trd>,
    <trd>&#x5716;&#x4F8B; <pr>t&#xFA;l&#xEC;</pr></trd>,
    <trd>&#x6587;&#x5B57;&#x56FE;&#x4F8B; <pr>w&#xE9;nz&#xEC;t&#xFA;l&#xEC;</pr></trd>,
    <trd>&#x6587;&#x5B57;&#x5716;&#x4F8B; <pr>w&#xE9;nz&#xEC;t&#xFA;l&#xEC;</pr></trd>,
    <trd>&#x6C11;&#x9593;&#x50B3;&#x8AAA; <pr>m&#xED;nji&#x101;nchu&#xE1;nshu&#x14D;</pr></trd>,
    <trd>&#x6C11;&#x95F4;&#x4F20;&#x8BF4; <pr>m&#xED;nji&#x101;nchu&#xE1;nshu&#x14D;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="legend.urba0o">
  <kap>urba <tld/>o</kap>
  <snc mrk="legend.urba0o.BELE">
    <dif>
      Rakonto aperanta en moderna, precipe urba medio, miksanta faktojn
      kun fantazia&jcirc;oj:
      <ekz>
        <ctl>ventumila morto</ctl> estas urba <tld/>o, kiu originis en
        Sudkoreio kaj nun disvasti&gcirc;as al aliaj aziaj landoj
        <fnt>
          <aut>Arono kaj Karlina</aut>,
          <vrk><url ref="http://radioverda.com/">
          Radio Verda</url></vrk>,
          <lok>rv151, 2009-09-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_a;&c_d;&c_z;&c_k;&c_a;&c_ja; <ind>&c_l;&c_je;&c_g;&c_je;&c_n;&c_d;&c_a;</ind></trd>,
    <trd>&c_g;&c_a;&c_r;&c_a;&c_d;&c_z;&c_k;&c_ib; <ind>&c_m;&c_ib;&c_t;</ind></trd>
  </trdgrp>
  <trd lng="fr">l&eacute;gende urbaine</trd>
  <trd lng="ja">&#x90FD;&#x5E02;&#x4F1D;&#x8AAC;  <pr>&#x3068;&#x3057;&#x3067;&#x3093;&#x305B;&#x3064;</pr></trd>
  <trdgrp lng="nl">
    <trd>stadssage</trd>,
    <trd>stadslegende</trd>
  </trdgrp>
  <trd lng="pl">legenda miejska</trd>
  <trd lng="ro">legend&abreve; urban&abreve;</trd>
</drv>
</art>
