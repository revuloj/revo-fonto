<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>frivol</rad>/a
</kap>
<drv mrk="frivol.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Malserioza, bagatela, vanta:
      <ekz><tld/>aj aferoj, pensoj;</ekz>
      <ekz><tld/>a konduto.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vent.0a">venta</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>nichtig</trd>,
     <trd>geringf&uuml;gig</trd></trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Inklina al malseriozaj, vantaj aferoj:
      <ekz><tld/>a virino.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="anim">vantanima</ref>,
      <ref cel="facil.0anima">facilanima</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>frivol</trd>,
     <trd>leichtsinnig</trd>,
     <trd>lose</trd>,
     <trd>locker</trd></trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>frivole</trd>,
    <trd>futile</trd>,
    <trd>insignifiant</trd>,
    <trd>superficiel</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_i;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_l;&c_je;&c_g;&c_k;&c_o;&c_m;&c_y;&c_s;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_t;&c_r;&c_je;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
