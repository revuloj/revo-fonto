<kap>
  <rad>ornat</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="ornat.0o">
  <kap><tld/>o</kap>
  <snc mrk="ornat.0o.TEKS">
    <uzo tip="fak">REL</uzo>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Rita vesto uzata por diservo:
      <ekz>
        li surmetis sian taoisman <tld/>on, kun broditaj
        blankaj drakoj dorse kaj anta&ubreve;e
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dalmat.0o">dalmatiko</ref>,
      <ref cel="surpli.0o.TEKS">surpliso</ref>,
      <ref cel="sutan.0o.TEKS">sutano</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_z;&c_a;</trd>
  <trd lng="ca">ornament <klr tip="amb">(lit&uacute;rgic)</klr></trd>
  <trdgrp lng="cs">
    <trd>me&scaron;n&#xED; roucho</trd>,
    <trd>orn&#xE1;t</trd>
  </trdgrp>
  <trd lng="de">Ornat</trd>
  <trdgrp lng="en">
    <trd>vestment</trd>,
    <trd>clerical robe</trd>
  </trdgrp>
  <trd lng="fr">aube<klr> (v&ecirc;tement)</klr></trd>
  <trdgrp lng="hu">
    <trd>orn&aacute;tus</trd>,
    <trd>d&iacute;sz&ouml;lt&ouml;zet</trd>
  </trdgrp>
  <trd lng="it">paramenti<klr> (rel)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x796D;&#x670D;  <pr>&#12373;&#12356;&#12405;&#x304F;</pr></trd>,
    <trd>&#x5F0F;&#x670D;  <pr>&#12375;&#x304D;&#12405;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ornaat</trd>,
    <trd>ambtsgewaad</trd>
  </trdgrp>
  <trd lng="pl">ornat</trd>
  <trdgrp lng="pt">
    <trd>paramento <klr tip="amb">(vest.)</klr></trd>,
    <trd>ornamento <klr tip="amb">(vest.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_i;&c_z;&c_a;</trd>
  <trd lng="sk">bohoslu&#x17E;obn&#xE9; r&#xFA;cho</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_i;&c_z;&c_a;</trd>,
    <trd>&c_f;&c_je;&#x43B;&#x43E;&#x43D;</trd>,
    <trd>&c_t;&#x43E;&c_gu;&c_a;</trd>,
    <trd>&#x43C;&c_a;&#x43D;&c_t;&c_ib;&#x44F; (&c_s;&c_u;&c_d;&c_d;&c_ib;&c_v;</trd>,
    <trd>&c_a;&c_d;&c_v;&#x43E;&#x43A;&c_a;&c_t;&c_ib;&c_v; &c_ib; &c_t;.&#x43F;.)</trd>
  </trdgrp>
</drv>
</art>
