<kap>
  <rad>ornat</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ornat.0o">
  <kap><tld/>o</kap>
  <snc mrk="ornat.0o.TEKS">
    <uzo tip="fak">REL</uzo>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Rita vesto uzata por diservo:
      <ekz>
        li surmetis sian taoisman <tld/>on, kun broditaj
        blankaj drakoj dorse kaj anta&ubreve;e
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dalmat.0o">dalmatiko</ref>,
      <ref cel="surpli.0o.TEKS">surpliso</ref>,
      <ref cel="sutan.0o.TEKS">sutano</ref>
    </refgrp>         
  </snc>
  <trd lng="be">&c_r;&c_y;&c_z;&c_a;</trd>
  <trd lng="ca">ornament (lit&uacute;rgic)</trd>
  <trdgrp lng="cs">
    <trd>me&#x161;n&#xED; roucho</trd>,
    <trd>orn&#xE1;t</trd>
  </trdgrp>
  <trd lng="de">Ornat</trd>
  <trdgrp lng="en">
    <trd>vestment</trd>,
    <trd>clerical robe</trd>
  </trdgrp>
  <trd lng="fr">aube<klr> (v&ecirc;tement)</klr></trd>
  <trdgrp lng="hu">
    <trd>orn&aacute;tus</trd>,
    <trd>d&iacute;sz&ouml;lt&ouml;zet</trd>
  </trdgrp>
  <trd lng="it">paramenti<klr> (rel)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x796D;&#x670D;  <pr>&#x3055;&#x3044;&#x3075;&#x304F;</pr></trd>,
    <trd>&#x5F0F;&#x670D;  <pr>&#x3057;&#x304D;&#x3075;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ornaat</trd>,
    <trd>ambtsgewaad</trd>
  </trdgrp>
  <trd lng="pl">ornat</trd>
  <trdgrp lng="pt">
    <trd>paramento <klr tip="amb">(vest.)</klr></trd>,
    <trd>ornamento <klr tip="amb">(vest.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_i;&c_z;&c_a;</trd>
  <trd lng="sk">bohoslu&#x17E;obn&#xE9; r&#xFA;cho</trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x438;&#x437;&#x430;</trd>,
    <trd>&#x444;&#x435;&#x43B;&#x43E;&#x43D;</trd>,
    <trd>&#x442;&#x43E;&#x491;&#x430;</trd>,
    <trd>&#x43C;&#x430;&#x43D;&#x442;&#x456;&#x44F; (&#x441;&#x443;&#x434;&#x434;&#x456;&#x432;</trd>,
    <trd>&#x430;&#x434;&#x432;&#x43E;&#x43A;&#x430;&#x442;&#x456;&#x432; &#x456; &#x442;.&#x43F;.)</trd>
  </trdgrp>
</drv>
</art>
