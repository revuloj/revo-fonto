<kap>
<rad>emfizem</rad>/o
</kap>
<drv mrk="emfize.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">Malsano</ref> ka&ubreve;zata de la dilati&gcirc;o de histo
      a&ubreve; organo
      pro gasa dispremo:
      <ekz>ha&ubreve;ta, pulma <tld/>o.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_f;&c_ib;&c_z;&c_e;&c_m;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>emfyz&#xE9;m</trd>,
    <trd>rozedma</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Emphysem<klr> [fak]</klr></trd>,
     <trd>Luftaufbl&auml;hung (in der Lunge)</trd>,
     <trd>Gasaufbl&auml;hung (in der Lunge)</trd>
  </trdgrp>
  <trd lng="en">emphysema</trd>
  <trd lng="es">enfisema</trd>
  <trd lng="fr">emphys&egrave;me</trd>
  <trd lng="hu">emfiz&eacute;ma</trd>
  <trd lng="id">emfisema</trd>
  <trd lng="ja">&#x6C17;&#x816B;  <pr>&#x304D;&#x3057;&#x3085;</pr></trd>
  <trd lng="pl">rozedma</trd>
  <trd lng="pt">enfisema</trd>
  <trd lng="ru">&c_e;&c_m;&c_f;&c_i;&c_z;&c_je;&c_m;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>napuchnutie <klr>(plynov&#xE9;)</klr></trd>,
    <trd>zdurenina</trd>
  </trdgrp>
  <trd lng="uk">&#x435;&#x43C;&#x444;&#x456;&#x437;&#x435;&#x43C;&#x430;</trd>
</drv>
</art>
