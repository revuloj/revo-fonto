<vortaro>
<art mrk="$Id$">
<kap>
<rad>emfizem</rad>/o
</kap>
<drv mrk="emfize.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malsano ka&ubreve;zata de la dilati&gcirc;o de histo a&ubreve; organo
      pro gasa dispremo:
      <ekz>ha&ubreve;ta, pulma <tld/>o.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_m;&c_f;&c_ib;&c_z;&c_e;&c_m;&c_a;</trd>
  <trdgrp lng="de">
     <trd>Emphysem<klr> [fak]</klr></trd>,
     <trd>Luftaufbl&auml;hung (in der Lunge)</trd>,
     <trd>Gasaufbl&auml;hung (in der Lunge)</trd>
  </trdgrp>
  <trd lng="en">emphysema</trd>
  <trd lng="es">enfisema</trd>
  <trd lng="fr">emphys&egrave;me</trd>
  <trd lng="hu">emfiz&eacute;ma</trd>
  <trd lng="pl">rozedma</trd>
  <trd lng="pt">enfisema</trd>
  <trd lng="ru">&c_e;&c_m;&c_f;&c_i;&c_z;&c_je;&c_m;&c_a;</trd>
</drv>
</art>
