<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ru&gcirc;</rad>/a
</kap>
<drv mrk="rugx.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Samkolora kiel sango:
      <ekz>
        <tld/>a kiel tomato, koralo, rubeno, papavo,
        grenato, cinabro, minio;
      </ekz>
      <ekz>
        flav<tld/>aj haroj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skarla">skarlata</ref>,
      <ref cel="punc.0a">punca</ref>,
      <ref cel="karmin">karmina</ref>,
      <ref cel="karmez">karmezina</ref>,
      <ref cel="purpur">purpura</ref>,
      <ref cel="roz.0a">roza</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;erven&yacute;</trd>,
    <trd>rud&yacute;</trd>
  </trdgrp>
  <trd lng="de">rot</trd>
  <trd lng="en">red</trd>
  <trd lng="es">rojo</trd>
  <trd lng="fr">rouge<klr> (adj.)</klr></trd>
  <trd lng="he">&alef;&dalet;&vav;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>piros</trd>,
    <trd>v&ouml;r&ouml;s</trd>
  </trdgrp>
  <trd lng="nl">rood</trd>
  <trd lng="pl">czerwony</trd>
  <trd lng="pt">vermelho</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">r&ouml;d</trd>
  <trdgrp lng="tr">
    <trd>k&inodot;rm&inodot;z&inodot;</trd>,
    <trd>k&inodot;z&inodot;l</trd>
  </trdgrp>
  <trd lng="tp">loje</trd>
  <trd lng="vo">ledik</trd>
</drv>

<drv mrk="rugx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="R"/>a <ref tip="super" cel="kolor.0o.propre">koloro</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_y;&c_r;&c_v;&c_a;&c_n;&c_mol;</trd>
  <trd lng="de">Rot</trd>
  <trd lng="en">red</trd>
  <trdgrp lng="fr">
    <trd>rouge<klr> (subst..)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&alef;&dalet;&vav;&fmem;</trd>,
    <trd>&alef;&vav;&dalet;&fmem;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>piros sz&iacute;n</trd>,
    <trd>v&ouml;r&ouml;s sz&iacute;n</trd>
  </trdgrp>
  <trd lng="nl">rood<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j; &c_c;&c_v;&c_je;&c_t;</trd>
</drv>

<drv mrk="rugx.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="rugx.0igxi.ajne">
    <dif>
      I&gcirc;i <tld/>a.
    </dif>
    <trd lng="de">sich rot f&auml;rben</trd>
    <trd lng="en">redden</trd>
    <trd lng="he">&lamed;<ind>&he;&alef;&dalet;&yod;&fmem;</ind></trd>
    <trdgrp lng="hu">
      <trd>kipirosodik</trd>,
      <trd>kipirul</trd>
    </trdgrp>
    <trd lng="nl">rood worden</trd>
  </snc>
  <snc mrk="rugx.0igxi.vizagxe">
    <dif>
      Viza&gcirc;e ek<tld/>i pro emocia sangalfluo, ekzemple
      pro honto, pudoro, kolero:
      <ekz>
        <tld/>i&gcirc;i pro honto, de honto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">err&ouml;ten</trd>
    <trd lng="en">blush</trd>
    <trd lng="he">&lamed;<ind>&he;&samekh;&mem;&yod;&qof;</ind></trd>
    <trdgrp lng="nl">
      <trd>blozen</trd>,
      <trd>kleuren</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_y;&c_r;&c_v;&c_a;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_ch;&c_y;&c_r;&c_v;&c_a;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">rot werden</trd>
  <trdgrp lng="fr">
    <trd>rougir<klr> (intr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elpirul</trd>,
    <trd>elv&ouml;r&ouml;s&ouml;dik</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_a;&c_s;&c_n;&c_je;&c_t;&c_mol;</trd>
</drv>

<drv mrk="rugx.trans0a">
  <kap>trans<tld/>a</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Kvalifikas nevideblajn malrapide vibrantajn radiojn deviitajn de la
      prismo trans la ru&gcirc;ajn.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_f;&c_r;&c_a;&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;</trd>
  <trd lng="de">infrarot</trd>
  <trd lng="en">infrared</trd>
  <trd lng="es">infrarrojo</trd>
  <trd lng="fr">infrarouge</trd>
  <trd lng="he">&alef;&yod;&nun;&pe;&resh;&alef;-&alef;&dalet;&vav;&fmem;</trd>
  <trd lng="hu">infrav&ouml;r&ouml;s</trd>
  <trd lng="nl">infrarood</trd>
  <trd lng="pl">podczerwony</trd>
  <trd lng="ru">&c_i;&c_n;&c_f;&c_r;&c_a;&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">infrar&ouml;d</trd>
  <trd lng="tr">k&inodot;rm&inodot;z&inodot;&ouml;te</trd>
</drv>

<drv mrk="rugx.R0aMaro">
  <kap><tld lit="R"/>a Maro</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="mar.0o">Maro</ref> en <ref tip="malprt"
      cel="hind.H0aOceano">Hinda Oceano</ref>
      inter nord-orienta <ref tip="vid" cel="afrik.A0o">Afriko</ref> kaj
      <ref tip="vid" cel="arab.A0aDuoninsulo">Araba
      Duoninsulo</ref>.
    </dif>
  </snc>
  <trd lng="de">Rotes Meer</trd>
  <trd lng="en">Red Sea</trd>
  <trd lng="he">&yod;&fmem; &samekh;&vav;&fpe;</trd>
  <trd lng="hu">V&ouml;r&ouml;s-tenger</trd>
  <trd lng="nl">Rode Zee</trd>
  <trd lng="ru">&c_K;&c_r;&c_a;&c_s;&c_n;&c_o;&c_je; &c_M;&c_o;&c_r;&c_je;</trd>
</drv>
</art>
