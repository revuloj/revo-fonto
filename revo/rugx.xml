<drv mrk="rugx.0igxema">
  <kap><tld/>i&gcirc;ema</kap>
  <snc mrk="rugx.0igxema.PSI">
    <dif>
      Kiu facile ru&gcirc;i&gcirc;as:
      <ekz>
        li <tld/>i&gcirc;is de plezuro, &ccirc;ar li estas tre <tld/>i&gcirc;ema
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ofte rimarkas vin, li diras, &ccirc;ar vi estas tiel <tld/>i&gcirc;ema
        <fnt>
          <aut>E. Turner, trad. R. McGrath</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0144/0001.php">Sep Junaj
          A&ubreve;stralianoj</url></vrk>
          <lok>1994</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_k;&c_ib; &c_l;&c_jo;&c_g;&c_k;&c_a; <ind>&c_ch;&c_y;&c_r;&c_v;&c_a;&c_n;&c_je;&c_je;</ind></trd>
  <trd lng="fr">rougissant</trd>
</drv>

