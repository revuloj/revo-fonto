<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
 <ofc>*</ofc>
 <rad>ru&gcirc;</rad>/a
</kap>
<drv mrk="rugx.0a">
 <kap><ofc>*</ofc><tld/>a</kap>
 <snc>
  <dif>
   Samkolora kiel sango:
   <ekz>
    <tld/>a kiel tomato, koralo, rubeno, papavo,
    grenato, cinabro, minio;
   </ekz>
   <ekz>flav<tld/>aj haroj;</ekz>
   <ekz><tld/>i&gcirc;i pro honto, de honto<fnt>Z</fnt>
   </ekz>
  </dif>
  <refgrp tip="sin">
   <ref cel="skarla">skarlata</ref>,
   <ref cel="punc.0a">punca</ref>,
   <ref cel="karmin">karmina</ref>,
   <ref cel="karmez">karmezina</ref>,
   <ref cel="purpur">purpura</ref>,
   <ref cel="roz.0a">roza</ref>.
  </refgrp>
 </snc>
 <trdgrp lng="cs">
  <trd>&ccaron;erven&yacute;</trd>,
  <trd>rud&yacute;</trd>
 </trdgrp>
 <trd lng="en">red</trd>
 <trd lng="es">rojo</trd>
 <trd lng="fr">rouge</trd>
 <trd lng="nl">rood</trd>
 <trd lng="de">rot</trd>
 <trdgrp lng="tr">
  <trd>k&inodot;rm&inodot;z&inodot;</trd>,
  <trd>k&inodot;z&inodot;l</trd>
 </trdgrp>
</drv>
<drv mrk="rugx.trans0a">
 <kap>trans<tld/>a</kap>
 <uzo tip="fak">FIZ</uzo>
 <snc>
  <dif>
   Kvalifikas nevideblajn malrapide vibrantajn radiojn deviitajn de la prismo
   trans la ru&gcirc;ajn.  
  </dif>
 </snc>
 <trd lng="es">infrarrojo</trd>
 <trd lng="fr">infrarouge</trd>
 <trd lng="de">infrarot</trd>
 <trd lng="tr">&inodot;rm&inodot;z&inodot;&ouml;te</trd>
</drv>
</art>
