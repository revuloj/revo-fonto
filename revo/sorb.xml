<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sorb.xml,v 1.27 2022/05/25 06:37:06 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sorb</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sorb.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sorb.0i.KOMUNE">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Enspirante <ref tip="super" cel="tir.al0i.al_si">altiri</ref> flua&jcirc;on iom post iom en sian bu&scirc;on:
      <ekz>
        Kiam &scirc;ia bu&scirc;' &dash; por brili &dash; <tld lit="S"/>as majmatenan roson
        <fnt>
          <bib>LSB</bib>
          <lok>Vila&gcirc;anino</lok>
        </fnt>;
      </ekz>
      <ekz>
        dol&ccirc;a estis la kiso, kiun mi <tld/>is iam desur la lipoj
        <fnt>
          <bib>VaK</bib>
          <lok>Sepa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bestoj <klr>[&#8230;]</klr> levante la grandegajn kapojn avide <tld/>is per la
        nazoj la homan &scirc;vitodoron
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXXVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i ostrojn
        <fnt>
          <bib>KrB</bib>
          <lok>Dubo gardas kontra&ubreve; risko</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni naski&gcirc;as kun certaj atavismaj trajtoj a&ubreve; <tld/>as ilin kune kun la patrina
        lakto
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="https://www.monato.be/2005/008548.php"
              >Lingva atavismo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sucx">ensu&ccirc;i</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>saugen</trd>,
      <trd>einsaugen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>chupar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&iacute;v</trd>,
      <trd>besz&iacute;v</trd>
    </trdgrp>
    <trd lng="nl">opzuigen</trd>
    <trdgrp lng="pl">
    <trd>wsysa&cacute;</trd>,
    <trd>wci&aogonek;ga&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="sorb.0i.FIZ">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Iom post iom en<ref tip="super" cel="pren.0i.akcepti">preni</ref> en si flua&jcirc;on:
      <ekz>
        la akvujo <klr>[&#8230;]</klr> dum la levi&gcirc;o de la rivero &gcirc;i <tld/>is la superfluon de
        la akvo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        Lastan sangon de libera homo // &Ccirc;e <nom>Philippi</nom> <tld/>is nun la tero
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        liaj bluaj okuloj rigardis la sunan brilon kaj <tld/>is la radiojn de la
        blindiganta lumo
        <fnt>
          <bib>IKr</bib>
          <lok>En Kvieta Vespero</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asimil.0i">asimili</ref>,
      <ref cel="cxerp.0i">&ccirc;erpi</ref>,
      <ref cel="visx.for0i">forvi&scirc;i</ref>,
      <ref cel="spong.0i">spongi</ref>,
      <ref cel="osmoz.0o">osmozo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>aufsaugen</trd>,
      <trd>l&ouml;schen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>absorber</trd>,
      <trd>embeber</trd>
    </trdgrp>
    <trd lng="hu">felsz&iacute;v</trd>    
    <trdgrp lng="nl">
      <trd>opzuigen</trd>,
      <trd>absorberen</trd>
    </trdgrp>
    <trd lng="nl">opzuigen</trd>
    <trd lng="pl">wch&lstroke;ania&cacute;</trd>
  </snc>
  <snc mrk="sorb.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="propr.al0igi">Alproprigi</ref> al sia spirito, 
      enspiritigi al si:
      <ekz>
        Bele sonas // La agoj de l&#8217; prapatroj, kiam ilin, // <klr>[&#8230;]</klr> Junulo
        <tld/>as kun la ton&#8217; de harpo
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia rigardo glitis de &scirc;ia viza&gcirc;o sur la kolon kaj sur la malkovritajn brakojn,
        karesis &scirc;ian belegan figuron, &gcirc;uis &scirc;in, posedprenis kaj <tld/>is
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li bezonis promeni en la urbo, <tld/>i ties etoson
        <fnt>
          <bib>ChB</bib>
          <lok>4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homoj sin &jcirc;etu sur la revuon kaj <tld/>u ties mesa&gcirc;on avide
        <fnt>
          <bib>ChV</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Morja</nom> provis <tld/>i la du &scirc;okojn kiujn ka&ubreve;zis, unue, la neordinara beleco de
        tiu blondulino, kaj due &scirc;ia ne pli ordinara konfeso
        <fnt>
          <bib>ChR</bib>
          <lok>Tri estas tro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>aufnehmen</trd>,
      <trd>einsenken</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>incorporar</trd>,
      <trd>embeber</trd>
    </trdgrp>
    <trd lng="hu">mag&aacute;ba <ind>sz&iacute;v</ind></trd>    
    <trd lng="nl">in zich <ind>opnemen</ind></trd>
    <trdgrp lng="pl">
    <trd>wch&lstroke;ania&cacute;</trd>,
    <trd>nasi&aogonek;ka&cacute;</trd>,
    <trd>przesi&aogonek;ka&cacute;</trd>
    </trdgrp>
  </snc>
   <refgrp tip="vid">
     Ne konfuzu kun:
     <ref cel="absorb.0i">absorbi</ref>.
   </refgrp>
   <trdgrp lng="be">
     <trd>&c_u;&c_p;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_a;&c_g;&c_l;&c_y;&c_n;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_u;&c_s;&c_m;&c_o;&c_k;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_u;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>
   </trdgrp>
  <trdgrp lng="cs">
    <trd>nas&aacute;t</trd>,
    <trd>p&iacute;t</trd>,
    <trd>vst&rcaron;ebat</trd>,
    <trd>vs&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>absorb</trd>,
    <trd>sip</trd>
  </trdgrp>
  <trd lng="fr">absorber</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_p;&c_i;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_s;&c_a;&c_s;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_l;&c_o;&c_shch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_v;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nasa&tcaron;</trd>,
    <trd>pi&tcaron;</trd>,
    <trd>vstreb&aacute;va&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&c_s;&c_m;&c_o;&c_k;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_l;&c_i;&c_n;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sorb.0a">
  <kap><tld/>a</kap>
  <snc mrk="sorb.0a.KOMUNE">
    <dif>
      Kiu <tld/>as, enlogas en sin:
      <ekz>
        la <tld/>a papero <tld/>as la inkon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>absob&ccaron;n&iacute;</trd>,
    <trd>pohlcuj&iacute;c&iacute;</trd>,
    <trd>sorbuj&iacute;c&iacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>absorbent</trd>,
    <trd>absorptive</trd>,
    <trd>of sipping</trd>
  </trdgrp>
  <trd lng="es">absorbente</trd>
  <trd lng="fr">absorbant</trd>
  <trdgrp lng="sk">
    <trd>absorp&ccaron;n&yacute;</trd>,
    <trd>pohlcuj&uacute;ci</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_g;&c_l;&c_i;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_m;&c_o;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="sorb.0etadi">
  <kap><tld/>etadi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sorb.0etadi.KOMUNE">
    <dif>
      Tute malrapide <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>slowly absorb</trd>,
    <trd>slowly sip</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>empapar</ind>se de</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>imbiber</ind> de</trd>
  <trdgrp lng="hu">
    <trd>kortyol</trd>,
    <trd>sz&ouml;rp&ouml;l</trd>,
    <trd>h&ouml;rp&ouml;l</trd>,
    <trd>iszogat</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_t;&c_ja;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr
  tip="amb">(&c_m;&c_je;&c_d;&c_l;&c_je;&c_n;&c_n;&c_o; &c_p;&c_i;&c_t;&c_mol;)</klr></trd>
</drv>

<drv mrk="sorb.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sorb.0igi.KOMUNE">
    <dif>
      Ka&ubreve;zi <tld/>on;
      <ref tip="vid" cel="impregn.0i.KOMUNE">impregni</ref>:
      <ekz>
        spongo <tld/>igita de akvo.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">cause to absorb</trd>
  <trdgrp lng="es">
    <trd>hacer <ind>absorber</ind></trd>,
    <trd>impregnar</trd>,
    <trd>empapar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire <ind>absorber</ind></trd>,
    <trd>impr&eacute;gner</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_v;&c_i;&c_p;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_l;&c_ib;&c_k;&c_i; &c_ib; &c_t;.&c_p;.)</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_o;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_m;&c_o;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_m;&c_o;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_i;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="sorb.0igxi">
  <kap><tld/>i&gcirc;i<fnt>Z</fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sorb.0igxi.objekto">
    <dif>
      Esti tute penetrita de iu flua&jcirc;o.
    </dif>
    <trdgrp lng="hu">
      <trd>telesz&iacute;vja mag&aacute;t</trd>,
      <trd>tel&iacute;t&odblac;dik</trd>
    </trdgrp>
    <trd lng="pl">nasi&aogonek;kn&aogonek;&cacute;</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_i;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    <klr>(&c_k;&c_a;&c_k;&c_o;&c_j;-&c_l;.
    &c_zh;&c_i;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;&c_ju;)</klr></trd>
  </snc>
  <snc mrk="sorb.0igxi.fluajxo">
    <uzo tip="klr">(pri flua&jcirc;o)</uzo>
    <dif>
      Esti el&ccirc;erpita de io.
    </dif>
    <trdgrp lng="hu">
      <trd>feliv&oacute;dik</trd>,
      <trd>felsz&iacute;v&oacute;dik</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_p;&c_i;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr>(&c_o;
    &c_zh;&c_i;&c_d;&c_k;&c_o;&c_s;&c_t;&c_i;)</klr></trd>
  </snc>
  <trdgrp lng="en">
    <trd>get absorbed</trd>,
    <trd>get soaked</trd>,
    <trd>get sipped</trd>,
    <trd>get swallowed</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>impregnarse</trd>,
    <trd>empaparse</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>impr&eacute;gner</ind></trd>
  <trd lng="nl">doortrokken worden</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_s;&c_m;&c_o;&c_k;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_l;&c_i;&c_n;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="sorb.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sorb.en0i.KOMUNE">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malaperigi en si:
      <ekz>
        Esperanto devas konstante en<tld/>i novajn sukojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiujn ideojn li en<tld/>is en la patra hejmo;
      </ekz>
      <ekz>
        lia impresema intelekto kaj varma koro en<tld/>adis &ccirc;iujn
        gravajn problemojn de l' aktuala tempo;
      </ekz>
      <ekz>
        li ne povis fari, ke la neekzercitaj organoj de komprenado
        en<tld/>u en sin tion, kion oni en&scirc;utas en ilin<fnt>Z</fnt>;
      </ekz>
      <ekz>
        post longa luktado la trusto sukcesis en<tld/>i &ccirc;i tiujn
        potencajn kompaniojn;
      </ekz>
      <ekz>
        la &ccirc;eestantoj disi&gcirc;is kaj balda&ubreve; en<tld/>i&gcirc;is en la amaso da
        veturiloj;
      </ekz>
      <ekz>
        tiu penso tute okupis la maljunulon kaj en<tld/>is &ccirc;iujn aliajn
        dezirojn kaj esperojn<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_c;&c_ja;&c_g;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_mol; <klr>(&c_u;&c_n;&c_u;&c_t;&c_r;, &c_u;
    &c_s;&c_ja;&c_b;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nas&aacute;t</trd>,
    <trd>vst&rcaron;ebat</trd>,
    <trd>vs&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eingliedern</trd>,
    <trd>einverleiben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>absorb</trd>,
    <trd>suck in</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aspirar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>aspirer</trd>,
    <trd>incorporer</trd>
  </trdgrp>
  <trd lng="hu">elnyel</trd>
  <trd lng="nl">zich <ind>verdiepen</ind></trd>
  <trdgrp lng="sk">
    <trd>vpi&tcaron; sa</trd>,
    <trd>vsiaknu&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_g;&c_l;&c_i;&c_n;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i; &c_v; &c_s;&c_je;&c_b;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="sorb.tra0igxi">
  <kap>tra<tld/>i&gcirc;i, <var><kap>el<tld/>i&gcirc;i</kap></var>, 
  <var><kap>mal<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sorb.tra0igxi.KOMUNE">
    <dif>
      Tre malrapide elguteti el ia materio,
      <ref tip="vid" cel="sxvit.0igxi.KOMUNE">&scirc;viti&gcirc;i</ref>:
      <ekz>
        la akvo tra<tld/>i&gcirc;adis el la alta musko<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_c;&c_ja;&c_k;&c_a;&c_c;&c_mol;
    <klr>(&c_p;&c_a;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_a;,
    &c_p;&c_a;&c_t;&c_r;&c_o;&c_h;&c_u;)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>rezumar</trd>
  </trdgrp>
  <trd lng="fr">suinter</trd>
  <trdgrp lng="hu">
    <trd>kicsepeg</trd>,
    <trd>kisziv&aacute;rog</trd>
  </trdgrp>
  <trd lng="nl">doorsijpelen</trd>
  <trdgrp lng="pl">
  <trd>przesi&aogonek;ka&cacute;</trd>,
  <trd>s&aogonek;czy&cacute; si&eogonek;</trd>,
  <trd>wys&aogonek;cza&cacute; si&eogonek;</trd>
  </trdgrp>
</drv>
</art>
