    <trd>empaparse</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>impr&eacute;gner</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x3057;&#x307F;&#x8FBC;&#x3080;  <pr>&#x3057;&#x307F;&#x3053;&#x3080;</pr></trd>,
    <trd>&#x6F64;&#x3046;  <pr>&#x3046;&#x308B;&#x304A;&#x3046;</pr></trd>,
    <trd>&#x6D78;&#x308B;  <pr>&#x3072;&#x305F;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">doortrokken worden</trd>
