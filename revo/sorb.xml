          <bib>KrM</bib>
          <lok>20. Kontra&ubreve; &Ccirc;iu Malfacilo Ekzistas Konsilo.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>absob&ccaron;n&iacute;</trd>,
    <trd>pohlcuj&iacute;c&iacute;</trd>,
    <trd>sorbuj&iacute;c&iacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>absorbent</trd>,
    <trd>absorptive</trd>,
    <trd>of sipping</trd>
  </trdgrp>
  <trd lng="es">absorbente</trd>
