<art mrk="$Id: cerume.xml,v 1.19 2021/03/05 21:59:32 revo Exp $">
<kap>
  <rad>cerumen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="cerume.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="cerume.0o.ANA">
    <dif>
      Flava substanco sekreciata en la <ref tip="vid" cel="orel.0o.ANA">orelo</ref>:
      <ekz>
        li ku&scirc;i&gcirc;is en la hamako, eltiris la <tld/>on el siaj oreloj
        per
        tran&ccirc;ileto kaj endormi&gcirc;is
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_r;&c_k;&c_a; <klr>(&c_v;&c_u;&c_sh;&c_n;&c_a;&c_ja;)</klr></trd>
  <trdgrp lng="cs">
    <trd>cerumen</trd>,
    <trd>mazov&#xE1; z&#xE1;tka</trd>,
    <trd>nahromad&#x11B;n&#xED; u&#x161;n&#xED;ho mazu</trd>
  </trdgrp>
  <trd lng="de">Ohrenschmalz</trd>
  <trd lng="el">&kappa;&ypsilon;&psi;&epsilon;&lambda;&jota_ton;&delta;&alfa; (&tau;&omikron;&ypsilon; &omega;&tau;&omikron_ton;&sigma_fina;)</trd>
  <trd lng="en">ear wax</trd>
  <trdgrp lng="es">
    <trd>cerilla</trd>,
    <trd>cerumen</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_j;&a_r;&a_m; &f_g;&a_w;&a_sh;</trd>,
    <trd>&a_s;&a_r;&a_w;&a_m;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">c&eacute;rumen</trd>
  <trd lng="he">&#x5E9;&#x5E2;&#x5D5;&#x5D5;&#x5EA; &#x5D0;&#x5D5;&#x5D6;&#x5DF;</trd>
  <trd lng="hu">f&uuml;lzs&iacute;r</trd>
  <trd lng="it">cerume</trd>
  <trd lng="nl">oorsmeer</trd>
  <trd lng="pl">woskowina uszna</trd>
  <trd lng="pt">cerume</trd>
  <trd lng="ru">&c_u;&c_sh;&c_n;&c_a;&c_ja; &c_s;&c_je;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>cerumen</trd>,
    <trd>nahromadenie u&#x161;n&#xE9;ho mazu</trd>
  </trdgrp>
</drv>
</art>
