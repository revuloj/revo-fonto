<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stenotip</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stenot.0o">
  <kap><tld/>o</kap>
  <snc mrk="stenot.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <ref tip="dif" cel="stenotajp.0ilo.TIP">stenotajpilo</ref>
  </snc>
  <trd lng="ca">estenot&iacute;pia<klr> (m&agrave;quina)</klr></trd>
  <trd lng="de">Kurzschriftmaschine</trd>
  <trd lng="fr">st&eacute;notype</trd>
  <trd lng="hu">gyors&iacute;r&oacute;g&eacute;p</trd>  
  <trd lng="nl">stenomachine</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_t;&c_i;&c_p;</trd>
</drv>

<drv mrk="stenot.0i">
  <kap><tld/>i</kap>
  <snc mrk="stenot.0i.TIP">
    <ref tip="dif" cel="stenotajp.0i.TIP">stenotajpi</ref>
  </snc>
  <trd lng="ca">escriure amb <ind>estenot&iacute;pia</ind></trd>
  <trd lng="fr">st&eacute;notyper</trd>
  <trdgrp lng="hu">
    <trd>gyorsg&eacute;p&iacute;r</trd>,
    <trd>gyors&iacute;r&oacute;g&eacute;ppel &iacute;r</trd>
  </trdgrp>
</drv>

<drv mrk="stenot.0isto">
  <kap><tld/>isto, <var><kap><tld/>istino</kap></var></kap>
  <snc mrk="stenot.0isto.TIP">
    <uzo tip="fak">TIP</uzo>
    <ref tip="dif" cel="stenotajp.0isto.TIP">stenotajpisto</ref>
    <ekz>
      la malnova ma&scirc;ino  staras sur sia tableto
      <klr>... </klr> <tld/>istoj rigardas &gcirc;in kompate
      <fnt>
        <aut>W. Heller</aut>,
        <vrk>Lidia: la vivo de Lidia Zamenhof</vrk>,
        <lok>1985</lok>
      </fnt>
    </ekz>
  </snc>
  <trd lng="ca">estenotipista</trd>
  <trd lng="cs">stenotypistka</trd>
  <trd lng="fr">st&eacute;notypiste</trd>
  <trdgrp lng="hu">
    <trd>gyorsg&eacute;p&iacute;r&oacute;</trd>,
    <trd>g&eacute;pi gyors&iacute;r&oacute;</trd>
  </trdgrp>
  <trd lng="nl">stenotypist</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_t;&c_i;&c_p;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">stenotypistka</trd>
</drv>
</art>
