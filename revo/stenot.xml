<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stenot.xml,v 1.20 2025/04/06 06:47:44 revo Exp $">
<kap>
  <rad>stenotip</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stenot.0o">
  <kap><tld/>o</kap>
  <snc mrk="stenot.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="stenotajp.0ilo.TIP">Stenotajpilo</ref>:
      <ekz>
        <tld/>ado estas steno-metodo farita per ma&scirc;ino nomita <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19341">Stenotipado</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>rapidtajpilo</ke>.
  </rim>

  <trd lng="be">&c_s;&c_t;&c_e;&c_n;&c_a;&c_t;&c_y;&c_p;</trd>
  <trd lng="ca">estenot&iacute;pia<klr> (m&agrave;quina)</klr></trd>
  <trd lng="de">Kurzschriftmaschine</trd>
  <trd lng="en">stenotype</trd>
  <trd lng="fr">st&eacute;notype</trd>
  <trd lng="hu">gyors&iacute;r&oacute;g&eacute;p</trd>
  <trdgrp lng="ja">
    <trd>&#36895;&#35352;&#12479;&#12452;&#12503;  <pr>&#12381;&#12387;&#12365;&#12383;&#12356;&#12407;</pr></trd>,
    <trd>&#12473;&#12486;&#12494;&#12479;&#12452;&#12503;</trd>
  </trdgrp>
  <trd lng="nl">stenomachine</trd>
  <trdgrp lng="pl">
    <trd>stenotyp</trd>,
    <trd>maszyna  <ind>stenograficzna</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_t;&c_i;&c_p;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_je;&#1085;&#1086;&c_t;&c_i;&#1087;</trd>,
    <trd>&c_s;&c_t;&c_je;&#1085;&#1086;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ch;&#1085;&c_a; &#1084;&c_a;&c_sh;&c_i;&#1085;&#1082;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="stenot.0i">
  <kap><tld/>i</kap>
  <snc mrk="stenot.0i.TIP">
    <uzo tip="fak">TIP</uzo>
    <ref tip="dif" cel="stenotajp.0i.TIP">stenotajpi</ref>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_n;&c_a;&c_t;&c_y;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">escriure amb <ind>estenot&iacute;pia</ind></trd>
  <trd lng="en">stenotype</trd>
  <trd lng="fr">st&eacute;notyper</trd>
  <trdgrp lng="hu">
    <trd>gyorsg&eacute;p&iacute;r</trd>,
    <trd>gyors&iacute;r&oacute;g&eacute;ppel &iacute;r</trd>
  </trdgrp>
  <trd lng="ja">&#36895;&#35352;&#12479;&#12452;&#12503;&#12391;&#35352;&#37682;&#12377;&#12427;
  <pr>&#12381;&#12387;&#12365;&#12383;&#12356;&#12407;&#12391;&#12365;&#12429;&#12367;&#12377;&#12427;</pr></trd>
  <trd lng="pl">pisa&cacute; na maszynie  <ind>stenograficznej</ind></trd>
  <trd lng="uk">&c_z;&c_a;&#1087;&c_i;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i; &c_s;&c_t;&c_je;&#1085;&#1086;&c_t;&c_i;&#1087;&#1086;&#1084;</trd>
</drv>

<drv mrk="stenot.0isto">
  <kap><tld/>isto, <var><kap><tld/>istino</kap></var></kap>
  <snc mrk="stenot.0isto.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      <ref tip="dif" cel="stenotajp.0isto.TIP">Stenotajpisto</ref>:
      <ekz>
        la malnova ma&scirc;ino  staras sur sia tableto
        <klr>[&#8230;]</klr> <tld/>istoj rigardas &gcirc;in kompate
        <fnt>
          <aut>W. Heller</aut>,
          <vrk>Lidia: la vivo de Lidia Zamenhof</vrk>,
          <lok>1985</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_e;&c_n;&c_a;&c_t;&c_y;&c_p;&c_ib;&c_s;&c_t;</trd>
  <trd lng="ca">estenotipista</trd>
  <trd lng="cs">stenotypistka</trd>
  <trd lng="de">Stenotypist</trd>
  <trd lng="en">stenotypist</trd>
  <trd lng="fr">st&eacute;notypiste</trd>
  <trdgrp lng="hu">
    <trd>gyorsg&eacute;p&iacute;r&oacute;</trd>,
    <trd>g&eacute;pi gyors&iacute;r&oacute;</trd>
  </trdgrp>
  <trd lng="ja">&#36895;&#35352;&#12479;&#12452;&#12500;&#12473;&#12488;
  <pr>&#12381;&#12387;&#12365;&#12479;&#12452;&#12500;&#12473;&#12488;</pr></trd>
  <trd lng="nl">stenotypist</trd>
  <trdgrp lng="pl">
    <trd>stenotypista</trd>,
    <trd>stenotypistka</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_t;&c_i;&c_p;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">stenotypistka</trd>
  <trd lng="uk">&c_s;&c_t;&c_je;&#1085;&#1086;&c_t;&c_i;&#1087;&c_ib;&c_s;&c_t;&#1082;&c_a;</trd>
</drv>
</art>
