<art mrk="$Id: analgezik.xml,v 1.10 2023/10/28 08:33:06 revo Exp $">
<kap>
  <rad>analgezik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="analgezik.0o">
  <kap><tld/>o</kap>
  <snc mrk="analgezik.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="dolor.kontraux0ilo.MED">Kontra&ubreve;dolorilo</ref>:
      <ekz>
        la konstanta prenado de <tld/>o foje ka&ubreve;zis al &scirc;i drogan
        toksi&gcirc;on
        <fnt>
          <aut>Zhang Qingzhen</aut>,
          <vrk><url ref="http://www.espero.com.cn/06s/06s-2/socio3-10a.htm">
          &Scirc;i kortu&scirc;as gejunulojn</url></vrk>,
          <lok>[2004]</lok>
        </fnt>;
      </ekz>
      <ekz>
        dekstrometorfano estas opieca <tld/>o uzata kiel kontra&ubreve;tusilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=330175">Dekstrometorfano</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x93AE;&#x75DB;&#x85E5; <pr>zh&#xE8;nt&#xF2;ngy&#xE0;o</pr></trd>,
    <trd>&#x9547;&#x75DB;&#x836F; <pr>zh&#xE8;nt&#xF2;ngy&#xE0;o</pr></trd>
  </trdgrp>
</snc>
    <trdgrp lng="be">
    <trd>&c_b;&c_o;&c_l;&c_je;&c_p;&c_a;&c_t;&c_o;&c_l;&c_mol;&c_n;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_je;&c_s;&c_u;&c_c;&c_ib;&c_sh;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
    <trd>&c_a;&c_n;&c_a;&c_l;&c_mol;&c_g;&c_ib;&c_j;&c_n;&c_y; &c_s;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">analgetikum</trd>
  <trd lng="de">Analgetikum</trd>
  <trd lng="fr">analg&eacute;sique<klr> (subst.)</klr></trd>
  <trdgrp lng="nl">
    <trd>analgeticum</trd>,
    <trd>pijnstiller</trd>
  </trdgrp>
  <trd lng="pl">&sacute;rodek przeciwb&oacute;lowy</trd>
  <trd lng="sk">analgezikum</trd>
  <trd lng="uk">&c_a;&#x43D;&c_a;&#x43B;&#x44C;&c_g;&c_je;&c_t;&c_i;&#x43A;</trd>
</drv>
</art>

