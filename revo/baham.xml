<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: baham.xml,v 1.22 2024/10/06 17:29:57 revo Exp $">
<kap><ofc>10</ofc>
  <rad>Baham</rad>/oj
</kap>

<drv mrk="baham.0oj">
  <kap>
    <ofc>10</ofc><tld lit="B"/>oj
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">BS</mlg>
  <snc mrk="baham.0oj.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref>
      lokita sur la samnoma insularo en la Kariba Maro kun
      &ccirc;efurbo Nasa&ubreve;o;
      teritorio de Komunumo de <tld/>aj Insuloj:
      <ekz>
        en la listo estas anka&ubreve; landoj kiel <klr>[&#8230;]</klr> <tld/>oj kaj Barbado
        <fnt>
          <aut>Evgeni Georgiev</aut>
            <vrk><url ref="&Monato;/2000/007128.html">&Ccirc;u venos fino de la impostaj
            paradizoj?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 10</lok>
          </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
      <trd>&#24052;&#21704;&#39340; <pr>b&amacron;h&amacron;m&#462;</pr></trd>,
      <trd>&#24052;&#21704;&#39340;&#32676;&#23798; <pr>b&amacron;h&amacron;m&#462;q&uacute;nd&#462;o</pr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_B;&c_a;&c_g;&c_a;&c_m;&c_y;</trd>,
    <trd>&c_B;&c_a;&c_g;&c_a;&c_m;&c_s;&c_k;&c_ib;&c_ja; &c_a;&c_s;&c_t;&c_r;&c_a;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>les <ind>Bahames</ind></trd>,
    <trd>Commonwealth de les <ind>Bahames</ind></trd>
  </trdgrp>
  <trd lng="de">Bahamas</trd>
  <trd lng="es">Bahamas</trd>
  <trd lng="fr">Bahamas</trd>
  <trd lng="he">&alef;&yod;&yod; &bet;&he;&alef;&mem;&he;</trd>
  <trdgrp lng="hu">
    <trd>Baham&aacute;k</trd>,
    <trd>Bahama-szigetek</trd>
  </trdgrp>
  <trd lng="id">Bahama</trd>
  <trd lng="ja">&#12496;&#12495;&#12510;&#35576;&#23798;  <pr>&#12496;&#12495;&#12510;&#12375;&#12423;&#12392;&#12358;</pr></trd>
  <trdgrp lng="pl">
    <trd>Bahamy</trd>,
    <trd>Wsp&oacute;lnota <ind>Baham&oacute;w</ind></trd>
  </trdgrp>
  <trd lng="pt">Bahamas</trd>
  <trd lng="uk">&c_B;&c_a;&c_g;&c_a;&c_m;&c_s;&c_mol;&c_k;&c_ib;
  &c_o;&c_s;&c_t;&c_r;&c_o;&c_v;&c_i;</trd>
</drv>

<drv mrk="baham.Granda_0o">
  <kap>
    <ofc>10</ofc> Granda <tld/>o
  </kap>
  <snc mrk="baham.Granda_0o.insulo">
    <dif>
      <ref tip="lst" lst="voko:insuloj" cel="insul.0o.GEOG">Insulo</ref>
      en la nordo de <tld/>oj, &ccirc;. 100&#8239;km oriente de Florido:
      <ekz>
        Granda <tld/>o estas la kvara plej granda insulo de la insulo&ccirc;eno de <tld/>oj
        kiu enhavas &ccirc;irka&ubreve; 700 insulojn kaj 2 400 rifojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=328482"
              >Granda Bahamo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_ib; <ind>&c_B;&c_a;&c_g;&c_a;&c_m;&c_a;</ind></trd>
  <trd lng="de">Grand Bahama</trd>
  <trd lng="en">Grand Bahama</trd>
  <trd lng="fr">Grand Bahama</trd>
</drv>

</art>

