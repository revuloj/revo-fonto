<vortaro>
<art mrk="$Id: hermes.xml,v 1.19 2022/12/27 10:30:17 revo Exp $">
<kap>
  <rad>Hermes</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hermes.0o">
  <kap><tld/>o</kap>
  <snc mrk="hermes.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Helena <ref tip="super" cel="di.0o.politeisma">dio</ref>,
      unue de bestobredado kaj pa&scirc;tistoj, poste heroldo de la
      olimpaj dioj kaj dio de komerco:
      <ekz>
        <tld/>o portas &ccirc;apelon kun flugiloj, sandalojn kun flugiloj, kaj la kaduceon, vergon kun du
        enplektitaj serpentoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1637">Hermeso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="merkur1.0o.mita">Merkuro</ref>
    <trd lng="az">Hermes</trd>
    <trd lng="bg">&c_H;&c_je;&c_r;&c_m;&c_je;&c_s;</trd>
    <trd lng="bn">&#2489;&#2503;&#2480;&#2509;&#2478;&#2503;&#2488;</trd>
    <trd lng="br">Hermes</trd>
    <trd lng="bs">Hermes</trd>
    <trd lng="da">Hermes</trd>
    <trd lng="et">Hermes</trd>
    <trd lng="eu">Hermes</trd>
    <trd lng="fi">Hermes</trd>
    <trd lng="gl">Hermes</trd>
