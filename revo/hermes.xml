<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hermes.xml,v 1.21 2024/12/02 10:35:02 revo Exp $">
<kap>
  <rad>Hermes</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hermes.0o">
  <kap><tld/>o</kap>
  <snc mrk="hermes.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Helena <ref tip="super" cel="di.0o.politeisma">dio</ref>,
      unue de bestobredado kaj pa&scirc;tistoj, poste heroldo de la
      olimpaj dioj kaj dio de komerco:
      <ekz>
        savu min, Ze&ubreve;so, savu Apolono, savu <tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o portas &ccirc;apelon kun flugiloj, sandalojn kun flugiloj, kaj la kaduceon, vergon kun du
        enplektitaj serpentoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1637">Hermeso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="merkur1.0o.mita">Merkuro</ref>
    <trd lng="az">Hermes</trd>
    <trd lng="bg">&c_H;&c_je;&c_r;&c_m;&c_je;&c_s;</trd>
    <trd lng="bn">&#2489;&#2503;&#2480;&#2509;&#2478;&#2503;&#2488;</trd>
    <trd lng="br">Hermes</trd>
    <trd lng="bs">Hermes</trd>
    <trd lng="da">Hermes</trd>
    <trd lng="et">Hermes</trd>
    <trd lng="eu">Hermes</trd>
    <trd lng="fi">Hermes</trd>
    <trd lng="gl">Hermes</trd>
    <trd lng="grc">&Epsilon_densa;&rho;&my;&eta_circ;&sigma_fina;</trd>
    <trd lng="he">&he;&resh;&mem;&samekh;</trd>
    <trd lng="hi">&#2361;&#2352;&#2350;&#2368;&#2360;</trd>
    <trd lng="hr">Hermes</trd>
    <trd lng="hy">&#1344;&#1381;&#1408;&#1396;&#1381;&#1405;</trd>
    <trd lng="id">Hermes</trd>
    <trd lng="is">Hermes</trd>
    <trd lng="ja">&#12504;&#12523;&#12513;&#12540;&#12473;</trd>
    <trd lng="ka">&#4336;&#4308;&#4320;&#4315;&#4308;&#4321;&#4312;</trd>
    <trd lng="ko">&#54756;&#47476;&#47700;&#49828;</trd>
    <trd lng="la">Hermes</trd>
    <trd lng="lb">Hermes</trd>
    <trd lng="lt">Hermis</trd>
    <trd lng="lv">Hermejs</trd>
    <trd lng="mk">&c_H;&c_je;&c_r;&c_m;&c_je;&c_s;</trd>
    <trd lng="ml">&#3385;&#3398;&#3452;&#3374;&#3392;&#3384;&#3405;</trd>
    <trd lng="mr">&#2361;&#2352;&#2381;&#2350;&#2368;&#2360;</trd>
    <trd lng="ms">Hermes</trd>
    <trd lng="nd">Hermes</trd>
    <trd lng="no">Hermes</trd>
    <trd lng="oc">Hermes</trd>
    <trd lng="ro">Hermes</trd>
    <trd lng="sl">Hermes</trd>
    <trd lng="sq">Hermesi</trd>
    <trd lng="sr">&c_H;&c_je;&c_r;&c_m;&c_je;&c_s;</trd>
    <trd lng="sv">Hermes</trd>
    <trd lng="ta">&#3001;&#3014;&#2992;&#3021;&#2990;&#3008;&#3000;&#3021;</trd>
    <trd lng="tl">Hermes</trd>
    <trd lng="tr">Hermes</trd>
    <trd lng="vi">Hermes</trd>
    <trd lng="zh">&#36203;&#32819;&#22696;&#26031;</trd>
  </snc>
  <snc mrk="hermes.0o.nomo">
    <uzo tip="fak">BIB</uzo>
    <dif>
      Vira nomo:
      <ekz>
        salutu Asinkriton, Flegonon, <tld/>on, Patrobason, Hermason, kaj la fratojn &ccirc;e ili
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 16:14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_je;&c_r;&c_m;&c_e;&c_s;</trd>
  <trd lng="ca">Hermes</trd>
  <trd lng="cs">Hermes</trd>
  <trd lng="de">Hermes</trd>
  <trd lng="el">&Epsilon;&rho;&my;&eta_ton;&sigma_fina;</trd>
  <trd lng="en">Hermes</trd>
  <trd lng="es">Hermes</trd>
  <trd lng="fa">&a_h;&a_r;&a_m;&a_s;</trd>
  <trd lng="fr">Herm&egrave;s</trd>
  <trd lng="hu">Herm&eacute;sz</trd>
  <trdgrp lng="it"><trd>Ermes</trd>, <trd>Ermete</trd></trdgrp>
  <trd lng="nl">Hermes</trd>
  <trd lng="pl">Hermes</trd>
  <trd lng="pt">Hermes</trd>
  <trd lng="ru">&c_G;&c_je;&c_r;&c_m;&c_je;&c_s;</trd>
  <trd lng="sk">Herm&eacute;s</trd>
  <trd lng="uk">&c_G;&c_je;&c_r;&c_m;&c_je;&c_s;</trd>
</drv>

<drv mrk="hermes.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        Hermesa kapo
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 4 (54)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Rilata al <tld/>o Trioble Grandega:
      <ekz>
        Seslingva Vortareto de Medikamentoj (<tld/>a Rondeto)
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tekstoj entenataj en tiu papirusoj estas, grandparte, gnostikaj verkoj, sed
        inkluzivas anka&ubreve; tri verkojn apartenanatajn al <ind><tld/>a korpuso</ind>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=396540"
              >Kodeksoj de Nag Hammadi</url></lok>
        </fnt>.
        <trd lng="la">Corpus hermeticum</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_r;&c_m;&c_e;&c_s;&c_a;&c_w;</trd>
  <trd lng="de">Hermes-</trd>
</drv>

<drv mrk="hermes.0o_Trismegisto">
  <kap>
    <tld/>o Trioble Grandega, <var><kap><tld/>o Trismegisto</kap></var>
  </kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Mita egipta filozofo, al kiu mezepoke estis atribuita la Korpuso <tld/>a;
      sinkretismo de la greka dio <tld/>o kaj egipta dio Toto:
      <ekz>
        <nom>Corpus hermeticum</nom> <klr>[&#8230;]</klr> estis ju&gcirc;ata pli antikva, ol e&ccirc; la moseaj
        libroj de la Biblio, kaj verkoj de la mita egipta <tld/>o Trismegisto
        (&leftquot;trioble grandega&rightquot;)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=447183"
              >Corpus hermeticum</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_je;&c_r;&c_m;&c_e;&c_s; &c_T;&c_r;&c_y;&c_s;&c_m;&c_e;&c_g;&c_ib;&c_s;&c_t;</trd>
  <trd lng="de">Hermes Trismegistos</trd>
</drv>

</art>
