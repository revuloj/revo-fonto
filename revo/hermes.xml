<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Hermes</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hermes.0o">
  <kap><tld/>o</kap>
  <snc mrk="hermes.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Helena <ref tip="super" cel="di.0o.politeisma">dio</ref>,
      unue de bestobredado kaj pa&scirc;tistoj, poste heroldo de la
      olimpaj dioj kaj dio de komerco.
    </dif>
    <ref tip="vid" cel="merkur1.0o.mita">Merkuro</ref>
  </snc>
  <trd lng="be">&c_G;&c_je;&c_r;&c_m;&c_e;&c_s;</trd>
  <trd lng="ca">Hermes</trd>
  <trd lng="de">Hermes</trd>
  <trd lng="el">&Epsilon;&rho;&my;&eta_ton;&sigma_fina;</trd>
  <trd lng="en">Hermes</trd>
  <trd lng="es">Hermes</trd>
  <trd lng="fa">&a_h;&a_r;&a_m;&a_s;</trd>
  <trd lng="fr">Herm&egrave;s</trd>
  <trd lng="hu">Herm&eacute;sz</trd>
  <trdgrp lng="it"><trd>Ermes</trd>, <trd>Ermete</trd></trdgrp>
  <trd lng="nl">Hermes</trd>
  <trd lng="pl">Hermes</trd>
  <trd lng="pt">Hermes</trd>
  <trd lng="ru">&c_G;&c_je;&c_r;&c_m;&c_je;&c_s;</trd>
  <adm>
    Indas kontroli la grekan tradukon
    kaj aldoni la klasikan grekan.
    <aut>JF</aut>
  </adm>
</drv>
</art>
