<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: legat.xml,v 1.18 2023/09/01 19:10:58 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>legat</rad>/o
</kap>
<drv mrk="legat.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">KAT</uzo>
    <dif>
      Altranga <ref tip="super" cel="eklezi.0ulo">ekleziulo</ref>
      komisiita por reprezenti la papon en ia difinita okazo:
      <ekz>
        papa <tld/>o a&ubreve; <tld/>o de la Roma Pontifiko, estas sendito de
        la papo, lia stabila reprezentato &ccirc;e propraj lokaj eklezioj a&ubreve; &ccirc;e &scirc;tataj
        a&ubreve;toritatoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=422641">Papa
          legato</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="legat1.0o">legato (muziko)</ref>
  <trd lng="be">&c_l;&c_je;&c_g;&c_a;&c_t;</trd>
  <trdgrp lng="de">
     <trd>Gesandter</trd>,
     <trd>Abgesandter</trd>
  </trdgrp>
  <trd lng="en">legate</trd>
  <trd lng="fr">l&eacute;gat</trd>
  <trdgrp lng="hu">
     <trd>p&aacute;pai <ind>k&ouml;vet</ind></trd>,
     <trd>leg&aacute;tus</trd>
  </trdgrp>  
  <trd lng="ja">&#x6559;&#x7687;&#x7279;&#x4F7F;  [<ind>&#x304D;&#x3087;&#x3046;&#x3053;&#x3046;&#x3068;&#x304F;&#x3057;</ind>]</trd>
  <trd lng="nl">pauselijk <ind>gezant</ind></trd>
  <trd lng="pl">legat <klr>(papieski)</klr></trd>
  <trd lng="pt">legado <klr tip="amb">(rel.)</klr></trd>
  <trd lng="ro">legat papal</trd>
  <trd lng="ru">&c_l;&c_je;&c_g;&c_a;&c_t;</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x435;&#x491;&#x430;&#x442;</trd>,
    <trd>&#x43F;&#x430;&#x43F;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x43F;&#x43E;&#x441;&#x43E;&#x43B;</trd>
  </trdgrp>
</drv>
</art>
