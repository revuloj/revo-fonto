<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>d</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<subart>
<drv mrk="d.0.konsonanto">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      <ref tip="dif" cel="konson1.0o">Konsonanto</ref>, vo&ccirc;a <ref
      tip="dif" cel="plosiv.0o">plosiva</ref> <ref tip="dif"
      cel="dental.0o">dentalo</ref> [d].
    </dif>
  <trd lng="fr">d<klr> (consonne)</klr></trd>
  </snc>
  <snc>
     <dif>
      La kvina <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto, uzata i.a. kiel:
     </dif>
     <trd lng="fr">d<klr> (lettre)</klr></trd>
     <subsnc>
     <uzo tip="fak">MUZ</uzo>
     <dif>
     (maj.) nomo de la dua tono de la C-ma&jcirc;ora gamo (294 Hz;
     absolut-solfe&gcirc;a re): <ekz><tld lit="D"/> bemola, diesa</ekz>;
     </dif>
     <trd lng="fr">r&eacute;</trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">MAT</uzo>
     <dif>
     (maj.) romana cifero: <ekz><tld lit="D"/> (500)</ekz>;
     </dif>
     <trd lng="fr">500<klr> (chiffre romain)</klr></trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">KEM</uzo>
     <dif>
     (maj.) simbolo de de&ubreve;terio: <ekz><tld lit="D"/></ekz>;
     </dif>
     <trd lng="fr">deut&eacute;rium</trd>
     </subsnc>
     <subsnc>
     <dif>
     (maj.)  signo de formo: <ekz><tld lit="D"/>-tenilo</ekz>;
     </dif>
     <trd lng="fr">D<klr> (forme de)</klr></trd>
     </subsnc>
     <subsnc>
     <uzo tip="fak">SHI</uzo>
     <dif>
     (maj.) mallongigo de deklinacio magneta: <ekz><tld lit="D"/></ekz>;
     </dif>
     <trd lng="fr">d&eacute;clinaison magn&eacute;tique</trd>
     </subsnc>    
  </snc>
  </drv>
</subart>

<subart>
<drv mrk="d.0.nomo">
  <kap><tld/>o</kap>
  <snc> 
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif>
    <trd lng="fr">d</trd>
   </snc>
  </drv>
</subart>
</art>

