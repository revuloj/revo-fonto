<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>do</rad>
</kap>

<drv mrk="d.0">
  <kap><tld/></kap>
  <snc> 
    <dif>
      Nomo de la kvina litero de la Esperanta alfabeto:
      <ekz>
        nomoj de la literoj: a, bo, <tld/>, &ccirc;o, do, e fo <klr>[...]</klr>
        <fnt>
          <bib>F</bib>
          <lok>&FE; 1</lok>
        </fnt>,
      </ekz>
      uzata i.a. kiel:
    </dif>
    <subsnc>
       <uzo tip="fak">MUZ</uzo>
       <dif>
       (maj.) nomo de la dua tono de la C-ma&jcirc;ora gamo (294 Hz;
       absolut-solfe&gcirc;a re): <ekz><tld lit="D"/> bemola, diesa</ekz>;
       </dif>
       <trd lng="es">r&eacute;<klr> (en la notaci&oacute;n alfab&eacute;tica de
         solfeo)</klr></trd>
       <trd lng="fr">r&eacute;</trd>
    </subsnc>
    <subsnc>
       <uzo tip="fak">MAT</uzo>
       <dif>
       (maj.) romana cifero: <ekz>D (500)</ekz>;
       </dif>
       <trd lng="es">500<klr> (n&uacute;mero romano)</klr></trd>
       <trd lng="fr">500<klr> (chiffre romain)</klr></trd>
    </subsnc>
    <subsnc>
       <uzo tip="fak">KEM</uzo>
       <dif>
       (maj.) simbolo de de&ubreve;terio: <ekz>D</ekz>;
       </dif>
       <trd lng="es">deuterio</trd>
       <trd lng="fr">deut&eacute;rium</trd>
    </subsnc>
    <subsnc>
       <uzo tip="fak">TEK</uzo>
       <dif>
       (maj.)  signo de formo: <ekz>D-tenilo</ekz>;
       </dif>
       <trd lng="es">D<klr> (forma de)</klr></trd>
       <trd lng="fr">D<klr> (forme de)</klr></trd>
    </subsnc>
    <subsnc>
       <uzo tip="fak">SHI</uzo>
       <dif>
       (maj.) mallongigo de deklinacio magneta: <ekz>D</ekz>.
       </dif>
       <trd lng="es">declinaci&oacute;n magn&eacute;tica</trd>
       <trd lng="fr">d&eacute;clinaison magn&eacute;tique</trd>
    </subsnc>    
  </snc>
</drv>

</art>

