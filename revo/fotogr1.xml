<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fotogr1.xml,v 1.24 2025/11/16 10:33:26 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>fotografi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fotogr1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="fotogr.0arto">Fotografarto</ref>:
      <ekz>
        kiel &leftquot;lingvaj agoj&#x201D; povas esti determinitaj plej diversaj manieroj komuniki&gcirc;i,
        inkluzive la&ubreve;dire de muziko, <tld/>o a&ubreve; pentrarto
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011278.php">Vortaro intuicia, horizonto
          internacia</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_f;&c_a;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>,
      <trd>&c_f;&c_o;&c_t;&c_a;&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_t;&c_v;&c_a;</trd>
    </trdgrp>
    <trd lng="de">Fotografie</trd>
    <trd lng="en">photography</trd>
    <trd lng="es">fotograf&iacute;a</trd>
    <trd lng="fa">&a_ayn;&f_k;&a_A;&a_s;&f_y;</trd>
    <trd lng="fr">photographie<klr tip="ind"> (discipline)</klr></trd>
    <trdgrp lng="hu">
      <trd>f&eacute;nyk&eacute;p&eacute;szet</trd>,
      <trd>fotogr&aacute;fia</trd>
    </trdgrp>
    <trd lng="id">fotografi</trd>
    <trd lng="nl">fotografie</trd>
    <trd lng="pl">fotografia</trd>
    <trd lng="pt">fotografia <klr tip="amb">(arte)</klr></trd>
    <trd lng="ro">fotografie</trd>
    <trdgrp lng="ru">
      <trd>&c_f;&c_o;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>,
      <trd>&c_f;&c_o;&c_t;&c_o;&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fotogr1.0o.foto">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="fot.0o">Foto</ref>:
      <ekz>
        <klr>[ili]</klr> faras en la propra libertempo <tld/>ojn pri si mem kaj poste al&scirc;utas tiujn
        bildojn al la reto
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011294.php">Tro da (mem)fotado, ja perturba agado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <!-- ne traduku evitindajn ...-->
  <trdgrp lng="id">
    <trd>foto</trd>,
    <trd>potret</trd>,
    <trd>fotograf</trd>
  </trdgrp>
  </snc>
  <trd lng="ja">&#20889;&#x771F;&#34899;  <pr>&#12375;&#12419;&#12375;&#12435;&#12376;&#12421;&#12388;</pr></trd>
  <trd lng="uk">&c_f;&#x43E;&c_t;&#x43E;&c_g;&c_r;&c_a;&c_f;&c_ib;&#x44F; (&#x44F;&#x43A; &c_v;&c_i;&c_d; &#x43C;&c_i;&c_s;&c_t;&c_je;&c_c;&c_t;&c_v;&c_a;)</trd>
</drv>
</art>
