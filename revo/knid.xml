<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>knid</rad>/uloj
</kap>
<drv mrk="knid.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="filum.0o"
      lst="voko:zoologiaj_grupoj">Filumo</ref> 
      de la <ref tip="malprt" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Cnidaria</trd>),
      kun &ccirc;eloj ellasantaj brulan likva&jcirc;on &ccirc;e tu&scirc;o:
      <ekz>
        <tld/>uloj estas filumo kies membroj estas <ref tip="prt" cel="hidroz.0oj">hidrozooj</ref>,
        <ref tip="prt" cel="anemon.mar0oj">maranemonoj</ref> <klr>[&#8230;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2429"
              >Knidulo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        meduzoj estas drolaj <tld/>uloj, ili ekzistas jam delonge
        <fnt>
          <aut>Janina Viandisto, trad. Dorothea kaj Hans-Georg Kaiser</aut>
          <vrk><url ref="https://karapacopanoramo.blogspot.com/2018/05/janina-fleischer-physalia-physalis.html"
              >Physalia physalis</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="meduz.0o">meduzo</ref>,
      <ref cel="polip.0o">polipo</ref>,
      <ref cel="koral.0o">koralo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Nesseltiere</trd>
  <trd lng="fr">cnidaires</trd>
  <trd lng="hu">csal&aacute;nosok</trd>
  <trdgrp lng="nl">
    <trd>holtedieren</trd>,
    <trd>neteldieren</trd>
  </trdgrp>
  <trd lng="pl">parzyde&lstroke;kowce</trd>     
  <trd lng="ro">cnidaria</trd>
  <trd lng="ru">&c_k;&c_i;&c_sh;&c_je;&c_ch;&c_n;&c_o;&c_p;&c_o;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_je;</trd>
</drv>
</art>
