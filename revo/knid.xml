<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: knid.xml,v 1.17 2023/12/03 09:58:51 revo Exp $">
<kap>
  <rad>knid</rad>/o
</kap>
<drv mrk="knid.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="cxel.0o.BIO">&Ccirc;elo</ref>,
      trovi&gcirc;anta &ccirc;e diversaj <ref tip="malprt" cel="knid.0uloj">kniduloj</ref>, kiu je tu&scirc;o
      el&jcirc;etas maldikan pikilon, inokulante jukan kaj paralizan venenon:
      <ekz>
        la <tld/>o estas &ccirc;elo entenanta organeton nomatan nematocito
        <klr>[kun]</klr>
        veziketo, fermata de operkulo, en kiu estas volvata kava fadeno
        entenanta
        malgrandegajn pikilojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=206290">Knido (&ccirc;elo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_n;&c_ib;&c_d;&c_a;&c_c;&c_y;&c_t;</trd>,
    <trd>&c_zh;&c_y;&c_g;&c_u;&c_ch;&c_a;&c_ja; <ind>&c_k;&c_l;&c_je;&c_t;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="de">Nesselzelle</trd>
  <trd lng="en">cnidocyte</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je;&#x43C;&c_a;&c_t;&#x43E;&c_c;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_zh;&c_a;&#x43B;&#x43A;&c_a; &#x43A;&c_a;&#x43F;&c_s;&c_u;&#x43B;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x523A;&#x4E1D;&#x80DE; <pr>c&#xEC;s&#x12B;b&#x101;o</pr></trd>,
    <trd>&#x523A;&#x80DE; <pr>c&#xEC;b&#x101;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="knid.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="filum.0o" lst="voko:zoologiaj_grupoj">Filumo</ref>
      de la <ref tip="malprt" cel="best.0oj">bestoj</ref>
      (<trd lng="la">Cnidaria</trd>),
      pluraj provizitaj per <tld/>oj por kapti predon:
      <ekz>
        <tld/>uloj estas filumo kies membroj estas <ref tip="prt" cel="hidroz.0oj">hidrozooj</ref>,
        <ref tip="prt" cel="anemon.mar0oj">maranemonoj</ref>
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2429">Knidulo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        meduzoj estas drolaj <tld/>uloj, ili ekzistas jam delonge
        <fnt>
          <aut>Janina Viandisto, trad. Dorothea kaj Hans-Georg
          Kaiser</aut>
          <vrk><url ref="https://karapacopanoramo.blogspot.com/2018/05/janina-fleischer-physalia-physalis.html">Physalia
          physalis</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="meduz.0o">meduzo</ref>,
      <ref cel="polip.0o">polipo</ref>,
      <ref cel="koral.0o">koralo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_y;&c_g;&c_u;&c_ch;&c_y;&c_ja; <klr tip="amb">(&c_z;&c_a;&c_a;&c_l;. &c_t;&c_y;&c_p;)</klr></trd>,
    <trd>&c_k;&c_n;&c_ib;&c_d;&c_a;&c_r;&c_y;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">Nesseltiere</trd>
  <trd lng="en">Cnidaria</trd>
  <trd lng="fr">cnidaires</trd>
  <trd lng="hu">csal&aacute;nosok</trd>
  <trdgrp lng="nl">
    <trd>holtedieren</trd>,
    <trd>neteldieren</trd>
  </trdgrp>
  <trd lng="pl">parzyde&lstroke;kowce</trd>
  <trd lng="ro">cnidaria</trd>
  <trd lng="ru">&c_k;&c_i;&c_sh;&c_je;&c_ch;&c_n;&c_o;&c_p;&c_o;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_i;&c_sh;&#x43A;&#x43E;&c_v;&#x43E;&#x43F;&#x43E;&c_r;&#x43E;&c_zh;&#x43D;&c_i;&#x43D;&#x43D;&c_ib;</trd>,
    <trd>&c_zh;&c_a;&#x43B;&#x43A;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x523A;&#x80DE;&#x52A8;&#x7269;&#x95E8; <pr>c&#xEC;b&#x101;od&#xF2;ngw&#xF9;m&#xE9;n</pr></trd>,
    <trd>&#x523A;&#x4E1D;&#x80DE;&#x52A8;&#x7269; <pr>c&#xEC;s&#x12B;b&#x101;od&#xF2;ngw&#xF9;</pr></trd>,
    <trd>&#x523A;&#x80DE;&#x52A8;&#x7269; <pr>c&#xEC;b&#x101;od&#xF2;ngw&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
