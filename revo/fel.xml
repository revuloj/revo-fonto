<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fel</rad>/o
</kap>
<drv mrk="fel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kruda besta ha&ubreve;to kun haroj:
      <ekz>la <tld/>o tanadon ne valoras
        <klr>(
        <refgrp tip="vid">
          <ref cel="kost.0o">kosto</ref>,
          <ref cel="kandel.0o">kandelo</ref>
        </refgrp>
        )</klr><fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hauxt.0o">ha&ubreve;to</ref>,
      <ref cel="pelt.0o">pelto</ref>,
      <ref cel="led.0o">ledo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_k;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_f;&c_u;&c_t;&c_r;&c_a; <klr tip="amb">(&c_n;&c_je;&c_a;&c_p;&c_r;&c_a;&c_c;&c_a;&c_v;&c_a;&c_n;&c_a;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="de">Fell</trd>
  <trd lng="fr">peau<klr tip="ind"> (d'un animal)</klr></trd>
  <trdgrp lng="hu">
    <trd>bunda<klr tip="ind"> (&aacute;llatb&odblac;r)</klr></trd>,
    <trd>gerezna</trd>,
    <trd>&aacute;llatb&odblac;r</trd>
  </trdgrp>
  <trdgrp lng="nl">
   <trd>vel</trd>,
   <trd>huid</trd>
       </trdgrp>
  <trd lng="pt">pele <klr tip="amb">(de animal, com p&#234;los)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_k;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_b;&c_r;&c_a;&c_b;&c_o;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j; &c_m;&c_je;&c_h;</trd>
  </trdgrp>
</drv>
<drv mrk="fel.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Pretigisto de <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_sh;&c_n;&c_je;&c_r;</trd>,
    <trd>&c_a;&c_w;&c_ch;&c_y;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="fr">peaussier</trd>
  <trdgrp lng="hu">
    <trd>t&iacute;m&aacute;r</trd>,
    <trd>cserz&odblac;varga</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_r;&c_n;&c_ja;&c_k;</trd>
</drv>
<drv mrk="fel.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc>
    <dif>
      Detiri la <tld/>on:
      <ekz>senlanigu, sed ne sen<tld/>igu
        <klr>(ne tro ekspluatu)</klr><fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_r;&c_a;&c_c;&c_mol; <ind>&c_sh;&c_k;&c_u;&c_r;&c_u;</ind></trd>,
    <trd>&c_z;&c_l;&c_u;&c_p;&c_ib;&c_c;&c_mol; <ind>&c_sh;&c_k;&c_u;&c_r;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="fr">d&eacute;pecer</trd>
  <trd lng="hu">megny&uacute;z</trd>
  <trd lng="nl">villen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_i;&c_r;&c_a;&c_t;&c_mol; &c_sh;&c_k;&c_u;&c_r;&c_u;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_zh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="fel.0sako">
  <kap><tld/>sako</kap>
  <snc>
    <dif>
      Pretigita <tld/>o por enteni flua&jcirc;on:
      <ekz>
        konsumi&gcirc;is la akvo de la <tld/>sako
        <fnt><bib>MT</bib>, <lok>&Gen; 21:15</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_r;&c_d;&c_z;&c_ju;&c_k;</trd>,
    <trd>&c_m;&c_je;&c_h; <klr tip="amb">(&c_p;&c_a;&c_s;&c_u;&c_d;&c_z;&c_ib;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="fr">outre<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>t&ouml;ml&odblac;<klr tip="ind"> (b&odblac;rzs&aacute;k)</klr></trd>,
    <trd>b&odblac;rzs&aacute;k</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_r;&c_d;&c_ju;&c_k;</trd>,
    <trd>&c_m;&c_je;&c_h; <klr>(&c_s;&c_o;&c_s;&c_u;&c_d;)</klr></trd>
  </trdgrp>
</drv>
</art>
