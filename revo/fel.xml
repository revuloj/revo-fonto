<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fel.xml,v 1.31 2023/11/14 06:34:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="fel.0o.KOMUNE">
    <dif>
      Kruda besta <ref tip="super" cel="hauxt.0o.ANA">ha&ubreve;to</ref>
      kun <ref tip="prt" cel="har.0o.mamulo">haro</ref>j:
      <ekz>
        antilopaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 25:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        tapi&scirc;o el kapra <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 19:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la falsaj profetoj <klr>[&#8230;]</klr> venas al vi en &scirc;afaj <tld/>oj, sed interne
        estas rabemaj lupoj
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 7:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        per la <tld/>oj de la kapridoj &scirc;i vestis liajn manojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 27:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> portis panteran <tld/>on sur la maldekstra &scirc;ultro
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> iris en la urbon, por vendi la <tld/>on de sia &ccirc;evalo
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la hundoj kuris post &gcirc;i kaj volis fliki &gcirc;ian <tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        oni vokas nur tial la policon, &ccirc;ar
        ne estas permesite mem dra&scirc;i la felon de la romp&scirc;telinto kaj forpreni mem la
        predon de li
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        de unu bovo oni du <tld/>ojn ne de&scirc;iras
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; mortigo de urso ne vendu &gcirc;ian felon
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o tanadon ne valoras
        <klr>(<refgrp tip="vid">
          <ref cel="kost.0o">kosto</ref>,
          <ref cel="kandel.0o">kandelo</ref>
        </refgrp>)</klr>
        <fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hauxt.0o">ha&ubreve;to</ref>,
      <ref cel="led.0o">ledo</ref>,
      <ref cel="pelt.0o">pelto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_k;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_f;&c_u;&c_t;&c_r;&c_a; <klr tip="amb">(&c_n;&c_je;&c_a;&c_p;&c_r;&c_a;&c_c;&c_a;&c_v;&c_a;&c_n;&c_a;&c_je;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ko&zcaron;ka</trd>,
    <trd>surov&aacute; k&uring;&zcaron;e se srst&iacute;</trd>
  </trdgrp>
  <trd lng="de">Fell</trd>
  <trdgrp lng="en">
    <trd>hide<klr> (noun, of animal, with hair)</klr></trd>,
    <trd>fur<klr> (noun, of animal, with hide)</klr></trd>,
    <trd>pelt<klr> (noun, of animal)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cuero (no curtido)</trd>,
    <trd>piel (ya separada del animal)</trd>
  </trdgrp>
  <trd lng="fr">peau<klr tip="ind"> (d'un animal)</klr></trd>
  <trdgrp lng="hu">
    <trd>bunda<klr tip="ind"> (&aacute;llatb&odblac;r)</klr></trd>,
    <trd>gerezna</trd>,
    <trd>&aacute;llatb&odblac;r</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x76AE;  <pr>&#x304B;&#x308F;</pr></trd>,
    <trd>&#x6BDB;&#x76AE;  <pr>&#12369;&#x304C;&#x308F;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vel</trd>,
    <trd>huid</trd>
  </trdgrp>
  <trd lng="pl">futro</trd>
  <trd lng="pt">pele <klr tip="amb">(de animal, com p&ecirc;los)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_k;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_b;&c_r;&c_a;&c_b;&c_o;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j; &c_m;&c_je;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ko&zcaron;a</trd>,
    <trd>r&uacute;no</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_k;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_i;&c_p;&c_r;&c_a;&c_v;&c_l;&c_je;&c_n;&c_je; &c_h;&c_u;&c_t;&c_r;&c_o;</trd>,
    <trd>&c_r;&c_u;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x517D;&#x76AE; <pr>sh&#xF2;up&#xED;</pr></trd>,
    <trd>&#x76AE;&#x6BDB; <pr>p&#xED;m&#xE1;o</pr></trd>,
    <trd>&#x6BDB;&#x76AE; <pr>m&#xE1;op&#xED;</pr></trd>,
    <trd>&#x7F8A;&#x6BDB; <pr>y&#xE1;ngm&#xE1;o</pr></trd>,
    <trd>&#x6BDB; <pr>m&#xE1;o</pr></trd>,
    <trd>&#x76AE;&#33609; <pr>p&#xED;c&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fel.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      El felo:
      <ekz>
        <tld/>a &ccirc;apo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        molaj <tld/>aj pantofloj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a vinujo
        <fnt>
          <bib>SatirRak</bib>
          <lok>Logos</lok>
        </fnt>;
      </ekz>
      <ekz>
        kapro<tld/>a vesto
        <fnt>
          <bib>Rob</bib>
          <lok>Parto III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> rapide ekdemetis miajn <tld/>ajn kolumon kaj manumojn
        <fnt>
          <vrk><url ref="&Gutenberg;files/44128/44128-h/44128-h.htm">The Esperantist (Jun
          1905)</url></vrk>
          <lok>Doni a&ubreve; ne Doni!</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;aloj &ccirc;irka&ubreve; la kapo kaj viza&gcirc;o a&ubreve; <tld/>aj kapkovriloj
        <fnt>
          <vrk><url
          ref="https://pola-retradio.org/2017/11/la-cerkoportreto-en-la-iama-pollando/">Pola
          Retradio (Nov 2017)</url></vrk>
          <lok>La &ccirc;erkoportreto en la iama Pollando</lok>
        </fnt>;
      </ekz>
      <ekz>
        vestitaj en dikaj peltoj kaj <tld/>aj &ccirc;apoj
        <fnt>
          <aut>Julio BAGHY</aut>
          <vrk><url ref="&DonH;migranta/feodor.html">Feodor Nikolaevi&ccirc; Fel&scirc;ov</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_u;&c_t;&c_r;&c_a;&c_v;&c_y;</trd>,
    <trd>&c_z; <ind>&c_f;&c_u;&c_t;&c_r;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="en">fur<klr> (adj, material)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x76AE;&#x306E;  <pr>&#x304B;&#x308F;&#x306E;</pr></trd>,
    <trd>&#x6BDB;&#x76AE;&#x306E;  <pr>&#12369;&#x304C;&#x308F;&#x306E;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fel.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc>
    <dif>
      Io farita el <tld/>o:
      <ekz>
        se estos infekta&jcirc;o de lepro sur vesto <klr>[&#8230;]</klr>, &ccirc;u sur la enteksita&jcirc;o,
        <klr>[&#8230;]</klr> &ccirc;u sur ia <tld/>a&jcirc;o, <klr>[&#8230;]</klr> oni devas montri &gcirc;in al la
        pastro
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:48</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        eble ni iru rigardi iujn fela&jcirc;ojn?&ndash; brila ideo! Tamen ni rapidu, &ccirc;ar la zoo
        balda&ubreve; fermi&gcirc;os
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 12</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pelt.0o">pelto</ref>
    <trdgrp lng="be">
      <trd>&c_f;&c_u;&c_t;&c_r;&c_a;&c_v;&c_y; &c_v;&c_y;&c_r;&c_a;&c_b;</trd>,
      <trd>&c_f;&c_u;&c_t;&c_r;&c_a;&c_v;&c_ib;&c_n;&c_a;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="fel.0ano">
  <kap>
    <tld/>ano
  </kap>
  <snc mrk="fel.0ano.membro">
    <dif>
      Adepto de subkulturo, kies anoj &scirc;atas arta&jcirc;ojn, rakontojn, kostumojn
      pri bestoj kun homsimilaj trajtoj:
      <ekz>
        marksisma teorio por <tld/>anoj
        <fnt>
          <aut>Pata Vermelha</aut>
          <vrk><url ref="https://www.tumblr.com/patavermelha">Tumblr</url></vrk>
          <lok>Teoria marxista para furries</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj <tld/>anoj ne nur interesi&gcirc;as pri homformaj bestoj, sed foje trovas
        identecon en ili
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="&Viki;/Felkostumo">Felanaro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ano, kiu portas <tld/>kostumon, estas nomata <tld/>kostumulo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="&Viki;/Felkostumo">Felkostumo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Furry</trd>
  <trd lng="en">furry<klr> (noun, person)</klr></trd>
  <trd lng="zh">&#x517D;&#x8FF7; <pr>sh&#xF2;um&#xED;</pr></trd>
</drv>

<drv mrk="fel.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="fel.0isto.KOMUNE">
    <dif>
      Pretigisto de <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_sh;&c_n;&c_je;&c_r;</trd>,
    <trd>&c_a;&c_w;&c_ch;&c_y;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ko&zcaron;i&scaron;n&iacute;k</trd>,
    <trd>zpracovatel k&uring;&zcaron;&iacute;</trd>
  </trdgrp>
  <trd lng="en">furrier<klr> (noun, profession)</klr></trd>
  <trd lng="es">peletero</trd>
  <trd lng="fr">peaussier</trd>
  <trdgrp lng="hu">
    <trd>t&iacute;m&aacute;r</trd>,
    <trd>cserz&odblac;varga</trd>
  </trdgrp>
  <trd lng="ja">&#x76AE;&#x306A;&#12417;&#12375;&#32887;&#x4EBA; 
  <pr>&#x304B;&#x308F;&#x306A;&#12417;&#12375;&#12375;&#12423;&#x304F;&#x306B;&#12435;</pr></trd>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_r;&c_n;&c_ja;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>ko&zcaron;u&scaron;n&iacute;k</trd>,
    <trd>spracovate&lcaron; ko&zcaron;&iacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_u;&c_sh;&c_n;&c_ib;&c_r;</trd>
</drv>

<drv mrk="fel.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc mrk="fel.sen0igi.KOMUNE">
    <dif>
      Detiri la <tld/>on:
      <ekz>
        li sen<tld/>igis la &ccirc;evalon, bone elsekigis la <tld/>on en la vento
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nobeleto<klr>[&#8230;]</klr>, kiu sen<tld/>igas siajn vila&gcirc;anojn kiel bruton
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        senlanigu, sed ne sen<tld/>igu
        <klr>(ne tro ekspluatu)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_r;&c_a;&c_c;&c_mol; <ind>&c_sh;&c_k;&c_u;&c_r;&c_u;</ind></trd>,
    <trd>&c_z;&c_l;&c_u;&c_p;&c_ib;&c_c;&c_mol; <ind>&c_sh;&c_k;&c_u;&c_r;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="cs">st&aacute;hnout z k&uring;&zcaron;e</trd>
  <trd lng="en">skin<klr> (vt, an animal)</klr></trd>
  <trd lng="es">descuerar (un animal)</trd>
  <trd lng="fr">d&eacute;pecer</trd>
  <trd lng="hu">megny&uacute;z</trd>
  <trd lng="ja">&#x76AE;&#12434;&#x306F;&#12368;  <pr>&#x304B;&#x308F;&#12434;&#x306F;&#12368;</pr></trd>
  <trd lng="nl">villen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_i;&c_r;&c_a;&c_t;&c_mol; &c_sh;&c_k;&c_u;&c_r;&c_u;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_zh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odra&tcaron; ko&zcaron;u <klr>(so srs&tcaron;ou)</klr></trd>,
    <trd>stiahnu&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_d;&c_i;&c_r;&c_a;&c_t;&c_i;/&c_z;&c_n;&c_ib;&c_m;&c_a;&c_t;&c_i; &c_sh;&c_k;&c_u;&c_r;&c_u;</trd>,
    <trd>&c_b;&c_ib;&c_l;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_d;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="fel.Ora_sxaf0o">
  <kap>
    Ora &scirc;af<tld/>o
  </kap>
  <snc>
    <uzo tip="fak">MIT</uzo>
    <dif>
      <klr>(helena mitologio)</klr> Mita &scirc;af<tld/>o, kiun rabis <ref cel="jazon.0o.mit">Jazono</ref>
      de la dioj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i sidas apud la dioj de nia popolo, bele kaj bone sur ora &scirc;afo<tld/>o, kaj
        man&gcirc;as mielkukojn
        <fnt>
          <bib>StU</bib>
          <lok>Britujo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_l;&c_a;&c_t;&c_o;&c_je; <ind>&c_r;&c_u;&c_n;&c_o;</ind></trd>
  <trd lng="de">Goldenes Vlies</trd>
  <trd lng="en">Golden Fleece</trd>
  <trd lng="fr">Toison d'or</trd>
  <trd lng="ru">&c_Z;&c_o;&c_l;&c_o;&c_t;&c_o;&c_je; &c_r;&c_u;&c_n;&c_o;</trd>
  <trd lng="zh">&#x91D1;&#x7F8A;&#x6BDB; <pr>j&#x12B;ny&#xE1;ngm&#xE1;o</pr></trd>
</drv>

