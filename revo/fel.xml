<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>fel</rad>/o
</kap>
<drv mrk="fel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kruda besta ha&ubreve;to kun a&ubreve; sen haroj:
      <ekz>
        la <tld/>o tanadon ne valoras
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kost.0o">kosto</ref>,
      <ref cel="kandel.0o">kandelo</ref>.
    </refgrp>
    <refgrp tip="sin">
      <ref cel="hauxt.0o">ha&ubreve;to</ref>,
      <ref cel="pelt.0o">pelto</ref>,
      <ref cel="led.0o">ledo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Fell</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_k;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_b;&c_r;&c_a;&c_b;&c_o;&c_t;&c_a;&c_n;&c_n;&c_y;&c_j; &c_m;&c_je;&c_h;</trd>
  </trdgrp>
</drv>
<drv mrk="fel.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Pretigisto de <tld/>oj. 
    </dif>
  </snc>
  <trd lng="ru">&c_s;&c_k;&c_o;&c_r;&c_n;&c_ja;&c_k;</trd>
</drv>
<drv mrk="fel.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc>
    <dif>
      Detiri la <tld/>on:
      <ekz>
        senlanigu, sed ne sen<tld/>igu
        <klr>(ne tro ekspluatu)</klr>.
      </ekz>  
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_i;&c_r;&c_a;&c_t;&c_mol; &c_sh;&c_k;&c_u;&c_r;&c_u;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_zh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="fel.0sako">
  <kap><tld/>sako</kap>
  <snc>
    <dif>
      Pretigita <tld/>o por enteni flua&jcirc;on:
      <ekz>
        konsumi&gcirc;is la akvo de la <tld/>sako
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Genezo 21, 15</lok>)</fnt>.
      </ekz>  
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_r;&c_d;&c_ju;&c_k;</trd>,
    <trd>&c_m;&c_je;&c_h; <klr>(&c_s;&c_o;&c_s;&c_u;&c_d;)</klr></trd>
  </trdgrp>
</drv>
</art>
