<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tenes.xml,v 1.4 2023/12/29 07:23:36 revo Exp $">
<kap>
  <rad>tenes</rad>/o
</kap>
<drv mrk="tenes.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Ts</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="117">elemento</ref> kun
      atomnumero 117, atommaso 294&#8239;u:
      <ekz>
        la lastaj elementoj kiuj estis malkovrataj estis nihonio (Nh),
        moskovio (Mc), <tld/>o (Ts) kaj oganesono (Og) kun la nombroj
        113, 115, 117 kaj 118
        <fnt>
          <aut>F. van Hertrooij</aut>
          <vrk><url
          ref="https://scivolemo.wordpress.com/2018/09/19/trovi-elementojn-estas-paciencludo/">Trovi
          elementojn estas paciencludo</url></vrk>
          <lok>Scivolemo, 2018-09-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_e;&c_n;&c_e;&c_s;</trd>,
    <trd>&c_t;&c_e;&c_n;&c_e;&c_s;&c_y;&c_n; <klr>(&c_h;&c_ib;&c_m;&c_ib;&c_ch;&c_n;&c_y; &c_e;&c_l;&c_je;&c_m;&c_e;&c_n;&c_t;)</klr></trd>
  </trdgrp>
  <trd lng="ca">tennes</trd>
  <trd lng="de">Tenness</trd>
  <trd lng="en">tennessine</trd>
  <trd lng="es">teneso</trd>
  <trd lng="fr">tennesse</trd>
  <trd lng="ja">&#12486;&#12493;&#12471;&#12531; <pr>&#12390;&#12397;&#12375;&#12435;</pr></trd>
  <trd lng="la">tennessine</trd>
  <trd lng="nl">tennessine</trd>
  <trd lng="pl">tenes</trd>
  <trdgrp lng="pt">
    <trd>tenesso</trd>,
    <trd>tennesso</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_je;&c_n;&c_n;&c_je;&c_s;&c_s;&c_i;&c_n;</trd>
  <trd lng="sv">tenness</trd>
  <trd lng="zh">&#40940; <pr>ti&aacute;n</pr></trd>
</drv>
</art>

