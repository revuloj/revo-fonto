<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kamfor.xml,v 1.21 2019/06/30 18:21:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kamfor</rad>/o
</kap>
<drv mrk="kamfor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Blanka aroma <ref tip="super" cel="substa.0o.FIZ">substanco</ref>,
      vapori&gcirc;ema, ekstraktata el la
      <tld/>arbo, kaj uzata en medicino kaj kontra&ubreve; tineoj:
      <ekz>
        Japanio eksportis unuatempe ar&gcirc;enton kaj kupron, kaj poste <tld/>on, porcelanon,
        laka&jcirc;ojn, sojsa&ubreve;con kaj marprodukta&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2008/009358.php"
              >Nagasako</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        bioplasto estas farita el plantoj, ekzemple maizo, por moligi oni uzas
        sendan&gcirc;erajn substancojn, ekzemple bio-devenajn oleojn a&ubreve; <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2014/011143.php"
              >Plasto planta</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_f;&c_o;&c_r;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_f;&c_o;&c_r;</trd>
  <trd lng="br">ka&ntilde;fr</trd>
  <trd lng="ca">c&agrave;mfora</trd>
  <trd lng="cs">kafr</trd>
  <trd lng="de">Kampfer</trd>
  <trd lng="en">camphor</trd>
  <trd lng="fa">&f_k;&a_A;&a_f;&a_w;&a_r;</trd>
  <trd lng="fr">camphre</trd>
  <trd lng="hu">k&aacute;mfor</trd>
  <trd lng="nl">kamfer</trd>
  <trd lng="pl">kamfora</trd>     
  <trd lng="ro">camfor</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_m;&c_f;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_f;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">g&aacute;for</trd>
</drv>

<drv mrk="kamfor.0ujo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="super" cel="cinam.0ujo">cinamujo</ref>
      (<trd lng="la">Cinnamomum camphora</trd>),
      kreskanta en orienta Azio kaj ri&ccirc;a je <tld/>oleo:
      <ekz>
        pli ol 200 specioj da plantoj produktas linalolon, &ccirc;efe el la familioj lamiacoj,
        la&ubreve;racoj (<klr>[&#8230;]</klr> kamforarbo, la&ubreve;ro, ktp), kaj rutacoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=525035"
              >Linalolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/b/bc/Cinnamomum_camphora4.jpg/640px-Cinnamomum_camphora4.jpg"
      prm="CC-BY-SA-3.0"
      >folioj kaj fruktoj de <tld/>arbo (Cinnamomum camphora)
        <fnt>
          <aut>Kenpei, propra foto</aut>
          <lok><url ref="&WCW;/File:Cinnamomum_camphora4.jpg"
              >https://commons.wikimedia.org/wiki/File:Cinnamomum_camphora4.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_f;&c_o;&c_r;&c_a;&c_v;&c_a;&c_je; &c_d;&c_r;&c_e;&c_v;&c_a;</trd>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_f;&c_o;&c_r;&c_o;&c_v;&c_o; &c_d;&c_malmol;&c_r;&c_v;&c_o;</trd>
  <trd lng="br">gwezenn-ga&ntilde;fr</trd>
  <trd lng="ca">camforer</trd>
  <trd lng="de">Kampferbaum</trd>
  <trd lng="en">camphor tree</trd>
  <trd lng="fa">&a_d;&a_r;&a_kh;&a_t; <ind>&f_k;&a_A;&a_f;&a_w;&a_r;</ind></trd>
  <trd lng="fr">camphrier</trd>
  <trd lng="hu">k&aacute;mforfa</trd>
  <trd lng="nl">kamferboom</trd>
  <trd lng="pl">cynamonowiec kamforowy</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_f;&c_o;&c_r;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
</drv>
</art>
