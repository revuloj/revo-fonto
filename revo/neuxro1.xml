<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ne&ubreve;roz</rad>/o
</kap>
<drv mrk="neuxro1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">PSI</uzo>
  <snc>
    <dif>
      Malordo en la funkcio de la nerva sistemo, kun psikaj fenomenoj kaj
      parte psikaj ka&ubreve;zoj.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_w;&c_r;&c_o;&c_z;</trd>
  <trd lng="de">Neurose</trd>
  <trd lng="el">&ny;&epsilon;&ypsilon;&rho;&omega_ton;&sigma;&eta;</trd>
  <trd lng="en">neurosis</trd>
  <trd lng="fr">n&eacute;vrose</trd>
  <trd lng="nl">neurose</trd>
  <trdgrp lng="pt">
    <trd>neurose</trd>,
    <trd>nevrose</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_v;&c_r;&c_o;&c_z;</trd>
</drv>
</art>
