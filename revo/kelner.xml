<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kelner</rad>/o
</kap>
<drv mrk="kelner.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Trinkeja a&ubreve; restoracia servisto kiu proponas trinka&jcirc;ojn,
      man&gcirc;a&jcirc;ojn, notas mendojn, alportas pladojn, prezentas
      fakturon:
      <ekz>
        post la man&gcirc;o la <tld/>oj alportis fakturon pri pli ol
        sesmil-dolara konsumo<fnt><aut>Kata Lius</aut>: <vrk>Honestaj
        hungaroj plu atendas vin</vrk>, <bib>Monato</bib><lok>jaro 1997a,
        numero 9a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_f;&c_ib;&c_c;&c_y;&c_ja;&c_n;&c_t;</trd>,
    <trd>&c_k;&c_je;&c_l;&c_mol;&c_n;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="cs">&ccaron;&iacute;&scaron;n&iacute;k</trd>
  <trd lng="en">waiter</trd>
  <trd lng="es">mesero</trd>
  <trdgrp lng="fr">
    <trd>gar&ccedil;on de caf&eacute;</trd>,
    <trd>serveur<klr> (de restaurant)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kellner</trd>,
    <trd>Bedienung</trd>
  </trdgrp>
  <trd lng="hu">pinc&eacute;r</trd>
  <trd lng="nl">kelner</trd>
  <trd lng="pl">kelner</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_f;&c_i;&c_c;&c_i;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_k;&c_je;&c_l;&c_mol;&c_n;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="sv">kypare</trd>
</drv>
</art>
