<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kelner</rad>/o
</kap>
<drv mrk="kelner.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Servisto, kiu alportas la trinka&jcirc;ojn a&ubreve; man&gcirc;a&jcirc;ojn al la klientoj
      de kafejo a&ubreve; restoracio.  
    </dif>
  </snc>
  <trd lng="cs">&ccaron;&iacute;&scaron;n&iacute;k</trd>
  <trd lng="en">waiter</trd>
  <trd lng="fr">serveur</trd>
  <trdgrp lng="de">
    <trd>Kellner</trd>,
    <trd>Bedienung</trd>
  </trdgrp>
  <trd lng="nl">kelner</trd>
  <trd lng="pl">kelner</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_f;&c_i;&c_c;&c_i;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_k;&c_je;&c_l;&c_mol;&c_n;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="sv">kypare</trd>
</drv>
</art>
