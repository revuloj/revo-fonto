<kap>
  <ofc>*</ofc>
  <rad>kelner</rad>/o
</kap>
<drv mrk="kelner.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Trinkeja a&ubreve; restoracia servisto kiu proponas trinka&jcirc;ojn,
      man&gcirc;a&jcirc;ojn, notas mendojn, alportas pladojn, prezentas
      fakturon:
      <ekz>
        post la man&gcirc;o la <tld/>oj alportis fakturon pri pli ol
        sesmil-dolara konsumo
        <fnt><aut>Kata Lius</aut>: <vrk>Honestaj
        hungaroj plu atendas vin</vrk>, <bib>Monato</bib><lok>jaro
        1997a,
        numero 9a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_f;&c_ib;&c_c;&c_y;&c_ja;&c_n;&c_t;</trd>,
    <trd>&c_k;&c_je;&c_l;&c_mol;&c_n;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_je;&c_l;&c_n;&c_je;&c_r;</trd>,
    <trd>&c_s;&c_je;&c_r;&c_v;&c_i;&c_t;&c_mol;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="br">mevel (an ostaleri)</trd>
  <trdgrp lng="cs">
    <trd>&ccaron;&iacute;&scaron;n&iacute;k</trd>,
    <trd>kelner</trd>,
    <trd>sklepn&iacute;k</trd>,
    <trd>&ccaron;&iacute;&scaron;n&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kellner</trd>,
    <trd>Bedienung</trd>
  </trdgrp>
  <trd lng="en">waiter</trd>
  <trdgrp lng="es">
    <trd>mesero</trd>,
    <trd>camarero</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_p;&f_y;&a_sh;&a_kh;&a_d;&a_m;&a_t;</trd>,
    <trd>&f_g;&a_A;&a_r;&a_s;&a_damma;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gar&ccedil;on de caf&eacute;</trd>,
    <trd>serveur<klr tip="ind"> (de restaurant)</klr></trd>
  </trdgrp>
  <trd lng="hu">pinc&eacute;r</trd>
  <trd lng="it">cameriere</trd>
  <trd lng="nl">kelner</trd>
  <trd lng="pl">kelner</trd>
  <trd lng="ro">chelner</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_f;&c_i;&c_c;&c_i;&c_a;&c_n;&c_t;</trd>,
    <trd>&c_k;&c_je;&c_l;&c_mol;&c_n;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">&ccaron;a&scaron;n&iacute;k</trd>
  <trd lng="sv">kypare</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x435;&#x43B;&#x44C;&#x43D;&#x435;&#x440;</trd>,
    <trd>&#x43E;&#x444;&#x456;&#x446;&#x456;&#x430;&#x43D;&#x442;</trd>
  </trdgrp>
</drv>
</art>
