<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gajakol</rad>/o
</kap>
<drv mrk="gajako.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      C<sub>7</sub>H<sub>8</sub>O<sub>2</sub>.
      Etero, kiun oni ekstraktas el rezino de gajako kaj el kreozoto de fago.  
    </dif>
  </snc>
  <trd lng="de">Guajakol</trd>
  <trd lng="en">guaiacol</trd>
  <trdgrp lng="fr">
    <trd>ga&iuml;acol</trd>,
    <trd>gayacol</trd></trdgrp> 
  <trd lng="nl">guajacol</trd>
  <trd lng="ru">&c_g;&c_v;&c_a;&c_ja;&c_k;&c_o;&c_l;</trd>
</drv>
<adm>
PIV1 preferas pli logikan formon "gvajak" de la radiko.
</adm>
</art>
