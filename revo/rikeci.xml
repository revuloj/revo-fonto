          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Stanislaus_von_Prowazek">
          Stanislaus von Prowazek</url>, 2013-11-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">rickettsie</trd>
  <trd lng="en">rickettsia</trd>
  <trd lng="fr">rickettsie</trd>
  <trd lng="sk">rickettsia</trd>
  <trd lng="uk">&#x440;&#x438;&#x43A;&#x435;&#x442;&#x441;&#x456;&#x439;</trd>
</drv>
</art>

