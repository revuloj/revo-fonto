<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rikeci.xml,v 1.8 2023/08/17 20:23:48 revo Exp $">
<kap>
  <rad>rikeci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rikeci.0o">
  <kap><tld/>o</kap>
  <snc mrk="rikeci.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="super" cel="bakter.0o">Bakterio</ref> kiu multi&gcirc;as en &ccirc;eloj kaj detruas ilin,
      ofte transdonita de parazitaj insektoj a&ubreve; akaroj;
      genro <trd lng="la">Rickettsia</trd>:
      <ekz>
        usonano <nom>Howard Taylor Ricketts</nom> (1871-1910)
        <klr>[&#8230;]</klr> malkovris <tld/>ojn kaj mortis kiel
        viktimo de ekzantema tifo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Stanislaus_von_Prowazek">
          Stanislaus von Prowazek</url>, 2013-11-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_k;&c_je;&c_c;&c_y;&c_ja;</trd>
  <trd lng="cs">rickettsie</trd>
  <trd lng="en">rickettsia</trd>
  <trd lng="fr">rickettsie</trd>
  <trd lng="pl">riketsja</trd>
  <trd lng="ro">rickettsia</trd>
  <trd lng="sk">rickettsia</trd>
  <trd lng="uk">&c_r;&c_i;&c_k;&c_je;&c_t;&c_s;&c_ib;&c_j;</trd>
</drv>
</art>

