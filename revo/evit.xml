<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: evit.xml,v 1.21 2015/07/04 08:37:33 revo Exp $">
<kap>
<ofc>*</ofc>
  <rad>evit</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="evit.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="evit.0i.iun">
    <dif>
      Peni ne renkonti iun:
      <ekz>
        <tld/>i la babilulojn.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>stroni&cacute;<klr> (od kogo&sacute;)</klr></trd>,
      <trd>unika&cacute;<klr> (kogo&sacute;)</klr></trd>,
      <trd>wystrzega&cacute; si&eogonek;<klr>
      (kogo&sacute;)</klr></trd>,
      <trd>omija&cacute;</trd>,
      <trd>wymija&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="evit.0i.ion">
    <dif>
      Peni ne esti trafita de io:
      <ekz>
        <tld/>i frapon, dan&gcirc;eron, morton, eraron;
      </ekz>
      <ekz>
        ankora&ubreve; neniu <tld/>is la sorton
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eskap.0i">eskapi</ref>,
      <ref cel="kur.for0i">forkuri</ref>,
      <ref cel="sav">sin forsavi</ref>.
    </refgrp>
    <trdgrp lng="pl">
      <trd>stroni&cacute;<klr> (od czego&sacute;)</klr></trd>,
      <trd>unika&cacute;<klr> (czego&sacute;)</klr></trd>,
      <trd>wystrzega&cacute; si&eogonek;<klr>
      (czego&sacute;)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_z;&c_mol;&c_b;&c_ja;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obcházet</trd>,
    <trd>obejít</trd>,
    <trd>předcházet</trd>,
    <trd>stranit se</trd>,
    <trd>střežit se</trd>,
    <trd>vyhnout se</trd>,
    <trd>vyhýbat se</trd>,
    <trd>vymykat se</trd>,
    <trd>zabránit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>meiden</trd>,
    <trd>vermeiden</trd>,
    <trd>ausweichen</trd>
  </trdgrp>
  <trd lng="en">avoid</trd>
  <trd lng="es">evitar</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_w;&a_r;&f_y; &a_j;&a_s;&a_t;&a_n;<klr>
    (&a_A;&a_z;)</klr></trd>,
    <trd>&a_A;&a_j;&a_t;&a_n;&a_A;&a_b; &f_k;&a_r;&a_d;&a_n;<klr>
    (&a_A;&a_z;)</klr></trd>
  </trdgrp>
  <trd lng="fr">&eacute;viter<klr tip="ind"> (qn ou qc)</klr></trd>
  <trdgrp lng="hu">
     <trd>ker&uuml;l</trd>,
     <trd>elker&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>evitar</trd>,
    <trd>evadir</trd>,
    <trd>esquivar</trd>,
    <trd>poupar</trd>,
    <trd>prevenir</trd>,
    <trd>presumir</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_b;&c_je;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obísť</trd>,
    <trd>strániť sa</trd>,
    <trd>vyhýbať sa</trd>
  </trdgrp>
  <trd lng="sv">undvika</trd>
</drv>

<drv mrk="evit.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="evit.0ema.KOMUNE">
    <dif>
      Penanta <tld/>i malfacila&jcirc;ojn:
      <ekz>
        <tld/>ema respondo;
      </ekz>
      <ekz>
        li <tld/>eme respondis la demandojn<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="preciz">malpreciza</ref>,
      <ref cel="gard">singarda</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_h;&c_ib;&c_l;&c_ib;&c_s;&c_t;&c_y;</trd>
  <trd lng="cs">vyhýbavý</trd>
  <trd lng="de">ausweichend</trd>
  <trd lng="fa">&a_m;&a_sh;&f_k;&a_l;&zwnj;&f_g;&a_r;&f_y;&a_z;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;vasif</trd>,
    <trd>fuyant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kit&eacute;r&odblac;</trd>,
    <trd>elker&uuml;l&odblac;</trd>
  </trdgrp>
  <trd lng="pl">wymijaj&aogonek;cy</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_k;&c_l;&c_o;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_s;&c_t;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">vyhýbavý</trd>
</drv>

<drv mrk="evit.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="evit.0ebla.KOMUNE">
    <dif>
      Kiun oni povas <tld/>i.
    </dif>
  </snc>
  <trd lng="de">vermeidbar</trd>
  <trd lng="es">evitable</trd>
  <trd lng="fa">&a_A;&a_j;&a_t;&a_n;&a_A;&a_b;&zwnj;&f_p;&a_dh;&f_y;&a_r;</trd>
  <trd lng="fr">&eacute;vitable</trd>
  <trd lng="hu">elker&uuml;lhet&odblac;</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je; &c_n;&c_je;&c_i;&c_z;&c_b;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="evit.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="evit.0igi.KOMUNE">
    <dif>
      Helpi al <tld/>ado;
      <ref tip="vid" cel="sxpar.0igi.FIG">&scirc;parigi</ref>:
      <ekz>
        li <tld/>igis al mi multajn erarojn.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>odvrátit</trd>,
    <trd>předejít</trd>,
    <trd>zabránit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ersparen</trd>,
    <trd>davor bewahren</trd>,
    <trd>vermeidbar machen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;pargner<klr tip="ind"> (qc &agrave;
    qn)</klr></trd>,
    <trd>&eacute;viter<klr tip="ind"> (qc &agrave;
    qn)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>meg&oacute;v</trd>,
    <trd>megk&iacute;m&eacute;l</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odvrátiť</trd>,
    <trd>predísť</trd>,
    <trd>zabrániť</trd>
  </trdgrp>
</drv>

<drv mrk="evit.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="evit.ne0ebla.KOMUNE">
    <dif>
      Kiun oni ne povas <tld/>i:
      <ekz>
        <ind>ne<tld/>ebla</ind> estas la morto.
      </ekz>
    <ref tip="sin" cel="nepr">nepre</ref>,
    <ref tip="vid" cel="kondam.0i">kondamni</ref>
    </dif>
    <trd
    lng="be">&c_n;&c_je;&c_p;&c_a;&c_z;&c_mol;&c_b;&c_je;&c_zh;&c_n;&c_y;</trd>
    <trdgrp lng="de">
       <trd>unvermeidbar</trd>,
       <trd>unvermeidlich</trd>
    </trdgrp>
    <trd lng="es">inevitable</trd>
    <trd
    lng="fa">&a_A;&a_j;&a_t;&a_n;&a_A;&a_b;&zwnj;&a_n;&a_A;&f_p;&a_dh;&f_y;&a_r;</trd>
    <trdgrp lng="fr">
      <trd>in&eacute;luctable</trd>,
      <trd>in&eacute;vitable</trd></trdgrp>
    <trd lng="hu">elker&uuml;lhetetlen</trd>
    <trdgrp lng="pl">
      <trd>niechybny</trd>,
      <trd>nieuchronny</trd>,
      <trd>nieunikniony</trd>
    </trdgrp>
    <trd
    lng="ru">&c_n;&c_je;&c_i;&c_z;&c_b;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>neodvratný</trd>,
    <trd>nevyhnutelný</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>neodvratný</trd>,
    <trd>nevyhnutný</trd>
  </trdgrp>
</drv>

<drv mrk="evit.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="evit.mal0i.KOMUNE">
    <dif>
      Kontra&ubreve;stari, kontra&ubreve;iri, spiti:
      <ekz>
        kura&gcirc;e mal<tld/>i la dan&gcirc;eron.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">narazit na něco</trd>
  <trd lng="fa">&a_r;&a_w;&f_y;&a_A;&a_r;&a_w;&f_y;&f_y;
  &a_j;&a_s;&a_t;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>braver</trd>,
    <trd>d&eacute;fier</trd>,
    <trd>provoquer<klr tip="ind"> (d&eacute;fier)</klr></trd>
  </trdgrp>
  <trd lng="hu">szembesz&aacute;ll</trd>
  <trdgrp lng="pl">
    <trd>stawia&cacute; <ind>czo&lstroke;a</ind><klr> (czemu&sacute;,
    komu&sacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_o;&c_s;&c_a;&c_t;&c_mol;
    <ind>&c_v;&c_y;&c_z;&c_o;&c_v;</ind></trd>,
    <trd>&c_i;&c_d;&c_t;&c_i;
    <ind>&c_n;&c_a;&c_v;&c_s;&c_t;&c_r;&c_je;&c_ch;&c_u;</ind><klr
    tip="amb">
    (&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;&c_i;)</klr></trd>
  </trdgrp>
  <trd lng="sk">naraziť na niečo</trd>
</drv>
</art>
