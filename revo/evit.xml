<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>evit</rad>/i
</kap>
<drv mrk="evit.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Peni ne renkonti iun:
      <ekz>
        <tld/>i la babilulojn.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Peni ne esti trafita de io:
      <ekz>
        <tld/>i frapon, dan&gcirc;eron, morton, eraron;
      </ekz>
      <ekz>ankora&ubreve; neniu <tld/>is la sorton<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        li <ind><tld/>igi</ind>s <klr>(&scirc;paris)</klr> al mi
        multajn erarojn.
        <trdgrp lng="de">
           <trd>ersparen</trd>,
           <trd>davor bewahren</trd>,
           <trd>vermeidbar machen</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>&eacute;pargner<klr tip="ind"> (qc &agrave; qn)</klr></trd>,
          <trd>&eacute;viter<klr tip="ind"> (qc &agrave; qn)</klr></trd></trdgrp>
        <trdgrp lng="hu">
           <trd>meg&oacute;v</trd>,
           <trd>megk&iacute;m&eacute;l</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eskap.0i">eskapi</ref>,
      <ref cel="kur.for0i">forkuri</ref>,
      <ref cel="sav">sin forsavi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_z;&c_mol;&c_b;&c_ja;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>meiden</trd>,
    <trd>vermeiden</trd>,
    <trd>ausweichen</trd>
  </trdgrp>
  <trd lng="en">avoid</trd>
  <trd lng="es">evitar</trd>
  <trd lng="fr">&eacute;viter<klr tip="ind"> (qn ou qc)</klr></trd>
  <trdgrp lng="hu">
     <trd>ker&uuml;l</trd>,
     <trd>elker&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_b;&c_je;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">undvika</trd>
</drv>
<drv mrk="evit.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Penanta <tld/>i la malfacila&jcirc;ojn:
      <ekz>
        <tld/>ema respondo;
      </ekz>
      <ekz>
        li <tld/>eme respondis la demandojn<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="preciz">malpreciza</ref>,
      <ref cel="gard">singarda</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_h;&c_ib;&c_l;&c_ib;&c_s;&c_t;&c_y;</trd>
  <trd lng="de">ausweichend</trd>
  <trdgrp lng="fr">
    <trd>&eacute;vasif</trd>,
    <trd>fuyant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kit&eacute;r&odblac;</trd>,
    <trd>elker&uuml;l&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_k;&c_l;&c_o;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_s;&c_t;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="evit.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Kiun oni povas <tld/>i:
      <ekz>
        <ind>ne<tld/>ebla</ind> (<ref tip="sin" cel="nepr">nepre</ref>,
        <ref tip="vid" cel="kondam.0i">kondamni</ref>)
        estas la morto.
        <trd lng="be">&c_n;&c_je;&c_p;&c_a;&c_z;&c_mol;&c_b;&c_je;&c_zh;&c_n;&c_y;</trd>
        <trdgrp lng="de">
           <trd>unvermeidbar</trd>,
           <trd>unvermeidlich</trd>
        </trdgrp>
        <trd lng="es">inevitable</trd>
        <trdgrp lng="fr">
          <trd>in&eacute;luctable</trd>,
          <trd>in&eacute;vitable</trd></trdgrp>
        <trd lng="hu">elker&uuml;lhetetlen</trd>
        <trd lng="ru">&c_n;&c_je;&c_i;&c_z;&c_b;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">vermeidbar</trd>
  <trd lng="es">evitable</trd>
  <trd lng="fr">&eacute;vitable</trd>
  <trd lng="hu">elker&uuml;lhet&odblac;</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je; &c_n;&c_je;&c_i;&c_z;&c_b;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="evit.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kontra&ubreve;stari, kontra&ubreve;iri, spiti:
      <ekz>
        kura&gcirc;e mal<tld/>i la dan&gcirc;eron.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>braver</trd>,
    <trd>d&eacute;fier</trd>,
    <trd>provoquer<klr tip="ind"> (d&eacute;fier)</klr></trd>
  </trdgrp>
  <trd lng="hu">szembesz&aacute;ll</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_o;&c_s;&c_a;&c_t;&c_mol; <ind>&c_v;&c_y;&c_z;&c_o;&c_v;</ind></trd>,
    <trd>&c_i;&c_d;&c_t;&c_i; <ind>&c_n;&c_a;&c_v;&c_s;&c_t;&c_r;&c_je;&c_ch;&c_u;</ind><klr tip="amb"> (&c_o;&c_p;&c_a;&c_s;&c_n;&c_o;&c_s;&c_t;&c_i;)</klr></trd>
  </trdgrp>
</drv>
</art>
