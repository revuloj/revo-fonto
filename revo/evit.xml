<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>evit</rad>/i
</kap>
<drv mrk="evit.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Peni ne renkonti iun:
      <ekz>
        <tld/>i la babilulojn.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Peni ne esti trafita de io:
      <ekz>
        <tld/>i frapon, dan&gcirc;eron, morton, eraron;
      </ekz>
      <ekz>
        ankora&ubreve; neniu <tld/>is la sorton
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        li <tld/>igis <klr>(&scirc;paris)</klr> al mi
        multajn erarojn.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kur.for0i">forkuri</ref>,
      <ref cel="sav">sin forsavi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>meiden</trd>,
    <trd>vermeiden</trd>,
    <trd>ausweichen</trd>
  </trdgrp>
  <trd lng="en">avoid</trd>
</drv>
<drv mrk="evit.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Penanta <tld/>i la malfacila&jcirc;ojn:
      <ekz>
        <tld/>ema respondo;
      </ekz>
      <ekz>
        li <tld/>eme respondis la demandojn<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="preciz">malpreciza</ref>,
      <ref cel="gard">singarda</ref>.
    </refgrp>
  </snc>
  <trd lng="de">ausweichend</trd>
</drv>
<drv mrk="evit.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Kiun oni povas <tld/>i: 
      <ekz>
        ne<tld/>ebla (<ref tip="sin" cel="nepr">nepre</ref>,
        <ref tip="vid" cel="kondam.0i">kondamni</ref>)
        estas la morto. 
      </ekz>
    </dif>
  </snc>
  <trd lng="de">vermeidbar</trd>
</drv>
<drv mrk="evit.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kontra&ubreve;stari, kontra&ubreve;iri, spiti:
      <ekz>
        kura&gcirc;e mal<tld/>i la dan&gcirc;eron.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
<!--
$Log$
-->
</vortaro>