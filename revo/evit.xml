<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: evit.xml,v 1.35 2024/11/11 11:23:18 revo Exp $">
<kap>
<ofc>*</ofc>
  <rad>evit</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="evit.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="evit.0i.iun">
    <dif>
      Peni ne <ref tip="ant" cel="renkon.0i.kunigxi">renkonti</ref> iun:
      <ekz>
        li estis malestimata kaj <tld/>ata de homoj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 53:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> tute <tld/>as la virinojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Tutmozis</nom> <tld/>is la faraonon kaj ne kura&gcirc;is rigardi en liajn okulojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelke da personoj <klr>[&#8230;]</klr> okupas la tutan lar&gcirc;on de la trotuaro, <tld/>i
        ilin ne estas eble
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        pli facile estas <tld/>i ol spiti
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>stroni&cacute;<klr> (od kogo&sacute;)</klr></trd>,
      <trd>unika&cacute;<klr> (kogo&sacute;)</klr></trd>,
      <trd>wystrzega&cacute; si&eogonek;<klr>
      (kogo&sacute;)</klr></trd>,
      <trd>omija&cacute;</trd>,
      <trd>wymija&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="evit.0i.ion">
    <dif>
      Peni ne esti trafita de io:
      <ekz>
        <tld/>i frapon, dan&gcirc;eron, morton, eraron;
      </ekz>
      <ekz>
        la vojo de virtuloj <tld/>as malbonon
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 16:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u profanajn babiladojn
        <fnt>
          <bib>NT</bib>
          <lok>II. Timoteo 2:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        via patro regas tridek tri jarojn kaj <tld/>as militojn, kiuj povus esti pereigaj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        por <tld/>i &ccirc;ian malkompreni&gcirc;on, mi tuj en la komenco atentigas vin, ke mia parolo
        ne estas io oficiala
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto
          1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi vidis, ke vi penas <tld/>i miajn demandojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la grafino <klr>[&#8230;]</klr> <tld/>is ke Valento trovi&gcirc;u en &scirc;ia &ccirc;eesto
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        la du fratoj <tld/>as sin montri kune
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        zorgege <tld/>i &ccirc;ion, kio povus naski e&ccirc; ombron de dubo
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        por <tld/>i &ccirc;iuspecan akcidenton, mi estis metinta la lulilon malproksime de l&#8217;
        kameno
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        por <tld/>i ke oni entute devu cerbumi pri tio, <klr>[&#8230;]</klr> redaktoroj tuj
        konfirmu la alvenon de manuskripto
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="https://www.monato.be/2011/010436.php">&Scirc;topi&gcirc;oj</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi konas vin, <ind>labor<tld/>ulo</ind>
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 7 (105)</lok>
        </fnt>;
        <trd lng="de">Arbeitsscheuer</trd>
      </ekz>
      <ekz>
        serpenton oni povas <tld/>i, sed kalumnion neniam
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu multe profitas, anka&ubreve; perdon ne <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ankora&ubreve; neniu <tld/>is la sorton
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>stroni&cacute;<klr> (od czego&sacute;)</klr></trd>,
      <trd>unika&cacute;<klr> (czego&sacute;)</klr></trd>,
      <trd>wystrzega&cacute; si&eogonek;<klr>
      (czego&sacute;)</klr></trd>
    </trdgrp>
  </snc>
  <refgrp tip="ant">
    <ref cel="front.al0i">alfronti</ref>,
    <ref cel="spit.0i">spiti</ref>
  </refgrp>
  <refgrp tip="vid">
    <ref cel="eskap.0i">eskapi</ref>,
    <ref cel="kur.for0i">forkuri</ref>,
    <ref cel="sav">sin forsavi</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_z;&c_mol;&c_b;&c_ja;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_h;&c_ib;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obch&aacute;zet</trd>,
    <trd>obej&iacute;t</trd>,
    <trd>p&rcaron;edch&aacute;zet</trd>,
    <trd>stranit se</trd>,
    <trd>st&rcaron;e&zcaron;it se</trd>,
    <trd>vyhnout se</trd>,
    <trd>vyh&yacute;bat se</trd>,
    <trd>vymykat se</trd>,
    <trd>zabr&aacute;nit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>meiden</trd>,
    <trd>vermeiden</trd>,
    <trd>ausweichen</trd>
  </trdgrp>
  <trd lng="en">avoid</trd>
  <trd lng="es">evitar</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_w;&a_r;&f_y; &a_j;&a_s;&a_t;&a_n;<klr>
    (&a_A;&a_z;)</klr></trd>,
    <trd>&a_A;&a_j;&a_t;&a_n;&a_A;&a_b; &f_k;&a_r;&a_d;&a_n;<klr>
    (&a_A;&a_z;)</klr></trd>
  </trdgrp>
  <trd lng="fr">&eacute;viter<klr tip="ind"> (qn ou qc)</klr></trd>
  <trdgrp lng="hu">
     <trd>ker&uuml;l</trd>,
     <trd>elker&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x907F;&#12369;&#x308B;  <pr>&#12373;&#12369;&#x308B;</pr></trd>,
    <trd>&#x56DE;&#x907F;&#12377;&#x308B;  <pr>&#x304B;&#12356;&#12402;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>evitar</trd>,
    <trd>evadir</trd>,
    <trd>esquivar</trd>,
    <trd>poupar</trd>,
    <trd>prevenir</trd>,
    <trd>presumir</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_b;&c_je;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ob&iacute;s&tcaron;</trd>,
    <trd>str&aacute;ni&tcaron; sa</trd>,
    <trd>vyh&yacute;ba&tcaron; sa</trd>
  </trdgrp>
  <trd lng="sv">undvika</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_n;&c_i;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_i;&c_n;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_c;&c_u;&c_r;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_t;&c_r;&c_i;&c_m;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_o;&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_mol;</trd>,
    <trd>&c_u;&c_t;&c_r;&c_i;&c_m;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_u;&c_h;&c_i;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8FF4;&#x907F; <pr>hu&#xED;b&#xEC;</pr></trd>,
    <trd>&#x514D; <pr>mi&#x1CE;n</pr></trd>,
    <trd>&#x8F9F; <pr>b&#xEC;</pr></trd>,
    <trd>&#x898F;&#x907F; <pr>gu&#x12B;b&#xEC;</pr></trd>,
    <trd>&#x907F;&#x514D; <pr>b&#xEC;mi&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="evit.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="evit.0ema.KOMUNE">
    <dif>
      Penanta <tld/>i malfacila&jcirc;ojn:
      <ekz>
        <nom>Friedrich</nom> <tld/>eme levas la &scirc;ultrojn
        &ndash; &ccirc;u mi mensogas?
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la lerte <tld/>emaj respondoj de <nom>Zalmuna</nom> ne kontentigis Omaron
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi sendube rimarkis, kiel <tld/>eme li respondis al miaj pli personaj demandoj
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        hom<tld/>ema vagabondo
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="preciz">malpreciza</ref>,
      <ref cel="gard">singarda</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_h;&c_ib;&c_l;&c_ib;&c_s;&c_t;&c_y;</trd>
  <trd lng="cs">vyh&yacute;bav&yacute;</trd>
  <trd lng="de">ausweichend</trd>
  <trd lng="fa">&a_m;&a_sh;&f_k;&a_l;&#x200C;&f_g;&a_r;&f_y;&a_z;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;vasif</trd>,
    <trd>fuyant</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kit&eacute;r&odblac;</trd>,
    <trd>elker&uuml;l&odblac;</trd>
  </trdgrp>
  <trd lng="ja">&#36867;&#12370;&#33136;&#x306E;  <pr>&#x306B;&#12370;&#12372;&#12375;&#x306E;</pr></trd>
  <trd lng="pl">wymijaj&aogonek;cy</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_k;&c_l;&c_o;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_s;&c_t;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">vyh&yacute;bav&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_n;&c_i;&c_k;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_je;&c_r;&c_je;&c_zh;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="evit.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="evit.0ebla.KOMUNE">
    <dif>
      Kiun oni povas <tld/>i:
      <ekz>
        &ccirc;e multo da vortoj ne <tld/>ebla estas peko
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 10:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        eraroj malfacile estas <tld/>eblaj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i devas ser&ccirc;i <klr>[lin]</klr> sen ia <tld/>ebla prokrasto, kaj jam faris &ccirc;iujn
        necesajn preparojn
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi cerbumadis pri tio, kiel estas <tld/>eblaj la ekonomiaj katastrofoj
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_k;&c_o;&c_g;&c_a; &c_m;&c_o;&c_zh;&c_n;&c_a; <ind>&c_p;&c_a;&c_z;&c_mol;&c_b;&c_je;&c_g;&c_n;&c_u;&c_c;&c_mol;</ind></trd>
  <trd lng="de">vermeidbar</trd>
  <trd lng="es">evitable</trd>
  <trd lng="fa">&a_A;&a_j;&a_t;&a_n;&a_A;&a_b;&#x200C;&f_p;&a_dh;&f_y;&a_r;</trd>
  <trd lng="fr">&eacute;vitable</trd>
  <trd lng="hu">elker&uuml;lhet&odblac;</trd>
  <trd lng="ja">&#x907F;&#12369;&#x5F97;&#x308B;  <pr>&#12373;&#12369;&#12360;&#x308B;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je; &c_n;&c_je;&c_i;&c_z;&c_b;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_a;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_shch;&c_o; &c_j;&c_o;&c_g;&c_o; &c_m;&c_o;&c_zh;&c_n;&c_a; &c_u;&c_n;&c_i;&c_k;&c_n;&c_u;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x53EF;&#38450;&#x6B62; <pr>k&#x11B;f&#xE1;ngzh&#x1D0;</pr></trd>,
    <trd>&#x53EF;&#x907F;&#x514D; <pr>k&#x11B;b&#xEC;mi&#x1CE;n</pr></trd>,
    <trd>&#x53EF;&#x898F;&#x907F; <pr>k&#x11B;gu&#x12B;b&#xEC;</pr></trd>,
    <trd>&#x53EF;&#36867;&#x907F; <pr>k&#x11B;t&#xE1;ob&#xEC;</pr></trd>,
    <trd>&#x53EF;&#38928;&#38450; <pr>k&#x11B;y&#xF9;f&#xE1;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="evit.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="evit.0igi.KOMUNE">
    <dif>
      Helpi al <tld/>ado;
      <ref tip="vid" cel="sxpar.0igi.evitigi">&scirc;parigi</ref>:
      <ekz>
        li <tld/>igis al mi multajn erarojn;
      </ekz>
      <ekz>
        &ccirc;u vi ne povas <tld/>igi al mi la doloron
        <fnt>
          <bib>ChL</bib>
        </fnt>?
      </ekz>
      <ekz>
        mi efektive deziris <tld/>igi al vi la jam delonge minacantan konflikton
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; sciencistoj la industri&scirc;tatoj devas redukti sian CO2-emisiadon je 80&#x202F;% &gcirc;is
        la jaro 2050 por <tld/>igi klimatkolapson
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008735.php">&Ccirc;u la tero fari&gcirc;os sa&ubreve;no?</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="indulg.0i">indulgi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_r;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_je;&c_r;&c_a;&c_g;&c_ch;&c_y; &c_a;&c_d; &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>odvr&aacute;tit</trd>,
    <trd>p&rcaron;edej&iacute;t</trd>,
    <trd>zabr&aacute;nit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ersparen</trd>,
    <trd>davor bewahren</trd>,
    <trd>vermeidbar machen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;pargner<klr tip="ind"> (qc &agrave;
    qn)</klr></trd>,
    <trd>&eacute;viter<klr tip="ind"> (qc &agrave;
    qn)</klr></trd></trdgrp>
  <trdgrp lng="hu">
    <trd>meg&oacute;v</trd>,
    <trd>megk&iacute;m&eacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x907F;&#12369;&#12373;&#x305B;&#x308B;  <pr>&#12373;&#12369;&#12373;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x514D;&#x308C;&#12373;&#x305B;&#x308B;
    <pr>&#x307E;&#x306C;&#x304C;&#x308C;&#12373;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odvr&aacute;ti&tcaron;</trd>,
    <trd>pred&iacute;s&tcaron;</trd>,
    <trd>zabr&aacute;ni&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_p;&c_o;&c_b;&c_ib;&c_g;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_v;&c_je;&c_r;&c_t;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="evit.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="evit.ne0ebla.KOMUNE">
    <dif>
      Kiun oni ne povas <tld/>i:
      <ekz>
        ne<tld/>eblaj disrevi&gcirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi demandos pri kelkaj ne<tld/>eblaj detala&jcirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la senvola ekmovo de returneniro <klr>[&#8230;]</klr> lin savis
        de <klr>(preska&ubreve;)</klr> ne<tld/>ebla morto
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        tediga sed ne<tld/>ebla afero
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        kvaza&ubreve; &gcirc;i estus natura ne<tld/>ebla&jcirc;o, la junulino toleris la fatalon de l&#8217; amo
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        kvankam &scirc;i atendis tiun ne<tld/>eblan katastrofon, tamen &scirc;i ne kredis, ke &gcirc;i
        estas tiel proksima
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fatal.0a.nepra">fatala<sncref/></ref>,
      <ref cel="nepr.0a">nepra</ref>
    </refgrp>
