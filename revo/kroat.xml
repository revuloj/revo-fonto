<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kroat</rad>/o
  <ofc>*</ofc>
</kap>

<drv mrk="kroat.0ujo">
  <kap>
    <tld lit="K"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="kroat.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la suda <ref tip="malprt"
      cel="euxrop1.E0o.geog">E&ubreve;ropo</ref>
      kun &ccirc;efurbo Zagrebo.
      Teritorio de Respubliko Kroatujo.
      ISO-3166 kodo: HR.
      &Gcirc;i limas kun
      <ref tip="vid" cel="bosni.0ohercegovino.sxtato">Bosnujo kaj
      Hercegovino</ref>,
      <ref tip="vid" cel="hungar.H0ujo">Hungarujo</ref>,
      <ref tip="vid" cel="monteneg.0o.GEOG">Montenegro</ref>,
      <ref tip="vid" cel="serb.0ujo.GEOG">Serbujo</ref>,
      <ref tip="vid" cel="sloven.0ujo.GEOG">Slovenujo</ref>
      kaj Adriatiko.
    </dif>
    <ref tip="sin" cel="kroati.0o.GEOG">Kroatio</ref>
    <trdgrp lng="pl">
      <trd>Chorwacja</trd>,
      <trd>Republika <ind>Chorwacji</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

<!--
$Log$
-->
</vortaro>