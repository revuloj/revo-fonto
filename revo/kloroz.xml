<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kloroz</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kloroz.0o">
  <kap><tld/>o</kap>
  <snc mrk="kloroz.0o.MED">
    <uzo tip="fak">MED</uzo>
    <fnt><bib>PV</bib></fnt>
    <dif>
      Malsano de la sango &ccirc;e maturi&gcirc;antaj knabinoj,
      ka&ubreve;zata de malplii&gcirc;o de hemoglobino kaj de ru&gcirc;aj
      globetoj, kaj karakterizata de palo de
      la ha&ubreve;to kaj manko de l' fortoj.
    </dif>
    <trd lng="de">Bleichsucht</trd>
    <trdgrp lng="en">
      <trd>chlorosis</trd>,
      <trd>iron-deficiency <ind>anemia</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>an&eacute;mie hypochrome</trd>,
      <trd>chlorose<klr tip="ind"> (an&eacute;mie hypochrome)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bloedarmoede</trd>,
      <trd>chlorose</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_zh;&c_je;&c_l;&c_je;&c_z;&c_o;&c_d;&c_je;&c_f;&c_i;&c_c;&c_i;&c_t;&c_n;&c_a;&c_ja; <ind>&c_a;&c_n;&c_je;&c_m;&c_i;&c_ja;</ind></trd>,
      <trd>&c_h;&c_l;&c_o;&c_r;&c_o;&c_z;</trd>,
      <trd>&c_b;&c_l;&c_je;&c_d;&c_n;&c_a;&c_ja; &c_n;&c_je;&c_m;&c_o;&c_ch;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kloroz.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Planta malsano, karakterizita per flavi&gcirc;o a&ubreve;
      blanki&gcirc;o de la verdaj plantopartoj.
    </dif>
    <trd lng="fr">chlorose<klr tip="ind"> (bot.)</klr></trd>
  </snc>
</drv>

<drv mrk="kloroz.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al la <tld/>o.
    </dif>
  </snc>
  <trd lng="de">bleichs&uuml;chtig</trd>
  <trd lng="en">anemic</trd>
  <trd lng="fr">chlorotique</trd>
</drv>
</art>
