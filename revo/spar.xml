<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spar.xml,v 1.16 2025/10/19 14:00:21 revo Exp $">
<kap>
  <rad>spar</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="spar.0o">
  <kap><tld/>o</kap>
  <snc mrk="spar.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro de maraj fi&scirc;oj kun mallar&gcirc;a korpo, arka dorso (<trd lng="la">Sparus</trd>):
      <ekz>
        kiun fi&scirc;on, &ccirc;u <tld/>on, a&ubreve; tinuson, oni ne scias
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;afablaj viroj, kiuj
        man&gcirc;as
        fi&scirc;ojn&rightquot;, p. 104a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_a;&c_r;</trd>
  <trd lng="cs">pod&eacute;ln&iacute;k stroje nebo dopravn&iacute;ho prost&rcaron;edku</trd>
  <trd lng="de">Meerbrassen</trd>
  <trdgrp lng="en">
    <trd>Sparus</trd>, <trd>sparus</trd>
  </trdgrp>
  <trd lng="fr">daurade</trd>
  <trd lng="hu">tengeri <ind>durbincs</ind></trd>
  <trdgrp lng="ja">
    <trd>&#12463;&#12525;&#12480;&#12452;</trd>,
    <trd>&#39899;  <pr>&#12383;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">brasem</trd>
  <trd lng="pl">dorada</trd>
  <trd lng="sk">pozd&lacute;&zcaron;nik stroja alebo dopravn&eacute;ho prostriedku</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_o;&c_n;&c_zh;&c_je;&c_r;&c_o;&c_n; (&c_k;&c_r;&c_i;&c_l;&c_a;)</trd>,
    <trd>&c_r;&c_a;&c_n;&c_g;&c_o;&c_u;&c_t;&c_n;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#39899; <pr>di&amacron;o</pr></trd>,
    <trd>&#40119; <pr>di&amacron;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="spar.ora0o">
  <kap>ora <tld/>o <fnt><bib>PIV2</bib></fnt></kap>
  <snc mrk="spar.ora0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de fi&scirc;o trovebla en Mediteraneo, kun orkoloraj
      makuloj sur la kapo, man&gcirc;anta konkojn
      (<trd lng="la">Sparus aurata</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_s;&c_t;&c_y; <ind>&c_s;&c_p;&c_a;&c_r;</ind></trd>,
    <trd>&c_a;&c_w;&c_r;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_d;&c_a;&c_r;&c_a;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>orada</trd>,
    <trd>aurada</trd>,
    <trd>dorada</trd>,
    <trd>daurada</trd>,
    <trd>moixarra</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Goldbrasse</trd>,
    <trd>Dorade</trd>
  </trdgrp>
   <trd lng="en">gilt-head sea bream</trd>
  <trd lng="es">dorada</trd>
  <trd lng="fr">daurade royale</trd>
  <trdgrp lng="hu">
    <trd>aranydurbincs</trd>,
    <trd>aranyfej&udblac; tengeri <ind>keszeg</ind></trd>
  </trdgrp>
  <trd lng="nl">goudbrasem</trd>
  <trdgrp lng="pl">
    <trd>dorada</trd>,
    <trd>sparus z&lstroke;otog&lstroke;owy</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#37329;&#22836;&#40119; <pr>j&#299;nt&oacute;udi&amacron;o</pr></trd>,
    <trd>&#37329;&#38957;&#39899; <pr>j&#299;nt&oacute;udi&amacron;o</pr></trd>
  </trdgrp>
</drv>
</art>

