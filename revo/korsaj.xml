<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>korsa&jcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="korsaj.0o">
  <kap><tld/>o</kap>
  <snc mrk="korsaj.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Parto de virina vesto, kovranta la buston:
      <ekz>
        dekoltita <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_s;&c_a;&c_zh;</trd>
  <trdgrp lng="de">
    <trd>Leibchen</trd>,
    <trd>Mieder</trd>,
    <trd>Korsage</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bodice</trd>,
    <trd>body</trd>,
    <trd>corsage</trd>
  </trdgrp>
  <trd lng="fr">corsage</trd>
  <trdgrp lng="hu">
    <trd>pruszlik</trd>,
    <trd>mell&eacute;nyke</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_s;&c_a;&c_zh;</trd>,
    <trd>&c_l;&c_i;&c_f;</trd>
  </trdgrp>
</drv>
</art>
