<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: korsaj.xml,v 1.19 2022/05/01 08:09:25 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>korsa&jcirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="korsaj.0o">
  <kap><tld/>o</kap>
  <snc mrk="korsaj.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Parto de virina <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="vest.0o.TEKS">vesto</ref>, 
      kovranta la buston:
      <ekz>
        &scirc;i fari&gcirc;is furioza, <klr>[&#8230;]</klr> dis&scirc;iris la <tld/>on kaj ekbatis la edzon per
        la piedo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fi&scirc;kaptisto kaj lia edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        butonumante la <tld/>on, &scirc;i ekstaris anta&ubreve; la fenestro
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        nigraj bukloj volvi&gcirc;is sur la klinita nuko kaj sur la kolumo de la <tld/>o
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        pa&scirc;tistino vestita de blua jupo, de ru&gcirc;a <tld/>o, kun &scirc;ultroj nudaj, kvaza&ubreve; &scirc;i
        estus ironta al kortega balo
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="bluz.0o">bluzo</ref>,
       <ref cel="kamizo.0o.manikhava">kamizolo</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/b/b0/Lady_with_a_corsage_tarbell.jpg/640px-Lady_with_a_corsage_tarbell.jpg"
      prm="PD"
      >sinjorino kun <tld/>o, pentra&jcirc;o de Edmund Charles Tarbell, 1911
        <fnt>
          <aut>Edmund Charles Tarbell</aut>
          <lok><url ref="&WCW;/File:Lady_with_a_corsage_tarbell.jpg"
              >&WCW;/File:Lady_with_a_corsage_tarbell.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_s;&c_a;&c_zh;</trd>
  <trdgrp lng="de">
    <trd>Leibchen</trd>,
    <trd>Mieder</trd>,
    <trd>Korsage</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bodice</trd>,
    <trd>body</trd>,
    <trd>corsage</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_n;&f_y;&a_m;&zwnj;&a_t;&a_n;&a_h;</trd>,
    <trd>&a_b;&a_A;&a_l;&a_A;&a_t;&a_n;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">corsage</trd>
  <trd lng="hu">ruhader&eacute;k</trd>
  <trd lng="id">korsase</trd>
  <trdgrp lng="pl">
    <trd>biustonosz</trd>,
    <trd>stanik</trd>,
    <trd>gorset</trd>
  </trdgrp>   
  <trdgrp lng="pt">
    <trd>busti&ecirc;</trd>,
    <trd>top</trd>,
    <trd>corpete <klr tip="amb">(&ecirc;)</klr></trd>,
    <trd>corpinho</trd>,
    <trd>corselete <klr tip="amb">(&ecirc;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>brasier&abreve;</trd>,
    <trd>sutien</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_s;&c_a;&c_zh;</trd>,
    <trd>&c_l;&c_i;&c_f;</trd>
  </trdgrp>
</drv>
</art>
