<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>korsa&jcirc;</rad>/o
</kap>
<drv mrk="korsaj.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Parto de virina vesto, kovranta la buston:
      <ekz>
        dekoltita <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>bodice</trd>,
    <trd>body</trd>,
    <trd>corsage</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_s;&c_a;&c_zh;</trd>,
    <trd>&c_l;&c_i;&c_f;</trd>
  </trdgrp>
</drv>
</art>
