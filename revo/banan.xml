<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>banan</rad>/o
</kap>
<drv mrk="banan.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Frukto de la <ref tip="malprt" cel="banan.0ujo"><tld/>ujo</ref>,
      flav&scirc;ela, kurbe longa; kelkaj specoj estas tre bongustaj.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_n;&c_a;&c_n; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="de">Banane</trd>
  <trdgrp lng="es">
     <trd>banana</trd>,
     <trd>platano</trd>
  </trdgrp>
  <trd lng="en">banana</trd>
  <trd lng="fr">banane</trd>
  <trd lng="hu">ban&aacute;n</trd>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_a;&c_n;</trd>
  <trd lng="nl">banaan</trd>
  <trd lng="pl">banan</trd>
  <trd lng="sk">ban&aacute;n</trd>
  <trd lng="tr">muz</trd>
  <trd lng="sv">banan</trd>
</drv>
<drv mrk="banan.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="banan1.0oj"><tld/>ujacoj</ref>
      (<trd lng="la">Musa</trd>), tropika vegeta&jcirc;o kun multaj
      specioj. La fruktoj de iuj specioj estas man&gcirc;ataj, la
      folioj de aliaj specioj liveras fibrojn uzatajn por &scirc;nuroj,
      retoj ktp.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_n;&c_a;&c_n; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="en">banana tree</trd>
  <trd lng="de">Bananenstaude</trd>
  <trd lng="fr">bananier</trd>
  <trd lng="hu">ban&aacute;nfa</trd>
  <trd lng="nl">bananenboom</trd>
  <trd lng="sk">ban&aacute;novn&iacute;k</trd>
</drv>

</art>
