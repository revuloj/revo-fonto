<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>banan</rad>/o
</kap>
<drv mrk="banan.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Frukto de la <ref tip="malprt" cel="banan.0ujo"><tld/>ujo</ref>,
      flav&scirc;ela, kurbe longa; kelkaj specoj estas tre bongustaj.
    </dif>
  </snc>
  <trd lng="de">Banane</trd>
  <trd lng="fr">banane</trd>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_a;&c_n;</trd>
  <trd lng="tr">muz</trd>
</drv>
<drv mrk="banan.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
   <dif>
     Genro el familio <ref tip="super" cel="banan.0ujacoj"><tld/>ujacoj</ref>
     (<trd lng="la">Musa</trd>), tropika vegeta&jcirc;o kun multaj
     specioj. La fruktoj de iuj specioj estas man&gcirc;ataj, la
     folioj de aliaj specioj liveras fibrojn uzatajn por &scirc;nuroj,
     retoj ktp.
    </dif>
  </snc>
  <trd lng="de">Bananenstaude</trd>
</drv>
<drv mrk="banan.0ujacoj">
  <kap><tld/>ujacoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
       Familio de tropikaj kaj subtropikaj plantoj
       (<trd lng="la">Musaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Bananengew&auml;chse</trd>
</drv>
</art>
