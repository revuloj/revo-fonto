<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>prometi</rad>/o
</kap>
<drv mrk="prometi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Pm</mlg>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj" val="61">elemento</ref> kun
      atomnumero 61, atompezo 146,9151; ar&gcirc;entgriza 
      <ref tip="super" cel="metal.0o">metalo</ref>, kies  
      &ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref>
      radioaktivaj; lantanoido.
    </dif>
    <ref tip="sin" cel="promeci.0o">promecio</ref>
    <rim>
      Eksaj nomoj de la
      elemento estas ilinio kaj florentino.
    </rim>
  </snc>                         
  <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_e;&c_t;&c_y;&c_j;</trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_m;&c_je;&c_t;&c_i;&c_j;</trd>
  <trd lng="br">prometiom</trd>
  <trd lng="ca">prometi</trd>
  <trd lng="de">Promethium</trd>
  <trd lng="en">promethium</trd>
  <trd lng="es">prometio</trd>
  <trd lng="fr">prom&eacute;thium</trd>
  <trd lng="hu">prom&eacute;tium</trd>
  <trd lng="id">prometium</trd>
  <trd lng="it">promezio</trd>
  <trd lng="la">Promethium</trd>
  <trd lng="nl">promethium</trd>
  <trd lng="pl">promet</trd>
  <trdgrp lng="pt">
    <trd>prom&eacute;cio</trd>,
    <trd>prom&eacute;tio</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_m;&c_je;&c_t;&c_i;&c_j;</trd>
  <trd lng="sv">prometium</trd>
</drv>
</art>

