<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>prometi</rad>/o
</kap>
<drv mrk="prometi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 61, atompezo 146,9151, simbolo Pm. Eksaj nomoj de la
      elemento estis ilinio kaj florentino. &Ccirc;iuj izotopoj de tiu elemento
      estas radioaktivaj.
    </dif>
    <ref tip="sin" cel="promeci.0o">promecio</ref>
  </snc>                          
  <trd lng="be">&c_p;&c_r;&c_a;&c_m;&c_e;&c_t;&c_y;&c_j;</trd>
  <trd lng="br">prometiom</trd>
  <trd lng="de">Promethium</trd>
  <trd lng="en">promethium</trd>  
  <trd lng="fr">prom&eacute;thium</trd>
  <trd lng="hu">prom&eacute;tium</trd>
  <trd lng="nl">promethium</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_m;&c_je;&c_t;&c_i;&c_j;</trd>
</drv>
</art>

