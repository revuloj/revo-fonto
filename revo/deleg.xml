<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: deleg.xml,v 1.29 2024/08/26 18:36:10 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>deleg</rad>/i
  <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="deleg.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sendi iun kiel reprezentanton:
      <ekz>
        nia societo <tld/>is tri membrojn al la kongreso;
      </ekz>
      <ekz>
        por fari reformojn en Esperanto, la <ctl>Delegacia
        Komitato</ctl>
        ricevis komision nek de la esperantistoj, nek de siaj propraj
        <ind>deleginto</ind>j
        <fnt><bib>OV</bib>, <lok>p. 447</lok></fnt>.
        <trd lng="fr">d&eacute;l&eacute;gant</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="deput.0i">deputi</ref>,
      <ref cel="komisi.0i">komisii</ref>,
      <ref cel="misi">misii</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_l;&c_je;&c_g;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">delegar</trd>
  <trd lng="cs">delegovat</trd>
  <trdgrp lng="de">
    <trd>delegieren</trd>,
    <trd>abordnen</trd>
  </trdgrp>
  <trd lng="en">delegate</trd>
  <trd lng="es">delegar</trd>
  <trd lng="fr">d&eacute;l&eacute;guer<klr tip="ind"> (qn)</klr></trd>
  <trd lng="he">&#x5DC;&#x5D9;&#x5D9;&#x5E6;&#x5D2;</trd>
  <trdgrp lng="hu">
    <trd>kik&uuml;ld<klr tip="ind"> (deleg&aacute;l)</klr></trd>,
    <trd>deleg&aacute;l</trd>
  </trdgrp>
  <trd lng="ils"
  kod="M531x576S2ff00482x483S20300493x545S20308493x561S37704507x551S3770e469x568S26620479x533"></trd>
  <trdgrp lng="ja">
    <trd>&#36865;&#x308A;&#x51FA;&#12377;  <pr>&#x304A;&#x304F;&#x308A;&#12384;&#12377;</pr></trd>,
    <trd>&#x4EFB;&#x547D;&#12377;&#x308B;  <pr>&#x306B;&#12435;&#12417;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x59D4;&#x4EFB;&#12377;&#x308B;  <pr>&#12356;&#x306B;&#12435;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>delegowa&cacute;</trd>,
    <trd>wysy&lstroke;a&cacute; <ind>delegacj&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>delegar</trd>,
    <trd>deputar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_je;&c_g;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_l;&c_a;&c_t;&c_mol;
    &c_d;&c_je;&c_l;&c_je;&c_g;&c_a;&c_t;&c_o;&c_m;</trd>
  </trdgrp>
  <trd lng="sgn"
  kod="M533x518S2ff00483x483S36d00479x498S15549470x457S15541506x456S20500497x450S22200483x450S26502501x468S26516487x468S22200511x450">W@33-</trd>
  <trdgrp lng="sk">
    <trd>delegova&tcaron;</trd>,
    <trd>vysla&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_s;&c_i;&#x43B;&c_a;&c_t;&c_i; &c_d;&c_je;&#x43B;&c_je;&c_gu;&c_a;&c_t;&#x43E;&#x43C;</trd>,
    <trd>&c_d;&c_je;&#x43B;&c_je;&c_gu;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="deleg.0ato">
  <kap><tld/>ato<fnt>PV</fnt></kap>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="deleg.0ito">delegito.</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>deleg&#xE1;t</trd>,
    <trd>vyslanec</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>deleg&#xE1;t</trd>,
    <trd>&#x10D;len deleg&#xE1;cie</trd>
  </trdgrp>
</drv>

<drv mrk="deleg.0ito">
  <kap><tld/>ito</kap>
  <snc>
    <dif>
      <tld lit="D"/>ita persono:
      <ekz>
        en tiu tago &gcirc;i la unuan fojon devas aperi en la urba domo,
        por ke
        sinjoroj la urbaj delegitoj &gcirc;in ekzamenu, &ccirc;u
        &gcirc;i
        ankora&ubreve; estas uzebla
        <fnt><bib>Fab1</bib>, <lok>Malnova strata lanterno</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_l;&c_je;&c_g;&c_a;&c_t;</trd>
  <trd lng="ca">delegat</trd>
  <trdgrp lng="cs">
    <trd>deleg&#xE1;t</trd>,
    <trd>vyslanec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stellvertreter</trd>,
    <trd>Abgeordneter [en la parlamento]</trd>
  </trdgrp>
  <trd lng="en">delegate</trd>
  <trd lng="es">delegado</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;l&eacute;gu&eacute;</trd>,
    <trd>repr&eacute;sentant<klr tip="ind">
    (d&eacute;l&eacute;gu&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5E0;&#x5B8;&#x5E6;&#x5B4;&#x5D9;&#x5D2;</trd>
  <trdgrp lng="hu">
    <trd>k&uuml;ld&ouml;tt</trd>,
    <trd>k&eacute;pvisel&odblac;</trd>,
    <trd>megb&iacute;zott</trd>,
    <trd>deleg&aacute;tus</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4EE3;&#34920;&#32773;  <pr>&#12384;&#12356;&#12402;&#12423;&#12358;&#12375;&#12419;</pr></trd>,
    <trd>&#x4EE3;&#x8B70;&#x54E1;  <pr>&#12384;&#12356;&#x304E;&#12356;&#12435;</pr></trd>,
    <trd>&#x30C7;&#x30EC;&#x30AE;&#x30FC;&#x30C8;</trd>
  </trdgrp>
  <trd lng="nl">vertegenwoordiger</trd>
  <trd lng="no">representant</trd>
  <trd lng="pl">delegat</trd>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_je;&c_g;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>deleg&#xE1;t</trd>,
    <trd>&#x10D;len deleg&#xE1;cie</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_je;&#x43B;&c_je;&c_gu;&c_a;&c_t;</trd>
  <trd lng="zh">&#x4E1E; <pr>ch&#xE9;ng</pr></trd>
</drv>

<drv mrk="deleg.0itaro">
  <kap><tld/>itaro</kap>
  <snc>
    <dif>
      Aro da <tld/>itoj, komisiitaj por specifa tasko:
      <ekz>
        ne forgesu la delegitaron!, diris la re&gcirc;o Hroar
        <fnt><bib>Fab1</bib>, <lok>Malgranda Tuk</lok></fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_l;&c_je;&c_g;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">delegaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>delegace</trd>,
    <trd>deputace</trd>
  </trdgrp>
  <trd lng="en">delegation</trd>
  <trd lng="fr">d&eacute;l&eacute;gation<klr tip="ind"> (groupe de
  d&eacute;l&eacute;gu&eacute;s)</klr></trd>
  <trdgrp lng="hu">
    <trd>k&uuml;ld&ouml;tts&eacute;g</trd>,
    <trd>deleg&aacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>delegatie</trd>,
    <trd>vertegenwoordiging</trd>
  </trdgrp>
  <trd lng="no">delegasjon</trd>
  <trd lng="pl">delegacja</trd>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_je;&c_g;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">vl&#xE1;dna deleg&#xE1;cia</trd>
</drv>

<drv mrk="deleg.D0itaro">
  <kap><tld lit="D"/>itaro</kap>
  <uzo tip="fak">ESP</uzo>
  <snc>
    <ref tip="dif" cel="delega.D0o">Delegacio.</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>delegace</trd>,
    <trd>deputace</trd>
  </trdgrp>
  <trd lng="ja">&#x4EE3;&#34920;&#x56E3;  <pr>&#12384;&#12356;&#12402;&#12423;&#12358;&#12384;&#12435;</pr></trd>
  <trd lng="sk">vl&#xE1;dna deleg&#xE1;cia</trd>
  <trd lng="uk">&c_d;&c_je;&#x43B;&c_je;&c_gu;&c_a;&c_c;&c_ib;&#x44F;</trd>
</drv>
</art>
