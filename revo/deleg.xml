<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>deleg</rad>/i
</kap>
<drv mrk="deleg.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Sendi iun, kiel reprezentanton:
<ekz>nia societo <tld/>is tri membrojn al la kongreso.
</ekz>
</dif>
<refgrp tip="sin">
<ref cel="deput.0i">deputi</ref>,
<ref cel="komisi.0i">komisii</ref>,
<ref cel="misi">misii</ref>.
</refgrp>
</snc>
<trdgrp lng="de">
   <trd>delegieren</trd>,
   <trd>abordnen</trd></trdgrp>
<trdgrp lng="ru">
  <trd>&c_d;&c_je;&c_l;&c_je;&c_g;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
  <trd>&c_p;&c_o;&c_s;&c_l;&c_a;&c_t;&c_mol; &c_d;&c_je;&c_l;&c_je;&c_g;&c_a;&c_t;&c_o;&c_m;</trd>
</trdgrp>
</drv>
<drv mrk="deleg.0ato0ito">
<kap><tld/>ato, <tld/>ito</kap>
<snc>
<dif>
<tld/>ita persono.  
</dif>
</snc>
<trdgrp lng="de">
   <trd>Stellvertreter</trd>,
   <trd>Abgeordneter [en la parlamento]</trd></trdgrp>
<trd lng="ru">&c_d;&c_je;&c_l;&c_je;&c_g;&c_a;&c_t;</trd>
</drv>
</art>
