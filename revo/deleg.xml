<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>deleg</rad>/i
</kap>
<drv mrk="deleg.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sendi iun kiel reprezentanton:
      <ekz>
        nia societo <tld/>is tri membrojn al la kongreso;
      </ekz>
      <ekz>
        por fari reformojn en Esperanto, la <ctl>Delegacia Komitato</ctl>
        ricevis komision nek de la esperantistoj, nek de siaj propraj
        <ind>deleginto</ind>j
        <fnt><aut>&Z;</aut><vrk>Originala Verkaro</vrk><lok>p. 447</lok></fnt>.
        <trd lng="fr">d&eacute;l&eacute;gant</trd>
      </ekz>    
    </dif>
    <refgrp tip="vid">
      <ref cel="deput.0i">deputi</ref>,
      <ref cel="komisi.0i">komisii</ref>,
      <ref cel="misi">misii</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>delegieren</trd>,
    <trd>abordnen</trd>
  </trdgrp>
  <trd lng="es">delegar</trd>
  <trd lng="fr">d&eacute;l&eacute;guer<klr tip="ind"> (qn)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_je;&c_g;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_l;&c_a;&c_t;&c_mol; &c_d;&c_je;&c_l;&c_je;&c_g;&c_a;&c_t;&c_o;&c_m;</trd>
  </trdgrp>
</drv>
<drv mrk="deleg.0ato">
  <kap><tld/>ato<fnt>PV</fnt></kap>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="deleg.0ito">delegito.</ref>
  </snc>
</drv>
<drv mrk="deleg.0ito">
  <kap><tld/>ito</kap>
  <snc>
    <dif>
      <tld lit="D"/>ita persono:
      <ekz>
        en tiu tago &gcirc;i la unuan fojon devas aperi en la urba domo, por ke
        sinjoroj la urbaj delegitoj &gcirc;in ekzamenu, &ccirc;u &gcirc;i
        ankora&ubreve; estas uzebla
        <fnt><bib>Fab1</bib>, <lok>Malnova strata lanterno</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Stellvertreter</trd>,
    <trd>Abgeordneter [en la parlamento]</trd>
  </trdgrp>
  <trd lng="es">delegado</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;l&eacute;gu&eacute;</trd>,
    <trd>repr&eacute;sentant<klr tip="ind"> (d&eacute;l&eacute;gu&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_je;&c_g;&c_a;&c_t;</trd>
</drv>
<drv mrk="deleg.0itaro">
  <kap><tld/>itaro</kap>
  <snc>
    <dif>
      Aro da <tld/>itoj, komisiitaj por specifa tasko:
      <ekz>
        ne forgesu la delegitaron!, diris la re&gcirc;o Hroar
        <fnt><bib>Fab1</bib>, <lok>Malgranda Tuk</lok></fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d&eacute;l&eacute;gation<klr tip="ind"> (groupe de d&eacute;l&eacute;gu&eacute;s)</klr></trd>
</drv>
<drv mrk="deleg.D0itaro">
  <kap><tld lit="D"/>itaro</kap>
  <uzo tip="fak">ESP</uzo>
  <snc>
    <ref tip="dif" cel="delega.D0o">Delegacio.</ref>
  </snc>
</drv>
</art>
