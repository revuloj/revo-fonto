<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: teleau.xml,v 1.13 2024/01/16 01:35:22 revo Exp $">
<kap>
  <rad>telea&ubreve;tograf</rad>/o
</kap>

<drv mrk="teleau.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">TEL</uzo>
  <snc>
    <dif>
       <ref tip="super" cel="aparat.0o.TEK">Aparato</ref> permesanta elektran
       analogan transsendon de skribo a&ubreve; desegno:
       <ekz>
         la <tld/>o estis inventita de <nom>Elisha Gray</nom> en 1888 kaj uzata &gcirc;is
         &ccirc;. 1970.
       </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="faks.0ilo">faksilo</ref>,
       <ref cel="telegr.0o">telegrafo</ref>
    </refgrp>
    <url ref="&Viki;/Elisha_Gray">Vikipedio: Elisha Gray</url>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_a;&c_w;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
  <trd lng="de">Teleautograph</trd>
  <trd lng="pl">telegraf samopisz&aogonek;cy</trd>
  <trd lng="uk">&c_f;&#x43E;&c_t;&#x43E;&c_t;&c_je;&#x43B;&c_je;&c_g;&c_r;&c_a;&c_f;</trd>
</drv>

<drv mrk="teleau.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEL</uzo>
  <snc>
    <dif>
      Transsendi mesa&gcirc;on per tia aparato.
    </dif>
    <refgrp tip="vid">
       <ref cel="telegr.0i">telegrafi</ref>,
       <ref cel="faks.0i">faksi</ref>,
       <ref cel="mors.m0i">morsi</ref>
    </refgrp>
  </snc>
    <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_p;&c_r;&c_a;&c_z; <ind>&c_t;&c_e;&c_l;&c_je;&c_a;&c_w;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</ind></trd>
    <trd lng="ca">teleautografiar</trd>
    <trdgrp lng="hu">
      <trd>teleautograf&aacute;l</trd>,
      <trd>k&eacute;z&iacute;r&aacute;st tov&aacute;bb&iacute;t</trd>
    </trdgrp>
    <trd lng="pl">telegrafowa&cacute; na teleautografie</trd>
</drv>
</art>
