<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: teleau.xml,v 1.9 2019/03/25 21:10:17 revo Exp $">
<kap>
  <rad>telea&ubreve;tograf</rad>/o
</kap>

<drv mrk="teleau.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">TEL</uzo>
  <snc>
    <dif>
       <ref tip="super" cel="aparat.0o.TEK">Aparato</ref> permesanta elektran
       analogan transsendon de skribo a&ubreve; desegno:
       <ekz>
         la <tld/>o estis inventita de <nom>Elisha Gray</nom> en 1888 kaj uzata &gcirc;is 
         &ccirc;. 1970.
       </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="faks.0ilo">faksilo</ref>,
       <ref cel="telegr.0o">telegrafo</ref>
    </refgrp>
    <url ref="https://eo.wikipedia.org/wiki/Elisha_Gray">Vikipedio: Elisha Gray</url>
  </snc>
  <trd lng="de">Teleautograph</trd>
</drv>

<drv mrk="teleau.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEL</uzo>
  <snc>
    <dif>
      Transsendi mesa&gcirc;on per tia aparato.  
    </dif>
    <refgrp tip="vid">
       <ref cel="telegr.0i">telegrafi</ref>,
       <ref cel="faks.0i">faksi</ref>,
       <ref cel="mors.m0i">morsi</ref>
    </refgrp>
    <trd lng="ca">teleautografiar</trd>
    <trdgrp lng="hu">
      <trd>teleautograf&aacute;l</trd>,
      <trd>k&eacute;z&iacute;r&aacute;st tov&aacute;bb&iacute;t</trd>
    </trdgrp>
  </snc>
</drv>
</art>
