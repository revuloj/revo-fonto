<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>puritan</rad>/o
</kap>
<drv mrk="purita.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Ano de severmora protestanta sekto, kiu celas restarigi la
      kristanismon en sian primitivan purecon.
    </dif>
  </snc>
  <snc>
    <dif>
      Tre severmora homo.
    </dif>
  </snc>
  <trd lng="de">Puritaner</trd>
  <trd lng="nl">puritein</trd>
  <trd lng="ru">&c_p;&c_u;&c_r;&c_i;&c_t;&c_a;&c_n;&c_i;&c_n;</trd>
</drv>

<drv mrk="purita.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Doktrino de la <tld/>oj.
    </dif>
    <refgrp tip="sin">
      <ref cel="pietis.0o">pietismo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Puritanismus</trd>
  <trd lng="nl">puritanisme</trd>
  <trd lng="ru">&c_p;&c_u;&c_r;&c_i;&c_t;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>
</art>
