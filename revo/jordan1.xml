<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Jordan</rad>/o
</kap>
<drv mrk="jordan1.0o">
  <kap><tld/>o,
    <var><kap><tld/></kap></var>
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="malprt" cel="azi.OkcidentaA0o">Okcident-azia</ref>
      <ref tip="super" cel="river.0o">rivero</ref>
      &ccirc;e la landlimo inter
      <ref tip="vid" cel="israel.0o">Israelo</ref> kaj
      <ref tip="vid" cel="jordan.0o">Jordanio</ref>,
      fluanta el la Galilea Maro en la Malvivan Maron:
      <ekz>
        Kiam vi transiros <tld/>on en la landon Kanaanan,
        tiam elektu al vi urbojn
        <fnt><bib>MT</bib>, <lok>&Nom; 35:10-11</lok>.</fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_J;&c_o;&c_r;&c_d;&c_a;&c_n;<klr tip="amb"> (&c_r;&c_je;&c_k;&c_a;)</klr></trd>
    <trd lng="de">Jordan</trd>
    <trd lng="en">Jordan <klr>River</klr></trd>
    <trd lng="he">&he;&yod;&resh;&dalet;&fnun;
      <klr>(&nun;&he;&resh;)</klr></trd>
    <trd lng="pt">Jord&atilde;o <klr tip="amb">jato</klr></trd>
    <trd lng="ru">&c_I;&c_o;&c_r;&c_d;&c_a;&c_n;</trd>
  </snc>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="jordan.0o">Jordanio</ref>
  </snc>
</drv>
</art>

