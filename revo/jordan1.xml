<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jordan1.xml,v 1.14 2023/12/03 07:09:18 revo Exp $">
<kap>
<rad>Jordan</rad>/o
</kap>
<drv mrk="jordan1.J0o">
  <kap><tld/>o
  </kap>
  <snc mrk="jordan1.J0o.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="malprt" cel="azi.OkcidentaA0o">Okcident-azia</ref>
      <ref tip="lst" cel="river.0o" lst="voko:riveroj">rivero</ref>
      &ccirc;e la landlimo inter
      <ref tip="vid" cel="israel.I0o">Israelo</ref> kaj
      <ref tip="vid" cel="jordan.J0o">Jordanio</ref>,
      fluanta el la Galilea Maro en la <ref cel="mar.MalvivaM0o">Malvivan Maron</ref>:
      <ekz>
        kiam vi transiros <tld/>on en la landon Kanaanan,
        tiam elektu al vi urbojn
        <fnt>
          <bib>MT</bib>, <lok>&Nom; 35:10-11</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi transiris <klr>[Jordanon]</klr> kaj venis al <ref cel="jerihx.0o">Jeri&hcirc;o</ref>
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 24:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        li foriris denove trans <tld/>on tien, kie Johano unue baptis
        <fnt>
          <bib>NT</bib>
          <lok>Johano 10:40</lok>
        </fnt>;
      </ekz>
      <ekz>
        Trans<tld lit="j"/>o
        <fnt>
          <bib>NT</bib>
          <lok>Marko 3:8</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_Ja;&c_r;&c_d;&c_a;&c_n;</trd>
    <trd lng="bg">&c_J;&c_o;&c_r;&c_d;&c_a;&c_n;<klr tip="amb">
(&c_r;&c_je;&c_k;&c_a;)</klr></trd>
    <trd lng="de">Jordan</trd>
    <trd lng="en">Jordan <klr>River</klr></trd>
    <trd lng="he">&he;&yod;&resh;&dalet;&fnun;
      <klr>(&nun;&he;&resh;)</klr></trd>
    <trd lng="pt">Jord&atilde;o <klr tip="amb">jato</klr></trd>
    <trd lng="ru">&c_I;&c_o;&c_r;&c_d;&c_a;&c_n;</trd>
    <trd lng="it">Giordano</trd>
    <trdgrp lng="zh">
    <trd>&#x7D04;&#x65E6;&#x6CB3; <pr>Yu&#x113;d&#xE0;nH&#xE9;</pr></trd>,
    <trd>&#x7D04;&#x65E6;&#x6CB3; <pr>yu&#x113;d&#xE0;nh&#xE9;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="jordan1.J0o.EVI">
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="jordan.J0o">Jordanio</ref>
    <trd lng="it">Giordania</trd>
  </snc>
<!-- necesas ekzemploj kaj kiam trovitaj kreu apartan artikolon!
  <snc mrk="jordan1.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>.
   </dif>
   <trd lng="it">Giordano</trd>
   </snc>
-->
  <trd lng="ja">&#x30E8;&#x30EB;&#x30C0;&#x30F3;&#x5DDD; 
  <pr>&#x3088;&#x308B;&#x3060;&#x3093;&#x304B;&#x308F;</pr></trd>
  <trd lng="nl">Jordaan</trd>
  <trd lng="uk">&c_J;&#x43E;&c_r;&c_d;&c_a;&#x43D;</trd>
</drv>
</art>

