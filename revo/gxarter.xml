<kap>
  <rad>&gcirc;arter</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="gxarter.0o">
  <kap><tld/>o</kap>
  <snc mrk="gxarter.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="dif" cel="ruband.sxtrumpa_0o">&Scirc;trumpa rubando</ref>:
      <ekz>
        &scirc;i fiksis sian <tld/>on, kie li &ccirc;esidis, kaj
        tiel li vidis parteton de &scirc;ia nuda
        genuo
        <fnt>
          <aut>J. H. F. Gr&ouml;nloh, trad. M. Meeuwissen</aut>
          <vrk><url ref="https://meeuw.org/profitulo/">La profitulo</url></vrk>
          <lok>2021-06-08</lok>
        </fnt>;
      </ekz>
      <ekz>
        kavaliroj de la <tld/>o
        <klr>(angla ordeno)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="&Monato;/2005/008702.php">Monarkia mistiko</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jxartel.0o">&jcirc;artelo</ref>,
      <ref cel="sxtrum.0oligilo0osxelko">&scirc;trumpoligilo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_ja;&c_z;&c_k;&c_a; <klr>(&c_n;&c_a; &c_p;&c_a;&c_n;&c_ch;&c_o;&c_s;&c_je;)</klr></trd>
  <trd lng="de">Strumpfband</trd>
  <trd lng="en">garter</trd>
  <trd lng="fr">jarreti&egrave;re</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_v;&c_ja;&#769;&c_z;&c_k;&c_i;</trd>
  <trd lng="zh">&#x889C;&#x5E26; <pr>w&#xE0;d&#xE0;i</pr></trd>
</drv>
</art>

