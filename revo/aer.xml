<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc><rad>aer</rad>/o
    </kap>

    <drv mrk="aer.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="aer.0o.KEM">
        <uzo tip="fak">KEM</uzo>
        <dif>
          Spirebla nesimpla gaso, kiu &ccirc;irka&ubreve;as la teron:
          <ekz>
            oro estas pli peza ol fero, pli malpeza ol <tld/>o
            <fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            preni iom da fre&scirc;a <tld/>o<fnt>Z</fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="pend.0i.FIG">pendi</ref>.
        </refgrp>
      </snc>
      <snc mrk="aer.0o.spaco">
        <dif>
          Libera spaco super la tero, ofte konsiderata kiel medio
          por <ref cel="flug.0o">flugado</ref> kaj
          <ref cel="aviad.0o">aviado</ref>:
          <ekz>
            granda blanka cigno &scirc;vebis en la <tld/>o
            <fnt><bib>Fab1</bib><lok>Vojkamarado</lok></fnt>;
          </ekz><ekz>
            <ind>konstrui kastelojn en <tld/>o</ind>
            <klr>(revi pri &hcirc;imeraj profitoj)</klr>
            <fnt><bib>PrV</bib></fnt>;
            <trd lng="fr">
              b&acirc;tir des ch&acirc;teaux en <ind>Espagne</ind>
            </trd>
            <trd lng="hu">l&eacute;gv&aacute;rakat &eacute;p&iacute;t</trd>
          </ekz><ekz>
            celi anseron, <ind>trafi <tld/>on</ind>
            <fnt><bib>PrV</bib></fnt>.
            <trdgrp lng="fr">
              <trd>faire <ind>chou</ind> blanc</trd>,
              <trd>
                donner un coup d'<ind>&eacute;p&eacute;e</ind> dans l'eau
              </trd>
            </trdgrp>
            <trd lng="hu">bakot l&odblac;</trd>
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="arme.aer0o"><tld/>armeo</ref>,
          <ref cel="flot.aero0o"><tld/>ofloto</ref>,
          <ref cel="sxip.aer0o"><tld/>o&scirc;ipo</ref>
        </refgrp>
      </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_t;&c_r;&c_a;</trd>
  <trd lng="bg">&c_v;&c_malmol;&c_z;&c_d;&c_u;&c_h;</trd>
  <trd lng="br">aer</trd>
  <trd lng="ca">aire</trd>
  
  <trdgrp lng="cs">
    <trd>ovzdu&scaron;&iacute;</trd>,
    <trd>vzduch</trd>
  </trdgrp>
  <trd lng="de">Luft</trd>
  <trd lng="en">air<klr tip="ind"> (gas)</klr></trd>
  <trd lng="es">aire</trd>
  <trd lng="fr">air</trd>
  <trd lng="hu">leveg&odblac;</trd>
  <trd lng="id">udara</trd>
  <trd lng="it">aria</trd>
  <trd lng="nl">lucht</trd>
  <trd lng="oc">aire</trd>
  <trd lng="pl">powietrze</trd>
  <trd lng="pt">ar</trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_u;&c_h;</trd>
  <trd lng="ms">udara</trd>
  <trdgrp lng="sk">
    <trd>povetrie</trd>,
    <trd>vzduch</trd>
  </trdgrp>
  <trd lng="sv">luft</trd>
  <trd lng="tp">kon</trd>
  <trd lng="tr">hava</trd>
  <trd lng="vo">lut</trd>
  <trdgrp lng="zh">
        <trd>&#31354;&#27668;</trd>,
        <trd>&#31354;&#27683;<klr> [k&#333;ngq&igrave;]</klr></trd>
      </trdgrp>
</drv>

    <drv mrk="aer.0a">
      <kap><tld/>a</kap>
      <snc mrk="aer.0a.el_0o">
        <dif>
          Konsistanta el <tld/>o, malpeza kvaza&ubreve; el <tld/>o:
          <ekz>
            <tld/>a veziketo<fnt>B</fnt>;
          </ekz><ekz>
            <klr>[la infanoj de&scirc;iris]</klr> tiun malkompaktan,
            <tld/>an, lansimilan floron <klr>[de leontodo]</klr>
            <fnt><bib>Fab3</bib><lok>Ekzistas diferenco</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="en">airy</trd>
      </snc>
      <snc mrk="aer.0a.en_0o">
        <dif>
          Estanta en la <tld/>o:
          <ekz>
            kelkaj vegeta&jcirc;oj havas <tld/>ajn radikojn.
          </ekz>
        </dif>
        <trd lng="en">airborne</trd>
        <trd lng="es">a&eacute;reo</trd>
      </snc>
      <snc mrk="aer.0a.0rilata">
        <dif>
          Rilata al <tld/>o:
          <ekz>
            la pantofloj flugis al ili trans la orelojn <klr>[:]</klr>
            tiajn <tld/>ajn fenomenojn ili neniam anta&ubreve;e vidis
            <fnt><bib>Fab1</bib><lok>Fluganta kofro</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="en">aerial</trd>
        <trd lng="pl">powietrzny</trd>
      </snc>
      <snc mrk="aer.0a.malfirma">
        <uzo tip="stl">FIG</uzo>
        <dif>
          <ref cel="firm.mal0a">Malfirma</ref>,
          <ref cel="baz.sen0a">senbaza</ref>:
          <ekz>
            gloramo havas esencon <tld/>an kaj
            senfundamentan<fnt>Z</fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="vant.0a">vanta</ref>,
          <ref cel="vent.0a">venta</ref>
        </refgrp>
        <trd lng="en">airy</trd>
      </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_t;&c_r;&c_a;&c_n;&c_y;</trd>
  <trd lng="br">-<ind>aer</ind></trd>
  <trd lng="ms">berangin</trd>
  <trdgrp lng="cs">
    <trd>leteck&yacute;</trd>,
    <trd>nadnesen&yacute;</trd>,
    <trd>nere&aacute;ln&yacute;</trd>,
    <trd>vzduchov&yacute;</trd>,
    <trd>vzdu&scaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>luftig</trd>,
        <trd>Luft-</trd>
      </trdgrp>
  <trdgrp lng="fr">
        <trd>d'<ind>air</ind></trd>,
        <trd>a&eacute;rien</trd>,
        <trd>illusoire</trd>,
        <trd>inconsistant</trd>,
        <trd>vain</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>l&eacute;gi</trd>,
        <trd>l&eacute;g-</trd>,
        <trd>leveg&odblac;-</trd>
      </trdgrp>
  <trdgrp lng="nl">
        <trd>luchtig</trd>,
        <trd>lucht-</trd>
      </trdgrp>
  <trd lng="pt">a&eacute;reo</trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>prevzdu&scaron;&ncaron;uj&uacute;ci</trd>,
    <trd>vetrac&iacute;</trd>,
    <trd>vzdu&scaron;n&yacute;</trd>
  </trdgrp>
</drv>

    <drv mrk="aer.0umi">
      <kap><ofc>*</ofc><tld/>umi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Enlasi eksteran <tld/>on en iun lokon, por forigi de &gcirc;i 
          malutilajn por la sano gasojn a&ubreve; malagrablajn odorojn:
          <ekz>
            oni zorge <tld/>umu la dormo&ccirc;ambrojn &ccirc;iun matenon.
          </ekz>
        </dif>
        <ref tip="vid" cel="ventol.0i">ventoli</ref>
      </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_je;&c_t;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="br">aera&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>prov&ecaron;trat</trd>,
    <trd>vyv&ecaron;trat</trd>,
    <trd>v&ecaron;trat</trd>
  </trdgrp>
  <trd lng="de">l&uuml;ften</trd>
  <trdgrp lng="en">
        <trd>aerate</trd>,
        <trd>air out</trd>
      </trdgrp>
  <trd lng="es">airear</trd>
  <trdgrp lng="fr">
        <trd>a&eacute;rer<klr tip="ind"> (renouveler l'air)</klr></trd>,
        <trd>ventiler</trd>
      </trdgrp>
  <trd lng="hu">szell&odblac;ztet</trd>
  <trd lng="nl">verluchten</trd>
  <trd lng="pl">wietrzy&cacute;</trd>
  <trdgrp lng="pt">
        <trd>arejar</trd>,
        <trd>ventilar</trd>
      </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_je;&c_t;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">vetra&tcaron;</trd>
  <trd lng="sv">v&auml;dra</trd>
</drv>

    <drv mrk="aer.Bon0o">
      <kap>Bon<tld/>o <fnt><bib>PIV2</bib></fnt></kap>
      <snc>
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">POL</uzo>
        <dif>
          &Ccirc;efurbo de Argentino:
          <ekz>
            luita a&ubreve;to devis veturigi nin al la flughaveno,
            situanta &ccirc;e la &ccirc;efurbo Bon<tld/>o, trovi&gcirc;anta je
            60 km for de La Plata
            <fnt>
              <aut>Jos&eacute; de N. MARQUES</aut>:
              <vrk>Voja&gcirc;o al Ushuaia</vrk>. <bib>Monato</bib>
            </fnt>;
          </ekz><ekz>
            en la sidejo de Bon<tld/>a Esperanto-Asocio dudeko da
            partoprenantoj plenumis la kutiman tagordon
            <fnt><aut>Roberto Sartor</aut>:
              <vrk>Argentina asembleo</vrk>.
              <bib>LOdE</bib>, <lok>2003:12 (110)</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_B;&c_u;&c_e;&c_n;&c_a;&c_s;-&c_A;&c_j;&c_r;&c_e;&c_s;</trd>
      <trd lng="en">Buenos Aires</trd>
      <trd lng="es">Buenos Aires</trd>
      <trd lng="fr">Buenos Aires</trd>
      <trd lng="hu">Buenos Aires</trd>
      <trd lng="id">Buenos Aires</trd>
      <trd lng="ru">&c_B;&c_u;&c_e;&c_n;&c_o;&c_s;-&c_A;&c_j;&c_r;&c_je;&c_s;</trd>
    </drv>

  </art>
