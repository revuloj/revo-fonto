<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>aer</rad>/o</kap>
<drv mrk="aer.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Spirebla nesimpla gaso, kiu &ccirc;irka&ubreve;as la teron:
      <ekz>
        preni iom da fre&scirc;a <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_t;&c_r;&c_a;</trd>
  <trd lng="br">aer</trd>
  <trd lng="cs">vzduch</trd>
  <trd lng="de">Luft</trd>
  <trd lng="en">air</trd>
  <trd lng="es">aire</trd>
  <trd lng="fr">air</trd>
  <trd lng="nl">lucht</trd>
  <trd lng="pt">ar</trd>
  <trd lng="it">aria</trd>
  <trd lng="tr">hava</trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_u;&c_h;</trd>
  <trd lng="sk">vzduch</trd>
  <trd lng="sv">luft</trd>
  <trd lng="vo">lut</trd>
  <trdgrp lng="zh">
    <trd>&#31354;&#27668;</trd>,
    <trd>&#31354;&#27683;<klr> [k&#333;ngq&igrave;]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="aer.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konsistanta el aero:
      <ekz>
        <tld/>a veziketo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        gloramo havas esencon <tld/>an kaj senfundamentan<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        konstrui <tld/>ajn kastelojn<fnt>Z</fnt>
        <klr>(revi pri &hcirc;imeraj profitoj)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="vent.0a">venta</ref>,
      <ref cel="baz.sen0a">senbaza</ref>,
      <ref cel="firm.0a">malfirma</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="pend.0i">pendi 3</ref>.
    </refgrp>
    <trdgrp lng="fr">
      <trd>constitu&eacute; d'<ind>air</ind></trd>,
      <trd>vain</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Estanta en la aero:
      <ekz>
        kelkaj vegeta&jcirc;oj havas <tld/>ajn radikojn.
      </ekz>
    </dif>
    <trd lng="en">airborne</trd>
    <trd lng="fr">a&eacute;rien</trd>
    <trd lng="es">a&eacute;reo</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_t;&c_r;&c_a;&c_n;&c_y;</trd>
  <trd lng="br">-<ind>aer</ind></trd>
  <trdgrp lng="de">
    <trd>luftig</trd>,
    <trd>Luft-</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>luchtig</trd>,
    <trd>lucht-</trd>
  </trdgrp>
  <trd lng="pt">a&eacute;reo</trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">vzdu&scaron;n&yacute;</trd>
</drv>

<drv mrk="aer.0umi">
  <kap><ofc>*</ofc><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Pasigi aerondon tra io, por forigi malbonajn haladzojn; ventoli:
      <ekz>
        oni zorge <tld/>umu la dormo&ccirc;ambrojn &ccirc;iun matenon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_je;&c_t;&c_r;&c_y;&c_v;&c_a;&c_c;
&c_mol;</trd>
  <trd lng="br">aera&ntilde;</trd>
  <trd lng="de">l&uuml;ften</trd>
  <trd lng="en">aerate</trd>
  <trd lng="es">aerear</trd>
  <trd lng="fr">a&eacute;rer</trd>
  <trd lng="nl">verluchten</trd>
  <trdgrp lng="pt">
    <trd>arejar</trd>,
    <trd>ventilar</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_je;&c_t;&c_r;&c_i;&c_v;&c_a;&c_t;
&c_mol;</trd>
  <trd lng="sk">vetra&tcaron;</trd>
  <trd lng="sv">v&auml;dra</trd>
</drv>

<drv mrk="aer.0sxtono">
  <kap><tld/>&scirc;tono</kap>
  <snc>
    <dif>
      Aerolito.
    </dif>
  </snc>
  <trd lng="br">aerolitenn</trd>
  <trdgrp lng="de">
    <trd>Meteorstein</trd>,
    <trd>Meteor</trd>,
    <trd>Aerolith</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>meteor</trd>,
    <trd>aerolite</trd>
  </trdgrp>
  <trd lng="fr">a&eacute;rolithe</trd>
  <trd lng="es">aerolito</trd>
  <trd lng="nl">a&euml;roliet</trd>
  <trd lng="pt">aer&oacute;lito</trd>
  <trd lng="sk">meteorit</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_e;&c_r;&c_o;&c_l;&c_i;&c_t;</trd>,
    <trd>&c_m;&c_je;&c_t;&c_je;&c_o;&c_r;&c_i;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="aer.0transporto">
  <kap><tld/>transporto</kap>
  <fnt><bib>IKEV</bib></fnt>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Transportado de pasa&gcirc;eroj kaj varoj per aviadiloj.
    </dif>
  </snc>
  <trd lng="en">air transport</trd>
  <trdgrp lng="de">
    <trd>Lufttransport</trd>,
    <trd>Luftverkehr</trd>
  </trdgrp>
  <trd lng="nl">luchttransport</trd>
  <trd lng="sk">leteck&aacute; doprava</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_v;&c_i;&c_a;&c_p;&c_je;&c_r;&c_je;&c_v;&c_o;&c_z;&c_k;&c_a;
    </trd>,
    <trd>&c_v;&c_o;&c_z;&c_d;&c_u;&c_sh;&c_n;&c_y;&c_j; &c_t;&c_r;&c_a;&c_n;
&c_s;&c_p;&c_o;&c_r;&c_t;</trd>
  </trdgrp>
</drv>
</art>
