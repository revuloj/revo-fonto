<drv mrk="amuz.0a">
  <kap><tld/>a</kap>
  <snc mrk="amuz.0a.KOMUNE">
    <dif>
      <tld lit="A"/>anta:
      <ekz>
        tia ludado <klr>[&#x2026;]</klr> estis tiel <tld/>a, ke li ne povis
        kontra&ubreve;stari
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        1a</lok></fnt>;
      </ekz>
      <ekz>
        la <klr>[&#x2026;]</klr> malgrandaj idoj estas ravataj <klr>[kaj]</klr> trovas tion
        treege <tld/>a
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estas <tld/>a, diris la malgranda Ida kaj pla&ubreve;dis per la manoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili estas la plej &ccirc;armaj infanoj kaj tiel <tld/>aj!
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rid.0iga">ridiga</ref>, 
      <ref cel="komik.0a">komika</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_je;&c_sh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
