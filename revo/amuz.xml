<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>amuz</rad>/i
</kap>
<drv mrk="amuz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Agrable kaj gajige distri:
      <ekz>
        infanoj bezonas <tld/>on.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="gaj.0igi">gajigi</ref>,
      <ref cel="enu">malenuigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_ja;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>didua&ntilde;</trd>,
    <trd>diduella&ntilde;</trd>
  </trdgrp>
  <trd lng="cs">bavit</trd>
  <trdgrp lng="de">
    <trd>unterhalten</trd>,
    <trd>zerstreuen</trd>
  </trdgrp>
  <trd lng="en">amuse</trd>
  <trdgrp lng="es">
    <trd>entretener</trd>,
    <trd>divertir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>amuser</trd>,
    <trd>divertir</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&shin;&ayin;&shin;&ayin;</ind></trd>,
    <trd>&lamed;<ind>&bet;&dalet;&resh;</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>amuseren</trd>,
    <trd>vermaken</trd>
  </trdgrp>
  <trd lng="it">divertire</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">bavi&tcaron;</trd>
</drv>

<drv mrk="amuz.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="A"/>anta.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_je;&c_sh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>fentus</trd>,
    <trd>diduellus</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unterhaltsam</trd>,
    <trd>am&uuml;sant</trd>
  </trdgrp>
  <trd lng="en">amusing</trd>
  <trd lng="fr">amusant</trd>
  <trdgrp lng="he">
    <trd>&mem;&shin;&ayin;&shin;&ayin;</trd>,
    <trd>&mem;&bet;&dalet;&resh;</trd>
  </trdgrp>
  <trd lng="it">divertente</trd>
  <trdgrp lng="nl">
    <trd>plezierig</trd>,
    <trd>amusant</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;bavn&yacute;</trd>
</drv>

<drv mrk="amuz.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Objekto por <tld/>i.
    </dif>
    <refgrp tip="sin">
      <ref cel="lud">ludilo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_a;&c_c;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="br">c'hoariell</trd>
  <trdgrp lng="de">
    <trd>Spielzeug</trd>
  </trdgrp>
  <trd lng="en">toy</trd>
  <trdgrp lng="fr">
    <trd>jouet</trd>,
    <trd>gadget</trd>
  </trdgrp>
  <trd lng="nl">gadget</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_g;&c_r;&c_u;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_d;&c_je;&c_l;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="amuz.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Profesia gajigisto en cirko, teatro, ktp:
      <ekz>
        li ne estas artisto, sed
        nur <tld/>isto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="arleke.0o">arlekeno</ref>,
      <ref cel="burles">burleskulo</ref>,
      <ref cel="pulcxi.0o">pul&ccirc;inelo</ref>,
      <ref cel="klauxn.0o">kla&ubreve;no</ref>,
      <ref cel="pajac.0o">pajaco</ref>,
      <ref cel="histri1.0o">histriono</ref>.
    </refgrp>
  </snc>
  <trd lng="br">diduer</trd>
  <trdgrp lng="de">
    <trd>Unterhalter</trd>,
    <trd>Animateur</trd>
  </trdgrp>
  <trd lng="en">entertainer</trd>
  <trdgrp lng="fr">
    <trd>amuseur</trd>,
    <trd>comique</trd>
  </trdgrp>
  <trd lng="he">&bet;&dalet;&resh;&fnun;</trd>
  <trd lng="nl">entertainer</trd>
</drv>
</art>
