<drv mrk="amuz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="amuz.0i.KOMUNE">
    <dif>
      Agrable <ref tip="sin" cel="distr.0i.amuzi">distri</ref> kaj <ref tip="vid" cel="gaj.0igi">gajigi</ref>:
      <ekz>
        juna homo, kies sola celo en la vivo estis bone man&gcirc;i,
        bone trinki kaj <tld/>i fra&ubreve;linojn
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
      <ekz>
        ni akceptas anka&ubreve; gastojn kaj <tld/>as nin &ccirc;e tio &ccirc;i tre bone
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        dum la tuta tago li sin <tld/>adis en la varma sunlumo
        <fnt>
          <bib>Fab2</bib>
          <lok>Elfo de la rozo</lok>
        </fnt>;
      </ekz>
      <ekz>
        voku &Scirc;im&scirc;onon, ke li <tld/>u nin, kaj oni vokis &Scirc;im&scirc;onon el la malliberejo, kaj
        li faris <tld/>on al ili
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <klr>[&#x2026;]</klr> moke <tld/>is vin pri mi!
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#x2026;]</klr> eltran&ccirc;adis plej <tld/>antajn bildojn: korojn kun malgrandaj
        knabinoj interne, kiuj dancis
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="delekt.0i">delekti</ref>,
      <ref cel="plezur.0igi">plezurigi</ref>,
      <ref cel="rav.0i">ravi</ref>,
      <ref cel="rid.0igi">ridigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_ja;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>didua&ntilde;</trd>,
    <trd>diduella&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>entretenir</trd>,
    <trd>divertir</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bavit</trd>,
    <trd>obveselovat</trd>,
    <trd>pobavit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unterhalten <klr>(am&uuml;sieren)</klr></trd>,
    <trd>zerstreuen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amuse</trd>,
    <trd>entertain</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>entretener</trd>,
    <trd>divertir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>amuser</trd>,
    <trd>divertir</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&shin;&ayin;&shin;&ayin;</ind></trd>,
    <trd>&lamed;<ind>&bet;&dalet;&resh;</ind></trd>
  </trdgrp>
  <trd lng="io">amuzar</trd>
  <trd lng="it">divertire</trd>
  <trd lng="ja">&#x697D;&#x3057;&#x307E;&#x305B;&#x308B; 
  <pr>&#x305F;&#x306E;&#x3057;&#x307E;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="nl">
