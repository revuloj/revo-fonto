<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: amuz.xml,v 1.29 2019/09/06 12:10:17 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>amuz</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="amuz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="amuz.0i.KOMUNE">
    <dif>
      Agrable distri kaj <ref tip="vid" cel="gaj.0igi">gajigi</ref>:
      <ekz>
        infanoj bezonas <tld/>on;
      </ekz>
      <ekz>
        juna homo, kies sola celo en la vivo estis bone man&gcirc;i,
        bone
        trinki
        kaj <tld/>i fra&ubreve;linojn
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_ja;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>didua&ntilde;</trd>,
    <trd>diduella&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>entretenir</trd>,
    <trd>divertir</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bavit</trd>,
    <trd>obveselovat</trd>,
    <trd>pobavit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unterhalten</trd>,
    <trd>zerstreuen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amuse</trd>,
    <trd>entertain</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>entretener</trd>,
    <trd>divertir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>amuser</trd>,
    <trd>divertir</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&shin;&ayin;&shin;&ayin;</ind></trd>,
    <trd>&lamed;<ind>&bet;&dalet;&resh;</ind></trd>
  </trdgrp>
  <trd lng="io">amuzar</trd>
  <trd lng="it">divertire</trd>
  <trdgrp lng="nl">
    <trd>amuseren</trd>,
    <trd>vermaken</trd>,
    <trd>entertainen</trd>
  </trdgrp>
  <trd lng="oc">amusar</trd>
  <trd lng="pt">divertir</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bavi&tcaron;</trd>,
    <trd>zabavi&tcaron;</trd>
  </trdgrp>
  <trd lng="th">&#3607;&#3635;&#3651;&#3627;&#3657;&#3626;&#3609;&#3640;&#3585;</trd>
</drv>

<drv mrk="amuz.0a">
  <kap><tld/>a</kap>
  <snc mrk="amuz.0a.KOMUNE">
    <dif>
      <tld lit="A"/>anta:
      <ekz>
        tia ludado <klr>...</klr> estis tiel <tld/>a, ke li ne povis
        kontra&ubreve;stari
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        1a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rid.0iga">ridiga</ref>, 
      <ref cel="komik.0a">komika</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_je;&c_sh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>fentus</trd>,
    <trd>diduellus</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>z&aacute;bavn&yacute;</trd>,
    <trd>&scaron;pr&yacute;movn&yacute;</trd>,
    <trd>&zcaron;ertovn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unterhaltsam</trd>,
    <trd>am&uuml;sant</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amusing</trd>,
    <trd>entertaining</trd>,
    <trd>fun</trd>,
    <trd>funny</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>entretenido</trd>,
    <trd>divertido</trd>
  </trdgrp>
  <trd lng="fr">amusant</trd>
  <trdgrp lng="he">
    <trd>&mem;&shin;&ayin;&shin;&ayin;</trd>,
    <trd>&mem;&bet;&dalet;&resh;</trd>
  </trdgrp>
  <trdgrp lng="io">
    <trd>amuziva</trd>,
    <trd>amuzanta</trd>
  </trdgrp>
  <trd lng="it">divertente</trd>
  <trdgrp lng="nl">
    <trd>vermakelijk</trd>,
    <trd>amusant</trd>,
    <trd>leuk</trd>,
    <trd>grappig</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;bavn&yacute;</trd>
  <trd lng="th">&#3626;&#3609;&#3640;&#3585;</trd>
</drv>

<drv mrk="amuz.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="amuz.0ajxo.KOMUNE">
    <dif>
      Afero, okupo, a&jcirc;o, kiu <tld/>as:
      <ekz>
        mi pensis en mi pri &ccirc;iuj &ccirc;i tiuj <tld/>a&jcirc;oj
        kiuj kutime 
        akompanas la sezonon de Kristnaskofesto: kial ni faras
        &ccirc;iujn &ccirc;i tiujn <tld/>a&jcirc;ojn?
        <fnt>
          <aut>B. Afolaranmi</aut>,
          <vrk><url
          ref="http://esperanto.com/enhavo/kristnaskofesto-sen-kristo">
          Kristnaskofesto sen Kristo</url></vrk>,
          <lok>2016</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">diversi&oacute;n</trd>
  <trd lng="fr">amusement</trd>
  <trd lng="io">amuzivo</trd>
</drv>

<drv mrk="amuz.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="amuz.0igxi.KOMUNE">
    <dif>
      Trovi ekscitan intereson, <tld/>on en io:
      <ekz>
        <ctl>Krakovanoj</ctl> estas grupo de junaj Esperantistoj, kiuj
        simple 
        volas <tld/>i&gcirc;i
        <fnt>
          <aut>LukiZ</aut>,
          <vrk><url
          ref="http://pej.pl/eo/2013/09/28/krakovanoj-nova-retpago-de-krakovaj-junuloj/">
          Nova retpa&gcirc;o de krakovaj junuloj</url></vrk>,
          <lok>2013-09-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>have fun</trd>,
    <trd>have a good time</trd>,
    <trd>enjoy oneself</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>entretener</ind>se</trd>,
    <trd><ind>divertir</ind>se</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>amuser</ind></trd>
  <trdgrp lng="io">
    <trd>amuzar su</trd>
    <trd>amuzesar</trd>
  </trdgrp>
  <trd lng="nl">zich <ind>vermaken</ind></trd>
  <trd lng="th">&#3626;&#3609;&#3640;&#3585;</trd>
</drv>

<drv mrk="amuz.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="amuz.0ilo.KOMUNE">
    <dif>
      Objekto por <tld/>i:
      <ekz>
        al la virino ili diras: estu ludilo, servu kiel <tld/>ilo al la
        viro
        <fnt><bib>Marta</bib></fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lud">ludilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_a;&c_c;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="br">c'hoariell</trd>
  <trdgrp lng="cs">
    <trd>hra&ccaron;ka</trd>,
    <trd>z&aacute;bavn&aacute; v&ecaron;c</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Spielzeug</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>toy</trd>,
    <trd>plaything</trd>
  </trdgrp>
  <trd lng="es">juego</trd>
  <trdgrp lng="fr">
    <trd>jouet</trd>,
    <trd>gadget</trd>
  </trdgrp>
  <trdgrp lng="io">
    <trd>amuzilo</trd>,
    <trd>amuzivo</trd>
  </trdgrp>
  <trd lng="nl">speeltje</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_g;&c_r;&c_u;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_d;&c_je;&c_l;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">hra&ccaron;ka</trd>
</drv>

<drv mrk="amuz.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="amuz.0isto.KOMUNE">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Profesia gajigisto en cirko, teatro, ktp:
      <ekz>
        li ne estas artisto, sed nur <tld/>isto.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">diduer</trd>
  <trdgrp lng="cs">
    <trd>bavi&ccaron;</trd>,
    <trd>humorista</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Unterhalter</trd>,
    <trd>Animateur</trd>
  </trdgrp>
  <trd lng="en">entertainer</trd>
  <trdgrp lng="es">
    <trd>animador</trd>,
    <trd>c&oacute;mico</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>amuseur</trd>,
    <trd>comique<klr tip="amb"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="he">&bet;&dalet;&resh;&fnun;</trd>
  <trd lng="io">amuzero</trd>
  <trd lng="nl">entertainer</trd>
  <trdgrp lng="sk">
    <trd>humorista</trd>,
    <trd>zab&aacute;va&ccaron;</trd>
  </trdgrp>
</drv>

<drv mrk="amuz.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="amuz.mal0i.KOMUNE">
    <dif>
      Malagrable impresi,
      <ref tip="vid" cel="ted.0i">tedi</ref> a&ubreve;
      <ref tip="vid" cel="incit.0i">inciti</ref>:
      <ekz>
        a&ubreve;skulti vin ege mal<tld/>as min
        <fnt>
          <aut>E. Fabian</aut>,
          <vrk><url
          ref="http://groups.google.de/group/soc.culture.esperanto/browse_thread/thread/b555a759f065100e">
          Respondo al Michel</url></vrk>,
          <lok>soc.culture.esperanto, 2010-03-16</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ted.0i">tedi</ref>, 
      <ref cel="enu.0igi">enuigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="es">
    <trd>molestar</trd>,
    <trd>chinchar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>agacer</trd>,
    <trd>ennuyer</trd>
  </trdgrp>
  <trdgrp lng="th">
    <trd>&#3607;&#3635;&#3651;&#3627;&#3657;&#3652;&#3617;&#3656;&#3626;&#3609;&#3640;&#3585;</trd>,
    <trd>&#3607;&#3635;&#3651;&#3627;&#3657;&#3609;&#3656;&#3634;&#3648;&#3610;&#3639;&#3656;&#3629;</trd>
  </trdgrp>
</drv>
</art>
