<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: amuz.xml,v 1.30 2021/02/18 10:38:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>amuz</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="amuz.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="amuz.0i.KOMUNE">
    <dif>
      Agrable <ref tip="sin" cel="distr.0i.amuzi">distri</ref> kaj <ref tip="vid" cel="gaj.0igi">gajigi</ref>:
      <ekz>
        juna homo, kies sola celo en la vivo estis bone man&gcirc;i,
        bone trinki kaj <tld/>i fra&ubreve;linojn
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>.
      </ekz>
      <ekz>
        ni akceptas anka&ubreve; gastojn kaj <tld/>as nin &ccirc;e tio &ccirc;i tre bone
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        dum la tuta tago li sin <tld/>adis en la varma sunlumo
        <fnt>
          <bib>Fab2</bib>
          <lok>Elfo de la rozo</lok>
        </fnt>;
      </ekz>
      <ekz>
        voku &Scirc;im&scirc;onon, ke li <tld/>u nin, kaj oni vokis &Scirc;im&scirc;onon el la malliberejo, kaj
        li faris <tld/>on al ili
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <klr>[&#8230;]</klr> moke <tld/>is vin pri mi!
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> eltran&ccirc;adis plej <tld/>antajn bildojn: korojn kun malgrandaj
        knabinoj interne, kiuj dancis
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="delekt.0i">delekti</ref>,
      <ref cel="plezur.0igi">plezurigi</ref>,
      <ref cel="rav.0i">ravi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_ja;&c_sh;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>didua&ntilde;</trd>,
    <trd>diduella&ntilde;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>entretenir</trd>,
    <trd>divertir</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bavit</trd>,
    <trd>obveselovat</trd>,
    <trd>pobavit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unterhalten <klr>(am&uuml;sieren)</klr></trd>,
    <trd>zerstreuen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amuse</trd>,
    <trd>entertain</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>entretener</trd>,
    <trd>divertir</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>amuser</trd>,
    <trd>divertir</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;<ind>&shin;&ayin;&shin;&ayin;</ind></trd>,
    <trd>&lamed;<ind>&bet;&dalet;&resh;</ind></trd>
  </trdgrp>
  <trd lng="io">amuzar</trd>
  <trd lng="it">divertire</trd>
  <trdgrp lng="nl">
    <trd>amuseren</trd>,
    <trd>vermaken</trd>,
    <trd>entertainen</trd>
  </trdgrp>
  <trd lng="oc">amusar</trd>
  <trd lng="pt">divertir</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bavi&tcaron;</trd>,
    <trd>zabavi&tcaron;</trd>
  </trdgrp>
  <trd lng="th">&#3607;&#3635;&#3651;&#3627;&#3657;&#3626;&#3609;&#3640;&#3585;</trd>
</drv>

<drv mrk="amuz.0a">
  <kap><tld/>a</kap>
  <snc mrk="amuz.0a.KOMUNE">
    <dif>
      <tld lit="A"/>anta:
      <ekz>
        tia ludado <klr>[&#8230;]</klr> estis tiel <tld/>a, ke li ne povis
        kontra&ubreve;stari
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        1a</lok></fnt>;
      </ekz>
      <ekz>
        la <klr>[&#8230;]</klr> malgrandaj idoj estas ravataj <klr>[kaj]</klr> trovas tion
        treege <tld/>a
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estas <tld/>a, diris la malgranda Ida kaj pla&ubreve;dis per la manoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili estas la plej &ccirc;armaj infanoj kaj tiel <tld/>aj!
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rid.0iga">ridiga</ref>, 
      <ref cel="komik.0a">komika</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_je;&c_sh;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>fentus</trd>,
    <trd>diduellus</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>z&aacute;bavn&yacute;</trd>,
    <trd>&scaron;pr&yacute;movn&yacute;</trd>,
    <trd>&zcaron;ertovn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unterhaltsam</trd>,
    <trd>am&uuml;sant</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>amusing</trd>,
    <trd>entertaining</trd>,
    <trd>fun</trd>,
    <trd>funny</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>entretenido</trd>,
    <trd>divertido</trd>
  </trdgrp>
  <trd lng="fr">amusant</trd>
  <trdgrp lng="he">
    <trd>&mem;&shin;&ayin;&shin;&ayin;</trd>,
    <trd>&mem;&bet;&dalet;&resh;</trd>
  </trdgrp>
  <trdgrp lng="io">
    <trd>amuziva</trd>,
    <trd>amuzanta</trd>
  </trdgrp>
  <trd lng="it">divertente</trd>
  <trdgrp lng="nl">
    <trd>vermakelijk</trd>,
    <trd>amusant</trd>,
    <trd>leuk</trd>,
    <trd>grappig</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_b;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_l;&c_je;&c_k;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">z&aacute;bavn&yacute;</trd>
  <trd lng="th">&#3626;&#3609;&#3640;&#3585;</trd>
</drv>

<drv mrk="amuz.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="amuz.0ajxo.KOMUNE">
    <dif>
      Afero, okupo, a&jcirc;o, kiu <tld/>as:
      <ekz>
        festeno sekvis festenon, <tld/>a&jcirc;o <tld/>a&jcirc;on
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        amba&ubreve; apartenis al severa religia sekto, kiu ne permesis alkohola&jcirc;ojn, fumadon
        a&ubreve; frivolajn <tld/>a&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="https://www.monato.be/2012/010361.php">Lisuarte</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi pensis en mi pri &ccirc;iuj &ccirc;i tiuj <tld/>a&jcirc;oj
        kiuj kutime akompanas la sezonon de Kristnaskofesto: kial ni faras
        &ccirc;iujn &ccirc;i tiujn <tld/>a&jcirc;ojn
        <fnt>
          <aut>B. Afolaranmi</aut>,
          <vrk><url ref="http://esperanto.com/enhavo/kristnaskofesto-sen-kristo">
          Kristnaskofesto sen Kristo</url></vrk>,
          <lok>2016</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Unterhaltung <klr>(Spa&szlig;)</klr></trd>,
    <trd>Am&uuml;sement</trd>,
    <trd>Zerstreuung</trd>
  </trdgrp>
  <trd lng="es">diversi&oacute;n</trd>
  <trd lng="fr">amusement</trd>
  <trd lng="io">amuzivo</trd>
</drv>

<drv mrk="amuz.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="amuz.0igxi.KOMUNE">
    <dif>
      Trovi ekscitan intereson, <tld/>on en io:
      <ekz>
        <nom>Isaak</nom> <tld/>i&gcirc;as kun sia edzino <nom>Rebeka</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 26:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni envokis la vagvendiston kun liaj komerca&jcirc;oj, sed nur por ke ili povu
        <tld/>i&gcirc;i pri li
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>i&gcirc;adis per tio, ke &scirc;i kaptadis mu&scirc;ojn, el&scirc;iris al ili la flugilojn kaj
        lasis ilin poste rampi
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniam oni tie havis ian <tld/>i&gcirc;on, e&ccirc; ne ian malgrandan balon de ursoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis aran&gcirc;isto de komedioj, maskobaloj kaj &ccirc;iaj elpenseblaj <tld/>i&gcirc;oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="distr.0igxi.amuzigxi">distri&gcirc;i<sncref/></ref>
  </snc>
  <trd lng="de">sich <ind>am&uuml;sieren</ind></trd>
  <trdgrp lng="en">
    <trd>have fun</trd>,
    <trd>have a good time</trd>,
    <trd>enjoy oneself</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd><ind>entretener</ind>se</trd>,
    <trd><ind>divertir</ind>se</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>amuser</ind></trd>
  <trdgrp lng="io">
    <trd>amuzar su</trd>
    <trd>amuzesar</trd>
  </trdgrp>
  <trd lng="nl">zich <ind>vermaken</ind></trd>
  <trd lng="th">&#3626;&#3609;&#3640;&#3585;</trd>
</drv>

<drv mrk="amuz.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="amuz.0ilo.KOMUNE">
    <dif>
      Objekto por <tld/>i:
      <ekz>
        diletanteco estas <tld/>ilo de la vivo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        al la virino ili diras: estu ludilo, servu kiel <tld/>ilo al la
        viro
        <fnt><bib>Marta</bib></fnt>!
      </ekz>
      <ekz>
        porinfanaj lingvaj <tld/>iloj
        <fnt>
          <bib>Monato</bib>
          <aut>Grigori Arosev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007106.html">&Ccirc;armaj kantoj por infanoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lud">ludilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_a;&c_c;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="br">c'hoariell</trd>
  <trdgrp lng="cs">
    <trd>hra&ccaron;ka</trd>,
    <trd>z&aacute;bavn&aacute; v&ecaron;c</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Spielzeug</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>toy</trd>,
    <trd>plaything</trd>
  </trdgrp>
  <trd lng="es">juego</trd>
  <trdgrp lng="fr">
    <trd>jouet</trd>,
    <trd>gadget</trd>
  </trdgrp>
  <trdgrp lng="io">
    <trd>amuzilo</trd>,
    <trd>amuzivo</trd>
  </trdgrp>
  <trd lng="nl">speeltje</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_g;&c_r;&c_u;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_d;&c_je;&c_l;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">hra&ccaron;ka</trd>
</drv>

<drv mrk="amuz.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="amuz.0isto.KOMUNE">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Profesia gajigisto en cirko, teatro, ktp:
      <ekz>
        la <tld/>isto de la re&gcirc;o
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        Til' Strigospegulo, legenda mezepoka voja&gcirc;anta <tld/>isto
        <fnt>
          <vrk><url ref="https://eo.wiktionary.org/wiki/espi%C3%A8gle">espi&egrave;gle - Vikivortaro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili strebas ricevi la premion por la &leftquot;plej bona <tld/>isto&rightquot;
        <fnt>
          <vrk><url ref="https://helen85.livejournal.com/">helen85 &mdash; LiveJournal</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="arleke.0o">arlekeno</ref>,
      <ref cel="bufon.0o">bufono</ref>,
      <ref cel="burles.0ulo">burleskulo</ref>,
      <ref cel="klauxn.0o">kla&ubreve;no</ref>
    </refgrp>
  </snc>
  <trd lng="br">diduer</trd>
  <trdgrp lng="cs">
    <trd>bavi&ccaron;</trd>,
    <trd>humorista</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Unterhalter</trd>,
    <trd>Animateur</trd>,
    <trd>Spa&szlig;macher</trd>
  </trdgrp>
  <trd lng="en">entertainer</trd>
  <trdgrp lng="es">
    <trd>animador</trd>,
    <trd>c&oacute;mico</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>amuseur</trd>,
    <trd>comique<klr tip="amb"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="he">&bet;&dalet;&resh;&fnun;</trd>
  <trd lng="io">amuzero</trd>
  <trd lng="nl">entertainer</trd>
  <trdgrp lng="sk">
    <trd>humorista</trd>,
    <trd>zab&aacute;va&ccaron;</trd>
  </trdgrp>
</drv>

<drv mrk="amuz.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="amuz.mal0i.KOMUNE">
    <dif>
      Malagrable impresi,
      <ref tip="vid" cel="ted.0i">tedi</ref> a&ubreve;
      <ref tip="vid" cel="incit.0i">inciti</ref>:
      <ekz>
        a&ubreve;skulti vin ege mal<tld/>as min
        <fnt>
          <aut>E. Fabian</aut>,
          <vrk><url ref="http://groups.google.de/group/soc.culture.esperanto/browse_thread/thread/b555a759f065100e">
          Respondo al Michel</url></vrk>,
          <lok>soc.culture.esperanto, 2010-03-16</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ted.0i">tedi</ref>, 
      <ref cel="enu.0igi">enuigi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>nerven</trd>,
      <trd>bel&auml;stigen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="es">
    <trd>molestar</trd>,
    <trd>chinchar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>agacer</trd>,
    <trd>ennuyer</trd>
  </trdgrp>
  <trdgrp lng="th">
    <trd>&#3607;&#3635;&#3651;&#3627;&#3657;&#3652;&#3617;&#3656;&#3626;&#3609;&#3640;&#3585;</trd>,
    <trd>&#3607;&#3635;&#3651;&#3627;&#3657;&#3609;&#3656;&#3634;&#3648;&#3610;&#3639;&#3656;&#3629;</trd>
  </trdgrp>
</drv>
