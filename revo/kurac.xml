    <trd>&c_l;&c_je;&c_k;&c_a;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_l;&c_je;&c_k;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="bn">&#x9A1;&#x9BE;&#2453;&#x9CD;&#x9A4;&#x9BE;&#x9B0;</trd>
  <trd lng="bo">&t_a;&t_ma;&t_tsheg;&t_cha;&t_gigu;&t_tsheg;</trd>
  <trd lng="bs">doktor</trd>
  <trd lng="co">medico</trd>
  <trdgrp lng="cs">
    <trd>doktor</trd>,
    <trd>l&eacute;ka&rcaron;</trd>
  </trdgrp>
  <trd lng="cy">meddyg</trd>
  <trd lng="da">l&aelig;ge</trd>
  <trdgrp lng="de">
    <trd>Arzt</trd>,
    <trd>Mediziner</trd>,
    <trd>Heiler</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>doctor</trd>,
    <trd>physician</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>m&eacute;dico</trd>,
    <trd>doctor</trd>
  </trdgrp>
  <trd lng="et">arst</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;decin</trd>,
    <trd>docteur<klr> (m&eacute;decin)</klr></trd>
  </trdgrp>
  <trd lng="fy">dokter</trd>
  <trd lng="ga">docht&uacute;ir</trd>
  <trd lng="gd">dotair</trd>
  <trd lng="gl">m&eacute;dico</trd>
  <trd lng="gu">&#xAA1;&#xAC9;&#xA95;&#xACD;&#xA9F;&#xAB0;</trd>
  <trd lng="ha">likita</trd>
  <trd lng="hi">&#x91A;&#x93F;&#2325;&#x93F;&#2340;&#x94D;&#2360;&#2325;</trd>
  <trd lng="hmn">tus kws kho mob</trd>
  <trd lng="hr">lije&ccaron;nik</trd>
  <trd lng="ht">dokt&egrave;</trd>
