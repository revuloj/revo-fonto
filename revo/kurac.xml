<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>kurac</rad>/i
</kap>
<drv mrk="kurac.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <uzo tip="fak">MED</uzo>
   <snc>
     <dif>
       Prizorgi malsanulon kaj fari &ccirc;ion necesan por lin resanigi:
       <ekz>
         oni ne <tld/>as sole per medikamentoj<fnt>B</fnt>;
       </ekz>
       <ekz>
         li <tld/>is sin de siaj vundoj<fnt>Z</fnt>;
       </ekz>
       <ekz>
         unu vundo alian <tld/>as<fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         <uzo tip="stl">FIG</uzo>
         &scirc;uldon tempo ne <tld/>as<fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         nia <tld/>ato estas mia konato<fnt>Z</fnt>;
       </ekz>
       <ekz>
         ne<tld/>ebla kripla&jcirc;o, malsanulo.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_l;&c_ja;&c_ch;&c_y;&c_c;&c_mol;</trd>
   <trd lng="cs">l&eacute;&ccaron;it</trd>
   <trdgrp lng="de">
     <trd>behandeln</trd>,
     <trd>verarzten</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>doctor</trd>,
     <trd>treat (medically)</trd>,
     <trd>attend (a patient)</trd>,
     <trd>apply <ind>remedies</ind></trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>soigner<klr> (m&eacute;d.)</klr></trd>,
     <trd>traiter<klr> (m&eacute;d.)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>gy&oacute;gy&iacute;t</trd>,
     <trd>meggy&oacute;gy&iacute;t</trd>,
     <trd>gy&oacute;gykezel</trd>,
     <trd>orvosol</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>genezen<klr tip="ind"> (tr.)</klr></trd>,
     <trd>gezond maken</trd>,
     <trd>helen<klr tip="ind"> (gezond maken)</klr></trd>
   </trdgrp>
   <trd lng="ru">&c_l;&c_je;&c_ch;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="kurac.0ado">
   <kap><tld/>ado</kap>
   <uzo tip="fak">MED</uzo>
   <snc>
     <dif>
       Rimedoj kaj agoj uzataj por resanigi malsanulon:
       <ekz>
         severa <tld/>ado;
       </ekz>
       <ekz>
         <tld/>ado per akvoj mineralaj.
       </ekz>
     </dif>
     <refgrp tip="sub">
       <ref cel="diet.0o">dieto</ref>.
     </refgrp>
   </snc>
   <trd lng="be">&c_l;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
   <trdgrp lng="de">
     <trd>Behandlung</trd>,
     <trd>Verarzten</trd>
   </trdgrp>
   <trd lng="en">(course of) <ind>treatment</ind></trd>
   <trdgrp lng="fr">
     <trd>soins<klr> (m&eacute;dicaux)</klr></trd>,
     <trd>traitement<klr> (m&eacute;dical)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>gy&oacute;gy&iacute;t&aacute;s</trd>,
     <trd>gy&oacute;gykezel;t&eacute;s</trd>,
     <trd>orvosol;t&aacute;s</trd>
   </trdgrp>
   <trd lng="nl">behandeling</trd>
   <trd lng="ru">&c_l;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="kurac.0ejo">
   <kap><tld/>ejo</kap>
   <uzo tip="fak">MED</uzo>
   <snc>
     <dif>
       Loko aran&gcirc;ita por <tld/>i malsanulojn:
       <ekz>
         lia filo Adamo transprenis la okulistan <tld/>ejon
         <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro &leftquot;morto de Zamenhof&rightquot;, p.
         127a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="fr">
     <trd>cabinet m&eacute;dical</trd>,
     <trd>lieu de <ind>soins</ind></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>gy&oacute;gyint&eacute;zet</trd>,
     <trd>orvosi <ind>rendel&odblac;</ind></trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_l;&c_je;&c_ch;&c_je;&c_b;&c_n;&c_i;&c_c;&c_a;</trd>,
     <trd>&c_k;&c_l;&c_i;&c_n;&c_i;&c_k;&c_a;</trd>
   </trdgrp>
</drv>

<drv mrk="kurac.0ilo">
   <kap><ofc>*</ofc><tld/>ilo</kap>
   <uzo tip="fak">MED</uzo>
   <snc>
     <dif>
       &Ccirc;io, kion <tld/>isto uzas por resanigi malsanulon.
     </dif>
   </snc>
   <refgrp tip="sin">
     <ref cel="medici.0ajxo">medicina&jcirc;o</ref>,
     <ref cel="san.re0igilo">resanigilo</ref>
   </refgrp>
   <trdgrp lng="be">
     <trd>&c_l;&c_ja;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
     <trd>&c_l;&c_je;&c_k;&c_ib;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Heilmittel</trd>,
     <trd>Medizin</trd>,
     <trd>Arznei</trd>
   </trdgrp>
   <trd lng="en">remedy</trd>
   <trdgrp lng="fr">
     <trd>rem&egrave;de</trd>
   </trdgrp>
   <trd lng="hu">gy&oacute;gyeszk&ouml;z</trd>
   <trdgrp lng="nl">
     <trd>medicijn</trd>,
     <trd>remedie</trd>,
     <trd>geneesmiddel</trd>
   </trdgrp>
   <trd lng="ru">&c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="kurac.0isto">
   <kap><tld/>isto<fnt>Z</fnt></kap>
   <uzo tip="fak">MED</uzo>
   <snc>
     <dif>
       Medicinisto kiu aktive <tld/>as homojn.
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_d;&c_o;&c_k;&c_t;&c_a;&c_r;</trd>,
     <trd>&c_l;&c_je;&c_k;&c_a;&c_r;</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Arzt</trd>,
     <trd>Mediziner</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>doctor</trd>,
     <trd>physician</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>m&eacute;decin</trd>,
     <trd>docteur<klr> (m&eacute;decin)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>orvos</trd>,
     <trd>doktor</trd>
   </trdgrp>
   <trd lng="nl">dokter</trd>
   <trd lng="ru">&c_v;&c_r;&c_a;&c_ch;</trd>
   <trd lng="vo">sanel</trd>
</drv>
</art>
