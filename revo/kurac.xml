<drv mrk="kurac.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kurac.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Agadi por forigi a&ubreve; mildigi malsanon, por redoni sanon.
    </dif>
    <subsnc mrk="kurac.0i.MED_iun">
    <dif>
      Prizorgi malsanulon kaj fari &ccirc;ion necesan por lin resanigi:
      <ekz>
        virino, kiu <tld/>as, estas <tld/>istino, edzino de <tld/>isto estas
        <tld/>istedzino
        <fnt>
          <bib>F</bib>
          <lok>&FE; 36</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o <nom>Joram</nom> revenis, por <tld/>i sin en <nom>Jizreel</nom> de la vundoj
        <fnt>
          <bib>MT</bib>
          <lok>&Re2; 8:29</lok>
        </fnt>.
      </ekz>
    </dif>
    </subsnc>
    <subsnc mrk="kurac.0i.MED_ion">
    <dif>
      Prizorgi organon a&ubreve; vivfunkcion por &gcirc;in resanigi:
      <ekz>
        unu vundo alian <tld/>as<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pastro, kiu <tld/>as la okulojn
        <fnt>
          <bib>Far1</bib>
          <lok>&ccirc;apitro 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;uldon tempo ne <tld/>as<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    </subsnc>
  </snc>  
  <trd lng="be">&c_l;&c_ja;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trd lng="bo">&t_ra;&t_tata;&t_gigu;&t_nga;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>hojit</trd>,
    <trd>kur&yacute;rovat</trd>,
    <trd>l&eacute;&ccaron;it</trd>,
    <trd>vyl&eacute;&ccaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>behandeln</trd>,
    <trd>verarzten</trd>,
    <trd>kuren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>doctor</trd>,
    <trd>treat (medically)</trd>,
    <trd>attend (a patient)</trd>,
    <trd>apply <ind>remedies</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>curar</trd>,
    <trd>tratar (m&eacute;dicamente)</trd>,
    <trd>atender (un paciente)</trd>,
    <trd>administrar <ind>medicamentos</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>soigner<klr> (m&eacute;d.)</klr></trd>,
    <trd>traiter<klr> (m&eacute;d.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&oacute;gy&iacute;t</trd>,
    <trd>meggy&oacute;gy&iacute;t</trd>,
    <trd>gy&oacute;gykezel</trd>,
    <trd>orvosol</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>genezen<klr tip="ind"> (tr.)</klr></trd>,
    <trd>gezond maken</trd>,
    <trd>helen<klr tip="ind"> (gezond maken)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>leczy&cacute;</trd>,
    <trd>kurowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_ch;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hoji&tcaron;</trd>,
    <trd>lie&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&#x43B;&c_ib;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="kurac.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="kurac.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Ago <tld/>i a&ubreve; ties rezulto:
      <ekz>
        por longa malsano <tld/>' estas vana
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        zorgu pri lia <tld/>ado
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 21:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        sin submeti al <tld/>ado
        <fnt>
          <bib>Far2</bib>
          <lok>&ccirc;apitro 23a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastroj, kiuj okupi&gcirc;as per la <tld/>ado, konsilas &scirc;an&gcirc;adi la nutra&jcirc;on
        <fnt>
          <bib>Far1</bib>
          <lok>&ccirc;apitro 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi iom konas la <tld/>arton
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="diet.0o">dieto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>l&eacute;&ccaron;ba</trd>,
    <trd>l&eacute;&ccaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Behandlung</trd>,
    <trd>Verarzten</trd>,
    <trd>Kur</trd>
  </trdgrp>
  <trd lng="en">(course of) <ind>treatment</ind></trd>
  <trdgrp lng="es">
    <trd>tratamiento</trd>,
    <trd>terapia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>soins<klr> (m&eacute;dicaux)</klr></trd>,
    <trd>traitement<klr> (m&eacute;dical)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&oacute;gy&iacute;t&aacute;s</trd>,
    <trd>gy&oacute;gykezel;t&eacute;s</trd>,
    <trd>orvosol;t&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">behandeling</trd>
  <trdgrp lng="pl">
    <trd>leczenie</trd>,
    <trd>kuracja</trd>,
    <trd>terapia</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>k&uacute;ra</trd>,
    <trd>lie&ccaron;ba</trd>
  </trdgrp>
  <trd lng="uk">&#x43B;&c_ib;&#x43A;&c_u;&c_v;&c_a;&#x43D;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="kurac.0ato">
  <kap><tld/>ato</kap>
  <snc mrk="kurac.0ato.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Tiu, kiu estas <tld/>ata, <ref tip="sin" cel="pacien1.0o.MED">paciento</ref>:
      <ekz>
        nia <tld/>ato estas mia konato
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        venigu la <tld/>aton 33
        <fnt>
          <aut>trad. senlime</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Diktator.htm">
          La Granda Diktatoro</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_c;&c_y;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_h;&c_v;&c_o;&c_r;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">pacient</trd>
  <trd lng="de">Patient</trd>
  <trd lng="en">patient</trd>
  <trd lng="es">paciente</trd>
