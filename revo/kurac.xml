<drv mrk="kurac.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="kurac.0ejo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Loko aran&gcirc;ita por <tld/>i malsanulojn:
      <ekz>
        por li la <tld/>ejo montri&gcirc;os <tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2004/008358.html">Kuracbaneje</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Malio ekzistas nun 600 <tld/>ejoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2005/008741.php">Norduloj ebligas sudan
          sanon</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        pacientoj estas devigitaj viziti privatajn <tld/>ejojn kaj, memevidente, pagi
        pli ol la&ubreve; la &scirc;tata sistemo
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2007/009246.php">Komence kora, fine
          rankora</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia filo Adamo transprenis la okulistan <tld/>ejon
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro &leftquot;morto de
        Zamenhof&rightquot;, p. 127a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="san.mal0ulejo">malsanulejo</ref>,
      <ref cel="sanato.0o.MED">sanatorio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>l&eacute;&ccaron;ebna</trd>,
    <trd>sanatorium</trd>,
    <trd>zdravotnick&eacute; st&rcaron;edisko</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Heilst&auml;tte</trd>,
    <trd>Kurort</trd>,
    <trd>Behandlungszimmer</trd>,
    <trd>Praxis <klr>(Med.)</klr></trd>,
  </trdgrp>
  <trdgrp lng="en">
    <trd>health resort</trd>,
    <trd>sanatorium</trd>,
    <trd>nursing home</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ambulatorio</trd>,
    <trd>consultorio m&eacute;dico</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cabinet m&eacute;dical</trd>,
    <trd>lieu de <ind>soins</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&oacute;gyint&eacute;zet</trd>,
    <trd>orvosi <ind>rendel&odblac;</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szpital</trd>,
    <trd>lecznica</trd>,
    <trd>sanatorium</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_ch;&c_je;&c_b;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_k;&c_l;&c_i;&c_n;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">lie&ccaron;eb&ncaron;a</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_ib;&#x43A;&c_a;&c_r;&#x43D;&#x44F;</trd>,
    <trd>&c_a;&#x43C;&c_b;&c_u;&#x43B;&c_a;&c_t;&#x43E;&c_r;&c_ib;&#x44F;</trd>,
    <trd>&c_s;&c_a;&#x43D;&c_a;&c_t;&#x43E;&c_r;&c_ib;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="kurac.0ilo">
  <kap><ofc>*</ofc><tld/>ilo</kap>
  <snc mrk="kurac.0ilo.MED">
    <tezrad fak="MED"/>
    <uzo tip="fak">MED</uzo>
    <dif>
      &Ccirc;io, kion <tld/>isto uzas por resanigi malsanulon:
      <ekz>
        <ind>sor&ccirc;a <tld/>ilo</ind>
        <fnt>
          <bib>Far2</bib>
          <lok>&ccirc;apitro 1a</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Zaubertrank</trd>,
          <trd>Wundermittel</trd>
        </trdgrp>
      </ekz>
      <ekz>
        parton de la radikoj &scirc;i volis uzi kiel aldonon al la kafo, la aliajn &scirc;i volis
        vendi al la apotekisto kiel <tld/>ilojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Ekzistas diferenco</lok>
        </fnt>;
      </ekz>
      <ekz>
        post morto <tld/>ilo jam estas sen utilo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="medici.0ajxo">medicina&jcirc;o</ref>,
      <ref cel="san.re0igilo">resanigilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_l;&c_je;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="bo">&t_sa;&t_mata;&t_na;&t_tsheg;</trd>
  <trdgrp lng="de">
    <trd>Heilmittel</trd>,
    <trd>Medizin</trd>,
    <trd>Arznei</trd>
  </trdgrp>
  <trd lng="en">remedy</trd>
  <trdgrp lng="es">
    <trd>medicamento</trd>,
    <trd>remedio</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rem&egrave;de</trd>
  </trdgrp>
  <trd lng="hu">gy&oacute;gyeszk&ouml;z</trd>
  <trdgrp lng="nl">
    <trd>medicijn</trd>,
    <trd>remedie</trd>,
    <trd>geneesmiddel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lekarstwo</trd>,
    <trd>lek</trd>,
    <trd>medykament</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">liek</trd>
  <trdgrp lng="cs">
    <trd>jin&yacute; n&aacute;stroj l&eacute;&ccaron;by</trd>,
    <trd>l&eacute;k <klr>(proti)</klr></trd>
  </trdgrp>
  <trd lng="uk">&#x43B;&c_ib;&#x43A;&c_i;</trd>
</drv>

<drv mrk="kurac.0isto">
  <kap><ofc>*</ofc><tld/>isto</kap>
  <snc mrk="kurac.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Homo kiu profesie <tld/>as:
      <ekz>
        venigu la <tld/>iston, &ccirc;ar mi estas malsana
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>isto konsilis al mi iri en &scirc;vitbanejon
        <fnt>
          <bib>F</bib>
          <lok>&FE; 40</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jozef</nom> ordonis al siaj servantoj <tld/>istoj, balzami lian patron
        <fnt>
          <bib>MT</bib>
          <lok>&Gen; 50:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne la sanuloj bezonas <tld/>iston, sed la malsanuloj
        <fnt>
          <bib>NT</bib>
          <lok>&Mat; 9:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> multe suferis pro multaj <tld/>istoj, kaj elspezis sian tutan
        havon, kaj estis neniel helpita, sed kontra&ubreve;e e&ccirc; pli malsani&gcirc;is
        <fnt>
          <bib>NT</bib>
          <lok>&Mar; 5:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej bona <tld/>isto estas la tempo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">dokter</trd>
  <trd lng="am">&#x12F6;&#x12AD;&#4720;&#x122D;</trd>
  <trd lng="ar">&a_A;&a_l;&a_T;&a_b;&a_y;&a_b;</trd>
