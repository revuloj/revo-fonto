<drv mrk="kurac.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="kurac.0ejo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Loko aran&gcirc;ita por <tld/>i malsanulojn:
      <ekz>
        por li la <tld/>ejo montri&gcirc;os <tld/>a
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2004/008358.html">Kuracbaneje</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Malio ekzistas nun 600 <tld/>ejoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2005/008741.php">Norduloj ebligas sudan
          sanon</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        pacientoj estas devigitaj viziti privatajn <tld/>ejojn kaj, memevidente, pagi
        pli ol la&ubreve; la &scirc;tata sistemo
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2007/009246.php">Komence kora, fine
          rankora</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia filo Adamo transprenis la okulistan <tld/>ejon
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro &leftquot;morto de
        Zamenhof&rightquot;, p. 127a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="san.mal0ulejo">malsanulejo</ref>,
      <ref cel="sanato.0o.MED">sanatorio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_p;&c_ib;&c_t;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_l;&c_ja;&c_k;&c_a;&c_r;&c_n;&c_ja;</trd>,
