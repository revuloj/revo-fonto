<drv mrk="kurac.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kurac.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Agadi por forigi a&ubreve; mildigi malsanon, por redoni sanon.
    </dif>
    <subsnc mrk="kurac.0i.MED_iun">
      <dif>
        Prizorgi malsanulon kaj fari &ccirc;ion necesan por lin <ref tip="sin"
        cel="san.re0igi.MED">resanigi</ref>:
        <ekz>
          virino, kiu <tld/>as, estas <tld/>istino, edzino de <tld/>isto estas
          <tld/>istedzino
          <fnt>
            <bib>F</bib>
            <lok>&FE; 36</lok>
          </fnt>;
        </ekz>
        <ekz>
          la re&gcirc;o <nom>Joram</nom> revenis, por <tld/>i sin en <nom>Jizreel</nom> de la vundoj
          <fnt>
            <bib>MT</bib>
            <lok>&Re2; 8:29</lok>
          </fnt>;
        </ekz>
        <ekz>
          oni min venigis por <tld/>i junulon, kiu, falinte el bicikleto, grave vundis
          sian genuon
          <fnt>
            <bib>ChL</bib>
            <lok>Noto De L&#8217; A&ubreve;toro</lok>
          </fnt>;
        </ekz>
        <ekz>
          ne, li ne studis, li <tld/>as pro filantropio
          <fnt>
            <bib>PatrojFiloj</bib>
            <lok>20</lok>
          </fnt>;
        </ekz>
        <ekz>
          se vi estos man&gcirc;inta la rusan salaton, vin oni <tld/>os en rusa disenteria
          hospitalo
          <fnt>
            <bib>Metrop</bib>
          </fnt>!
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="kurac.0i.MED_ion">
      <dif>
        Prizorgi organon a&ubreve; vivfunkcion por &gcirc;in resanigi:
        <ekz>
          pastro, kiu <tld/>as la okulojn
          <fnt>
            <bib>Far1</bib>
            <lok>&ccirc;apitro 12a</lok>
          </fnt>;
        </ekz>
        <ekz>
          aliaj kuracistoj opinias, ke kiam la hepato estas malsana, oni devas &gcirc;in <tld/>i
          per &gcirc;uste kontra&ubreve;aj rimedoj
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro 1a</lok>
          </fnt>;
        </ekz>
        <ekz>
          unu vundo alian <tld/>as<fnt><bib>PrV</bib></fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          &scirc;uldon tempo ne <tld/>as<fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
    </subsnc>
    <ref tip="vid" cel="san.0igi">sanigi</ref>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trd lng="bo">&t_ra;&t_tata;&t_gigu;&t_nga;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>hojit</trd>,
    <trd>kur&yacute;rovat</trd>,
    <trd>l&eacute;&ccaron;it</trd>,
    <trd>vyl&eacute;&ccaron;it</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>behandeln</trd>,
    <trd>verarzten</trd>,
    <trd>kuren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>doctor</trd>,
    <trd>treat (medically)</trd>,
    <trd>attend (a patient)</trd>,
    <trd>apply <ind>remedies</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>curar</trd>,
    <trd>tratar (m&eacute;dicamente)</trd>,
    <trd>atender (un paciente)</trd>,
    <trd>administrar <ind>medicamentos</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>soigner<klr> (m&eacute;d.)</klr></trd>,
    <trd>traiter<klr> (m&eacute;d.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&oacute;gy&iacute;t</trd>,
    <trd>meggy&oacute;gy&iacute;t</trd>,
    <trd>gy&oacute;gykezel</trd>,
    <trd>orvosol</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>obat</baz>mengobati</trd>,
    <trd><baz>pulih</baz>memulihkan</trd>,
    <trd><baz>rawat</baz>merawat</trd>,
    <trd><baz>sembuh</baz>menyembuhkan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#27835;&#30274;&#12377;&#12427;  <pr>&#12385;&#12426;&#12423;&#12358;&#12377;&#12427;</pr></trd>,
    <trd>&#27835;&#12377;  <pr>&#12394;&#12362;&#12377;</pr></trd>,
    <trd>&#30290;&#12420;&#12377;  <pr>&#12356;&#12420;&#12377;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>genezen<klr tip="ind"> (tr.)</klr></trd>,
    <trd>gezond maken</trd>,
    <trd>helen<klr tip="ind"> (gezond maken)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>leczy&cacute;</trd>,
    <trd>kurowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_ch;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hoji&tcaron;</trd>,
    <trd>lie&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_l;&c_ib;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#21307;&#27835; <pr>y&imacron;zh&igrave;</pr></trd>,
    <trd>&#37291;&#27835; <pr>y&imacron;zh&igrave;</pr></trd>,
    <trd>&#21307; <pr>y&imacron;</pr></trd>,
    <trd>&#37291; <pr>y&imacron;</pr></trd>,
    <trd>&#30103; <pr>li&aacute;o</pr></trd>,
    <trd>&#30274; <pr>li&aacute;o</pr></trd>,
    <trd>&#27835;&#30103; <pr>zh&igrave;li&aacute;o</pr></trd>,
    <trd>&#27835;&#30274; <pr>zh&igrave;li&aacute;o</pr></trd>,
    <trd>&#30456;&#24453; <pr>xi&amacron;ngd&agrave;i</pr></trd>,
    <trd>&#21338;&#22763; <pr>b&oacute;sh&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kurac.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="kurac.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Ago <tld/>i a&ubreve; ties rezulto:
      <ekz>
        por longa malsano <tld/>' estas vana
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        zorgu pri lia <tld/>ado
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 21:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        sin submeti al <tld/>ado
        <fnt>
          <bib>Far2</bib>
          <lok>&ccirc;apitro 23a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastroj, kiuj okupi&gcirc;as per la <tld/>ado, konsilas &scirc;an&gcirc;adi la nutra&jcirc;on
        <fnt>
          <bib>Far1</bib>
          <lok>&ccirc;apitro 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi iom konas la <tld/>arton
        <fnt>
          <bib>ElektFab</bib>
          <lok>Kamarado Gaja</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="diet.0o">dieto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>l&eacute;&ccaron;ba</trd>,
    <trd>l&eacute;&ccaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Behandlung</trd>,
    <trd>Verarzten</trd>,
    <trd>Kur</trd>,
    <trd>Heilkunde</trd>
  </trdgrp>
  <trd lng="en">(course of) <ind>treatment</ind></trd>
  <trdgrp lng="es">
    <trd>tratamiento</trd>,
    <trd>terapia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>soins<klr> (m&eacute;dicaux)</klr></trd>,
    <trd>traitement<klr> (m&eacute;dical)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&oacute;gy&iacute;t&aacute;s</trd>,
    <trd>gy&oacute;gykezel;t&eacute;s</trd>,
    <trd>orvosol;t&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>obat</baz>pengobatan</trd>,
    <trd><baz>pulih</baz>pemulihan</trd>,
    <trd><baz>rawat</baz>perawatan</trd>,
    <trd><baz>sembuh</baz>penyembuhan</trd>,
    <trd>terapi</trd>
  </trdgrp>
  <trd lng="ja">&#27835;&#30274;  <pr>&#12385;&#12426;&#12423;&#12358;</pr></trd>
  <trd lng="nl">behandeling</trd>
  <trdgrp lng="pl">
    <trd>leczenie</trd>,
    <trd>kuracja</trd>,
    <trd>terapia</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>k&uacute;ra</trd>,
    <trd>lie&ccaron;ba</trd>
  </trdgrp>
  <trd lng="uk">&c_l;&c_ib;&c_k;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#21307;&#27835; <pr>y&imacron;zh&igrave;</pr></trd>,
    <trd>&#37291;&#27835; <pr>y&imacron;zh&igrave;</pr></trd>,
    <trd>&#21307;&#30103; <pr>y&imacron;li&aacute;o</pr></trd>,
    <trd>&#37291;&#30274; <pr>y&imacron;li&aacute;o</pr></trd>,
    <trd>&#30103;&#27861; <pr>li&aacute;of&#462;</pr></trd>,
    <trd>&#30274;&#27861; <pr>li&aacute;of&#462;</pr></trd>
  </trdgrp>
</drv>

