    <trd>orvos</trd>,
    <trd>doktor</trd>
  </trdgrp>
  <trd lng="hy">&#1378;&#x56A;&#x56B;&#1399;&#x56F;</trd>
  <trd lng="ig">d&#x1ECD;k&#x1ECB;ta</trd>
  <trd lng="is">l&aelig;knir</trd>
  <trd lng="ja">&#x533B;&#x5E2B;</trd>
  <trd lng="jv">dhokter</trd>
  <trd lng="ka">&#x10D4;&#x10E5;&#x10D8;&#x10DB;&#x10D8;</trd>
