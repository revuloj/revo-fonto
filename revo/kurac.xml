  </trdgrp>
  <trd lng="it">paziente</trd>
  <trd lng="nl">pati&euml;nt</trd>
  <trd lng="pl">pacjent</trd>
  <trd lng="pt">paciente</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_c;&c_i;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_b;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_j;<klr> (&c_v; &c_z;&c_n;. &c_s;&c_u;&c_shch;.)</klr></trd>
  </trdgrp>
  <trd lng="sk">pacient</trd>
  <trd lng="uk">&#x43F;&c_a;&c_c;&c_ib;&c_jeu;&#x43D;&c_t;</trd>
