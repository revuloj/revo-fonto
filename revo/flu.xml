        <trd>kifolyik</trd>,
        <trd>ki&ouml;mlik</trd>,
        <trd>ki&aacute;ramlik</trd>
      </trdgrp>
      <trd lng="nl">ontspringen</trd>
      <trd lng="ru">&c_v;&c_y;&c_t;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="flu.en0ejo">
