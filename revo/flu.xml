    <drv mrk="flu.dis0i">
      <kap>dis<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="flu.dis0i.cxiudirekten">
        <dif>
          <tld lit="F"/>i &ccirc;iudirekten.
        </dif>
        <trd lng="pl">rozp&lstroke;ywa&cacute;<klr> si&eogonek;</klr></trd>
      </snc>
      <snc mrk="flu.dis0i.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Disi&gcirc;i, disvasti&gcirc;i:
          <ekz>
            sonoj de flutoj dis<tld/>is en la silento;
          </ekz><ekz>
            en la urbo dis<tld/>as paroloj alarmaj;
          </ekz><ekz>
            la tuta popolo dis<tld/>is sur la placoj kaj stratoj
            <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 16a</lok></fnt>;
          </ekz><ekz>
            en la mallumo dis<tld/>is <klr>(malaperis)</klr> la suproj
            de la
            kolonoj<fnt>B</fnt>;
          </ekz><ekz>
            la koloroj kaj formoj dis<tld/>as en
            neesto<fnt>B</fnt>;
          </ekz><ekz>
            mi dis<tld/>i&gcirc;os kiel &scirc;a&ubreve;mo sur la maro
            <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="flug">disflugi</ref>,
          <ref cel="ir.dis0i">disiri</ref>,
          <ref cel="perd">perdi&gcirc;i</ref>.
        </refgrp>
        <trdgrp lng="pl">
          <trd>rozp&lstroke;ywa&cacute; si&eogonek;</trd>,
          <trd>rozmywa&cacute; si&eogonek;</trd>,
          <trd>rozpuszcza&cacute; si&eogonek;</trd>
        </trdgrp>
      </snc>
      <trd lng="be">
        &c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>
      <trdgrp lng="de">
        <trd>zerflie&szlig;en</trd>,
        <trd>sich <ind>ergie&szlig;en</ind></trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>se <ind>d&eacute;verser</ind><klr tip="ind"> (dans tous les
            sens)</klr></trd>,
        <trd>se <ind>r&eacute;pandre</ind></trd>,
        <trd>se <ind>diss&eacute;miner</ind></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>sz&eacute;folyik</trd>,
        <trd>sz&eacute;t&ouml;mlik</trd>,
        <trd>sz&eacute;t&aacute;ramlik</trd>
      </trdgrp>
      <trd lng="nl">wegvloeien</trd>
      <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_je;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </drv>

    <drv mrk="flu.el0i">
      <kap>el<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="flu.el0i.fluiEl">
        <dif>
          <tld lit="F"/>i el io:
          <ekz>
            fonto el<tld/>as el la monto.
          </ekz>
        </dif>
        <trd lng="de">herausflie&szlig;en</trd>
