<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>flu</rad>/i
    </kap>

    <drv mrk="flu.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="flu.0i.malsupren">
        <dif>
          Movi&gcirc;i seninterrompe malsupren la&ubreve; deklivo (p.p.
          flua&jcirc;oj):
          <ekz>
            la rivero rapide <tld/>as;
          </ekz><ekz>
            akvo <tld/>as en riveron, tra truo de barelo<fnt>B</fnt>;
          </ekz><ekz>
            sango <tld/>as en la korpo, el vundo;
          </ekz><ekz>
            liaj larmoj torente, rivere <tld/>is;
          </ekz><ekz>
            fandita bronzo <tld/>as en la modelilon.
          </ekz>
        </dif>
      </snc>
      <snc mrk="flu.0i.amase">
        <dif>
          Amase, seninterrompe direkti&gcirc;i ien:
          <ekz>
            mono <tld/>as al ri&ccirc;ulo, batoj al
            malri&ccirc;ulo<fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            &scirc;iaj abundaj haroj <tld/>is en grandaj bukloj super la
            &scirc;ultroj;
          </ekz><ekz>
            el <klr>[tiu nebulo]</klr> rivere <tld/>is nehomaj kriegoj
            <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 20</lok></fnt>;
          </ekz><ekz>
            en la landon al<tld/>is pli kaj pli multnombre fremdaj elementoj
            <fnt><bib>Far1</bib>, <lok>Anta&ubreve;parolo</lok></fnt>;
          </ekz><ekz>
            al<tld/>as fremdaj friponoj<fnt>Z</fnt>;
          </ekz><ekz>
            rigardi la preter<tld/>antan homamason.
          </ekz>
        </dif>
      </snc>
      <snc mrk="flu.0i.trapasi">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Facile kaj glate trapasi de iu loko a&ubreve; momento al alia:
          <ekz>
            la vivo en la urbeto <tld/>is unutone<fnt>B</fnt>, trankvile;
          </ekz><ekz>
            lia parolo <tld/>as dol&ccirc;e kaj agrable
            <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 31</lok></fnt>;
          </ekz><ekz>
            la tempo <tld/>is gaje
            <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 8</lok></fnt>;
          </ekz><ekz>
            en la aero <tld/>is odoro de violoj
            <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 24</lok></fnt>;
          </ekz><ekz>
            el &scirc;iaj haroj <tld/>is ebriiga odoro;
          </ekz><ekz>
            benoj <tld/>is sur la landon<fnt>Z</fnt>;
          </ekz><ekz>
            ondo da malvarmeta vento <tld/>is <klr>(enblovis)</klr> en la
            &ccirc;ambron<fnt>Z</fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="versx">ver&scirc;i&gcirc;i</ref>,
          <ref cel="kur.0i">kuri</ref>,
          <ref cel="flug.0i">flugi</ref>,
          <ref cel="rul.0igxi">ruli&gcirc;i</ref>.
        </refgrp>
      </snc>
      <trd lng="be">&c_c;&c_ja;&c_ch;&c_y;</trd>
      <trd lng="cs">t&eacute;ci</trd>
      <trd lng="de">flie&szlig;en</trd>
      <trd lng="en">flow</trd>
      <trd lng="es">fluir</trd>
      <trdgrp lng="fr">
        <trd>couler</trd>,
        <trd>s'<ind>&eacute;couler</ind></trd>,
        <trd>courir<klr tip="ind"> (passer, couler)</klr></trd>
      </trdgrp>
      <trd lng="nl">vloeien</trd>
      <trd lng="ru">&c_t;&c_je;&c_ch;&c_mol;</trd>
    </drv>

    <drv mrk="flu.0o">
      <kap><tld/>o</kap>
      <snc mrk="flu.0o.malsupren">
        <dif>
          Movo de tio, kio <tld/>as:
          <ekz>
            la <tld/>o de la rivero estas rapida;
          </ekz><ekz>
            na&gcirc;i kontra&ubreve; la <tld/>o;
          </ekz><ekz>
            <ind>sen<tld/>a</ind> akvo;
            <trd lng="fr">dormant<klr tip="ind"> (en p. d'eau)</klr></trd>
            <trd lng="nl">stil<klr tip="ind"> (water)</klr></trd>
          </ekz><ekz>
            la <tld/>o de la tempo, de la okazoj, de l' rememoroj.
          </ekz>
        </dif>
      </snc>
      <snc mrk="flu.0o.amasa">
        <dif>
          Io, kio amase kaj seninterrompe samdirekti&gcirc;as:
          <ekz>
            <tld/>o da homoj rapidas al la festo;
          </ekz><ekz>
            tiu regiono tre altiras la <tld/>on de la turistoj;
          </ekz><ekz>
            diversaj <tld/>oj da opinioj;
          </ekz><ekz>
            mi ne povas kontra&ubreve;stari al la <tld/>o<fnt>Z</fnt>;
          </ekz><ekz>
            <klr>[li]</klr> sin lasis kapti de l' &gcirc;enerala <tld/>o
            <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 17</lok></fnt>;
          </ekz><ekz>
            nova <tld/>o da pensoj regis lin<fnt>Z</fnt>;
          </ekz><ekz>
            elektra <tld/>o <klr>(<ref cel="kurent.0o">kurento</ref>)</klr>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="ond.0o">ondo</ref>,
          <ref cel="river.0o">rivero</ref>,
          <ref cel="torent.0o">torento</ref>.
        </refgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_c;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
        <trd>&c_p;&c_a;&c_t;&c_o;&c_k;</trd>
      </trdgrp>
      <trd lng="cs">tok</trd>
      <trdgrp lng="de">
        <trd><klr>(das)</klr> Flie&szlig;en</trd>,
        <trd>Strom</trd>,
        <trd>Fluss</trd>
      </trdgrp>
      <trd lng="es">flujo</trd>
      <trdgrp lng="fr">
        <trd>flux</trd>,
        <trd>cours<klr tip="ind"> (flux)</klr></trd>,
        <trd>courant<klr tip="ind"> (subst.)</klr></trd>
      </trdgrp>
      <trd lng="nl">stroom<klr tip="ind"> (v.rivier)</klr></trd>
      <trdgrp lng="ru">
        <trd>&c_t;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_p;&c_o;&c_t;&c_o;&c_k;</trd>
      </trdgrp>
    </drv>

    <drv mrk="flu.0a">
      <kap><tld/>a</kap>
      <snc mrk="flu.0a.fluanta">
        <dif>
          <tld lit="F"/>anta:
          <ekz>
            <tld/>a akvo.
          </ekz>
        </dif>
        <trd lng="be">&c_c;&c_ja;&c_k;&c_u;&c_ch;&c_y;</trd>
        <trd lng="de">flie&szlig;end</trd>
        <trd lng="fr">courant<klr tip="ind"> (adj., qui s'&eacute;coule,
            rapide)</klr></trd>
        <trd lng="ru">&c_t;&c_je;&c_k;&c_u;&c_ch;&c_i;&c_j;</trd>
      </snc>
      <snc mrk="flu.0a.facila">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Facilmova, facila, senhezita, rapida:
          <ekz>
            <tld/>a stilo;
          </ekz><ekz>
            <tld/>e paroli<fnt>Z</fnt>;
          </ekz><ekz>
            feli&ccirc;o venas gute, malfeli&ccirc;o venas
            <tld/>e<fnt><bib>PrV</bib></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_b;&c_je;&c_g;&c_l;&c_y;</trd>,
          <trd>&c_n;&c_ja;&c_z;&c_m;&c_u;&c_sh;&c_a;&c_n;&c_y;</trd>
        </trdgrp>
        <trd lng="fr">fluide<klr tip="ind"> (adj.)</klr></trd>
        <trdgrp lng="ru">
          <trd>&c_b;&c_je;&c_g;&c_l;&c_y;&c_j;</trd>,
         
<trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_n;&c_u;&c_zh;&c_d;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </snc>
      <trd lng="de">fl&uuml;ssig</trd>
      <trd lng="nl">vloeiend</trd>
    </drv>

    <drv mrk="flu.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc mrk="flu.0ajxo.FIZ">
        <uzo tip="fak">FIZ</uzo>
        <dif>
          Io <tld/>eca.
        </dif>
        <refgrp tip="sin">
          <ref cel="likv.0ajxo">likva&jcirc;o</ref>;
        </refgrp>
        <refgrp tip="vid">
          <ref cel="fluid.0ajxo">fluida&jcirc;o</ref>.
        </refgrp>
        <trd
lng="be">&c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
        <trd lng="de">Fl&uuml;ssigkeit</trd>
        <trd lng="fr">liquide<klr tip="ind"> (subst.)</klr></trd>
        <trd lng="ru">&c_zh;&c_i;&c_d;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
      </snc>
      <snc mrk="flu.0ajxo.fluas">
        <dif>
          Io, kio <tld/>as (p. f.).
        </dif>
      </snc>
      <trd lng="es">fluido</trd>
      <trd lng="nl">vloeistof</trd>
    </drv>

    <drv mrk="flu.0ajxa0eca">
      <kap><tld/>a&jcirc;a, <tld/>eca</kap>
      <uzo tip="fak">FIZ</uzo>
      <snc>
        <dif>
          Kvalifikas unu el la 3 statoj de la materio, inter la solida kaj
la
          gasa, kaj karakterizas substancon, kiu pro granda movebleco de la
          molekuloj prenas la formon de la ujo entenanta &gcirc;in, sed kies
          volumenon kunpremado povas apena&ubreve; &scirc;an&gcirc;i.
        </dif>
      </snc>
      <ref tip="sin" cel="likv.0a">likva</ref>
      <trd lng="be">&c_v;&c_a;&c_d;&c_k;&c_ib;</trd>
      <trd lng="de">fl&uuml;ssig</trd>
      <trd lng="fr">liquide<klr tip="ind"> (adj.)</klr></trd>
      <trd lng="nl">vloeibaar</trd>
      <trd lng="ru">&c_zh;&c_i;&c_d;&c_k;&c_i;&c_j;</trd>
    </drv>

    <drv mrk="flu.0ejo">
      <kap><tld/>ejo</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Kava&jcirc;o pli malpli profunda, en kiu <tld/>as rivero.
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_r;&c_e;&c_ch;&c_y;&c_sh;&c_ch;&c_a;</trd>,
        <trd>&c_l;&c_o;&c_zh;&c_a; <klr
tip="ind">(&c_r;&c_a;&c_k;&c_ib;)</klr></trd>
      </trdgrp>
      <trd lng="de">Flussbett</trd>
      <trdgrp lng="fr">
        <trd>cours<klr tip="ind"> (d'une rivi&egrave;re)</klr></trd>,
        <trd>lit<klr tip="ind"> (d'une rivi&egrave;re)</klr></trd>
      </trdgrp>
      <trd lng="nl">bedding</trd>
      <trd lng="ru">&c_r;&c_u;&c_s;&c_l;&c_o;</trd>
    </drv>

    <drv mrk="flu.al0i">
      <kap>al<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          <tld lit="F"/>i al iu loko:
          <ekz>
            al<tld/>as al tiu lago pluraj riveroj
            <fnt><bib>PIV1</bib></fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            per lerta admono al<tld/>as la mono
            <fnt><bib>PrV</bib></fnt>
          </ekz><ekz>
            <uzo tip="fak">GEOG</uzo>
            <ind>al<tld/>o</ind> de la tajdo;
            <trdgrp lng="de">
              <trd>Zustrom</trd>,
              <trd>Zufluss</trd>
            </trdgrp>
            <trdgrp lng="fr">
              <trd>afflux</trd>,
              <trd>flot<klr tip="ind"> (mar&eacute;e montante)</klr></trd>
            </trdgrp>
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            al<tld/>o da importitaj varoj
            <fnt><bib>PIV1</bib></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_r;&c_y;&c_c;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_p;&c_r;&c_y;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>zustr&ouml;men</trd>,
        <trd>zuflie&szlig;en</trd>
      </trdgrp>
      <trd lng="fr">affluer</trd>
    </drv>

    <drv mrk="flu.al0okfor0o">
      <kap>al<tld/>o kaj for<tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <ref tip="dif" cel="tajd.0o">tajdo</ref>
      <trd lng="be">&c_p;&c_r;&c_y;&c_l;&c_ib;&c_w; &c_ib;
&c_a;&c_d;&c_l;&c_ib;&c_w;</trd>
      <trdgrp lng="de">
        <trd>Gezeiten</trd>,
        <trd>Tide</trd>,
        <trd>Ebbe und Flut</trd>
      </trdgrp>
      <trd lng="en">ebb and flow</trd>
      <trd lng="fr">flux et reflux</trd>
      <trdgrp lng="nl">
        <trd>getijden</trd>,
        <trd>eb en vloed</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_r;&c_i;&c_l;&c_i;&c_v; &c_i;
&c_o;&c_t;&c_l;&c_i;&c_v;</trd>
    </drv>

    <drv mrk="flu.cxirkaux0i">
      <kap>&ccirc;irka&ubreve;<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          <tld lit="F"/>i &ccirc;e la &ccirc;irka&ubreve;o de:
          <ekz>
            <klr>[ili]</klr> ploras super la riveroj de Babilono,
            &ccirc;irka&ubreve;<tld/>antaj la ruinojn de ilia perdita
bonstato
            <fnt><bib>Marta</bib></fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            abunda gasa lumo &ccirc;irka&ubreve;<tld/>is tiun figuron
            <fnt><bib>Marta</bib></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="fr">
        <trd>couler<klr tip="ind"> (autour de)</klr></trd>,
        <trd>entourer<klr tip="ind"> (de ses flots)</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="flu.de0i">
      <kap>de<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Malproksimi&gcirc;i de io <tld/>ante:
          <ekz>
            mi &jcirc;etis &gcirc;ian polvon en la torenton,
            kiu de<tld/>as de la monto
            <fnt><bib>MT</bib><lok>&Rea; 9:21</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="fr">couler<klr tip="ind"> (&agrave; partir d'un
endroit)</klr></trd>
    </drv>

    <drv mrk="flu.de0ilo">
      <kap>de<tld/>ilo</kap>
      <snc>
        <dif>
          Kanaleto servanta por for<tld/>igi la pluvon, la malpurajn akvojn:
          <ekz>
            tegmenta, strata de<tld/>ilo<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_s;&c_mol;&c_c;&c_jo;&c_k;</trd>
      <trdgrp lng="de">
        <trd>Rinne</trd>,
        <trd>Abfluss</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>caniveau</trd>,
        <trd>goutti&egrave;re</trd>,
        <trd>canal d'&eacute;vacuation</trd>,
        <trd>d&eacute;versoir<klr tip="ind"> (canal
d'&eacute;vacuation)</klr></trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>goot</trd>,
        <trd>dakgoot</trd>
      </trdgrp>
      <trd lng="ru">&c_s;&c_t;&c_o;&c_k;</trd>
    </drv>

    <drv mrk="flu.dis0i">
      <kap>dis<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="flu.dis0i.cxiudirekten">
        <dif>
          <tld lit="F"/>i &ccirc;iudirekten.
        </dif>
      </snc>
      <snc mrk="flu.dis0i.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Disi&gcirc;i, disvasti&gcirc;i:
          <ekz>
            sonoj de flutoj dis<tld/>is en la silento;
          </ekz><ekz>
            en la urbo dis<tld/>as paroloj alarmaj;
          </ekz><ekz>
            la tuta popolo dis<tld/>is sur la placoj kaj stratoj
            <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 16</lok></fnt>;
          </ekz><ekz>
            en la mallumo dis<tld/>is <klr>(malaperis)</klr> la suproj de la
            kolonoj<fnt>B</fnt>;
          </ekz><ekz>
            la koloroj kaj formoj dis<tld/>as en
            neesto<fnt>B</fnt>;
          </ekz><ekz>
            mi dis<tld/>i&gcirc;os kiel &scirc;a&ubreve;mo sur la maro
            <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="flug">disflugi</ref>,
          <ref cel="ir.dis0i">disiri</ref>,
          <ref cel="perd">perdi&gcirc;i</ref>.
        </refgrp>
      </snc>
      <trd
lng="be">&c_r;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>
      <trdgrp lng="de">
        <trd>zerflie&szlig;en</trd>,
        <trd>sich <ind>ergie&szlig;en</ind></trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>se <ind>d&eacute;verser</ind><klr tip="ind"> (dans tous les
            sens)</klr></trd>,
        <trd>se <ind>r&eacute;pandre</ind></trd>,
        <trd>se <ind>diss&eacute;miner</ind></trd>
      </trdgrp>
      <trd lng="nl">wegvloeien</trd>
      <trd
lng="ru">&c_r;&c_a;&c_s;&c_t;&c_je;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </drv>

    <drv mrk="flu.el0i">
      <kap>el<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc mrk="flu.el0i.fluiEl">
        <dif>
          <tld lit="F"/>i el io:
          <ekz>
            fonto el<tld/>as el la monto.
          </ekz>
        </dif>
        <trd lng="be">&c_v;&c_y;&c_c;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>
      </snc>
      <snc mrk="flu.el0i.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Eli&gcirc;i el io:
          <ekz>
            el tiuj du faktoj jam per si mem el<tld/>as la
            konkludo, ke<klr>[...]</klr><fnt>Z</fnt>;
          </ekz><ekz>
            el<tld/>os multaj profitoj el tiu fonto<fnt>Z</fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_v;&c_y;&c_c;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>,
          <trd>&c_v;&c_y;&c_n;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
        </trdgrp>
        <trd lng="fr">d&eacute;couler</trd>
      </snc>
      <trdgrp lng="fr">
        <trd>jaillir</trd>,
        <trd>sortir</trd>,
        <trd>sourdre</trd>,
        <trd>surgir</trd>
      </trdgrp>
      <trd lng="nl">ontspringen</trd>
      <trd lng="ru">&c_v;&c_y;&c_t;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="flu.en0ejo">
      <kap>en<tld/>ejo</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Loko, kie rivero <tld/>as en alian riveron, en lagon a&ubreve; en
maron.
        </dif>
      </snc>
      <trd lng="be">&c_v;&c_u;&c_s;&c_mol;&c_c;&c_je; <klr
tip="amb">(&c_r;&c_a;&c_k;&c_ib;)</klr></trd>
      <trd lng="de">M&uuml;ndung</trd>
      <trdgrp lng="fr">
        <trd>embouchure<klr tip="ind"> (d'une rivi&egrave;re)</klr></trd>
      </trdgrp>
      <trd lng="nl">monding</trd>
      <trd lng="ru">&c_u;&c_s;&c_t;&c_mol;&c_je;</trd>
    </drv>

    <drv mrk="flu.kun0i">
      <kap>kun<tld/>(i&gcirc;)i</kap>
      <snc mrk="flu.kun0i.intermiksante">
        <dif>
          Kuni&gcirc;i intermiksante siajn akvojn:
          <ekz>
            Rejno kaj Mozelo kun<tld/>i&gcirc;as.
          </ekz>
        </dif>
      </snc>
      <snc mrk="flu.kun0i.FIG">
        <uzo tip="stl">FIG</uzo>
        <dif>
          Kuni&gcirc;i:
          <ekz>
            &ccirc;io kun<tld/>is en unu melodion
            <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la
paradizo</lok></fnt>;
          </ekz><ekz>
            espereble fari&gcirc;os iam kun<tld/>i&gcirc;o de la homoj
            en unu &ccirc;iuhoman popolon<fnt>Z</fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_s;&c_mol;&c_c;&c_ja;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>,
        <trd>&c_z;&c_mol;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
      </trdgrp>
      <trd lng="de">zusammenflie&szlig;en</trd>
      <trd lng="fr">confluer</trd>
      <trd lng="nl">samenvloeien</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_s;&c_t;&c_je;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </trdgrp>
    </drv>

    <drv mrk="flu.kun0ejo">
      <kap>kun<tld/>ejo</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Loko, kie du riveroj kun<tld/>i&gcirc;as.
        </dif>
      </snc>
      <trd lng="be">&c_z;&c_mol;&c_l;&c_ib;&c_c;&c_mol;&c_c;&c_jo;</trd>
      <trd lng="de">Zusammenfluss</trd>
      <trd lng="fr">confluent</trd>
      <trd lng="nl">samenvloeiing</trd>
      <trd lng="ru">&c_s;&c_l;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>
    </drv>

    <drv mrk="flu.sangal0o">
      <kap>sangal<tld/>o</kap>
      <snc>
        <dif>
          <ref tip="dif" cel="konges.0o">Kongesto</ref>.
        </dif>
      </snc>
    </drv>

    <drv mrk="flu.super0a">
      <kap>super<tld/>a</kap>
      <snc>
        <dif>
          Superanta la bezonon, superabunda:
          <ekz>
            spesmilo super<tld/>a po&scirc;on ne
&scirc;iras<fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            tiamaniere grandega
            nombro da vortoj fari&gcirc;as super<tld/>a por la
lernado<fnt>Z</fnt>;
          </ekz><ekz>
            de super<tld/>o malboni&gcirc;as la
&gcirc;uo<fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            pri tio mi trovas super<tld/>e tie &ccirc;i detale
            paroli<fnt>Z</fnt>.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="suficx.0ega">sufi&ccirc;ega</ref>,
          <ref cel="mank.mal0a">malmanka</ref>.
        </refgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_z;&c_a;&c_l;&c_ib;&c_sh;&c_n;&c_ib;</trd>,
        <trd>&c_p;&c_r;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_r;&c_n;&c_y;</trd>
      </trdgrp>
      <trd lng="de">&uuml;berfl&uuml;ssig</trd>
      <trd lng="fr">superflu</trd>
      <trd lng="nl">overbodig</trd>
      <trdgrp lng="ru">
        <trd>&c_l;&c_i;&c_sh;&c_n;&c_i;&c_j;</trd>,
        <trd>&c_i;&c_z;&c_l;&c_i;&c_sh;&c_n;&c_i;&c_j;</trd>
      </trdgrp>
    </drv>

    <drv mrk="flu.tra0eti">
      <kap>tra<tld/>eti</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Traguteti:
          <ekz>
            pluvo tra<tld/>etis tra fendo de la tegmento.
          </ekz>
        </dif>
      </snc>
      <adm>
        Kontrasti kun &leftquot;liki&rightquot; (ankoraux ne en Revo).
<aut>MB</aut>
      </adm>
      <trd lng="be">&c_p;&c_r;&c_a;&c_c;&c_ja;&c_k;&c_a;&c_c;&c_mol; <klr
tip="ind">(&c_p;&c_r;&c_a;&c_z;
&c_sh;&c_t;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
      <trd lng="fr">fuire<klr tip="ind"> (couler par une fente)</klr></trd>
      <trd lng="nl">lekken</trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>
    </drv>
