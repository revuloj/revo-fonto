    <drv mrk="flu.laux0e">
      <kap>la&ubreve;<tld/>e</kap>
      <snc mrk="flu.laux0e.KOMUNE">
        <dif>
          Irante la&ubreve; la direkto de fluo, la&ubreve; la natura,
          &gcirc;enerala irado:
          <ekz>
            la boato <klr>...</klr> rapide ekglitis la&ubreve;<tld/>e de la
            rivero <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>.
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            &scirc;i ne volis lasi la aferojn flosi la&ubreve;<tld/>e
            <fnt><bib>I&scirc;tar</bib>,
              <lok>&ccirc;apitro 21a, Zalmuna batalas kontra&ubreve; la
              &ccirc;efpastroj</lok>
            </fnt>.
          </ekz>
        </dif>
        <refgrp tip="ant">
          <ref cel="flu.kontraux0e.KOMUNE">kontra&ubreve;flue</ref>
        </refgrp>
      </snc>
      <trdgrp lng="fr">
        <trd>au fil de l'eau</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>z pr&aogonek;dem</trd>,
        <trd>z nurtem</trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_o; &c_t;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_ju;</trd>
    </drv>

    <drv mrk="flu.sangal0o">
      <kap>sangal<tld/>o</kap>
