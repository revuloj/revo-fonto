<drv mrk="flug.0pendi">
  <kap><tld/>pendi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="flug.0pendi.pende">
    <dif>
      <tld lit="F"/>i fiksita &ccirc;e &scirc;nuro ks:
      <ekz>
        super mia kapo <tld/>pendas grandega araneo kaj teksas &ccirc;irka&ubreve; mi retojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <klr>[&#x2026;]</klr> rekonscii&gcirc;is el mia rigideco, ekvidinte subite &ccirc;irka&ubreve; mi
        homajn figurojn, kun <tld/>pendantaj haroj kaj kun lar&gcirc;e malfermitaj okuloj
        <fnt>
          <bib>FdO</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="pendol.0i">pendoli</ref>
    </refgrp>
  </snc>
  <snc mrk="flug.0pendi.sxvebe">
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="sxveb.0i">&Scirc;vebi</ref>:
      <ekz>
        aglo vekas sian neston, <tld/>pendas super siaj idoj
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 32:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        blua fumo de pulvo <klr>[&#x2026;]</klr> <tld/>pendis super la akvo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glis.0i">glisi</ref>,
      <ref cel="sor1.0i">sori</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_n;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">klouzat vzduchem <klr>(kluz&aacute;k)</klr></trd>
  <trdgrp lng="es">
    <trd>planear</trd>,
    <trd>pender<klr tip="ind"> (en el aire)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>flotter<klr tip="ind"> (dans l'air)</klr></trd>,
    <trd>planer</trd>
  </trdgrp>
  <trd lng="nl">zweefvliegen</trd>
  <trdgrp lng="pl">
    <trd>zawisn&aogonek;&cacute;<klr> w powietrzu, na skrzyd&lstroke;ach
    </klr></trd>,
    <trd>szybowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">k&lacute;za&tcaron; vzduchom</trd>
</drv>
