<drv mrk="flug.0ila">
  <kap><tld/>ila</kap>
  <snc>
    <dif>
      <tld lit="F"/>ilhava:
      <ekz>
        <tld/>ila esta&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 10:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        preterflugis libeloj kaj aliaj <tld/>ilaj bestetoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        bildoj de <tld/>ilaj serpentoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        nigra <tld/>ila &ccirc;evalo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ila statuo de la diino
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_r;&c_y;&c_l;&c_a;&c_t;&c_y;</trd>
    <trdgrp lng="cs">
      <trd>k&rcaron;&iacute;dlov&yacute;</trd>,
      <trd>leteck&yacute;</trd>,
      <trd>ok&rcaron;&iacute;dlen&yacute;</trd>
    </trdgrp>
    <trd lng="de">gefl&uuml;gelt</trd>
    <trd lng="en">winged</trd>
    <trd lng="es">alado</trd>
    <trd lng="fr">ail&eacute;</trd>
    <trd lng="hu">sz&aacute;rnyas</trd>
    <trd lng="id">ber<ind>sayap</ind></trd>
    <trd lng="it">alato</trd>
    <trd lng="nl">gevleugeld</trd>
    <trdgrp lng="pl">
      <trd>skrzydlaty</trd>,
      <trd>uskrzydlony</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_r;&c_y;&c_l;&c_a;&c_t;&c_y;&c_j;</trd>
    <trdgrp lng="sk">
      <trd>kr&iacute;dlov&yacute;</trd>,
      <trd>okr&iacute;dlen&yacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="flug.0ila.de">
    <dif>
      De <tld/>ilo:
      <ekz>
        blanka <ind><tld/>ila plumo</ind>
        <fnt>
          <bib>Fab4</bib>
          <lok>Malbona princo (Legendo)</lok>
        </fnt>.
        <trd lng="de">Schwungfeder</trd>
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="flug.al0i">
  <kap><ofc>*</ofc>al<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="proksi.0igxi">Proksimi&gcirc;i</ref> al io <tld/>ante:
      <ekz>
        la birdo ne for<tld/>is: &gcirc;i nur de<tld/>is de la arbo,
        al<tld/>is al la domo kaj sur<tld/>is sur la tegmenton
        <fnt><bib>F</bib><lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        kiam venis la vespero, al<tld/>is koturnoj kaj kovris la tendaron
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 16:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite al<tld/>is granda majskarabo
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu anaso al<tld/>is kaj mordis la anasidon en la nuko
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trd lng="fr">voler<klr> (vers un endroit)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_i;&#x43B;&c_ib;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&#x43B;&c_ib;&c_t;&c_a;&c_t;&c_i; <klr>(&c_d;&#x43E; &c_ch;&#x43E;&c_g;&#x43E;-&#x43D;.)</klr></trd>
  </trdgrp>
</drv> 

<drv mrk="flug.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="flug.cxirkaux0i.kun_objekto">
    <gra><vspec>tr</vspec></gra>
    <dif>
      <tld lit="F"/>i &ccirc;irka&ubreve;:
      <ekz>
        Kaj &ccirc;u ne &ccirc;irka&ubreve;<tld/>as la armeon / La venko kaj e&ccirc; anta&ubreve; &gcirc;i rapidas
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>?
      </ekz>
      <ekz>
        kiam mi eniris en la &ccirc;ambron, <klr>[la pigoj]</klr>
        &ccirc;irka&ubreve;<tld/>is min da&ubreve;re dancante kaj kantante
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk><url ref="http://es.chinabroadcast.cn/1/2004/08/06/1@13346.htm">
        Du pigoj en mia hejmo</url></vrk>,
        <lok>2004-08-06</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">fly<klr tip="amb"> around</klr></trd>
    <trd lng="es">volar<klr tip="amb"> alrededor</klr></trd>
    <trdgrp lng="fr">
      <trd>contourner<klr tip="ind"> (en volant)</klr></trd>,
      <trd>voler autour (de)</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>oblecie&cacute;</trd>,
      <trd>lata&cacute; wok&oacute;&lstroke;<klr>
      kogo&sacute;, czego&sacute;</klr></trd>,
      <trd>lata&cacute; dooko&lstroke;a<klr>
      kogo&sacute;, czego&sacute;</klr></trd>,
      <trd>fruwa&cacute; wok&oacute;&lstroke;<klr>
      kogo&sacute;, czego&sacute;</klr></trd>,
      <trd>fruwa&cacute; dooko&lstroke;a<klr>
      kogo&sacute;, czego&sacute;</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="flug.cxirkaux0i.absolute">
    <gra><vspec>abs.</vspec></gra>
    <dif>
      <tld lit="F"/>i en la &ccirc;irka&ubreve;a&jcirc;o:
      <ekz>
        na&gcirc;u do, se &gcirc;uste &ccirc;irka&ubreve;<tld/>anta kargomasto, a&ubreve; kio ajn estis, dra&scirc;is sur
        viajn brakojn
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        skaraboj noktaj kaj sep noktaj papilioj senbrue
        &ccirc;irka&ubreve;<tld/>as &ccirc;irka&ubreve; &ccirc;ies kap'
        <fnt>
        <aut>K. Pi&ccirc;</aut>
        <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr45/fremdaj.html">
        Fremdaj landoj</url></vrk>
        <lok>Nica Literatura Revuo, 1959-04 &gcirc;is 05, p.
        184a-188a</lok>
        </fnt>;
      </ekz>
      <ekz>
        verdis tie herboj, prosperis floroj,
        &ccirc;irka&ubreve;<tld/>is paroj kaj paroj da blankaj gruoj
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk><url ref="http://es.chinabroadcast.cn/1/2004/07/12/1@12132.htm">
        Ru&gcirc;a fonto</url></vrk>,
        <lok>2004-07-12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la maljuna Kristoforo &ccirc;irka&ubreve;<tld/>is kiel juna birdo, &ccirc;iam vigla kaj kun vivema
        brilo en la okuloj
        <fnt>
          <bib>HsT</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">fly<klr tip="amb"> in circles</klr></trd>
    <trd lng="es">volar<klr tip="amb"> en c&iacute;rculos</klr></trd>
    <trd lng="fr">voler<klr tip="ind"> (dans les environs)</klr></trd>
    <trdgrp lng="pl">
      <trd>lata&cacute; dooko&lstroke;a</trd>,
      <trd>lata&cacute; woko&lstroke;o</trd>,
      <trd>fruwa&cacute; dooko&lstroke;a</trd>,
      <trd>fruwa&cacute; woko&lstroke;o</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol; <klr tip="amb">
(&c_v;&c_a;&c_k;&c_o;&c_l; &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)
    </klr></trd>,
    <trd>&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol; <klr tip="amb">
(&c_v;&c_a;&c_k;&c_o;&c_l; &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)
    </klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;<klr tip="amb">
    (&c_v;&c_o;&c_k;&c_r;&c_u;&c_g; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr>
    </trd>,
    <trd>&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;<klr tip="amb">
    (&c_v;&c_o;&c_k;&c_r;&c_u;&c_g; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x440;&#x443;&#x436;&#x43B;&#x44F;&#x442;&#x438;</trd>,
    <trd>&#x43E;&#x431;&#x43B;&#x456;&#x442;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv> 

<drv mrk="flug.de0i">
  <kap><ofc>*</ofc>de<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Malproksimi&gcirc;i de io <tld/>ante, malsupreniri <tld/>ante:
      <ekz>
        la birdo ne for<tld/>is: &gcirc;i nur de<tld/>is de la arbo,
        al<tld/>is al la domo kaj sur<tld/>is sur la tegmenton
        <fnt><bib>F</bib><lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        estis tia ventego, ke la tegoloj de<tld/>is de la tegmentoj
        <fnt><bib>Fab1</bib><lok>Vojkamarado</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i haltigis la obeeman balailon, iomete de<tld/>is
        flanken, poste subite atakis la diskon
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 21</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
