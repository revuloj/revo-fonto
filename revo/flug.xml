<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>flug</rad>/i
</kap>
<drv mrk="flug.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="flug.0i.sin_movi">
    <dif>
      Sin movi en la aero per tiucelaj membroj, kiel la birdoj kaj
      insektoj.
    </dif>
  </snc>
  <snc mrk="flug.0i.aviadile">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Sin movi en la aero per tiucela aparato, avio, aerostato a&ubreve; alia.
    </dif>
  </snc>
  <snc mrk="flug.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rapide trapasi de iu loko a&ubreve; momento en alian:
      <ekz>
        dikaj nuboj <tld/>is en la &ccirc;ielo;
      </ekz>
      <ekz>
        la rigardo <tld/>adas super bluaj vasta&jcirc;oj;
      </ekz>
      <ekz>
        <tld/>ema kiel vento<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> venis kun turbano kaj <tld/>etanta beduena
        mantelo
        <fnt><bib>Fab1</bib>, <lok>&Gcirc;ardeno de la paradizo</lok></fnt>;
      </ekz>
      <ekz>
        estis tia ventego, ke la tegoloj de<tld/>is de la tegmentoj
        <fnt><bib>Fab1</bib>, <lok>Vojkamarado</lok></fnt>;
      </ekz>
      <ekz>
        al li <tld/>as <klr>(sin direktas)</klr> mia kora
        danko<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la pulvo dis<tld/>as;
      </ekz>
      <ekz>
        dis<tld/>antaj haroj;
      </ekz>
      <ekz>
        la nova&jcirc;o rapide <ind>dis<tld/>i</ind>s tra la lando;
        <trd lng="fr">se <ind>r&eacute;pandre</ind></trd>
        <trd lng="nl">zich <ind>verspreiden</ind></trd>
      </ekz>
      <ekz>
        en la aero dis<tld/>is akraj krioj;
      </ekz>
      <ekz>
        murmuro de surprizi&gcirc;o el<tld/>is el la bu&scirc;oj,
        tra<tld/>as inter la &ccirc;eestantoj;
      </ekz>
      <ekz>
        <ind>tra<tld/>i</ind> senzorge la vivon;
        <trd lng="fr">traverser</trd>
      </ekz>
      <ekz>
        tiu penso tra<tld/>is lian spiriton;
      </ekz>
      <ekz>
        stranga ideo tra<tld/>is al li tra la kapo;
      </ekz>
      <ekz>
        la famo trans<tld/>is el unu bu&scirc;o en alian<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &jcirc;eti preter<tld/>antan rigardon sur iun<fnt>Z</fnt>.
      </ekz>
    </dif>
    <adm>
      "~ema kiel la vento". Mi sxatus koni la intencitan sencon
      de tiu adjektivo. PIV donas &leftquot;malkvietega&rightquot;, sed mi pensas pri
      fr &leftquot;volage&rightquot; (malkonstanta, malfidela). <aut>MB</aut>
    </adm>
  </snc>
  <refgrp tip="vid">
    <ref cel="flagr.0i">flagri</ref>,
    <ref cel="flirt.0i">flirti</ref>,
    <ref cel="flos.0i">flosi</ref>,
    <ref cel="flu.0i">flui</ref>,
    <ref cel="kur.0i">kuri</ref>,
    <ref cel="nagx.0i">na&gcirc;i</ref>,
    <ref cel="sxveb.0i">&scirc;vebi</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_jo;&c_t;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">let&ecaron;t</trd>
  <trd lng="de">fliegen</trd>
  <trd lng="en">fly</trd>
  <trd lng="es">volar</trd>
  <trdgrp lng="fr">
    <trd>voler<klr tip="ind"> (se d&eacute;placer dans l'air)</klr></trd>
  </trdgrp>
  <trd lng="hu">rep&uuml;l</trd>
  <trd lng="nl">vliegen</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_je;&c_t;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="flug.0o0ado">
  <kap><tld/>o, <tld/>ado</kap>
  <snc>
    <dif>
      Movo de io a&ubreve; iu <tld/>anta.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_jo;&c_t;</trd>
  <trd lng="de">Flug</trd>
  <trd lng="es">vuelo</trd>
  <trd lng="fr">vol<klr tip="ind"> (dans l'air)</klr></trd>
  <trd lng="hu">rep&uuml;l&eacute;s</trd>
  <trd lng="nl">vlucht</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_jo;&c_t;</trd>
</drv>

<drv mrk="flug.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="flug.0ilo.birda">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Organo &ccirc;e diversaj bestoj, kiun ili movas por sin porti en la aero:
      <ekz>
        <tld/>ilo de birdo, insekto;
      </ekz>
      <ekz>
        disfaldi, disvastigi<fnt>Z</fnt>,
        disvolvi la <tld/>ilojn<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="flug.0ilo.avia">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Simila kaj samcela parto de <tld/>aparato:
      <ekz>
        <tld/>iloj de aeroplano.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxveb">&scirc;vebilo</ref>.
    </refgrp>
  </snc>
  <snc mrk="flug.0ilo.POE">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="fak">POE</uzo>
    <dif>
      Movilo ebliganta facilan kaj rapidan <tld/>on, fizikan a&ubreve;
      metaforan:
      <ekz>
        <tld/>iloj de facila vento
        <fnt><aut>&Z;</aut>: <vrk>La espero.</vrk></fnt>;
      </ekz>
      <ekz>
        rigardu la perlon de la mal&gcirc;ojo, en &gcirc;i ku&scirc;as
        la spiritaj <tld/>iloj, kiuj portas nin for de &ccirc;i tie
        <fnt><bib>Fab3</bib>, <lok>Lasta perlo.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="flug.0ilo.alo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io forme simila al birda <tld/>ilo, precipe pri objektoj
      etendi&gcirc;antaj duope a&ubreve; kvarope &ccirc;e amba&ubreve;
      flankoj de iu a&jcirc;o:
      <ekz>
        <tld/>iloj de nazo, helico, muelilo, konstrua&jcirc;o, kastelo,
        armeo;
      </ekz>
      <ekz>
        la Alpoj &scirc;ajnis al li kvaza&ubreve; la kunmetitaj
        <tld/>iloj de la tero
        <fnt><bib>Fab3</bib>, <lok>Sub la saliko.</lok></fnt>;
      </ekz>
      <ekz>
        sur la <tld/>iloj de la sanktejo estos abomeninda dezerteco
        <fnt><bib>MT</bib>, <lok>&Dan; 9:27</lok>.</fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="al1.0o">alo.</ref>
    <refgrp tip="vid">
      <ref cel="flank.0ajxo">flanka&jcirc;o</ref>,
      <ref cel="lob.0o">lobo</ref>.
    </refgrp>
  </snc>
  <snc mrk="flug.0ilo.svingigxi">
    <dif>
      Tio, kio povas svingi&gcirc;i kiel <tld/>ilo:
      <ekz>
        <tld/>ilo de pordo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klap.0ego">klapego</ref>.
    </refgrp>
    <adm>
      En kio konsistas la sencdiferenco inter tiu senco de
      ~ilo kaj klapego? PIV1 ne donas Z-an ekz-on.
      <aut>MB</aut>
    </adm>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_l;&c_o;</trd>
  <trd lng="de">Fl&uuml;gel</trd>
  <trd lng="es">ala</trd>
  <trdgrp lng="fr">
    <trd>aile</trd>,
    <trd>battant</trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;rny</trd>
  <trd lng="nl">vleugel</trd>
  <trd lng="ru">&c_k;&c_r;&c_y;&c_l;&c_o;</trd>
</drv>

<drv mrk="flug.0ila">
  <kap><tld/>ila</kap>
  <snc>
    <dif>
      <tld lit="F"/>ilhava.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_l;&c_a;&c_t;&c_y;</trd>
  <trd lng="de">gefl&uuml;gelt</trd>
  <trd lng="es">alado</trd>
  <trd lng="fr">ail&eacute;</trd>
  <trd lng="nl">gevleugeld</trd>
  <trd lng="hu">sz&aacute;rnyas</trd>
  <trd lng="ru">&c_k;&c_r;&c_y;&c_l;&c_a;&c_t;&c_y;&c_j;</trd>
</drv>

<drv mrk="flug.0pendi0gliti">
  <kap><tld/>pendi, <tld/>gliti</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="sxveb.0i">&scirc;vebi.</ref>
  </snc>
  <trd lng="be">&c_l;&c_u;&c_n;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>flotter<klr tip="ind"> (dans l'air)</klr></trd>,
    <trd>planer</trd>
  </trdgrp>
  <trd lng="nl">zweefvliegen</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="flug.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <fnt><bib>PIV1</bib></fnt>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="flug.cxirkaux0i.kun_objekto">
    <dif>
      <tld lit="F"/>i &ccirc;irka&ubreve;:
      <ekz>
        kiam mi eniris en la &ccirc;ambron, <klr>[la pigoj]</klr>
        &ccirc;irka&ubreve;<tld/>is min da&ubreve;re dancante kaj kantante
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>
        <vrk>Du pigoj en mia hejmo</vrk>
        <lok><url ref="http://es.chinabroadcast.cn/1/2004/08/06/1@13346.htm">http://es.chinabroadcast.cn/1/2004/08/06/1@13346.htm</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>contourner<klr tip="ind"> (en volant)</klr></trd>,
      <trd>voler autour (de)</trd>
    </trdgrp>
  </snc>
  <snc mrk="flug.cxirkaux0i.absolute">
    <uzo tip="klr">(abs.)</uzo>
    <dif>
      <tld lit="F"/>i en la &ccirc;irka&ubreve;a&jcirc;o:
      <ekz>
        skaraboj noktaj kaj sep noktaj papilioj senbrue
        &ccirc;irka&ubreve;<tld/>as &ccirc;irka&ubreve; &ccirc;ies kap'
        <fnt>
        <aut>K. Pi&ccirc;</aut>
        <vrk>Fremdaj landoj</vrk>
        <lok><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr45/fremdaj.html">http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr45/fremdaj.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        verdis tie herboj, prosperis floroj,
        &ccirc;irka&ubreve;<tld/>is paroj kaj paroj da blankaj gruoj
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>
        <vrk>Ru&gcirc;a fonto</vrk>
        <lok><url ref="http://es.chinabroadcast.cn/1/2004/07/12/1@12132.htm">http://es.chinabroadcast.cn/1/2004/07/12/1@12132.htm</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">voler<klr tip="ind"> (dans les environs)</klr></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_v;&c_a;&c_k;&c_o;&c_l; &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>,
    <trd>&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_v;&c_a;&c_k;&c_o;&c_l; &c_ch;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_v;&c_o;&c_k;&c_r;&c_u;&c_g; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_v;&c_o;&c_k;&c_r;&c_u;&c_g; &c_ch;&c_je;&c_g;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="flug.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Malproksimi&gcirc;i de io <tld/>ante, malsupreniri <tld/>ante:
      <ekz>
        la birdo ne for<tld/>is: &gcirc;i nur de<tld/>is de la arbo,
        al<tld/>is al la domo kaj sur<tld/>is sur la tegmenton
        <fnt><bib>F</bib><lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        estis tia ventego, ke la tegoloj de<tld/>is de la tegmentoj
        <fnt><bib>Fab1</bib><lok>Vojkamarado</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i haltigis la obeeman balailon, iomete de<tld/>is flanken,
        poste subite atakis la diskon
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 21</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;e vi kun &ccirc;iu vorto kvaza&ubreve; Cicerono
        de<tld/>as de la lango
        <fnt><bib>Revizoro</bib><lok>4</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>&eacute;loigner</ind><klr tip="ind"> (en volant)</klr></trd>,
    <trd>voler<klr tip="ind"> (d'un endroit)</klr></trd>,
    <trd>s'<ind>envoler</ind><klr tip="ind"> (d'un endroit)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_t;&c_l;&c_je;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_je;&c_t;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="flug.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="flug.for0i.flugante">
    <dif>
      Foriri <tld/>ante.
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_c;&c_je;&c_c;&c_mol;</trd>
    <trd lng="hu">elrep&uuml;l</trd>
    <trd lng="nl">wegvliegen</trd>
    <trd lng="ru">&c_u;&c_l;&c_je;&c_t;&c_je;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="flug.for0i.malaperi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malaperi, fori&gcirc;i:
      <ekz>
        jam de longe for<tld/>is mia juneco;
      </ekz>
      <ekz>
        la dormo for<tld/>is de liaj okuloj.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
    <trd lng="de">verfliegen</trd>
    <trd lng="hu">elsz&aacute;ll</trd>
    <trd lng="nl">vervliegen</trd>
    <trd lng="ru">&c_u;&c_l;&c_je;&c_t;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>s'<ind>envoler</ind><klr tip="ind"> (au loin, dispara&icirc;tre)</klr></trd>,
    <trd>dispara&icirc;tre</trd>
  </trdgrp>
</drv>

<drv mrk="flug.alte0a">
  <kap>alte<tld/>a, alt<tld/>a</kap>
  <uzo tip="stl">FIG</uzo>
  <snc>
    <dif>
      Idealema, nobla, altespirita:
      <ekz>
        alt<tld/>aj pensoj;
      </ekz>
      <ekz>
        alte<tld/>aj kvaza&ubreve;sciencaj frazoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_s;&c_a;&c_k;&c_a;&c_m;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_y;&c_sh;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="de">hochfliegend</trd>
  <trd lng="fr">de haute <ind>vol&eacute;e</ind></trd>
  <trd lng="hu">sz&aacute;rnyal&oacute;</trd>
  <trd lng="ru">&c_v;&c_y;&c_s;&c_o;&c_k;&c_o;&c_p;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="flug.preter0e">
  <kap>preter<tld/>e</kap>
  <snc>
    <dif>
      Preterpase, rapide, senatente a&ubreve; sendetale:
      <ekz>
        preter<tld/>e a&ubreve;di ion<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>en <ind>passant</ind></trd>,
    <trd>au <ind>vol</ind></trd>
  </trdgrp>
  <trd lng="nl">in het <ind>voorbijgaan</ind></trd>
  <trd lng="hu">kutyafutt&aacute;ban</trd>
  <trd lng="ru">&c_n;&c_a; <ind>&c_l;&c_je;&c_t;&c_u;</ind></trd>
</drv>

<drv mrk="flug.ret0iluloj">
  <kap>ret<tld/>iluloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Planipennia</trd>, <trd lng="la">Neuroptera</trd>),
      kun kvar egalaj, membranecaj, rete nervuritaj <tld/>iloj.
    </dif>
  </snc>
  <trd lng="de">Netzfl&uuml;gler</trd>
  <trdgrp lng="fr">
    <trd>n&eacute;vropt&egrave;res</trd>,
    <trd>planipennes</trd>
  </trdgrp>
  <trd lng="nl">netvleugeligen</trd>
  <trd lng="hu">rec&eacute;ssz&aacute;rny&uacute;ak</trd>
  <trd lng="ru">&c_s;&c_je;&c_t;&c_ch;&c_a;&c_t;&c_o;&c_k;&c_r;&c_y;&c_l;&c_y;&c_je;<klr tip="amb"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
