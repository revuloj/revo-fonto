    <trd>&#x43F;&#x43E;&#x43B;&#x456;&#x442;</trd>
  </trdgrp>
</drv>

<drv mrk="flug.0ilo">
  <kap><ofc>*</ofc><tld/>ilo <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5028">flug'il'</url></lok></fnt></kap>
  <snc mrk="flug.0ilo.fizika">
    <dif>
      Solida surfaco, per kiu io porti&gcirc;as en aero.
    </dif>
    <subsnc mrk="flug.0ilo.birda">
      <uzo tip="fak">ZOO</uzo>
      <dif>
        <ref tip="super" cel="organ.0o.BIO">Organo</ref>
        &ccirc;e diversaj bestoj, kiun ili movas por sin porti en la aero:
        <ekz>
          aglaj <tld/>iloj
          <fnt>
            <bib>MT</bib>
            <lok>Eliro 19:4</lok>
          </fnt>;
        </ekz>
        <ekz>
          la flugiloj de <klr>[&#x2026;]</klr> vesperto
          <fnt>
            <bib>Far3</bib>
            <lok>&Ccirc;apitro IX</lok>
          </fnt>;
        </ekz>
        <ekz>
          disetendi
          <fnt>
            <bib>Far3</bib>
            <lok>&Ccirc;apitro XVI</lok>
          </fnt>,
          disvolvi
          <fnt>
            <bib>Marta</bib>
          </fnt>,
          disvastigi
          <fnt>
            <bib>FK</bib>
          </fnt> la <tld/>ilojn;
        </ekz>
        <ekz>
          se mi havus <tld/>ilojn kiel kolombo, mi for<tld/>us
          <fnt>
            <bib>MT</bib>
            <lok>Psalmaro 55:6</lok>
          </fnt>;
        </ekz>
        <ekz>
          kiel mu&scirc;o kun detran&ccirc;itaj <tld/>iloj
          <fnt>
            <bib>Revizoro</bib>
            <lok>Akto tria</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="ro">arip&abreve;</trd>
    </subsnc>
    <subsnc mrk="flug.0ilo.avia">
      <uzo tip="fak">AVI</uzo>
      <dif>
        Simila kaj samcela parto de 
        <ref tip="malprt" cel="aparat.flug0o"><tld/>aparato</ref>:
        <ekz>
          la piloto pendas de la &scirc;tofa <tld/>ilo per &scirc;nuroj
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=203908">Glispara&scirc;uto</url></lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>il&scirc;ar&gcirc;o estas la pezo de la aviadilo dividata per la areo de la <tld/>ilo
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=422577">Flugil&scirc;ar&gcirc;o</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="sxveb">&scirc;vebilo</ref>
      </refgrp>
      <trd lng="ro">palet&abreve;</trd>
    </subsnc>
  </snc>
  <snc mrk="flug.0ilo.POE">
    <uzo tip="stl">FIG</uzo>
    <uzo tip="fak">POE</uzo>
    <dif>
      Movilo ebliganta facilan kaj rapidan <tld/>on, fizikan a&ubreve;
      metaforan:
      <ekz>
        <tld/>iloj de facila vento
        <fnt><aut>&Z;</aut>: <vrk>La espero</vrk></fnt>;
      </ekz>
      <ekz>
        li porti&gcirc;is sur la <tld/>iloj de la vento
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 22:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        Dio de Izrael, sub kies <tld/>iloj vi ser&ccirc;is protekton
        <fnt>
          <bib>MT</bib>
          <lok>Rut 2:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        rigardu la perlon de la mal&gcirc;ojo, en &gcirc;i ku&scirc;as
        la spiritaj <tld/>iloj, kiuj portas nin for de &ccirc;i tie
        <fnt><bib>Fab3</bib>, <lok>Lasta perlo</lok></fnt>;
      </ekz>
      <ekz>
        flamaj <tld/>iloj, el kiuj unu estis doloro kaj la dua estis timo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i donis <tld/>ilojn al lia penso
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="flug.0ilo.alo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io forme simila al birda <tld/>ilo, precipe pri objektoj
      etendi&gcirc;antaj duope a&ubreve; kvarope &ccirc;e amba&ubreve; flankoj de iu a&jcirc;o:
      <ekz>
        koloraj kravatoj disvolvis siajn papiliajn <tld/>ilojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>iloj de vento<ref tip="malprt" cel="muel.0ilo.AGR">muelilo</ref>
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Steloj;monato2/007052.html">Verda nature, science,
          socie</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la Alpoj &scirc;ajnis al li kvaza&ubreve; la kunmetitaj
        <tld/>iloj de la tero
        <fnt><bib>Fab3</bib>, <lok>Sub la saliko</lok></fnt>;
      </ekz>
      <ekz>
        sur la <tld/>iloj de la sanktejo estos abomeninda dezerteco
        <fnt><bib>MT</bib>, <lok>&Dan; 9:27</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="al1.0o">alo</ref>
    <refgrp tip="vid">
      <ref cel="flank.0ajxo">flanka&jcirc;o</ref>,
      <ref cel="lob.0o">lobo</ref>.
    </refgrp>
  </snc>
  <snc mrk="flug.0ilo.svingigxi">
    <dif>
      Tio, kio povas svingi&gcirc;i kiel <tld/>ilo:
      <ekz>
        la dikaj, lignaj <tld/>iloj de la urbopordego estis kadukaj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>En la Lando de la Noktuloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;losil<tld/>ilo
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="klap.0o">klapo</ref>
    </refgrp>
  </snc>
