<drv mrk="flug.0o">
  <kap><tld/>o</kap>
  <snc mrk="flug.0o.fizika">
    <dif>
      Movo de io a&ubreve; iu <tld/>anta:
      <ekz>
        la le&gcirc;o faris limakan iron el tio, kio devis fari&gcirc;i <tld/>o de aglo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la trans<tld/>o de birdoj klare parolas pri la proksimeco de tiu &ccirc;i tero
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kura&gcirc;a subponta <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2003/008028.html">Alteri&gcirc;as nova
          prezidanto</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        ludante, l'&#x202F;araboj ponardojn &jcirc;etadis kaj ilin en <tld/>o kun &gcirc;ojo kaptadis
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la hejmon &scirc;i atingis <klr>[&#x2026;]</klr> en <tld/>o de penso
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        koni&gcirc;as birdo la&ubreve; <tld/>o kaj homo la&ubreve; ago
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="flug.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Planita iro de flugaparato:
      <ekz>
        la <tld/>o estis prokrastita je kvin horoj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Steloj;monato2/007007.html">Jugoslavio: Atenco ne
          preteratentita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pasa&gcirc;eroj de la <tld/>o 3-2-5 al <nom>Kuritiba</nom> kaj
        <nom>Porto Alegre</nom>, bonvolu direkti&gcirc;i al enaviiga pordo
        numero 2
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_jo;&c_t;</trd>
  <trd lng="bo">&t_ga;&t_na;&t_ma;&t_tsheg;&t_ga;&t_rata;&t_shabchu;&t_tsheg;&amp;t_-a;&t_ga;&t_rata;&t_naro;&t_tsheg;&t_ya;&t_ga;</trd>
  <trd lng="cs">let</trd>
  <trd lng="de">Flug</trd>
  <trd lng="en">flight</trd>
  <trd lng="es">vuelo</trd>
  <trd lng="fr">vol<klr tip="ind"> (dans l'air)</klr></trd>
  <trd lng="hu">rep&uuml;l&eacute;s</trd>
  <trd lng="id"><baz>terbang</baz>penerbangan</trd>
  <trd lng="it">volo</trd>
  <trd lng="nl">vlucht</trd>
  <trd lng="pl">lot</trd>
  <trd lng="ro">zbor</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_jo;&c_t;</trd>
  <trd lng="sk">let</trd>
