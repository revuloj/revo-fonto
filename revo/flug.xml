</drv>

<drv mrk="flug.for0i">
  <kap><ofc>*</ofc>for<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="flug.for0i.flugante">
    <dif>
      <ref tip="super" cel="ir.for0i">Foriri</ref> <tld/>ante:
      <ekz>
        la birdoj for<tld/>is, kiam la virino ekkriis
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li ellasis korvon, kaj &gcirc;i el<tld/>is, for<tld/>adis kaj revenadis
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 8:7</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ja;&c_c;&c_je;&c_c;&c_mol;</trd>
    <trd lng="de">wegfliegen</trd>
    <trd lng="en">fly<klr tip="amb"> away</klr></trd>
    <trd lng="es"><klr tip="amb">irse </klr>volando</trd>
    <trd lng="hu">elrep&uuml;l</trd>
    <trd lng="nl">wegvliegen</trd>
    <trdgrp lng="pl">
      <trd>odlatywa&cacute;</trd>,
      <trd>odfruwa&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_u;&c_l;&c_je;&c_t;&c_je;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="flug.for0i.malaperi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="aper.mal0i">Malaperi</ref>, 
      <ref tip="super" cel="for.0igxi">fori&gcirc;i</ref>:
      <ekz>
        kiel son&gcirc;o li for<tld/>os, kaj oni lin ne trovos
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 20:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;ojon kaj gajecon ili atingos, kaj for<tld/>os afliktoj kaj &gcirc;emado
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 51:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tago for<tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>Cefanja 2:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la momento de la morto mem for<tld/>as la iluzio
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        supra&jcirc;aj, ne precize komprenitaj scioj <klr>[&#x2026;]</klr> for<tld/>is el mia memoro
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
    <trd lng="de">verfliegen</trd>
    <trd lng="es">desaparecer<klr> (figurado)</klr></trd>
    <trd lng="hu">elsz&aacute;ll</trd>
    <trd lng="nl">vervliegen</trd>
    <trd lng="pl">odlatywa&cacute;</trd>
    <trd lng="ru">
    &c_u;&c_l;&c_je;&c_t;&c_u;&c_ch;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>ulet&ecaron;t</trd>,
    <trd>ul&eacute;tnout</trd>
  </trdgrp>
  <trd lng="de">wegfliegen</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>envoler</ind><klr tip="ind">
    (au loin, dispara&icirc;tre)</klr></trd>,
    <trd>dispara&icirc;tre</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odletie&tcaron;</trd>,
    <trd>uletie&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x434;&#x43B;&#x456;&#x442;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x43F;&#x443;&#x440;&#x445;&#x43D;&#x443;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="flug.for0ema">
  <kap>for<tld/>ema</kap>
  <snc mrk="flug.for0ema.KOMUNE">
    <dif>
      Kiu facile for<tld/>as, facile &scirc;an&gcirc;as sian lokon, sian
      inklinon;
      <ref tip="vid" cel="konsta.mal0a.KOMUNE">malkonstanta</ref>,
      <ref tip="vid" cel="pas.0ema.KOMUNE">pasema</ref>:
      <ekz>
        for<tld/>emaj iluzioj
        <fnt>
          <aut>R. Artigues</aut>,
          <vrk><url ref="http://eo.wikisource.org/wiki/La%C5%ADroj/La_filino_de_la_feinoj">
          La filino de la feinoj</url></vrk>,
          <lok>[1908]</lok>
        </fnt>.
      </ekz>                 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_jo;&c_t;&c_k;&c_ib;</trd>,
    <trd>&c_l;&c_ja;&c_t;&c_u;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;vanescent</trd>,
    <trd>volatil</trd>
  </trdgrp>
</drv>

<drv mrk="flug.surr0i">
  <kap><ofc>*</ofc>sur<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="F"/>i sur ion:
      <ekz>
        mi mortigus mu&scirc;on, kiu obstine sur<tld/>us mian man&gcirc;on
        <fnt>
          <bib>Kon11</bib>
          <aut>Luc Gouverneur</aut>
          <vrk>&Ccirc;u veganismo troigas?</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la usona aerforto sur<tld/>is la kuban teritorion por dissendi programojn de
        Televido &leftquot;Mart&iacute;&rightquot;, tiel rompante la regularon de la Internacia Unio pri
        Telekomunikado
        <fnt>
          <bib>MD02</bib>
          <vrk>La E&ubreve;ropa Unio kaj Havano</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la vagonaro sur<tld/>as viadukton, dis&jcirc;etante tondron en asfaltajn stratojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trd lng="fr">se poser sur <klr>(apr&egrave;s un vol)</klr></trd>
</drv>

<drv mrk="flug.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="flug.tra0i.fizike">
    <dif>
      <tld lit="F"/>e trairi:
      <ekz>
        falstelo kiel brilanta linio tra<tld/>is la &ccirc;ielon
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        sova&gcirc;aj kolomboj tra<tld/>adis inter la bran&ccirc;oj de la altaj arboj
        <fnt>
          <bib>Fab2</bib>
          <lok>Rozo de la tombo de Homero</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta aeroplano tra<tld/>is faskojn de grize blanka nebulo
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>, 
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="flug.tra0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rapide <ref tip="super" cel="pas.tra0i">trapasi</ref>:
      <ekz>
        &scirc;i tra<tld/>is malgrandan distancon, kaj la idoj faris malgrandan mallertan
        movi&gcirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>Cikonioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iumomente tra<tld/>is super li noktaj birdoj kaj grandaj vespertoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj &ccirc;i rimarkoj kun rapideco de l' vento tra<tld/>is la animon de l' kronprinco
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        murmureto de admiro tra<tld/>is inter la &ccirc;eestantoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        spirito tra<tld/>is anta&ubreve; mi, kaj la haroj sur mia korpo rigidi&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 4:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        tra<tld/>is an&gcirc;elo preter orelo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>
  <trd lng="fr">traverser en volant</trd>
  <trd lng="uk">&#x43F;&#x440;&#x43E;&#x43B;&#x456;&#x442;&#x430;&#x442;&#x438; <klr>(&#x43A;&#x440;&#x456;&#x437;&#x44C;)</klr></trd>
</drv>

<drv mrk="flug.alte0a">
  <kap>alte<tld/>a, <var><kap>alt<tld/>a</kap></var></kap>
  <uzo tip="stl">FIG</uzo>
  <snc>
    <dif>
      Idealema, nobla, altspirita:
      <ekz>
        alte<tld/>aj kvaza&ubreve;-sciencaj frazoj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        kie estas viaj alt<tld/>aj planoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>?
      </ekz>
      <ekz>
        simpla, ne alt<tld/>a poezio
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="&Monato;/2013/010730.php">Brile versform-fidela traduko
          &gcirc;uinda</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_s;&c_a;&c_k;&c_a;&c_m;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_y;&c_sh;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">vzletn&yacute;</trd>
  <trd lng="de">hochfliegend</trd>
  <trdgrp lng="es">
    <trd>idealista</trd>,
    <trd>de altos <ind>vuelos</ind></trd>
  </trdgrp>
  <trd lng="fr">de haute <ind>vol&eacute;e</ind></trd>
  <trd lng="hu">sz&aacute;rnyal&oacute;</trd>
  <trd lng="pl">g&oacute;rnolotny</trd>
  <trd lng="ru">
  &c_v;&c_y;&c_s;&c_o;&c_k;&c_o;&c_p;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">vzletn&yacute;</trd>
  <trd lng="uk">&c_d;&c_a;&#x43B;&c_je;&#x43A;&#x43E;&c_s;&#x44F;&c_zh;&#x43D;&c_i;&c_j;</trd>
</drv>

