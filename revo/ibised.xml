<kap>
  <rad>ibised</rad>/oj
</kap>
<drv mrk="ibised.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_familioj" cel="famili.0o.BIO">Familio</ref>
      el la ordo <ref tip="super" cel="bird.vad0oj">vadbirdoj</ref>
      (<trd lng="la">Threskiornithidae</trd>):
      <ekz>
        Ermitibiso estas migranta birdo el la <klr>[&#x2026;]</klr> familio de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=56774">Norda senharibiso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="ibis.0o">ibiso</ref>
  </snc>
  <trd lng="be">&c_ib;&c_b;&c_ib;&c_s;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="bg">&c_i;&c_b;&c_i;&c_s;&c_o;&c_v;&c_i;</trd>
  <trd lng="ca">tresquiorn&iacute;tids</trd>
  <trd lng="de">Ibisse<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="en">ibises</trd>
  <trd lng="fr">threskiornithid&eacute;s</trd>
  <trd lng="hu">&iacute;biszf&eacute;l&eacute;k</trd>
  <trd lng="nl">ibissen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">ibisowate</trd>
  <trd lng="pt">tresquiornit&iacute;deos</trd>
  <trd lng="ru">&c_i;&c_b;&c_i;&c_s;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="zh">&#x6731;&#x9DFA;&#x79D1; <pr>zh&#x16B;l&#xF9;k&#x113;</pr></trd>
</drv>
