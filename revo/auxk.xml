<vortaro>
<art mrk="$Id$">
<kap>
  <rad>a&ubreve;k</rad>/o
</kap>
<drv mrk="auxk.0o">
  <kap>
    <tld/>o <fnt><url ref="&Bib;PIV1">PIV</url></fnt>
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el la familio <ref
      tip="malprt"
      cel="auxked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Alca</trd>).
    </dif>
  </snc>
  <trd lng="cs">alka</trd>
  <trd lng="de">Alk</trd>
  <trd lng="en">razorbill</trd>
  <trd lng="fr">petit <ind>pingouin</ind></trd>
  <trd lng="hu">alka</trd>
  <trd lng="it">alca</trd>
  <trd lng="pl">alka</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_g;&c_a;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
