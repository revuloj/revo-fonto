<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>a&ubreve;k</rad>/o
</kap>
<drv mrk="auxk.0o">
  <kap>
    <tld/>o <fnt><url ref="&Bib;PIV1">PIV</url></fnt>
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
      cel="auxked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Alca</trd>).
    </dif>
  </snc>
  <trd lng="cs">alka</trd>
  <trd lng="de">Alk</trd>
  <trd lng="en">razorbill</trd>
  <trd lng="fr">petit <ind>pingouin</ind></trd>
  <trd lng="hu">alka</trd>
  <trd lng="pl">alka</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_g;&c_a;&c_r;&c_k;&c_a;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="auxk.0oformaj">
  <kap>
    <tld/>oformaj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Alciformes</trd>).
    </dif>
  </snc>
  <rim>
  Alko estas de familio "Alcidae", (la&ubreve; kelkaj
  fontoj subordo "Alcae") ordo "Charadriiformes".
  <aut>SC</aut>
  </rim>
  <trd lng="de">Alkv&ouml;gel</trd>
  <trd lng="hu">alkaalak&uacute;ak</trd>
  <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_i;&c_k;&c_i;<klr tip="amb">
  (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
