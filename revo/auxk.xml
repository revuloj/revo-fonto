<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>a&ubreve;k</rad>/o
</kap>
<drv mrk="auxk.0o">
   <kap><tld/>o <fnt><url ref="&Bib;PIV1">PIV</url></fnt></kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Genro el la familio <ref tip="super" cel="auxk.0edoj"><tld/>edoj</ref>
       (<trd lng="la">Alca</trd>).
     </dif>
   </snc>
   <trd lng="cs">alka</trd>
   <trd lng="de">Alk</trd>
   <trd lng="en">razorbill</trd>
   <trd lng="fr">petit <ind>pingouin</ind></trd>
   <trd lng="hu">alka</trd>
   <trd lng="pl">alka</trd>
   <trd lng="ru">&c_g;&c_a;&c_g;&c_a;&c_r;&c_k;&c_a;</trd>
</drv>
<drv mrk="auxk.0edoj">
   <kap><tld/>edoj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Familio el la ordo <ref tip="super" cel="auxk.0oformaj"><tld/>oformaj
       birdoj</ref> (<trd lng="la">Alcidae</trd>).
     </dif>
   </snc>
   <trd lng="de">Alken <klr tip="ind">(Familie)</klr></trd>
   <trd lng="fr">alcid&eacute;s</trd>
</drv>
<drv mrk="auxk.0oformaj">
   <kap><tld/>oformaj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
       (<trd lng="la">Alciformes</trd>).
     </dif>
   </snc>
   <trd lng="de">Alkv&ouml;gel</trd>
</drv>
</art>

