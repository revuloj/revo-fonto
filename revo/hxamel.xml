<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hxamel.xml,v 1.41 2022/10/02 17:39:24 revo Exp $">
<kap>
  <rad>&hcirc;ameleon</rad>/o <fnt>Z</fnt>,
  <var><kap><rad var="k">kameleon</rad>/o</kap></var>
</kap>

<drv mrk="hxamel.0o">
  <kap><tld/>o,
    <var><kap>kameleono</kap></var>
  </kap>
  <snc mrk="hxamel.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="reptil.0o"
      lst="voko:reptilioj">Reptilio</ref>
      kiu el&jcirc;etas sian longan langon por kapti insektojn kaj
      &scirc;an&gcirc;as sian koloron;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref>
      el la familio 
      <ref tip="super" cel="hxameleoned.0oj"><tld/>edoj</ref>
      (<trd lng="la">Chamaeleon</trd>):
      <ekz>
        jen, kio estas malpura por vi <klr>[&#8230;]</klr>: la talpo kaj
        la muso <klr>[&#8230;]</klr> kaj la <tld/>o
        <fnt><bib>MT</bib><lok>&Lev; 11:29&ndash;30.</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        nia &leftquot;de&#8221; estas speco de <tld/>o kun pluraj signifoj kaj roloj
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2011/010411.php"
              >La gramatikiloj de Esperanto la&ubreve; ABC</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la moralo estas nek <tld var="k"/>o nek mod-artikolo la&ubreve;pla&ccirc;e, la&ubreve;bezone kaj la&ubreve;okaze
        modifebla, for&jcirc;etebla a&ubreve; restarigebla
        <fnt>
          <bib>Monato</bib>
          <aut>Reinhard Haupenthal</aut>
          <vrk><url ref="&Steloj;monato2/006848.html"
              >Mallogika kaj senracia</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/2/2e/BennyTrapp_Chamaeleo_chamaeleon_Samos_Griechenland.jpg/640px-BennyTrapp_Chamaeleo_chamaeleon_Samos_Griechenland.jpg"
      prm="CC BY 3.0"
      ><tld/>o (Chamaeleo chamaeleon) en insulo Samos, Grekujo
        <fnt>
          <aut>Benny Trapp, propra verko</aut>
          <lok><url ref="&WCW;/File:BennyTrapp_Chamaeleo_chamaeleon_Samos_Griechenland.jpg"
              >&WCW;/File:BennyTrapp_Chamaeleo_chamaeleon_Samos_Griechenland.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_m;&c_e;&c_l;&c_je;&c_o;&c_n;</trd>
  <trd lng="bg">&c_h;&c_a;&c_m;&c_je;&c_l;&c_je;&c_o;&c_n;</trd>
  <trd lng="ca">camale&oacute;</trd>
  <trd lng="de">Cham&auml;leon</trd>
  <trd lng="en">chameleon</trd>
  <trd lng="es">camale&oacute;n</trd>
  <trd lng="fa">&a_A_madda;&a_f;&a_t;&a_A;&a_b;&#8204;&f_p;&a_r;&a_s;&a_t;</trd>
  <trd lng="fi">kameleontti</trd>
  <trd lng="fr">cam&eacute;l&eacute;on</trd>
  <trd lng="hr">kameleon</trd>
  <trd lng="hu">kam&eacute;leon</trd>
  <trd lng="id">bunglon</trd>
  <trd lng="it">camaleonte</trd>
  <trd lng="nl">kameleon</trd>
  <trd lng="pl">kameleon</trd>
  <trd lng="pt">camale&atilde;o</trd>
  <trd lng="ro">cameleon</trd>
  <trd lng="ru">&c_h;&c_a;&c_m;&c_je;&c_l;&c_je;&c_o;&c_n;</trd>
  <trd lng="sk">chamele&oacute;n</trd>
  <trd lng="sr">&c_k;&c_a;&c_m;&c_je;&c_l;&c_je;&c_o;&c_n;</trd>
  <trd lng="sv">kameleont</trd>
  <trd lng="uk">&c_h;&c_a;&c_m;&c_je;&c_l;&c_je;&c_o;&c_n;</trd>
</drv>

<drv mrk="hxamel.Hx0o">
  <kap><tld lit="&Hcirc;"/>o
<fnt>
  <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
  <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
  universo</vrk>.
  <lok>FEL, Antverpeno, 2005</lok>
</fnt>
</kap>
  <mlg>Cha</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="stel.0figuro"
      lst="voko:stelfiguroj">Stelfiguro</ref> (<trd
      lng="la">Chamaeleon</trd>) de la suda
      duonsfero:
      <ekz>
        NGC 2915 estas galaksio en konstelacio <tld lit="&Hcirc;"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=112847"
              >NGC 2915</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_H;&c_a;&c_m;&c_e;&c_l;&c_je;&c_o;&c_n;</trd>
  <trd lng="ca">el <ind>Camale&oacute;</ind></trd>
  <trd lng="de">Cham&auml;leon</trd>
  <trd lng="en">Chamaeleon</trd>
  <trd lng="es">Camele&oacute;n</trd>
  <trd lng="fa">&a_A_madda;&a_f;&a_t;&a_A;&a_b;&#8204;&f_p;&a_r;&a_s;&a_t;<klr tip="amb"> (&a_S;&a_w;&a_r;&a_t;
      &a_f;&a_l;&f_k;&f_y;)</klr></trd>
  <trd lng="fi">Kameleontti <klr tip="amb">
      (t&auml;hdist&ouml;)</klr></trd>
  <trd lng="fr">Cam&eacute;l&eacute;on<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="hu">Kam&eacute;leon</trd>
  <trd lng="nl">Kameleon<klr tip="ind">
      (sterrenbeeld)</klr></trd>
  <trd lng="pl">Kameleon  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Cameleonul  <klr>(constela&#539;ie)</klr></trd>
  <trd lng="ru">&c_H;&c_a;&c_m;&c_je;&c_l;&c_je;&c_o;&c_n;</trd>
  <trd lng="it">Camaleonte</trd>
</drv>

<drv mrk="hxamel.0a">
  <kap><tld/>a</kap>
  <snc mrk="hxamel.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre <ref tip="vid" cel="sxangx.0igxema">
        &scirc;an&gcirc;i&gcirc;ema</ref>,
      sin rapide konformanta al &ccirc;iuj novaj situacioj:
      <ekz>
        lia <tld/>a naturo kovradis al li la viza&gcirc;on kaj la
        staturon per <klr>[&#8230;]</klr> diversaj kaj rapide
        &scirc;an&gcirc;i&gcirc;antaj koloroj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_h;&c_a;&c_m;&c_e;&c_l;&c_je;&c_o;&c_n;&c_s;&c_k;&c_ib;</trd>
    <trd lng="ca">camale&ograve;nic</trd>
    <trd lng="de">wie ein <ind>Cham&auml;leon</ind></trd>
    <trd lng="fi">kameleonttimainen</trd>
    <trdgrp lng="fr">
      <trd>cam&eacute;l&eacute;onesque</trd>,
      <trd>souple<klr tip="ind"> (caract&egrave;re)</klr></trd>
    </trdgrp>
    <trd lng="hu">k&ouml;p&ouml;nyegforgat&oacute;</trd>
    <trd lng="nl">kameleon-</trd>
    <trdgrp lng="pl">
      <trd>kameleonowy</trd>,
      <trd>zmienny jak  <ind>kameleon</ind></trd>
    </trdgrp>
    <trd lng="pt">camale&ocirc;nico</trd>
    <trd lng="ro">cameleon&abreve;</trd>
    <trd lng="ru">&c_h;&c_a;&c_m;&c_je;&c_l;&c_je;&c_o;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
    <trd lng="it">camaleontico</trd>
  </snc>
</drv>
</art>
