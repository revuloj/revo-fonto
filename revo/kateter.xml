<kap>
  <rad>kateter</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kateter.0o">
  <kap><tld/>o</kap>
  <snc mrk="kateter.0o.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Maldika <ref tip="super" cel="tub.0o.TEK">tubeto</ref>
      uzata por enfluigo en organon a&ubreve; defluigo de &gcirc;i:
      <ekz>
        la injekto de kontrastiga substanco per la <tld/>o, permesas
        vidi la urinvezikon
        <fnt>
          <aut>M. Casartelli</aut>,
          <vrk><url ref="&UEA;vikio/Marcelo_Casartelli">
          Simptomoj de la Urogenitala Sistemo</url></vrk>,
          <lok>2006-07-16</lok>
        </fnt>;
      </ekz>
      <ekz>
        41-jarulo <klr>[&#8230;]</klr> tenis dum 24 jaroj en sia korpo <tld/>on,
        etan plastan tubon postlasitan post operacio
        <fnt>
          <bib>Monato</bib>
          <aut>last/pg</aut>
          <vrk><url ref="&Monato;/2006/008852.php"
              >Fremdaj korpoj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_k;&c_a;&c_t;&c_je;&c_t;&c_malmol;&c_r;</trd>
  <trd lng="ca">cat&egrave;ter</trd>
  <trdgrp lng="cs">
    <trd>kat&eacute;tr</trd>,
    <trd>sonda</trd>
  </trdgrp>
  <trd lng="da">kateter</trd>
  <trd lng="de">Katheter</trd>
  <trd lng="en">catheter</trd>
  <trdgrp lng="fr">
    <trd>cath&eacute;ter</trd>,
    <trd>sonde m&eacute;dicale</trd>
  </trdgrp>
  <trdgrp lng="hr">
    <trd>kateter</trd>,
    <trd>sonda</trd>
  </trdgrp>
  <trd lng="hu">kat&eacute;ter</trd>
  <trd lng="it">catetere</trd>
  <trd lng="ja">&#12459;&#12486;&#12540;&#12486;&#12523;</trd>
  <trdgrp lng="nl">
    <trd>katheter</trd>,
    <trd>sonde<klr tip="ind"> (med.)</klr></trd>
  </trdgrp>
  <trd lng="pl">cewnik</trd>
  <trdgrp lng="sk">
    <trd>kat&eacute;ter</trd>,
    <trd>sonda</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_a;&c_t;&c_je;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_z;&c_o;&c_n;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#23548;&#31649; <pr>d&#462;ogu&#462;n</pr></trd>,
    <trd>&#23548;&#28082;&#31649; <pr>d&#462;oy&egrave;gu&#462;n</pr></trd>
  </trdgrp>
</drv>
</art>

