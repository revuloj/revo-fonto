<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: davit.xml,v 1.4 2025/06/20 08:39:53 afido Exp $">
<kap>
    <rad>davit</rad>/o <fnt><aut>P. Clissold</aut> <vrk>Marista Terminaro</vrk> <lok>en: Jarlibro,
    1949</lok></fnt>
</kap>
<drv mrk="davit.0o">
  <kap><tld/>o</kap>
  <snc mrk="davit.0o.MAS">
    <uzo tip="fak">MAS</uzo>
    <uzo tip="fak">SHI</uzo>
    <dif>
      <ref tip="super" cel="masxin.lev0o.MAS">Levma&scirc;ino</ref>,
      per kiu oni transigas peza&jcirc;ojn (savboaton) super la rando de &scirc;ipo:
      <ekz>
        trans la triborda <tld/>o
        <fnt>
          <aut>[J. Verne], trad. Vikifontara komunumo</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/La_Lernejo_por_Robinsonoj/7">
          La Lernejo por Robinsonoj</url></vrk>,
          <lok>[vidita en 2024]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_l;&c_ju;&c_p;&c_b;&c_e;&c_l;&c_mol;&c_k;&c_a;</trd>
  <trd lng="en">davit</trd>
  <trd lng="de">Davit</trd>
  <trd lng="fr">bossoir</trd>
  <trdgrp lng="pl">
    <trd>szlupbelka</trd>,
    <trd>&zdot;urawik</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_l;&c_ju;&c_p;&c_b;&c_a;&c_l;&c_k;&c_a;</trd>
</drv>
</art>

