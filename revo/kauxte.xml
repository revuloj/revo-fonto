<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ka&ubreve;teriz</rad>/i
</kap>
<drv mrk="kauxte.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Vundi per fajro a&ubreve; ka&ubreve;stika&jcirc;oj por kuracaj celoj. 
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>cauteriseren</trd>,
    <trd>uitbranden</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="kauxte.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Me&hcirc;anika rimedo por brulkuraci karnon. 
    </dif>
  </snc>
  <trd lng="nl">brandijzer</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_u;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="kauxte.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      &Hcirc;emia rimedo por sama celo.  
    </dif>
  </snc>
  <trd lng="nl">cauteriserende stof</trd>
</drv>
</art>
