</drv>
<drv mrk="erozi.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Fari <tld/>on al.  
</dif>
</snc>
  <trdgrp lng="de">
     <trd>erodieren</trd>,
     <trd>abtragen</trd>,
     <trd>auswaschen</trd>,
     <trd>abschwemmen</trd></trdgrp>
  <trd lng="es">erocionar</trd>
  <trd lng="fr">&eacute;roder</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<adm>
Cxu ne ekzistas sencoj MED aux FIG? <aut>MB</aut>
</adm>
