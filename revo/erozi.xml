<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: erozi.xml,v 1.22 2021/03/09 18:26:22 revo Exp $">
<kap>
  <rad>erozi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="erozi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="erozi.0i.GEOL">
    <dif>
      Nature iom post iom foruzi, skrapi, supra&jcirc;e
      difekti:
      <ekz>
        mi rigardis vulkanajn insulojn, kiuj estis
        <tld/>itaj de ondoj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 9a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="erod.0i">erodi</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_r;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_r;&c_o;&c_d;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="ca">erosionar</trd>
  <trdgrp lng="cs">
    <trd>erodovat</trd>,
    <trd>odn&#xE1;&#x161;et p&#x16F;du</trd>,
    <trd>vyml&#xED;t</trd>,
    <trd>vym&#xED;lat</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>erodieren</trd>,
     <trd>abtragen</trd>,
     <trd>auswaschen</trd>,
     <trd>abschwemmen</trd>
  </trdgrp>
  <trd lng="en">erode</trd>
  <trd lng="es">erosionar</trd>
  <trd lng="fa">&a_f;&a_r;&a_s;&a_w;&a_d;&a_n;</trd>
  <trd lng="fr">&eacute;roder</trd>
  <trdgrp lng="hu">
     <trd>erod&aacute;l</trd>,
     <trd>lehord</trd>,
     <trd>lepuszt&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">eroderen</trd>
  <trd lng="pl">erodowa&cacute;</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>erodova&#x165;</trd>,
    <trd>odn&#xE1;&#x161;a&#x165; p&#xF4;du</trd>
  </trdgrp>
  <trd lng="sv">erodera<klr> (n&aring;got)</klr></trd>
</drv>

<drv mrk="erozi.0o">
  <kap><tld/>o</kap>
  <snc mrk="erozi.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Geologia difekti&gcirc;o de la rokoj, ka&ubreve;zata de akvo
      a&ubreve; vento.
    </dif>
    <ref tip="sin" cel="erod.0o">erodo</ref> 
  </snc>
  <trd lng="be">&c_e;&c_r;&c_o;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_r;&c_o;&c_z;&c_i;&c_ja;</trd>
  <trd lng="ca">erosi&oacute;</trd>
  <trdgrp lng="cs">
    <trd>eroze</trd>,
    <trd>vym&#xED;l&#xE1;n&#xED;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Erosion</trd>,
     <trd>Abtragung</trd>,
     <trd>Abschwemmung</trd></trdgrp>
  <trd lng="en">erosion</trd>
  <trd lng="es">eroci&oacute;n</trd>
  <trd lng="fa">&a_f;&a_r;&a_s;&a_A;&f_y;&a_sh;</trd>
  <trd lng="fr">&eacute;rosion</trd>
  <trd lng="he">&#x5E9;&#x5C1;&#x5B0;&#x5D7;&#x5B4;&#x5D9;&#x5E7;&#x5B8;&#x5D4;</trd>
  <trdgrp lng="hu">
     <trd>er&oacute;zi&oacute;</trd>,
     <trd>lehord&aacute;s</trd>,
     <trd>lepusztul&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">erosie</trd>
  <trd lng="pl">erozja</trd>
  <trd lng="pt">eros&atilde;o</trd>
  <trd lng="ru">&c_e;&c_r;&c_o;&c_z;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>er&#xF3;zia</trd>,
    <trd>vymie&#x13E;anie</trd>
  </trdgrp>
  <trd lng="sv">erosion</trd>
</drv>

<drv mrk="erozi.0a">
  <kap><tld/>a</kap>
  <snc mrk="erozi.0a.GEOL">
    <dif>
      Rilatanta al <tld/>o a&ubreve; produktata de &gcirc;i.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_r;&c_a;&c_z;&c_ib;&c_j;&c_n;&c_y;</trd>
  <trd lng="bg">&c_je;&c_r;&c_o;&c_z;&c_i;&c_v;&c_je;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>d'erosi&oacute;</trd>,
    <trd>erosionat</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>erodierend</trd>,
     <trd>abtragend</trd>,
     <trd>abschwemmend</trd></trdgrp>
  <trd lng="en">erosional</trd>
  <trd lng="fa">&a_f;&a_r;&a_s;&a_A;&f_y;&a_sh;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;rosif</trd>,
    <trd>d'<ind>&eacute;rosion</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5E1;&#x5B8;&#x5D7;&#x5D5;&#x5BC;&#x5E3;</trd>
  <trdgrp lng="hu">
     <trd>er&oacute;zi&oacute;s</trd>,
     <trd>er&oacute;z&iacute;v</trd>,
     <trd>lepuszt&iacute;t&oacute;</trd>,
     <trd>erod&aacute;l&oacute;</trd>
  </trdgrp>
  <trd lng="nl">erosief</trd>
  <trd lng="pl">erozyjny</trd>
  <trd lng="ru">&c_e;&c_r;&c_o;&c_z;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>

<adm>
  &Ccirc;u ne ekzistas sencoj MED aux FIG?
  <aut>MB</aut>
</adm>
</art>
