<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>erozi</rad>/o
</kap>
<drv mrk="erozi.0o">
<kap><tld/>o</kap>
<uzo tip="fak">MIN</uzo>
<snc>
<dif>
Geologia difekti&gcirc;o de la rokoj, ka&ubreve;zata de la akvo. 
</dif>
</snc>
<trdgrp lng="de">
   <trd>Erosion</trd>,
   <trd>Abtragung</trd>,
   <trd>Abschwemmung</trd></trdgrp>
<trd lng="ru">&c_e;&c_r;&c_o;&c_z;&c_i;&c_ja;</trd>
</drv>
<drv mrk="erozi.0a">
<kap><tld/>a</kap>
<snc>
<dif>
Rilatanta al <tld/>o a&ubreve; produktita de &gcirc;i. 
</dif>
</snc>
<trdgrp lng="de">
   <trd>erodierend</trd>,
   <trd>abtragend</trd>,
   <trd>abschwemmend</trd></trdgrp>
<trd lng="ru">&c_e;&c_r;&c_o;&c_z;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="erozi.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Fari <tld/>on al.  
</dif>
</snc>
<trdgrp lng="de">
   <trd>erodieren</trd>,
   <trd>abtragen</trd>,
   <trd>auswaschen</trd>,
   <trd>abschwemmen</trd></trdgrp>
<trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
