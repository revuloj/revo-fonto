<vortaro>
<art mrk="$Id: erozi.xml,v 1.34 2023/11/07 20:34:11 revo Exp $">
<kap>
  <rad>erozi</rad>/i <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="d">erod</rad>/i <fnt><bib>SPV</bib></fnt></kap></var>
</kap>
<drv mrk="erozi.0i">
  <kap><tld/>i, <var><kap><tld var="d"/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="erozi.0i.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Nature iom post iom foruzi, skrapi, supra&jcirc;e
      <ref tip="super" cel="difekt.0i">difekti</ref>:
      <ekz>
        mi rigardis vulkanajn insulojn, kiuj estis
        <tld/>itaj de ondoj
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 9a</lok></fnt>;
      </ekz>
      <ekz>
        kelkaj vulkanoj, malaktivaj kaj ege <tld/>itaj, superregas la pejza&gcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2014/011328.php">Iamaj vulkanoj, iama maro, &ccirc;e nuna
          montaro</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej bonaj el <klr>[la domoj]</klr> aspektis malnovaj kaj iomete
        <tld var="d"/>itaj de la vetero
        <fnt><bib>DOz</bib> <lok>Mirinda eskapo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="abrazi.0i">abrazii</ref>,
      <ref cel="korod.0i">korodi</ref>
    </refgrp>
  </snc>
  <snc mrk="erozi.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iom post iom <ref tip="super" cel="degrad.0i.FIG">degradi</ref>, 
      disfaligi:
      <ekz>
        <klr>[troa montri&gcirc;emo]</klr> balda&ubreve; <tld var="d"/>is lian a&ubreve;toritaton kaj transformis lin
        en lian propran karikaturon
        <fnt>
          <bib>MD11</bib>
          <vrk>Elektado en Francio</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la merkataj fortoj <klr>[&#x2026;]</klr> komencas <tld/>i la potencon de la &scirc;tato
        <fnt>
          <bib>MD02</bib>
          <vrk>Logikoj de milito &dash; Partio, klanoj kaj triboj, la fragila ekvilibro de la iraka
          re&gcirc;imo</vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="degene.0igi">degenerigi</ref>,
      <ref cel="dekade.0igi">dekadencigi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>zersetzen</trd>,
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_r;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_y;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_je;&c_r;&c_o;&c_d;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="ca">erosionar</trd>
  <trdgrp lng="cs">
    <trd>erodovat</trd>,
    <trd>odn&aacute;&scaron;et p&uring;du</trd>,
    <trd>rozm&ecaron;l&ncaron;ovat</trd>,
    <trd>rozt&iacute;rat</trd>,
    <trd>vyml&iacute;t</trd>,
    <trd>vym&iacute;lat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd><klr>(etwas)</klr> erodieren</trd>,
    <trd>abtragen</trd>,
    <trd>auswaschen</trd>,
    <trd>abschwemmen</trd>,
  </trdgrp>
  <trd lng="en">erode</trd>
  <trd lng="es">erosionar</trd>
  <trd lng="fa">&a_f;&a_r;&a_s;&a_w;&a_d;&a_n;</trd>
  <trd lng="fr">&eacute;roder</trd>
  <trdgrp lng="hu">
     <trd>erod&aacute;l</trd>,
     <trd>lehord</trd>,
     <trd>lepuszt&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3059;&#x308A;&#x6E1B;&#x3089;&#x3059;  <pr>&#x3059;&#x308A;&#x3078;&#x3089;&#x3059;</pr></trd>,
    <trd>&#x6469;&#x6EC5;&#x3055;&#x305B;&#x308B;  <pr>&#x307E;&#x3081;&#x3064;&#x3055;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x7814;&#x78E8;&#x3059;&#x308B;  <pr>&#x3051;&#x3093;&#x307E;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x3053;&#x3059;&#x308A;&#x5408;&#x308F;&#x305B;&#x308B;  <pr>&#x3053;&#x3059;&#x308A;&#x3042;&#x308F;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">eroderen</trd>
  <trd lng="pl">erodowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>desgastar</trd>,
    <trd>esmerilar</trd>,
    <trd>erodir</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_m;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>erodova&tcaron;</trd>,
    <trd>odn&aacute;&scaron;a&tcaron; p&ocirc;du</trd>,
    <trd>rozr&yacute;va&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">erodera<klr> (n&aring;got)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_j;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_n;&c_o;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_r;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_v;&c_ib;&c_t;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_m;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_g;&c_a;&c_n;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_t;&c_i;&c_r;&c_a;&c_t;&c_i; (&c_o;&c_d;&c_n;&c_u; &c_d;&c_je;&c_t;&c_a;&c_l;&c_mol; &c_d;&c_o; &c_ib;&c_n;&c_sh;&c_o;&c_ji;)</trd>
  </trdgrp>
  <trd lng="zh">&#x4FB5;&#x8680; <pr>q&#x12B;nsh&#xED;</pr></trd>
</drv>

<drv mrk="erozi.0o">
  <kap><tld/>o, <var><kap><tld var="d"/>o</kap></var></kap>
  <snc mrk="erozi.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Geologia difekti&gcirc;o de la grundo, ka&ubreve;zata akvo, glacio, vento ktp:
      <ekz>
        &ccirc;iuformaj roka&jcirc;oj, kreitaj el la <tld/>o de sablo&scirc;tonaj tavoloj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2015/011471.php">Palatinata rokolando</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;tonkolonoj ekestis sekve de <tld/>o sur tereno de malkompakta grundo
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitrije Jani&ccaron;i&cacute;</aut>
          <vrk><url ref="&Monato;/2008/009444.php">Diabla urbeto</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        tia briko enhavas truetojn, povas su&ccirc;i akvon kaj ne estas rezistokapabla kontra&ubreve;
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010935.php">Briko-muzeo</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="aluvi.0o.GEOL">aluvio<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_r;&c_o;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_r;&c_o;&c_z;&c_i;&c_ja;</trd>
  <trd lng="ca">erosi&oacute;</trd>
  <trdgrp lng="cs">
    <trd>eroze</trd>,
    <trd>vym&iacute;l&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erosion</trd>,
    <trd>Verwitterung</trd>,
    <trd>Abschwemmung</trd>,
    <trd>Abtragung</trd>
  </trdgrp>
  <trd lng="en">erosion</trd>
  <trd lng="es">eroci&oacute;n</trd>
  <trd lng="fa">&a_f;&a_r;&a_s;&a_A;&f_y;&a_sh;</trd>
  <trd lng="fr">&eacute;rosion</trd>
  <trdgrp lng="he">
    <trd>&shin;&het;&yod;&qof;&he;</trd>,
    <trd>&bet;&lamed;&yod;&he;</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>er&oacute;zi&oacute;</trd>,
     <trd>lehord&aacute;s</trd>,
     <trd>lepusztul&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6469;&#x6EC5;  <pr>&#x307E;&#x3081;&#x3064;</pr></trd>,
    <trd>&#x6469;&#x8017;  <pr>&#x307E;&#x3082;&#x3046;</pr></trd>,
    <trd>&#x7814;&#x78E8;  <pr>&#x3051;&#x3093;&#x307E;</pr></trd>,
    <trd>&#x3053;&#x3059;&#x308A;&#x5408;&#x308F;&#x305B;  <pr>&#x3053;&#x3059;&#x308A;&#x3042;&#x308F;&#x305B;</pr></trd>
  </trdgrp>
  <trd lng="nl">erosie</trd>
  <trd lng="pl">erozja</trd>
  <trd lng="pt">eros&atilde;o</trd>
  <trd lng="ru">&c_e;&c_r;&c_o;&c_z;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>er&oacute;zia</trd>,
    <trd>vymie&lcaron;anie</trd>
  </trdgrp>
  <trd lng="sv">erosion</trd>
  <trd lng="uk">&c_je;&c_r;&c_o;&c_z;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x51B2;&#x5237; <pr>ch&#x14D;ngshu&#xE0;</pr></trd>,
    <trd>&#x51B2;&#x8680; <pr>ch&#x14D;ngsh&#xED;</pr></trd>
  </trdgrp>
</drv>

