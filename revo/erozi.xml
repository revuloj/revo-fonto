<drv mrk="erozi.0igxi">
  <kap>
    <tld/>i&gcirc;i, 
    <var><kap><tld var="d"/>i&gcirc;i</kap></var>
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="erozi.0igxi.KOMUNE">
    <dif>
      Foruzi&gcirc;i, trivi&gcirc;i:
      <ekz>
        la milit&scirc;ipoj da&ubreve;re ekzercis sin per pafado
        al imagataj malamikoj, kaj &ccirc;iufoje la paftuboj
        de la kanonoj iom <tld var="d"/>i&gcirc;is
        <fnt>
          <aut>T. Steele</aut>
          <vrk>Konvinka Kamufla&jcirc;o</vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en Britio <klr>[&#x2026;]</klr> <tld var="d"/>i&gcirc;as jaroj da pena laboro por starigi harmoniajn
        rilatojn inter etnaj kaj religiaj malplimultoj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2006/009061.php">Faktoj kaj famoj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>    
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_m;&c_y;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_v;&c_je;&c_t;&c_r;&c_y;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_a;&c_h;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verwittern</trd>,
    <trd>erodieren</trd>,
    <trd>sich <ind>zersetzen</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>erode</trd>,
    <trd>wear away</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>&eacute;roder</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x3059;&#x308A;&#x6E1B;&#x308B;  <pr>&#x3059;&#x308A;&#x3078;&#x308B;</pr></trd>,
    <trd>&#x6469;&#x6EC5;&#x3059;&#x308B;  <pr>&#x307E;&#x3081;&#x3064;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_v;&c_i;&c_n;&c_je;&c_s;&c_je;&c_n;&c_i;&c_m; &c_z; &c_g;&c_a;&c_v;&c_a;&c_n;&c_ib; &c_u; &c_v;&c_ib;&c_d;&c_k;&c_r;&c_i;&c_t;&c_je; &c_m;&c_o;&c_r;&c_je;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_z;&c_ib;&c_r;&c_v;&c_a;&c_n;&c_i;&c_m; &c_z; &c_ja;&c_k;&c_o;&c_r;&c_ja;</trd>
  </trdgrp>
</drv>

