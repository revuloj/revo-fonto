<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: virt.xml,v 1.21 2021/05/29 14:33:27 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>virt</rad>/o
</kap>
<drv mrk="virt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Konstanta emo fari bonon:
      <ekz>
        pli bona <tld/>o sen oro, ol oro sen honoro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la <tld/>o sin mem rekompencas<fnt>Z</fnt>;
      </ekz>
      <ekz>
        el la neceseco oni devas fari <tld/>on
        <klr>(oni devas akiri meriton farante propramove tion, kion oni
        ne
        povas eviti)</klr>
        <fnt><bib>PrV</bib></fnt>..
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      &Ccirc;iu el la apartaj bonaj inklinoj de la animo:
      <ekz>
        li havas &ccirc;iujn <tld/>ojn, krom la malavareco;
      </ekz>
      <ekz>
        la civitanaj, teologiaj <tld/>oj;
      </ekz>
      <ekz>
        niaj <tld/>oj estas nur plej ofte maskitaj mal<tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kvalit.0o">kvalito 4</ref>.
    </refgrp>
  </snc>
  <trd lng="bg">&c_d;&c_o;&c_b;&c_r;&c_o;&c_d;&c_je;&c_t;&c_je;&c_l;</trd>
  <trd lng="bo">&#xF51;&#xF42;&#xF7A;&#xF0B;&#xF56;&#xF0B;</trd>
  <trd lng="ca">virtut</trd>
  <trd lng="cs">ctnost</trd>
  <trd lng="de">Tugend</trd>
  <trd lng="en">virtue</trd>
  <trd lng="es">virtud</trd>
  <trd lng="fr">vertu</trd>
  <trd lng="hu">er&eacute;ny</trd>
  <trd lng="nl">deugd</trd>
  <trd lng="pl">cnota</trd>
  <trd lng="pt">virtude</trd>
  <trd lng="ru">&c_d;&c_o;&c_b;&c_r;&c_o;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>cnos&tcaron;</trd>,
    <trd>&#x10D;nos&#x165;</trd>
  </trdgrp>
</drv>

<drv mrk="virt.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Posedanta <tld/>o(j)n:
      <ekz>
        <tld/>a edzino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>ulo kompatos la vivon de sia bruto<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Konforma al <tld/>o:
      <ekz>
        <tld/>a ago.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_d;&c_o;&c_b;&c_r;&c_o;&c_d;&c_je;&c_t;&c_je;&c_l;&c_je;&c_n;</trd>
  <trd lng="ca">virtu&oacute;s</trd>
  <trd lng="cs">ctnostn&#xFD;</trd>
  <trd lng="de">tugendhaft</trd>
  <trd lng="en">virtuous</trd>
  <trd lng="es">virtuoso</trd>
  <trd lng="fr">vertueux</trd>
  <trd lng="hu">er&eacute;nyes</trd>
  <trd lng="nl">deugdzaam</trd>
  <trdgrp lng="pl">
    <trd>cnotliwy</trd>,
    <trd>zacny</trd>
  </trdgrp>
  <trd lng="pt">virtuoso</trd>
  <trd lng="ru">&c_d;&c_o;&c_b;&c_r;&c_o;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>cnostn&yacute;</trd>,
    <trd>&#x10D;nostn&#xFD;</trd>
  </trdgrp>
</drv>

<drv mrk="virt.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      Konstanta emo fari malbonon:
      <ekz>
        ne fortiki&gcirc;as homo per mal<tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      &Ccirc;iu el malbonaj inklinoj de la animo:
      <ekz>
        malri&ccirc;eco ne estas mal<tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la ka&ubreve;zo, pro kiu ni ne fordonas nin al unu sola mal<tld/>o
        estas
        ofte, ke ni havas plurajn aliajn.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_z;&c_a;&c_m;&c_je;&c_s;&c_t;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="ca">
    <trd>vilesa</trd>,
    <trd>vici</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nepravost</trd>,
    <trd>ne&#x159;est</trd>,
    <trd>zh&#xFD;ralost</trd>
  </trdgrp>
  <trd lng="de">Laster</trd>
  <trd lng="en">vice</trd>
  <trd lng="fr">vice</trd>
  <trdgrp lng="hu">
    <trd>b&udblac;n</trd>,
    <trd>v&eacute;tek</trd>
  </trdgrp>
  <trd lng="nl">ondeugd</trd>
  <trdgrp lng="pl">
    <trd>nieprawo&sacute;&cacute;</trd>,
    <trd>wyst&eogonek;pek</trd>
  </trdgrp>
  <trd lng="pt">pervers&atilde;o</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>necnos&#x165;</trd>,
    <trd>neres&#x165;</trd>,
    <trd>zlozvyk</trd>,
    <trd>zl&#xE1; vlastnos&#x165;</trd>
  </trdgrp>
</drv>

<drv mrk="virt.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Posedanta mal<tld/>o(j)n
    </dif>
    <refgrp tip="sin">
      <ref cel="perver.0a">perversa</ref>.
    </refgrp>
  </snc>
  <trd lng="bg">&c_p;&c_o;&c_r;&c_o;&c_ch;&c_je;&c_n;</trd>
  <trd lng="bo">&#xF66;&#xF90;&#xFB1;&#xF42;&#xF0B;&#xF63;&#xF44;&#xF0B;&#xF64;&#xF7C;&#xF62;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>vil</trd>,
    <trd>pervers</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ne&#x159;estn&#xFD;</trd>,
    <trd>zh&#xFD;ral&#xFD;</trd>
  </trdgrp>
  <trd lng="de">lasterhaft</trd>
  <trd lng="en">vicious</trd>
  <trd lng="fr">pervers</trd>
  <trdgrp lng="hu">
    <trd>b&udblac;n&ouml;s</trd>,
    <trd>erk&ouml;lcstelen</trd>
  </trdgrp>
  <trd lng="nl">ondeugdzaam</trd>
  <trdgrp lng="pl">
    <trd>nieprawy</trd>,
    <trd>wyst&eogonek;pny</trd>
  </trdgrp>
  <trd lng="pt">perverso</trd>
  <trd lng="ru">&c_p;&c_o;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">nerestn&#xFD;</trd>
</drv>
</art>
