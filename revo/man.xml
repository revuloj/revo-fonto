<drv mrk="man.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="man.en0igi.lauxvorta">
    <dif>
      Meti en la <tld/>on:
      <ekz>
        en<tld/>igi leteron, skatolon
        <fnt>
          <bib>Marta</bib>
        </fnt> al iu;
      </ekz>
      <ekz>
        li en<tld/>igis al mi paka&jcirc;eton da buterpanoj
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
        8a</lok></fnt>;
      </ekz>
    </dif>
    <trd lng="pl">wr&eogonek;cza&cacute;</trd>   
  </snc>
  <snc mrk="man.en0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Meti sub la povon de iu:
      <ekz>
        al mi vin la diino en<tld/>igis
        <klr>(vian sorton komisiis)</klr>
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konfid.0i.pro_honesteco">konfidi</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_u;&c_ch;&c_y;&c_c;&c_mol;</trd>
  <trd lng="br">lakaat udb. etre daouarn ub.</trd>
  <trdgrp lng="cs">
    <trd>dodat</trd>,
    <trd>doru&ccaron;it</trd>
  </trdgrp>
  <trd lng="es">entregar</trd>
  <trdgrp lng="fr">
    <trd>remettre<klr> (confier)</klr></trd>,
    <trd>prendre en main</trd>
  </trdgrp>
  <trd lng="it">mettere in <ind>mano</ind></trd>
  <trd lng="nl">overhandigen</trd>
  <trd lng="pl">powierza&cacute;</trd>
  <trd lng="ru">&c_v;&c_r;&c_u;&c_ch;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">doru&ccaron;i&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_r;&c_u;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_v; &c_r;&c_u;&#x43A;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="man.manen0e_kun">
