<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>man</rad>/o
</kap>
<drv mrk="man.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="man.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Ekstrema parto de amba&ubreve; brakoj de la homo, provizita per kvin
      fingroj kaj servanta por kapti kaj palpi:
      <ekz>
        etendi la <tld/>on al iu;
      </ekz>
      <ekz>
        teni ion per la <tld/>o;
      </ekz>
      <ekz>
        konduki infanon per la <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sup.0o">supo</ref>.
    </refgrp>
  </snc>
  <snc mrk="man.0o.agado">
    <dif>
      Tiu sama organo rigardata kiel ilo de agado:
      <ekz>
        ankora&ubreve; la gajno ne estas en la <tld/>o
        <klr>(= certa)</klr>
        (<ref tip="vid" cel="fru">tro frue</ref>)
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        havi ion sub la <tld/>o <klr>(tute disponeblan kaj pretan)</klr>;
      </ekz>
      <ekz>
        havi pruvon en la <tld/>o<fnt>B</fnt>;
      </ekz>
      <ekz>
        ekkapti iun kun la peko en la <tld/>o<fnt>Z</fnt>
        (<ref tip="vid" cel="fresx.0a">fre&scirc;a</ref>);
      </ekz>
      <ekz>
        kiel per turno de la <tld/>o <klr>(rapidege, subite)</klr>
        tio forpasis<fnt>Z</fnt>;
      </ekz>
      <ekz>
        lasi fali la <tld/>ojn
        <klr>(senkura&gcirc;i&gcirc;i;
        <ref tip="vid" cel="parali.0i">paralizi</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        energie meti la <tld/>on al <klr>(ekpartopreni en)</klr>
        la laboro;
      </ekz>
      <ekz>
        peti la <tld/>on de junulino <klr>(svati)</klr>;
      </ekz>
      <ekz>
        ligi la <tld/>ojn al iu <klr>(senigi iun je la libera
        agado)</klr>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="man.0o.povo">
    <dif>
      Tiu sama organo rigardata kiel ilo de povo, potenco:
      <ekz>
        preni la regadon en la <tld/>ojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        la afero sidas en bonaj <tld/>oj
        <klr>(sub bona gvidado)</klr>;
      </ekz>
      <ekz>
        teni en siaj <tld/>oj la sorton de<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la sorto de la afero ku&scirc;as en la <tld/>oj (dependas) de tiu
        ligo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fali en la <tld/>ojn (sub la povo) de iu<fnt>B</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="posed.0i">posedi</ref>,
      <ref cel="reg.0i">regi</ref>,
      <ref cel="dispon.0i">disponi</ref>,
      <ref cel="mastr.0i">mastri</ref>,
      <ref cel="estr.0i">estri</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">ruka</trd>
  <trd lng="de">Hand</trd>
  <trd lng="en">hand</trd>
  <trd lng="es">mano</trd>
  <trd lng="fr">main</trd>
  <trd lng="nl">hand</trd>
  <trd lng="pl">r&ecaron;ka</trd>
  <trd lng="ru">&c_r;&c_u;&c_k;&c_a; <klr tip="amb">(&c_k;&c_i;&c_s;&c_t;&c_mol;)</klr></trd>
  <trd lng="sv">hand</trd>
</drv>

<drv mrk="man.0kavo0plato">
  <kap><tld/>kavo, <tld/>plato</kap>
  <snc>
    <dif>
      Interna kava flanko de <tld/>o:
      <ekz>
        la me&hcirc;aniko de la lingvo estis anta&ubreve; mi kvaza&ubreve; sur
        <tld/>plato<fnt>Z</fnt>
        <klr>(tute je mia dispono)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Handfl&auml;che</trd>
  <trd lng="es">palma <klr>(de la mano)</klr></trd>
  <trd lng="fr">paume</trd>
  <trd lng="nl">handpalm</trd>
  <trd lng="ru">&c_l;&c_a;&c_d;&c_o;&c_n;&c_mol;</trd>
  <trd lng="sv">handflata</trd>
</drv>

<drv mrk="man.0supro0dorso">
  <kap><tld/>supro, <tld/>dorso</kap>
  <snc>
    <dif>
      Ekstera flanko de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Handr&uuml;cken</trd>
  <trd lng="es">dorso <klr>(de la mano)</klr></trd>
  <trd lng="fr">dos de la main</trd>
</drv>

<drv mrk="man.0radiko0artiko">
  <kap><tld/>radiko, <tld/>artiko</kap>
  <snc>
    <dif>
      Artiko de <tld/>o kun brako.
    </dif>
  </snc>
  <trd lng="de">Handgelenk</trd>
  <trd lng="fr">poignet</trd>
  <trd lng="nl">pols</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_ja;&c_s;&c_t;&c_mol;&c_je;</trd>
  <trd lng="sv">handled</trd>
</drv>

<drv mrk="man.0premo">
  <kap><tld/>premo</kap>
  <snc>
    <dif>
      Gesto per kiu du personoj interpremas al si la <tld/>on kiel signo
      de amika saluto.
    </dif>
  </snc>
  <trd lng="de">H&auml;ndedruck</trd>
  <trd lng="fr">poign&eacute;e de main</trd>
  <trd lng="nl">handdruk</trd>
  <trd lng="ru">&c_r;&c_u;&c_k;&c_o;&c_p;&c_o;&c_zh;&c_a;&c_t;&c_i;&c_je;</trd>
</drv>

<drv mrk="man.0laboro">
  <kap><tld/>laboro</kap>
  <snc>
    <dif>
      Laboro postulanta precipe muskolan penadon kontraste kun cerba
      laboro.
    </dif>
  </snc>
  <trd lng="de">Handarbeit</trd>
  <trd lng="fr">travail manuel</trd>
  <trd lng="nl">handenarbeid</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_ch;&c_n;&c_o;&c_j; &c_t;&c_r;&c_u;&c_d;</trd>,
    <trd>&c_r;&c_u;&c_ch;&c_n;&c_a;&c_ja; &c_r;&c_a;&c_b;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="man.0uzi">
  <kap><tld/>uzi</kap>
  <snc>
    <dif>
      Manipuli ilon per <tld/>o por elfari ion.
    </dif>
  </snc>
  <trd lng="fr">manipuler</trd>
  <trd lng="nl">behandelen</trd>
</drv>

<drv mrk="man.0umo">
  <kap><tld/>umo</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Ekstrema parto de la maniko, kiu &ccirc;irka&ubreve;as la <tld/>radikon, a&ubreve;
      aparta peco da tolo, punto ks, almetebla al la maniko kaj &ccirc;irka&ubreve;anta
      la radikon.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>manchette</trd>,
    <trd>poignet</trd>
  </trdgrp>
  <trd lng="nl">manchet</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_zh;&c_je;&c_t;&c_a;</trd>
</drv>

<drv mrk="man.cxirkaux0o">
  <kap>&ccirc;irka&ubreve;<tld/>o</kap>
  <snc>
    <dif>
      Braceleto.
    </dif>
  </snc>
  <trd lng="fr">bracelet</trd>
  <trd lng="nl">armband</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_s;&c_l;&c_je;&c_t;</trd>
</drv>

<drv mrk="man.en0igi">
  <kap>en<tld/>igi</kap>
  <snc mrk="man.en0igi.lauxvorta">
    <dif>
      Meti en la <tld/>on:
      <ekz>
        en<tld/>igi leteron, skatolon al iu<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="man.en0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Meti sur la povon de iu:
      <ekz>
        al mi vin en<tld/>igis (vian sorton komisiis) la diino<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">remettre</trd>
  <trd lng="nl">overhandigen</trd>
  <trd lng="ru">&c_v;&c_r;&c_u;&c_ch;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="man.plen0o">
  <kap>plen<tld/>o</kap>
  <snc>
    <dif>
      Kvanto, kiun povas enteni la fermita <tld/>o:
      <ekz>
        plen<tld/>o da greno, da &ccirc;erizoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        plen<tld/>o da <klr>(kelkaj)</klr> soldatoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">pu&ntilde;o <klr>(cantidad)</klr></trd>
  <trd lng="fr">poign&eacute; <klr>(quantit&eacute;)</klr></trd>
  <trd lng="nl">handvol</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sv">handfull</trd>
</drv>
</art>
