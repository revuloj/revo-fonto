  </trdgrp>
  <trd lng="en">cuff</trd>
  <trd lng="es">pu&ntilde;o<klr> (de camisa)</klr></trd>
  <trdgrp lng="fr">
    <trd>manchette</trd>,
    <trd>poignet</trd>
  </trdgrp>
  <trd lng="id">manset</trd>
  <trd lng="it">polsino</trd>
  <trdgrp lng="ja">
    <trd>&#12381;&#12391;&#21475;  <pr>&#12381;&#12391;&#12368;&#12385;</pr></trd>,
    <trd>&#12459;&#12501;&#12473;</trd>
  </trdgrp>
  <trd lng="nl">manchet</trd>
  <trd lng="pl">mankiet</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_zh;&c_je;&c_t;&c_a;</trd>
  <trd lng="sk">man&zcaron;eta</trd>
  <trd lng="uk">&c_m;&c_a;&c_n;&c_zh;&c_je;&c_t;&c_a;</trd>
