<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>rat</rad>/o
</kap>
<drv mrk="rat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="mus.0edoj">musedoj</ref>
      (<trd lng="la">Rattus</trd>), rodulo, pli granda ol muso,
      konta&gcirc;anto de pluraj malsanoj kaj ofte uzata por
      bestaj eksperimentoj.
    </dif>
  </snc>
  <trd lng="de">Ratte</trd>
  <trd lng="nl">rat</trd>
</drv>
</art>
