<drv mrk="met.sur0i">
  <kap>sur<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Sin vesti per:
      <ekz>
        Sur<tld/>u mantelon!
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 73a, p. 534a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_z;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_r;&c_a;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enfiler<klr> (un v&ecirc;tement)</klr></trd>,
    <trd>mettre<klr> (un v&ecirc;tement)</klr></trd>,
    <trd>passer<klr> (un v&ecirc;tement)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
