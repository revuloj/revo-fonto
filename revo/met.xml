    <trd>componer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>assembler</trd>,
    <trd>composer</trd>,
    <trd>unir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;sszetesz</trd>,
    <trd>&ouml;sszerak</trd>,
    <trd>egybetesz</trd>,
    <trd>&ouml;ssze&aacute;ll&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">samenstellen</trd>
  <trd lng="pl">sk&lstroke;ada&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pripoji&tcaron;</trd>,
    <trd>spoji&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>sammanst&auml;lla</trd>,
    <trd>s&auml;tta samman</trd>,
    <trd>l&auml;gga ihop</trd>
  </trdgrp>
</drv>

<drv mrk="met.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Pretigi (lokon) por iu celo, <tld/>ante sur &gcirc;in la
      bezonatajn
      objektojn kaj la&ubreve;cele aran&gcirc;ante ilin:
      <ekz>
        <ind>pri<tld/>i la tablon</ind>
