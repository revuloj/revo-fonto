<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hot.xml,v 1.6 2025/10/30 16:40:49 revo Exp $">
<kap>
  <rad>hot</rad> <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="hot.0">
  <kap><tld/>, <var><kap><tld/>o</kap></var></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="hot.0.KOMUNE">
    <dif>
      Ekkrio por peli tirbeston:
      <ekz>
        la ko&ccirc;ero vipklakis kaj kriis &leftquot;<tld/>&rightquot;
        <fnt>
        <aut>Karel &Ccaron;apek, trad. Josef Vondrou&scaron;ek</aut>,
        <vrk><url ref="http://mujweb.cz/Kultura/malovec/naufabel.htm">Na&ubreve;
        fabeloj</url></vrk>,
        <lok>La granda katfabelo, [2002?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia &ccirc;evalo peze tiras la plugilon, kaj al mi kelkfoje venis lia
        instiga krio: &leftquot;<tld lit="H"/>o! <tld lit="H"/>!&rightquot;
        <fnt>
          <aut>F. Dostojevskij, trad. A. Kor&jcirc;enkov</aut>,
          <vrk>La taglibro de verkisto</vrk>,
          <bib>LOdE</bib>, <lok>2003-04 (102)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">hue</trd>
  <trdgrp lng="de">
    <trd>h&uuml;</trd>,
    <trd>h&uuml;h</trd>,
    <trd>h&uuml;ja</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>hop</trd>,
    <trd>arri</trd>, <trd>al&egrave;</trd>,
    <trd>eia</trd>
  </trdgrp>
  <trd lng="pl">wio!</trd>
  <trd lng="pt">eia</trd>
  <trdgrp lng="zh">
    <trd>&#21649; <pr>d&amacron;</pr></trd>,
    <trd>&#21649; <pr>d&amacron;</pr></trd>
  </trdgrp>
</drv>
</art>

