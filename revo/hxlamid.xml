<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hxlamid.xml,v 1.9 2022/08/10 08:36:52 revo Exp $">
<kap>
  <rad>&hcirc;lamid</rad>/o,
  <var><kap><rad var="k">klamid</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="hxlamid.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <snc mrk="hxlamid.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Antikva vira
      <ref tip="super" cel="vest.super0o.TEKS">supervesto</ref>
      grekdevena, ortangula teksa&jcirc;o kiun oni
      agrafis &gcirc;enerale sur la dekstra &scirc;ultro:
      <ekz>
        li vidis malgrandan figuron en purpura militista <tld/>o
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 10a</lok></fnt>;
      </ekz>
      <ekz>
        militistoj, senseliginte kaj implikinte la &ccirc;evalojn, instalis 
        sin kiel ajn, sterninte siajn <tld/>ojn sur la tero
        <fnt>
          <aut>I. Efremov, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Efremov/tais_10.html">
          Tais el Ateno</url></vrk>,
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        kune kun la <tld var="k"/>o <klr>[petazo]</klr> estis portata de la
        efeboj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Petazo">
          Petazo</url>, 2011-11-15</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d0/Chlamys.PNG/158px-Chlamys.PNG" prm="PD">roma statuo kun <tld/>o
        <fnt>
          <aut>Silenus &ccirc;e germana Vikipedio</aut>
          <lok><url ref="&WCW;/File:Chlamys.PNG">&WCW;/File:Chlamys.PNG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_h;&c_l;&c_a;&c_m;&c_ib;&c_d;&c_a;</trd>
  <trd lng="de">Chlamys</trd>
  <trd lng="en">chlamys</trd>
  <trd lng="fr">chlamyde</trd>
  <trd lng="pl">chlamida</trd>     
  <trd lng="ro">fibul&abreve;</trd>
  <trd lng="ru">&c_h;&c_l;&c_a;&c_m;&c_i;&c_d;&c_a;</trd>
  <trd lng="uk">&c_h;&#x43B;&c_a;&#x43C;&c_i;&c_d;&c_a; (&c_ch;&#x43E;&#x43B;&#x43E;&c_v;&c_ib;&c_ch;&c_i;&c_j; &c_v;&c_je;&c_r;&c_h;&#x43D;&c_ib;&c_j; &#x43E;&c_d;&#x44F;&c_g; &c_u;
  &c_s;&c_t;. &c_g;&c_r;&c_je;&#x43A;&c_ib;&c_v; &c_ib; &c_r;&c_i;&#x43C;&#x43B;&#x44F;&#x43D;)</trd>
</drv>
</art>

