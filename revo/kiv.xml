<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kiv</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="kiv.0o">
  <kap><tld/>o</kap>
  <snc mrk="kiv.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref>,
      plejofte ovo-forma, inter 5 kaj 8 centimetrojn longa,
      kun bruna &scirc;elo, mallongaj haroj kaj
      verda karno punktita de nigraj semetoj.
    </dif>
  </snc>
  <trd lng="de">Kiwi</trd>
  <trd lng="en">kiwi</trd>
  <trd lng="fr">kiwi<klr> (fruit)</klr></trd>
  <trdgrp lng="pt">
    <trd>quiu&iacute;</trd>,
    <trd>kiwi</trd>
  </trdgrp>
</drv>
<!--
  ta&ubreve;gos por la arbo-deriva&jcirc;o:
  (<trd lng="la">Actinidia deliciosa</trd>)
-->
</art>
