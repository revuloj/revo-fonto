<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: drasx.xml,v 1.32 2024/08/12 21:26:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>dra&scirc;</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="drasx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="drasx.0i.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="super" cel="bat.0i">Bati</ref> la <ref cel="gren.0o">greno</ref>n
      por eligi la grajnojn:
      <ekz>
        <tld/>ita tritiko
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 2:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is tion, kion &scirc;i kolektis, kaj tio estis &ccirc;irka&ubreve; efo da hordeo
        <fnt>
          <bib>MT</bib>
          <lok>Rut 2:17</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">battre<klr tip="ind"> (une c&eacute;r&eacute;ale)</klr></trd>
    <trd lng="hu">cs&eacute;pel</trd>
    <trd lng="pl">m&lstroke;&oacute;ci&cacute;</trd>
    <trdgrp lng="pt">
      <trd>bater <klr tip="amb">(cereais)</klr></trd>,
      <trd>debulhar</trd>,
      <trd>trilhar</trd>,
      <trd>malhar</trd>
    </trdgrp>
  </snc>
  <snc mrk="drasx.0i.ion">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <tld lit="D"/>i ion kvaza&ubreve; greno:
      <ekz>
        pro la frapado de la hajlo <tld/>i&gcirc;anta sur la &scirc;tupoj de la balkono, neniu ajn
        povis a&ubreve;di, ke la prokuratoro ion murmuras
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        obstina kaj ne&ccirc;esigebla vorto<tld/>ado
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i fojnon <klr>(vane peni)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="drasx.0i.bategi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forte <ref tip="vid" cel="bat.0i.ektusxegi">bati</ref>, bategi:
      <ekz>
        mi <tld/>os viajn korpojn per dornoj de dezerto
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 8:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> tiel <tld/>is per la ulno la dorson de la knabo, ke la junulo
        forkuris
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili per bastonegoj <tld/>as al si reciproke la kapon
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>pr&uuml;geln</trd>,
      <trd>verdreschen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>frotter<klr tip="ind"> (battre)</klr></trd>,
      <trd>rouer<klr tip="ind"> (de coups)</klr></trd>,
      <trd>taper<klr tip="ind"> (battre)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>p&uuml;f&ouml;l</trd>,
      <trd>p&aacute;hol</trd>,
      <trd>elt&aacute;ng&aacute;l</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>m&lstroke;&oacute;ci&cacute;</trd>,
      <trd>la&cacute;<klr> (bi&cacute;)</klr></trd>,
      <trd>spuszcza&cacute; <ind>manto</ind><klr> (pot.)</klr></trd>,
      <trd>spuszcza&cacute; <ind>&lstroke;omot</ind><klr>
      (pot.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>bater</trd>,
      <trd>espancar</trd>,
      <trd>esbordoar</trd>,
      <trd>sovar</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="de">dreschen</trd>
  <trdgrp lng="en">
    <trd>thresh</trd>,
    <trd>thrash</trd>,
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25171;&#31296;&#12377;&#12427; <pr>&#12384;&#12371;&#12367;&#12377;&#12427;</pr></trd>,
    <trd>&#33073;&#31296;&#12377;&#12427; <pr>&#12384;&#12387;&#12371;&#12367;&#12377;&#12427;</pr></trd>,
    <trd>&#24375;&#12367;&#12383;&#12383;&#12367; <pr>&#12388;&#12424;&#12367;&#12383;&#12383;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trd lng="es">trillar</trd>
  <trd lng="uk">&c_m;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_i; (&c_t;&c_zh; &c_p;&c_je;&c_r;&c_je;&c_n;.)</trd>
  <trdgrp lng="zh">
    <trd>&#33067;&#31890; <pr>tu&omacron;l&igrave;</pr></trd>,
    <trd>&#33067;&#31296; <pr>tu&omacron;g&#468;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="drasx.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <snc mrk="drasx.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Ago <ref cel="drasx.0i.AGR"><tld/>i<sncref/></ref>:
      <ekz>
        la tempo de <tld/>ado da&ubreve;ros &ccirc;e vi &gcirc;is la enkolektado
        de la vinberoj
        <fnt><bib>MT</bib><lok>&Lev; 26:5.</lok></fnt>.
      </ekz>
      <ekz>
        &ccirc;iuj manoj estis plenaj de laboro: sekalfal&ccirc;o, transporto, <tld/>o
        <fnt>
          <bib>HsT</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_c;&c_mol;&c_b;&c_a;</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_o;&c_l;&c_o;&c_t;&c_mol;&c_b;&c_a;</trd>,
      <trd>&c_o;&c_b;&c_m;&c_o;&c_l;&c_o;&c_t;</trd>
    </trdgrp>
  </snc>
    <snc mrk="drasx.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forta <ref cel="bat.0ado">batado</ref>:
      <ekz>
        &ccirc;iuj firme esperis, ke li rekonscii&gcirc;os morga&ubreve; matene, &leftquot;sed ja
        pro tia <tld/>ado &ccirc;io povos okazi, kaj eble la homo mortos&rightquot;
        <fnt><aut>F. Dostojevskij, tr. A. Kor&jcirc;enkov</aut>:
          <vrk>Kampulo Marej. Fragmento el &leftquot;La taglibro de verkisto&rightquot;</vrk>.
          <bib>LOdE</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_l;&c_u;&c_p;&c_c;&c_o;&c_w;&c_k;&c_a;</trd>,
      <trd>&c_l;&c_a;&c_z;&c_mol;&c_n;&c_ja; <klr tip="amb">(&c_p;&c_je;&c_r;&c_a;&c_n;.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_r;&c_jo;&c_p;&c_k;&c_a;</trd>,
      <trd>&c_v;&c_z;&c_b;&c_u;&c_ch;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_o;&c_b;&c_o;&c_i;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#25171;&#12388;&#12371;&#12392;  <pr>&#12358;&#12388;&#12371;&#12392;</pr></trd>,
    <trd>&#33073;&#31296;  <pr>&#12384;&#12387;&#12371;&#12367;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>m&lstroke;ocka</trd>,
    <trd>om&lstroke;ot</trd>
  </trdgrp>
  <trd lng="uk">&c_m;&c_o;&c_l;&c_o;&c_t;&c_mol;&c_b;&c_a;</trd>
</drv>

<drv mrk="drasx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="drasx.0ajxo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <tld lit="D"/>ita greno:
      <ekz>
        mia <tld/>a&jcirc;o
        <fnt><bib>MT</bib>, <lok>&Jes; 21:10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_m;&c_a;&c_l;&c_o;&c_ch;&c_a;&c_n;&c_a;&c_je; &c_z;&c_b;&c_o;&c_zh;&c_zh;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Drusch</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kics&eacute;pelt gabona</trd>
  </trdgrp>
  <trd lng="ja">&#33073;&#31296;&#12375;&#12383;&#31296;&#29289;  <pr>&#12384;&#12387;&#12371;&#12367;&#12375;&#12383;&#12371;&#12367;&#12418;&#12388;</pr></trd>
  <trd lng="pl">om&lstroke;ot</trd>
  <trd lng="ru">&c_n;&c_a;&c_m;&c_o;&c_l;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j; &c_h;&c_l;&c_je;&c_b;</trd>
</drv>

<drv mrk="drasx.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="drasx.0ejo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Loko, el seka kaj malmola tero, kie oni <tld/>as:
      <ekz>
        <klr>[li]</klr> ventumas hordeon dum &ccirc;i tiu nokto en la <tld/>ejo
        <fnt>
          <bib>MT</bib>
          <lok>Rut 3:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        feston de la&ubreve;boj faru al vi dum sep tagoj, kiam vi kolektos el via <tld/>ejo kaj
        el via vinpremejo
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 16:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        li elpurigos sian <tld/>ejon
        <fnt><bib>NT</bib>, <lok>&Mat; 3:12</lok></fnt>;
      </ekz>
      <ekz>
        se silentas <tld/>ejo, malpacas lo&gcirc;ejo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_m;&c_n;&c_o;</trd>
  <trdgrp lng="de">
    <trd>Dreschboden</trd>,
    <trd>Tenne</trd>
  </trdgrp>
  <trd lng="en">threshing floor</trd>
  <trd lng="fr">aire de battage</trd>
  <trd lng="he">&gimel;&vav;&resh;&fnun;</trd>
  <trd lng="hu">sz&eacute;r&udblac;</trd>
  <trd lng="ja">&#33073;&#31296;&#22580;  <pr>&#12384;&#12387;&#12371;&#12367;&#12376;&#12423;&#12358;</pr></trd>
  <trdgrp lng="pl">
    <trd>klepisko</trd>,
    <trd>gumno</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_k;</trd>,
    <trd>&c_g;&c_u;&c_m;&c_n;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_t;&c_ib;&c_k;</trd>,
    <trd>&c_g;&c_u;&c_m;&c_n;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="drasx.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="drasx.0ilo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Instrumento por <tld/>i, plejofte konsistanta el du bastonoj
      kunigitaj per rimeno, el kiuj la pli longa servas kiel tenilo
      kaj la malpli longa kaj pli dika kiel batilo:
      <ekz>
        jen estas bovoj <klr>[&#8230;]</klr> kaj la <tld/>iloj kaj la
        jungilaro de la bovoj
        <fnt><bib>MT</bib>, <lok>&Sa2; 24:22</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>iloj frapadis la nenion
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni mortbatus lin per <tld/>ilo kiel oldan hundon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_e;&c_p;</trd>
  <trdgrp lng="de">
    <trd>Dreschflegel</trd>,
    <trd>Dreschfl&uuml;gel</trd>
  </trdgrp>
  <trd lng="en">flail</trd>
  <trd lng="fr">fl&eacute;au<klr tip="ind"> (&agrave;
  battre)</klr></trd>
  <trd lng="he">&mem;&vav;&resh;&gimel;</trd>
  <trd lng="hu">cs&eacute;phadar&oacute;</trd>
  <trd lng="ja">&#12363;&#12425;&#12374;&#12362;</trd>
  <trd lng="pl">cep</trd>
  <trd lng="ru">&c_c;&c_je;&c_p;</trd>
  <trd lng="uk">&c_c;&c_ib;&c_p;</trd>
  <trd lng="zh">&#33067;&#31890;&#27231;&#27083; <pr>tu&omacron;l&igrave;j&imacron;g&ograve;u</pr></trd>
</drv>

<drv mrk="drasx.tra0i">
  <kap>tra<tld/>i</kap>
  <snc mrk="drasx.tra0i.ripete">
    <dif>
      <tld lit="D"/>i komplete, ripete &gcirc;is nenio restas ne<tld/>ita:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi neniam penetris pli ol la unuan &ccirc;apitron, sed tiun mi devis tra<tld/>i
        ripetitajn fojojn, &ccirc;ar pa&ccirc;jo estis obstina kiel kapro
        <fnt>
          <bib>DKo</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_m;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol; <klr>(&c_c;&c_a;&c_l;&c_k;&c_a;&c_m;)</klr></trd>,
      <trd>&c_z;&c_m;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="drasx.tra0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Doni <ref cel="drasx.0o.FIG"><tld/>adon<sncref/></ref>:
      <ekz>
        estis afero kun tra<tld/>itaj strikrompantoj
        <fnt><bib>Mortul&Scirc;ip</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_l;&c_u;&c_p;&c_c;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_mol;&c_b;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">durchpr&uuml;geln</trd>
    <trdgrp lng="ru">
      <trd>&c_i;&c_z;&c_b;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_k;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_d;&c_a;&c_t;&c_mol; <ind>&c_t;&c_r;&c_jo;&c_p;&c_k;&c_u;</ind></trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="drasx.fera0ilo">
  <kap>fera <tld/>ilo</kap>
  <snc mrk="drasx.fera0ilo.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="super" cel="drasx.0ilo.AGR"><tld lit="D"/>ilo</ref>
      konsistanta el framo kun subaj nadloj, tirata de bruto:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ili <tld/>is Gileadon per fera <tld/>ilo
        <fnt><bib>MT</bib>, <lok>&Amo; 1:3</lok></fnt>;
      </ekz>
      <ekz>
        ne per fera <tld/>ilo oni <tld/>as nigelon
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 28:27</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Dreschschlitten</trd>
    <trd lng="en">threshing board</trd>
  </snc>
  <trd lng="fr">traineau &agrave; battre</trd>
</drv>

<drv mrk="drasx.0masxino">
  <kap><tld/>ma&scirc;ino</kap>
  <snc mrk="drasx.0masxino.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="super" cel="masxin.0o.TEK">Ma&scirc;ino</ref> por <tld/>i:
      <ekz>
        bovoj, gvidataj de subservisto, tiradis en rondo la <tld/>ma&scirc;inon
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ma&scirc;ino, venigita el Moskvo, estis tro peza por esti uzata
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_l;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Dreschmaschine</trd>
  </trdgrp>
  <trd lng="en">threshing machine</trd>
  <trd lng="fr">batteuse<klr tip="ind"> (&agrave;
  c&eacute;r&eacute;ales)</klr></trd>
  <trd lng="he">&mem;&kaf;&vav;&nun;&tav; &dalet;&yod;&shin;&he;</trd>
  <trd lng="hu">cs&eacute;pl&odblac;g&eacute;p</trd>
  <trd lng="ja">&#33073;&#31296;&#27231;  <pr>&#12384;&#12387;&#12371;&#12367;&#12365;</pr></trd>
  <trd lng="pl">m&lstroke;ockarnia</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_o;&c_t;&c_i;&c_l;&c_k;&c_a;</trd>
  <trd lng="uk">&c_m;&c_o;&c_l;&c_o;&c_t;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="zh">&#36554;&#24202; <pr>ch&emacron;chu&aacute;ng</pr></trd>
</drv>

</art>
