<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>marionet</rad>/o
</kap>
<drv mrk="marion.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Malgranda homfigura
      <ref tip="super" cel="pup.0o.ludilo">pupo</ref>
      kun moveblaj membroj movataj per fadenoj a&ubreve;  risortoj.
    </dif>
    <trd lng="en">marionette</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo sen karaktero, sen propra volo
    </dif>
    <refgrp tip="sin">
      <ref cel="pulcxi.0o">pul&ccirc;inelo</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">marionet</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_i;&c_o;&c_n;&c_je;&c_t;&c_k;&c_a;</trd>
</drv>
</art>
