<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc> <rad>&hcirc;irurgi</rad>/o <fnt>Z</fnt>
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="hxirur.0o">
  <kap><tld/>o</kap>
  <snc mrk="hxirur.0o.MED">
    <uzo tip="fak">KIR</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="medici.0o.SCI">medicino</ref> 
      pritraktanta la kuracadon de malsanoj per operacioj;
      <ref tip="sin" cel="kirurg1.0o">kirurgio</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_r;&c_u;&c_r;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_h;&c_i;&c_r;&c_u;&c_r;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">cirurgia</trd>
  <trd lng="de">Chirurgie</trd>
  <trdgrp lng="en">
    <trd>surgery</trd>
  </trdgrp>
  <trd lng="es">cirug&iacute;a</trd>
  <trd lng="fa">&a_j;&a_r;&a_A;&a_H;&f_y;</trd>
  <trd lng="fr">chirurgie</trd>
  <trd lng="hu">seb&eacute;szet</trd>
  <trd lng="nl">chirurgie</trd>
  <trd lng="pl">chirurgia</trd>
  <trd lng="pt">cirurgia</trd>
  <trd lng="ru">&c_h;&c_i;&c_r;&c_u;&c_r;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">chirurgia</trd>
  <trd lng="sv">kirurgi</trd>
  <trd lng="it">chirurgia</trd>
</drv>

<drv mrk="hxirur.0a">
  <kap><tld/>a</kap>
  <snc mrk="hxirur.0a.MED">
    <dif>
      Rilata al <tld/>o, al &gcirc;iaj operaciaj metodoj kaj rimedoj;
      <ref tip="sin" cel="kirurg1.0a">kirurgia</ref>.
    </dif>
  </snc>
  <trd lng="bg">&c_h;&c_i;&c_r;&c_u;&c_r;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">quir&uacute;rgic</trd>
  <trd lng="de">chirurgisch</trd>
  <trd lng="fr">chirurgical</trd>
  <trd lng="pl">chirurgiczny</trd>
  <trd lng="pt">cir&uacute;rgico</trd>
  <trd lng="ru">&c_h;&c_i;&c_r;&c_u;&c_r;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sv">kirurgisk</trd>
  <trd lng="it">chirurgico</trd>
</drv>

<drv mrk="hxirur.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="hxirur.0isto.MED">
    <uzo tip="fak">KIR</uzo>
    <dif>
      Specialisto pri <tld/>o;
      <ref tip="sin" cel="hxirurg.0o">&hcirc;irurgo</ref>,
      <ref tip="sin" cel="kirurg.0o">kirurgo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_r;&c_u;&c_r;&c_g;</trd>
  <trd lng="bg">&c_h;&c_i;&c_r;&c_u;&c_r;&c_g;</trd>
  <trd lng="ca">cirurgi&agrave;</trd>
  <trd lng="de">Chirurg</trd>
  <trd lng="en">surgeon</trd>
  <trd lng="es">cirujano</trd>
  <trd lng="fa">&a_j;&a_r;&a_A;&a_H;</trd>
  <trd lng="fr">chirurgien</trd>
  <trd lng="hu">seb&eacute;sz</trd>
  <trd lng="nl">chirurg</trd>
  <trd lng="pl">chirurg</trd>
  <trd lng="pt">cirurgi&atilde;o</trd>
  <trd lng="ru">&c_h;&c_i;&c_r;&c_u;&c_r;&c_g;</trd>
  <trd lng="sv">kirurg</trd>
  <trd lng="sk">chirurg</trd>
  <trd lng="it">chirurgo</trd>
</drv>
</art>
