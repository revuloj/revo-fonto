<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>8</ofc> <rad>&hcirc;irurgi</rad>/o <fnt>Z</fnt></kap>
<drv mrk="hxirur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Parto de la medicino pritraktanta la kuracadon de malsanoj per
      operacioj.
    </dif>
  </snc>
  <snc>
    <dif>
      Arto kuraci malsanulojn per operacioj.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_r;&c_u;&c_r;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="de">Chirurgie</trd>
  <trdgrp lng="fr">
    <trd>chirurgie</trd>
  </trdgrp>
  <trd lng="nl">chirurgie</trd>
  <trd lng="pt">cirurgia</trd>
  <trd lng="ru">&c_h;&c_i;&c_r;&c_u;&c_r;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="hxirur.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Specialisto en <tld/>o.
    </dif>
    <ref tip="sin" cel="hxirurg.0o">&hcirc;irurgo.</ref>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_r;&c_u;&c_r;&c_g;</trd>
  <trdgrp lng="fr">
    <trd>chirurgien</trd>
  </trdgrp>
  <trd lng="nl">chirurg</trd>
  <trd lng="pt">cirurgi&atilde;o</trd>
  <trd lng="ru">&c_h;&c_i;&c_r;&c_u;&c_r;&c_g;</trd>
</drv>
</art>
