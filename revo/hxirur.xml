<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc> <rad>&hcirc;irurgi</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="hxirur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Parto de la medicino pritraktanta la kuracadon de malsanoj per
      operacioj.
    </dif>
    <refgrp tip="sin">
      <ref cel="kirurg1.0o">kirurgio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_r;&c_u;&c_r;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="de">Chirurgie</trd>
  <trdgrp lng="en">
    <trd>surgery</trd>,
    <trd>operation</trd>
  </trdgrp>
  <trd lng="es">cirug&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>chirurgie</trd>
  </trdgrp>
  <trd lng="hu">seb&eacute;szet</trd>
  <trd lng="nl">chirurgie</trd>
  <trd lng="pt">cirurgia</trd>
  <trd lng="ru">&c_h;&c_i;&c_r;&c_u;&c_r;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">chirurgia</trd>
</drv>

<drv mrk="hxirur.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Specialisto pri <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="hxirurg.0o">&hcirc;irurgo</ref>,
      <ref cel="kirurg.0o">kirurgo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_ib;&c_r;&c_u;&c_r;&c_g;</trd>
  <trd lng="de">Chirurg</trd>
  <trd lng="en">surgeon</trd>
  <trd lng="es">cirujano</trd>
  <trd lng="fr">chirurgien</trd>
  <trd lng="hu">seb&eacute;sz</trd>
  <trd lng="nl">chirurg</trd>
  <trd lng="pt">cirurgi&atilde;o</trd>
  <trd lng="ru">&c_h;&c_i;&c_r;&c_u;&c_r;&c_g;</trd>
  <trd lng="sv">kirurg</trd>
  <trd lng="sk">chirurg</trd>
</drv>
</art>
