<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: verteb.xml,v 1.44 2023/10/21 16:50:36 revo Exp $">
  <kap>
    <ofc>*</ofc>
    <rad>vertebr</rad>/o
  </kap>

<drv mrk="verteb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;iu <ref tip="super" cel="ost.0o">osto</ref> de la <ref tip="malprt" cel="spin.0o">spino</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_v;&c_a;&c_n;&c_o;&c_k;</trd>
  <trd lng="ca">v&egrave;rtebra</trd>
  <trd lng="cs">obratel</trd>
  <trd lng="de">Wirbel</trd>
  <trd lng="el">&sigma;&pi;&omikron_ton;&ny;&delta;&ypsilon;&lambda;&omikron;&sigma_fina;</trd>
  <trd lng="en">vertebra</trd>
  <trd lng="es">v&eacute;rtebra</trd>
  <trd lng="fr">vert&egrave;bre</trd>
  <trd lng="hu">csigolya</trd>
  <trd lng="it">vertebra</trd>
  <trdgrp lng="ja">
    <trd>&#x690E;&#x9AA8;  <pr>&#x3064;&#x3044;&#x3053;&#x3064;</pr></trd>,
    <trd>&#x810A;&#x690E;  <pr>&#x305B;&#x304D;&#x3064;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">wervel</trd>
  <trd lng="pl">kr&aogonek;g <klr>(np. pacierzowy)</klr></trd>
  <trd lng="pt">v&eacute;rtebra</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_k;</trd>
  <trd lng="sk">stavec</trd>
  <trd lng="tr">omur</trd>
  <trd lng="uk">&#x445;&#x440;&#x435;&#x431;&#x435;&#x446;&#x44C;</trd>
</drv>

<drv mrk="verteb.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o:
      <ekz>
        <ind><tld/>a kolono</ind> <klr>(= <tld/>aro)</klr>.
        <trd lng="hu">gerincoszlop</trd>
        <trd lng="ru">&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_s;&c_t;&c_o;&c_l;&c_b;</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_z;&c_v;&c_a;&c_n;&c_k;&c_o;&c_v;&c_y;</trd>
    <trd lng="ca">vertebral</trd>
    <trd lng="el">&sigma;&pi;&omikron;&ny;&delta;&ypsilon;&lambda;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
    <trd lng="es">vertebrado</trd>
    <trd lng="fr">vert&eacute;bral</trd>
    <trdgrp lng="hu">
      <trd>csigolya-</trd>,
      <trd>gerinc-</trd>
    </trdgrp>
    <trd lng="it">vertebrale</trd>
    <trd lng="pl">kr&eogonek;gowy</trd>
    <trd lng="pt">vertebral</trd>
    <trd lng="ru">&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Havanta <tld/>ojn:
      <ekz>
        <tld/>a besto.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_z;&c_v;&c_a;&c_n;&c_o;&c_ch;&c_n;&c_y;</trd>
    <trd lng="ca">vertebrat</trd>
    <trd lng="en">vertebrate</trd>
    <trd lng="fr">vert&eacute;br&eacute;<klr> (adj.)</klr></trd>
    <trd lng="hu">gerinces</trd>
    <trd lng="it">vertebrato<klr> (agg.)</klr></trd>
    <trd lng="pl">kr&eogonek;gowy</trd>
    <trd lng="pt">vertebrado</trd>
    <trd lng="ru">&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="en">vertebral</trd>
  <trdgrp lng="ja">
    <trd>&#x690E;&#x9AA8;&#x306E;  <pr>&#x3057;&#x3044;&#x3053;&#x3064;&#x306E;</pr></trd>,
    <trd>&#x810A;&#x690E;&#x306E;  <pr>&#x305B;&#x304D;&#x3064;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x810A;&#x690E;&#x306E;&#x3042;&#x308B;  <pr>&#x305B;&#x304D;&#x3064;&#x3044;&#x306E;&#x3042;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">ge<ind>wervel</ind>d</trd>
  <trdgrp lng="uk">
    <trd>&#x445;&#x440;&#x435;&#x431;&#x446;&#x435;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x445;&#x440;&#x435;&#x431;&#x435;&#x442;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="verteb.0aro">
  <kap><tld/>aro</kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <dif>  
      <ref tip="dif" cel="spin.0o">Spino</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &ccirc;efa el tiuj entreprenoj, la <tld/>aro de la afero,
        estis la Bagdada fervojo;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        tiuj ideoj i&gcirc;is la <tld/>aro de nia movado.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_v;&c_a;&c_n;&c_o;&c_ch;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="ca">
        <trd>columna <ind>vertebra</ind>l</trd>,
        <trd><ind>espina</ind> dorsal</trd>
      </trdgrp>
  <trd lng="cs">p&aacute;te&rcaron;</trd>
  <trd lng="de">Wirbels&auml;ule</trd>
  <trd lng="en">spine</trd>
  <trd lng="fr">colonne vert&eacute;brale</trd>
  <trdgrp lng="hu">
        <trd>gerinc</trd>,
        <trd>gerincoszlop</trd>
      </trdgrp>
  <trd lng="it"><ind>colonna</ind> vertebrale</trd>
  <trd lng="ja">&#x810A;&#x67F1;  <pr>&#x305B;&#x304D;&#x3061;&#x3085;&#x3046;</pr></trd>
  <trdgrp lng="nl">
        <trd>wervelkolom</trd>,
        <trd>ruggegraat</trd>
      </trdgrp>
  <trd lng="pl">kr&eogonek;gos&lstroke;up</trd>
  <trd lng="pt">espinha</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">chrbtica</trd>
  <trd lng="tr">omurga</trd>
  <trdgrp lng="uk">
    <trd>&#x445;&#x440;&#x435;&#x431;&#x435;&#x442;</trd>,
    <trd>&#x445;&#x440;&#x435;&#x431;&#x435;&#x442;&#x43D;&#x438;&#x439; &#x441;&#x442;&#x43E;&#x432;&#x43F;</trd>
  </trdgrp>
</drv>

<drv mrk="verteb.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="verteb.0ulo.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="verteb.0uloj"><tld/>uloj</ref>.
    </dif>
  </snc>
  <trd lng="cs">obratlovec</trd>
  <trd lng="fr">vert&eacute;br&eacute;<klr> (subst.)</klr></trd>
  <trd lng="sk">stavovec</trd>
</drv>

<drv mrk="verteb.0uloj">
  <kap><tld/>uloj</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="filum.sub0o" lst="voko:zoologiaj_grupoj">Subfilumo</ref>
      de <ref tip="malprt" cel="hxord.0uloj">&hcirc;orduloj</ref>
      (<trd lng="la">Vertebrata</trd>),
      havantaj <tld/>aron, al kiu apartenas i.a. la grupoj
      <refgrp tip="prt">
        <ref cel="busx.rond0uloj">rondbu&scirc;uloj</ref>,
        <ref cel="fisx.kartilago0oj">kartilagofi&scirc;oj</ref>,
        <ref cel="fisx.osto0oj">ostofi&scirc;oj</ref>,
        <ref cel="amfibi.0oj">amfibioj</ref>,
        <ref cel="reptil.0oj">reptilioj</ref>,
        <ref cel="bird.0oj">birdoj</ref> kaj
        <ref cel="mam.0uloj">mamuloj</ref>.
      </refgrp>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_v;&c_a;&c_n;&c_o;&c_ch;&c_n;&c_y;&c_ja;</trd>
  <trd lng="ca">vertebrats</trd>
  <trd lng="cs">obratlovci</trd>
  <trd lng="de">Wirbeltiere</trd>
  <trd lng="en">vertebrates</trd>
  <trd lng="fr">vert&eacute;br&eacute;s</trd>
  <trd lng="hu">gerincesek</trd>
  <trd lng="it">vertebrati<klr> (sost.)</klr></trd>
  <trd lng="ja">&#x810A;&#x690E;&#x52D5;&#x7269;  <pr>&#x305B;&#x304D;&#x3064;&#x3044;&#x3069;&#x3046;&#x3076;&#x3064;</pr></trd>
  <trd lng="nl">ge<ind>wervel</ind>de dieren</trd>
  <trd lng="pl">kr&eogonek;gowce</trd>
  <trd lng="pt">vertebrados</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_ch;&c_n;&c_y;&c_je;</trd>
  <trd lng="sk">stavovce</trd>
  <trd lng="tr">omurgal&inodot; hayvanlar</trd>
  <trd lng="uk">&#x445;&#x440;&#x435;&#x431;&#x435;&#x442;&#x43D;&#x456;</trd>
</drv>

<drv mrk="verteb.sen0uloj">
  <kap>sen<tld/>uloj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="malprt" cel="best.0oj">Bestoj</ref> ne havantaj
      <tld/>aron, 
      kontra&ubreve;aj al la
      <ref tip="ant" cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Evertebrata</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_s;&c_p;&c_a;&c_z;&c_v;&c_a;&c_n;&c_o;&c_ch;&c_n;&c_y;&c_ja;</trd>
  <trd lng="ca">invertebrats</trd>
  <trd lng="cs">bezobratl&iacute;</trd>
  <trd lng="en">invertebrates</trd>
  <trd lng="fr">invert&eacute;br&eacute;s</trd>
  <trd lng="hu">gerinctelenek</trd>
  <trd lng="it">invertebrati<klr> (sost.)</klr></trd>
  <trd lng="ja">&#x7121;&#x810A;&#x690E;&#x52D5;&#x7269;  <pr>&#x3080;&#x305B;&#x304D;&#x3064;&#x3044;&#x3069;&#x3046;&#x3076;&#x3064;</pr></trd>
  <trd lng="nl">onge<ind>wervel</ind>de dieren</trd>
  <trd lng="pl">bezkr&eogonek;gowce</trd>
  <trd lng="pt">invertebrados</trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_ch;&c_n;&c_y;&c_je;</trd>
  <trd lng="sk">bezstavovce</trd>
  <trd lng="uk">&#x431;&#x435;&#x437;&#x445;&#x440;&#x435;&#x431;&#x435;&#x442;&#x43D;&#x456;</trd>
</drv>
