<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vertebr</rad>/o
</kap>
<drv mrk="verteb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      &Ccirc;iu osto de la <ref tip="malprt" cel="spin.0o">spino</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_v;&c_a;&c_n;&c_o;&c_k;</trd>
  <trd lng="de">Wirbel</trd>
  <trd lng="el">&sigma;&pi;&omikron_ton;&ny;&delta;&ypsilon;&lambda;&omikron;&sigma_fina;</trd>
  <trd lng="hu">csigolya</trd>
  <trd lng="nl">wervel</trd>
  <trd lng="pt">v&eacute;rtebra</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_k;</trd>
  <trd lng="sk">stavec</trd>
  <trd lng="tr">omur</trd>
</drv>

<drv mrk="verteb.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o:
      <ekz>
        <tld/>a kolono
        <klr>(<tld/>aro)</klr>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_z;&c_v;&c_a;&c_n;&c_k;&c_o;&c_v;&c_y;</trd>
    <trd lng="el">&sigma;&pi;&omikron;&ny;&delta;&ypsilon;&lambda;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
    <trd lng="fr">vert&eacute;bral</trd>
    <trdgrp lng="hu">
      <trd>csigolya-</trd>,
      <trd>gerinc-</trd>
    </trdgrp>
    <trd lng="pt">vertebral</trd>
    <trd lng="ru">&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Havanta <tld/>ojn:
      <ekz>
        <tld/>a besto.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_z;&c_v;&c_a;&c_n;&c_o;&c_ch;&c_n;&c_y;</trd>
    <trd lng="fr">vert&eacute;br&eacute;</trd>
    <trd lng="hu">gerinces</trd>
    <trd lng="pt">vertebrado</trd>
    <trd lng="ru">&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="nl">gewerveld</trd>
</drv>

<drv mrk="verteb.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      <ref tip="sin" cel="spin.0o">Spino</ref>:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &ccirc;efa el tiuj entreprenoj,
        la <tld/>aro de la afero, estis la Bagdad'a fervojo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiuj ideoj i&gcirc;is la <tld/>aro de nia movado.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_v;&c_a;&c_n;&c_o;&c_ch;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Wirbels&auml;ule</trd>
  <trd lng="fr">colonne vert&eacute;brale</trd>
  <trd lng="hu">gerinc</trd>
  <trdgrp lng="nl">
    <trd>wervelkolom</trd>,
    <trd>ruggegraat</trd>
  </trdgrp>
  <trd lng="pt">espinha</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>
  <trd lng="tr">omurga</trd>
</drv>

<drv mrk="verteb.0uloj">
  <kap><tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Subfilumo de <ref tip="super" cel="hxord.0uloj">&hcirc;orduloj</ref>
      (<trd lng="la">Vertebrata</trd>),
      havantaj <tld/>aron, al kiu apartenas i.a. la klasoj
      <refgrp tip="sub">
        <ref cel="busx.rond0uloj">rondbu&scirc;uloj</ref>,
        <ref cel="fisx.kartilago0oj">kartilagofi&scirc;oj</ref>,
        <ref cel="fisx.osto0oj">ostofi&scirc;oj</ref>,
        <ref cel="amfibi.0oj">amfibioj</ref>,
        <ref cel="ramp.0uloj">rampuloj</ref>,
        <ref cel="bird.0oj">birdoj</ref> kaj
        <ref cel="mam.0uloj">mamuloj</ref>.
      </refgrp>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_v;&c_a;&c_n;&c_o;&c_ch;&c_n;&c_y;&c_ja;</trd>
  <trd lng="de">Wirbeltiere</trd>
  <trd lng="fr">vert&eacute;br&eacute;s</trd>
  <trd lng="hu">gerincesek</trd>
  <trd lng="nl">gewervelde dieren</trd>
  <trd lng="pt">vertebrados</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_v;&c_o;&c_n;&c_o;&c_ch;&c_n;&c_y;&c_je;</trd>
  <trd lng="sk">stavovce</trd>
  <trd lng="tr">omurgal&inodot; hayvanlar</trd>
</drv>
</art>
