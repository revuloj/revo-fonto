<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: blus.xml,v 1.14 2023/10/29 19:11:02 revo Exp $">
<kap>
  <rad>blus</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="blus.0o">
  <kap><tld/>o</kap>
  <snc mrk="blus.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Malgaja kanzono kun ripeta melodio kaj propraj tonoj,
      kreita de sudusonaj sklavidoj &ccirc;irka&ubreve; 1900, iu el la
      bazoj de
      <ref tip="vid" cel="jxaz.0o">&jcirc;azo</ref> kaj roko:
      <ekz>
        la pasportservan <tld/>on kantas mi
        <fnt><aut>Duncan Thomson</aut>, <vrk><url ref="http://www.duncanthomson.com/esperanto/pasportservabluso.html">pasportserva
        bluso</url></vrk>, <lok>en interreto en 2005-02</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5E03;&#x9B6F;&#x65AF; <pr>b&#xF9;l&#x1D4;s&#x12B;</pr></trd>,
    <trd>&#x84DD;&#x8C03; <pr>l&#xE1;ndi&#xE0;o</pr></trd>
  </trdgrp>
</snc>
  <ref tip="lst" cel="danc.0o" lst="voko:dancoj">danco</ref>
  <trd lng="be">&c_b;&c_l;&c_ju;&c_z;</trd>
  <trd lng="bg">&c_b;&c_l;&c_u;&c_s;</trd>
  <trd lng="cs">blues</trd>
  <trd lng="de">Blues</trd>
  <trd lng="en">blues</trd>
  <trd lng="es">blues</trd>
  <trd lng="fa">&a_b;&a_l;&a_w;&a_z;<klr tip="ind"> (&a_m;&a_w;&a_s;&f_y;&a_q;&f_y;)</klr></trd>
  <trd lng="fr">blues</trd>
  <trd lng="he">&#x5D1;&#x5DC;&#x5D5;&#x5D6;</trd>
  <trd lng="it">blues</trd>
  <trd lng="pl">blues</trd>
  <trd lng="sk">blues</trd>
  <trd lng="uk">&c_b;&#x43B;&#x44E;&c_z;</trd>
</drv>
</art>

