<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mustel1.xml,v 1.14 2022/03/23 14:02:27 revo Exp $">
<kap>
  <rad>musteled</rad>/oj
</kap>
<drv mrk="mustel1.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Mustelidae</trd>), al kiu apartenas
      <refgrp tip="prt">
        <ref cel="lutr.0o">lutro</ref>,
        <ref cel="martes.0o">marteso</ref>,
        <ref cel="mustel.0o">mustelo</ref>,
        k.a.</refgrp>:
      <ekz>
        <tld/>oj estas familio de rabobestoj kun preska&ubreve; serpente elasta korpo kaj
        valora felo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9134">Musteledoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        grizono, konata anka&ubreve; kiel Sud-amerika gulo, estas neotropika <tld/>o el
        Sud-Ameriko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=7059">Grizono (sudamerika gulo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_n;&c_ib;&c_c;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">must&egrave;lids</trd>
  <trd lng="de">Marder</trd>
  <trd lng="fr">Must&eacute;lid&eacute;s</trd>
  <trd lng="hu">meny&eacute;tf&eacute;l&eacute;k</trd>
  <trd lng="nl">marterachtigen</trd>
  <trdgrp lng="pl">
    <trd>&lstroke;asicowate</trd>,
    <trd>kunowate</trd>
  </trdgrp>
  <trd lng="pt">mustel&iacute;deos <klr tip="amb">(zool.)</klr></trd>
  <trd lng="ru">&c_k;&c_u;&c_n;&c_mol;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&#x43A;&#x443;&#x43D;&#x438;&#x446;&#x435;&#x432;&#x456;</trd>
</drv>
</art>
