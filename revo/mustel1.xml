<drv mrk="mustel1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super"
      cel="best.rabo0oj">rabobestoj</ref>
      (<trd lng="la">Mustelidae</trd>), al kiu apartenas
