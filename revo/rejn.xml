<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Rejn</rad>/o
</kap>
<drv mrk="rejn.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="river.0o">Rivero</ref>, fluanta el
      <ref tip="malprt" cel="svis.S0ujo">Svisujo</ref>, tra
      <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref> al
      <ref tip="malprt" cel="nederl.0o">Nederlando</ref>,
      kie &gcirc;i fluas en la Nordan Maron:
      <ekz>
        tiu, kiu veturis sur <tld/>o de Bazelo al Kolonjo, devis
        fine de la 16a jarcento doganpagi je 31 lokoj.
        <fnt><aut>Bo SANDELIN</aut>:
        <vrk><url ref="&Monato;/007278.html">Historio de la ekonomika
        pensado</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 02/2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">Rhin</trd>
  <trd lng="bg">Pe&c_j;&c_n;</trd>
  <trd lng="cs">R&yacute;n</trd>
  <trd lng="en">Rhine</trd>
  <trd lng="es">Rin</trd>
  <trd lng="de">Rhein</trd>
  <trd lng="fr">Rhin</trd>
  <trd lng="gd">An <ind>R&eacute;in</ind></trd>
  <trd lng="is">R&iacute;n</trd>
  <trd lng="it">Reno</trd>
  <trd lng="hu">Rajna</trd>
  <trd lng="nl">Rijn</trd>
  <trd lng="no">Rhinen</trd>
  <trd lng="pt">Reno</trd>
  <trd lng="pl">Ren</trd>
  <trd lng="ru">&c_R;&c_je;&c_j;&c_n;</trd>
  <trd lng="sl">Ren</trd>
  <trd lng="sv">Rhen</trd>
</drv>
</art>
