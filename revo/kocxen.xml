<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ko&ccirc;enil</rad>/o
</kap>
<drv mrk="kocxen.0o">
  <kap><tld/>o</kap>
  <snc mrk="kocxen.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <fnt><bib>VdE</bib></fnt>
    <dif>
      Specio de insektoj, vivantaj sur kaktoj kaj enhavantaj
      <ref cel="karmin.0o">karminon</ref>.
    </dif>
    <ref tip="sin" cel="kocx.0o">ko&ccirc;o</ref>
  </snc>
  <snc mrk="kocxen.0o.karmino">
    <uzo tip="stl">EVI</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <ref tip="dif" cel="karmin.0o">karmino</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_e;&c_n;&c_ib;&c_l;&c_mol;</trd>
  <trd lng="de">Cochenille</trd>
  <trd lng="en">cochineal</trd>
  <trd lng="fr">cochenille</trd>
  <trd lng="nl">cochenille</trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_l;&c_mol;</trd>
</drv>
</art>
