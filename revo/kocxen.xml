<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kocxen.xml,v 1.24 2023/12/03 09:58:51 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>ko&ccirc;enil</rad>/o
</kap>
<drv mrk="kocxen.0o">
  <kap><tld/>o</kap>
  <snc mrk="kocxen.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <fnt><bib>VdE</bib></fnt>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o">Specio</ref>
      el familio <ref tip="malprt" cel="kocxed.0oj">ko&ccirc;edoj</ref>
      (<trd lng="la">Dactylopius coccus</trd>), kiun oni bredas
      pro la <ref cel="karmin.0o">karmino</ref>, kiun eblas prepari el ili.
    </dif>
    <ref tip="vid" cel="kocx.0o">ko&ccirc;o</ref>
    <trd lng="ca">cotxinilla</trd>
    <trd lng="br">taneenn</trd>
    <trd lng="de">Cochenilleschildlaus</trd>
    <trd lng="es">cochinilla <klr>(Dactylopius coccus)</klr></trd>
  </snc>
  <snc mrk="kocxen.0o.preparajxo">
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Prepara&jcirc;o el mortigitaj inaj <tld/>oj <sncref ref="kocxen.0o.ZOO"/>, seka
      a&ubreve; flua&jcirc;a:
      <ekz>
        karmino estas ru&gcirc;a kolorilo <klr>[&#x2026;]</klr> el kalciaj kaj 
        aluminiaj saloj de karminata acido kaj estas ekstraktebla el <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=123285">Karmino</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ankora&ubreve; hodia&ubreve;, e&ccirc; se tre modere,
        oni eksportas <tld/>on <klr>[&#x2026;]</klr>, &ccirc;ar &gcirc;i
        montri&gcirc;is speciale ta&ubreve;ga por tinkturi nilonon
        kaj aliajn sintezajn fadenojn
        <fnt>
          <aut>J. R&eacute;gulo-P&eacute;rez</aut>
          <vrk>Misteroj, mitoj kaj faktoj</vrk>
          <lok><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr35/mitoju.html">Nica
          Literatura Revuo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">carm&iacute;</trd>
    <trdgrp lng="de">
      <trd><ofc>*</ofc>Cochenille</trd>,
      <trd>Karmins&auml;ure</trd>
    </trdgrp>
    <trd lng="es">cochinilla <klr>(colorante)</klr></trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_e;&c_n;&c_ib;&c_l;&c_mol;</trd>
  <trd lng="br">tane</trd>
  <trd lng="de"/>
  <trd lng="en">cochineal</trd>
  <trd lng="fr">cochenille</trd>
  <trdgrp lng="ja">
    <trd>&#x30B3;&#x30C1;&#x30CB;&#x30FC;&#x30EB;</trd>,
    <trd>&#x81D9;&#x8102;  <pr>&#x3048;&#x3093;&#x3058;</pr></trd>
  </trdgrp>
  <trd lng="nl">cochenille</trd>
  <trd lng="pl">koszenila</trd>     
  <trd lng="ro">co&#x219;enil&abreve;</trd>
  <trd lng="ru">&c_k;&c_o;&c_sh;&c_je;&c_n;&c_i;&c_l;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&#x448;&#x435;&#x43D;&#x456;&#x43B;&#x44C;</trd>,
    <trd>&#x43A;&#x430;&#x440;&#x43C;&#x456;&#x43D; (&#x444;&#x430;&#x440;&#x431;&#x430;)</trd>
  </trdgrp>
  <trd lng="zh">&#x80ED;&#x8102;&#x866B; <pr>y&#x101;nzh&#x12B;ch&#xF3;ng</pr></trd>
</drv>
</art>
