    <trd>tomar um <ind>drinque</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_t;&c_mol;
    <klr>(&c_s;&c_p;&c_i;&c_r;&c_t;&c_n;&c_o;&c_je;)</klr></trd>,
    <trd>&c_p;&c_mol;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ch&lcaron;asta&tcaron;</trd>,
    <trd>pi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>i&ccedil;mek</trd>,
    <trd>i&ccedil;ki i&ccedil;mek</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_i;&c_ja;&c_ch;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x72C2;&#x98F2; <pr>ku&#xE1;ngy&#x1D0;n</pr></trd>
</drv>

<drv mrk="drink.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="drink.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Ebriiga, alkoholenhava <ref tip="super" cel="trink.0ajxo">trinka&jcirc;o</ref>:
      <ekz>
        pastro kaj profeto &scirc;anceli&gcirc;as de <tld/>a&jcirc;o, senkonscii&gcirc;as de vino
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 28:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        festenoj okazis, kaj tiam oni &scirc;paris nek man&gcirc;on nek <tld/>a&jcirc;on
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la delikata bonodoro de la floroj ebriigis &scirc;in
        kvaza&ubreve; <tld/>a&jcirc;o
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k; <klr>(&c_a;&c_l;&c_mol;&c_k;&c_a;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;)</klr></trd>,
    <trd>&c_n;&c_a;&c_p;&c_o;&c_j; <klr>(&c_a;&c_l;&c_mol;&c_k;&c_a;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;)</klr></trd>,
    <trd>&c_v;&c_y;&c_p;&c_ib;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_l;&c_mol;&c_k;&c_a;&c_g;&c_o;&c_l;&c_mol; <klr>(&c_n;&c_a;&c_p;&c_ib;&c_t;&c_a;&c_k;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_a;&c_p;&c_i;&c_t;&c_k;&c_a;</trd>
  <trd lng="bo">&t_-a;&t_tha;&t_shabchu;&t_nga;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trd lng="ca">beguda alcoh&ograve;lica</trd>
  <trd lng="de"><klr tip="amb">(alkoholisches) </klr><ind>Getr&auml;nk</ind></trd>
  <trd lng="en"><klr tip="amb">(alcoholic) </klr>drink</trd>
  <trdgrp lng="es">
    <trd>bebida alcoh&oacute;lica</trd>,
    <trd>copete <klr>(Lat. Am.)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>alcool<klr tip="amb"> (boisson
    alcoolis&eacute;e)</klr></trd>,
    <trd>boisson alcoolis&eacute;e</trd>
  </trdgrp>
  <trd lng="he"><ind>&mem;&shin;&qof;&he;</ind>
  &het;&resh;&yod;&fpe;</trd>
