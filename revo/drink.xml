    <drv mrk="drink.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          <ref tip="super" cel="trink.0i">Trinki</ref>
          <ref cel="ebri.0igajxo">ebriiga&jcirc;on</ref> por plezuro
          a&ubreve;
          memforgeso a&ubreve; ceremonie ktp:
          <ekz>
            <tld/>i vinon;
          </ekz><ekz>
            <ind><tld/>i kiel funelo</ind>;
            <trd lng="ca">beure m&eacute;s que un clot d'arena</trd>
            <trd lng="fr">boire comme un <ind>trou</ind></trd>
            <trd lng="hu">iszik, mint a kefek&ouml;t&odblac;</trd>
            <trd lng="nl">drinken als een <ind>Zwitser</ind></trd>
            <trdgrp lng="ru">
              <trd>&c_p;&c_i;&c_t;&c_mol;
              &c_m;&c_jo;&c_r;&c_t;&c_v;&c_u;&c_ju;</trd>,
              <trd>&c_p;&c_i;&c_t;&c_mol;, &c_k;&c_a;&c_k;
              &c_s;&c_a;&c_p;&c_o;&c_zh;&c_n;&c_i;&c_k;</trd>
            </trdgrp>
          </ekz><ekz>
            li malsanas, &ccirc;ar li <ind><tld/>adi</ind>s la tutan
            semajnon;
            <trd lng="fr">picoler</trd>
          </ekz><ekz>
            <ind>post<tld/>a kapdoloro</ind><fnt>Z</fnt>;
            <trd lng="be">&c_p;&c_a;&c_h;&c_m;&c_je;&c_l;</trd>
            <trd lng="ca">ressaca</trd>
            <trd lng="de">Kater<klr tip="amb"> (Alkohol)</klr></trd>
            <trd lng="fr">gueule de bois</trd>
            <trdgrp lng="hu">
              <trd>m&aacute;snaposs&aacute;g</trd>,
              <trd>macskajaj</trd>
            </trdgrp>
            <trd lng="nl">kater<klr tip="ind"> (na drinken)</klr></trd>
            <trd lng="pl">kac</trd>
            <trd lng="ru">&c_p;&c_o;&c_h;&c_m;&c_je;&c_l;&c_mol;&c_je;</trd>
          </ekz><ekz>
            la festenestro alvokis la fian&ccirc;on kaj diris al li:
            &ccirc;iu homo prezentas unue la bonan vinon kaj kiam oni
            jam <tld/>is tiam la malpli bonan
            <fnt><bib>NT</bib>, <lok>&Joh; 2:10</lok></fnt>;
          </ekz><ekz>
            <uzo tip="stl">FRAZ</uzo>
            ne pekas <ind><tld/>ulo</ind>, pekas ebriulo
            <fnt><bib>PrV</bib></fnt>;
            <trd lng="ca">bevedor</trd>
            <trd lng="fr">buveur<klr tip="ind"> (qn qui
            boit)</klr></trd>
          </ekz><ekz>
            kia ofero, devi <tld/>i bieron por la sano!!!
            <fnt><aut>Krys Ungar</aut>,
              <vrk><url ref="&Monato;/004583.html">Vegetarismo -
              &ccirc;u
              granda
                  mispa&scirc;o a&ubreve; sa&gcirc;a
                  konduto?</url></vrk>,
              <bib>Monato</bib>,
              <lok>numero 07/1995</lok>.
            </fnt>;
          </ekz><ekz>
            en tiuj pasintaj multaj jaroj ni a&ubreve;dis pri neniu
            kri&scirc;nano,
            kiu uzas a&ubreve; vendas drogojn, fumas, <tld/>as alkoholon
            <fnt><aut>Bob Felby</aut>,
              <vrk><url ref="&Monato;/006704.html">Ne tiom
              dan&gcirc;era</url></vrk>,
              <bib>Monato</bib>,
              <lok>numero 12/1999</lok>.
            </fnt>;
          </ekz><ekz>
            mi iras al taverno kaj <tld/>as tekilon, a&ubreve;
            &gcirc;inon sen
            toniko
            <fnt><aut>Grigorij L. Arosev</aut>,
              <vrk><url ref="&Monato;/007399.html">Nerekonebla</url></vrk>,
              <bib>Monato</bib>,
              <lok>numero 05/2001</lok>.
            </fnt>.
          </ekz>
        </dif>
        <ref tip="vid" cel="fum.0i.tabakon">fumi</ref>
        <rim num="1">
          Do, se gasto diris
          <ekz>
            <ctl>mi trinkas nur akvon</ctl>,
          </ekz>
          tio normale implicas ke li ne trinkas alkoholon, teon, kafon
          ktp, sed se li diris
          <ekz>
            <ctl>mi <tld/>as nur vinon</ctl>
          </ekz>
          oni ja povas krome proponi al li teon a&ubreve; kafon &#x2010;
          sed ne
          brandon a&ubreve; bieron.
        </rim>
        <rim num="2">
          PIV2 precizigas, ke <tld/>i estas <ctl>trinki
          alkohola&jcirc;on en
            nemodera kvanto</ctl>. Tio &scirc;ajnas troa: supozeble
            &ccirc;e la
          edzi&gcirc;festo en la Kana Galilea ne temis pri
          maldeca&jcirc;oj, kaj
          la vorto &leftquot;<tld/>ejo&rightquot; ne neprigas tion.
          <aut>Sergio Pokrovskij</aut>
        </rim>
      </snc>
  <trdgrp lng="be">
        <trd>&c_p;&c_ib;&c_c;&c_mol;<klr>
        (&c_a;&c_l;&c_mol;&c_k;&c_a;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_ja;
        &c_n;&c_a;&c_p;&c_ib;&c_t;&c_k;&c_ib;)</klr></trd>,
        <trd>&c_p;'&c_ja;&c_n;&c_s;&c_t;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
  <trd lng="bg">&c_p;&c_i;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_a;&c_m;</trd>
  <trd lng="bo">&#xF60;&#xF50;&#xF74;&#xF44; &#xF56;&#xF4F;&#xF74;&#xF44;&#xF66; &#xF56;&#xF4F;&#xF74;&#xF44; &#xF60;&#xF50;&#xF74;&#xF44;&#xF66;&#xF0B;</trd>
  <trd lng="ca">beure<klr tip="amb"> (una beguda
      alcoh&ograve;lica)</klr></trd>
  <trd lng="cs">nem&iacute;rn&ecaron; p&iacute;t</trd>
  <trdgrp lng="de">
        <trd>saufen</trd>,
        <trd>zechen</trd>,
        <trd>bechern</trd>
      </trdgrp>
  <trd lng="en">drink <klr>(alcohol)</klr></trd>
  <trdgrp lng="es">
        <trd>trincar</trd>,
        <trd>beber ([mucho] alcohol)</trd>
      </trdgrp>
  <trd lng="fr">boire<klr tip="amb"> (une boisson
      alcoolis&eacute;e)</klr></trd>
  <trd lng="he">&lamed;<ind>&he;&shin;&tav;&kaf;&resh;</ind></trd>
  <trd lng="hu">iszik<klr tip="amb"> (alkoholt)</klr></trd>
  <trdgrp lng="id">
        <trd>mabuk</trd>,
        <trd>minum<klr tip="amb"> (minuman keras)</klr></trd>
      </trdgrp>
  <trd lng="nl">alcohol <ind>drinken</ind></trd>
  <trdgrp lng="pl">
        <trd>pi&cacute;<klr> (alkohol)</klr></trd>,
        <trd>chla&cacute;<klr> (pot.)</klr></trd>
      </trdgrp>
  <trdgrp lng="pt">
        <trd>beber <klr tip="amb">(bebidas
        alco&oacute;licas)</klr></trd>,
        <trd>tomar um <ind>drinque</ind></trd>
      </trdgrp>
  <trdgrp lng="ru">
        <trd>&c_p;&c_i;&c_t;&c_mol;
        <klr>(&c_s;&c_p;&c_i;&c_r;&c_t;&c_n;&c_o;&c_je;)</klr></trd>,
        <trd>&c_p;&c_mol;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
  <trdgrp lng="sk">
    <trd>ch&lcaron;asta&tcaron;</trd>,
    <trd>pi&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="tr">
        <trd>i&ccedil;mek</trd>,
        <trd>i&ccedil;ki i&ccedil;mek</trd>
      </trdgrp>
</drv>

    <drv mrk="drink.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc mrk="drink.0ajxo.KUI">
        <uzo tip="fak">KUI</uzo>
