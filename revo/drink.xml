    <drv mrk="drink.for0i">
      <kap>for<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Perdi, forigi per <tld/>ado:
          <ekz>
            en tiu &ccirc;i jaro li jam havis tempon for<tld/>i sian
            tutan
            havon kaj la havon de sia edzino
            <fnt><bib>FK</bib>, <lok>4, rakontoj, la forgesita pipo,
            rakonto de A. Edelmann, tradukis I. Kaminski</lok></fnt>.
          </ekz><ekz>
            for<tld/>i sian &ccirc;agrenon.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
