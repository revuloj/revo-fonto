<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>drink</rad>/i
    </kap>

    <drv mrk="drink.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          <ref tip="super" cel="trink.0i">Trinki</ref>
          <ref cel="ebri.0igajxo">ebriiga&jcirc;on</ref> por plezuro
          a&ubreve;
          memforgeso a&ubreve; ceremonie ktp:
          <ekz>
            <tld/>i vinon;
          </ekz><ekz>
            <ind><tld/>i kiel funelo</ind>;
            <trd lng="fr">boire comme un <ind>trou</ind></trd>
            <trd lng="hu">iszik, mint a kefek&ouml;t&odblac;</trd>
            <trd lng="nl">drinken als een <ind>Zwitser</ind></trd>
            <trdgrp lng="ru">
              <trd>&c_p;&c_i;&c_t;&c_mol;
              &c_m;&c_jo;&c_r;&c_t;&c_v;&c_u;&c_ju;</trd>,
              <trd>&c_p;&c_i;&c_t;&c_mol;, &c_k;&c_a;&c_k;
              &c_s;&c_a;&c_p;&c_o;&c_zh;&c_n;&c_i;&c_k;</trd>
            </trdgrp>
          </ekz><ekz>
            li malsanas, &ccirc;ar li <ind><tld/>adi</ind>s la tutan
            semajnon;
            <trd lng="fr">picoler</trd>
          </ekz><ekz>
            <ind>post<tld/>a kapdoloro</ind><fnt>Z</fnt>;
            <trd lng="be">&c_p;&c_a;&c_h;&c_m;&c_je;&c_l;</trd>
            <trd lng="ca">ressaca</trd>
            <trd lng="de">Kater<klr tip="amb"> (Alkohol)</klr></trd>
            <trd lng="fr">gueule de bois</trd>
            <trdgrp lng="hu">
              <trd>m&aacute;snaposs&aacute;g</trd>,
              <trd>macskajaj</trd>
            </trdgrp>
            <trd lng="nl">kater<klr tip="ind"> (na drinken)</klr></trd>
            <trd lng="pl">kac</trd>
            <trd
            lng="ru">&c_p;&c_o;&c_h;&c_m;&c_je;&c_l;&c_mol;&c_je;</trd>
          </ekz><ekz>
            la festenestro alvokis la fian&ccirc;on kaj diris al li:
            &ccirc;iu homo prezentas unue la bonan vinon kaj kiam oni
            jam <tld/>is tiam la malpli bonan
            <fnt><bib>NT</bib>, <lok>&Joh; 2:10</lok></fnt>;
          </ekz><ekz>
            <uzo tip="stl">FRAZ</uzo>
            ne pekas <ind><tld/>ulo</ind>, pekas ebriulo
            <fnt><bib>PrV</bib></fnt>;
            <trd lng="ca">bevedor</trd>
            <trd lng="fr">buveur<klr tip="ind"> (qn qui
            boit)</klr></trd>
          </ekz><ekz>
            kia ofero, devi <tld/>i bieron por la sano!!!
            <fnt><aut>Krys Ungar</aut>,
              <vrk><url ref="&Monato;/004583.html">Vegetarismo -
              &ccirc;u
              granda
                  mispa&scirc;o a&ubreve; sa&gcirc;a
                  konduto?</url></vrk>,
              <bib>Monato</bib>,
              <lok>numero 07/1995</lok>.
            </fnt>;
          </ekz><ekz>
            en tiuj pasintaj multaj jaroj ni a&ubreve;dis pri neniu
            kri&scirc;nano,
            kiu uzas a&ubreve; vendas drogojn, fumas, <tld/>as alkoholon
            <fnt><aut>Bob Felby</aut>,
              <vrk><url ref="&Monato;/006704.html">Ne tiom
              dan&gcirc;era</url></vrk>,
              <bib>Monato</bib>,
              <lok>numero 12/1999</lok>.
            </fnt>;
          </ekz><ekz>
            mi iras al taverno kaj <tld/>as tekilon, a&ubreve;
            &gcirc;inon sen
            toniko
            <fnt><aut>Grigorij L. Arosev</aut>,
              <vrk><url
              ref="&Monato;/007399.html">Nerekonebla</url></vrk>,
              <bib>Monato</bib>,
              <lok>numero 05/2001</lok>.
            </fnt>.
          </ekz>
        </dif>
        <ref tip="vid" cel="fum.0i.tabakon">fumi</ref>
        <rim num="1">
          Do, se gasto diris
          <ekz>
            <ctl>mi trinkas nur akvon</ctl>,
          </ekz>
          tio normale implicas ke li ne trinkas alkoholon, teon, kafon
          ktp, sed se li diris
          <ekz>
            <ctl>mi <tld/>as nur vinon</ctl>
          </ekz>
          oni ja povas krome proponi al li teon a&ubreve; kafon &dash;
          sed ne
          brandon a&ubreve; bieron.
        </rim>
        <rim num="2">
          PIV2 precizigas, ke <tld/>i estas <ctl>trinki
          alkohola&jcirc;on en
            nemodera kvanto</ctl>. Tio &scirc;ajnas troa: supozeble
            &ccirc;e la
          edzi&gcirc;festo en la Kana Galilea ne temis pri
          maldeca&jcirc;oj, kaj
          la vorto &leftquot;<tld/>ejo&rightquot; ne neprigas tion.
          <aut>Sergio Pokrovskij</aut>
        </rim>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_ib;&c_c;&c_mol;<klr>
        (&c_a;&c_l;&c_mol;&c_k;&c_a;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_ja;
        &c_n;&c_a;&c_p;&c_ib;&c_t;&c_k;&c_ib;)</klr></trd>,
        <trd>&c_p;'&c_ja;&c_n;&c_s;&c_t;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="bg">&c_p;&c_i;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_a;&c_m;</trd>
      <trdgrp lng="de">
        <trd>saufen</trd>,
        <trd>zechen</trd>,
        <trd>bechern</trd>
      </trdgrp>
      <trd lng="ca">beure<klr tip="amb"> (una beguda
      alcoh&ograve;lica)</klr></trd>
      <trd lng="en">drink <klr>(alcohol)</klr></trd>
      <trd lng="fr">boire<klr tip="amb"> (une boisson
      alcoolis&eacute;e)</klr></trd>
      <trd lng="he">&lamed;<ind>&he;&shin;&tav;&kaf;&resh;</ind></trd>
      <trd lng="hu">iszik<klr tip="amb"> (alkoholt)</klr></trd>
      <trd lng="nl">alcohol <ind>drinken</ind></trd>
      <trdgrp lng="pl">
        <trd>pi&cacute;<klr> (alkohol)</klr></trd>,
        <trd>chla&cacute;<klr> (pot.)</klr></trd>
      </trdgrp>
      <trdgrp lng="pt">
        <trd>beber <klr tip="amb">(bebidas
        alco&oacute;licas)</klr></trd>,
        <trd>tomar um <ind>drinque</ind></trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_i;&c_t;&c_mol;
        <klr>(&c_s;&c_p;&c_i;&c_r;&c_t;&c_n;&c_o;&c_je;)</klr></trd>,
        <trd>&c_p;&c_mol;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
      <trdgrp lng="tr">
        <trd>i&ccedil;mek</trd>,
        <trd>i&ccedil;ki i&ccedil;mek</trd>
      </trdgrp>
    </drv>

    <drv mrk="drink.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc mrk="drink.0ajxo.KUI">
        <uzo tip="fak">KUI</uzo>
        <dif>
          Ebriiga, alkoholenhava <ref tip="super"
            cel="trink.0ajxo">trinka&jcirc;o</ref>:
          <ekz>
            la delikata bonodoro de la floroj ebriigis &scirc;in
            kvaza&ubreve; <tld/>a&jcirc;o
            <fnt><bib>Marta</bib></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="bg">&c_n;&c_a;&c_p;&c_i;&c_t;&c_k;&c_a;</trd>
      <trd lng="en">beguda<klr tip="amb"> (alcoh&ograve;lica)</klr></trd>
      <trd lng="de"><klr tip="amb">(alkoholisches)
        </klr><ind>Getr&auml;nk</ind></trd>
      <trd lng="en"><klr tip="amb">(alcoholic) </klr>drink</trd>
      <trdgrp lng="fr">
        <trd>alcool<klr tip="amb"> (boisson
        alcoolis&eacute;e)</klr></trd>,
        <trd>boisson alcoolis&eacute;e</trd>
      </trdgrp>
      <trd lng="he"><ind>&mem;&shin;&qof;&he;</ind>
      &het;&resh;&yod;&fpe;</trd>
      <trd lng="nl">alcoholische <ind>drank</ind></trd>
      <trdgrp lng="pl">
        <trd>nap&oacute;j alkoholowy</trd>,
        <trd>nap&oacute;j wyskokowy</trd>,
        <trd>drink</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_v;&c_y;&c_p;&c_i;&c_v;&c_k;&c_a;</trd>,
        <trd>&c_a;&c_l;&c_k;&c_o;&c_g;&c_o;&c_l;&c_mol;</trd>,
        <trd>&c_a;&c_l;&c_k;&c_o;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;
        &c_n;&c_a;&c_p;&c_i;&c_t;&c_o;&c_k;</trd>
      </trdgrp>
      <trd lng="tr">alkoll&uuml; i&ccedil;ecek</trd>
    </drv>

    <drv mrk="drink.0ejo">
      <kap><ofc>*</ofc><tld/>ejo</kap>
      <snc>
        <dif>
          Trinkejo de alkohola&jcirc;oj:
          <ekz>
            vi volas forlasi min, por foriri en la <tld/>ejon
            <fnt><bib>FK</bib>, <lok>4, rakontoj, la forgesita pipo,
            rakonto
                de A. Edelmann, tradukis I. Kaminski</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_sh;&c_y;&c_n;&c_o;&c_k;</trd>
      <trdgrp lng="ca">
        <trd>pub</trd>,
        <trd>taverna</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Kneipe</trd>,
        <trd>Trinkhalle</trd>,
        <trd>Zechbude</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>bar</trd>,
        <trd>pub</trd>,
        <trd>saloon</trd>,
        <trd>tavern</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>bar</trd>,
        <trd>caf&eacute;<klr tip="ind"> (bar)</klr></trd>,
        <trd>d&eacute;bit de boissons</trd>
      </trdgrp>
      <trdgrp lng="he">
        <trd>&bet;&alef;&resh;</trd>,
        <trd>&pe;&alef;&bet;</trd>,
        <trd>&mem;&samekh;&bet;&alef;&he;</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>kocsma</trd>,
        <trd>italbolt</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>bar<klr tip="ind"> (buffet, caf&eacute;)</klr></trd>,
        <trd>caf&eacute;</trd>,
        <trd>buffet</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>bar</trd>,
        <trd>knajpa</trd>,
        <trd>tawerna<klr> (rzad.)</klr></trd>,
        <trd>szynk<klr> (przest.)</klr></trd>,
        <trd>karczma<klr> (przest.)</klr></trd>,
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_i;&c_t;&c_je;&c_j;&c_n;&c_o;&c_je;
        &c_z;&c_a;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_k;&c_a;&c_b;&c_a;&c_k;</trd>
      </trdgrp>
      <trdgrp lng="tr">
        <trd>meyhane</trd>,
        <trd>bar</trd>
      </trdgrp>
    </drv>

    <drv mrk="drink.0emulo">
      <kap><tld/>emulo</kap>
      <snc>
        <dif>
          Homo, kiu volonte <tld/>as:
          <ekz>
            <tld/>emulo kaj man&gcirc;egemulo malri&ccirc;i&gcirc;as
            <fnt><bib>MT</bib>, <lok>&Sen; 23:21</lok></fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;'&c_ja;&c_n;&c_ib;&c_c;&c_a;</trd>,
        <trd>&c_a;&c_l;&c_mol;&c_k;&c_a;&c_g;&c_o;&c_l;&c_ib;&c_k;</trd>,
        <trd>&c_p;'&c_ja;&c_n;&c_mol;</trd>,
        <trd>&c_p;'&c_ja;&c_n;&c_ch;&c_u;&c_g;&c_a;</trd>,
        <trd>&c_p;'&c_ja;&c_n;&c_ju;&c_g;&c_a;</trd>,
        <trd>&c_p;&c_r;&c_a;&c_p;&c_o;&c_j;&c_c;&c_a;</trd>,
        <trd>&c_v;&c_y;&c_p;&c_ib;&c_v;&c_a;&c_h;&c_a;</trd>,
        <trd>&c_zh;&c_l;&c_u;&c_k;&c_t;&c_a;</trd>
      </trdgrp>
      <trdgrp lng="ca">
        <trd>bevedor</trd>,
        <trd>barraler</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>S&auml;ufer</trd>,
        <trd>Trinker</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>drunk</trd>,
        <trd>drunkard</trd>,
        <trd>alcoholic</trd>
      </trdgrp>
      <trd lng="fr">buveur<klr tip="ind"> (qn qui s'adonne &agrave; la
          boisson)</klr></trd>
      <trdgrp lng="he">
        <trd>&shin;&yod;&kaf;&vav;&resh;</trd>,
        <trd>&shin;&tav;&yod;&yod;&fnun;</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>r&eacute;szeges<klr tip="ind"> (ember)</klr></trd>,
        <trd>isz&aacute;kos</trd>
      </trdgrp>
      <trd lng="nl">alcoholicus</trd>
      <trdgrp lng="pl">
        <trd>alkoholik</trd>,
        <trd>pijak</trd>,
        <trd>op&oacute;j</trd>,
        <trd>ochlaptus</trd>,
        <trd>opil&sacute;&nacute;<klr> (przest.)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_p;&c_mol;&c_ja;&c_n;&c_i;&c_c;&c_a;</trd>
    </drv>

    <drv mrk="drink.for0i">
      <kap>for<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Perdi, forigi per <tld/>ado:
          <ekz>
            en tiu &ccirc;i jaro li jam havis tempon for<tld/>i sian
            tutan
            havon kaj la havon de sia edzino
            <fnt><bib>FK</bib>, <lok>4, rakontoj, la forgesita pipo,
            rakonto de A. Edelmann, tradukis I. Kaminski</lok></fnt>.
          </ekz><ekz>
            for<tld/>i sian &ccirc;agrenon.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
      <trdgrp lng="de">
        <trd>ers&auml;ufen</trd>,
        <trd>versaufen</trd>,
        <trd>verzechen</trd>,
        <trd>vertrinken</trd>
      </trdgrp>
      <trd lng="en"><ind>drink</ind> away</trd>
      <trdgrp lng="fr">
        <trd>boire<klr tip="amb"> (d&eacute;penser en
        beuverie)</klr></trd>,
        <trd>noyer<klr tip="amb"> (dans l'alcool)</klr></trd>
      </trdgrp>
      <trd lng="hu">elissza<klr tip="amb"> (pl. a
      p&eacute;nz&eacute;t)</klr></trd>
      <trdgrp lng="nl">
        <trd>verbrassen</trd>,
        <trd>verdrinken<klr tip="ind"> (zijn zorgen bv.)</klr></trd>
      </trdgrp>
      <trd lng="pl">przepi&cacute;</trd>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
    </drv>
  </art>
