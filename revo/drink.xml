<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>drink</rad>/i
</kap>
<drv mrk="drink.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
   <dif>
     Plezurcele trinki ebriiga&jcirc;on:
     <ekz><tld/>i vinon;</ekz>
     <ekz><tld/>i kiel funelo;</ekz>
     <ekz>li malsanas &ccirc;ar li <tld/>adis la tutan semajnon;</ekz>
     <ekz><tld/>emulo malri&ccirc;i&gcirc;as;</ekz>
     <ekz>post<tld/>a kapdoloro<fnt>Z</fnt>.</ekz> 
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_t;&c_mol; <klr>(&c_s;&c_p;&c_i;&c_r;&c_t;&c_n;&c_o;&c_je;)</klr></trd>,
    <trd>&c_p;&c_mol;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="drink.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Perdi per <tld/>ado: <ekz>for<tld/>i sian monon.</ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_p;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="drink.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Trinkejo de alkohola&jcirc;oj.  
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_t;&c_je;&c_j;&c_n;&c_o;&c_je; &c_z;&c_a;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_b;&c_a;&c_k;</trd>
  </trdgrp>
</drv>
</art>
