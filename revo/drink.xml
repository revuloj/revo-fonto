        <trd>nap&oacute;j wyskokowy</trd>,
        <trd>drink</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_v;&c_y;&c_p;&c_i;&c_v;&c_k;&c_a;</trd>,
        <trd>&c_a;&c_l;&c_k;&c_o;&c_g;&c_o;&c_l;&c_mol;</trd>,
        <trd>&c_a;&c_l;&c_k;&c_o;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;
        &c_n;&c_a;&c_p;&c_i;&c_t;&c_o;&c_k;</trd>
      </trdgrp>
      <trd lng="tr">alkoll&uuml; i&ccedil;ecek</trd>
