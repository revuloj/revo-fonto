<drv mrk="magaze.0ego">
  <kap><tld/>ego</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Tre granda <ref tip="super" cel="vend.0ejo">vendejo</ref>,
      kie oni vendas &ccirc;iuspecajn komerca&jcirc;ojn:
      <ekz>
        la sinjorino ne delasis la trotuaron, kaj sin direktis al la <tld/>ego
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        strato <nom>Marienhilfer</nom> <klr>[estas]</klr> tre &scirc;atatan de a&ccirc;etemuloj pro la multaj
        <tld/>egoj
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/011140.php">Vieno pliverdi&gcirc;as</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="bazar.super0o">superbazaro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="bg">&c_m;&c_o;&c_l;</trd>
  <trd lng="br">gourstal</trd>
  <trd lng="ca">grans magatzems</trd>
  <trdgrp lng="de">
    <trd>Warenhaus</trd>,
