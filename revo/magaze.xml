<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>magazen</rad>/o
</kap>
<drv mrk="magaze.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="magaze.0o.stokejo">
    <uzo tip="fak">EKON</uzo>
    <ref tip="dif" cel="stok.0ejo">stokejo</ref>
    <trdgrp lng="de">
      <trd>Lager</trd>,
      <trd>Lagerhaus</trd>,
      <trd>Speicher</trd>,
      <trd>Magazin</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>warehouse</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>magasin<klr> (r&eacute;serve)</klr></trd>,
      <trd>r&eacute;serve<klr> (lieu)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>rakt&aacute;r</trd>,
      <trd>t&aacute;rh&aacute;z</trd>
    </trdgrp>
  </snc>
  <snc mrk="magaze.0o.butiko">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Luksa butiko:
      <ekz>
        <tld/>o, en kiu oni vendas cigarojn, estas
        cigarejo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        se kliento en <tld/>o pagas per nacia valuto, &ccirc;u la magazeno devos
        redoni nacian valuton a&ubreve; e&ubreve;rojn<fnt><aut>Marc Vanden Bempt</aut>:
        <vrk>Al nova komuna epoko</vrk>, <bib>Monato</bib><lok>jaro 1997a,
        numero 3a, p. 22a</lok></fnt>?
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gesch&auml;ft</trd>,
      <trd>Laden</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>department <ind>store</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>magasin<klr> (lieu de vente)</klr></trd>
    </trdgrp>
    <trd lng="hu">&uuml;zlet</trd>
    <trd lng="ru">&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;</trd>
  </snc>
  <snc mrk="magaze.0o.magazenego">
    <uzo tip="fak">EKON</uzo>
    <ref tip="dif" cel="magaze.0ego">magazenego</ref>
    <trdgrp lng="de">
      <trd>Warenhaus</trd>,
      <trd>Kaufhaus</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>department <ind>store</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>grand <ind>magasin</ind></trd>
    </trdgrp>
    <trd lng="hu">&aacute;ruh&aacute;z</trd>
    <trd lng="nl">magazijn</trd>
    <trd lng="ru">&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;</trd>
  </snc>
</drv>

<drv mrk="magaze.0ego">
  <kap><tld/>ego</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Tre granda moderna <ref tip="super" cel="vend.0ejo">vendejo</ref>,
      kie oni vendas multspecajn komerca&jcirc;ojn
    </dif>
    <refgrp tip="sin">
      <ref cel="magaze.0o.magazenego">magazeno <sncref
      /></ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Warenhaus</trd>,
    <trd>Kaufhaus</trd>
  </trdgrp>
    <trdgrp lng="en">
      <trd>department <ind>store</ind></trd>
    </trdgrp>
  <trdgrp lng="fr">
    <trd>grand <ind>magasin</ind></trd>
  </trdgrp>
  <trd lng="hu">&aacute;ruh&aacute;z</trd>
  <trd lng="nl">magazijn</trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;</trd>
</drv>
</art>
