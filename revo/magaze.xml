<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>magazen</rad>/o
</kap>
<drv mrk="magaze.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="magaze.0o.stokejo">
    <uzo tip="fak">EKON</uzo>
    <ref tip="dif" cel="stok.0ejo">stokejo</ref>
    <trd lng="ca">magatzem</trd>
    <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
    <trd lng="br">ti-dastum</trd>
    <trdgrp lng="de">
      <trd>Lager</trd>,
      <trd>Lagerhaus</trd>,
      <trd>Speicher</trd>,
      <trd>Magazin</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>warehouse</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_A;&a_n;&a_b;&a_A;&a_r;</trd>,
      <trd>&a_m;&a_kh;&a_z;&a_n;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>magasin<klr> (r&eacute;serve)</klr></trd>,
      <trd>r&eacute;serve<klr> (lieu)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>rakt&aacute;r</trd>,
      <trd>t&aacute;rh&aacute;z</trd>
    </trdgrp>
    <trd lng="nl">magazijn</trd>
  <trdgrp lng="pt">
    <trd>dep&oacute;sito</trd>,
    <trd>armaz&eacute;m</trd>
  </trdgrp>
    <trd lng="ru">&c_s;&c_k;&c_l;&c_a;&c_d;</trd>
  </snc>

  <snc mrk="magaze.0o.butiko">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Luksa butiko:
      <ekz>
        <tld/>o, en kiu oni vendas cigarojn, estas
        cigarejo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        se kliento en <tld/>o pagas per nacia valuto, &ccirc;u la magazeno
        devos
        redoni nacian valuton a&ubreve; e&ubreve;rojn<fnt><aut>Marc Vanden
        Bempt</aut>:
        <vrk>Al nova komuna epoko</vrk>, <bib>Monato</bib><lok>jaro
        1997a,
        numero 3a, p. 22a</lok></fnt>?
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_r;&c_a;&c_m;&c_a;</trd>
    <trd lng="br">stal</trd>
    <trdgrp lng="ca">
      <trd>botiga</trd>,
      <trd>establiment</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Gesch&auml;ft</trd>,
      <trd>Laden</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>department <ind>store</ind></trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_m;&a_gh;&a_A;&a_z;&a_h;</trd>,
      <trd>&a_f;&a_r;&a_w;&a_sh;&f_g;&a_A;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>magasin<klr> (lieu de vente)</klr></trd>
    </trdgrp>
    <trd lng="hu">&uuml;zlet</trd>
    <trd lng="nl">winkel</trd>
  <trdgrp lng="pt">
    <trd>butique</trd>,
    <trd>boutique</trd>
  </trdgrp>
    <trd lng="ru">&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;</trd>
  </snc>

  <snc mrk="magaze.0o.magazenego">
    <uzo tip="fak">EKON</uzo>
    <ref tip="dif" cel="magaze.0ego">magazenego</ref>
    <trd lng="be">&c_k;&c_r;&c_a;&c_m;&c_a;</trd>
    <trd lng="br">gourstal</trd>
    <trd lng="ca">gran magatzem</trd>
    <trdgrp lng="de">
      <trd>Warenhaus</trd>,
      <trd>Kaufhaus</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>department <ind>store</ind></trd>
    </trdgrp>
    <trd lng="fa">&a_f;&a_r;&a_w;&a_sh;&f_g;&a_A;&a_h; &a_b;&a_z;&a_r;&f_g;</trd>
    <trdgrp lng="fr">
      <trd>grand <ind>magasin</ind></trd>
    </trdgrp>
    <trd lng="hu">&aacute;ruh&aacute;z</trd>
    <trd lng="nl">koopcentrum</trd>
    <trdgrp lng="ru">
      <trd>&c_u;&c_n;&c_i;&c_v;&c_je;&c_r;&c_m;&c_a;&c_g;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_je;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="magaze.0ego">
  <kap><tld/>ego</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Tre granda moderna <ref tip="super" cel="vend.0ejo">vendejo</ref>,
      kie oni vendas multspecajn komerca&jcirc;ojn
    </dif>
    <refgrp tip="sin">
      <ref cel="magaze.0o.magazenego">magazeno <sncref
      /></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_m;&c_a;</trd>
  <trd lng="br">gourstal</trd>
  <trd lng="ca">gran magatzem</trd>
  <trdgrp lng="de">
    <trd>Warenhaus</trd>,
    <trd>Kaufhaus</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>department <ind>store</ind></trd>
  </trdgrp>
  <trd lng="fa">&a_f;&a_r;&a_w;&a_sh;&f_g;&a_A;&a_h; &a_b;&a_z;&a_r;&f_g;</trd>
  <trdgrp lng="fr">
    <trd>grand <ind>magasin</ind></trd>
  </trdgrp>
  <trd lng="hu">&aacute;ruh&aacute;z</trd>
  <trd lng="nl">koopcentrum</trd>
  <trdgrp lng="pt">
    <trd>magazine</trd>,
    <trd>loja de departamentos</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_n;&c_i;&c_v;&c_je;&c_r;&c_m;&c_a;&c_g;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_je;&c_r;&c_m;&c_a;&c_r;&c_k;&c_je;&c_t;</trd>
  </trdgrp>
</drv>
</art>
