<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: euxhxaristi.xml,v 1.8 2020/09/09 18:10:35 revo Exp $">
<kap>
  <rad>e&ubreve;&hcirc;aristi</rad>/o,
  <var><kap><rad var="k">e&ubreve;karisti</rad>/o <fnt><bib>Fab2</bib></fnt></kap></var>
</kap>
<drv mrk="euxhxaristi.0o">
  <kap><tld/>o,
    <var><kap><tld var="k"/>o</kap></var>
  </kap>
  <snc>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Kristana <ref tip="super" cel="sakram.0o.kri">sakramento</ref>,
      rememoriganta la <ref cel="mangx.sankta_0o">Sanktan
      Man&gcirc;on</ref>, dum kiu okazas konsekrado de pano kaj vino:
      <ekz>
        la sekvantan diman&ccirc;on estis la unua <tld var="k"/>o de la konfirmitoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        plurfoje mi jam kuncelebris anka&ubreve; la <tld/>on okaze de intereklezia nupto
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006823.html"
              >Opinio: Tro senkosta pentemo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komuni.0o">komunio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_w;&c_h;&c_a;&c_r;&c_y;&c_s;&c_t;&c_y;&c_ja;</trd>
  <trd lng="ca">eucaristia</trd>
  <trd lng="de">Eucharistie</trd>
  <trd lng="en">eucharist</trd>
  <trd lng="fa">&a_ayn;&a_sh;&a_A;&f_y; &a_r;&a_b;&a_A;&a_n;&f_y;</trd>
  <trd lng="fr">eucharistie</trd>
  <trdgrp lng="hu">
    <trd>olt&aacute;riszents&eacute;g</trd>,
    <trd>eucharisztia</trd>
  </trdgrp>
  <trd lng="nl">eucharistie</trd>
  <trd lng="pl">eucharystia</trd>
  <trd lng="pt">eucaristia</trd>
  <trd lng="ru">&c_je;&c_v;&c_h;&c_a;&c_r;&c_i;&c_s;&c_t;&c_i;&c_ja;</trd>
  <trd lng="sv">nattvard</trd>
</drv>
</art>

