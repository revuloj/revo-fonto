<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>e&ubreve;&hcirc;aristi</rad>/o
</kap>
<drv mrk="euxhxaristi.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Kristana <ref tip="super" cel="sakram.0o.kri">sakramento</ref>,
      rememoriganta la <ref cel="mangx.sankta-0o">Sanktan
      Manĝon</ref>, dum kiu okazas konsekrado de pano kaj vino.
    </dif>
    <refgrp tip="vid">
      <ref cel="komuni.0o">komunio</ref>;
    </refgrp>
    <refgrp tip="sin">
      <ref cel="euxkar.0o">e&ubreve;karistio</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_w;&c_h;&c_a;&c_r;&c_y;&c_s;&c_t;&c_y;&c_ja;</trd>
  <trd lng="de">Eucharistie</trd>
  <trd lng="en">eucharist</trd>
  <trd lng="fr">eucharistie</trd>
  <trdgrp lng="hu">
    <trd>oltáriszentség</trd>,
    <trd>eucharisztia</trd>
  </trdgrp>
  <trd lng="pl">eucharystia</trd>
  <trd lng="pt">eucaristia</trd>
  <trd lng="ru">&c_je;&c_v;&c_h;&c_a;&c_r;&c_i;&c_s;&c_t;&c_i;&c_ja;</trd>
  <trd lng="sv">nattvard</trd>
</drv>
</art>

