  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Cochlearia</trd>),
      uzita kiel stimula, kontra&ubreve;skorbuta medikamento.
    </dif>
  </snc>
  <trd lng="de">L&ouml;ffelkraut</trd>
  <trdgrp lng="nl">
    <trd>lepelblad</trd>,
    <trd>lepelkruid</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_o;&c_zh;&c_je;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
