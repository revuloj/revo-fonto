<vortaro>
<art mrk="$Id$">
<kap>
<rad>kokleari</rad>/o
</kap>
<drv mrk="koklea.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Cochlearia</trd>),
      uzita kiel stimula, kontra&ubreve;skorbuta medikamento.
    </dif>
  </snc>
  <trd lng="de">L&ouml;ffelkraut</trd>
  <trd lng="fr">cochl&eacute;aria</trd>
  <trdgrp lng="hu">
    <trd>kal&aacute;nf&udblac;</trd>,
    <trd>kan&aacute;lf&udblac;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lepelblad</trd>,
    <trd>lepelkruid</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_o;&c_zh;&c_je;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
