<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: koklea.xml,v 1.19 2023/09/01 15:46:40 revo Exp $">
<kap>
<rad>kokleari</rad>/o
</kap>
<drv mrk="koklea.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Cochlearia</trd>),
      <ref tip="lst" cel="herb.0o.BOT" lst="voko:brasikoparencoj">herbo</ref>,
      kies unu specio <trd lng="la">Cochlearia officinalis</trd>
      kun renformaj bazaj folioj kaj korformaj tigofolioj
      en E&ubreve;ropo estis uzata kontra&ubreve; skorbuto kaj tiucele salite
      kunportita dum marvoja&gcirc;oj:
      <ekz>
        <tld/>o, dujara herbo, kuireja uzo: aldona&jcirc;oj al salatoj kaj
        buterpanoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=331514">Spicherbo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/ab/Skj%C3%B8rbuksurt_(Cochlearia_officinalis)_01.jpg/428px-Skj%C3%B8rbuksurt_(Cochlearia_officinalis)_01.jpg" prm="CC BY-SA 4.0"><tld/>o oficina (Cochlearia officinalis), Ytre Nordfjordo
        <fnt>
          <aut>Atle R&aring;sberg, propra verko</aut>
          <lok><url ref="&WCW;/File:Skj%C3%B8rbuksurt_(Cochlearia_officinalis)_01.jpg">&WCW;/File:Skj&oslash;rbuksurt_(Cochlearia_officinalis)_01.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="cs">l&zcaron;i&ccaron;n&iacute;k</trd>
  <trd lng="de">L&ouml;ffelkraut</trd>
  <trd lng="fr">cochl&eacute;aria</trd>
  <trdgrp lng="hu">
    <trd>kal&aacute;nf&udblac;</trd>,
    <trd>kan&aacute;lf&udblac;</trd>
  </trdgrp>
  <trd lng="ja">&#x30C8;&#x30E2;&#x30B7;&#x30EA;&#x30BD;&#x30A6;</trd>
  <trdgrp lng="nl">
    <trd>lepelblad</trd>,
    <trd>lepelkruid</trd>
  </trdgrp>
  <trd lng="pl">warzucha</trd>     
  <trdgrp lng="ro">
    <trd>lingurea</trd>,
    <trd>scorbut-iarb&abreve;</trd>
  </trdgrp>                                                             
                                                                        
                                                                        
                                                                        
                                                                        
                                                                        
                                                        
  <trd lng="ru">&c_l;&c_o;&c_zh;&c_je;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">ly&zcaron;i&ccaron;n&iacute;k</trd>
  <trd lng="uk">&#x43B;&#x43E;&#x436;&#x435;&#x447;&#x43D;&#x438;&#x446;&#x44F;</trd>
</drv>
</art>
