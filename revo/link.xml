<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: link.xml,v 1.35 2022/10/03 22:38:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>link</rad>/o
</kap>
<drv mrk="link.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="link.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="best.rabo0o" lst="voko:rabobestoj">Rabobesto</ref>,
      kun <ref tip="prt" cel="tuf.har0o">hartufoj</ref> &ccirc;e la 
      oreloj kaj nigra vostopinto, vivante en la norda duonsfero;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> (<trd
      lng="la">Lynx</trd>)
      el familio <ref tip="malprt" cel="felise.0oj">felisedoj</ref> kun kvar vivantaj specioj:
      <ekz>
        la etuloj rigardis <tld/>on kaj vulpon kiel siajn gardhundojn
        <fnt>
          <aut>Selma Lagerl&ouml;f, trad. Stellan Engholm</aut>
          <vrk>G&ouml;sta Berling</vrk>
          <lok>&ccirc;apitro 35a</lok>
        </fnt>;
      </ekz>
      <ekz>
        nenio dan&gcirc;era estis videbla, neniu urso, lupo, <tld/>o a&ubreve; alia
        malamiko
        <fnt>
          <aut>Astrid Lindgren, trad. Runo Stridell</aut>
          <vrk>Ronjo, rabista filino</vrk>
          <lok>&ccirc;apitro 8a</lok>
        </fnt>;
      </ekz>
      <ekz>
        por sekvi ilian migradon oni markas la <tld/>ojn anta&ubreve; ilia eliro al sia natura hejmo per
        specialaj dissendiloj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2012/010875.php">Pereon de specioj malhelpas homoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/0/0e/Lynx_lynx_-_03.jpg/576px-Lynx_lynx_-_03.jpg" prm="CC BY-SA 3.0"><tld/>o
  (Lynx lynx), Madrida Besto&gcirc;ardeno, Hispanujo
       <fnt>
         <aut>Carlos Delgado, propra verko</aut>
         <lok><url ref="&WCW;/File:Lynx_lynx_-_03.jpg">&WCW;/File:Lynx_lynx_-_03.jpg</url></lok>
       </fnt>
  </bld>
  <trd lng="be">&c_r;&c_y;&c_s;&c_mol;</trd>
  <trd lng="bg">&c_r;&c_i;&c_s;</trd>
  <trd lng="cs">rys</trd>
  <trd lng="de">Luchs</trd>
  <trd lng="el">&lambda;&ypsilon_ton;&gamma;&xi;</trd>
  <trd lng="en">lynx</trd>
  <trd lng="es">lince</trd>
  <trd lng="fa">&a_s;&f_y;&a_A;&a_h;&f_g;&a_w;&a_sh;</trd>
  <trd lng="fr">lynx</trd>
  <trd lng="hu">hi&uacute;z</trd>
  <trdgrp lng="nl">
    <trd>lynx</trd>,
    <trd>los<klr tip="ind"> (lynx)</klr></trd>
  </trdgrp>
  <trd lng="pl">ry&sacute;</trd>
  <trd lng="pt">lince</trd>
  <trd lng="ro">r&acirc;s  <klr>(animal)</klr></trd>
  <trd lng="ru">&c_r;&c_y;&c_s;&c_mol;</trd>
  <trd lng="sk">rys</trd>
</drv>

<drv mrk="link.L0o">
  <kap><tld lit="L"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt></kap>
  <mlg>Lyn</mlg>
  <snc>
  <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> (<trd
      lng="la">Lynx</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_R;&c_y;&c_s;&c_mol;</trd>
  <trd lng="bg">&c_R;&c_i;&c_s;<klr tip="amb"> (&c_s;&c_malmol;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
  <trd lng="cs">rys</trd>
  <trd lng="de">Luchs</trd>
  <trd lng="en">Lynx</trd>
  <trd lng="es">Lince</trd>
  <trd lng="fa">&a_s;&f_y;&a_A;&a_h;&f_g;&a_w;&a_sh;</trd>
  <trd lng="fr">Lynx<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="nl">Lynx<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trd lng="pl">Ry&sacute;  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Linxul  <klr>(constela&#539;ie)</klr></trd>
  <trd lng="sk">rys</trd>
  <trd lng="uk">&c_r;&c_i;&c_s;&#x44C;</trd>
</drv>

</art>
