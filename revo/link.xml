<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>link</rad>/o
</kap>
<drv mrk="link.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="link.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="best.rabo0o"
      lst="voko:rabobestoj">rabobesto</ref>,
      kun <ref tip="prt" cel="tuf.har0o">hartufoj</ref> &ccirc;e la 
      oreloj kaj nigra vostopinto, vivante en la norda duonsfero;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref> (<trd lng="la">Lynx</trd>)
      el familio <ref tip="malprt" 
      cel="felise.0oj">felisedoj</ref> kun kvar vivantaj specioj:
      <ekz>La etuloj rigardis <tld/>on kaj vulpon kiel siajn gardhundojn
        <fnt>
          <aut>Selma Lagerl&ouml;f, trad. Stellan Engholm</aut>
          <vrk>G&ouml;sta Berling</vrk>
          <lok>&ccirc;apitro 35a</lok>
        </fnt>
      </ekz>;
      <ekz>
        nenio dan&gcirc;era estis videbla, neniu urso, lupo, <tld/>o a&ubreve; alia
        malamiko
        <fnt>
          <aut>Astrid Lindgren, trad. Runo Stridell</aut>
          <vrk>Ronjo, rabista filino</vrk>
          <lok>&ccirc;apitro 8a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/link.jpg">
    <tld/>o en besta &gcirc;ardeno
  </bld>
  <trd lng="be">&c_r;&c_y;&c_s;&c_mol;</trd>
  <trd lng="bg">&c_r;&c_i;&c_s;</trd>
  <trd lng="cs">rys</trd>
  <trd lng="de">Luchs</trd>
  <trd lng="el">&lambda;&ypsilon_ton;&gamma;&xi;</trd>
  <trd lng="en">lynx</trd>
  <trd lng="es">lince</trd>
  <trd lng="fa">&a_s;&f_y;&a_A;&a_h;&f_g;&a_w;&a_sh;</trd>
  <trd lng="fr">lynx</trd>
  <trd lng="hu">hi&uacute;z</trd>
  <trdgrp lng="nl">
    <trd>lynx</trd>,
    <trd>los<klr tip="ind"> (lynx)</klr></trd>
  </trdgrp>
  <trd lng="pl">ry&sacute;</trd>
  <trd lng="pt">lince</trd>
  <trd lng="ru">&c_r;&c_y;&c_s;&c_mol;</trd>
  <trd lng="sk">rys</trd>
</drv>

<drv mrk="link.L0o">
  <kap><tld lit="L"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt></kap>
  <mlg>Lyn</mlg>
  <snc>
  <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera <ref tip="lst" 
cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> (<trd
lng="la">Lynx</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_R;&c_i;&c_s;<klr tip="amb"> (&c_s;&c_malmol;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
  <trd lng="cs">rys</trd>
  <trd lng="de">Luchs</trd>
  <trd lng="en">Lynx</trd>
  <trd lng="es">Lince</trd>
  <trd lng="fa">&a_s;&f_y;&a_A;&a_h;&f_g;&a_w;&a_sh;</trd>
  <trd lng="fr">Lynx<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="nl">Lynx<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trd lng="pl">Ry&sacute;</trd>
  <trd lng="sk">rys</trd>
</drv>

</art>
