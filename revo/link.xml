<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>link</rad>/o
</kap>
<drv mrk="link.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="felise.0oj">felisedoj</ref>
      (<trd lng="la">Lynx</trd>),
      sova&gcirc;a karnoman&gcirc;anta mamulo, kun hartufoj &ccirc;e  la oreloj.
    </dif>
  </snc>
  <bld lok="../bld/link.jpg"><tld/>o en besta &gcirc;ardeno</bld>
  <trd lng="de">Luchs</trd>
  <trd lng="en">lynx</trd>
  <trd lng="es">lince</trd>
  <trd lng="nl">lynx</trd>
  <trd lng="ru">&c_r;&c_y;&c_s;&c_mol;</trd>
</drv>
</art>
