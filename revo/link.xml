<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>link</rad>/o
</kap>
<drv mrk="link.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="link.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="felise.0oj">felisedoj</ref>
      (<trd lng="la">Lynx</trd>) el Norda Azio kaj Norda Ameriko,
      sova&gcirc;a karnoman&gcirc;anta mamulo, kun hartufoj &ccirc;e la oreloj.
    </dif>
  </snc>
  <bld lok="../bld/link.jpg">
    <tld/>o en besta &gcirc;ardeno
  </bld>
  <trd lng="be">&c_r;&c_y;&c_s;&c_mol;</trd>
  <trd lng="cs">rys</trd>
  <trd lng="de">Luchs</trd>
  <trd lng="el">&lambda;&ypsilon_ton;&gamma;&xi;</trd>
  <trd lng="en">lynx</trd>
  <trd lng="es">lince</trd>
  <trd lng="fr">lynx</trd>
  <trd lng="hu">hi&uacute;z</trd>
  <trd lng="nl">lynx</trd>
  <trd lng="pt">lince</trd>
  <trd lng="ru">&c_r;&c_y;&c_s;&c_mol;</trd>
</drv>
</art>
