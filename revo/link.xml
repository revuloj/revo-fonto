<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>link</rad>/o
</kap>
<drv mrk="link.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="link.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro (<trd lng="la">Lynx</trd>) el familio <ref tip="super" 
      cel="felise.0oj">felisedoj</ref> el Norda Azio kaj Norda Ameriko, 
      sova&gcirc;a karnoman&gcirc;anta mamulo, kun hartufoj &ccirc;e la oreloj.
    </dif>
  </snc>
  <bld lok="../bld/link.jpg">
    <tld/>o en besta &gcirc;ardeno
  </bld>
  <trd lng="be">&c_r;&c_y;&c_s;&c_mol;</trd>
  <trd lng="bg">&c_r;&c_i;&c_s;</trd>
  <trd lng="cs">rys</trd>
  <trd lng="de">Luchs</trd>
  <trd lng="el">&lambda;&ypsilon_ton;&gamma;&xi;</trd>
  <trd lng="en">lynx</trd>
  <trd lng="es">lince</trd>
  <trd lng="fr">lynx</trd>
  <trd lng="hu">hi&uacute;z</trd>
  <trdgrp lng="nl">
    <trd>lynx</trd>,
    <trd>los<klr tip="ind"> (lynx)</klr></trd>
  </trdgrp>
  <trd lng="pt">lince</trd>
  <trd lng="ru">&c_r;&c_y;&c_s;&c_mol;</trd>
</drv>

<drv mrk="link.L0o">
  <kap><tld lit="L"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <mlg>Lyn</mlg>
  <snc>
    <dif>
      Nord-hemisfera <ref tip="super" 
cel="stel.0figuro">stelfiguro</ref> (<trd lng="la">Lynx</trd>).
    </dif>
  </snc>
  <trd lng="bg">&c_R;&c_i;&c_s;<klr tip="amb"> (&c_s;&c_malmol;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;)</klr></trd>
  <trd lng="de">Luchs</trd>
  <trd lng="en">Lynx</trd>
  <trd lng="es">Lince</trd>
  <trd lng="nl">Lynx<klr tip="ind"> (sterrenbeeld)</klr></trd>
</drv>

</art>
