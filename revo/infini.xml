<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>infinit</rad>/o
    </kap>
    <drv mrk="infini.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">MAT</uzo>
      <snc>
        <dif>
          Senlima grando, senlima malproksimo. 
        </dif>
      </snc>
    </drv>
    <drv mrk="infini.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Senlime granda, senlime malproksima  (= malfinita). 
        </dif>
      </snc>
    </drv>
    <drv mrk="infini.0igi">
      <kap><tld/>igi</kap>
      <snc>
        <dif>
          Fari senlime granda, malproksima. 
        </dif>
      </snc>
    </drv>
    <drv mrk="infini.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          Fari&gcirc;i senlime granda, malproksima. 
        </dif>
        <refgrp tip="vid">
          <ref cel="infini1.0o">infinitezimo</ref>.
        </refgrp>
      </snc>
    </drv>
    <rim>
      Almena&ubreve; en matematiko &ccirc;i tiu vortofamilio arkaiki&gcirc;is.  Nun oni
      preferas diri <ref tip="sin" cel="fini.mal0o">malfinio</ref>.
    </rim>
  </art>

<!--
$Log$
-->
</vortaro>