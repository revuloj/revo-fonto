<drv mrk="haven.fisx0o">
  <kap>fi&scirc;<tld/>o</kap>
  <snc mrk="haven.fisx0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <tld lit="H"/>o a&ubreve; parto de haveno kun instala&jcirc;oj por fi&scirc;kaptaj
      &scirc;ipoj:
      <ekz>
	la fi&scirc;<tld/>oj estas multaj, precipe sude de Bretonio
	<fnt><aut>J. Ducloyer</aut>,
<vrk><url ref="&Monato;/2000/006755.html">Bretonio</url></vrk>,
	<bib>Monato</bib>,
<lok>2000/02, p. 18</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">porzh pesketa</trd>
  <trd lng="de">Fischereihafen</trd>
  <trdgrp lng="fr">
    <trd>port de p&ecirc;che</trd>
  </trdgrp>
  <trd lng="hu">hal&aacute;szkik&ouml;t&odblac;</trd>
  <trd lng="nl">vissershaven</trd>
  <trd lng="pt">porto pesqueiro</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_r;&c_t; &c_r;&c_y;&c_b;&c_o;&c_l;&c_o;&c_v;&c_n;&c_o;&c_g;&c_o; &c_f;&c_l;&c_o;&c_t;&c_a;</trd>
    <trd>&c_r;&c_y;&c_b;&c_a;&c_ch;&c_i;&c_j; <ind>&c_p;&c_o;&c_r;&c_t;</ind></trd>
  </trdgrp>
  <trd lng="zh">&#x6E14;&#x6E2F; <pr>y&#xFA;g&#x1CE;ng</pr></trd>
</drv>

<drv mrk="haven.flug0o">
  <kap>flug<tld/>o,
  <var><kap>aer<tld/>o</kap></var></kap>
  <snc mrk="haven.flug0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="prt" cel="aerodrom.0o">Aerodromo</ref> kun
      administrejoj,
      riparejoj, doganejoj, a&ubreve; ejoj por komerca&jcirc;oj kaj pasa&gcirc;eroj,
      kiuj faciligas intensan trafikon de grandaj avioj:
      <ekz>
	luita a&ubreve;to devis veturigi nin al la flug<tld/>o
	<fnt><aut>J. de N. Marques</aut> <vrk>Voja&gcirc;o al Ushuaia</vrk>
	<bib>Monato</bib> <lok>2002:2, p. 18a</lok></fnt>;
      </ekz>
      <ekz>
        helpantoj atendos &ccirc;e aer<tld/>o <nom>Taegu</nom>,
        stacio <nom>Dongtaegu</nom> au stacio <nom>Kyongsan</nom>
        <fnt>
          <aut>-</aut>
          <vrk><url ref="http://www.espero.com.cn/old/epch/05e/05e-02k/esperantujo2-1a.htm">La 33-a
          Korea Kongreso de Esperanto</url></vrk>
          <lok>Espero.com, 2001</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_e;&c_r;&c_a;&c_p;&c_o;&c_r;&c_t;</trd>
  <trdgrp lng="bg">
    <trd>&c_a;&c_je;&c_r;&c_o;&c_g;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_l;&c_je;&c_t;&c_i;&c_shch;&c_je;</trd>
  </trdgrp>
  <trd lng="bo">&t_ga;&t_na;&t_ma;&t_tsheg;&t_ga;&t_rata;&t_shabchu;&t_tsheg;&t_-a;&t_ba;&t_ba;&t_tsheg;&t_tha;&t_nga;&t_tsheg;</trd>
  <trd lng="br">aerborzh</trd>
  <trd lng="cs">leti&scaron;t&ecaron;</trd>
  <trd lng="de">Flughafen</trd>
  <trdgrp lng="fr">
    <trd>a&eacute;roport</trd>
  </trdgrp>
  <trd lng="hu">l&eacute;gikik&ouml;t&odblac;</trd>
  <trd lng="it">aeroporto</trd>
  <trd lng="ja">&#x7A7A;&#x6E2F;  <pr>&#x304F;&#x3046;&#x3053;&#x3046;</pr></trd>
  <trd lng="nl">luchthaven</trd>
  <trd lng="pt">aeroporto</trd>
  <trd lng="ru">&c_a;&c_e;&c_r;&c_o;&c_p;&c_o;&c_r;&c_t;</trd>
  <trd lng="sk">letisko</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_je;&c_r;&#x43E;&#x43F;&#x43E;&c_r;&c_t;</trd>,
    <trd>&#x43B;&c_je;&c_t;&#x43E;&c_v;&c_i;&c_shch;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7A7A;&#x6E2F; <pr>k&#x14D;ngg&#x1CE;ng</pr></trd>,
    <trd>&#x822A;&#x7A7A;&#x7AD9; <pr>h&#xE1;ngk&#x14D;ngzh&#xE0;n</pr></trd>,
    <trd>&#x822A;&#x7A7A;&#x6E2F; <pr>h&#xE1;ngk&#x14D;ngg&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="haven.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="haven.mar0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <tld lit="H"/>o kun aliro al maro, kontraste al rivera a&ubreve; laga
      <tld/>o:
      <ekz>
	tie estas la nura mar<tld/>o de la lando
	<fnt><aut>S. Zandandu Ntomono Zola</aut>, <vrk>Ekonomio dum la
	milito</vrk>, <bib>Monato</bib>, <lok>1999:3, p.
	9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_k;&c_ib; &c_p;&c_o;&c_r;&c_t;</trd>
  <trd lng="br">porzh-mor</trd>
