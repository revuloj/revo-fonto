<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>haven</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="haven.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="haven.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Nature a&ubreve; arte &scirc;irmita loko &ccirc;e bordo de maro a&ubreve;
      rivero, kie la &scirc;ipoj povas rifu&gcirc;i, efektivigi sian
      en&scirc;ar&gcirc;adon kaj el&scirc;ar&gcirc;adon ktp:
      <ekz>
	milita, komerca <tld/>o;
      </ekz>
      <ekz>
	<uzo tip="stl">FRAZ</uzo>
	<ind>feli&ccirc;e alveni &ccirc;e la <tld/>on</ind>
	<klr>(sendifekte atingi la celon)</klr>;
	<trd lng="fr">arriver &agrave; bon <ind>port</ind></trd>
      </ekz>
      <ekz>
	en la <tld/>o de Hamburgo jare alvenas 600 000 konteneroj el &Ccirc;inio
	<fnt><aut>Stefan Maul</aut>: <vrk>Surprizo el ligna kesto</vrk>,
	<bib>Monato</bib><lok>jaro 2002a, numero 1a, p. 21a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="basen.0o">baseno</ref>,
      <ref cel="dig.0o">digo</ref>,
      <ref cel="rod.0o">rodo</ref>,
      <ref cel="varf.0o">varfo</ref>.
    </refgrp>
    <trd lng="br">porzh<klr tip="ind"> (mor)</klr></trd>
    <trdgrp lng="fr">
      <trd>port<klr> (mar.)</klr></trd>
    </trdgrp>
    <trd lng="hu">kik&ouml;t&odblac;</trd>
  </snc>
  <snc mrk="haven.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Rifu&gcirc;ejo:
      <ekz>
	lia domo estis <tld/>o por la persekutatoj;
      </ekz>
      <ekz>
	ser&ccirc;i ripozan <tld/>on en mona&hcirc;ejo;
      </ekz>
      <ekz>
	vi <klr>...</klr> ricevos sekuran havenon en niaj landoj
	<fnt><aut>Stefan Maul</aut>: <vrk>Trompitaj dizertintoj</vrk>,
	<bib>Monato</bib><lok>jaro 2000a, numero 12a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>bodenn<klr tip="ind"> (repu)</klr></trd>,
      <trd>goudor<klr tip="ind"> (repu)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>havre<klr> (refuge)</klr></trd>,
      <trd>port<klr> (refuge)</klr></trd>
    </trdgrp>
    <trd lng="hu">r&eacute;v</trd>
    <trdgrp lng="pt">
     <trd>abrigo<klr tip="amb"> (ref&uacute;gio)</klr></trd>, 
     <trd>ref&uacute;gio</trd> 
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_v;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_t;</trd>
  </trdgrp>
  <trd lng="bg">&c_g;&c_a;&c_r;&c_a;</trd>
  <trd lng="de">Hafen</trd>
  <trd lng="el">&lambda;&jota;&my;&alfa_ton;&ny;&jota;</trd>
  <trd lng="en">harbor</trd>
  <trd lng="es">puerto</trd>
  <trd lng="it">porto</trd>
  <trd lng="nl">haven</trd>
  <trd lng="pl">port</trd>
  <trd lng="pt">porto</trd>
  <trd lng="tr">liman</trd>
  <trd lng="sv">hamn</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_v;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="haven.fisx0o">
  <kap>fi&scirc;<tld/>o</kap>
  <snc mrk="haven.fisx0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <tld lit="H"/>o a&ubreve; parto de haveno kun instala&jcirc;oj por fi&scirc;kaptaj
      &scirc;ipoj:
      <ekz>
	la fi&scirc;<tld/>oj estas multaj, precipe sude de Bretonio
	<fnt><aut>J. Ducloyer</aut>, <vrk>Bretonio</vrk>,
	<bib>Monato</bib><lok>jaro 2000a, numero 2a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">porzh pesketa</trd>
  <trd lng="de">Fischereihafen</trd>
  <trdgrp lng="fr">
    <trd>port de p&ecirc;che</trd>
  </trdgrp>
  <trd lng="hu">hal&aacute;szkik&ouml;t&odblac;</trd>
  <trd lng="pt">porto pesqueiro</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_r;&c_t; &c_r;&c_y;&c_b;&c_o;&c_l;&c_o;&c_v;&c_n;&c_o;&c_g;&c_o; &c_f;&c_l;&c_o;&c_t;&c_a;</trd>
    <trd>&c_r;&c_y;&c_b;&c_a;&c_ch;&c_i;&c_j; <ind>&c_p;&c_o;&c_r;&c_t;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="haven.flug0o">
  <kap>flug<tld/>o</kap>
  <snc mrk="haven.flug0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      <ref tip="prt" cel="aerodrom.0o">Aerodromo</ref> kun
      administrejoj,
      riparejoj, doganejoj, a&ubreve; ejoj por komerca&jcirc;oj kaj pasa&gcirc;eroj,
      kiuj
      faciligas intensan trafikon de grandaj avioj:
      <ekz>
	luita a&ubreve;to devis veturigi nin al la flug<tld/>o
	<fnt><aut>J. de N. Marques</aut>, <vrk>Voja&gcirc;o al Ushuaia</vrk>,
	<bib>Monato</bib><lok>jaro 2002a, numero 2a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_e;&c_r;&c_a;&c_p;&c_o;&c_r;&c_t;</trd>	
  <trdgrp lng="bg">
    <trd>&c_a;&c_je;&c_r;&c_o;&c_g;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_l;&c_je;&c_t;&c_i;&c_shch;&c_je;</trd>
  </trdgrp>
  <trd lng="br">aerborzh</trd>
  <trd lng="de">Flughafen</trd>
  <trdgrp lng="fr">
    <trd>a&eacute;roport</trd>
  </trdgrp>
  <trd lng="hu">l&eacute;gikik&ouml;t&odblac;</trd>
  <trd lng="it">aeroporto</trd>
  <trd lng="pt">aeroporto</trd>
  <trd lng="ru">&c_a;&c_e;&c_r;&c_o;&c_p;&c_o;&c_r;&c_t;</trd>
</drv>

<drv mrk="haven.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="haven.mar0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <tld lit="H"/>o kun aliro al maro, konstraste al rivera a&ubreve; laga
      <tld/>o:
      <ekz>
	tie estas la nura mar<tld/>o de la lando
	<fnt><aut>Zandandu Ntomono Zola</aut>, <vrk>Ekonomio dum la
	milito</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 3a, p.
	9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_s;&c_k;&c_ib; &c_p;&c_o;&c_r;&c_t;</trd>  
  <trd lng="br">porzh-mor</trd>
  <trd lng="de">Seehafen</trd>
  <trdgrp lng="fr">
    <trd>port de mer</trd>
  </trdgrp>
  <trd lng="hu">tengeri <ind>kik&ouml;t&odblac;</ind></trd>
  <trdgrp lng="it">
    <trd>porto<klr> di mare</klr></trd>
    <trd>porto<klr> marittimo</klr></trd>
  </trdgrp>
  <trd lng="pt">porto mar&iacute;timo</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; <ind>&c_p;&c_o;&c_r;&c_t;</ind></trd>
</drv>
</art>
