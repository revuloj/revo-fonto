<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plauxd.xml,v 1.24 2024/02/17 07:52:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pla&ubreve;d</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="plauxd.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="plauxd.0i.SON">
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> kiel akvo kaj
      plata&jcirc;o
      interbati&gcirc;antaj:
      <ekz>
        la ondoj <tld/>is sub la remiloj;
      </ekz>
      <ekz>
        fi&scirc;oj <klr>...</klr> elsaltis super la akvon, kiu <tld/>i&gcirc;is
        <klr>(ek<tld/>is)</klr>
        <fnt><bib>Fab2</bib>, <lok>dio de dormo, Mardo</lok></fnt>;
      </ekz>
      <ekz>
        la maro <tld/>as kontra&ubreve; la rokoj, kontra&ubreve; la &scirc;ipo;
      </ekz>
      <ekz>
        la veturiloj <tld/>a&ccirc;as tra la vojkavoj;
      </ekz>
      <ekz>
        anseroj <tld/>as en akvo per la flugiloj;
      </ekz>
      <ekz>
        la remiloj <tld/>as sur la akvo;
      </ekz>
      <ekz>
        &ccirc;e la fenestroj <tld/>as la flagoj;
      </ekz>
      <ekz>
        <tld/>u per viaj manoj, frapu per viaj piedoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <tld/>os <klr>(pene vados)</klr>  en sia vomado
         <fnt>
          <bib>MT</bib>
          <lok>Jeremia 48:26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aplaux.0i">apla&ubreve;di</ref>,
      <ref cel="klak.0i">klaki</ref>,
      <ref cel="sxmac.0i">&scirc;maci</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_ju;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_jo;&c_h;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>splash</trd>,
    <trd>beat</trd>,
    <trd>flap</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>claquer</trd>,
    <trd>clapoter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csobog</trd>,
    <trd>locsog</trd>,
    <trd>lubickol</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kabbelen</trd>,
    <trd>klotsen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bulgota&cacute;</trd>,
    <trd>pluska&cacute;</trd>,
    <trd>chlapa&cacute;</trd>,
    <trd>kapa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">chapinhar</trd>
  <trdgrp lng="ro">
    <trd>fle&#x219;c&abreve;i</trd>,
    <trd>stropi</trd>,
    <trd>pic&abreve;tur&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_s;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_sh;&c_l;&c_jo;&c_p;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">plaska</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&#x43B;&#x44E;&#x43F;&c_a;&c_t;&c_i;(&c_s;&#x44F;)</trd>,
    <trd>&c_h;&#x43B;&#x44E;&#x43F;&#x43E;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&#x43B;&#x44E;&#x43F;&#x43E;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>(&#x43F;&c_r;&#x43E; &c_b;&#x43E;&#x43B;&#x43E;&c_t;&#x43E;</trd>,
    <trd>&c_t;&c_r;&#x44F;&c_s;&#x43E;&c_v;&c_i;&#x43D;&c_u;</trd>,
    <trd>&c_h;&#x43E;&c_d;&c_ib;&#x43D;&#x43D;&#x44F; &#x43F;&#x43E; &c_g;&c_r;&#x44F;&c_z;&#x44E;&c_c;&c_ib; &c_shch;&c_je;) &c_ch;&c_a;&c_v;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_i;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_b;&c_r;&c_i;&c_z;&#x43A;&c_a;&c_t;&c_i;(&c_s;&#x44F;)</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_s;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43B;&c_je;&c_s;&#x43A;&c_a;&c_t;&c_i;(&c_s;&#x44F;)</trd>,
    <trd>&#x43F;&#x43B;&#x44E;&c_s;&#x43A;&c_a;&c_t;&c_i;(&c_s;&#x44F;)</trd>,
    <trd>&c_t;&c_r;&c_ib;&#x43F;&#x43E;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&#x43B;&#x43E;&#x43F;&#x43E;&c_t;&c_ib;&c_t;&c_i; (&#x43F;&c_r;&#x43E; &#x43F;&c_r;&c_a;&#x43F;&#x43E;&c_r;)</trd>,
    <trd>&#x43B;&#x44F;&c_s;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&#x43B;&c_je;&c_s;&#x43A;&c_a;&c_t;&c_i; (&c_v; &c_d;&#x43E;&#x43B;&#x43E;&#x43D;&c_ib;)</trd>
  </trdgrp>
</drv>

<drv mrk="plauxd.0">
  <kap><tld/></kap>
  <snc mrk="plauxd.0.SON">
    <gra><vspec>sonimito</vspec></gra>
    <dif>
      <ref tip="lst" cel="bru.0o" lst="voko:sonoj">Bruo</ref> de
      akvobato a&ubreve; simila:
      <ekz>
        la &scirc;tonoj <tld/>' <tld/>' falis en la puton
        <fnt><bib>ElektFab</bib>, <lok>feli&ccirc;a Jo&ccirc;jo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_ju;&c_h;!</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_mol;!</trd>
  </trdgrp>
  <trd lng="fr">plouf</trd>
  <trd lng="nl">klots</trd>
  <trdgrp lng="pl">
    <trd>kap</trd>,
    <trd>plusk</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pica</trd>,
    <trd>strop</trd>
  </trdgrp>
  <trd lng="sv">plask</trd>
</drv>

<drv mrk="plauxd.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="plauxd.0ado.SON">
    <dif>
      Tia sono, kian faras akvo batanta a&ubreve; batata:
      <ekz>
        la <tld/>ado de la akvo sur la flankojn de &leftquot;Santa Maria"
        <klr>(&scirc;ipo)</klr> sonis en liaj oreloj kiel funebra sonorado
        <fnt><bib>FK</bib>, <lok>4, rakontoj, nokto, (de Eube el
        Odeso)</lok></fnt>;
      </ekz>
      <ekz>
        la sopira <tld/>o de la senfina pluvo<fnt>B</fnt>;
      </ekz>
      <ekz>
        subite interrompis la silenton <tld/>ado de &ccirc;evalaj piedoj en
        koto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_l;&c_ju;&c_h;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_l;&c_jo;&c_s;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_jo;&c_s;&c_k;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>clapotis</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csobog&aacute;s</trd>,
    <trd>lubickol&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">gekabbel</trd>
  <trd lng="pl">pluskanie</trd>
  <trd lng="ro">stropi</trd>
  <trd lng="ru">&c_sh;&c_l;&c_jo;&c_p;&c_a;&c_n;&c_mol;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&#x43B;&#x44E;&#x43F;&c_ib;&c_t;</trd>,
    <trd>&c_h;&#x43B;&#x44E;&#x43F;&#x43E;&c_t;&c_ib;&#x43D;&#x43D;&#x44F;
    &c_ch;&c_a;&c_v;&#x43A;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_b;&c_r;&c_i;&c_z;&#x43A;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_s;&#x43A;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x43B;&c_je;&c_s;&#x43A;&c_ib;&c_t;</trd>,
    <trd>&#x43F;&#x43B;&#x44E;&c_s;&#x43A;&c_ib;&c_t;</trd>,
    <trd>&c_t;&c_r;&c_ib;&#x43F;&#x43E;&c_t;&c_ib;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43B;&#x43E;&#x43F;&#x43E;&c_t;&c_ib;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43F;&#x43B;&c_je;&c_s;&#x43A;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43B;&#x44F;&c_s;&#x43A;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43B;&#x44F;&c_s;&#x43A;&c_ib;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6DD9; <pr>c&#xF3;ng</pr></trd>,
    <trd>&#x703A; <pr>ch&#xE1;n</pr></trd>,
    <trd>&#x6CD9; <pr>p&#xED;ng</pr></trd>,
    <trd>&#x98DB;&#x6FFA; <pr>f&#x113;iji&#xE0;n</pr></trd>,
    <trd>&#x98DE;&#x6E85; <pr>f&#x113;iji&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="plauxd.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="plauxd.0ejo.GEOG">
    <dif>
      Loko de la <ref tip="malprt" cel="bord.0o.GEOG">bordo</ref>, kie
      la
      ondoj <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_a;&c_s;&c_a; &c_n;&c_a;&c_p;&c_a;&c_d;&c_u; &c_h;&c_v;&c_a;&c_l;&c_ja;&c_w;</trd>
  <trd lng="nl">branding</trd>
  <trd lng="pl">brzeg  <klr>(morza, jeziora)</klr></trd>
  <trdgrp lng="ro">
    <trd>coast&abreve;</trd>,
    <trd>hotar</trd>
  </trdgrp>
</drv>

<drv mrk="plauxd.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plauxd.0igi.SON">
    <dif>
      Fari, ke akvo a&ubreve; plata objekto <tld/>u:
      <ekz>
        la balailoj de la lavistinoj <tld/>igis la &scirc;a&ubreve;mon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_b;&c_o;&c_w;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_o;&c_w;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_mol;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr></trd>
  </trdgrp>
  <trd lng="hu">paskol</trd>
  <trd lng="nl">doen <ind>klotsen</ind></trd>
  <trdgrp lng="pl">
    <trd>chlupa&cacute;</trd>,
    <trd>chlupn&aogonek;&cacute;</trd>
  </trdgrp>
  <trd lng="ro">fle&#x219;c&abreve;i</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_b;&c_i;&c_v;&c_a;&c_t;&c_i; &c_v;&#x43E;&c_d;&c_u;</trd>,
    <trd>&c_z;&c_d;&c_ib;&c_j;&#x43C;&c_a;&c_t;&c_i; &c_b;&c_r;&c_i;&c_z;&#x43A;&c_i;</trd>,
    <trd>&c_v;&c_i;&#x43A;&#x43B;&c_i;&#x43A;&c_a;&c_t;&c_i; &c_h;&#x43B;&#x44E;&#x43F;&c_ib;&c_t;</trd>,
    <trd>&#x43F;&#x43B;&c_je;&c_s;&#x43A;&c_ib;&c_t;</trd>,
    <trd>&#x43B;&#x44F;&c_s;&#x43A;&c_ib;&c_t; &c_ib; &c_t;.&#x43F;.</trd>
  </trdgrp>
</drv>

<drv mrk="plauxd.man0i">
  <kap>man<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="plauxd.man0i.SON">
    <dif>
      Kunbati la manojn kun <tld/>a bruo,
      <ref tip="vid" cel="aplaux.0i">apla&ubreve;di</ref>:
      <ekz>
        Tadeo kaj Zofio man<tld/>is konstante, /
        Kvaza&ubreve; la&ubreve;de, sed vere, a&ubreve;di ne volante
        <fnt>
          <aut>A. Mickiewicz, trad. A. Grabowski</aut>,
          <vrk>Sinjoro Tadeo</vrk>,
          <lok>1918</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a;&c_c;&c_mol; &c_u; <ind>&c_l;&c_a;&c_d;&c_k;&c_ib;</ind></trd>,
    <trd>&c_b;&c_ib;&c_c;&c_mol; &c_u; <ind>&c_l;&c_a;&c_d;&c_k;&c_ib;</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>applaudir</trd>,
    <trd>claquer des mains</trd>
  </trdgrp>
  <trd lng="pl">klaska&cacute;</trd>
  <trd lng="ro">aplauda</trd>
  <trd lng="zh">&#x62CD;&#x624B; <pr>p&#x101;ish&#x1D2;u</pr></trd>
</drv>
</art>
