<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plauxd.xml,v 1.26 2025/12/26 20:33:12 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pla&ubreve;d</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="plauxd.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="plauxd.0i.SON">
    <dif>
      <ref tip="super" cel="bru.0i.soni">Brui</ref> kiel akvo kaj
      plata&jcirc;o interbati&gcirc;antaj:
      <ekz>
        ondetoj <tld/>is &ccirc;e la bordo
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la altaj marmoraj salonoj <tld/>is grandaj fontanoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        fi&scirc;oj <klr>[&#x2026;]</klr> elsaltis super la akvon, kiu <tld/>i&gcirc;is
        <klr>(ek<tld/>is)</klr>
        <fnt><bib>Fab2</bib>, <lok>dio de dormo, Mardo</lok></fnt>;
      </ekz>
      <ekz>
        la re&gcirc;ido <tld/>is per la manoj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en mano <tld/>os <tld/>ileto
        <klr>(kp <ref cel="kastan.0o">kastanjeto</ref>)</klr>
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VII.</lok>
        </fnt>;
      </ekz>
      <ekz>
        pluvo <tld/>as en la tegmenta defluilo
        <fnt>
          <bib>Kiso</bib>
          <lok>La Pluveto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj tri knabinoj <tld/>is en la lago
        <fnt>
          <bib>Kiso</bib>
          <lok>La Bovinoj de la Paro&hcirc;estro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u per viaj manoj, frapu per via piedo
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 6:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>os <klr>(pene vados)</klr> en sia vomado
         <fnt>
          <bib>MT</bib>
          <lok>Jeremia 48:26</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aplaux.0i">apla&ubreve;di</ref>,
      <ref cel="klak.0i">klaki</ref>,
      <ref cel="sxmac.0i">&scirc;maci</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_ju;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_l;&c_jo;&c_h;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>platschen</trd>,
    <trd>planschen</trd>,
    <trd>klatschen <klr>(Wasser)</klr></trd>,
    <trd>branden <klr>(Wellen)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>splash</trd>,
    <trd>beat</trd>,
    <trd>flap</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>claquer</trd>,
    <trd>clapoter</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csobog</trd>,
    <trd>locsog</trd>,
    <trd>lubickol</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kabbelen</trd>,
    <trd>klotsen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bulgota&cacute;</trd>,
    <trd>pluska&cacute;</trd>,
    <trd>chlapa&cacute;</trd>,
    <trd>kapa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">chapinhar</trd>
  <trdgrp lng="ro">
    <trd>fle&scomma;c&abreve;i</trd>,
    <trd>stropi</trd>,
    <trd>pic&abreve;tur&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_s;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_sh;&c_l;&c_jo;&c_p;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">plaska</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_l;&c_ju;&c_p;&c_a;&c_t;&c_i;(&c_s;&c_ja;)</trd>,
    <trd>&c_h;&c_l;&c_ju;&c_p;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_l;&c_ju;&c_p;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>(&c_p;&c_r;&c_o; &c_b;&c_o;&c_l;&c_o;&c_t;&c_o;</trd>,
    <trd>&c_t;&c_r;&c_ja;&c_s;&c_o;&c_v;&c_i;&c_n;&c_u;</trd>,
    <trd>&c_h;&c_o;&c_d;&c_ib;&c_n;&c_n;&c_ja; &c_p;&c_o; &c_g;&c_r;&c_ja;&c_z;&c_ju;&c_c;&c_ib; &c_shch;&c_je;) &c_ch;&c_a;&c_v;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_b;&c_r;&c_i;&c_z;&c_k;&c_a;&c_t;&c_i;(&c_s;&c_ja;)</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_l;&c_je;&c_s;&c_k;&c_a;&c_t;&c_i;(&c_s;&c_ja;)</trd>,
    <trd>&c_p;&c_l;&c_ju;&c_s;&c_k;&c_a;&c_t;&c_i;(&c_s;&c_ja;)</trd>,
    <trd>&c_t;&c_r;&c_ib;&c_p;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_l;&c_o;&c_p;&c_o;&c_t;&c_ib;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_p;&c_r;&c_a;&c_p;&c_o;&c_r;)</trd>,
    <trd>&c_l;&c_ja;&c_s;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_l;&c_je;&c_s;&c_k;&c_a;&c_t;&c_i; (&c_v; &c_d;&c_o;&c_l;&c_o;&c_n;&c_ib;)</trd>
  </trdgrp>
</drv>

<drv mrk="plauxd.0">
  <kap><tld/></kap>
  <snc mrk="plauxd.0.SON">
    <gra><vspec>sonimito</vspec></gra>
    <dif>
      <ref tip="lst" cel="bru.0o" lst="voko:sonoj">Bruo</ref> de
      akvobato a&ubreve; simila:
      <ekz>
        la &scirc;tonoj <tld/>' <tld/>' falis en la puton
        <fnt><bib>ElektFab</bib>, <lok>Feli&ccirc;a Jo&ccirc;jo</lok></fnt>;
      </ekz>
      <ekz>
        Se mi, ekzemple, povus salti pli facile &parsl; Al tron&#x2019;, kaj <tld/>! sur&scirc;pruci caron
        aspergile
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VII.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_ju;&c_h;!</trd>,
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_mol;!</trd>
  </trdgrp>
  <trd lng="de">platsch!</trd>
  <trd lng="fr">plouf</trd>
  <trd lng="nl">klots</trd>
  <trdgrp lng="pl">
    <trd>kap</trd>,
    <trd>plusk</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>pica</trd>,
    <trd>strop</trd>
  </trdgrp>
  <trd lng="sv">plask</trd>
</drv>

<drv mrk="plauxd.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="plauxd.0ado.SON">
    <dif>
      Tia sono, kian faras akvo batanta a&ubreve; batata:
      <ekz>
        <tld/>ado de fontano
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de remiloj rompis la silenton
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lastaj ondoj, kaj la longaj strioj de la &scirc;a&ubreve;moj sin kro&ccirc;is al la &scirc;tonoj kun
        plenda, mortanta <tld/>o
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ado de la akvo sur la flankojn de &leftquot;<nom>Santa Maria</nom>"
        <klr>(&scirc;ipo)</klr> sonis en liaj oreloj kiel funebra sonorado
        <fnt><bib>FK</bib>, <lok>4, rakontoj, nokto, (de Eube el
        Odeso)</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o de kisado alvenis malkonfuze &gcirc;is &scirc;iaj oreloj
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deksepa</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_l;&c_ju;&c_h;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_l;&c_jo;&c_s;&c_k;</trd>,
    <trd>&c_p;&c_l;&c_jo;&c_s;&c_k;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Platschen</trd>,
    <trd>Planschen</trd>,
    <trd>Branden <klr>(Wellen)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>clapotis</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csobog&aacute;s</trd>,
    <trd>lubickol&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">gekabbel</trd>
  <trd lng="pl">pluskanie</trd>
  <trd lng="ro">stropi</trd>
  <trd lng="ru">&c_sh;&c_l;&c_jo;&c_p;&c_a;&c_n;&c_mol;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_l;&c_ju;&c_p;&c_ib;&c_t;</trd>,
    <trd>&c_h;&c_l;&c_ju;&c_p;&c_o;&c_t;&c_ib;&c_n;&c_n;&c_ja;
    &c_ch;&c_a;&c_v;&c_k;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_b;&c_r;&c_i;&c_z;&c_k;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_k;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_l;&c_je;&c_s;&c_k;&c_ib;&c_t;</trd>,
    <trd>&c_p;&c_l;&c_ju;&c_s;&c_k;&c_ib;&c_t;</trd>,
    <trd>&c_t;&c_r;&c_ib;&c_p;&c_o;&c_t;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_l;&c_o;&c_p;&c_o;&c_t;&c_ib;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_l;&c_je;&c_s;&c_k;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_l;&c_ja;&c_s;&c_k;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_l;&c_ja;&c_s;&c_k;&c_ib;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6DD9; <pr>c&oacute;ng</pr></trd>,
    <trd>&#x703A; <pr>ch&aacute;n</pr></trd>,
    <trd>&#x6CD9; <pr>p&iacute;ng</pr></trd>,
    <trd>&#x98DB;&#x6FFA; <pr>f&emacron;iji&agrave;n</pr></trd>,
    <trd>&#x98DE;&#x6E85; <pr>f&emacron;iji&agrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="plauxd.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="plauxd.0ejo.GEOG">
    <dif>
      Loko de la <ref tip="malprt" cel="bord.0o.GEOG">bordo</ref>, kie
      la ondoj <tld/>as.
    </dif>
    <refgrp tip="vid">
      <ref cel="lof.0o">lofo</ref>,
      <ref cel="rif.0o.roko">rifo<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Brandung</trd>,
      <trd>Brandungszone</trd>,
      <trd>Luv-K&uuml;ste</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_a;&c_s;&c_a; &c_n;&c_a;&c_p;&c_a;&c_d;&c_u; &c_h;&c_v;&c_a;&c_l;&c_ja;&c_w;</trd>
  <trd lng="nl">branding</trd>
  <trd lng="pl">brzeg  <klr>(morza, jeziora)</klr></trd>
  <trdgrp lng="ro">
    <trd>coast&abreve;</trd>,
    <trd>hotar</trd>
  </trdgrp>
</drv>

<drv mrk="plauxd.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plauxd.0igi.SON">
    <dif>
      Fari, ke akvo a&ubreve; plata objekto <tld/>u:
      <ekz>
        la balailoj de la lavistinoj <tld/>igis la &scirc;a&ubreve;mon;
      </ekz>
      <ekz>
        li plon&gcirc;igis sian kapon en la malvarman akvon kaj <tld/>igis &gcirc;in sur siajn nukon
        kaj orelojn
        <fnt>
          <aut>J.R.R. Tolkien, trad. William Auld</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0177/0040.php"
              >La Mastro de l' Ringoj</url></vrk>
          <lok>&Ccirc;apitro 5: La fenestro al la Okcidento</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">ins Wasser <ind>schlagen</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_b;&c_o;&c_w;&c_t;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_o;&c_w;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_mol;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_v;&c_a;&c_d;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;)</klr></trd>
  </trdgrp>
  <trd lng="hu">paskol</trd>
  <trd lng="nl">doen <ind>klotsen</ind></trd>
  <trdgrp lng="pl">
    <trd>chlupa&cacute;</trd>,
    <trd>chlupn&aogonek;&cacute;</trd>
  </trdgrp>
  <trd lng="ro">fle&scomma;c&abreve;i</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_b;&c_i;&c_v;&c_a;&c_t;&c_i; &c_v;&c_o;&c_d;&c_u;</trd>,
    <trd>&c_z;&c_d;&c_ib;&c_j;&c_m;&c_a;&c_t;&c_i; &c_b;&c_r;&c_i;&c_z;&c_k;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_l;&c_i;&c_k;&c_a;&c_t;&c_i; &c_h;&c_l;&c_ju;&c_p;&c_ib;&c_t;</trd>,
    <trd>&c_p;&c_l;&c_je;&c_s;&c_k;&c_ib;&c_t;</trd>,
    <trd>&c_l;&c_ja;&c_s;&c_k;&c_ib;&c_t; &c_ib; &c_t;.&c_p;.</trd>
  </trdgrp>
</drv>

<drv mrk="plauxd.man0i">
  <kap>man<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="plauxd.man0i.SON">
    <dif>
      Kunbati la manojn kun <tld/>a bruo,
      <ref tip="vid" cel="aplaux.0i">apla&ubreve;di</ref>:
      <ekz>
        Tadeo kaj Zofio man<tld/>is konstante, &parsl;
        Kvaza&ubreve; la&ubreve;de, sed vere, a&ubreve;di ne volante
        <fnt>
          <aut>A. Mickiewicz, trad. A. Grabowski</aut>,
          <vrk>Sinjoro Tadeo</vrk>,
          <lok>1918</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">mit den H&auml;nden <ind>klatschen</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_ja;&c_s;&c_k;&c_a;&c_c;&c_mol; &c_u; <ind>&c_l;&c_a;&c_d;&c_k;&c_ib;</ind></trd>,
    <trd>&c_b;&c_ib;&c_c;&c_mol; &c_u; <ind>&c_l;&c_a;&c_d;&c_k;&c_ib;</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>applaudir</trd>,
    <trd>claquer des mains</trd>
  </trdgrp>
  <trd lng="pl">klaska&cacute;</trd>
  <trd lng="ro">aplauda</trd>
  <trd lng="zh">&#x62CD;&#x624B; <pr>p&amacron;ish&#x1D2;u</pr></trd>
</drv>
</art>
