<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pendol.xml,v 1.23 2022/08/23 07:35:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pendol</rad>/o
</kap>

<drv mrk="pendol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pendol.0o.pezajxo">
    <dif>
      Peza&jcirc;o pendanta de fiksa punkto kaj balanci&gcirc;anta
      la&ubreve; egalaj temperoj:
      <ekz>
        la <tld/>o &ccirc;e la granda horlo&gcirc;o iradis tien kaj reen
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mano tenanta la revolveron da&ubreve;rigis sian agitadon, a&ubreve;tomate movi&gcirc;ante kiel
        <tld/>o
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la koro de Fernando estis vera <tld/>o, balanci&gcirc;anta el unu junulino al la alia
        kaj ne povanta fiksi&gcirc;i
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lumigita lampo malrapide balanci&gcirc;is tien kaj reen de la plafono kvaza&ubreve; <tld/>o
        <fnt>
          <bib>Kiso</bib>
          <lok>La Sakramento De La Komunio</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kelkajn jarojn poste la <tld/>o svingis denove en la alian direkton: homojn
        tedis kaj na&ubreve;zis politiko kaj militoj kun ties terura&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2004/008384.html"
              >Alveno al la moderna vivo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o de la historio movi&gcirc;as onde, sed ne strikte sinuse, kun regulaj
        pintoj kaj valoj
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2007/009145.php"
              >Politika historio de la ne&ubreve;trala UEA</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_ja;&c_t;&c_n;&c_ib;&c_k;</trd>
    <trd lng="bg">&c_m;&c_a;&c_h;&c_a;&c_l;&c_o;</trd>
    <trd lng="br">momeder</trd>
    <trdgrp lng="cs">
      <trd>kyvadlo hodin</trd>,
      <trd>perpendikl</trd>,
      <trd>perpentikl</trd>,
      <trd>perpentykl</trd>
    </trdgrp>
    <trd lng="de">Pendel</trd>
    <trdgrp lng="en">
      <trd>pendulum</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_A_madda;&a_w;&a_n;&f_g;</trd>,
      <trd>&f_p;&a_A;&a_n;&a_d;&a_w;&a_l;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>balancier</trd>,
      <trd>pendule<klr tip="ind"> (masc.)</klr></trd>
    </trdgrp>
    <trd lng="hu">inga</trd>
    <trdgrp lng="nl">
      <trd>slinger</trd>,
      <trd>pendel</trd>
    </trdgrp>
    <trd lng="pt">p&ecirc;ndulo</trd>
    <trd lng="ru">&c_m;&c_a;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd>
    <trd lng="sk">z&aacute;vesn&aacute; h&uacute;pa&ccaron;ka</trd>
    <trd lng="sv">pendel</trd>
  </snc>
  <snc mrk="pendol.0o.ludilo">
    <ref tip="dif" cel="pendol.0ilo.ludilo">pendolilo</ref>
    <trd lng="en">swing</trd>
  </snc>
  <trd lng="pl">wahad&lstroke;o</trd>
  <trd lng="ro">pendul</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_g;&c_o;&c_j;&c_d;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="pendol.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="balanc1.0igxi">Balanci&gcirc;i</ref> regule
      kiel <tld/>o:
      <ekz>
        el liaj oreloj pendolis la kuprokuboj
        <fnt>
          <bib>VaK</bib>
          <lok>Deka &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la anta&ubreve;brakoj pendis perpendikulare kaj <tld/>is de la kubutoj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 38</lok>
        </fnt>;
      </ekz>
      <ekz>
        la longa travidebla civitano <tld/>is anta&ubreve; li dekstren kaj maldekstren sen tu&scirc;i
        la teron, la teruro tiom ekposedis la redaktoron, ke li fermis la okulojn
        <fnt>
          <bib>MkM</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sur liaj genuoj kaj sur la maldekstra sidvango oscedis grandaj truoj, de tiuj
        <tld/>is la &ccirc;ifonaj &scirc;ira&jcirc;oj de la drapo
        <fnt>
          <bib>VaK</bib>
          <lok>Dekdua &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        eblas <tld/>i <klr>(<ref cel="altern.0i">alterni</ref>)</klr>
        inter lo&gcirc;- kaj labor-loko en la &ccirc;efurbo kontra&ubreve; ekvivalento de nur
        0,13 e&ubreve;roj
        <fnt>
          <bib>Monato</bib>
          <aut>J&eacute;r&eacute;mie Sabiyumva</aut>
          <vrk><url ref="&Monato;/2009/009690.php"
              >Pendole pedale</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sving">svingi&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_j;&c_d;&c_a;&c_c;&c_c;&c_a; <klr tip="ind">(&c_n;&c_ib;&c_b;&c_y;
&c_m;&c_a;&c_ja;&c_t;&c_n;&c_ib;&c_k;)</klr></trd>,
    <trd>&c_h;&c_ib;&c_s;&c_t;&c_a;&c_c;&c_c;&c_a; <klr tip="amb">(&c_n;&c_ib;&c_b;&c_y;
&c_m;&c_a;&c_ja;&c_t;&c_n;&c_ib;&c_k;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_l;&c_a;&c_t;&c_ja; &c_s;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>houpat se</trd>,
    <trd>k&yacute;vat se</trd>
  </trdgrp>
  <trd lng="de">pendeln</trd>
  <trd lng="en">swing</trd>
  <trdgrp lng="hu">
    <trd>ing&aacute;zik</trd>,
    <trd>ing</trd>,
    <trd>leng</trd>,
    <trd>kileng</trd>
  </trdgrp>
  <trd lng="nl">slingeren</trd>
  <trd lng="pl">waha&cacute; si&eogonek;  <klr>(ruch)</klr></trd>
  <trd lng="ro">balansa</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="ind">
(&c_k;&c_a;&c_k; &c_m;&c_a;&c_ja;&c_t;&c_n;&c_i;&c_k;)</klr></trd>,
    <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr tip="amb">(&c_k;&c_a;&c_k;
&c_m;&c_a;&c_ja;&c_t;&c_n;&c_i;&c_k;)</klr></trd>
  </trdgrp>
  <trd lng="sk">k&yacute;va&tcaron; sa <klr>(ako kyvadlo)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_l;&c_i;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_h;&c_i;&c_t;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="pendol.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="pendol.0ilo.ludilo">
    <dif>
      <ref tip="dif" cel="balanc1.0ilo.SPO.pendolilo">Balancilo<sncref/></ref>:
      <ekz>
        Mumintrolo estis okupita fiksi <tld/>ilon  
        <fnt>
          <aut>Tove Jansson, trad. Sten Johansson</aut>,
          <vrk><url ref="http://www.geocities.ws/muminvalo/komet/k1.html"
            >Kometo en Muminvalo</url>
          </vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="nl">schommel</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_r;&c_e;&c_l;&c_ib;</trd>,
    <trd>&c_g;&c_u;&c_sh;&c_k;&c_a;&c_l;&c_k;&c_a;</trd>
  </trdgrp>
<trd lng="de">Schaukel</trd>
<trd lng="en">swing</trd>
<trdgrp lng="pl">
  <trd>perpendyku&lstroke;</trd>,
  <trd>ci&eogonek;&zdot;arek na lince</trd>
</trdgrp>
<trd lng="ro">leag&abreve;n</trd>
</drv>
</art>
