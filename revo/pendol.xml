<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pendol</rad>/o
</kap>
<drv mrk="pendol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Peza&jcirc;o pendanta de fiksa punkto kaj balanci&gcirc;anta la&ubreve; egalaj temperoj:
      <ekz>
        la <tld/>o de la horlo&gcirc;o iradis tien kaj reen<fnt>Z</fnt>..
      </ekz>
    </dif>
  </snc>                     
  <trd lng="be">&c_m;&c_a;&c_ja;&c_t;&c_n;&c_ib;&c_k;</trd>
  <trd lng="br">momeder</trd> 
  <trdgrp lng="fr">
    <trd>balancier</trd>,
    <trd>pendule<klr tip="ind"> (masc.)</klr></trd>
  </trdgrp>
  <trd lng="hu">inga</trd>  
  <trdgrp lng="nl">
    <trd>slinger</trd>,
    <trd>pendel</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd>
  <trd lng="sv">pendel</trd>
</drv>

<drv mrk="pendol.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Balanci&gcirc;i kiel <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="sving">svingi&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_j;&c_d;&c_a;&c_c;&c_c;&c_a; <klr tip="ind">(&c_n;&c_ib;&c_b;&c_y; &c_m;&c_a;&c_ja;&c_t;&c_n;&c_ib;&c_k;)</klr></trd>,
    <trd>&c_h;&c_ib;&c_s;&c_t;&c_a;&c_c;&c_c;&c_a; <klr tip="amb">(&c_n;&c_ib;&c_b;&c_y; &c_m;&c_a;&c_ja;&c_t;&c_n;&c_ib;&c_k;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ing&aacute;zik</trd>,
    <trd>ing</trd>,
    <trd>leng</trd>,
    <trd>kileng</trd>
  </trdgrp>
  <trd lng="nl">slingeren</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="ind"> (&c_k;&c_a;&c_k; &c_m;&c_a;&c_ja;&c_t;&c_n;&c_i;&c_k;)</klr></trd>,
    <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr tip="amb">(&c_k;&c_a;&c_k; &c_m;&c_a;&c_ja;&c_t;&c_n;&c_i;&c_k;)</klr></trd>
  </trdgrp>
</drv>
</art>
