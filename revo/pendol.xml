<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pendol.xml,v 1.17 2021/02/12 18:10:32 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pendol</rad>/o
</kap>

<drv mrk="pendol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pendol.0o.pezajxo">
    <dif>
      Peza&jcirc;o pendanta de fiksa punkto kaj balanci&gcirc;anta
la&ubreve; egalaj temperoj:
      <ekz>
        la <tld/>o de la horlo&gcirc;o iradis tien kaj reen<fnt>Z</fnt>..
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_ja;&c_t;&c_n;&c_ib;&c_k;</trd>
    <trd lng="bg">&c_m;&c_a;&c_h;&c_a;&c_l;&c_o;</trd>
    <trd lng="br">momeder</trd>
    <trdgrp lng="cs">
      <trd>kyvadlo hodin</trd>,
      <trd>perpendikl</trd>,
      <trd>perpentikl</trd>,
      <trd>perpentykl</trd>
    </trdgrp>
    <trd lng="de">Pendel</trd>
    <trd lng="en">pendulum</trd>
    <trdgrp lng="fa">
      <trd>&a_A_madda;&a_w;&a_n;&f_g;</trd>,
      <trd>&f_p;&a_A;&a_n;&a_d;&a_w;&a_l;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>balancier</trd>,
      <trd>pendule<klr tip="ind"> (masc.)</klr></trd>
    </trdgrp>
    <trd lng="hu">inga</trd>
    <trdgrp lng="nl">
      <trd>slinger</trd>,
      <trd>pendel</trd>
    </trdgrp>
    <trd lng="pt">p&ecirc;ndulo</trd>
    <trd lng="ru">&c_m;&c_a;&c_ja;&c_t;&c_n;&c_i;&c_k;</trd>
    <trd lng="sk">z&aacute;vesn&aacute; h&uacute;pa&ccaron;ka</trd>
    <trd lng="sv">pendel</trd>
  </snc>
  <snc mrk="pendol.0o.ludilo">
    <ref tip="sin" cel="pendol.0ilo.ludilo">pendolilo</ref>
  </snc>
</drv>

<drv mrk="pendol.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Balanci&gcirc;i kiel <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="sving">svingi&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_j;&c_d;&c_a;&c_c;&c_c;&c_a; <klr
tip="ind">(&c_n;&c_ib;&c_b;&c_y;
&c_m;&c_a;&c_ja;&c_t;&c_n;&c_ib;&c_k;)</klr></trd>,
    <trd>&c_h;&c_ib;&c_s;&c_t;&c_a;&c_c;&c_c;&c_a; <klr
tip="amb">(&c_n;&c_ib;&c_b;&c_y;
&c_m;&c_a;&c_ja;&c_t;&c_n;&c_ib;&c_k;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_l;&c_a;&c_t;&c_ja; &c_s;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>houpat se</trd>,
    <trd>k&yacute;vat se</trd>
  </trdgrp>
  <trd lng="de">pendeln</trd>
  <trd lng="en">swing</trd>
  <trdgrp lng="hu">
    <trd>ing&aacute;zik</trd>,
    <trd>ing</trd>,
    <trd>leng</trd>,
    <trd>kileng</trd>
  </trdgrp>
  <trd lng="nl">slingeren</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_ch;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="ind">
(&c_k;&c_a;&c_k; &c_m;&c_a;&c_ja;&c_t;&c_n;&c_i;&c_k;)</klr></trd>,
    <trd>&c_k;&c_o;&c_l;&c_je;&c_b;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr
tip="amb">(&c_k;&c_a;&c_k;
&c_m;&c_a;&c_ja;&c_t;&c_n;&c_i;&c_k;)</klr></trd>
  </trdgrp>
  <trd lng="sk">k&yacute;va&tcaron; sa <klr>(ako kyvadlo)</klr></trd>
</drv>

<drv mrk="pendol.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="pendol.0ilo.ludilo">
    <dif>
      <tld lit="P"/>simila ludilo, kiu konsistas el sidtabulo, pendanta
      de du
      longaj &scirc;nuroj kaj la&ubreve;vole svingebla:
      <ekz>
        Mumintrolo estis okupita fiksi <tld/>ilon  
        <fnt>
        <aut>Tove Jansson, trad. Sten Johansson</aut>,
        <vrk>
          <url ref="http://www.geocities.ws/muminvalo/komet/k1.html">
          Kometo en Muminvalo</url>
        </vrk>
        </fnt>
      </ekz>
    </dif>
    <trd lng="nl">schommel</trd>
  </snc>
</drv>
</art>
