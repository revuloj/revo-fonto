<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kapok</rad>/o
</kap>
<drv mrk="kapok.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Plantharoj el la interna frukt&scirc;elo de la 
      <ref tip="malprt" cel="kapok.0ujo"><tld/>ujo</ref>,
      uzataj por remburi savringojn, na&gcirc;zonojn, 
      kusenojn, matracojn, ktp.
    </dif>
  </snc>
  <trd lng="de">Kapok</trd>
  <trd lng="en">kapok</trd>
</drv>
<drv mrk="kapok.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="bombak.0acoj">bombakacoj</ref>
      (<trd lng="la">Ceiba</trd>), azia arbo.
    </dif>
  </snc>
  <trd lng="de">Kapokbaum</trd>
  <trd lng="en">kapok tree</trd>
</drv>
</art>

