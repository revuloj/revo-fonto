<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kapok.xml,v 1.19 2023/09/01 13:58:55 revo Exp $">
<kap>
<rad>kapok</rad>/o
</kap>
<drv mrk="kapok.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Plantharoj el la interna frukt&scirc;elo de la 
      <ref tip="malprt" cel="kapok.0ujo"><tld/>ujo</ref>,
      uzataj por remburi savringojn, na&gcirc;zonojn, 
      kusenojn, matracojn, ktp:
      <ekz>
        ekzistas diversaj specoj de palmoj en Gambio, precipe la
        kokoso-palmo,
        <klr>[&#x2026;anka&ubreve;]</klr> okulfrapaj estas la koto-arbo a&ubreve; la
        <tld/>-arbo kiuj estas pli
        ol 50 metrojn altaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1440">Gambio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">capoc</trd>
  <trd lng="cs">kapok</trd>
  <trd lng="de">Kapok</trd>
  <trd lng="en">kapok</trd>
  <trd lng="fr">kapok</trd>
  <trd lng="hu">kapok<klr tip="ind"> (sz&aacute;las anyag)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30DD;&#x30C3;&#x30AF;</trd>,
    <trd>&#x30D1;&#x30F3;&#x30E4;&#x7DBF;  [<ind>&#x30D1;&#x30F3;&#x30E4;&#x3081;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">kapok</trd>
  <trd lng="pl">kapok <klr>(w&lstroke;&oacute;no)</klr></trd>     
  <trd lng="ro">capoc </trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_o;&c_k;
<klr>(&c_r;&c_a;&c_s;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;
&c_p;&c_u;&c_h;)</klr></trd>
  <trd lng="sk">kapok</trd>
  <trd lng="uk">&#x43A;&#x430;&#x43F;&#x43E;&#x43A;</trd>
</drv>
<drv mrk="kapok.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio 
      <ref tip="malprt" cel="bombak1.0oj">bombakacoj</ref>
      (<trd lng="la">Ceiba</trd>), azia arbo.
    </dif>
    <bld lok="&WCU;/thumb/8/85/Caulepaineira4.jpg/375px-Caulepaineira4.jpg" prm="PD"><tld/>ujo (Ceiba speciosa)
        <fnt>
          <aut>Mauroguanandi, propra verko</aut>
          <lok><url ref="&WCW;/File:Caulepaineira4.jpg">&WCW;/File:Caulepaineira4.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">capoquer</trd>
  <trd lng="de">Kapokbaum</trd>
  <trd lng="en">kapok tree</trd>
  <trd lng="fr">kapokier</trd>
  <trdgrp lng="hu">
    <trd>kapok(fa)</trd>,
    <trd>pamutfa</trd></trdgrp>
  <trd lng="nl">kapokboom</trd>
  <trdgrp lng="pl">
    <trd>kapokowe  drzewo</trd>,
    <trd>puchowiec</trd>
  </trdgrp>   
  <trd lng="ro">capoc </trd>
  <trd lng="ru">&c_k;&c_a;&c_p;&c_o;&c_k;
<klr>(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x435;&#x439;&#x431;&#x430;</trd>,
    <trd>&#x43A;&#x43B;&#x43E;&#x447;&#x447;&#x430;&#x441;&#x442;&#x435; &#x434;&#x435;&#x440;&#x435;&#x432;&#x43E;</trd>
  </trdgrp>
</drv>
</art>

