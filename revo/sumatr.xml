<drv mrk="sumatr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La plej okcidenta <ref tip="lst" lst="voko:insuloj" cel="insul.0o.GEOG">insulo</ref>
      de <ref tip="malprt" cel="indonezi.I0o">Indonezio</ref>, kaj la sesa plej granda de la mondo:
      <ekz>
        la tertremo kaj cunamo en <tld/>o en 2004
        <fnt>
          <bib>Monato</bib>
          <aut>Gotoo Hitosi</aut>
          <vrk><url ref="&Monato;/2011/010354.php">Frustri&gcirc;o de mar&gcirc;ena
          suferanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ielo de Tajlando estas kovrita de fumo pro <klr>[&#x2026;]</klr> arbarbrulego en
        <tld/>o kaj Borneo
        <fnt>
          <bib>Monato</bib>
          <aut>Syauqi Ahmad Zulfauzi Stya Lacksana</aut>
          <vrk><url ref="&Monato;/2015/011592.php">Kiam fini&gcirc;os la fumego?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_S;&c_u;&c_m;&c_a;&c_t;&c_r;&c_a;</trd>
    <trd lng="de">Sumatra</trd>
    <trd lng="en">Sumatra</trd>
    <trd lng="fr">Sumatra</trd>
  </snc>
  <trd lng="ja">&#x30B9;&#x30DE;&#x30C8;&#x30E9;&#x5CF6;  [<ind>&#x30B9;&#x30DE;&#x30C8;&#x30E9;&#x3057;&#x307E;</ind>]</trd>
  <trd lng="uk">&c_S;&c_u;&#x43C;&c_a;&c_t;&c_r;&c_a;</trd>
</drv>

