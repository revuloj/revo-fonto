<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rumor.xml,v 1.7 2022/03/26 19:19:21 revo Exp $">
<kap>
  <rad>rumor</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rumor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">POE</uzo>
  <snc mrk="rumor.0i.SON">
    <dif>
      Konfuze <ref tip="super" cel="bru.0i.soni">brui</ref>, kiel
      homamaso, akvo sur &scirc;tonoj, vagonaro:
      <ekz>
        la rabistoj tuj ek<tld/>is: &leftquot;jes, li devas i&gcirc;i nia metilernanto!&rightquot;
        <fnt><bib>RabistFab</bib></fnt>.
      </ekz>
      <ekz>
        ni mortas sen fanfarmuziko, sen la ridado de belaj virinoj, sen la apla&ubreve;da
        <tld/>ado de homamaso en festa eksciti&gcirc;o
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="klak.0i">klaki</ref>,
      <ref cel="murmur.0i.brui">murmuri<sncref/></ref>,
      <ref cel="tumult.0i">tumulti</ref>
    </refgrp>
    <trd lng="de">rumoren</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>bourdonner</trd>,
    <trd>bruire</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="rumor.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc mrk="rumor.0o.SON">
    <dif>
      <ref tip="super" cel="bru.0o.sonaro">Bruo</ref> farata de multaj samtempe:
      <ekz>
        maltrankviliga <tld/>o disvasti&gcirc;is, <klr>[&#x2026;]</klr> &jcirc;us, partianoj de <nom>Jorge
        Alessandri</nom> subeniradis <klr>[&#x2026;]</klr> meze de hupa koncerto
        <fnt>
          <bib>MD08</bib>
          <vrk>Venka vespero</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tumult.0o">tumulto</ref>
    <trd lng="de">Rumor</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>halas</trd>,
    <trd>hluk</trd>,
    <trd>povyk</trd>,
    <trd>r&aacute;mus</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>brouhaha</trd>,
    <trd>rumeur<klr tip="amb"> (bruit confus)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>zumbir</trd>,
    <trd>rumorejar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_u;&c_l;</trd>,
    <trd>&c_g;&c_a;&c_m;</trd>,
    <trd>&c_g;&c_o;&c_m;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hluk</trd>,
    <trd>r&aacute;mus</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x433;&#x443;&#x43B;</trd>,
    <trd>&#x433;&#x430;&#x43C;&#x456;&#x440;</trd>,
    <trd>&#x433;&#x43E;&#x43C;&#x456;&#x43D;</trd>,
    <trd>&#x433;&#x430;&#x43B;&#x430;&#x441;</trd>
  </trdgrp>
</drv>
</art>

