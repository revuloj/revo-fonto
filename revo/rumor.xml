<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rumor.xml,v 1.11 2025/04/05 10:02:33 revo Exp $">
<kap>
  <rad>rumor</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rumor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">POE</uzo>
  <snc mrk="rumor.0i.SON">
    <dif>
      Konfuze <ref tip="super" cel="bru.0i.soni">brui</ref>, kiel
      homamaso, akvo sur &scirc;tonoj, vagonaro:
      <ekz>
        la rabistoj tuj ek<tld/>is: &leftquot;jes, li devas i&gcirc;i nia metilernanto!&rightquot;
        <fnt><bib>RabistFab</bib></fnt>.
      </ekz>
      <ekz>
        ni mortas sen fanfarmuziko, sen la ridado de belaj virinoj, sen la apla&ubreve;da
        <tld/>ado de homamaso en festa eksciti&gcirc;o
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="klak.0i">klaki</ref>,
      <ref cel="murmur.0i.brui">murmuri<sncref/></ref>,
      <ref cel="tumult.0i">tumulti</ref>
    </refgrp>
    <trd lng="de">rumoren</trd>
    <trdgrp lng="zh">
      <trd>&#21927;&#22179; <pr>xu&amacron;nxi&amacron;o</pr></trd>,
      <trd>&#21927;&#22210; <pr>xu&amacron;nxi&amacron;o</pr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_m;&c_a;&c_n;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_u;&c_m;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bourdonner</trd>,
    <trd>bruire</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>robi&cacute;  <ind>rumor</ind></trd>,
    <trd>robi&cacute; ha&lstroke;as</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_o;&c_n;&c_i;&c_t;&c_mol;</trd>
</drv>

<drv mrk="rumor.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">POE</uzo>
  <snc mrk="rumor.0o.SON">
    <dif>
      <ref tip="super" cel="bru.0o.sonaro">Bruo</ref> farata de multaj samtempe:
      <ekz>
        maltrankviliga <tld/>o disvasti&gcirc;is, <klr>[&#8230;]</klr> &jcirc;us, partianoj de <nom>Jorge
        Alessandri</nom> subeniradis <klr>[&#8230;]</klr> meze de hupa koncerto
        <fnt>
          <bib>MD08</bib>
          <vrk>Venka vespero</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tumult.0o">tumulto</ref>
    <trd lng="de">Rumor</trd>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>bruo</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_g;&c_o;&c_m;&c_a;&c_n;</trd>,
    <trd>&c_sh;&c_u;&c_m;</trd>,
    <trd>&c_p;&c_o;&c_sh;&c_u;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>halas</trd>,
    <trd>hluk</trd>,
    <trd>povyk</trd>,
    <trd>r&aacute;mus</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>brouhaha</trd>,
    <trd>rumeur<klr tip="amb"> (bruit confus)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rumor</trd>,
    <trd>szum</trd>,
    <trd>ha&lstroke;as</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>zumbir</trd>,
    <trd>rumorejar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_u;&c_l;</trd>,
    <trd>&c_g;&c_a;&c_m;</trd>,
    <trd>&c_g;&c_o;&c_m;&c_o;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hluk</trd>,
    <trd>r&aacute;mus</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_u;&#1083;</trd>,
    <trd>&c_g;&c_a;&#1084;&c_ib;&c_r;</trd>,
    <trd>&c_g;&#1086;&#1084;&c_ib;&#1085;</trd>,
    <trd>&c_g;&c_a;&#1083;&c_a;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21927;&#21719; <pr>xu&amacron;nhu&#225;</pr></trd>,
    <trd>&#21927;&#22057; <pr>xu&amacron;nhu&#225;</pr></trd>,
    <trd>&#21927;&#33150; <pr>xu&amacron;nt&#233;ng</pr></trd>,
    <trd>&#21927;&#39472; <pr>xu&amacron;nt&#233;ng</pr></trd>
  </trdgrp>
</drv>
</art>

