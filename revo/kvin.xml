<drv mrk="kvin.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="kvin.0o.nombro">
    <dif>
      La nombro <tld/>.
    </dif>
    <trd lng="de">F&uuml;nf</trd>
    <trd lng="pl">pi&aogonek;tka</trd>
  </snc>
  <snc mrk="kvin.0o.kvinopo">
    <dif>
      <tld lit="K"/> aferoj kune:
      <ekz>
        <tld/>o da infanoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 2 (100)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Izabela</nom> alvenis kun <tld/>o da fotoj
        <fnt>
          <bib>ChC</bib>
          <lok>16</lok>
        </fnt>;
      </ekz>
      <ekz>
        nur anta&ubreve; <tld/>o da jaroj ekaperis en Esperanto la unuaj laosaj literatura&jcirc;oj
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni legas dek-<tld/>on da lokaj historioj
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2011/010155.php"
              >Admirinda historilibro</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        sekvis silento dum <tld/>o da sekundoj
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 7 (57)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kvin.0opo">kvinopo</ref>,
      <ref cel="kvinte1.0o">kvinteto</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>F&uuml;nfer</trd>,
      <trd>Quintett</trd>
    </trdgrp>
    <trd lng="pl">pi&aogonek;tka</trd>
  </snc>
</drv>

<drv mrk="kvin.0ono">
<kap><tld/>ono</kap>
  <snc>
    <dif>
      Kvina parto:
      <ekz>
        ok estas kvar <tld/>onoj de dek
        <fnt>
          <bib>F</bib>
          <lok>&FE; 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu tago estas tricent-sesdek-<tld/>ono <klr>[&#8230;]</klr> de jaro
        <fnt>
          <bib>F</bib>
          <lok>&FE; 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam vi havos rikolton, vi donos <tld/>onon al Faraono
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 47:24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">pempvedenn</trd>
  <trd lng="cs">p&ecaron;tina</trd>
  <trd lng="de">F&uuml;nftel</trd>
  <trdgrp lng="fi">
     <trd>viidennes</trd>,
     <trd>viidesosa</trd>
  </trdgrp>
  <trdgrp lng="be">
    <trd>&c_p;&c_ja;&c_t;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_d;&c_n;&c_a; <ind>&c_p;&c_ja;&c_t;&c_a;&c_ja;</ind></trd>
  </trdgrp>
  <trd lng="fr">cinqui&egrave;me</trd>
  <trd lng="hu">&ouml;t&ouml;d</trd>
  <trd lng="id">seper<ind>lima</ind></trd>
  <trd lng="ja">&#20116;&#20998;&#12398;&#19968;  <pr>&#12372;&#12406;&#12435;&#12398;&#12356;&#12385;</pr></trd>
  <trd lng="nl">vijfde <klr tip="ind"> (deel)</klr></trd>
  <trd lng="pl">jedna <klr tip="amb">pi&aogonek;ta</klr></trd>
  <trd lng="ro">al <klr tip="amb">cincilea</klr></trd>
  <trd lng="ru">&c_p;&c_ja;&c_t;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">p&auml;tina</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_d;&c_n;&c_a; &c_p;&#8217;&c_ja;&c_t;&c_a;</trd>,
    <trd>&c_p;&#8217;&c_ja;&c_t;&c_a; &c_ch;&c_a;&c_s;&c_t;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#20116;&#20998;&#20043;&#19968; <pr>w&#468;f&emacron;nzh&imacron;y&imacron;</pr></trd>
</drv>

<drv mrk="kvin.0opo">
  <kap><tld/>opo</kap>
  <snc>
    <dif>
      Grupo de <tld/> aferoj:
      <ekz>
        oni a&ccirc;etis <tld/>opon da loteriaj biletoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk>Lasta spiro, lasta espero</vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>opo da turistoj, spiregantaj pro la kruta vojo
        <fnt>
          <bib>ChB</bib>
          <lok>1</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la kantino sidis <tld/>opo laborista
        <fnt>
          <bib>ChB</bib>
          <lok>17</lok>
        </fnt>;
      </ekz>
      <ekz>
        la slava skolo <klr>[&#8230;]</klr> havas gvidan <tld/>opon, konsistantan el
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
   <trd>&c_p;&c_ja;&c_c;&c_jo;&c_r;&c_k;&c_a;</trd>,
   <trd>&c_g;&c_r;&c_u;&c_p;&c_a; &c_z;&c_mol; <ind>&c_p;&c_ja;&c_c;&c_ib;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>F&uuml;nfer</trd>,
    <trd>Quintett</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>quinte</trd>,
    <trd>groupe de cinq</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20116;&#20154;&#36899;&#12428;  <pr>&#12356;&#12388;&#12426;&#12388;&#12428;</pr></trd>,
    <trd>&#20116;&#12388;&#32068;  <pr>&#12356;&#12388;&#12388;&#12367;&#12415;</pr></trd>
  </trdgrp>
  <trd lng="pl">pi&aogonek;tka</trd>
  <trd lng="ro">cinci</trd>
  <trd lng="uk">&c_p;&#8217;&c_ja;&c_t;&c_ib;&c_r;&c_k;&c_a; (&c_g;&c_r;&c_u;&c_p;&c_a; &c_z; 5 &c_o;&c_s;&c_ib;&c_b;)</trd>
</drv>

<drv mrk="kvin.0ope">
