<drv mrk="kvin.0a">
<kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="K"/>aranga, venanta post kvar anta&ubreve;aj:
      <ekz>
        hodia&ubreve; estas la dek <tld/>a (tago) de Aprilo
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la sekva, <tld/>a, parto la oratoro refutas la argumentojn de la kontra&ubreve;ulo
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi prenas la <tld/>an glason
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi uzu la vagonaron, kiu pasas je la <tld/>a <klr>(horo)</klr> kaj dudek
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la dek-<tld/>a tago de tiu monato estas la festo
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 23:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a rado &ccirc;e veturado
        <klr>(apena&ubreve; ne bezonata)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ja">
    <trd>&#65301;&#30058;&#30446;&#12398;  <pr>&#12356;&#12388;&#12400;&#12435;&#12417;&#12398;</pr></trd>,
    <trd>&#31532;&#20116;&#12398;  <pr>&#12384;&#12356;&#12356;&#12388;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="lat">quintus</trd>
  <trd lng="be">&c_p;&c_ja;&c_t;&c_y;</trd>
  <trdgrp lng="br">
   <trd>pempvet</trd>,
   <trd>pempet</trd>
</trdgrp>
  <trd lng="ca">cinqu&egrave;</trd>
  <trd lng="cs">p&aacute;t&yacute;</trd>
  <trd lng="de">f&uuml;nfter</trd>
  <trd lng="fi">viides</trd>
  <trd lng="fr">cinqui&egrave;me</trd>
  <trd lng="hu">&ouml;t&ouml;dik</trd>
  <trd lng="id">ke<ind>lima</ind></trd>
  <trd lng="nl">vijfde</trd>
  <trd lng="oc">cinquen</trd>
  <trdgrp lng="pl">
    <trd>pi&aogonek;ty</trd>,
    <trd>pi&aogonek;ta</trd>,
    <trd>pi&aogonek;te</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>cincilea</trd>,
    <trd>cincime</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_ja;&c_t;&c_y;&c_j;</trd>
