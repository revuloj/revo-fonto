      En grupo de <tld/>:
      <ekz>
        <tld/>ope ili tiris la keston kaj tamen ne povis &gcirc;in altiri al la domo
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        iru kun mi, <tld/>ope ni trairos la tutan mondon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
   <trd>&c_u;&c_p;&c_ja;&c_c;&c_jo;&c_h;</trd>,
   <trd>&c_u;&c_p;&c_je;&c_c;&c_ja;&c_r;&c_y;&c_h;</trd>,
   <trd>&c_p;&c_ja;&c_c;&c_jo;&c_r;&c_k;&c_a;&c_j;</trd>
  </trdgrp>
  <trd lng="de">zu <ind>f&uuml;nft</ind></trd>
  <trdgrp lng="fr">
    <trd>par groupe de cinq</trd>,
    <trd>&agrave; cinq</trd>
  </trdgrp>
  <trd lng="id">ber<ind>lima</ind></trd>
  <trdgrp lng="ja">
    <trd>&#20116;&#20154;&#36899;&#12428;&#12391;  <pr>&#12356;&#12388;&#12426;&#12388;&#12428;&#12391;</pr></trd>,
    <trd>&#20116;&#12388;&#32068;&#12391;  <pr>&#12356;&#12388;&#12388;&#12367;&#12415;&#12391;</pr></trd>
  </trdgrp>
