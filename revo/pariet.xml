<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pariet.xml,v 1.21 2024/02/16 19:24:11 revo Exp $">
<kap>
  <rad>pariet</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pariet.0">
  <kap><tld/>o</kap>
  <uzo tip="fak">BIO</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;ferma&jcirc;o de organa kava&jcirc;o:
      <ekz>
       &ccirc;iu plantspecio produktas polenon
        rekoneblan la&ubreve; &gcirc;ia dimensio, &gcirc;ia formo kaj
        la&ubreve; la ornamado de &gcirc;ia interna <tld/>o
        <fnt><aut>Robert Roze</aut>:
        <vrk><url ref="&Monato;/005775.html">Vivisekcio de
          polenero</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 11/1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj de kranio, de stomako.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vand.0o.ANA">vando<sncref/></ref>,
      <ref cel="sept1.0o">septo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_je;&c_n;&c_k;&c_a; <klr tip="amb">(&c_u;&c_n;&c_u;&c_t;&c_r;&c_a;&c_n;&c_a;&c_g;&c_a; &c_v;&c_o;&c_r;&c_g;&c_a;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="cs">
    <trd>pod&scaron;&iacute;vka</trd>,
    <trd>rub</trd>,
    <trd>vnit&rcaron;n&iacute; strana</trd>
  </trdgrp>
  <trd lng="de">Organwand</trd>
  <trd lng="fr">paroi</trd>
  <trd lng="hu">fal<klr tip="amb"> (szerv&eacute;)</klr></trd>
  <trd lng="it">parete<klr> (anat.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x5185;&#x58C1;  <pr>&#x306A;&#x3044;&#x3078;&#x304D;</pr></trd>,
    <trd>&#x58C1;  <pr>&#x304B;&#x3079;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_k;&c_a; <klr tip="amb">(&c_o;&c_r;&c_g;&c_a;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="sk">
    <trd>rub</trd>,
    <trd>vn&uacute;torn&aacute; strana</trd>
  </trdgrp>
  <trd lng="uk">&c_v;&c_n;&c_u;&c_t;&c_r;&c_ib;&c_sh;&c_n;&c_ja; &c_s;&c_t;&c_ib;&c_n;&c_k;&c_a;</trd>
</drv>

<drv mrk="pariet.0osto">
  <kap><tld/>osto
  <fnt><bib>PV</bib></fnt>
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      &Ccirc;iu el la du <ref tip="super" cel="ost.0o">osto</ref>j de la
      homa <ref tip="malprt" cel="skelet.0o.ANA">skeleto</ref>, formantaj la
      flankojn kaj la supron de la <ref tip="malprt" cel="krani.0o">kranio</ref>.
    </dif>
    <ref tip="sin" cel="parietal.0o">parietalo</ref>
    <refgrp tip="vid">
      <ref cel="frunt.0osto">fruntosto</ref>,
      <ref cel="okcipi.0osto">okcipitosto</ref>,
      <ref cel="temporal.0o">temporalo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_je;&c_m;&c_ja;&c_n;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="cs">kost temenn&iacute;</trd>
  <trd lng="de">Scheitelbein</trd>
  <trd lng="fr">os <ind>parietal</ind></trd>
  <trd lng="hu">falcsont</trd>
  <trd lng="it">osso <ind>parietale</ind></trd>
  <trd lng="ja">&#x982D;&#x9802;&#x9AA8;  <pr>&#x3068;&#x3046;&#x3061;&#x3087;&#x3046;&#x3053;&#x3064;</pr></trd>
  <trd lng="nl">wandbeen</trd>
  <trd lng="pl">ko&sacute;&cacute; ciemieniowa</trd>
  <trd lng="ru">&c_t;&c_je;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja;
&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">temenn&aacute; kos&tcaron;</trd>
  <trd lng="uk">&c_t;&c_ib;&c_m;&#x2019;&c_ja;&c_n;&c_a; &c_k;&c_ib;&c_s;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x9802;&#x9AA8; <pr>d&#x1D0;ngg&#x1D4;</pr></trd>,
    <trd>&#x9876;&#x9AA8; <pr>d&#x1D0;ngg&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>
