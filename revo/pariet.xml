<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pariet</rad>/o
</kap>
<drv mrk="pariet.0">
  <kap><tld/>o</kap>
  <uzo tip="fak">BIO</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;ferma&jcirc;o de organa kava&jcirc;o:
      <ekz>
        Fakte, oni prave diras, ke la polenero korespondas specife al
        &leftquot;fingrospuro&rightquot; de la planto, &ccirc;ar &ccirc;iu
plantspecio produktas polenon
        rekoneblan la&ubreve; &gcirc;ia dimensio, &gcirc;ia formo kaj
la&ubreve; la ornamado de &gcirc;ia
        interna <tld/>o
        <fnt><aut>Robert ROZE</aut>:
        <vrk><url ref="&Monato;/005775.html">Vivisekcio de
polenero</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 11/1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj de kranio, de stomako.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_c;&c_je;&c_n;&c_k;&c_a; <klr tip="amb">(&c_u;&c_n;&c_u;&c_t;&c_r;&c_a;&c_n;&c_a;&c_g;&c_a; &c_v;&c_o;&c_r;&c_g;&c_a;&c_n;&c_a;)</klr></trd>
  <trd lng="hu">fal<klr tip="amb"> (szerv&eacute;)</klr></trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_k;&c_a; <klr tip="amb">(&c_o;&c_r;&c_g;&c_a;&c_n;&c_a;)</klr></trd>
</drv>

<drv mrk="pariet.0osto">
  <kap><tld/>osto</kap>
  <uzo tip="fak">ANA</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      &Ccirc;iu el la du <ref tip="super" cel="ost.0o">osto</ref>j de la
homa
      <ref tip="malprt" cel="skelet.0o.ANA">skeleto</ref>, formantaj la
      flankojn kaj la supron de la <ref tip="malprt"
      cel="krani.0o">kranio</ref>.
    </dif>
    <ref tip="sin" cel="parietal.0o">parietalo</ref>
    <refgrp tip="vid">
      <ref cel="frunt.0osto">fruntosto</ref>,
      <ref cel="okcipi.0osto">okcipitosto</ref>,
      <ref cel="temporal.0o">temporalo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Scheitelbein</trd>
  <trd lng="hu">falcsont</trd>
  <trd lng="nl">wandbeen</trd>
  <trd lng="ru">&c_t;&c_je;&c_m;&c_je;&c_n;&c_n;&c_a;&c_ja;
&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
