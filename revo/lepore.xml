<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lepore.xml,v 1.12 2024/01/15 16:14:09 revo Exp $">
<kap>
  <rad>lepored</rad>/oj
</kap>
<drv mrk="lepore.0oj">
  <kap>
    <tld/>oj
  </kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo <ref tip="malprt" cel="lepor.0uloj">leporuloj</ref>
      (<trd lng="la">Leporidae</trd>):
      <ekz>
        la Flavgor&gcirc;a leporo a&ubreve; Leporo de Tehuantepeko estas specio
        <klr>[&#x2026;]</klr> de la
        familio de <tld lit="L"/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=293531">Flavgor&gcirc;a leporo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
       <ref cel="kunikl.0o">kuniklo</ref>,
       <ref cel="lepor.0o">leporo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_j;&c_c;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Hasen<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">L&eacute;porid&eacute;s</trd>
  <trd lng="hu">ny&uacute;lf&eacute;l&eacute;k</trd>
  <trd lng="nl">hazen en konijnen<klr tip="ind"> (fam.)</klr></trd>
  <trd lng="pl">zaj&aogonek;cowate</trd>
  <trd lng="pt">lepor&iacute;deos</trd>
  <trd lng="ro">leporide</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_ja;&c_ch;&c_mol;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_j;&c_c;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="zh">&#x5154;&#x79D1; <pr>t&#xF9;k&#x113;</pr></trd>
</drv>
</art>
