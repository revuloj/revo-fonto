<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sublimat</rad>/o
</kap>
<drv mrk="sublim2.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Substanco vaporigita kaj poste ricevita en solida stato.
    </dif>
    <trd lng="be">&c_s;&c_u;&c_b;&c_l;&c_ib;&c_m;&c_a;&c_t;</trd>
    <trd lng="de">Sublimat</trd>
    <trd lng="nl">sublimaat</trd>
    <trd lng="ru">&c_s;&c_u;&c_b;&c_l;&c_i;&c_m;&c_a;&c_t;</trd>
  </snc>
  <snc>
    <dif>
      Hidrarga biklorido. HgCl<sub>2</sub>.
    </dif>
    <trd lng="be">&c_s;&c_u;&c_l;&c_je;&c_m;&c_a;</trd>
    <trd lng="ru">&c_s;&c_u;&c_l;&c_je;&c_m;&c_a;</trd>
  </snc>
</drv>
</art>
