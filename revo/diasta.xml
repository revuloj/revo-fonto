<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diasta.xml,v 1.17 2023/08/31 10:44:00 revo Exp $">
<kap>
  <rad>diastaz</rad>/o
</kap>
<drv mrk="diasta.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="fermen.0o">Fermento</ref>, kiu aliigas amelon en dekstrinon,
      nuntempe nomata amelazo:
      <ekz>
        amelazo estas enzimo havanta fiziologian, komercan, kaj historian signifon, &gcirc;i
        anka&ubreve; nomi&gcirc;as <tld/>o, kiu estas miksa&jcirc;o de amelazoj de malto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=148">Amelazo</url></lok>
        </fnt>.
      </ekz> 
    </dif>
    <adm>Aldonu artikolon amelazo</adm>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_s;&c_t;&c_a;&c_z;&c_a;</trd>
  <trd lng="ca">diastasa</trd>
  <trdgrp lng="cs">
    <trd>amyl&aacute;za</trd>,
    <trd>diast&aacute;za</trd>
  </trdgrp>
  <trd lng="de">Diastase</trd>
  <trd lng="es">diastasa</trd>
  <trd lng="fr">diastase</trd>
  <trd lng="he">&#x5EA;&#x5E1;&#x5E1;</trd>
  <trdgrp lng="hu">
      <trd>diaszt&aacute;z</trd>,
      <trd>anil&aacute;z</trd>
    </trdgrp>
  <trd lng="ja">&#x30B8;&#x30A2;&#x30B9;&#x30BF;&#x30FC;&#x30BC;</trd>
  <trd lng="pl">diastaza</trd>
  <trd lng="pt">di&aacute;stase</trd>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_s;&c_t;&c_a;&c_z;&c_a;</trd>
  <trd lng="sk">diast&aacute;za</trd>
  <trd lng="uk">&#x434;&#x456;&#x430;&#x441;&#x442;&#x430;&#x437;</trd>
</drv>
</art>
