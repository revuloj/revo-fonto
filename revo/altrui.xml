  </trdgrp>
  <trd lng="fr">altruiste<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="he">&#1488;&#1500;&#1496;&#1512;&#1493;&#1488;&#1497;&#1496;&#1497;</trd>
  <trd lng="it">altruistico</trd>
  <trd lng="ja">&#21033;&#20182;&#30340;&#12394;  <pr>&#12426;&#12383;&#12390;&#12365;&#12394;</pr></trd>
  <trd lng="pl">altruistyczny</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_t;&c_r;&c_u;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>altruistick&#253;</trd>,
    <trd>nesebeck&#253;</trd>,
    <trd>nezi&#353;tn&#253;</trd>
  </trdgrp>
  <trd lng="uk">&#1072;&#1083;&#1100;&#1090;&#1088;&#1091;&#1111;&#1089;&#1090;&#1080;&#1095;&#1085;&#1080;&#1081;</trd>
</drv>
