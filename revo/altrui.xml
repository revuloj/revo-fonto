<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>altruism</rad>/o
</kap>
<drv mrk="altrui.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malegoismo:
      <ekz>
        &gcirc;i [la letero al Borovko] prezentis la a&ubreve;toron de Esperanto kiel
        <tld/>an batalanton por interkompreni&gcirc;o trans &ccirc;iujn naciajn
        antagonismojn <fnt>(<aut>Ulrich Lins</aut>: <vrk>La dan&gcirc;era
        lingvo</vrk>, <lok>1.1</lok>)</fnt>.
      </ekz> Kp. <ref tip="vid" cel="altruist.0o">altruisto</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Altruismus</trd>,
     <trd>Selbstlosigkeit</trd>,
     <trd>Uneigenn&uuml;tzigkeit</trd>
  </trdgrp>
  <trd lng="en">altruism</trd>
  <trd lng="es">altruismo</trd>
  <trd lng="fr">altruisme</trd>
  <trd lng="nl">altru&iuml;sme</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_t;&c_r;&c_u;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="altrui.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Malegoisma.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>altruistisch</trd>,
     <trd>selbstlos</trd>,
     <trd>uneigenn&uuml;tzig</trd>
  </trdgrp>
</drv>

</art>
