<kap>
  <rad>altruism</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="altrui.0o">
  <kap><tld/>o<fnt><bib>PV</bib></fnt></kap>
  <snc mrk="altrui.0o.FIL">
    <uzo tip="fak">FIL</uzo>  
    <ref tip="dif" cel="ego.mal0ismo.PSI">malegoismo</ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>bonfarado</ke>.
  </rim>

  <ref tip="vid" cel="altruist.0o">altruisto</ref>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_t;&c_r;&c_u;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_a;&c_l;&c_t;&c_r;&c_u;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Altruismus</trd>,
    <trd>Selbstlosigkeit</trd>,
    <trd>Uneigenn&uuml;tzigkeit</trd>
  </trdgrp>
  <trd lng="en">altruism</trd>
  <trd lng="es">altruismo</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_w;&a_ayn;&zwnj;&a_d;&a_w;&a_s;&a_t;&f_y;</trd>,
    <trd>&a_A;&a_z;&a_kh;&a_w;&a_d;&f_g;&a_dh;&a_sh;&a_t;&f_g;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">altruisme</trd>
  <trd lng="he">&#1488;&#1500;&#1496;&#1512;&#1493;&#1488;&#1497;&#1494;&#1501;</trd>
  <trdgrp lng="hu">
    <trd>&ouml;nzetlens&eacute;g</trd>,
    <trd>altruizmus</trd>
  </trdgrp>
  <trd lng="it">altruismo</trd>
  <trd lng="ja">&#21033;&#20182;&#20027;&#32681;  <pr>&#12426;&#12383;&#12375;&#12421;&#12366;</pr></trd>
  <trd lng="nl">altru&iuml;sme</trd>
  <trd lng="pl">altruizm</trd>
  <trd lng="ru">&c_a;&c_l;&c_mol;&c_t;&c_r;&c_u;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">altruizmus</trd>
  <trd lng="uk">&#1072;&#1083;&#1100;&#1090;&#1088;&#1091;&#1111;&#1079;&#1084; (&#1073;&#1077;&#1079;&#1082;&#1086;&#1088;&#1080;&#1089;&#1083;&#1080;&#1074;&#1077; &#1087;&#1110;&#1082;&#1083;&#1091;&#1074;&#1072;&#1085;&#1085;&#1103; &#1087;&#1088;&#1086; &#1073;&#1083;&#1072;&#1075;&#1086; &#1110;&#1085;&#1096;&#1080;&#1093;)</trd>
</drv>

<drv mrk="altrui.0a">
  <kap><tld/>a</kap>
  <snc mrk="altrui.0a.FIL">
    <dif>
      Malegoisma:
      <ekz>
        <klr>[la letero al Borovko]</klr> prezentis la a&ubreve;toron de
        Esperanto kiel <tld/>an batalanton por
        interkompreni&gcirc;o
        trans &ccirc;iujn naciajn antagonismojn
        <fnt><bib>Dan&gcirc;Lng</bib>, <lok>1.1</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="ego.0isma">egoisma.</ref>
    <trdgrp lng="zh">
    <trd>&#19981;&#39015;&#33258;&#24049; <pr>b&#249;g&#249;z&#236;j&#464;</pr></trd>,
    <trd>&#19981;&#39038;&#33258;&#24049; <pr>b&#249;g&#249;z&#236;j&#464;</pr></trd>,
    <trd>&#19981;&#35584;&#21462;&#31169;&#21033; <pr>b&#249;m&#243;uq&#468;s&#299;l&#236;</pr></trd>,
    <trd>&#19981;&#35851;&#21462;&#31169;&#21033; <pr>b&#249;m&#243;uq&#468;s&#299;l&#236;</pr></trd>,
    <trd>&#21033;&#20182; <pr>l&#236;t&#257;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_t;&c_r;&c_u;&c_ib;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>altruistick&#253;</trd>,
    <trd>altruistn&#237;</trd>,
    <trd>nesobeck&#253;</trd>,
    <trd>nezi&#353;tn&#253;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>altruistisch</trd>,
    <trd>selbstlos</trd>,
    <trd>uneigenn&uuml;tzig</trd>
  </trdgrp>
  <trd lng="en">altruistic</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_w;&a_ayn;&zwnj;&a_d;&a_w;&a_s;&a_t;</trd>,
    <trd>&a_A;&a_z;&a_kh;&a_w;&a_d;&f_g;&a_dh;&a_sh;&a_t;&a_h;</trd>,
    <trd>&a_A;&f_y;&a_th;&a_A;&a_r;&f_g;&a_r;</trd>,
