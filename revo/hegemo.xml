<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>hegemoni</rad>/o
</kap>
<drv mrk="hegemo.0o">
  <kap><tld/>o</kap>
  <snc mrk="hegemo.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Superregeco de iu popolo, homgrupo ks:
      <ekz>
        bataladi pri <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="domin.0o.KOMUNE">domino</ref>,
      <ref cel="mult.pli0o.POL">plimulto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_g;&c_je;&c_m;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Hegemonie</trd>,
    <trd>Vormacht</trd>
  </trdgrp>
  <trd lng="el">&eta;&gamma;&epsilon;&my;&omikron;&ny;&jota_ton;&alfa;</trd>
  <trd lng="es">hegemon&iacute;a</trd>
  <trdgrp lng="en">
    <trd>hegemony</trd>,
    <trd>dominance</trd>,
    <trd>supremacy</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>domination</trd>,
    <trd>h&eacute;g&eacute;monie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hegem&oacute;nia</trd>,
    <trd>hatalmi f&ouml;l&eacute;ny</trd>,
    <trd>vezet&odblac; szerep</trd>
  </trdgrp>
  <trd lng="nl">hegemonie</trd>
  <trd lng="pt">hegemonia</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_g;&c_je;&c_m;&c_o;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="hegemo.0i">
  <kap><tld/>i</kap>
  <snc mrk="hegemo.0i.POL">
    <dif>
      <ref tip="super" cel="reg.super0i">Superregi</ref> super aliaj 
      homgrupoj:
      <ekz>
        en la pasinteco ofte religioj uzis la perforton por <tld/>i
        <fnt>
          <aut>E. Pauli</aut>,
          <vrk><url ref="http://www.cfh.ufsc.br/~simpozio/">Enciklopedio
          simpozio</url></vrk>,
          <lok>artikolo &leftquot;hegemonio&rightquot;, 1996-05-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">dominate</trd>
  <trd lng="fr">dominer</trd>
</drv>
</art>
