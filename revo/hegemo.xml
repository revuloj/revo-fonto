<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hegemo.xml,v 1.22 2022/09/22 08:43:15 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>hegemoni</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hegemo.0o">
  <kap><tld/>o</kap>
  <snc mrk="hegemo.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Superregeco de iu popolo, homgrupo ks:
      <ekz>
        la korporacio de la bur&gcirc;oj-patricioj kaj la korporacio de la gildoj
        <klr>[&#8230;]</klr> reciproke bataladis pri <tld/>o
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        rakontante la batalojn de niaj prauloj, per kiuj ili fondis la nunan
        mond<tld/>on de mia patrio
        <fnt>
          <bib>VaK</bib>
          <lok>Kvina &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        al NATO <klr>[&#8230;]</klr> tiom da landoj, kiuj &gcirc;is disfalo de komunismo vivis sub
        sovetia <tld/>o, nun deziras ali&gcirc;i
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007003.html"
              >Internaciaj Rilatoj: El flamo en fajron</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        nuntempa <tld/>o de la angla lingvo
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 2 (64)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="domin.0o.KOMUNE">domino</ref>,
      <ref cel="monopo.0o">monopolo</ref>,
      <ref cel="mult.pli0o.POL">plimulto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_g;&c_je;&c_m;&c_o;&c_n;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>hegemonie</trd>,
    <trd>nadvl&aacute;da</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hegemonie</trd>,
    <trd>Vormacht</trd>
  </trdgrp>
  <trd lng="el">&eta;&gamma;&epsilon;&my;&omikron;&ny;&jota_ton;&alfa;</trd>
  <trdgrp lng="en">
    <trd>hegemony</trd>,
    <trd>dominance</trd>,
    <trd>supremacy</trd>
  </trdgrp>
  <trd lng="es">hegemon&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>domination</trd>,
    <trd>h&eacute;g&eacute;monie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hegem&oacute;nia</trd>,
    <trd>hatalmi f&ouml;l&eacute;ny</trd>,
    <trd>vezet&odblac; szerep</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>egemonia</trd>,
    <trd>supremazia</trd>
  </trdgrp>
  <trd lng="nl">hegemonie</trd>
  <trd lng="pl">hegemonia</trd>
  <trd lng="pt">hegemonia</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_je;&c_g;&c_je;&c_m;&c_o;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_g;&c_o;&c_s;&c_p;&c_o;&c_d;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">nadvl&aacute;da</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_je;&c_g;&c_je;&c_m;&c_o;&c_n;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_i;&c_l;&c_l;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="hegemo.0i">
  <kap><tld/>i</kap>
  <snc mrk="hegemo.0i.POL">
    <dif>
      <ref tip="super" cel="reg.super0i">Superregi</ref> super aliaj 
      homgrupoj:
      <ekz>
        en la pasinteco ofte religioj uzis la perforton por <tld/>i
        <fnt>
          <aut>E. Pauli</aut>,
          <vrk><url ref="http://www.cfh.ufsc.br/~simpozio/">Enciklopedio
          simpozio</url></vrk>,
          <lok>artikolo &leftquot;hegemonio&rightquot;, 1996-05-12</lok>
        </fnt>;
      </ekz>
      <ekz>
        ju pli <tld/>as la angla, des pli malfacili&gcirc;as la decido
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki LINS</aut>
          <vrk><url ref="https://www.monato.be/2004/008309.html"
              >Plurlingvismo valoras siajn kostojn</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_m;&c_ib;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>dominieren</trd>,
    <trd>die <ind>Vormachtstellung</ind> aus&uuml;ben</trd>
  </trdgrp>
  <trd lng="en">dominate</trd>
  <trd lng="fr">dominer</trd>
  <trd lng="it">dominare</trd>
  <trdgrp lng="pl">
    <trd>g&oacute;rowa&cacute;</trd>,
    <trd>panowa&cacute;</trd>
  </trdgrp>
</drv>
</art>
