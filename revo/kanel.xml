<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kanel</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kanel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <uzo tip="fak">ARKI</uzo>  
  <snc>
    <dif>
      Longa rekta ornama sulko. 
    </dif>
  </snc>
  <trd lng="fr">cannelure</trd>
  <trdgrp lng="nl">
    <trd>cannelure</trd>,
    <trd>groef</trd>,
    <trd>ribbel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_n;&c_n;&c_je;&c_l;&c_ju;&c_r;&c_a;</trd>,
    <trd>&c_zh;&c_je;&c_l;&c_o;&c_b;&c_o;&c_k;</trd>,
    <trd>&c_v;&c_y;&c_je;&c_m;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_o;&c_z;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_z;</trd>
  </trdgrp>
</drv>
<drv mrk="kanel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ornami per paralelaj <tld/>oj:
      <ekz><tld/>itaj kolonoj<fnt>Z</fnt>.</ekz>  
    </dif>
    <trd lng="fr">canneler</trd>
    <trdgrp lng="nl">
      <trd>canneleren</trd>,
      <trd>kartelen</trd>,
      <trd>ribbelen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol; &c_k;&c_a;&c_n;&c_n;&c_je;&c_l;&c_ju;&c_r;&c_y;</trd>
    </trdgrp>
</snc>
</drv>
</art>
