<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: varp.xml,v 1.19 2023/09/05 09:43:54 revo Exp $">
<kap>
  <rad>varp</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="varp.0o">
  <kap><tld/>o</kap>
  <snc mrk="varp.0o.TEKS">
    <uzo tip="fak">TEK</uzo>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Paralelaj fadenoj, inter kiujn teksisto intermetas
      la <ref tip="vid" cel="veft.0o">vefton</ref> por
      fari <ref tip="malprt" cel="teks.0ajxo.TEKS">teksa&jcirc;on</ref>:
      <ekz>
        sateno (fajna mola teksa&jcirc;o farita per neegala nodado de la vefto a&ubreve; la <tld/>o)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=69637">&Scirc;tofo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="teks.0bazo">teksbazo</ref>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_n;&c_o;&c_v;&c_a;</trd>
  <trd lng="ca">ordit</trd>
  <trd lng="cs">osnova</trd>
  <trdgrp lng="de">
    <trd>Kette <klr>(Weben)</klr></trd>,
    <trd>Zettel <klr>(Weben)</klr></trd>,
    <trd>Aufzug <klr>(Weben)</klr></trd>
  </trdgrp>
  <trd lng="en">warp</trd>
  <trd lng="fr">fils de chaine</trd>
  <trd lng="hu">l&aacute;nc <klr tip="ind">(sz&ouml;v&eacute;sn&eacute;l)</klr></trd>
  <trd lng="ja">&#x7E26;&#x7CF8;  [<ind>&#x305F;&#x3066;&#x3044;&#x3068;</ind>]</trd>
  <trd lng="nl">schering</trd>
  <trd lng="pl">osnowa<klr> (materia&lstroke;u)</klr></trd>
  <trd lng="pt">urdidura</trd>
  <trd lng="ru">&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;<klr tip="ind">
  (&c_t;&c_je;&c_k;&c_s;.)</klr></trd>
  <trd lng="sk">osnova</trd>
  <trd lng="uk">&#x43E;&#x441;&#x43D;&#x43E;&#x432;&#x430; (&#x442;&#x43A;&#x430;&#x43D;&#x438;&#x43D;&#x438;)</trd>
</drv>

<drv mrk="varp.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="varp.0i.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Aran&gcirc;i paralele la <tld/>ojn sur la teksma&scirc;ino
      anta&ubreve; la veftado.
    </dif>
    <trdgrp lng="de">
      <trd>aufb&auml;umen <klr>(Weben)</klr></trd>,
      <trd>zetteln <klr>(Weben)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_mol;
  <ind>&c_a;&c_s;&c_n;&c_o;&c_v;&c_u;</ind></trd>
  <trd lng="ca">ordir</trd>
  <trd lng="cs">vytvo&rcaron;it osnovu</trd>
  <trd lng="hu">felvet <klr tip="ind">(fonalat)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x7E26;&#x7CF8;&#x3092;&#x305D;&#x308D;&#x3048;&#x308B;  [<ind>&#x305F;&#x3066;&#x3044;&#x3068;&#x3092;&#x305D;&#x308D;&#x3048;&#x308B;</ind>]</trd>,
    <trd>&#x6574;&#x7D4C;&#x3059;&#x308B;  [<ind>&#x3068;&#x3068;&#x306E;&#x3078;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">scheren<klr tip="ind"> (op touw zetten)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol;
  <ind>&c_o;&c_s;&c_n;&c_o;&c_v;&c_u;</ind><klr tip="ind">
  (&c_t;&c_je;&c_k;&c_s;.)</klr></trd>
  <trd lng="sk">vytvori&tcaron; osnovu</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43D;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43D;&#x430;&#x431;&#x438;&#x440;&#x430;&#x442;&#x438;/&#x43D;&#x430;&#x432;&#x438;&#x432;&#x430;&#x442;&#x438; &#x43E;&#x441;&#x43D;&#x43E;&#x432;&#x443;</trd>
  </trdgrp>
</drv>

<drv mrk="varp.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="varp.0ero.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Unuopa fadeno de <tld/>o:
      <ekz>
        la navedo <klr>[&#x2026;]</klr> estas la&ubreve; la bufro batita tra la <tld/>ero kaj eniras je la alia flanko en samspecan navedujon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=399600">Navedo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_n;&c_o;&c_w;&c_n;&c_a;&c_ja;
  <ind>&c_n;&c_ib;&c_t;&c_k;&c_a;</ind></trd>
  <trd lng="ca">fil d'<ind>ordit</ind></trd>
  <trd lng="cs">nit osnovy</trd>
  <trd lng="de">Kettfaden</trd>
  <trd lng="fr">fil de chaine</trd>
  <trd lng="hu">l&aacute;ncfonal</trd>
  <trd lng="ja">&#x7E26;&#x7CF8;  [<ind>&#x305F;&#x3066;&#x3044;&#x3068;</ind>]</trd>
  <trd lng="nl">kettingdraad</trd>
  <trd lng="pl">ni&cacute; <ind>osnowy</ind></trd>
  <trd lng="ru">&c_o;&c_s;&c_n;&c_o;&c_v;&c_n;&c_a;&c_ja;
  &c_n;&c_i;&c_t;&c_mol;<klr tip="ind">
  (&c_t;&c_je;&c_k;&c_s;.)</klr></trd>
  <trd lng="sk">ni&tcaron; osnovy</trd>
  <trd lng="uk">&#x43D;&#x438;&#x442;&#x43A;&#x430; &#x43E;&#x441;&#x43D;&#x43E;&#x432;&#x438;</trd>
</drv>

<drv mrk="varp.0ringo">
  <kap><tld/>ringo</kap>
  <snc mrk="varp.0ringo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Speciala ringo &ccirc;irka&ubreve;anta la <tld/>erojn, kiu helpas
      al interpaso de
      <ref tip="vid" cel="veft.0o">vefto</ref>.
    </dif>
  </snc>
  <trd lng="cs">tkalcovsk&yacute; krou&zcaron;ek</trd>
  <trdgrp lng="hu">
    <trd>sz&aacute;d</trd>,
    <trd>ny&uuml;st</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7D9C;&#x7D56;&#x306E;&#x76EE;  [<ind>&#x7D9C;&#x7D56;&#x306E;&#x3081;</ind>]</trd>,
    <trd>&#x30D8;&#x30EB;&#x30C9;</trd>
  </trdgrp>
  <trd lng="sk">tk&aacute;&ccaron;sky kr&uacute;&zcaron;ok</trd>
  <trd lng="uk">&#x440;&#x435;&#x43C;&#x456;&#x437;&#x43A;&#x430;</trd>
</drv>
</art>
