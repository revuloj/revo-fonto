<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>varp</rad>/o
</kap>
<drv mrk="varp.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Paralelaj fadenoj, inter kiujn teksisto intermetas
      la <ref tip="vid" cel="veft.0o">vefton</ref> por
      fari <ref tip="malprt" cel="teks.0ajxo">teksa&jcirc;on</ref>.
    </dif>
    <ref tip="sin" cel="teks.0bazo">teksbazo</ref>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_n;&c_o;&c_v;&c_a;</trd>
  <trd lng="en">warp</trd>
  <trd lng="nl">schering</trd>
  <trd lng="ru">&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;<klr tip="ind"> (&c_t;&c_je;&c_k;&c_s;.)</klr></trd>
</drv>

<drv mrk="varp.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Aran&gcirc;i paralele la <tld/>ojn sur la teksma&scirc;ino anta&ubreve; la veftado.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_mol; 
    <ind>&c_a;&c_s;&c_n;&c_o;&c_v;&c_u;</ind></trd>
  <trd lng="nl">scheren<klr tip="ind"> (op touw zetten)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol; <ind>&c_o;&c_s;&c_n;&c_o;&c_v;&c_u;</ind><klr tip="ind"> (&c_t;&c_je;&c_k;&c_s;.)</klr></trd>
</drv>

<drv mrk="varp.0ero">
  <kap><tld/>ero</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Unuopa fadeno de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_n;&c_o;&c_w;&c_n;&c_a;&c_ja; 
    <ind>&c_n;&c_ib;&c_t;&c_k;&c_a;</ind></trd>
  <trd lng="nl">kettingdraad</trd>
  <trd lng="ru">&c_o;&c_s;&c_n;&c_o;&c_v;&c_n;&c_a;&c_ja; &c_n;&c_i;&c_t;&c_mol;<klr tip="ind"> (&c_t;&c_je;&c_k;&c_s;.)</klr></trd>
</drv>

<drv mrk="varp.0ringo">
  <kap><tld/>ringo</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Speciala ringo &ccirc;irka&ubreve;anta la <tld/>erojn, kiu helpas al interpaso de
      <ref tip="vid" cel="veft.0o">vefto</ref>.
    </dif>
  </snc>
</drv>
</art>
