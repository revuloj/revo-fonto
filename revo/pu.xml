<art mrk="$Id: pu.xml,v 1.1 2023/09/08 18:29:40 afido Exp $">
<kap>
    <rad>pu</rad>
</kap>
<drv mrk="pu.0">
  <kap><tld/></kap>
  <gra><vspec>ekkrio</vspec></gra>
  <snc mrk="pu.0.KOMUNE">
    <dif>
      Ekkrio pro malestimto, na&ubreve;zi&gcirc;o; <ref tip="vid" cel="fi.0">fi</ref>:
      <ekz>
        <tld/>! Vi tute ne komprenas la virinan koron
        <fnt>
          <aut>Ba Jin, trad. Li Shijun</aut>
          <vrk>A&ubreve;tuno en la Printempo</vrk>
          <lok>1980</lok>
        </fnt>;
      </ekz>
      <ekz>
        sed vi sida&ccirc;as apud la forno, man&gcirc;a&ccirc;as kaj drinkas; kvaza&ubreve; brutoj maldecaj! <tld/>!
        <fnt>
          <aut>A. &Ccirc;e&hcirc;ov, trad. V. Vy&ccirc;eg&jcirc;anin</aut>
          <vrk><url ref="https://esperanto-ondo.ru/Rn-che10.htm">
          Sa&gcirc;a kortisto</url></vrk>
          <lok>en: &Ccirc;eriza &gcirc;ardeno, 2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>pah</trd>,
    <trd>pouah</trd>
  </trdgrp>
</drv>
</art>

