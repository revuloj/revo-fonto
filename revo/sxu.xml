<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;u</rad>/o
</kap>
<drv mrk="sxu.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Piedvesto, ordinare leda kaj al&gcirc;ustigita,
      kovranta la piedon nur &gcirc;is maleoloj:
      <ekz>
        <ind>ligna <tld/>o</ind>;
        <trd lng="nl">klomp</trd>
      </ekz>
      <ekz>
        &ccirc;e <tld/>isto la <tld/>o estas &ccirc;iam kun truo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        esti sub la <tld/>o de (f: humile kaj sklave obei al) sia
        edzino
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        trovi &gcirc;ustmezuran <tld/>on
        <klr>(ion a&ubreve; iun konforman al siaj deziroj)</klr>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bot.0eto">boteto</ref>,
      <ref cel="bot.0o">boto</ref>,
      <ref cel="pantof.0o">pantoflo</ref>,
      <ref cel="sandal.0o">sandalo</ref>,
      <ref cel="galosx.0o">galo&scirc;o</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>polobotka</trd>,
    <trd>st&rcaron;ev&iacute;c</trd>
  </trdgrp>
  <trd lng="de">Schuh</trd>
  <trd lng="el">&pi;&alfa;&pi;&omikron;&ypsilon_ton;&tau;&sigma;&jota;</trd>
  <trd lng="en">shoe</trd>
  <trd lng="es">zapato</trd>
  <trd lng="fr">chaussure</trd>
  <trd lng="nl">schoen</trd>
  <trd lng="pt">sapato</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_t;&c_i;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_b;&c_a;&c_sh;&c_m;&c_a;&c_k;</trd>,
    <trd>&c_t;&c_u;&c_f;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">sko</trd>
  <trd lng="tr">ayakkab&inodot;</trd>
</drv>

<drv mrk="sxu.glit0o">
  <kap>glit<tld/>o</kap>
  <snc>
    <dif>
      Ilo konsistanta el fortika, ordinare metala, plandumo,
      sub kiu estas la&ubreve;longe kaj vertikale fiksita fera lameno,
      kaj kiun oni alligas al <tld/>o por rapide gliti sur la glacio.
    </dif>
  </snc>
  <trd lng="de">Schlittschuh</trd>
  <trd lng="fr">patin &agrave; glace</trd>
  <trdgrp lng="nl">
    <trd>ijsschaats</trd>,
    <trd>schaats</trd>
  </trdgrp>
  <trd lng="pt">patim</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
  <trd lng="sv">skridsko</trd>
</drv>

<drv mrk="sxu.ligno0o">
  <kap>ligno<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="&Scirc;"/>o farita komplete el ligno.
    </dif>
  </snc>
  <trd lng="nl">klomp</trd>
</drv>

<drv mrk="sxu.rad0o">
  <kap>rad<tld/>o</kap>
  <snc>
    <dif>
      Ilo konsistanta el plandumo provizita per kvar radetoj,
      kiun oni alligas al <tld/>o kaj kiu ebligas rapide gliti sur glata&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Rollschuh</trd>
  <trd lng="fr">patin &agrave; roulettes</trd>
  <trd lng="nl">rolschaats</trd>
  <trd lng="ru">&c_r;&c_o;&c_l;&c_i;&c_k;&c_o;&c_v;&c_y;&c_j; &c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
  <trd lng="sv">rullskridsko</trd>
</drv>
</art>
