<drv mrk="sxu.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxu.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="vest.pied0o">Piedvesto</ref> kun solida
      plandumo, ta&ubreve;ga por uzo eksterhejma:
      <ekz>
        paro da <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Amos 2:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        la botisto faras <ref tip="vid" cel="bot.0o">boto</ref>jn kaj <tld/>ojn
        <fnt>
          <bib>F</bib>
          <lok>&FE; 37</lok>
        </fnt>;
      </ekz>
      <ekz>
        deprenu viajn <tld/>ojn de viaj piedoj, &ccirc;ar la loko <klr>[&#x2026;]</klr> estas tero
        sankta
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 3:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la akvo fluis de &scirc;iaj haroj kaj vestoj, kaj ver&scirc;i&gcirc;is en &scirc;iajn <tld/>ojn kaj elen
        <fnt>
          <bib>Fab1</bib>
          <lok>Re&gcirc;idino sur pizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ordigis siajn <ref tip="sub" cel="pantof.0o">pantoflo</ref>jn, &ccirc;ar tio estis <tld/>oj, kiujn li estis
        tretplatiginta
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>&ccirc;e botisto la <tld/>o estas &ccirc;iam kun truo</ind>
        <trd lng="fr">c'est le cordonnier le plus mal
        chauss&ecirc;</trd>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="de">der Schuster tr&auml;gt die schlechtesten Schuhe</trd>
      </ekz>
      <ekz>
        &scirc;i havis sur si belegajn ru&gcirc;ajn marokenajn <tld/>ojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        klakbruo de <ind>klak<tld/>o</ind>j 
        <klr>(ligna <ref tip="sub" cel="sandal.0o">sandalo</ref>)</klr>
        sur asfalto
        <fnt>
          <bib>MD11</bib>
          <vrk>Pafiloj kaj kas-kalkuliloj</vrk>
        </fnt>;
        <trd lng="de">Klapperlatsche</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>esti sub la <tld/>o de</ind> sia edzino
        <trd lng="fr">&ecirc;tre &agrave; la botte de</trd>
        <klr>(humile kaj sklave obei)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="de">unter dem <ind>Pantoffel</ind> stehen</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="af">skoen</trd>
