<drv mrk="sxu.rul0o">
  <kap>rul<tld/>o</kap>
  <snc mrk="sxu.rul0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Scirc;uo a&ubreve; fiksebla plandumo kun radetoj, por glitado sur
      glata grundo:
      <ekz>
        uzo de rultabuloj, skutiloj kaj rul<tld/>oj estas malpermesata
        en la &gcirc;ardenaro
        <fnt>
        <aut>A&ubreve;stralia Nacia Botanika &Gcirc;ardenaro, trad. D.
        Broadribb
        k.a.</aut>,
        <vrk>
<url ref="http://www.anbg.gov.au/anbg/esperanto/index.html">Bonvenon...</url>
</vrk>,
        <lok>2002-06-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_l;&c_ib;&c_k;&c_a;&c_v;&c_y;
  &c_k;&c_a;&c_n;&c_jo;&c_k;</trd>
  <trd lng="ca">pat&iacute; <klr>(de rodes)</klr></trd>
  <trd lng="cs">kole&ccaron;kov&aacute; brusle</trd>
  <trd lng="de">Rollschuh</trd>
  <trd lng="en">roller skate</trd>
  <trd lng="es">pat&iacute;n <klr>(de ruedas)</klr></trd>
  <trdgrp lng="fr">
    <trd>patin &agrave; roulettes</trd>,
    <trd>roller</trd>
  </trdgrp>
  <trd lng="he">&#x2137;&lamed;&#x2137;&yod;&lamed;&yod;&tav;</trd>
  <trd lng="hu">g&ouml;rkorcsolya</trd>
  <trd lng="id"><ind>sepatu</ind> roda</trd>
  <trd lng="it">pattino<klr> a rotelle</klr></trd>
  <trd lng="nl">rolschaats</trd>
  <trd lng="pl">wrotka<klr> z butem</klr></trd>
  <trd lng="ru">&c_r;&c_o;&c_l;&c_i;&c_k;&c_o;&c_v;&c_y;&c_j;
  &c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
  <trd lng="sv">rullskridsko</trd>
</drv>

<drv mrk="sxu.tol0o">
  <kap>tol<tld/>o</kap>
  <snc mrk="sxu.tol0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxu.0o.TEKS"><tld lit="&Scirc;"/>o</ref>
      tute
      a&ubreve; grandparte el tolo:
      <ekz>
        La viroj kun falditaj pantalonoj, &ccirc;emizoj de konataj
        markoj,
        <klr>...</klr> tol<tld/>oj anka&ubreve; de konataj markoj
        <fnt>
        <aut>-</aut>,
        <vrk>
<url ref="http://www.vilaweb.com/www/especials/cronica?p_idcmp=570421">
Mogao-grotoj</url></vrk>,
        <lok>vilaweb, [vidita je 2006-01-31]</lok>
        </fnt>;
      </ekz>
      <ekz>
        tradiciaj tol<tld/>oj kaj vasta pantalono montras lin tipa
        pekinano
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk>
<url ref="http://es1.chinabroadcast.cn/1/2004/01/07/1@2474.htm">Triciklisto
        de Pekino</url></vrk>,
        <lok>2004-01-07</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="en">
    <trd>sneaker<klr tip="amb"> (shoe)</klr></trd>,
    <trd>canvas shoe</trd>,
    <trd>athletic shoe</trd>,
    <trd>running shoe</trd>,
    <trd>runner<klr tip="amb"> (shoe)</klr></trd>,
   <trd>tennis shoe</trd>,
    <trd>boat shoe</trd>,
    <trd>gym shoe</trd>,
    <trd>deck shoe</trd>,
    <trd>bowling shoe</trd>
  </trdgrp>
 <trdgrp lng="fr">
    <trd>basket<klr tip="amb"> (chaussure)</klr></trd>,
    <trd>chaussure de toile</trd>,
    <trd>tennis<klr tip="amb"> (chaussure)</klr></trd>
  </trdgrp>
  <trd lng="id"><ind>sepatu</ind> tenis</trd>
  <trd lng="it">scarpa<klr> di tela</klr></trd>
  <trdgrp lng="pl">
    <trd>tenis&oacute;wek</trd>,
    <trd>&lstroke;ape&cacute;</trd>,
    <trd>walonka</trd>
  </trdgrp>
  </snc>
  <trd lng="cs">pl&aacute;t&ecaron;nka</trd>
</drv>
