<drv mrk="sxu.danc0o">
  <kap>danc<tld/>o, <var><kap>bal<tld/>o</kap></var></kap>
  <snc mrk="sxu.danc0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxu.0o.TEKS"><tld lit="&Scirc;"/>o</ref>
      speciale farita por dancado:
      <ekz>
        vidu, kiaj belegaj danc<tld/>oj!
        <fnt><bib>Fab2</bib>, <lok>ru&gcirc;aj &scirc;uoj</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi do estas pli deca en bal<tld/>oj?
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_a;&c_b;&c_u;&c_t;&c_a;&c_k;</ind></trd>
  <trd lng="cs">tane&ccaron;n&iacute; bota</trd>
  <trd lng="de">Tanzschuh</trd>
  <trdgrp lng="en">
    <trd>dance shoe</trd>,
    <trd>ballet shoe</trd>,,
    <trd>pointe shoe</trd>
    <trd>ghillie</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ballerine</trd>,
    <trd>chaussure de dance</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>sepatu</baz>sepatu balet</trd>,
    <trd><baz>sepatu</baz>sepatu dansa</trd>
  </trdgrp>
  <trd lng="it">scarpa<klr> da ballo</klr></trd>
  <trdgrp lng="pl">
    <trd>buty do ta&nacute;ca</trd>,
    <trd>baleriny</trd>,
    <trd>baletki<klr> (buty do baletu)</klr></trd>
  </trdgrp>
  <trd lng="tr">dans ayakkab&inodot;s&inodot;</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_l;&c_mol;&c_n;&c_je; &c_v;&c_z;&c_u;&c_t;&c_t;&c_ja;</trd>,
    <trd>&c_b;&c_a;&c_l;&c_mol;&c_n;&c_ib; &c_t;&c_u;&c_f;&c_l;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="sxu.glit0o">
  <kap>glit<tld/>o</kap>
  <snc mrk="sxu.glit0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Scirc;uo a&ubreve; fiksebla plandumo kun suba lameno por glitado
      sur glacio:
      <ekz>
        infanoj <klr>[&#8230;]</klr> &scirc;ajne neniam laci&gcirc;is gliti <klr>[&#8230;]</klr> sur siaj
        neakrigitaj glitum<tld/>oj
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi, kiel &scirc;atanto, spertos plezuron gliti kvaza&ubreve; sur grandega glit<tld/>o
        <fnt>
          <vrk><url ref="https://www.rulit.me/books/kuranta-sur-ondoj-read-292384-9.html">Kuranta
          sur ondoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/3/33/Figure-skates-1.jpg/360px-Figure-skates-1.jpg" prm="PD">glit<tld/>oj
        <fnt>
          <aut>en:User:Dr.frog, en:Image:Figure-skates-1.jpg</aut>
          <lok><url ref="&WCW;/File:Figure-skates-1.jpg">&WCW;/File:Figure-skates-1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_jo;&c_k;</trd>
  <trd lng="ca">pat&iacute; <klr>(de gel)</klr></trd>
  <trd lng="cs">brusle</trd>
  <trd lng="de">Schlittschuh</trd>
  <trd lng="en">ice skate</trd>
  <trd lng="es">pat&iacute;n <klr>(de hielo)</klr></trd>
  <trd lng="fr">patin &agrave; glace</trd>
  <trd lng="he">&mem;&het;&lamed;&yod;&qof;&yod;&yod;&fmem;<klr>
  (&yod;&het;&yod;&dalet;)</klr></trd>
  <trd lng="hu">korcsolya</trd>
  <trd lng="id"><baz>sepatu</baz>sepatu es</trd>
  <trd lng="it">pattino<klr> da ghiaccio</klr></trd>
  <trd lng="ja">&#12473;&#12465;&#12540;&#12488;&#38772;  <pr>&#12473;&#12465;&#12540;&#12488;&#12368;&#12388;</pr></trd>
  <trdgrp lng="nl">
    <trd>ijsschaats</trd>,
    <trd>schaats</trd>
  </trdgrp>
  <trd lng="pl">&lstroke;y&zdot;wa<klr> z butem</klr></trd>
  <trd lng="pt">patim</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
  <trd lng="sv">skridsko</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_v;&c_z;&c_a;&c_n;&c_ja;&c_r;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_ch;&c_je;&c_r;&c_je;&c_v;&c_i;&c_k;</trd>,
    <trd>&c_l;&c_i;&c_zh;&c_n;&c_i;&c_j; &c_ch;&c_je;&c_r;&c_je;&c_v;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20912;&#20992; <pr>b&imacron;ngd&amacron;o</pr></trd>,
    <trd>&#28316;&#20912;&#38795; <pr>li&umacron;b&imacron;ngxi&eacute;</pr></trd>,
    <trd>&#20912;&#20992;&#38795; <pr>b&imacron;ngd&amacron;oxi&eacute;</pr></trd>,
    <trd>&#24179;&#24213;&#38634;&#27207; <pr>p&iacute;ngd&#464;xu&ecaron;qi&amacron;o</pr></trd>,
    <trd>&#38634;&#25772; <pr>xu&ecaron;qi&agrave;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxu.haltig0o">
