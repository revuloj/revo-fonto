<drv mrk="sxu.danc0o">
  <kap>danc<tld/>o</kap>
  <snc mrk="sxu.danc0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxu.0o.TEKS"><tld lit="&Scirc;"/>o</ref>
      speciale farita por danco:
      <ekz>
        &leftquot;vidu, kiaj belegaj danc<tld/>oj!&rightquot;
        <fnt><bib>Fab2</bib>, <lok>ru&gcirc;aj &scirc;uoj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">tane&ccaron;n&iacute; bota</trd>
  <trd lng="de">Tanzschuh</trd>
  <trdgrp lng="en">
    <trd>dance shoe</trd>,
    <trd>ballet shoe</trd>,,
    <trd>pointe shoe</trd>
    <trd>ghillie</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ballerine</trd>,
    <trd>chaussure de dance</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>sepatu</ind> balet</trd>,
    <trd><ind>sepatu</ind> dansa</trd>
  </trdgrp>
  <trd lng="it">scarpa<klr> da ballo</klr></trd>
  <trdgrp lng="pl">
    <trd>buty do ta&nacute;ca</trd>,
    <trd>baleriny</trd>,
    <trd>baletki<klr> (buty do baletu)</klr></trd>
  </trdgrp>
  <trd lng="tr">dans ayakkab&inodot;s&inodot;</trd>
</drv>

<drv mrk="sxu.glit0o">
  <kap>glit<tld/>o</kap>
  <snc mrk="sxu.glit0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Scirc;uo a&ubreve; fiksebla plandumo kun suba lameno por glitado
      sur glacio.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_jo;&c_k;</trd>
  <trd lng="ca">pat&iacute; <klr>(de gel)</klr></trd>
  <trd lng="cs">brusle</trd>
  <trd lng="de">Schlittschuh</trd>
  <trd lng="en">ice skate</trd>
  <trd lng="es">pat&iacute;n <klr>(de hielo)</klr></trd>
  <trd lng="fr">patin &agrave; glace</trd>
  <trd lng="he">&mem;&het;&lamed;&yod;&qof;&yod;&yod;&fmem;<klr>
  (&yod;&het;&yod;&dalet;)</klr></trd>
  <trd lng="hu">korcsolya</trd>
  <trd lng="id"><ind>sepatu</ind> es</trd>
  <trd lng="it">pattino<klr> da ghiaccio</klr></trd>
  <trdgrp lng="nl">
    <trd>ijsschaats</trd>,
    <trd>schaats</trd>
  </trdgrp>
  <trd lng="pl">&lstroke;y&zdot;wa<klr> z butem</klr></trd>
  <trd lng="pt">patim</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
  <trd lng="sv">skridsko</trd>
</drv>

<drv mrk="sxu.ligno0o">
  <kap>ligno<tld/>o, <var><kap>ligna <tld/>o</kap></var></kap>
  <snc mrk="sxu.ligno0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxu.0o.TEKS"><tld lit="&Scirc;"/>o</ref>
      farita tute
      el ligno:
      <ekz>
        li estis lerta ligno<tld/>isto
        <fnt><bib>Fab3</bib>, <lok>Ib kaj malgranda
        kristino</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="galosx.0o">galo&scirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>esclop</trd>,
    <trd>galotxa</trd>
  </trdgrp>
  <trd lng="cs">d&rcaron;ev&aacute;k</trd>
  <trd lng="de">Holzschuh</trd>
  <trdgrp lng="en">
    <trd>sabot</trd>,
    <trd>wooden shoe</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>zueco <klr>(de madera)</klr></trd>,
    <trd>zapato de madera</trd>
  </trdgrp>
  <trd lng="fr">sabot</trd>
  <trdgrp lng="he">
    <trd>&qof;&bet;&qof;&bet;</trd>,
    <trd>&nun;&ayin;&lamed; &ayin;&ftsadi;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>facip&odblac;</trd>,
    <trd>klumpa</trd>
  </trdgrp>
  <trd lng="id"><ind>sepatu</ind> galos</trd>
  <trd lng="it">zoccolo</trd>
  <trd lng="nl">klomp</trd>
  <trdgrp lng="pl">
    <trd>kierpc<klr> (reg.)</klr></trd>,
    <trd>mokasyny</trd>
  </trdgrp>
  <trd lng="tr">takunya</trd>
</drv>

<drv mrk="sxu.negx0o">
  <kap>ne&gcirc;<tld/>o</kap>
  <snc mrk="sxu.negx0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
    Rakedforma ilo por promeni sur ne&gcirc;o.
    </dif>
  <trd lng="en">snowshoe</trd>
  <trd lng="es">raqueta de nieve</trd>
   <trdgrp lng="it">
    <trd>ciaspola</trd>,
    <trd><ind>racchetta</ind> da neve</trd>
  </trdgrp>
    </snc>
  <trd lng="cs">sn&ecaron;&zcaron;nice</trd>
  <trd lng="tr">kar ayakkab&inodot;s&inodot;</trd>
</drv>
    
