<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;u</rad>/o
</kap>
<drv mrk="sxu.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxu.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.pied0o">Piedvesto</ref> kun solida
      plandumo, ta&ubreve;ga por uzo eksterhejma:
      <ekz>
        <ind>&ccirc;e botisto la <tld/>o estas &ccirc;iam kun truo</ind>
        <trd lng="fr">c'est le cordonnier le plus mal chauss&ecirc;</trd>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        esti sub la <tld/>o de sia edzino
        <klr>(humile kaj sklave obei)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>trovi &gcirc;ustmezuran <tld/>on</ind>
        <klr>(trovi ion a&ubreve; iun konforman al siaj deziroj)</klr>
        <trd lng="fr">trouver chaussure &agrave; son pied</trd>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&Ha;&kasra;&dhal;&fatha_alif;&hamza;&dammatan; &jim; &alif_hamza_sure;&fatha;&Ha;&sukun;&dhal;&kasra;&ya;&fatha;&ta_marbuta;&dammatan;</trd>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_a;&c_v;&c_ib;&c_k;</trd>,
    <trd>&c_t;&c_u;&c_f;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_t;&c_o;&c_f;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">sabata</trd>
  <trdgrp lng="cs">
    <trd>polobotka</trd>,
    <trd>st&rcaron;ev&iacute;c</trd>
  </trdgrp>
  <trd lng="de">Schuh</trd>
  <trd lng="el">&pi;&alfa;&pi;&omikron;&ypsilon_ton;&tau;&sigma;&jota;</trd>
  <trd lng="en">shoe</trd>
  <trd lng="es">zapato</trd>
  <trd lng="fr">chaussure</trd>
  <trd lng="he">&nun;&ayin;&lamed;</trd>
  <trd lng="hu">cip&odblac;</trd>
  <trd lng="nl">schoen</trd>
  <trd lng="pt">sapato</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_t;&c_i;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_b;&c_a;&c_sh;&c_m;&c_a;&c_k;</trd>,
    <trd>&c_t;&c_u;&c_f;&c_l;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">sko</trd>
  <trd lng="tr">ayakkab&inodot;</trd>
  <trd lng="vo">juk</trd>
</drv>

<drv mrk="sxu.glit0o">
  <kap>glit<tld/>o</kap>
  <snc mrk="sxu.glit0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Scirc;uo a&ubreve; fiksebla plandumo kun suba la&ubreve;longa
      metala lameno por rapida glitado sur glacio.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_jo;&c_k;</trd>
  <trd lng="ca">pat&iacute; <klr>(de gel)</klr></trd>
  <trd lng="de">Schlittschuh</trd>
  <trd lng="es">pat&iacute;n <klr>(de hielo)</klr></trd>
  <trd lng="fr">patin &agrave; glace</trd>
  <trd lng="he">&mem;&het;&lamed;&yod;&qof;&yod;&yod;&fmem;<klr>
(&yod;&het;&yod;&dalet;)</klr></trd>
  <trd lng="hu">korcsolya</trd>
  <trdgrp lng="nl">
    <trd>ijsschaats</trd>,
    <trd>schaats</trd>
  </trdgrp>
  <trd lng="pt">patim</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
  <trd lng="sv">skridsko</trd>
</drv>

<drv mrk="sxu.ligno0o">
  <kap>ligno<tld/>o, ligna <tld/>o</kap>
  <snc mrk="sxu.ligno0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxu.0o.TEKS"><tld lit="&Scirc;"/>o</ref> farita tute
el ligno.
    <refgrp tip="vid">
      <ref cel="galosx.0o">galo&scirc;o</ref>.
    </refgrp>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>esclop</trd>,
    <trd>galotxa</trd>
  </trdgrp>
  <trd lng="fr">sabot</trd>
  <trdgrp lng="he">
    <trd>&qof;&bet;&qof;&bet;</trd>,
    <trd>&nun;&ayin;&lamed; &ayin;&ftsadi;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>facip&odblac;</trd>,
    <trd>klumpa</trd>
  </trdgrp>
  <trd lng="nl">klomp</trd>
</drv>

<drv mrk="sxu.rad0o">
  <kap>rad<tld/>o</kap>
  <snc mrk="sxu.rad0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      &Scirc;uo a&ubreve; fiksebla plandumo kun subaj radetoj,
      por rapida glitado sur glata&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_l;&c_ib;&c_k;&c_a;&c_v;&c_y;
&c_k;&c_a;&c_n;&c_jo;&c_k;</trd>
  <trd lng="ca">pat&iacute; <klr>(de rodes)</klr></trd>
  <trd lng="de">Rollschuh</trd>
  <trd lng="es">pat&iacute;n <klr>(de ruedas)</klr></trd>
  <trdgrp lng="fr">
    <trd>patin &agrave; roulettes</trd>,
    <trd>roller</trd>
  </trdgrp>
  <trd lng="he">&gimel;&lamed;&gimel;&yod;&lamed;&yod;&tav;</trd>
  <trd lng="hu">g&ouml;rkorcsolya</trd>
  <trd lng="nl">rolschaats</trd>
  <trd lng="ru">&c_r;&c_o;&c_l;&c_i;&c_k;&c_o;&c_v;&c_y;&c_j;
&c_k;&c_o;&c_n;&c_jo;&c_k;</trd>
  <trd lng="sv">rullskridsko</trd>
</drv>
</art>
