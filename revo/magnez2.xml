<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>magnezit</rad>/o
</kap>
<drv mrk="magnez2.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      MgCO<sub>3</sub>. Natura magnezia karbonato.
    </dif>
  </snc>
  <trd lng="hu">magnezit</trd>
  <trdgrp lng="nl">
    <trd>magneziet</trd>,
    <trd>magnesiumcarbonaat</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_je;&c_z;&c_i;&c_t;</trd>
</drv>
</art>
