<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: magnez2.xml,v 1.17 2024/01/15 21:20:53 revo Exp $">
<kap>
  <rad>magnezit</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="magnez2.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="magnez2.0o.MIN">
  <uzo tip="fak">MIN</uzo>
    <dif>
      <frm>MgCO<sub>3</sub></frm>, natura magnezia <ref tip="super" cel="karbonat.0o">karbonato</ref>:
      <ekz>
        la <tld/>o<klr>[&#8230;]</klr>n la industrio profitas por fajrorezista
        materio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=272664">Magnezito</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> fondis la firmaon &leftquot;<nom>Carl Spaeter</nom>&rightquot; en
        <nom>Duisburg</nom>,
        en 1881 li komencigis ekspluatadon de <tld/>o en
        <nom>Veitsch</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=402532">Friedrich Albert Carl Spaeter</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/6/6a/Magnesite-121892.jpg/600px-Magnesite-121892.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o el Bahia, Brazilo
        <fnt>
          <aut>Rob Lavinsky, iRocks.com</aut>
          <lok><url ref="&WCW;/File:Magnesite-121892.jpg"
              >https://commons.wikimedia.org/wiki/File:Magnesite-121892.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_e;&c_z;&c_ib;&c_t;</trd>
  <trd lng="ca">magnesita</trd>
  <trdgrp lng="de">
    <trd>Magnesit</trd>,
    <trd>Bitterspat</trd>
  </trdgrp>
  <trd lng="fr">magn&eacute;site</trd>
  <trd lng="hu">magnezit</trd>
  <trdgrp lng="ja">
    <trd>&#12510;&#12464;&#12493;&#12469;&#12452;&#12488;</trd>,
    <trd>&#33777;&#33510;&#22303;&#37489;  <pr>&#12426;&#12423;&#12358;&#12367;&#12393;&#12371;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>magneziet</trd>,
    <trd>magnesiumcarbonaat</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>magnezyt</trd>,
    <trd>w&eogonek;glan magnezu</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>magnesita</trd>,
    <trd>giobertita</trd>,
    <trd>espato-de-magn&eacute;sio</trd>
  </trdgrp>
  <trd lng="ro">magnezit</trd>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_je;&c_z;&c_i;&c_t;</trd>
  <trd lng="uk">&c_m;&c_a;&c_g;&c_n;&c_je;&c_z;&c_i;&c_t;</trd>
</drv>
</art>
