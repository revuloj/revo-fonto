<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rekvie.xml,v 1.25 2024/04/13 20:15:52 revo Exp $">
<kap>
  <rad>rekviem</rad>/o
  <fnt><bib>Hamlet</bib></fnt>
</kap>
<drv mrk="rekvie.0o">
  <kap><tld/>o</kap>
  <snc mrk="rekvie.0o.KAT">
    <uzo tip="fak">KAT</uzo>
    <dif>
      Funebra <ref tip="super" cel="serv.Di0o">diservo</ref> por
      mortintoj:
      <ekz>
        ni ja malsanktigus / Per tio la servadon de mortintoj / Se ni al &scirc;i ekkantus
        rekviemon / Simile al mortintaj en pieco
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la futbaldio de la aliuloj montri&gcirc;is nekontesteble pli forta, la postan tagon
        Munkeno silente funebris en profana <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2012/010769.php">Patro nia parodia</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">responso</trd>
    <trd lng="hu">gy&aacute;szmise</trd>
    <trd lng="pt">r&eacute;quiem</trd>
    <trdgrp lng="pl">
      <trd>requiem</trd>,
      <trd>rekwiem</trd>,
      <trd>msza &zdot;a&lstroke;obna</trd>,
      <trd>msza rekwialna</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_je;&c_k;&c_v;&c_i;&c_je;&c_m;</trd>,
      <trd>&c_p;&c_a;&c_n;&c_i;&c_h;&c_i;&c_d;&c_a;</trd>,
      <trd>&c_z;&c_a;&c_u;&c_p;&c_o;&c_k;&c_o;&c_j;&c_n;&c_a;&c_ja;
      &c_s;&c_l;&c_u;&c_zh;&c_b;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc mrk="rekvie.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="kompon.0ajxo">Kompona&jcirc;o</ref>
      por orkestro, &hcirc;oro, ofte anka&ubreve; orgeno,
      por akompani funebran diservon:
      <ekz>
        &Ccirc;ina Filharmonia Orkestro de Pekino kaj &Hcirc;oro de la &Scirc;anhaja Operejo ludis la
        <tld/>on de <nom>Mozart</nom> anta&ubreve; la papo kaj la kurio
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2009/009523.php">&Ccirc;ina koncerto en Vatikano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">r&eacute;quiem</trd>
    <trdgrp lng="hu">
      <trd>halotti <ind>mise</ind></trd>,
      <trd>rekviem</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>requiem</trd>,
      <trd>rekwiem</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_je;&c_k;&c_v;&c_i;&c_je;&c_m;</trd>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_k;&c_v;&c_ib;&c_je;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>rekviem</trd>,
    <trd>z&aacute;du&scaron;n&iacute; m&scaron;e</trd>
  </trdgrp>
  <trd lng="de">Requiem</trd>
  <trd lng="en">requiem</trd>
  <trd lng="fr">requiem</trd>
  <trd lng="it">requiem</trd>
  <trdgrp lng="ja">
    <trd>&#x6B7B;&#x8005;&#x306E;&#x305F;&#x3081;&#x306E;&#x30DF;&#x30B5;  <pr>&#x3057;&#x3057;&#x3083;&#x306E;&#x305F;&#x3081;&#x306E;&#x30DF;&#x30B5;</pr></trd>,
    <trd>&#x93AE;&#x9B42;&#x66F2;  <pr>&#x3061;&#x3093;&#x3053;&#x3093;&#x304D;&#x3087;&#x304F;</pr></trd>,
    <trd>&#x30EC;&#x30AF;&#x30A4;&#x30A8;&#x30E0;</trd>
  </trdgrp>
  <trd lng="nl">requiem</trd>
  <trdgrp lng="sk">
    <trd>rekviem</trd>,
    <trd>z&aacute;du&scaron;n&aacute; om&scaron;a</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x440;&#x435;&#x43A;&#x432;&#x456;&#x454;&#x43C;</trd>,
    <trd>&#x43F;&#x430;&#x43D;&#x430;&#x445;&#x438;&#x434;&#x430;</trd>,
    <trd>&#x437;&#x430;&#x443;&#x43F;&#x43E;&#x43A;&#x456;&#x439;&#x43D;&#x430; &#x441;&#x43B;&#x443;&#x436;&#x431;&#x430;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5B89;&#x9B42;&#x5F25;&#x6492; <pr>&#x101;nh&#xFA;nm&#xED;s&#x1CE;</pr></trd>,
    <trd>&#x5B89;&#x9B42;&#x5F4C;&#x6492; <pr>&#x101;nh&#xFA;nm&#xED;s&#x1CE;</pr></trd>
  </trdgrp>
</drv>
</art>
