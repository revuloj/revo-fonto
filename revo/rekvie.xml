<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rekviem</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="rekvie.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc mrk="rekvie.0o.KAT">
  <uzo tip="fak">KAT</uzo>
    <dif>
      Funebra <ref tip="super" cel="serv.Di0o">diservo</ref> por mortintoj.
    </dif>
  </snc>
  <snc mrk="rekvie.0o.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      Plur-parta muzika&jcirc;o plenumata per orkestro, &hcirc;oro, ofte
      anka&ubreve; orgeno, por akompani funebran diservon.
    </dif>
  </snc>
  <trd lng="de">Requiem</trd>
  <trd lng="fr">requiem</trd>
  <trd lng="nl">requiem</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_k;&c_v;&c_i;&c_je;&c_m;</trd>,
    <trd>&c_p;&c_a;&c_n;&c_i;&c_h;&c_i;&c_d;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_u;&c_p;&c_o;&c_k;&c_o;&c_j;&c_n;&c_a;&c_ja; &c_s;&c_l;&c_u;&c_zh;&c_b;&c_a;</trd>
  </trdgrp>
</drv>
</art>
