<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>* kunm</ofc>
  <rad>procent</rad>/o
</kap>
<drv mrk="procen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Profito produktata de monsumo pruntita a&ubreve; &scirc;uldata:
      <ekz>
        <tld/>a dono;
      </ekz>
      <ekz>
        pruntado pro <tld/>oj.
      </ekz>
    </dif>
    <ref tip="sin" cel="intere1.0o">interezo</ref>
    <trdgrp lng="en">
      <trd>rate<klr tip="amb"> (financial)</klr></trd>,
      <trd>interest rate</trd>
    </trdgrp>
    <trd lng="es">inter&eacute;s<klr> (financiero)</klr></trd>
    <trd lng="fr">int&eacute;r&ecirc;t<klr> (financier)</klr></trd>
    <trd lng="hu">kamat</trd>
    <trd lng="nl">rente</trd> 
    <trdgrp lng="pt">
      <trd>porcentagem</trd>,  
      <trd>percentagem</trd>
    </trdgrp>
  </snc>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      Profito produktata de cent monunuoj dum unu jaro:
      <ekz>
        la <ref tip="dif" cel="divide.0o">dividendo</ref> estos 5
        <tld/>oj
        <klr>(5%)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rent.0o">rento</ref>.
    </refgrp>
    <trdgrp lng="en">
      <trd>rate<klr tip="amb"> (financial)</klr></trd>,
      <trd>yield<klr tip="amb"> (financial)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>renta</trd>,
      <trd>dividendo</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>rente</trd>,
      <trd>dividende</trd>
    </trdgrp>
    <trd lng="hu">kamatl&aacute;b</trd>
    <trd lng="nl">procent</trd>
  </snc>
  <snc>
  <uzo tip="fak">MAT</uzo>
    <dif>
      Valoro a&ubreve; taksado difinita rilate al la nombro cent:
      <ekz>
        en la vortaro oni povus &scirc;an&gcirc;i nur iujn 10 <tld/>ojn
        (centonojn) da
        vortoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mi ricevas <tld/>on de &ccirc;iuj eldonoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li kalkulis la <tld/>ojn de l' pola lo&gcirc;antaro en la prusaj
        provincoj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cent.el0o">elcento</ref>,
      <ref cel="cent.po0o">pocento</ref>,
      <ref cel="cent.0ono">centono</ref>
    </refgrp>
    <rim>
      Por la senco 3 &scirc;ajnas preferinde uzi <em>elcento</em> a&ubreve;
      <em>pocento</em> a&ubreve; <em>centono</em> kaj lasi al <tld/>o sencon
      nur
      financan.
    </rim>
    <trdgrp lng="es">
      <trd>por ciento</trd>,
      <trd>porcentaje</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>pourcent</trd>,
      <trd>pourcentage</trd>
    </trdgrp>

    <trd lng="hu">sz&aacute;zal&eacute;k</trd>
    <trd lng="nl">percent<klr tip="ind"> (ontr.)</klr></trd>
  </snc>
  <rim>
   Vidu tiun <ref cel="re.0rim">rimarkon</ref>.
   </rim>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_c;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_a;&c_d;&c_s;&c_o;&c_t;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>per cent</trd>,
    <trd>percent</trd>,
    <trd>per centum</trd>,
    <trd>percentage</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>inter&eacute;s</trd>,
    <trd>renta</trd>,
    <trd>por ciento</trd>,
    <trd>porcentaje</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_c;&c_je;&c_n;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>percento</trd>,
    <trd>&uacute;rok</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>procento ,, procentu&aacute;ln&iacute; v&yacute;nos</trd>,
    <trd>zisk</trd>
  </trdgrp>
</drv>

<drv mrk="procen.0ego">
  <kap><ofc>*</ofc><tld/>ego <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5172">pro'cent'eg'</url></lok></fnt></kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="P"/>o pli granda ol la le&gcirc;a, kutima; <ref tip="sin"
      cel="uzur.0o">uzuro</ref>:
      <ekz>
        prunte doni <tld/>ege<fnt>Z</fnt>,
      </ekz>
      <ekz>
        kontra&ubreve; <tld/>ego<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>egisto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>nadm&ecaron;rn&yacute; zisk</trd>,
    <trd>nadm&ecaron;rn&yacute; &uacute;rok</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>extortionate interest</trd>,
    <trd>usury</trd>
  </trdgrp>
  <trd lng="es">usura</trd>
  <trd lng="fr">usure</trd>
  <trd lng="hu">uzsorakamat</trd>
  <trd lng="nl">woekerrente</trd>
  <trd lng="sk">&uacute;&zcaron;ern&iacute;cky &uacute;rok</trd>
</drv>
</art>
