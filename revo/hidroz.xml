<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>hidrozo</rad>/oj
</kap>
<drv mrk="hidroz.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso el la filumo <ref tip="super" cel="knid.0uloj">kniduloj</ref>
      (<trd lng="la">Hydrozoa</trd>), kies anoj havas du generaciojn de la
      formoj
      <ref tip="prt" cel="meduz.0o">meduzo</ref> kaj
      <ref tip="prt" cel="polip.0o">polipo</ref>.
    </dif>
  </snc>
  <trd lng="de">Hydrozoen</trd>
</drv>
</art>
