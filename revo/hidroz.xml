<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hidrozo</rad>/oj
</kap>
<drv mrk="hidroz.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso el la filumo <ref tip="super" cel="knid.0uloj">kniduloj</ref>
      (<trd lng="la">Hydrozoa</trd>), kies anoj havas du generaciojn de la
      formoj <ref tip="prt" cel="meduz.0o">meduzo</ref> kaj
      <ref tip="prt" cel="polip.0o">polipo</ref>.
    </dif>
  </snc>
  <trd lng="de">Hydrozoen</trd>
  <trd lng="fr">hydrozoaires</trd>
  <trdgrp lng="nl">
    <trd>kwalpoliepen</trd>,
    <trd>hydro&iuml;dpoliepen</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_o;&c_i;&c_d;&c_n;&c_y;&c_je; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
</drv>
</art>
