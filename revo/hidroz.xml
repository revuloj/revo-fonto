<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hidroz.xml,v 1.17 2023/11/15 17:08:00 revo Exp $">
<kap>
  <rad>hidrozo</rad>/oj
</kap>
<drv mrk="hidroz.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="klas.0o.BIO" lst="voko:zoologiaj_klasoj">Klaso</ref>
       el la filumo <ref tip="super" cel="knid.0uloj">kniduloj</ref>
      (<trd lng="la">Hydrozoa</trd>), kies anoj havas du generaciojn de
      la formoj <ref tip="prt" cel="meduz.0o">meduzo</ref> kaj
      <ref tip="prt" cel="polip.0o">polipo</ref>:
      <ekz>
        <tld/>oj <klr>[&#x2026;]</klr> estas <klr>[&#x2026;]</klr> malgrandaj, predantaj bestoj, el
        kiuj kelkaj solemaj kaj kelkaj koloniaj, plej parto vivantaj en sala akvo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=609590">Hidrozoo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_d;&c_r;&c_o;&c_ib;&c_d;&c_n;&c_y;&c_ja; <klr>(&c_k;&c_l;&c_ja;&c_s;)</klr></trd>
  <trd lng="ca">hidrozous</trd>
  <trd lng="cs">polyp</trd>
  <trd lng="de">Hydrozoen</trd>
  <trd lng="en">Hydrozoa</trd>
  <trd lng="fr">hydrozoaires</trd>
  <trd lng="hu">hidra&aacute;llatok</trd>
  <trd lng="it">idrozoi</trd>
  <trdgrp lng="nl">
    <trd>kwalpoliepen</trd>,
    <trd>hydro&iuml;dpoliepen</trd>
  </trdgrp>
  <trd lng="pl">stu&lstroke;biop&lstroke;awy</trd>
  <trd lng="pt">hidrozo&aacute;rios</trd>
  <trd lng="ru">&c_g;&c_i;&c_d;&c_r;&c_o;&c_i;&c_d;&c_n;&c_y;&c_je; <klr tip="amb">(&c_k;&c_l;&c_a;&c_s;&c_s;)</klr></trd>
  <trd lng="sk">polyp</trd>
  <trd lng="zh">&#x6C34;&#x8785;&#x7DB1; <pr>shu&#x1D0;x&#x12B;g&#x101;ng</pr></trd>
</drv>
</art>
