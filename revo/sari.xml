<art mrk="$Id: sari.xml,v 1.16 2023/09/03 12:09:44 revo Exp $">
<kap>
  <rad>sari</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sari.0o">
  <kap><tld/>o</kap>
  <snc mrk="sari.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tradicia vesto de hindaj inoj, proksimume 5 metrojn longa tuko
      malstrikte volvita kaj drapirita &ccirc;irka&ubreve; la korpo, ordinare super
      robo:
      <ekz>
        alproksimi&gcirc;is kvin virinoj en <tld/>o
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;man&gcirc;i
        rubman&gcirc;a&jcirc;on&rightquot;, p.
        13a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_r;&c_y;</trd>
  <trd lng="ca">sari</trd>
  <trd lng="cs">s&aacute;r&iacute;</trd>
  <trd lng="de">Sari</trd>
  <trd lng="en">sari</trd>
  <trd lng="es">sari</trd>
  <trd lng="fr">sari</trd>
  <trd lng="hu">sz&aacute;ri</trd>
  <trd lng="it">sari</trd>
  <trd lng="ja">&#x30B5;&#x30EA;&#x30FC;</trd>
  <trd lng="nl">sari</trd>
  <trd lng="pl">sari</trd>
  <trdgrp lng="pt">
   <trd>s&aacute;ri</trd>,
   <trd>sari</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_a;&c_r;&c_i;</trd>
  <trd lng="sk">s&aacute;r&iacute;</trd>
  <trd lng="sv">sari</trd>
  <trd lng="uk">&c_s;&c_a;&c_r;&c_ib; (&#x43E;&c_d;&#x44F;&c_g; &c_ib;&#x43D;&c_d;&c_ib;&c_j;&c_s;&#x44C;&#x43A;&c_i;&c_h; &c_zh;&c_ib;&#x43D;&#x43E;&#x43A;)</trd>
</drv>
</art>

