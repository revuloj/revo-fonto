<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sxraman.xml,v 1.9 2023/12/15 16:34:06 revo Exp $">
<kap>
  <rad>&scirc;raman</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="sxraman.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxraman.0o.HIN">
    <uzo tip="fak">BUD</uzo>
    <uzo tip="fak">HIN</uzo>
    <dif>
      <ref tip="super" cel="asket.0o.REL">Asketo</ref> vivanta
      senhejme la&ubreve; hindua tradicio:
      <ekz>
        tiu, kiu subpremas aliulon, ne estas <tld/>o
        <fnt>
          <aut>trad. T. T. Anuruddha</aut>,
          <vrk>La Vojo al Nirvano a&ubreve; la Dhammapado</vrk>,
          <lok>B&#7891;-T&aacute;t-Csoma-Instituto por Budhologio, 1973</lok>
        </fnt>;
      </ekz>
      <ekz>
        du <tld/>oj, Ma&ubreve;dgaljajano kaj Purna alvenis &ccirc;i tien tra la
        aero kaj predikis la Darmon al via patro
        <fnt>
          <aut>trad. Asano San&ccirc;i</aut>,
          <vrk><url
          ref="http://www.ebudhano.cn/esperanto/sutro/guanjin/guanjing.htm">Amitajur

          Djana Sutro, enkonduko</url></vrk>,
          <lok>La Japana Budhano, 1993-04, numero 264a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
     <ref cel="biksxu.0o">bik&scirc;uo</ref>,
     <ref cel="bonz.0o">bonzo</ref>
   </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_r;&c_a;&c_m;&c_a;&c_n;</trd>
  <trd lng="de">Samana<klr tip="amb"> (M&ouml;nch)</klr></trd>
  <trd lng="en">shramana</trd>
  <trd lng="fr">shramana</trd>
  <trd lng="ja">&#27801;&#38272; <pr>&#12375;&#12419;&#12418;&#12435;</pr></trd>
  <trdgrp lng="pl">
    <trd>&sacute;raman</trd>,
    <trd>saman</trd>
  </trdgrp>
</drv>
</art>

