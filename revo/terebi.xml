<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>terebint</rad>/o
</kap>
<drv mrk="terebi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="terebi.0o.vera">
    <dif>
      Rezineca diafana duonflua&jcirc;o, kiu elfluas &scirc;vite el
      <ref tip="malprt" cel="terebi.0arbo"><tld/>arbo</ref>.
    </dif>
  </snc>
  <snc mrk="terebi.0o.konifera">
    <dif>
      Simila substanco el&scirc;vitata el diversaj koniferoj.
    </dif>
  </snc>
  <trd lng="de">Terpentin</trd>
  <trd lng="nl">terpentijn</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_v;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;&c_t;&c_i;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="terebi.0arbo">
  <kap><tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="pistak.0ujo">pistakujo</ref>
      (<trd lng="la">Pistacia terebinthus</trd>), malgranda arbo.
    </dif>
  </snc>
  <trd lng="de">Terpentinpistazie</trd>
  <trd lng="nl">terpentijnboom</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;&c_t;&c_i;&c_n;&c_n;&c_o;&c_je; &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
</drv>

<drv mrk="terebi.0oleo">
  <kap><tld/>oleo</kap>
  <snc>
    <dif>
      Hidrokarbonido ricevita per distilado de la <tld/>o kaj
      servanta precipe por solvi la grasa&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="de">Terpentin&ouml;l</trd>
  <trd lng="nl">terpentijnolie</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;&c_t;&c_i;&c_n;&c_n;&c_o;&c_je; &c_m;&c_a;&c_s;&c_l;&c_o;</trd>,
    <trd>&c_s;&c_k;&c_i;&c_p;&c_i;&c_d;&c_a;&c_r;</trd>
  </trdgrp>
</drv>
</art>
