<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: terebi.xml,v 1.22 2023/10/21 13:02:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>terebint</rad>/o
</kap>
<drv mrk="terebi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="terebi.0o.vera">
    <dif>
      Rezineca diafana duonflua&jcirc;o, kiu elfluas &scirc;vite el
      <ref tip="malprt" cel="terebi.0arbo"><tld/>arbo</ref>.
    </dif>
    <trd lng="pl">terpentyna</trd>
  </snc>
  <snc mrk="terebi.0o.konifera">
    <dif>
      Simila substanco el&scirc;vitata el diversaj koniferoj:
      <ekz>
        Junipero <klr>[&#x2026;]</klr>: &gcirc;ia odoro estas neforta, similas al <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2048">Junipero</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">trementina</trd>
  <trdgrp lng="cs">
    <trd>pist&aacute;cie terebintov&aacute;</trd>,
    <trd>terebint <klr>(strom)</klr></trd>,
    <trd>&rcaron;e&ccaron;&iacute;k terebintov&yacute;</trd>
  </trdgrp>
  <trd lng="de">Terpentin</trd>
  <trdgrp lng="en">
    <trd>oleoresin</trd>,
    <trd>turpentine</trd>
  </trdgrp>
  <trd lng="fr">t&eacute;r&eacute;benthine</trd>
  <trd lng="hu">terpentin</trd>
  <trd lng="id">terpentin</trd>
  <trd lng="ja">&#x30C6;&#x30EC;&#x30D3;&#x30F3;</trd>
  <trd lng="nl">terpentijn</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_v;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;&c_t;&c_i;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">pist&aacute;cia terebintov&aacute;</trd>
</drv>

<drv mrk="terebi.0arbo">
  <kap><tld/>arbo</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="super" cel="pistak.0ujo">pistakujo</ref>
      (<trd lng="la">Pistacia terebinthus</trd>), malgranda arbo:
      <ekz>
        la terebintarbo <klr>[estas]</klr> falfolia <klr>[&#x2026;]</klr> arbeto kun 
        nepare plume kunmetaj folioj, kiu liveras la veran <tld/>on 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=180507">Terebintarbo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/6d/Pistacia_terebinthus_1c.JPG/576px-Pistacia_terebinthus_1c.JPG" prm="CC BY-SA 4.0"><tld/>arbo (Pistacia terebinthus), Botanika &Gcirc;ardeno de Barcelono
         <fnt>
           <aut>Consultaplantas, propra verko</aut>
           <lok><url ref="&WCW;/File:Pistacia_terebinthus_1c.JPG">&WCW;/File:Pistacia_terebinthus_1c.JPG</url></lok>
         </fnt>
    </bld>
  </snc>
  <trdgrp lng="ca">
    <trd>noguerola</trd>,
    <trd>terebint</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pist&aacute;cie terebintov&aacute;</trd>,
    <trd>terebint</trd>,
    <trd>&rcaron;e&ccaron;&iacute;k terebintov&yacute;</trd>
  </trdgrp>
  <trd lng="de">Terpentinpistazie</trd>
  <trd lng="en">terebinth</trd>
  <trd lng="fr">t&eacute;r&eacute;binthe</trd>
  <trdgrp lng="hu">
    <trd>terebint piszt&aacute;cia</trd>,
    <trd>terebintfa</trd>
  </trdgrp>
  <trd lng="ja">&#x30C6;&#x30EC;&#x30D3;&#x30F3;&#x30CE;&#x30AD;</trd>
  <trd lng="nl">terpentijnboom</trd>
  <trd lng="pl">pistacja <ind>terpentynowa</ind></trd>
  <trd lng="ru">
	&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;&c_t;&c_i;&c_n;&c_n;&c_o;&c_je;
  &c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sk">terebinta</trd>
  <trd lng="uk">&#x444;&#x456;&#x441;&#x442;&#x430;&#x448;&#x43A;&#x430; &#x442;&#x435;&#x440;&#x43F;&#x435;&#x43D;&#x442;&#x438;&#x43D;&#x43D;&#x430;</trd>
</drv>

<drv mrk="terebi.0oleo">
  <kap><tld/>oleo</kap>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      Hidrokarbonido ricevita per distilado de la <tld/>o kaj
      servanta precipe por solvi la grasa&jcirc;ojn:
      <ekz>
        sigelvakso estas miksa&jcirc;o el kolofono kun <tld/>a oleo, &scirc;elako, koloriga&jcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18248">Sigelvakso</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">ess&egrave;ncia de <ind>trementina</ind></trd>
  <trd lng="cs">terpent&yacute;nov&yacute; olej</trd>
  <trd lng="de">Terpentin&ouml;l</trd>
  <trdgrp lng="en">
    <trd>turpentine</trd>,
    <trd>oil of turpentine</trd>
  </trdgrp>
  <trd lng="fr">essence de <ind>t&eacute;r&eacute;benthine</ind></trd>
  <trd lng="hu">terpentinolaj</trd>
  <trd lng="ja">&#x30C6;&#x30EC;&#x30D3;&#x30F3;&#x6CB9;  <pr>&#x30C6;&#x30EC;&#x30D3;&#x30F3;&#x3086;</pr></trd>
  <trd lng="nl">terpentijnolie</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_r;&c_p;&c_je;&c_n;&c_t;&c_i;&c_n;&c_n;&c_o;&c_je;
    &c_m;&c_a;&c_s;&c_l;&c_o;</trd>,
    <trd>&c_s;&c_k;&c_i;&c_p;&c_i;&c_d;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">terpent&iacute;nov&yacute; olej</trd>
</drv>
</art>
