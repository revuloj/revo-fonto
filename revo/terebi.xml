<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>terebint</rad>/o
</kap>
<drv mrk="terebi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="terebi.0o.vera">
    <dif>
      Rezineca diafana duonflua&jcirc;o, kiu elfluas &scirc;vite el
      <ref tip="malprt" cel="terebi.0arbo"><tld/>arbo</ref>.
    </dif>
  </snc>
  <snc mrk="terebi.0o.konifera">
    <dif>
      Simila substanco el&scirc;vitata el diversaj koniferoj.
    </dif>
  </snc>
  <trd lng="de">Terpentin</trd>
</drv>
<drv mrk="terebi.0arbo">
  <kap><tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="pistak.0ujo">pistakujo</ref>
      (<trd lng="la">Pistacia terebinthus</trd>), malgranda arbo.
    </dif>
  </snc>
  <trd lng="de">Terpentinpistazie</trd>
</drv>
<drv mrk="terebi.0oleo">
  <kap><tld/>oleo</kap>
  <snc>
    <dif>
      Hidrokarbonido ricevita per distilado de la <tld/>o kaj
      servanta precipe por solvi la grasa&jcirc;ojn.
    </dif>
  </snc>
  <trd lng="de">Terpentin&ouml;l</trd>
</drv>
</art>
