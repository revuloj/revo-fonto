<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diskotek.xml,v 1.12 2023/08/31 10:44:01 revo Exp $">
<kap>
  <rad>diskotek</rad>/o
</kap>

<drv mrk="diskotek.0o">
  <kap><tld/>o</kap>
  <snc mrk="diskotek.0o.MUZ">
    <uzo tip="stl">KOMUNE</uzo>  
    <dif>
      <ref tip="dif" cel="danc.0ejo.MUZ">Dancejo</ref>:
      <ekz>
        alparolas min <nom>Kerstin</nom> &ndash; knabino, kiu &scirc;atas festojn kaj
        <tld/>ojn  
        <fnt>
          <aut>U. Matthias</aut>,
          <vrk><url ref="http://www.u-matthias.de/verko/fajron.htm">
          Fajron sentas mi interne</url></vrk>,
          <lok>Pro Esperanto, 1990</lok>
        </fnt>;
      </ekz>
      <ekz>
        por ili ne estas kompreneble, kial viro ekkonstruis ne profitodonan restoracion
        a&ubreve; <tld/>on, sed pre&gcirc;ejon, kiu alportos neniun monon
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2003/007949.html">Sur propra tereno propra pre&gcirc;ejo</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuvespere ni kune pre&gcirc;as, por ke almena&ubreve; neniu el la kunposedantoj enkapigu al
        si malfermi <tld/>on <klr>[&#x2026;]</klr> super nia dormo&ccirc;ambro
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010183.php">Terasa apartamento</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="bg">&c_d;&c_i;&c_s;&c_k;&c_o;&c_t;&c_je;&c_k;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Disco</trd>,
      <trd>Diskothek</trd>
    </trdgrp>
    <trd lng="en">discoth&egrave;que</trd>
    <trd lng="es">discoteca</trd>
    <trd lng="pl">dyskoteka</trd>
    <trdgrp lng="pt">
      <trd>discoteca</trd>,
      <trd>danceteria</trd>
    </trdgrp>
  </snc>
  <snc mrk="diskotek.0o.BIB">
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      Kolekto de diskaj a&ubreve; aliaj sonregistra&jcirc;oj, ofte parto
      de <ref tip="malprt" cel="biblio1.0o.ejo">biblioteko</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Plattensammlung</trd>,
      <trd>Phonothek</trd>
    </trdgrp>
    <trd lng="pl">p&lstroke;ytoteka</trd>
    <trd lng="pt">discoteca</trd>
  </snc>
  <trd lng="cs">diskot&eacute;ka</trd>
  
  <trd lng="fr">discoth&egrave;que</trd>
  <trd lng="he">&#x5D3;&#x5D9;&#x5E1;&#x5E7;&#x5D5;&#x5D8;&#x5E7;</trd>
  <trd lng="ja">&#x30EC;&#x30B3;&#x30FC;&#x30C9;&#x30E9;&#x30A4;&#x30D6;&#x30E9;&#x30EA;</trd>
  <trd lng="sk">diskot&eacute;ka</trd>
  <trd lng="uk">&#x434;&#x438;&#x441;&#x43A;&#x43E;&#x442;&#x435;&#x43A;&#x430;</trd>
</drv>
</art>

