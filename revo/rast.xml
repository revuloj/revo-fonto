<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rast</rad>/i
</kap>
<drv mrk="rast.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">AGR</uzo>
    <dif>
      Kunkolekti fojnon, pajlon, falintajn foliojn ks per speciala ilo
      provizita je dentoj.
    </dif>
  </snc>
  <snc>
    <dif>
      Ta&ubreve;ge purigi kaj ebenigi grundan supra&jcirc;on, skrapante per analoga ilo.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>rasclar</trd>,
    <trd>rampinar</trd>
  </trdgrp>
  <trd lng="de">rechen</trd>
  <trdgrp lng="es">
    <trd>rastrillar</trd>
  </trdgrp>
  <trd lng="en">rake</trd>
  <trd lng="hu">gerebly&eacute;z</trd>
  <trd lng="nl">harken</trd>
  <trd lng="ru">&c_s;&c_g;&c_r;&c_je;&c_b;&c_a;&c_t;&c_mol; <ind>&c_g;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_m;&c_i;</ind></trd>
</drv>

<drv mrk="rast.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Diversspeca kultura kaj &gcirc;ardena ilo, konsistanta el longa bastono
      kun transversa ligna a&ubreve; fera stangeto provizita je dentoj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_b;&c_l;&c_ib;</trd>
  <trd lng="ca">rastell</trd>
  <trd lng="de">Rechen</trd>
  <trd lng="en">rake</trd>
  <trd lng="es">rastrillo</trd>
  <trd lng="hu">gereblye</trd>
  <trd lng="nl">hark</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_b;&c_l;&c_i;</trd>
</drv>
</art>
