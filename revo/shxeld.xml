<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: shxeld.xml,v 1.11 2023/10/21 07:46:47 revo Exp $">
  <kap>
    <rad>S&hcirc;eld</rad>/o,
    <var><kap><rad var="k">Skeld</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
  </kap>

<drv mrk="shxeld.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <snc mrk="shxeld.0o.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="river.0o" lst="voko:riveroj">Rivero</ref>, fluanta el
      <ref tip="malprt" cel="franc.F0ujo">Francujo</ref>, tra
      <ref tip="malprt" cel="belg.B0ujo">Belgujo</ref> al
      <ref tip="malprt" cel="nederl.N0o">Nederlando</ref>,
      kie &gcirc;i fluas en la Nordan Maron:
      <ekz>
        li atentigis, ke surstrandi&gcirc;oj
        de ka&ccirc;alotoj sur la flandra marbordo a&ubreve; en la rivero <tld var="k"/>o jam
        plurfoje estis konstatitaj
        <fnt><aut>Wim M. A. de Smet</aut>:
          <vrk><url ref="&Monato;/1998/006047.html">Gigantoj sur
          strandoj</url></vrk>,
          <bib>Monato</bib>,
          <lok>1998/11</lok>
        </fnt>;
      </ekz>
      <ekz>
        Zelando <klr>[&#x2026;]</klr> konsistas el kelkaj duoninsuloj kiuj iam
        estis apartaj insuloj <klr>[&#x2026;]</klr>,
        Zelanda Flandrujo estas parto de Zelando sude de la elfluofunelo
        de la rivero <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4609">Zelando</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la kompatindulo dronis en <tld var="k"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Yves Bellefeuille</aut>
          <vrk><url ref="&Monato;/2008/009598.php">Ne por lernantoj, sed por
          spertuloj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa, okulfrapa parto kombinis novan tunelon sub la rivero
        <tld var="k"/>o kun aldona
        dunivela ponto, du kilometrojn longa, super lo&gcirc;kvartaloj kaj
        haveno, por norde kompletigi ring-&scirc;oseon
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2010/010057.php">Ponto per tunelo
          subfosita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_Sh;&c_e;&c_l;&c_mol;&c_d;&c_a;</trd>,
      <trd>&c_E;&c_s;&c_k;&c_o;</trd>
    </trdgrp>
    <trd lng="bg">&c_Sh;&c_je;&c_l;&c_d;&c_a;</trd>
    <trd lng="ca">Escalda</trd>
    <trd lng="cs">&Scaron;elda</trd>
    <trd lng="en">Scheldt</trd>
    <trd lng="es">Escalda</trd>
    <trd lng="de">Schelde</trd>
    <trd lng="fr">Escaut</trd>
    <trd lng="hu">Schelde</trd>
    <trd lng="it">Schelda</trd>
    <trd lng="nl">Schelde</trd>
    <trd lng="pl">Skalda</trd>
    <trd lng="pt">Escalda</trd>
    <trdgrp lng="ru">
      <trd>&c_Sh;&c_je;&c_l;&c_mol;&c_d;&c_a;</trd>,
      <trd>&c_E;&c_s;&c_k;&c_o;</trd>
    </trdgrp>
  </snc>
  <trd lng="ja">&#x30B9;&#x30B1;&#x30EB;&#x30C7;&#x5DDD;  <pr>&#x30B9;&#x30B1;&#x30EB;&#x30C7;&#x304B;&#x308F;</pr></trd>
  <trd lng="uk">&c_Sh;&c_je;&#x43B;&#x44C;&c_d;&c_a;</trd>
</drv>
</art>

