<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akredi1.xml,v 1.23 2022/03/11 13:29:02 revo Exp $">
<kap>
  <rad>akreditiv</rad>/o
</kap>
<drv mrk="akredi1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="leter.0o">Letero</ref>
      per kiu banko a&ubreve; bankiero de unu loko malfermas al
      iu &ccirc;e sia korespondanto krediton de fiksita sumo:
      <ekz>
        pago povas esti farita ekzemple per: kontanta mono, remburso, &gcirc;iro, da&ubreve;ra
        transpago, kreditkarto, debetkarto, debet-enkasigo, kambio, akreditivo, &ccirc;eko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=265404">Pago</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="dokume.0akredito">dokumenta kredito</ref>, 
      <ref cel="kredit.0letero">kreditletero</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_r;&c_e;&c_d;&c_y;&c_t;&c_y;&c_w;</trd>
  <trdgrp lng="cs">
    <trd>akreditiv</trd>,
    <trd>pov&ecaron;&rcaron;ovac&iacute; listina</trd>,
    <trd>&uacute;v&ecaron;rov&yacute; doklad</trd>
  </trdgrp>
  <trd lng="de">Akkreditiv</trd>
  <trdgrp lng="en">
    <trd>letter of <ind>credit</ind><klr> (LC, L/C, LOC)</klr></trd>,
    <trd>request for credit</trd>
  </trdgrp>
  <trd lng="es">aval<klr tip="amb"> (bancario)</klr></trd>
  <trd lng="fr">accr&eacute;ditif</trd>
  <trd lng="he">&#x5DB;&#x5EA;&#x5D1; &#x5D4;&#x5D0;&#x5DE;&#x5E0;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>meghitelez&eacute;s</trd>,
    <trd>akkredit&iacute;v</trd>
  </trdgrp>
  <trd lng="nl">accreditief</trd>
  <trd lng="pl">akredytywa</trd>
  <trd lng="pt">carta de cr&eacute;dito</trd>
  <trd lng="ru">&c_a;&c_k;&c_k;&c_r;&c_je;&c_d;&c_i;&c_t;&c_i;&c_v;</trd>
  <trdgrp lng="sk">
    <trd>&uacute;verov&yacute; list</trd>,
    <trd>akredit&iacute;v</trd>,
    <trd>&uacute;verov&yacute; list</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x43A;&#x440;&#x435;&#x434;&#x438;&#x442;&#x438;&#x432;</trd>
</drv>

<drv mrk="akredi1.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Malfermi akreditivon por iu.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_k;&c_r;&c_y;&c_c;&c_mol;
  <ind>&c_a;&c_k;&c_r;&c_e;&c_d;&c_y;&c_t;&c_y;&c_w;</ind></trd>
  <trdgrp lng="cs">
    <trd>akreditovat</trd>,
    <trd>otev&rcaron;&iacute;t akreditiv</trd>,
    <trd>pov&ecaron;&rcaron;it</trd>,
    <trd>zmocnit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>akkreditieren</trd>,
    <trd>ein Akkreditiv stellen</trd>
  </trdgrp>
  <trd lng="en">arrange a credit</trd>
  <trd lng="es">avalar<klr tip="amb"> (bancario)</klr></trd>
  <trd lng="he">&#x5DC;&#x5D4;&#x5D2;&#x5D9;&#x5E9; &#x5DB;&#x5EA;&#x5D1; &#x5D4;&#x5D0;&#x5DE;&#x5E0;&#x5D4;</trd>
  <trd lng="nl">een <ind>accreditief</ind> openen</trd>
  <trd lng="ru">&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_mol;
  <ind>&c_a;&c_k;&c_k;&c_r;&c_je;&c_d;&c_i;&c_t;&c_i;&c_v;</ind></trd>
  <trdgrp lng="sk">
    <trd>akreditova&tcaron;</trd>,
    <trd>poveri&tcaron;</trd>
  </trdgrp>
</drv>

</art>
