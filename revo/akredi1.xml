<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>akreditiv</rad>/o
</kap>
<drv mrk="akredi1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Letero per kiu banko a&ubreve; bankiero de unu loko malfermas al iu
      &ccirc;e sia korespondanto krediton de fiksita sumo. Anka&ubreve;: kreditletero, dokumenta kredito.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_r;&c_e;&c_d;&c_y;&c_t;&c_y;&c_w;</trd>
  <trd lng="de">Akkreditiv</trd>
  <trdgrp lng="en">
    <trd>letter of <ind>credit</ind></trd>,
    <trd>request for credit</trd>
  </trdgrp>
  <trd lng="fr">accr&eacute;ditif</trd>
  <trdgrp lng="hu">
    <trd>meghitelez&eacute;s</trd>,
    <trd>akkredit&iacute;v</trd>
  </trdgrp>
  <trd lng="nl">accreditief</trd>
  <trd lng="pt">carta de cr&eacute;dito</trd>
  <trd lng="ru">&c_a;&c_k;&c_k;&c_r;&c_je;&c_d;&c_i;&c_t;&c_i;&c_v;</trd>
  <trdgrp lng="sk">
    <trd>&uacute;verov&yacute; list</trd>,
    <trd>akredit&iacute;v</trd>
  </trdgrp>
</drv>

<drv mrk="akredi1.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Malfermi akreditivon por iu.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_k;&c_r;&c_y;&c_c;&c_mol; <ind>&c_a;&c_k;&c_r;&c_e;&c_d;&c_y;&c_t;&c_y;&c_w;</ind></trd>
  <trdgrp lng="de">
    <trd>akkreditieren</trd>,
    <trd>ein Akkreditiv stellen</trd>
  </trdgrp>
  <trd lng="en">arrange a credit</trd>
</drv>

</art>
