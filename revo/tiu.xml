<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tiu</rad>
</kap>
<drv mrk="tiu.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Montra pronomo:
      <ekz>
        <tld/> estas mia amiko;
      </ekz>
      <ekz>
        &ccirc;i <tld/> legas, <tld/> skribas kaj la aliaj dormas;
      </ekz>
      <ekz>
        plej bone ridas <tld/>, kiu ridas la lasta<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiu vivos, <tld/> vidos
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kiu rompis, <tld/> pagu
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        doktrino apogita sur <tld/> de Platono.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxiu">&ccirc;iu RIM</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Montra adj.:
      <ekz>
        mi ne konas <tld/>n homon;
      </ekz>
      <ekz>
        <tld/>j &ccirc;i du amikoj promenas &ccirc;iam duope<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ne timu <tld/>n hundeton;
      </ekz>
      <ekz>
        <tld/> monto ne estas tre alta;
      </ekz>
      <ekz>
        <tld/> &ccirc;i libro havas 60 pa&gcirc;ojn;
      </ekz>
      <ekz>
        prenu <tld/>n glason, kiu estas sur la tablo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kiu.0">kiu</ref>,
      <ref cel="iu.0">iu</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>dieser</trd>,
    <trd>diese</trd>,
    <trd>dieses</trd>,
    <trd>jener</trd>,
    <trd>jene</trd>,
    <trd>jenes</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>die</trd>,
    <trd>dat</trd>,
    <trd>deze</trd> (&ccirc;i tiu),
    <trd>dit</trd> (&ccirc;i tiu)
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_t;</trd>,
    <trd>&c_t;&c_a;</trd>,
    <trd>&c_t;&c_o;</trd>,
    <trd>&c_e;&c_t;&c_o;&c_t;</trd> (&ccirc;i tiu),
    <trd>&c_e;&c_t;&c_a;</trd> (&ccirc;i tiu),
    <trd>&c_e;&c_t;&c_o;</trd> (&ccirc;i tiu)
  </trdgrp>
</drv>
</art>
