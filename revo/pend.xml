  &c_v;&c_y;&c_n;&c_o;&c_s;&c_n;&c_o;&c_j;
  &c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;<klr>
  (&c_b;&c_u;&c_k;&c_v;&c_y;)</klr></trd>
  <trd lng="sv">underh&auml;ng</trd>
</drv>

