<drv mrk="pend.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="pend.super0i.KOMUNE">
    <dif>
      <tld lit="P"/>i, resti senmove super:
      <ekz>
        el fendoj kaj truoj kreskis siringaj arbeta&jcirc;oj, kiuj super<tld/>igis siajn 
        bran&ccirc;ojn super la malgrandaj malaltaj domoj &ccirc;e la interna flanko de la urba murego
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malpli granda luno aperis post la 
        super<tld/>antaj tegmentoj
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">&uuml;berh&auml;ngen</trd>
  </snc>
  <trd lng="cs">b&yacute;t p&rcaron;evisl&yacute;</trd>
  <trd lng="fr">suplomber</trd>
  <trd lng="sk">by&tcaron; previsnut&yacute;</trd>
</drv>

