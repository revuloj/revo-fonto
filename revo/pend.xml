<drv mrk="pend.0umi">
  <kap><tld/>umi</kap>
  <snc mrk="pend.0umi.KOMUNE">
    <dif>
      <ref tip="super" cel="mort.0igi.malvivigi">Mortigi</ref>
      pendigante per 
      &ccirc;irka&ubreve;kola &scirc;nuro; <ref tip="sin"
      cel="pend.0igi.pendumi">pendigi<sncref/></ref>:
      <ekz>
        <tld/>umi krimulon;
      </ekz><ekz>
        sin <tld/>umi;
      </ekz><ekz>
        <tld/>uminda fripono;
      </ekz>
      <ekz>
        drakona ju&gcirc;isto <nom>Lynch</nom>, kiu la&ubreve; legendo en la 15a
        jarcento kondamnis
        kaj <tld/>umis sian propran filon je la fenestro de turo
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url
          ref="https://www.monato.be/2004/008094.html">Galway
          &#8208;
          urbo de triboj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li tiom suferis de depresio, ke post unu tago li provis
        <tld/>umi sin en la karcero
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url
          ref="&Monato;/2009/009680.php">Proksimaj kaj
          malproksimaj najbaroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="ekzeku.0i">ekzekuti</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_sh;&c_a;&c_c;&c_mol; <klr
    tip="amb">(&c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_c;&c_mol;)</klr></trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_s;&c_ib;&c_c;&c_mol; <klr
    tip="amb">(&c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_c;&c_mol;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_b;&c_je;&c_s;&c_v;&c_a;&c_m;</trd>
  <trd lng="br">krouga&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>ob&ecaron;sit</trd>,
    <trd>pov&ecaron;sit</trd>,
    <trd>v&ecaron;&scaron;et</trd>,
    <trd>zav&ecaron;sit</trd>
  </trdgrp>
  <trd lng="de">aufh&auml;ngen</trd>
  <trd lng="en">hang</trd>
  <trdgrp lng="es">
    <trd>colgar</trd>,
    <trd>ahorcar</trd>
  </trdgrp>
  <trd lng="fr">pendre<klr tip="ind"> (ex&eacute;cuter)</klr></trd>
  <trdgrp lng="hu">
    <trd>felakaszt</trd>,
    <trd>fell&oacute;gat</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ophangen<klr tip="ind"> (als straf)</klr></trd>,
    <trd>opknopen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_sh;&c_a;&c_t;&c_mol;<klr tip="ind">
    (&c_k;&c_a;&c_z;&c_n;&c_i;&c_t;&c_mol;)</klr></trd>,
    <trd>&c_p;&c_o;&c_v;&c_je;&c_s;&c_i;&c_t;&c_mol;<klr tip="amb">
    (&c_k;&c_a;&c_z;&c_n;&c_i;&c_t;&c_mol;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obesi&tcaron;</trd>,
    <trd>zavesi&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="pend.0umilo">
  <kap><tld/>umilo</kap>
  <snc mrk="pend.0umilo.KOMUNE">
    <dif>
      Traba&jcirc;o destinita por <tld/>umi ekzekutatojn; <ref tip="sin"
      cel="pend.0igilo">pendigilo</ref>:
      <ekz>
        personoj, kiuj zorgis pri la starigo a&ubreve; pri riparo de la
        <tld/>umiloj, havis aparte malaltan renomon, kaj same la
        personoj, kiuj plenumis la mortpunon
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=258266">Pendumilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_y;&c_b;&c_je;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_je;&c_s;&c_i;&c_l;&c_o;</trd>
  <trd lng="br">kroug</trd>
  <trd lng="cs">&scaron;ibenice</trd>
  <trd lng="de">Galgen</trd>
  <trd lng="en">gallows</trd>
  <trd lng="es">horca</trd>
  <trdgrp lng="fr">
    <trd>gibet</trd>,
    <trd>potence<klr tip="ind"> (gibet)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>akaszt&oacute;fa</trd>,
    <trd>bit&oacute;</trd>
  </trdgrp>
  <trd lng="nl">galg</trd>
  <trd lng="ru">&c_v;&c_i;&c_s;&c_je;&c_l;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">&scaron;ibenica</trd>
</drv>

<drv mrk="pend.de0a">
  <kap>de<tld/>a</kap>
  <snc mrk="pend.de0a.KOMUNE">
    <dif>
      De<tld/>anta, nelibera:
      <ekz>
        de<tld/>a situacio;
      </ekz>
      <ekz>
         lingvo &ccirc;iam de<tld/>a de &gcirc;ia a&ubreve;toro
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        la lo&gcirc;antaro de tiuj landoj i&gcirc;as pli kaj pli
        de<tld/>a de nutra&jcirc;liverado el okcidento
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007272.html">Reen al
          la edena paradizo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">abh&auml;ngig</trd>
    <trd lng="br">e-dalc'h</trd>
    <trd lng="en">dependent</trd>
    <trd lng="fr">d&eacute;pendant</trd>
    <trd lng="nl">afhankelijk</trd>
    <trd lng="ru">&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="pend.de0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <ref tip="ant" cel="pend.nede0a.MAT">nede<tld/>a.</ref>
    <trd lng="br">depantus</trd>
    <trd lng="de">abh&auml;ngig</trd>
    <trd lng="en">dependent</trd>
    <trd lng="fr">d&eacute;pendant</trd>
    <trd lng="hu">f&uuml;gg&odblac;</trd>
    <trd lng="pl">zale&zdot;ny</trd>
    <trd lng="ru">&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_y;&c_j;</trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>odvisl&yacute;</trd>,
    <trd>z&aacute;visl&yacute;</trd>
  </trdgrp>
  <trd lng="es">dependiente</trd>
  <trd lng="sk">z&aacute;visl&yacute;</trd>
</drv>

<drv mrk="pend.de0eje">
  <kap>de<tld/>e de, <var><kap>de<tld/>e je</kap></var></kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="pend.de0eje.KOMUNE">
    <dif>
      Ne memstare, sed influite de,
      <ref tip="vid"
      cel="laux.0.cirkonstanco_influanta">la&ubreve;<sncref/></ref>:
      <ekz>
        lia frako el silko <klr>[&#8230;]</klr> briletas verde,
        ru&gcirc;e kaj blue,
        de<tld/>e de tio, en kia direkto li sin turnas
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        supre kaj malsupre estas la sama afero, de<tld/>e de la
        vidpunkto, kiun oni okupas
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        de<tld/>e de la cirkonstancoj
        <fnt>
          <bib>LR</bib>
          <lok>13. Prepozicioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sonoj povas diferenci iomete, de<tld/>e de
        kia komputilo a&ubreve;digas ilin
        <fnt>
          <aut>L. Ganman</aut>,
          <vrk><url ref="http://www.linusganman.se/u7g.htm">
              Matenson&gcirc;o</url></vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_v; &c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_o;&c_s;&c_t;
  &c_o;&c_t;</trd>
  <trdgrp lng="fr">
    <trd>selon</trd>,
    <trd>en fonction de</trd>
  </trdgrp>
</drv>

<drv mrk="pend.de0i">
  <kap><ofc>*</ofc>de<tld/>i <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5146">de'pend'</url></lok></fnt></kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="pend.de0i.obei">
    <dif>
      Esti subigita sub ies volo a&ubreve; povo:
      <ekz>
        la filo de<tld/>as de sia patro;
      </ekz><ekz>
        la afero ne de<tld/>as de mia volo
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvina Kongreso Esperantista en
          Barcelona en la 6a de septembro 1909</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la pago <klr>[&#8230;]</klr> de<tld/>os <klr>[&#8230;]</klr> de via laboro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        de via lango de<tld/>as ilia vivo
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne de ili de<tld/>as ilia bonstato
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 21:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sorto de homo de<tld/>as de la Eternulo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 29:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        de ili de<tld/>as, &ccirc;u la nova organizo estos kontentiga
        a&ubreve; ne.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="obe.0i">obei</ref>,
      <ref cel="met.sub0i">submetita</ref>.
    </refgrp>
    <trd lng="be">&c_z;&c_a;&c_l;&c_je;&c_zh;&c_a;&c_c;&c_mol;</trd>
    <trd lng="br">beza&ntilde; <ind>e-dalc'h</ind></trd>
    <trd lng="en">depend</trd>
    <trd lng="hu">f&uuml;gg <klr tip="amb">(vt&odblac;l)</klr></trd>
    <trdgrp lng="nl">
      <trd>afhangen</trd>,
      <trd>afhankelijk zijn</trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_a;&c_v;&c_i;&c_s;&c_je;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="pend.de0i.rilati">
    <dif>
      Esti ligita kun io per mallar&gcirc;a rilato de konsekvenco:
      <ekz>
        <klr>[tio]</klr> de<tld/>as de diversaj cirkonstancoj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvina Kongreso Esperantista en
          Barcelona en la 6a de septembro 1909</vrk>
        </fnt>;
      </ekz>
      <ekz>
        tio de<tld/>as de la vento kaj vetero
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io de<tld/>as de la kutimo
        <fnt>
          <bib>LR</bib>
          <lok>4. Fonetiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io dependas de se kaj kiam
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia feli&ccirc;o de<tld/>as de tio, &ccirc;u mi edzi&gcirc;os al
        &scirc;i;
      </ekz><ekz>
        de<tld/>e de tio, kiel <klr>(la&ubreve; tio kiel)</klr>
        li al mi respondos.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_l;&c_je;&c_zh;&c_a;&c_c;&c_mol;</trd>
    <trd lng="br">beza&ntilde; <ind>diouzh</ind></trd>
    <trd lng="en">depend</trd>
    <trd lng="hu">f&uuml;gg <klr tip="amb">(vt&odblac;l)</klr></trd>
    <trdgrp lng="nl">
      <trd>afhangen</trd>,
      <trd>afhankelijk zijn</trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_a;&c_v;&c_i;&c_s;&c_je;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="pend.de0i.aparteni">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Esti konsistiga parto de tuto:
      <ekz>
        tiu &gcirc;ardeno de<tld/>as de la bieno;
      </ekz><ekz>
        la aferoj, kiuj de<tld/>as de nia metio;
      </ekz><ekz>
        tiu vila&gcirc;o ne de<tld/>as de nia distrikto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aparte.0i">aparteni</ref>.
    </refgrp>
    <rim>
      &Ccirc;i tiun sencon <sncref ref="pend.de0i.aparteni"/>
      mi opinias fremda al Esperanto.
      <aut>Sergio Pokrovskij</aut>
    </rim>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_l;&c_je;&c_zh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_n;&c_a;&c_l;&c_je;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="hu">tartozik <klr tip="amb">(vhez)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_n;&c_o;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_n;&c_a;&c_d;&c_l;&c_je;&c_zh;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_z;&c_a;&c_v;&c_i;&c_s;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t z&aacute;visl&yacute;</trd>,
    <trd>z&aacute;viset</trd>
  </trdgrp>
  <trd lng="de">abh&auml;ngen</trd>
  <trd lng="es">depender</trd>
  <trd lng="fr">d&eacute;pendre</trd>
  <trd lng="sk">z&aacute;visie&tcaron;</trd>
</drv>

<drv mrk="pend.el0ajxo">
  <kap><ofc>*</ofc>el<tld/>a&jcirc;o <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5147">el'pend'a&jcirc;'</url></lok></fnt></kap>
  <snc mrk="pend.el0ajxo.sxildo">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Plato pendanta super butiko kaj
      signanta la specon de komerco;
      <ref tip="vid" cel="sxild.0o.butika">&scirc;ildo<sncref/></ref>:
      <ekz>
        La ru&gcirc;eta el<tld/>a&jcirc;o elrigardis kun siaj oraj, en
        la suno 
        brilantaj literoj
        <fnt>
          <aut>C. Dickens, trad. L. L. Zamenhof</aut>,
          <vrk>La Batalo de l' Vivo</vrk>,
          <lok>1891</lok>
        </fnt>;        
      </ekz><ekz>
        la el<tld/>a&jcirc;o, fleksita super la enirejo en la angulo
        &ccirc;e amba&ubreve; flankoj de la fasado, estis montranta
        elefantan kapon
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Grin/vojo.html">
              La vojo nenien</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Aush&auml;ngeschild</trd>
    <trd lng="fr">enseigne</trd>
    <trd lng="nl">uithangbord</trd>
  </snc>
  <trd lng="bg">&c_v;&c_i;&c_s;&c_ja;&c_shch;&c_a;
  &c_t;&c_a;&c_b;&c_je;&c_l;&c_a;</trd>
  <trd lng="ru">&c_v;&c_y;&c_v;&c_je;&c_s;&c_k;&c_a;</trd>
</drv>

<drv mrk="pend.nede0a">
  <kap>nede<tld/>a</kap>
  <snc mrk="pend.nede0a.KOMUNE">
    <dif>
      Ne de<tld/>anta:
      <ekz>
        lia agado subite &ccirc;esis pro kialoj, de li nede<tld/>aj
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 22a</lok></fnt>;
      </ekz><ekz>
        mia nede<tld/>ema naturo igis min &ccirc;iam nekapabla por
        la submeti&gcirc;oj, kiuj necesas por tiu, kiu volas kunvivadi
        kun
        la homoj
        <fnt><bib>Revadoj</bib>, <lok>Sesa promenado</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      Kvaza&ubreve;a sinonimo:
      <ref cel="pend.sende0a">sende<tld/>a</ref>.
    </refgrp>
    <trd lng="br">dizalc'h</trd>
    <trd
    lng="ru">&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_y;&c_j;</trd>
    <adm>
      Indas precizigi la sencnuancon inter "nede~a" kaj "sende~a",
      se &gcirc;i ekzistas.
      <aut>MB</aut>
    </adm>
  </snc>
  <snc mrk="pend.nede0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="pend.nede0a.okazoj">
      <dif>
        <klr>(p.p. <ref cel="okaz.0o.MAT">okazoj</ref>)</klr>
        Tiaj, ke la probablo de ilia
        <ref cel="komun.0ajxo.aro">komuna&jcirc;o</ref> egalas al
        la produto de iliaj probabloj:
        <ekz>
          se <frm am="bb a"><g>a</g></frm> kaj <frm am="bb
          b"><g>b</g></frm> 
          estas okazoj nede<tld/>aj kun nenula probablo,
          la <ref cel="probab.kondicxa0o">kondi&ccirc;a probablo</ref>
          de
          unu,
          sciante, ke okazis la dua, egalas al la probablo de la
          unua
          <klr>(simb. <frm am="P(bb a|bb b)=P(bb a)"
          >P(<g>a</g>|<g>b</g>) = P(<g>a</g>)</frm>)</klr>.
        </ekz>
      </dif>
      <trd lng="de">unabh&auml;ngig<klr tip="ind"> (-es
      Ereignis)</klr></trd>
      <trd lng="en">independent<klr tip="ind"> (event)</klr></trd>
      <trd lng="fr"><klr tip="ind">(&eacute;v&eacute;nement)
      </klr>ind&eacute;pendant</trd>
      <trd lng="hu">f&uuml;ggetlen<klr tip="ind">
      (esem&eacute;ny)</klr></trd>
      <trd lng="pl"><klr tip="amb">(zdarzenie losowe)
        </klr>niezale&zdot;ne</trd>
      <trd
      lng="ru">&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_o;&c_je;<klr
      tip="amb"> (&c_s;&c_o;&c_b;&c_y;&c_t;&c_i;&c_je;)</klr></trd>
    </subsnc>
    <subsnc mrk="pend.nede0a.hazardaj_variabloj">
      <dif>
        <klr>(p.p. du <ref cel="variab.hazarda0o">hazardaj
            variabloj</ref>
          <frm am="X">X</frm> kaj <frm am="Y">Y</frm> super la sama
          <ref cel="spac.probablo0o">probablospaco</ref>)</klr>
        Tiaj, ke por ajnaj reeloj <frm am="x"><k>x</k></frm> kaj
        <frm am="y"><k>y</k></frm>
        la okazoj <frm am="(X lt x)">(X &lt; <k>x</k>)</frm> kaj
        <frm am="(Y lt y)">(Y &lt; <k>y</k>)</frm>
        estas nede<tld/>aj <sncref ref="pend.nede0a.okazoj"/>:
        <ekz>
          se du variabloj estas nede<tld/>aj, la ekspekto de ilia
          produto
          egalas al la produto de iliaj respektivaj ekspektoj
          <klr>(simb. <frm am="E(X*Y)=E(X)*E(Y)"
          >E(X&middot;Y) = E(X)&middot;E(Y)</frm>)</klr>.
        </ekz>
      </dif>
      <rim>
        Troveblas <ctl>stokaste nede<tld/>a</ctl> en
        <fnt><bib>MatVort</bib></fnt>, probable kun la sencoj
        <sncref ref="pend.nede0a.okazoj"/> kaj
        <sncref ref="pend.nede0a.hazardaj_variabloj"/>.
        Por la samaj sencoj troveblas <ctl>stokaste
        sende<tld/>a</ctl>
        en <fnt><bib>MatStokTerm</bib>, <lok>p. 51</lok></fnt>.
      </rim>
      <trd lng="de">unabh&auml;ngig<klr tip="ind"> (-e
          Zufallsvariable)</klr></trd>
      <trd lng="en">independent<klr tip="ind"> (random
          variable)</klr></trd>
      <trd lng="fr"><klr tip="amb">(variable al&eacute;atoire)
        </klr>ind&eacute;pendante</trd>
      <trd lng="hu">f&uuml;ggetlen<klr tip="ind">
      (v&aacute;ltoz&oacute;)</klr></trd>
      <trd lng="pl"><klr tip="amb">(zmienna losowa)
        </klr>niezale&zdot;na</trd>
      <trd
      lng="ru">&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_a;&c_ja;<klr
      tip="amb"> (&c_s;&c_l;&c_u;&c_ch;&c_a;&c_j;&c_n;&c_a;&c_ja;
          &c_v;&c_je;&c_l;&c_i;&c_ch;&c_i;&c_n;&c_a;)</klr></trd>
    </subsnc>
    <subsnc mrk="pend.nede0a.diversaj">
      <refgrp tip="vid">
        <ref cel="pend.linearenede0a">lineare nede<tld/>a</ref>,
        dependa kaj nedependa <ref cel="variab.0o.MAT">variabloj</ref>.
      </refgrp>
    </subsnc>
    <trd lng="br">dizepant</trd>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_z;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_y;</trd>
  <trd lng="cs">nez&aacute;visl&yacute;</trd>
  <trd lng="fr">ind&eacute;pendant</trd>
  <trd lng="sk">nez&aacute;visl&yacute;</trd>
</drv>

<drv mrk="pend.sende0a">
  <kap>sende<tld/>a</kap>
  <snc mrk="pend.sende0a.KOMUNE">
    <dif>
      De<tld/>anta de neniu a&ubreve; nenio, tute libera:
      <ekz>
        mi jam estas plena&gcirc;a kaj sende<tld/>a
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sende<tld/>a pola Respubliko konsistis el du &ccirc;efaj
        partoj: Pola krono kaj Litvo
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        ri&ccirc;eco estas unu el la plej necesaj kondi&ccirc;oj de
        feli&ccirc;o, &ccirc;ar nur &gcirc;i donas sende<tld/>an
        situacion
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_z;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_y;</trd>
  <trd lng="bg">&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;</trd>
  <trd lng="br">dizalc'h</trd>
  <trdgrp lng="cs">
    <trd>independentn&iacute;</trd>,
    <trd>neodvisl&yacute;</trd>,
    <trd>nez&aacute;visl&yacute;</trd>
  </trdgrp>
  <trd lng="de">unabh&auml;ngig</trd>
  <trd lng="en">independent</trd>
  <trd lng="es">independiente</trd>
  <trd lng="fr">ind&eacute;pendant</trd>
  <trd lng="hu">f&uuml;ggetlen <klr tip="amb">(szabad)</klr></trd>
  <trd lng="nl">onafhankelijk</trd>
  <trd
  lng="ru">&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>nez&aacute;visl&yacute;</trd>,
    <trd>samostatn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="pend.sende0ecxu">
  <kap>sende<tld/>e &ccirc;u</kap>
  <gra>dependiga <vspec>konjunkcio</vspec></gra>
  <snc mrk="pend.sende0ecxu.KOMUNE">
    <dif>
      Egale &ccirc;u tio efektivas a&ubreve; ne;
      kia ajn estas la tiurilata situacio:
      <ekz>
        la estraro klopodas sekvi &ccirc;ion, kio okazas favore al
        Esperanto, 
        sende<tld/>e &ccirc;u tio estas atribuebla a&ubreve; ne al la
        propra
        organiza&jcirc;o
        <fnt>
          <aut>J. A. Vergara</aut>,
          <vrk>en: <url
          ref="http://www.liberafolio.org/2008/lernurenkonto">
          Retaj lernintoj renkonti&gcirc;is en la vera vivo</url></vrk>,
          <lok>libera folio, 2008-08-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_o;
  &c_d;&c_a;&c_l;&c_i;</trd>
  <trd lng="fr">que (+subj.)... ou non</trd>
</drv>

<drv mrk="pend.sende0eje">
  <kap>sende<tld/>e de,
  <var><kap>sende<tld/>e je</kap></var></kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="pend.sende0ede.KOMUNE">
    <dif>
      Sen influo de, ne regate de, memstare:
      <ekz>
        sen<tld/>e de la situacio, kiun homo havas en la 
        mondo <klr>[&#8230;]</klr> &ccirc;iuj estas egalaj
        <fnt>
          <aut>L. Tolstoj, trad. A. &Scirc;aparov</aut>,
          <vrk>Unu animo en &ccirc;iuj</vrk>,
          <lok>1912</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_o;
  &c_o;&c_t;</trd>
  <trd lng="fr">ind&eacute;pendamment de</trd>
</drv>

<drv mrk="pend.sende0eco">
  <kap>sende<tld/>eco</kap>
  <snc mrk="pend.sende0eco.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Plena <ref tip="super" cel="reg.mem0ado.POL">memregeco</ref>:
      <ekz>
        &ccirc;iuj deziras la sende<tld/>econ de la lando;
      </ekz>
      <ekz>
        Hindia sende<tld/>eco reali&gcirc;is sen armiloj, alfrontinte
        potencegan britan armeon
        <fnt>
          <bib>Monato</bib>
          <aut>Michel MARKO</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007265.html">Senperforta
          respondo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxton.0eco.POL">a&ubreve;tonomeco</ref>,
      <ref cel="suvere.0eco.JUR">suvereneco</ref>
    </refgrp>
    <trd lng="de">Unabh&auml;ngigkeit</trd>
    <trdgrp lng="nl">
      <trd>onafhankelijkheid</trd>,
      <trd>zelfstandigheid</trd>,
      <trd>autonomie</trd>
    </trdgrp>
  </snc>
  <trd
  lng="bg">&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_o;&c_s;&c_t;</trd>
  <trd lng="cs">nez&aacute;vislost</trd>
  <trd lng="fr">ind&eacute;pendance</trd>
  <trd
  lng="ru">&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">nez&aacute;vislos&tcaron;</trd>
</drv>

<drv mrk="pend.sende0ismo">
  <kap>sende<tld/>ismo</kap>
  <snc mrk="pend.sende0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Politika movado por memstarigo de provinco a&ubreve; de parto de
      regno:
      <ekz>
        balotado en Skotlando povas signi kreskon de la landa
        sende<tld/>ismo
        <fnt>
          <aut>&Gcirc;angalo</aut>,
          <vrk><url
          ref="http://gxangalo.com/modules/news/article.php?storyid=412">Balotado...</url></vrk>,
          <lok>2003-04-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="bg">&c_s;&c_je;&c_p;&c_a;&c_r;&c_a;&c_t;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trdgrp lng="en">
    <trd>separatism</trd>,
    <trd>secessionism</trd>
  </trdgrp>
  <trd lng="fr">ind&eacute;pendantisme</trd>
  <trd lng="pt">separatismo</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_p;&c_a;&c_r;&c_a;&c_t;&c_i;&c_z;&c_m;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_je;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je; &c_k;
    <ind>&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_o;&c_s;&c_t;&c_i;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="pend.sende0isto">
  <kap>sende<tld/>isto</kap>
  <snc mrk="pend.sende0isto.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="vid" cel="pend.sende0ismo.POL">sen<tld/>ism</ref>ano:
      <ekz>
        la okupado transformi&gcirc;as en armita batalo kontra&ubreve;
        la
        sende<tld/>istoj
        <fnt>
          <aut>N. Birnbaum, trad. V. Lutermano</aut>,
          <vrk><url ref="">&Ccirc;e la radikoj de la usona
          naciismo</url></vrk>,
          <lok>le monde diplomatique, 2002-10</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd
  lng="bg">&c_s;&c_je;&c_p;&c_a;&c_r;&c_a;&c_t;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="en">
    <trd>separatist</trd>,
    <trd>secessionist</trd>
  </trdgrp>
  <trd lng="fr">ind&eacute;pendantiste</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_je;&c_p;&c_a;&c_r;&c_a;&c_t;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_n;&c_i;&c_k;
    <ind>&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_o;&c_s;&c_t;&c_i;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="pend.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="pend.super0i.KOMUNE">
    <dif>
      <tld lit="P"/>i, resti senmove super:
      <ekz>
        el fendoj kaj truoj kreskis siringaj arbeta&jcirc;oj, kiuj
        super<tld/>igis siajn 
        bran&ccirc;ojn super la malgrandaj malaltaj domoj &ccirc;e la
        interna flanko de la urba murego
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malpli granda luno aperis post la 
        super<tld/>antaj tegmentoj
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url
          ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">&uuml;berh&auml;ngen</trd>
    <trd lng="nl">overhangen</trd>
  </snc>
  <trd lng="cs">b&yacute;t p&rcaron;evisl&yacute;</trd>
  <trd lng="fr">suplomber</trd>
  <trd lng="sk">by&tcaron; previsnut&yacute;</trd>
</drv>

<drv mrk="pend.linearede0a">
  <kap>lineare de<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>EKV</bib>, <lok>&FE;257</lok></fnt>
  <snc>
    <ref tip="ant" cel="pend.linearenede0a">lineare
    nede<tld/>a.</ref>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_j;&c_n;&c_a;
  <ind>&c_z;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_y;</ind></trd>
  <trd lng="bg">&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_o;
  &c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;</trd>
  <trd lng="br">depantus ent-linennel</trd>
  <trd lng="de">linear <ind>abh&auml;ngig</ind></trd>
  <trd lng="en">linearly <ind>dependent</ind></trd>
  <trd lng="fr">lin&eacute;airement <ind>d&eacute;pendant</ind></trd>
  <trd lng="hu">line&aacute;risan <ind>f&uuml;gg&odblac;</ind></trd>
  <trd lng="nl">lineaire afhankelijk</trd>
  <trd lng="pl">liniowo <ind>zale&zdot;ny</ind></trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_o;
  <ind>&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_y;&c_j;</ind></trd>
</drv>

<drv mrk="pend.linearenede0a">
  <kap>lineare nede<tld/>a<fnt><bib>MatVort</bib></fnt>,
  <var><kap>lineare
        sende<tld/>a<fnt><bib>EKV</bib>,
        <lok>&FE;258</lok></fnt></kap></var></kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(p.p. <ref cel="vektor.0o.MAT">vektoroj</ref>)</klr>
      Tiaj, ke <ref cel="kombin.lineara0ajxo">lineara
      kombina&jcirc;o</ref>
      el ili estas nula, se kaj nur se &ccirc;iuj &gcirc;iaj
      <ref cel="koefic.0o.lineara">koeficientoj</ref> estas mem
      nulaj:
      <ekz>
        en polinom-ringo, konsiderata kiel
        <ref cel="modul.0o.strukt">modulo</ref> super &gcirc;ia baza
        ringo,
        la polinomoj <frm am="bb X^i"><g>X</g><sup><k>i</k></sup></frm>
        estas
        nede<tld/>aj unuj de la aliaj.
      </ekz>
    </dif>
    <ref tip="vid" cel="liber.0a.lineare">libera.</ref>
    <rim>
      Pro oportuneco oni diras anka&ubreve;
      <ctl><frm am="x"><k>x</k></frm> de<tld/>as de
      <frm am="y"><k>y</k></frm></ctl>,
      <ctl><frm am="x"><k>x</k></frm> estas de<tld/>a de
        <frm am="y"><k>y</k></frm></ctl>
      anstata&ubreve; <ctl><frm am="x"><k>x</k></frm> kaj <frm
      am="y"><k>y</k></frm>
        ne estas nede<tld/>aj</ctl>, kaj
      <ctl><frm am="x"><k>x</k></frm> ne de<tld/>as de
        <frm am="y"><k>y</k></frm></ctl>
      anstata&ubreve;
      <ctl><frm am="x"><k>x</k></frm> kaj <frm am="y"><k>y</k></frm>
      estas
        nede<tld/>aj</ctl>.
    </rim>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_j;&c_n;&c_a;
  <ind>&c_n;&c_je;&c_z;&c_a;&c_l;&c_je;&c_zh;&c_n;&c_y;</ind></trd>
  <trd lng="bg">&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_o;
  &c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;</trd>
  <trd lng="br">dizepant ent-linennel</trd>
  <trd lng="de">linear <ind>unabh&auml;ngig</ind></trd>
  <trd lng="en">linearly <ind>independent</ind></trd>
  <trd lng="es">linealmente <ind>independiente</ind></trd>
  <trdgrp lng="fr">
    <trd>lin&eacute;airement <ind>ind&eacute;pendant</ind></trd>,
    <trd>libre<klr tip="ind"> (lin&eacute;airement
    ind&eacute;pendant)</klr></trd>
  </trdgrp>
  <trd lng="hu">line&aacute;risan <ind>f&uuml;ggetlen</ind></trd>
  <trd lng="nl">lineaire onafhankelijk</trd>
  <trd lng="pl">liniowo <ind>niezale&zdot;ny</ind></trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_o;
  <ind>&c_n;&c_je;&c_z;&c_a;&c_v;&c_i;&c_s;&c_i;&c_m;&c_y;&c_j;</ind></trd>
</drv>

<drv mrk="pend.liter0ajxo">
  <kap>liter<tld/>a&jcirc;o</kap>
  <snc mrk="pend.liter0ajxo.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Tiu parto de la <ctl>profundaj</ctl>
      <ref tip="vid" cel="minuskl.0o">minuskloj</ref> <klr>(g, &gcirc;,
        j, &jcirc;, p ktp)</klr> kiu sinkas pli malalten ol la trunko
        (ol
      la suba rando de la literoj kiel z a&ubreve; m):
      <ekz>
        kompakta tiparo kun malgrandaj liter<tld/>a&jcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Unterl&auml;nge <klr>(Typografie)</klr></trd>
  <trd lng="en">descender</trd>
  <trd lng="fr">jambage</trd>
  <trd lng="ru">&c_n;&c_i;&c_zh;&c_n;&c_i;&c_j;
  &c_v;&c_y;&c_n;&c_o;&c_s;&c_n;&c_o;&c_j;
  &c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;<klr>
  (&c_b;&c_u;&c_k;&c_v;&c_y;)</klr></trd>
  <trd lng="sv">&ouml;verh&auml;ng</trd>
</drv>

<drv mrk="pend.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc mrk="pend.mal0ajxo.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Tiu parto de la <ctl>altaj</ctl>
      <ref tip="vid" cel="minuskl.0o">minuskloj</ref> <klr>(b, d,
        f, h, k ktp)</klr> kiu etendi&gcirc;as super la trunkon (pli
        alten
      ol la supra rando de la literoj kiel z a&ubreve; v):
      <ekz>
        la litero &hcirc; havas &ccirc;apelon super sia
        mal<tld/>a&jcirc;o a&ubreve; super
        la genuo.
      </ekz>
    </dif>
    <trd lng="de">Oberl&auml;nge <klr>(Typografie)</klr></trd>
    <trd lng="nl">stokletter</trd>
  </snc>
  <trd lng="en">ascender</trd>
  <trdgrp lng="fr">
    <trd>jambage sup&eacute;rieur</trd>,
    <trd>hampe</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_h;&c_n;&c_i;&c_j;
  &c_v;&c_y;&c_n;&c_o;&c_s;&c_n;&c_o;&c_j;
  &c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;<klr>
  (&c_b;&c_u;&c_k;&c_v;&c_y;)</klr></trd>
  <trd lng="sv">underh&auml;ng</trd>
</drv>

<drv mrk="pend.orel0ajxo">
  <kap>orel<tld/>a&jcirc;o</kap>
  <snc mrk="pend.orel0ajxo.JUV">
    <dif>
      <ref tip="dif" cel="ring.orel0o.JUV">Orelringo</ref>:
      <ekz>
        la sinjorino estis ri&ccirc;e kaj kokete vestita,
        &scirc;iaj orelpenda&jcirc;oj el perloj brile radiis malproksimen
        sub la suno
        <fnt>
          <aut>Yang Mo, trad, Yang Yongsen</aut>
          <vrk><url
          ref="http://www.elerno.cn/elibro/Kanto%20de%20Juneco.pdf">
          Kanto de Juneco</url></vrk>
          <lok>1981</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_v;&c_u;&c_sh;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="br">kloc'hig-skouarn</trd>
  <trd lng="cs">n&aacute;u&scaron;nice</trd>
  <trd lng="de">Ohrring</trd>
  <trd lng="es">pendiente</trd>
  <trd lng="fr">boucle d'oreille</trd>
  <trd lng="he">&ayin;&gimel;&yod;&lamed;</trd>
  <trd lng="hu">f&uuml;lkarika</trd>
  <trd lng="nl">oorring</trd>
  <trd lng="no">&oslash;redobbe</trd>
  <trd lng="pl">kolczyk</trd>
  <trd lng="pt">brinco</trd>
  <trdgrp lng="ru">
     <trd>&c_s;&c_je;&c_r;&c_mol;&c_g;&c_a;</trd>,
     <trd>&c_s;&c_je;&c_r;&c_jo;&c_zh;&c_k;&c_a;</trd>
   </trdgrp>
  <trd lng="sk">n&aacute;u&scaron;nica</trd>
  <trd lng="sv">&ouml;rh&auml;nge</trd>
</drv>
