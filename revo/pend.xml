  <trd lng="de">Galgen</trd>
  <trd lng="es">horca</trd>
  <trdgrp lng="hu">
    <trd>akaszt&oacute;fa</trd>,
    <trd>bit&oacute;</trd>
  </trdgrp>
  <trd lng="nl">galg</trd>
</drv>

<drv mrk="pend.0umi">
  <kap><tld/>umi</kap>
  <snc mrk="pend.0umi.KOMUNE">
    <dif>
      Levi iun de tero per &scirc;nuro &ccirc;irka&ubreve;anta lian kolon por
      <ref tip="super" cel="mort.0igi.malvivigi">mortigi</ref> lin per
      gor&gcirc;opremo:
      <ekz>
        <tld/>umi krimulon;
      </ekz>
      <ekz>
        sin <tld/>umi;
      </ekz>
      <ekz>
        <tld/>uminda fripono.
      </ekz>
    </dif>
    <ref tip="super" cel="ekzeku.0i">ekzekuti</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_je;&c_sh;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_c;&c_mol;)</klr></trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_s;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_c;&c_mol;)</klr></trd>
  </trdgrp>
  <trd lng="br">krouga&ntilde;</trd>
  <trd lng="de">aufh&auml;ngen</trd>
  <trdgrp lng="es">
    <trd>colgar</trd>,
    <trd>ahorcar</trd>
  </trdgrp>
  <trd lng="fr">pendre<klr tip="ind"> (ex&eacute;cuter)</klr></trd>
  <trdgrp lng="hu">
    <trd>felakaszt</trd>,
    <trd>fell&oacute;gat</trd>
  </trdgrp>
  <trd lng="nl">ophangen<klr tip="ind"> (als straf)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_sh;&c_a;&c_t;&c_mol;<klr tip="ind"> (&c_k;&c_a;&c_z;&c_n;&c_i;&c_t;&c_mol;)</klr></trd>,
    <trd>&c_p;&c_o;&c_v;&c_je;&c_s;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_k;&c_a;&c_z;&c_n;&c_i;&c_t;&c_mol;)</klr></trd>
  </trdgrp>
</drv>

