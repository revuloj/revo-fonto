</drv>

<drv mrk="pend.0ingo">
  <kap><tld/>ingo</kap>
  <fnt><bib>PV</bib></fnt>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="pend.0ingo.pendumilo">
    <ref tip="dif" cel="pend.0umilo"><tld/>umilo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_y;&c_b;&c_je;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_ja;</trd>
