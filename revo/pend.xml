  <kap><tld/>ingo</kap>
  <fnt><bib>Rabistoj</bib> </fnt>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="pend.0ingo.pendumilo">
    <dif>
      <ref tip="dif" cel="pend.0umilo"><tld lit="P"/>umilo</ref>:
      <ekz>
        sur la unua renkontita <tld/>ingo oni devas <tld/>igi tiun
        kanajlon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
