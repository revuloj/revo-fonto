<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vejn.xml,v 1.33 2024/08/04 12:44:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vejn</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vejn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vejn.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Tubeto rekondukanta la sangon al la koro.
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_je;&c_n;&c_a;</trd>,
      <trd>&c_zh;&c_y;&c_l;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">vena</trd>
    <trd lng="de">Vene</trd>
    <trdgrp lng="hu">
      <trd>v&eacute;na</trd>,
      <trd>vissz&eacute;r</trd>
    </trdgrp>
    <trd lng="es">vena</trd>
    <trdgrp lng="id">
      <trd>vena</trd>,
      <trd>pem<ind>buluh</ind> balik</trd>
    </trdgrp>
    <trd lng="it">vena</trd>
    <trd lng="pl">&zdot;y&lstroke;a</trd>
    <trd lng="ru">&c_v;&c_je;&c_n;&c_a;</trd>
    <trd lng="sv">ven</trd>
    <trd lng="tr">kan damar&inodot;</trd>
  </snc>
  <snc mrk="vejn.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Longa mallar&gcirc;a tavolo el sama substanco en grundo:
      <ekz>
        mineja <tld/>o el karbo.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_l;&c_o;&c_zh;&c_zh;&c_a;</trd>,
      <trd>&c_zh;&c_y;&c_l;&c_a; <klr tip="amb">(&c_g;&c_je;&c_a;&c_l;.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>fil&oacute;</trd>,
      <trd>bei</trd>
    </trdgrp>
    <trd lng="de">Ader</trd>
    <trdgrp lng="es">
      <trd>veta</trd>,
      <trd>fil&oacute;n</trd>
    </trdgrp>
    <trd lng="hu">tel&eacute;r</trd>
    <trd lng="it">vena</trd>
    <trd lng="pl">&zdot;y&lstroke;a</trd>
    <trd lng="ru">&c_zh;&c_i;&c_l;&c_a; <klr tip="amb">(&c_g;&c_je;&c_o;&c_l;.)</klr></trd>
  </snc>
  <snc mrk="vejn.0o.FIG">
    <dif>
      Ondlinia streko, simila al <tld/>oj viditaj sub la ha&ubreve;to:
      <ekz>
        la <tld/>oj de marmoro, ligno;
      </ekz>
      <ekz>
        la rivero estis <tld/>ita per longa poststreko da sango.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_zh;&c_y;&c_l;&c_a;&c_k;</trd>
    <trdgrp lng="ca">
      <trd>veta</trd>,
      <trd>nervi</trd>,
      <trd>vin&ccedil;a</trd>
    </trdgrp>
    <trd lng="es">vena</trd>
    <trd lng="hu">erezet</trd>
    <trd lng="it">venatura</trd>
    <trd lng="pl">&zdot;y&lstroke;a</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_zh;&c_i;&c_l;&c_k;&c_a;</trd>
  </snc>
  <trd lng="bg">&c_v;&c_je;&c_n;&c_a;</trd>
  <trd lng="bo">&#xF62;&#xFA9;&#xF0B;</trd>
  <trd lng="ca">vena</trd>
  <trdgrp lng="cs">
    <trd>v&#xE9;na</trd>,
    <trd>&#x17E;&#xED;la</trd>
  </trdgrp>
  <trd lng="el">&alfa;&gamma;&gamma;&epsilon;&jota_ton;&omikron;</trd>
  <trd lng="en">vein</trd>
  <trd lng="fr">veine</trd>
  <trdgrp lng="ja">
    <trd>&#38745;&#33032;  <pr>&#12376;&#12423;&#12358;&#x307F;&#12419;&#x304F;</pr></trd>,
    <trd>&#37489;&#33032;  <pr>&#12371;&#12358;&#x307F;&#12419;&#x304F;</pr></trd>,
    <trd>&#26408;&#x76EE;  <pr>&#12418;&#x304F;&#12417;</pr></trd>,
    <trd>&#x77F3;&#x76EE;  <pr>&#12356;&#12375;&#12417;</pr></trd>
  </trdgrp>
  <trd lng="nl">ader</trd>
  <trd lng="pl">&zdot;y&lstroke;a</trd>
  <trd lng="pt">veia</trd>
  <trd lng="sk">&zcaron;ila</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&#x43D;&c_a;</trd>,
    <trd>&c_zh;&c_i;&#x43B;&c_a;</trd>,
    <trd>&c_zh;&c_i;&#x43B;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38738;&#x7B4B; <pr>q&#x12B;ngj&#x12B;n</pr></trd>,
    <trd>&#x7D0B;&#29702; <pr>w&#xE9;nl&#x1D0;</pr></trd>,
    <trd>&#x7EB9;&#29702; <pr>w&#xE9;nl&#x1D0;</pr></trd>,
    <trd>&#38745;&#33033; <pr>j&#xEC;ngm&#xE0;i</pr></trd>,
    <trd>&#x975C;&#33032; <pr>j&#xEC;ngm&#xE0;i</pr></trd>,
    <trd>&#x739F; <pr>w&#xE9;n</pr></trd>,
    <trd>&#x7D0B;&#x7E37; <pr>w&#xE9;nl&#x1D4;:</pr></trd>,
    <trd>&#x7EB9;&#x7F15; <pr>w&#xE9;nl&#x1D4;:</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vejn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vejn.0i.FIG">
    <dif>
      Marki <tld/>ojn sur ion.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_p;&c_r;&c_a;&c_zh;&c_y;&c_l;&c_k;&c_ib;</ind></trd>
  <trd lng="ca">marcar les venes</trd>
  <trd lng="cs">imitovat &#x17E;ilkov&#xE1;n&#xED;</trd>
  <trdgrp lng="en">
    <trd>mark with veins</trd>,
    <trd>marble</trd>
  </trdgrp>
  <trd lng="es">marcar las venas</trd>
  <trd lng="fr">veiner</trd>
  <trd lng="hu">erez</trd>
  <trd lng="it">venare</trd>
  <trdgrp lng="ja">
    <trd>&#26408;&#x76EE;&#x6A21;&#x69D8;&#12434;&#12388;&#12369;&#x308B;  <pr>&#12418;&#x304F;&#12417;&#12418;&#12424;&#12358;&#12434;&#12388;&#12369;&#x308B;</pr></trd>,
    <trd>&#x77F3;&#x76EE;&#x6A21;&#x69D8;&#12434;&#12388;&#12369;&#x308B;  <pr>&#12356;&#12375;&#12417;&#12418;&#12424;&#12358;&#12434;&#12388;&#12369;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>aderen</trd>,
    <trd>marmeren</trd>
  </trdgrp>
  <trd lng="pl">&zdot;y&lstroke;&eogonek; zaznacza&cacute;</trd>
  <trd lng="sk">&#x17E;ilkova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_zh;&c_i;&#x43B;&#x43A;&c_i;</trd>,
    <trd>&#x43E;&c_b;&c_r;&#x43E;&c_b;&#x43B;&#x44F;&c_t;&c_i; &#x43F;&c_ib;&c_d; &c_d;&c_je;&c_r;&c_je;&c_v;&#x43E;</trd>,
    <trd>&#x43C;&c_a;&c_r;&#x43C;&c_u;&c_r;</trd>
  </trdgrp>
</drv>
</art>
