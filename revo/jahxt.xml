<art mrk="$Id: jahxt.xml,v 1.22 2023/12/03 07:09:06 revo Exp $">
<kap>
  <rad>ja&hcirc;t</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="k">jakt</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="jahxt.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Eleganta, luksa, malpeza promen<ref tip="super" cel="sxip.0o">&scirc;ipo</ref>:
      <ekz>
        milde luli&gcirc;as lia &jcirc;us akirita <tld/>o
        <fnt>
          <bib>KrB</bib>
          <lok>Por kapti ezokon, bongustigu la hokon</lok>
        </fnt>;
      </ekz>
      <ekz>
        Paskalo kaj Heleno <klr>[&#x2026;]</klr> mem ellaboris la tutan konstruadon de la
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2003/007992.html">Persistemo donis plezuron!</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        vidi&gcirc;as golfejo, <tld var="k"/>a klubo, &ccirc;asejo, hipodromo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50150">Dalian</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kutimi&gcirc;inta al grandaj domoj, palacoj, <tld/>oj, la plej
        luksaj hoteloj
        <fnt>
          <aut>Paul Gubbins</aut>:
          <vrk>Diana... &ccirc;u diino?</vrk>,
          <bib>Monato</bib><lok>jaro 1997a, numero 9a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_h;&c_t;&c_a;</trd>
  <trd lng="ca">iot</trd>
  <trd lng="de">Jacht</trd>
  <trd lng="el">&kappa;&omikron_ton;&tau;&epsilon;&rho;&omikron;</trd>
  <trd lng="en">yacht</trd>
  <trd lng="es">yate</trd>
  <trdgrp lng="fr">
    <trd>yacht</trd>
  </trdgrp>
  <trd lng="hu">jacht</trd>
  <trdgrp lng="it">
    <trd>panfilo</trd>,
    <trd>yatch</trd>
  </trdgrp>
  <trd lng="ja">&#x30E8;&#x30C3;&#x30C8;  <pr>&#x3088;&#x3063;&#x3068;</pr></trd>
  <trd lng="nl">jacht</trd>
  <trd lng="pl">jacht</trd>
  <trd lng="pt">iate</trd>
  <trd lng="ru">&c_ja;&c_h;&c_t;&c_a;</trd>
  <trd lng="sk">jachta</trd>
  <trd lng="sv"><klr>(lust)</klr>jakt</trd>
  <trd lng="uk">&c_ja;&c_h;&c_t;&c_a;</trd>
  <trd lng="zh">&#x6E38;&#x8247; <pr>y&#xF3;ut&#x1D0;ng</pr></trd>
</drv>
</art>
