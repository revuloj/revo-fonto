<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ja&hcirc;t</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="jahxt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Eleganta, luksa, malpeza promen&scirc;ipo:
      <ekz>
        kutimi&gcirc;inta al grandaj domoj, palacoj, <tld/>oj, la plej luksaj
        hoteloj<fnt><aut>Paul Gubbins</aut>: <vrk>Diana... &ccirc;u
        diino?</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 9a, p.
        16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_h;&c_t;&c_a;</trd>
  <trd lng="de">Jacht</trd>
  <trd lng="el">&kappa;&omikron_ton;&tau;&epsilon;&rho;&omikron;</trd>
  <trd lng="en">yatch</trd>
  <trd lng="es">yate</trd>
  <trdgrp lng="fr">
    <trd>yacht</trd>
  </trdgrp>
  <trd lng="hu">jacht</trd>
  <trdgrp lng="it">
    <trd>panfilo</trd>,
    <trd>yatch</trd>
  </trdgrp>
  <trd lng="nl">jacht</trd>
  <trd lng="pt">iate</trd>
  <trd lng="ru">&c_ja;&c_h;&c_t;&c_a;</trd>
  <trd lng="sk">jachta</trd>
  <trd lng="sv"><klr>(lust)</klr>jakt</trd>
</drv>
</art>
