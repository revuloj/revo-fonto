<vortaro>
  <art mrk="$Id: psoriaz.xml,v 1.2 2023/09/01 18:29:28 afido Exp $">
    <kap>
      <rad>psoriaz</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="psoriaz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="psoriaz.0o.malsano">
    <dif>
      Nekonta&gcirc;a &hcirc;ronika memimun<ref tip="lst" lst="voko:malsanoj" cel="san.mal0o.MED">malsano</ref>,
      manifesti&gcirc;anta aparte per jukanta skvameca ha&ubreve;to:
      <ekz>
        en la banlokoj oni kuracas <klr>[&#8230;]</klr> anka&ubreve; ginekologiajn kaj ha&ubreve;tajn
        malsanojn inkluzive de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2015/011372.php"
              >Moderna banloko</url></vrk>
          <lok>2015/01, p. 20</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Psoriasis</trd>,
      <trd>Schuppenflechte</trd>
    </trdgrp>
    <trd lng="en">Psoriasis</trd>
    <trd lng="fr">Psoriasis</trd>
    <trd lng="nl">psoriasis</trd>
  </snc>
</drv>

  </art>

