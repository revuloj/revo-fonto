    <trd>&#x94F6;&#x5C51;&#x75C5; <pr>y&#xED;nxi&#xE8;b&#xEC;ng</pr></trd>,
    <trd>&#x725B;&#x76AE;&#30307; <pr>ni&#xFA;p&#xED;xi&#x1CE;n</pr></trd>,
    <trd>&#x725B;&#x76AE;&#x766C; <pr>ni&#xFA;p&#xED;xi&#x1CE;n</pr></trd>
