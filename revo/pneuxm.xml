<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pneuxm.xml,v 1.19 2022/03/26 12:56:09 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>pne&ubreve;matik</rad>/o
</kap>
<drv mrk="pneuxm.0o">
  <kap><tld/>o</kap>
  <snc mrk="pneuxm.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Gas<ref tip="malprt" cel="fizik.0o.FIZ">fiziko</ref>.
    </dif>
    <trd lng="be">&c_p;&c_n;&c_e;&c_w;&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;
    </trd>
    <trd lng="de">Pneumatik</trd>
    <trd lng="en">pneumatics</trd>
    <trd lng="fr">pneumatique<klr> (subst.)</klr></trd>
    <trdgrp lng="hu">
      <trd>pneumatika</trd>,
      <trd>g&aacute;ztan</trd>
    </trdgrp>
    <trd lng="it">pneumatica<klr> (scienza)</klr></trd>
    <trd lng="nl">pneumatiek</trd>
    <trd lng="pl">pneumatyka</trd>
    <trd lng="pt">pneum&aacute;tica</trd>
    <trd lng="ru">&c_p;&c_n;&c_je;&c_v;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;
    </trd>
  </snc>
  <snc mrk="pneuxm.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <ref tip="dif" cel="pneux.0o.AUT">pne&ubreve;o</ref>
    <trd lng="be">&c_sh;&c_y;&c_n;&c_a;
    <klr>
    (&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_a;&c_g;&c_a;
    &c_s;&c_r;&c_o;&c_d;&c_k;&c_a;)</klr>
    </trd>
    <trd lng="de">Reifen</trd>
    <trd lng="en">tire</trd>
    <trd lng="fr">pneu</trd>
    <trd lng="hu">gumiabroncs</trd>    
    <trd lng="it">pneumatico<klr> (sost.)</klr></trd>
    <trd lng="nl">band<klr tip="ind"> (v.auto, fiets...)</klr></trd>
    <trdgrp lng="pl">
      <trd>opona<klr> (z ew. d&eogonek;tk&aogonek;)</klr></trd>,
      <trd>pneumatyka<klr> (przest.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>pneu</trd>,
      <trd>pneum&aacute;tico</trd>
    </trdgrp>
    <trd lng="ru">&c_sh;&c_i;&c_n;&c_a;</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43D;&#x435;&#x432;&#x43C;&#x430;&#x442;&#x438;&#x43A;&#x430;</trd>,
    <trd>&#x448;&#x438;&#x43D;&#x430; (&#x430;&#x432;&#x442;&#x43E;&#x43C;&#x43E;&#x431;&#x456;&#x43B;&#x44C;&#x43D;&#x430;</trd>,
    <trd>&#x432;&#x435;&#x43B;&#x43E;&#x441;&#x438;&#x43F;&#x435;&#x434;&#x43D;&#x430;)</trd>
  </trdgrp>
</drv>

<drv mrk="pneuxm.0a">
  <kap><tld/>a</kap>
  <snc mrk="pneuxm.0a.FIZ">
    <dif>
      Uzanta la aeron kiel movilon a&ubreve; moderigilon:
      <ekz>
        <tld/>a tubo, depe&scirc;o, ma&scirc;ino, kuseno.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_n;&c_e;&c_w;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;
  </trd>
  <trd lng="de">pneumatisch</trd>
  <trd lng="fr">pneumatique<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>pneumatikus</trd>,
    <trd>s&udblac;r&iacute;tett leveg&odblac;s</trd>
  </trdgrp>
  <trd lng="it">pneumatico<klr> (agg.)</klr></trd>
  <trd lng="nl">pneumatisch</trd>
  <trd lng="pl">pneumatyczny</trd>
  <trd lng="ru">&c_p;&c_n;&c_je;&c_v;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43D;&#x435;&#x432;&#x43C;&#x430;&#x442;&#x438;&#x447;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x448;&#x438;&#x43D;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>
</art>
