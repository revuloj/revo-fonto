<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pneuxm.xml,v 1.20 2023/09/25 06:28:37 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>pne&ubreve;matik</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="pneuxm.0o">
  <kap><tld/>o</kap>
  <snc mrk="pneuxm.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Bran&ccirc;o de <ref tip="malprt" cel="fizik.0o.FIZ">fiziko</ref> kaj te&hcirc;niko
      okupi&gcirc;anta pri la me&hcirc;anikaj ecoj de gasoj.
    </dif>
    <trd lng="be">&c_p;&c_n;&c_e;&c_w;&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;
    </trd>
    <trd lng="de">Pneumatik</trd>
    <trd lng="en">pneumatics</trd>
    <trd lng="fr">pneumatique<klr> (subst.)</klr></trd>
    <trdgrp lng="hu">
      <trd>pneumatika</trd>,
      <trd>g&aacute;ztan</trd>
    </trdgrp>
    <trd lng="it">pneumatica<klr> (scienza)</klr></trd>
    <trd lng="nl">pneumatiek</trd>
    <trd lng="pl">pneumatyka</trd>
    <trd lng="pt">pneum&aacute;tica</trd>
    <trd lng="ru">&c_p;&c_n;&c_je;&c_v;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;
    </trd>
  </snc>
  <snc mrk="pneuxm.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      <ref tip="dif" cel="pneux.0o.AUT">Pne&ubreve;o</ref>:
      <ekz>
        unu el la <tld/>oj trapiki&gcirc;is kaj pro la malsperteco de la veturigisto,
        prokrastado okazis
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la berlina asfalto estas varmigita de la suno kaj trotado de ka&ubreve;&ccirc;ukaj <tld/>oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_y;&c_n;&c_a;
    <klr>
    (&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_a;&c_g;&c_a;
    &c_s;&c_r;&c_o;&c_d;&c_k;&c_a;)</klr>
    </trd>
    <trdgrp lng="de">
      <trd>Luftbereifung</trd>,
      <trd>Reifen</trd>
    </trdgrp>
    <trd lng="en">tire</trd>
    <trd lng="fr">pneu</trd>
    <trd lng="hu">gumiabroncs</trd>
    <trd lng="it">pneumatico<klr> (sost.)</klr></trd>
    <trd lng="nl">band<klr tip="ind"> (v.auto, fiets...)</klr></trd>
    <trdgrp lng="pl">
      <trd>opona<klr> (z ew. d&eogonek;tk&aogonek;)</klr></trd>,
      <trd>pneumatyka<klr> (przest.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>pneu</trd>,
      <trd>pneum&aacute;tico</trd>
    </trdgrp>
    <trd lng="ru">&c_sh;&c_i;&c_n;&c_a;</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&c_p;&c_n;&c_je;&c_v;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_i;&c_n;&c_a; (&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_ib;&c_l;&c_mol;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_o;&c_s;&c_i;&c_p;&c_je;&c_d;&c_n;&c_a;)</trd>
  </trdgrp>
</drv>

<drv mrk="pneuxm.0a">
  <kap><tld/>a</kap>
  <snc mrk="pneuxm.0a.FIZ">
    <dif>
      Uzanta la aeron kiel movilon a&ubreve; moderigilon:
      <ekz>
        <tld/>a tubo, depe&scirc;o, ma&scirc;ino
      </ekz>
      <ekz>
        <ind><tld/>a kuseno</ind>
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
        <trd lng="de">Luftkissen</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_n;&c_e;&c_w;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;
  </trd>
  <trd lng="de">pneumatisch</trd>
  <trd lng="fr">pneumatique<klr> (adj.)</klr></trd>
  <trdgrp lng="hu">
    <trd>pneumatikus</trd>,
    <trd>s&udblac;r&iacute;tett leveg&odblac;s</trd>
  </trdgrp>
  <trd lng="it">pneumatico<klr> (agg.)</klr></trd>
  <trd lng="nl">pneumatisch</trd>
  <trd lng="pl">pneumatyczny</trd>
  <trd lng="ru">&c_p;&c_n;&c_je;&c_v;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_n;&c_je;&c_v;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_sh;&c_i;&c_n;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
