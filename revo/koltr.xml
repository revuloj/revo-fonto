<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: koltr.xml,v 1.3 2023/08/24 08:30:02 revo Exp $">
<kap>
  <rad>koltr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="koltr.0o">
  <kap><tld/>o</kap>
  <snc mrk="koltr.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Parto de iuj
      <ref tip="malprt" cel="plug.0ilo.AGR">plugiloj</ref>,
      anta&ubreve;a vertikala klingo, kiu tran&ccirc;as la grundon prepare
      al la posta laboro de la soko:
      <ekz>
        iun nokton mi deprenis
        feran <tld/>on de plugilo
        <fnt>
          <aut>-, trad. P.-J. Doumen</aut>,
          <vrk>Karlo kaj Elegasto</vrk>,
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ib;&c_r;&c_v;&c_a;&c_n;&c_a;&c_z;&c_d;&c_y;&c_m;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_d;&c_p;&c_l;&c_u;&c_zh;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="fr">coutre</trd>
  <trd lng="it">coltro</trd>
  <trd lng="uk">&#x43F;&c_je;&c_r;&c_je;&c_d;&#x43F;&#x43B;&c_u;&c_zh;&#x43D;&c_i;&#x43A;</trd>
</drv></art>

