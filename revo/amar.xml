<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: amar.xml,v 1.22 2021/05/29 10:56:10 revo Exp $">
    <kap>
      <ofc>4</ofc>
      <rad>amar</rad>/a
      <fnt>Z</fnt>
    </kap>

    <drv mrk="amar.0a">
      <kap><tld/>a</kap>
      <snc mrk="amar.0a.gusto">
	<dif>
  	  <ref tip="sin" cel="dolcx.mal0a.gusto">Maldol&ccirc;a</ref> por la
          <ref tip="lst" cel="gust.0o.eco" lst="voko:gustoj">gusto</ref>, kiel galo, aloo, genciano,
          leontodo, kakao:
  	  <ekz>
  	    por bu&scirc;o <tld/>a, por koro dol&ccirc;a.
  	  </ekz>
      </dif>
      </snc>
      <snc mrk="amar.0a.sento">
	<dif>
  	  Tiel maldol&ccirc;a por la koro, kiel por la lipoj; akra, malmilda:
  	  <ekz>
  	    morto de patrino estas <tld/>a doloro;
  	  </ekz>
  	  <ekz>
  	    <tld/>a beda&ubreve;ro, moko, parolo, kritiko, ripro&ccirc;o.
  	  </ekz>
        </dif>
        <ref tip="sin" cel="dolcx.mal0a.sento">maldol&ccirc;a</ref>
      </snc>
  <trd lng="be">&c_g;&c_o;&c_r;&c_k;&c_ib;</trd>
  <trd lng="bo">&#xF41;&#xF0B;&#xF4F;&#xF72;&#xF42;&#xF0B;</trd>
  <trd lng="ca">amarg</trd>
  <trdgrp lng="cs">
    <trd>ho&#x159;k&#xFD;</trd>,
    <trd>trpk&#xFD; <klr>(i citov&#x11B;)</klr></trd>,
    <trd>zaho&#x159;kl&#xFD;</trd>
  </trdgrp>
  <trd lng="de">bitter</trd>
  <trd lng="en">bitter</trd>
  <trd lng="es">amargo</trd>
  <trd lng="fr">amer</trd>
  <trd lng="he">&#x5D8;&#x5E2;&#x5DD; &#x5DE;&#x5E8;</trd>
  <trd lng="id">pahit</trd>
  <trd lng="it">amaro</trd>
  <trd lng="nl">bitter</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_mol;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>hork&yacute;</trd>,
    <trd>trpk&#xFD;</trd>
  </trdgrp>
</drv>
  </art>
