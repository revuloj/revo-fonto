<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>4</ofc>
      <rad>amar</rad>/a
      <fnt>Z</fnt>
    </kap>

    <drv mrk="amar.0a">
      <kap><tld/>a</kap>
      <snc mrk="amar.0a.gusto">
	<dif>
  	  <ref tip="sin" cel="dolcx.mal0a.gusto">Maldol&ccirc;a</ref> por la
          gusto, kiel galo, aloo, genciano, leontodo, kakao:
  	  <ekz>
  	    por bu&scirc;o <tld/>a, por koro dol&ccirc;a.
  	  </ekz>
      </dif>
      </snc>
      <snc mrk="amar.0a.sento">
	<dif>
  	  Tiel maldol&ccirc;a por la koro, kiel por la lipoj; akra, malmilda:
  	  <ekz>
  	    morto de patrino estas <tld/>a doloro;
  	  </ekz>
  	  <ekz>
  	    <tld/>a beda&ubreve;ro, moko, parolo, kritiko, ripro&ccirc;o.
  	  </ekz>
        </dif>
        <ref tip="sin" cel="dolcx.mal0a.sento">maldol&ccirc;a</ref>
      </snc>
      <trd lng="be">&c_g;&c_o;&c_r;&c_k;&c_ib;</trd>
      <trd lng="en">bitter</trd>
      <trd lng="es">amargo</trd>
      <trd lng="fr">amer</trd>
      <trd lng="it">amaro</trd>
      <trd lng="nl">bitter</trd>
      <trd lng="ru">&c_g;&c_o;&c_r;&c_mol;&c_k;&c_i;&c_j;</trd>
      <trd lng="sk">hork&yacute;</trd>
    </drv>
  </art>
