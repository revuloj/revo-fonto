<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kapriol</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kaprio.0o">
  <kap><tld/>o</kap>
  <snc mrk="kaprio.0o.SPO">
  <uzo tip="fak">SPO</uzo>
    <dif>
      Salto en aero, la&ubreve; kapra maniero.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_a;&c_ch;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Luftsprung</trd>,
    <trd>Bocksprung</trd>,
    <trd>Kapriole</trd>
  </trdgrp>
  <trd lng="en">caper</trd>
  <trd lng="es">cabriola</trd>
  <trdgrp lng="fr">
    <trd>cabriole</trd>,
    <trd>galipette</trd>
  </trdgrp>
  <trd lng="hu">bakugr&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>capriool</trd>,
    <trd>bokkensprong</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kozio&lstroke;ek</trd>,
    <trd>salto</trd>,
    <trd>fiko&lstroke;ek</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_ch;&c_o;&c_k;</trd>
</drv>

<drv mrk="kaprio.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kaprio.0i.SPO">
    <dif>
      Aersalti; salti multe kaj malpeze.
    </dif>
    <refgrp tip="vid">
      <ref cel="petol.0i">petoli</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_a;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_s;&c_k;&c_o;&c_k;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Luftspr&uuml;nge machen</trd>,
    <trd>Bockspr&uuml;nge machen</trd>,
    <trd>Kapriolen schlagen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>caper</trd>,
    <trd>frisk</trd>,
    <trd>gambol</trd>,
    <trd>leap about</trd>,
    <trd>prance</trd>,
    <trd>romp</trd>
  </trdgrp>
  <trd lng="fr">cabrioler</trd>
  <trdgrp lng="hu">
    <trd>sz&ouml;kd&eacute;csel</trd>,
    <trd>ugr&aacute;ndozik</trd>
  </trdgrp>
  <trd lng="nl">bokkensprongen maken</trd>
  <trdgrp lng="pl">
    <trd>robi&cacute; <ind>kozio&lstroke;ki</ind></trd>,
    <trd>fika&cacute; <ind>kozio&lstroke;ki</ind></trd>,
    <trd>robi&cacute; <ind>salta</ind></trd>,
    <trd>robi&cacute; <ind>fiko&lstroke;ki</ind></trd>,
    <trd>fika&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
