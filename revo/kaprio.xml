<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kapriol</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kaprio.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Salto en aero, la&ubreve; kapra maniero.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Luftsprung</trd>,
    <trd>Bocksprung</trd>,
    <trd>Kapriole</trd>
  </trdgrp>
  <trd lng="en">caper</trd>
  <trd lng="nl">bokkensprong</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_ch;&c_o;&c_k;</trd>
</drv>

<drv mrk="kaprio.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Aersalti; salti multe kaj malpeze.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Luftspr&uuml;nge machen</trd>,
    <trd>Bockspr&uuml;nge machen</trd>,
    <trd>Kapriolen schlagen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>caper</trd>,
    <trd>frisk</trd>,
    <trd>gambol</trd>,
    <trd>leap about</trd>,
    <trd>prance</trd>,
    <trd>romp</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
