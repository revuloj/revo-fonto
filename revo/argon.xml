  <trd lng="es">arg&oacute;n</trd>
  <trd lng="fr">argon</trd>
  <trd lng="hu">argon</trd>
  <trd lng="it">argon</trd>
  <trd lng="nl">argon</trd>
  <trdgrp lng="pt">
    <trd>arg&ocirc;nio</trd>,
    <trd>&aacute;rgon</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_r;&c_g;&c_o;&c_n;</trd>
  <trd lng="sk">arg&oacute;n</trd>
  <trd lng="tr">argon</trd>
</drv>
