<art mrk="$Id$">
<kap>
  <rad>argon</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="argon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref>
      kun atomnumero 18, atompezo 39,948, simbolo Ar.
