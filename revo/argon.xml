<art mrk="$Id: argon.xml,v 1.34 2023/08/30 18:23:36 revo Exp $">
<kap>
  <rad>argon</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="argon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Ar</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="18">elemento</ref>
      kun atomnumero 18, atompezo 39,948;
      senodora, senkolora <ref tip="super" cel="gas.nobla0o">nobla
      gaso</ref>, ekzistanta je malgranda kvanto
      en la tera atmosfero:
      <ekz>
        la atmosfero de Tero konsistas de proksime 1&#x202F;% <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22190">Argono</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_g;&c_o;&c_n;</trd>
  <trd lng="bg">&c_a;&c_r;&c_g;&c_o;&c_n;</trd>
  <trd lng="br">argon</trd>
  <trd lng="ca">arg&oacute;</trd>
  <trd lng="cs">argon</trd>
  <trd lng="de">Argon</trd>
  <trd lng="en">argon</trd>
  <trd lng="es">arg&oacute;n</trd>
  <trd lng="fa">&a_A_madda;&a_r;&f_g;&a_w;&a_n;</trd>
  <trd lng="fr">argon</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E8;&#x5D2;&#x5D5;&#x5B9;&#x5DF;</trd>
  <trd lng="hu">argon</trd>
  <trd lng="id">argon</trd>
  <trd lng="it">argon</trd>
  <trd lng="ja">&#x30A2;&#x30EB;&#x30B4;&#x30F3;</trd>
  <trd lng="la">Argon</trd>
  <trd lng="nl">argon</trd>
  <trd lng="pl">argon</trd>
  <trdgrp lng="pt">
    <trd>arg&ocirc;nio</trd>,
    <trd>&aacute;rgon</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_r;&c_g;&c_o;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>arg&oacute;n</trd>,
    <trd>arg&oacute;n</trd>
  </trdgrp>
  <trd lng="sv">argon</trd>
  <trd lng="tr">argon</trd>
  <trd lng="uk">&#x430;&#x440;&#x491;&#x43E;&#x43D;</trd>
</drv>
