<art mrk="$Id$">
<kap>
  <ofc>*</ofc><rad>Nikola</rad>/o
</kap>

<drv mrk="nikola.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">NOM</uzo>
  <snc>
    <dif>
      Vira anta&ubreve;nomo grekdevena:
