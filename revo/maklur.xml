<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: maklur.xml,v 1.1 2025/10/26 18:37:50 afido Exp $">
    <kap>
      <rad>maklur</rad>/o
    </kap>

<drv mrk="maklur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="maklur.0o.genro">
    <dif>
      <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">Genro</ref>
      de lignaj vegeta&jcirc;oj el la familio <ref tip="malprt" cel="morusa.0oj">morusacoj</ref>
      (<trd lng="la">Maclura</trd>), kun laktosuko kaj karnecaj, globformaj plurfruktoj:
      <ekz>
        altvaloraj lignoj kiel mahagono, cedro kaj <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1640"
              >Honduro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Maclura</trd>
    <trd lng="en">Maclura</trd>
    <trd lng="fr">Maclura</trd>
    <trd lng="ru">&c_M;&c_a;&c_k;&c_l;&c_ju;&c_r;&c_a;</trd>
  </snc>
</drv>

  </art>

