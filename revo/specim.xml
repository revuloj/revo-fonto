<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: specim.xml,v 1.20 2022/09/21 17:42:51 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>specimen</rad>/o
</kap>
<drv mrk="specim.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Unu a&ubreve; kelkaj provekzempleroj; eta kvanto da venda&jcirc;oj
      a&ubreve; koninda&jcirc;oj montrataj por veki scivolon, informi,
      reklami, ebligi takson, studon:
      <ekz>
        mi permesas al mi adresi al vi <klr>[&#8230;]</klr> kelkajn <tld/>ojn de la
        vendobjektoj paperaj kaj kartonaj, kiujn mi havas ordinare en mia magazeno
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mielkuko<klr>[j&#8230;]</klr> ku&scirc;is kiel <tld/>oj sur la tablo
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi sendos al vi <tld/>ojn de tieaj teksa&jcirc;oj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estus bonega <tld/>o de bonvivanto
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la edzi&gcirc;o, kvankam nature ne vere kortu&scirc;anta <tld/>o de &leftquot;La Granda Pasio&rightquot;, &scirc;ajne
        kontentigis amba&ubreve;
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 1</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi mem prezentis al &scirc;i hodia&ubreve; <tld/>on de via gusto kaj poezia talento
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        Petro la Granda komencis kolektadi <tld/>ojn de diversaj lingvoj sur la
        teritorio de la Rusa Imperio
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li forportis la sango<tld/>on
        <fnt><aut>David Curtis</aut>:
          <vrk>Koratako</vrk>, <bib>Monato</bib>
          <lok>jaro 2002a, numero 1a, p. 28a</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiaj kolektoj pruvas kiel la kolektantoj de naturaj <tld/>oj
        stimulis la botanikon, kiu dum la renesanco apartigis sin de la
        medicino
        <fnt><aut>Johan Van Eenoo</aut>: 
          <vrk>La bru&gcirc;a Centuriae plantarum rariorum retrovita en Krakovo</vrk>,
          <bib>Monato</bib><lok>jaro 1998a, numero 1a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzemp.0o">ekzemplo</ref>,
      <ref cel="ekzemp1.0o">ekzemplero</ref>,
      <ref cel="protot.0o.TEK">prototipo</ref>,
      <ref cel="prov.0ajxo">prova&jcirc;o</ref>,
      <ref cel="tip.0o">tipo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_z;&c_o;&c_r;</trd>,
    <trd>&c_r;&c_e;&c_k;&c_l;&c_ja;&c_m;&c_n;&c_y; &c_e;&c_k;&c_z;&c_e;&c_m;&c_p;&c_l;&c_ja;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">esp&egrave;cimen</trd>
  <trdgrp lng="de">
    <trd>Muster</trd>,
    <trd>Probe</trd>,
    <trd>Probest&uuml;ck</trd>
  </trdgrp>
  <trd lng="en">specimen</trd>
  <trdgrp lng="fr">
    <trd>&eacute;chantillon<klr> (sp&eacute;cimen)</klr></trd>,
    <trd>sp&eacute;cimen</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>minta</trd>,
    <trd>&aacute;ruminta</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>specimen</trd>,
    <trd>monster</trd>,
    <trd>staal</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pr&oacute;bka</trd>,
    <trd>okaz</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_je;&c_c;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>model</trd>,
    <trd>predloha</trd>,
    <trd>vzor</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>specimen</trd>,
    <trd>uk&aacute;zka</trd>,
    <trd>vzorek zbo&zcaron;&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Muster</trd>,
    <trd>Probe</trd>,
    <trd>Probest&uuml;ck</trd>,
    <trd>Anschauungsst&uuml;ck</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_r;&c_a;&c_z;&c_o;&c_k;</trd>,
    <trd>&c_v;&c_z;&c_ib;&c_r;&c_je;&c_c;&c_mol;</trd>,
    <trd>(&c_s;&c_t;&c_a;&c_t;&c_i;&c_s;&c_t;&c_i;&c_ch;&c_n;&c_a;) &c_v;&c_i;&c_b;&c_ib;&c_r;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="specim.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Kolektitaj <tld/>oj de diversaj rilataj a&jcirc;oj kune konservataj
      por komparo, studo, &gcirc;uo:
      <ekz>
        eksterordinara <tld/>aro pri stiloj, dialogaj metodoj, &gcirc;enraj teknikoj
        <fnt>
          <bib>Plumamikoj</bib>
          <lok>Dramoj en esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ie konataj estis liaj botanikaj &gcirc;ardenoj kaj naturaj
        <tld/>aroj
        <fnt>
          <aut>Johan Van Eenoo</aut>: <vrk>La bru&gcirc;a Centuriae
          plantarum rariorum retrovita en Krakovo</vrk>,
          <bib>Monato</bib><lok>jaro 1998a, numero 1a, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kabine.0o.ekspozicio">kabineto<sncref ref="kabine.0o.ekspozicio"/></ref>,
      <ref cel="kolekt.0o.art">kolekto<sncref ref="kolekt.0o.art"/></ref>,
      <ref cel="muze.0o.kolekto">muzeo<sncref ref="muze.0o.kolekto"/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>col&middot;lecci&oacute;</trd>,
    <trd>mostrari</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Sammlung</trd>,
    <trd>Stichprobe</trd>,
    <trd>Kollektion</trd>
  </trdgrp>
  <trd lng="en">collection</trd>
  <trdgrp lng="fr">
    <trd>collection</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mintagy&udblac;jtem&eacute;ny</trd>,
    <trd>&aacute;ruminta-kollekci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zbi&oacute;r</trd>,
    <trd>kolekcja</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_l;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>vzorkovnica</trd>,
    <trd>zbierka vzoriek</trd>
  </trdgrp>
</drv>
</art>
