<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>specimen</rad>/o
</kap>
<drv mrk="specim.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malgranda kvanto da ia komerca&jcirc;o servanta por konigi &gcirc;in al la
      eventualaj a&ccirc;etontoj.
    </dif>
    <trdgrp lng="nl">
      <trd>specimen</trd>,
      <trd>monster</trd>,
      <trd>staal</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Skizo de ia afero destinita por veki scivolon kaj atenton de tiuj,
      kiuj povas interesi&gcirc;i pri tio.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Muster</trd>,
    <trd>Probe</trd>,
    <trd>Probest&uuml;ck</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_je;&c_c;</trd>,
    <trd>&c_o;&c_b;&c_r;&c_a;&c_z;&c_ch;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
