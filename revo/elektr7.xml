<kap>
  <rad>elektrofor</rad>/o<fnt>Z</fnt>
</kap>
<drv mrk="elektr7.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref> de elektrostatika indukto,
      kiu servas por produkti malgrandajn &scirc;argojn de statika elektro:
      <ekz>
        la germana scienculo <nom>Georg Christoph Lichtenberg</nom> konstruis en 1777 unu el la
        plej grandaj <tld/>oj, &gcirc;i havis preska&ubreve; du metrojn da diametro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=557065">Elektroforo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <bld lok="&WCU;/thumb/9/92/Electrophorus_device.png/320px-Electrophorus_device.png" prm="PD"><tld/>o inventita de Johan Carl Wilcke in 1764
        <fnt>
          <aut>Robert Hare (1840): A Compendium of the Course of Chemical Instruction in the Medical Department of the University of Pennsylvania, 4th Ed., Part 1, J.G. Auner, Philadelphia, p.49</aut>
          <lok><url ref="&WCW;/File:Electrophorus_device.png">&WCW;/File:Electrophorus_device.png</url></lok>
        </fnt>
    </bld>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_f;&c_o;&c_r;</trd>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_f;&c_o;&c_r;</trd>
  <trd lng="de">Elektrophor</trd>
  <trd lng="en">electrophorus</trd>
  <trd lng="fr">&eacute;lectrophore</trd>
  <trd lng="hu">elektrofor</trd>
  <trd lng="nl">elektrofoor</trd>
  <trd lng="pt">eletr&oacute;foro</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_f;&c_o;&c_r;</trd>
  <trd lng="uk">&#x435;&#x43B;&#x435;&#x43A;&#x442;&#x440;&#x43E;&#x444;&#x43E;&#x440;</trd>
</drv>
