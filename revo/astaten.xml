<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>astaten</rad>/o
</kap>
<drv mrk="astaten.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
     &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
     atomnumero 85, atompezo 209,9871, simbolo At. &Ccirc;iuj izotopoj de tiu
     elemento estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="de">Astat</trd>
  <trd lng="fr">astate</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_s;&c_t;&c_a;&c_t;</trd>,
    <trd>&c_a;&c_s;&c_t;&c_a;&c_t;&c_i;&c_n;</trd>
  </trdgrp>
</drv>
</art>

