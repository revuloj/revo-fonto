<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: duz.xml,v 1.8 2023/11/04 18:50:52 revo Exp $">
<kap>
  <rad>duz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="duz.0o">
  <kap><tld/>o</kap>
  <snc mrk="duz.0o.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Ellasa tubo egallar&gcirc;a, funela, eventuale aliforma,
      tra kiu eli&gcirc;as premata a&ubreve; varmega gaso, a&ubreve; flua&jcirc;o.
    </dif>
    <subsnc mrk="duz.0o.MAS_reakcieja">
      <dif>
        Ellasa tubo de reakciejo, de reakcia motoro:
        <ekz>
          al &ccirc;i tiu etapo de du<tld/>aj aeroplanoj sekvis ties 
          pligrandi&gcirc;o al kvar<tld/>aj
          <fnt>
            <aut>R. Partecke</aut>,
            <vrk><url ref="&STEB;ingxenierado/ceteraj/aviado/trafikaj-aeroplanoj.htm">
            La trafikaj aeroplanoj de la estonto</url></vrk>,
            <lok>AEST '81</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="duz.0o.MAS_forna">
      <dif>
        Ellasa tubo de fornego.
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="ajut.0o.MAS">ajuto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>d&yacute;za</trd>,
    <trd>tryska karbur&aacute;toru</trd>,
    <trd>tryska motoru</trd>
  </trdgrp>
  <trd lng="de">D&uuml;se</trd>
  <trd lng="fr">tuy&egrave;re</trd>
  <trd lng="he">&#x5D0;&#x5D2;&#x5D6;&#x5D5;&#x5D6;</trd>
  <trd lng="sk">tryska motora</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x43F;&#x443;&#x441;&#x43A;&#x43D;&#x430; &#x442;&#x440;&#x443;&#x431;&#x430;</trd>,
    <trd>&#x441;&#x442;&#x440;&#x443;&#x43C;&#x438;&#x43D;&#x43D;&#x435; &#x441;&#x43E;&#x43F;&#x43B;&#x43E;</trd>,
    <trd>&#x444;&#x443;&#x440;&#x43C;&#x430; (&#x443; &#x43F;&#x435;&#x447;&#x430;&#x445;)</trd>,
    <trd>&#x434;&#x44E;&#x437;&#x430;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x55B7;&#x5934; <pr>p&#x113;nt&#xF3;u</pr></trd>,
    <trd>&#x55B7;&#x7BA1; <pr>p&#x113;nguan</pr></trd>,
    <trd>&#x6F0F;&#x677F; <pr>l&#xF2;ub&#x1CE;n</pr></trd>,
    <trd>&#x5438;&#x6C14;&#x5634; <pr>x&#x12B;q&#xEC;zu&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>

