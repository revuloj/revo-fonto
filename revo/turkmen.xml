<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: turkmen.xml,v 1.21 2024/04/20 18:27:56 revo Exp $">
<kap>
  <rad>turkmen</rad>/o
</kap>

<drv mrk="turkmen.0o">
  <kap><tld/>o</kap>
  <snc mrk="turkmen.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="turkmen.T0ujo"><tld lit="T"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="turkmen.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_k;&c_m;&c_je;&c_n;</trd>
  <trd lng="de">Turkmene</trd>
  <trd lng="fr">Turkm&egrave;ne<klr> (subst.)</klr></trd>
  <trd lng="ca">turcman<klr> (subst.)</klr></trd>
  <trd lng="id">Turkmen</trd>
  <trd lng="ja">&#x30C8;&#x30EB;&#x30AF;&#x30E1;&#x30F3;&#x4EBA; 
  <pr>&#x30C8;&#x30EB;&#x30AF;&#x30E1;&#x30F3;&#12376;&#12435;</pr></trd>
  <trd lng="nl">Turkmeen</trd>
  <trd lng="pl">Turkmen</trd>
  <trd lng="pt">turcomeno <klr tip="ind"> (subst.)</klr></trd>
  <trd lng="uk">&c_t;&c_u;&c_r;&#x43A;&#x43C;&c_je;&#x43D;&c_i;&#x43D;</trd>
</drv>

<drv mrk="turkmen.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        <tld/>a lingvo estas lingvo el oguza grupo de tjurka lingvaro, &gcirc;i
        estas &scirc;tata lingvo de Turkmenio kaj anka&ubreve; havas regionan statuson en Afganio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=24025">Turkmena lingvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_k;&c_m;&c_je;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">turcman<klr> (adj.)</klr></trd>
  <trd lng="cs">turkmensk&yacute;</trd>
  <trd lng="de">turkmenisch</trd>
  <trd lng="fr">turkm&egrave;ne<klr> (adj.)</klr></trd>
  <trd lng="nl">Turkmeens</trd>
  <trd lng="pl">turkme&nacute;ski</trd>
  <trd lng="pt">turcomeno <klr tip="ind"> (adj.)</klr></trd>
  <trd lng="sk">turkm&eacute;nsky</trd>
  <trd lng="uk">&c_t;&c_u;&c_r;&#x43A;&#x43C;&c_je;&#x43D;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
</drv>

<drv mrk="turkmen.T0ujo">
  <kap>
    <tld lit="T"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <mlg>TM</mlg>
  <snc mrk="turkmen.T0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la centra <ref tip="malprt" cel="azi.A0o">Azio</ref>
      kun <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
A&scirc;gabato; &gcirc;is 1991 <ref tip="vid" cel="sovet.S0unio">Sovetunia</ref> respubliko.
      &Gcirc;i limas kun <ref tip="vid" cel="afgan.A0ujo.GEOG">Afganujo</ref>,
      <ref tip="vid" cel="iran.I0o.GEOG">Irano</ref>,
      <ref tip="vid" cel="kazahx.K0ujo.GEOG">Kaza&hcirc;ujo</ref>,
      <ref tip="vid" cel="uzbek.U0ujo">Uzbekujo</ref>
      kaj kun <ref tip="vid" cel="kaspi.K0o">Kaspio</ref>:
      <ekz>
        multaj landoj, kiel Argentino, Centra &Ccirc;inio, Kartvelio, Kaza&hcirc;ujo, Mongolio,
        Senegalo, <tld lit="T"/>ujo kaj Turkio, povas esti konsiderataj uzi &leftquot;fakte&rightquot;
        tempon da&ubreve;re anta&ubreve;enigitan je unu horo, &ccirc;ar iliaj <klr>[la&ubreve;le&gcirc;aj]</klr> tempoj
        estas la sunaj tempoj de la meridianoj lokataj 15 gradojn pli oriente ol iliaj
        propraj meridianoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=89772">Somera tempo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="turkmeni.T0o.GEOG">Turkmenio</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_T;&c_u;&c_r;&c_k;&c_m;&c_je;&c_n;&c_ib;&c_ja;</trd>,
    <trd>&c_T;&c_u;&c_r;&c_k;&c_m;&c_je;&c_n;&c_ib;&c_s;&c_t;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="bg">&c_T;&c_u;&c_r;&c_k;&c_m;&c_je;&c_n;&c_i;&c_s;&c_t;&c_a;&c_n;</trd>
  <trdgrp lng="ca">
    <trd>el <ind>Turkmenistan</ind></trd>,
    <trd>Rep&uacute;blica del <ind>Turkmenistan</ind></trd>
  </trdgrp>
  <trd lng="de">Turkmenistan</trd>
  <trd lng="en">Turkmenistan</trd>
  <trd lng="fa">&a_t;&a_r;&f_k;&a_m;&a_n;&a_s;&a_t;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>Turkm&eacute;nistan</trd>,
    <trd>R&eacute;publique du Turkm&eacute;nistan</trd>
  </trdgrp>
  <trd lng="hu">T&uuml;rkmeniszt&aacute;n</trd>
  <trd lng="id">Turkmenistan</trd>
  <trd lng="it">Turkmenistan</trd>
  <trd lng="nl">Turkmenistan</trd>
  <trd lng="pl">Turkmenistan</trd>
  <trd lng="pt">Turquemenist&atilde;o</trd>
</drv>
</art>

