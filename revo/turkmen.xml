<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>turkmen</rad>/o
</kap>

<drv mrk="turkmen.0ujo">
  <kap>
    <tld lit="T"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="turkmen.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>TM</mlg>
    <dif>
      &Scirc;tato en la centra Azio kun &ccirc;efurbo A&scirc;gabato.
      &Gcirc;is 1991
      <ref tip="vid" cel="sovet.S0unio">Sovetunia</ref> respubliko.
      &Gcirc;i limas kun <ref tip="vid"
      cel="afgan.0ujo.GEOG">Afganujo</ref>,
      <ref tip="vid" cel="iran.0o.GEOG">Irano</ref>,
      <ref tip="vid" cel="kazahx.0ujo.GEOG">Kaza&hcirc;ujo</ref>,
      <ref tip="vid" cel="uzbek.0ujo">Uzbekujo</ref>
      kaj kun <ref tip="vid" cel="kaspi.0o">Kaspio</ref>.
    </dif>
    <ref tip="sin" cel="turkmeni.0o.GEOG">Turkmenio</ref>
    <trd lng="it">Turkmenistan</trd>
    <trd lng="pl">Turkmenistan</trd>
  </snc>
</drv>

</art>

