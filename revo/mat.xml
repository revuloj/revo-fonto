<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mat.xml,v 1.31 2024/01/25 20:10:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="mat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mat.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Maldelikata kovra&jcirc;o a&ubreve; <ref tip="super" cel="tapisx.0o.TEKS">tapi&scirc;o</ref> el plektita pajlo,
      junko,
      basto, stipo a&ubreve; simila:
      <ekz>
        anta&ubreve; la pordo ku&scirc;is bonega kaj tre fortika <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur <tld/>oj sidis kelkdeko da nudaj lernantoj kun vaksitaj
        tabuletoj en la manoj
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz>
      <ekz>
        la akvo fluis de la basta <tld/>o, kiun &scirc;i por &scirc;irmo alligis &ccirc;irka&ubreve; sia korpo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        tapi&scirc;on ne deziris, <tld/>on akiris
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_t;&c_a;</trd>
    <trd lng="de">Matte</trd>
    <trd lng="en">mat</trd>
    <trdgrp lng="fr">
      <trd>natte</trd>,
      <trd>paillasson</trd>
    </trdgrp>
    <trd lng="es">estera</trd>
    <trdgrp lng="hu">
      <trd>gy&eacute;k&eacute;ny(sz&odblac;nyeg)</trd>,
      <trd>l&aacute;bsz&odblac;nyeg</trd>
    </trdgrp>
    <trd lng="nl">mat</trd>
    <trdgrp lng="pl">
      <trd>mata</trd>,
      <trd>plecionka</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>esteira</trd>,
      <trd>capacho</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_c;&c_i;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_je;&c_t;&c_jo;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_t; <klr tip="amb">(&c_k;&c_o;&c_v;&c_r;&c_i;&c_k;) </klr></trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x84D0; <pr>r&#xF9;</pr></trd>,
    <trd>&#x5E2D;&#x5B50; <pr>x&#xED;zi</pr></trd>,
    <trd>&#x88C0; <pr>y&#x12B;n</pr></trd>,
    <trd>&#x5E2D; <pr>x&#xED;</pr></trd>,
    <trd>&#x84C6; <pr>x&#xED;</pr></trd>,
    <trd>&#x82C7;&#x5E2D; <pr>w&#x11B;ix&#xED;</pr></trd>,
    <trd>&#x8466;&#x5E2D; <pr>w&#x11B;ix&#xED;</pr></trd>,
    <trd>&#x82A6;&#x5E2D; <pr>l&#xFA;x&#xED;</pr></trd>,
    <trd>&#x8606;&#x5E2D; <pr>l&#xFA;x&#xED;</pr></trd>,
    <trd>&#x8335; <pr>y&#x12B;n</pr></trd>
  </trdgrp>
</snc>
  <ref tip="hom" cel="mat1.0o">mato (&scirc;ako)</ref>
  <trd lng="bo">&#xF42;&#xF51;&#xF53;&#xF0B;</trd>
  <trd lng="cs">roho&zcaron;ka</trd>
  <trdgrp lng="ja">
    <trd>&#x8A70;&#x307F;  <pr>&#x3064;&#x307F;</pr></trd>,
    <trd>&#x30C1;&#x30A7;&#x30C3;&#x30AF;&#x30E1;&#x30A4;&#x30C8;</trd>
  </trdgrp>
  <trd lng="sk">roho&zcaron;ka</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&c_t;</trd>,
    <trd>&c_c;&c_i;&#x43D;&#x43E;&c_v;&#x43A;&c_a;</trd>,
    <trd>&#x43F;&#x43B;&c_je;&c_t;&c_ib;&#x43D;&#x43A;&c_a;</trd>,
    <trd>&c_s;&#x43E;&#x43B;&#x43E;&#x43C;&#x2019;&#x44F;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="mat.pordo0o">
  <kap>pordo<tld/>o</kap>
  <snc mrk="mat.pordo0o.HEJM">
    <dif>
      <tld lit="M"/>o ku&scirc;anta anta&ubreve; lo&gcirc;eja pordo,
      sur kiu, enirante, oni frotpurigas a&ubreve; lasas la &scirc;uojn:
      <ekz>
        lasu la &scirc;losilon sub la pordo<tld/>o
        <fnt>
          <aut>M. Halvelik</aut>,
          <vrk><url ref="https://cindymckee.com/librejo/GAVARO.pdf">
          Gavaro</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Abtreter</trd>,
      <trd>Fu&szlig;matte</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x59D4;&#x6D3E;&#x8005; <pr>w&#x113;ip&#xE0;izh&#x11B;</pr></trd>,
    <trd>&#x8F49;&#x8B93;&#x8005; <pr>zhu&#x1CE;nr&#xE0;ngzh&#x11B;</pr></trd>,
    <trd>&#x8F6C;&#x8BA9;&#x8005; <pr>zhu&#x1CE;nr&#xE0;ngzh&#x11B;</pr></trd>,
    <trd>&#x9580;&#x53E3;&#x64E6;&#x978B;&#x588A; <pr>m&#xE9;nk&#x1D2;uc&#x101;xi&#xE9;di&#xE0;n</pr></trd>,
    <trd>&#x95E8;&#x53E3;&#x64E6;&#x978B;&#x57AB; <pr>m&#xE9;nk&#x1D2;uc&#x101;xi&#xE9;di&#xE0;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_ib;&c_m;&c_o;&c_k; <klr>(&c_d;&c_l;&c_ja; &c_v;&c_y;&c_c;&c_ib;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ja; &c_n;&c_o;&c_g;)</klr></trd>
  <trd lng="fr">paillasson</trd>
  <trd lng="pl">wycieraczka</trd>
</drv>
</art>
