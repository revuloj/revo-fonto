<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mat.xml,v 1.33 2026/02/07 20:00:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mat</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="mat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mat.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Maldelikata kovra&jcirc;o a&ubreve; <ref tip="super" cel="tapisx.0o.TEKS">tapi&scirc;o</ref> el plektita pajlo,
      junko,
      basto, stipo a&ubreve; simila:
      <ekz>
        anta&ubreve; la pordo ku&scirc;is bonega kaj tre fortika <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur <tld/>oj sidis kelkdeko da nudaj lernantoj kun vaksitaj
        tabuletoj en la manoj
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 1a</lok></fnt>;
      </ekz>
      <ekz>
        la akvo fluis de la basta <tld/>o, kiun &scirc;i por &scirc;irmo alligis &ccirc;irka&ubreve; sia korpo
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        tapi&scirc;on ne deziris, <tld/>on akiris
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_a;&c_t;&c_a;</trd>
    <trd lng="de">Matte</trd>
    <trd lng="en">mat</trd>
    <trdgrp lng="fr">
      <trd>natte</trd>,
      <trd>paillasson</trd>
    </trdgrp>
    <trd lng="es">estera</trd>
    <trdgrp lng="hu">
      <trd>gy&eacute;k&eacute;ny(sz&odblac;nyeg)</trd>,
      <trd>l&aacute;bsz&odblac;nyeg</trd>
    </trdgrp>
    <trd lng="nl">mat</trd>
    <trdgrp lng="pl">
      <trd>mata</trd>,
      <trd>plecionka</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>esteira</trd>,
      <trd>capacho</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_c;&c_i;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_l;&c_je;&c_t;&c_jo;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_m;&c_a;&c_t; <klr tip="amb">(&c_k;&c_o;&c_v;&c_r;&c_i;&c_k;) </klr></trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#34000; <pr>r&ugrave;</pr></trd>,
    <trd>&#24109;&#23376; <pr>x&iacute;zi</pr></trd>,
    <trd>&#35008; <pr>y&#299;n</pr></trd>,
    <trd>&#24109; <pr>x&iacute;</pr></trd>,
    <trd>&#33990; <pr>x&iacute;</pr></trd>,
    <trd>&#33479;&#24109; <pr>w&ecaron;ix&iacute;</pr></trd>,
    <trd>&#33894;&#24109; <pr>w&ecaron;ix&iacute;</pr></trd>,
    <trd>&#33446;&#24109; <pr>l&uacute;x&iacute;</pr></trd>,
    <trd>&#34310;&#24109; <pr>l&uacute;x&iacute;</pr></trd>,
    <trd>&#33589; <pr>y&#299;n</pr></trd>
  </trdgrp>
</snc>
  <ref tip="hom" cel="mat1.0o">mato (&scirc;ako)</ref>
  <trd lng="bo">&#3906;&#3921;&#3923;&#3851;</trd>
  <trd lng="ca">estora</trd>
  <trd lng="cs">roho&zcaron;ka</trd>
  <trdgrp lng="ja">
    <trd>&#35440;&#12415;  <pr>&#12388;&#12415;</pr></trd>,
    <trd>&#12481;&#12455;&#12483;&#12463;&#12513;&#12452;&#12488;</trd>
  </trdgrp>
  <trd lng="sk">roho&zcaron;ka</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_t;</trd>,
    <trd>&c_c;&c_i;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_je;&c_t;&c_ib;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_o;&c_l;&c_o;&c_m;&#8217;&c_ja;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="mat.pordo0o">
  <kap>pordo<tld/>o</kap>
  <snc mrk="mat.pordo0o.HEJM">
    <dif>
      <tld lit="M"/>o ku&scirc;anta anta&ubreve; lo&gcirc;eja pordo,
      sur kiu, enirante, oni frotpurigas a&ubreve; lasas la &scirc;uojn:
      <ekz>
        lasu la &scirc;losilon sub la pordo<tld/>o
        <fnt>
          <aut>M. Halvelik</aut>,
          <vrk><url ref="https://cindymckee.com/librejo/GAVARO.pdf">
          Gavaro</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Abtreter</trd>,
      <trd>Fu&szlig;matte</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#22996;&#27966;&#32773; <pr>w&#275;ip&agrave;izh&ecaron;</pr></trd>,
    <trd>&#36681;&#35731;&#32773; <pr>zhu&#462;nr&agrave;ngzh&ecaron;</pr></trd>,
    <trd>&#36716;&#35753;&#32773; <pr>zhu&#462;nr&agrave;ngzh&ecaron;</pr></trd>,
    <trd>&#38272;&#21475;&#25830;&#38795;&#22666; <pr>m&eacute;nk&#466;uc&#257;xi&eacute;di&agrave;n</pr></trd>,
    <trd>&#38376;&#21475;&#25830;&#38795;&#22443; <pr>m&eacute;nk&#466;uc&#257;xi&eacute;di&agrave;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_ib;&c_m;&c_o;&c_k; <klr>(&c_d;&c_l;&c_ja;
  &c_v;&c_y;&c_c;&c_ib;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ja; &c_n;&c_o;&c_g;)</klr></trd>
  <trdgrp lng="ca">
    <trd>estora de l'entrada</trd>,
    <trd>estoreta de l'entrada</trd>
  </trdgrp>
  <trd lng="fr">paillasson</trd>
  <trd lng="pl">wycieraczka</trd>
</drv>
</art>
