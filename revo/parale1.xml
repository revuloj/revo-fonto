<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>paralelepiped</rad>/o</kap>
<drv mrk="parale1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="edr.n0o">Sesedro</ref>, kiun limas ses
      <ref cel="parale2.0o">paralelogramoj</ref>:
      <ekz>
        ortangula, neortangula <tld/>o.
      </ekz>
    </dif>
    <rim>
      Ortangulan <tld/>on oni foje nomas <ctl>kvadro</ctl> a&ubreve;
      <ctl>ortepipedo</ctl><fnt><bib>MatVort</bib></fnt>.
    </rim>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_l;&c_je;&c_l;&c_je;&c_p;&c_ib;&c_p;&c_e;&c_d;</trd>
  <trd lng="de">Parallelepiped</trd>
  <trd lng="en">parallelepiped[on]</trd>
  <trd lng="fr">parall&eacute;l&eacute;pip&egrave;de</trd>
  <trd lng="hu">parallelepipedon</trd>
  <trd lng="nl">parallellepipedum</trd>
  <trd lng="pl">r&oacute;wnoleg&lstroke;o&sacute;cian</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_l;&c_l;&c_je;&c_l;&c_je;&c_p;&c_i;&c_p;&c_je;&c_d;</trd>
</drv>
</art>
