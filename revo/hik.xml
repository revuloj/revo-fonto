<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>hik</rad>/i <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="hik.0i">
   <kap><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Ne plore <ref tip="super" cel="singul1.0i">singulti</ref>:
       <ekz>
         sattrinkinte, la literaturistoj tuj komencis <tld/>i
         <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 1a, p. 6a</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
   <trd lng="es">tener <ind>hipo</ind></trd>
   <trd lng="fa">&a_s;&f_k;&a_s;&f_k;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
   <trdgrp lng="fr">
     <trd>avoir le <ind>hoquet</ind></trd>,
     <trd>hoqueter</trd>
   </trdgrp>
   <trd lng="hu">csuklik</trd>
   <trd lng="it">singhiozzare<klr> (avere il singhiozzo)</klr></trd>
   <trdgrp lng="de">
     <trd>hicksen</trd>,
     <trd>Schluckauf haben</trd>
   </trdgrp>
   <trd lng="en">hiccup</trd>
   <trd lng="pt">solu&ccedil;ar</trd>
</drv>
</art>

