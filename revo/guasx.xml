<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gua&scirc;</rad>/o
</kap>
<drv mrk="guasx.0o">
  <kap><tld/>o</kap>
  <snc mrk="guasx.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Pentra&jcirc;o, en kiu oni uzas farbojn miksitajn kun gumo kaj mielo.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_a;&c_sh;</trd>
  <trdgrp lng="de">
    <trd>Gouache</trd>,
    <trd>Guasche</trd>,
    <trd>Guaschmalerei</trd>
  </trdgrp>
  <trd lng="fr">gouache</trd>
  <trd lng="hu">gouache</trd>
  <trd lng="nl">gouache<klr tip="ind"> (schilderij)</klr></trd>
  <trd lng="ru">&c_g;&c_u;&c_a;&c_sh;&c_mol;</trd>
</drv>
</art>
