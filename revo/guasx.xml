<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gua&scirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="guasx.0o">
  <kap><tld/>o</kap>
  <snc mrk="guasx.0o.KOL">
    <dif>
      Pentra <ref tip="super" cel="farb.0o.KOL">farbo</ref>
      diluebla en akvo, simila al
      <ref tip="vid" cel="akvare.0o.KOL">akvarelo</ref>
      sed densa, ne travidebla pro aldono de ceruzo
      a&ubreve; alia blankigilo.
    </dif>
  </snc>
  <snc mrk="guasx.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Pentra&jcirc;o, en kiu oni uzas <tld/>ajn farbojn.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_a;&c_sh;</trd>
  <trd lng="bg">&c_g;&c_v;&c_a;&c_sh;</trd>
  <trdgrp lng="ca">
    <trd>aiguada</trd>,
    <trd>guaix</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gouache</trd>,
    <trd>Guasche</trd>,
    <trd>Guaschmalerei</trd>
  </trdgrp>
  <trd lng="fa">&f_g;&a_w;&a_A;&a_sh;</trd>
  <trd lng="fr">gouache</trd>
  <trd lng="hu">gouache</trd>
  <trd lng="it">guazzo</trd>
  <trd lng="nl">gouache<klr tip="ind"> (schilderij)</klr></trd>
  <trdgrp lng="pt">
   <trd>guache</trd>, 
   <trd>guacho</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_u;&c_a;&c_sh;&c_mol;</trd>
</drv>
</art>
