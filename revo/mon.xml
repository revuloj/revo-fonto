<drv mrk="mon.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc mrk="mon.sen0igi.KOMUNE">
    <dif>
      Senigi je <tld/>o:
      <ekz>
        tute sen<tld/>igis min infanteria kapitano, tiel, ke la
        hotelmastro volis jam meti min en malliberejon
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        maksimumfilatelo &#8208; nova maniero sen<tld/>igi la filatelistojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007065.html">Filatelo:
          VIPE 2000, a&ubreve; &ccirc;u la po&scirc;tmarkoj malaperos?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sucx.el0i">elsu&ccirc;i</ref>,
      <ref cel="hav.sen0igi">senhavigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>um sein Geld <ind>bringen</ind></trd>,
    <trd>das Geld aus der Tasche <ind>ziehen</ind></trd>,
    <trd>abzocken</trd>,
    <trd>berauben</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;pouiller</trd>,
    <trd>d&eacute;trousser</trd>
  </trdgrp>
