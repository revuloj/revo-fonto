</kap>

<drv mrk="mon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <tezrad fak="EKON"/>
  <snc mrk="mon.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Pagilo la&ubreve;le&gcirc;e fabrikita kaj konsistanta el stampita
      metaldisketo a&ubreve; surpresita paperfolio kun difinita valoro
      por servi kiel a&ccirc;etrimedo:
      <ekz>
        <tld/>o plenumas &ccirc;iujn oficojn
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        por la <tld/>o pastra pre&gcirc;o, por la <tld/>o romp' de
        le&gcirc;o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kest.0o">kesto</ref>,
      <ref cel="or.0o">oro</ref>,
      <ref cel="sak.0o">sako</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_o;&c_sh;&c_y;</trd>
  <trd lng="bg">&c_p;&c_a;&c_r;&c_i;</trd>
  <trdgrp lng="br">
    <trd>arc'hant</trd>,
    <trd>moneiz</trd>
  </trdgrp>
  <trd lng="ca">diners</trd>
  <trd lng="cs">pen&iacute;ze</trd>
  <trd lng="de">Geld</trd>
  <trd lng="en">money</trd>
  <trd lng="es">dinero</trd>
  <trd lng="fa">&f_p;&a_w;&a_l;</trd>
  <trdgrp lng="fr">
    <trd>argent</trd>,
    <trd>monnaie</trd>
  </trdgrp>
  <trd lng="hu">p&eacute;nz</trd>
  <trd lng="id">uang</trd>
  <trdgrp lng="ku">
    <trd>pere</trd>,
    <trd>draf</trd>
  </trdgrp>
  <trd lng="nl">geld</trd>
  <trd lng="oc">argent</trd>
  <trd lng="pl">pieni&aogonek;dze</trd>
  <trd lng="pt">dinheiro</trd>
  <trd lng="ro">ban</trd>
  <trd lng="ru">&c_d;&c_je;&c_n;&c_mol;&c_g;&c_i;</trd>
  <trd lng="sk">peniaze</trd>
  <trd lng="sv">pengar</trd>
  <trd lng="tp">mani</trd>
  <trd lng="tr">para</trd>
  <trd lng="vo">mon</trd>
  <trdgrp lng="zh">
    <trd>钱 [<ind>qián</ind>]</trd>
    <trd>金钱 [<ind>jīn qián</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="mon.0a">
  <kap><tld/>a</kap>
  <snc mrk="mon.0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a unuo;
      </ekz><ekz>
        <tld/>aj aferoj, interesoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="financ.0a">financa</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="br">
    <trd>-<ind>arc'hant</ind></trd>,
    <trd>moneizel</trd>
  </trdgrp>
  <trd lng="es">monetario</trd>
  <trd lng="fa">&f_p;&a_w;&a_l;&f_y;</trd>
  <trd lng="fr">mon&eacute;taire</trd>
  <trdgrp lng="hu">
    <trd>p&eacute;nz-</trd>,
    <trd>p&eacute;nz&uuml;gyi</trd>,
    <trd>monet&aacute;ris</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>geld-</trd>,
    <trd>monetair</trd>
  </trdgrp>
  <trd lng="pl">pieni&eogonek;&zdot;ny</trd>
  <trd lng="ru">&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">peňažný</trd>
</drv>

<drv mrk="mon.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="mon.0ero.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Aparta peco de metala <tld/>o:
      <ekz>
        ora <tld/>ero;
      </ekz><ekz>
        unu <tld/>ero ne estos <tld/>o<fnt>Z</fnt>;
      </ekz><ekz>
        oni lin konas kiel malbonan <tld/>eron<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>erigi &ccirc;ekon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_e;&c_t;&c_a;</trd>
  <trd lng="bg">&c_m;&c_o;&c_n;&c_je;&c_t;&c_a;</trd>
  <trd lng="br">pezh moneiz</trd>
  <trdgrp lng="de">
    <trd>M&uuml;nze</trd>,
    <trd>Geldst&uuml;ck</trd>
  </trdgrp>
  <trd lng="es">moneda</trd>
  <trd lng="fa">&a_s;&f_k;&a_h;</trd>
  <trd lng="fr">pi&egrave;ce de monnaie</trd>
  <trd lng="hu">p&eacute;nz&eacute;rme</trd>
  <trd lng="id"><ind>uang</ind> receh</trd>
  <trd lng="nl">muntstuk</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_je;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>minca</trd>,
    <trd>peniaz</trd>
  </trdgrp>
  <trd lng="sv">mynt</trd>
</drv>

<drv mrk="mon.0ereto">
  <kap><tld/>ereto</kap>
  <snc mrk="mon.0ereto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Malgrandvalora <tld/>ero.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_o;&c_b;&c_n;&c_a;&c_ja; 
&c_m;&c_a;&c_n;&c_e;&c_t;&c_a;</trd>
  <trd lng="br">moneiz bihan</trd>
  <trdgrp lng="de">
    <trd>kleine M&uuml;nze</trd>,
    <trd>kleines Geldst&uuml;ck</trd>
  </trdgrp>
  <trd lng="fa">&f_p;&a_w;&a_l; &a_kh;&a_r;&a_d;</trd>
  <trdgrp lng="fr">
    <trd>petite monnaie</trd>,
    <trd>pi&egrave;cette</trd>
  </trdgrp>
  <trd lng="hu">apr&oacute;p&eacute;nz</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_k;&c_a;&c_ja; 
&c_m;&c_o;&c_n;&c_je;&c_t;&c_a;</trd>
</drv>

<drv mrk="mon.0ujo">
  <kap><ofc>*</ofc><tld/>ujo</kap>
  <snc mrk="mon.0ujo.KOMUNE">
    <dif>
      Po&scirc;o a&ubreve; saketo destinita por kunporti <tld/>erojn:
      <ekz>
        en la po&scirc;o de mia pantalono mi portas <tld/>ujon
        <fnt><bib>F</bib>, <lok>&FE;40</lok></fnt>;
      </ekz><ekz>
        la vidvino metis la manon en la po&scirc;on kaj eltiris
        <tld/>ujon, kiu enhavis kelke da <tld/>paperetoj
        <fnt><bib>Marta</bib></fnt>;
      </ekz><ekz>
        Tolstoj haltis kaj donis el sia <tld/>ujo du ar&gcirc;entajn
        <tld/>erojn
        <fnt>
          <aut>V. Langlet</aut>,
          <vrk>Voja&gcirc;impresoj</vrk>,
          <lok>1895</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_ib;&c_t;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_a;&c_l;&c_jo;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_r;&c_t;&c_m;&c_o;&c_n;&c_je;</trd>
  <trd lng="br">doug-moneiz</trd>
  <trdgrp lng="de">
    <trd>Geldbeutel</trd>,
    <trd>Geldtasche</trd>,
    <trd>Brieftasche</trd>,
    <trd>Portemonnaie</trd>
  </trdgrp>
  <trd lng="es">monedero</trd>
  <trd lng="fa">&f_k;&f_y;&a_f; &f_p;&a_w;&a_l;</trd>
  <trd lng="fr">porte-monnaie</trd>
  <trdgrp lng="hu">
    <trd>ersz&eacute;ny</trd>,
    <trd>p&eacute;nzt&aacute;rca</trd>
  </trdgrp>
  <trd lng="id">dompet</trd>
  <trd lng="nl">portemonnee</trd>
  <trdgrp lng="pl">
    <trd>portfel</trd>,
    <trd>portmonetka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_sh;&c_je;&c_l;&c_jo;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_t;&c_m;&c_o;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">peňaženka</trd>
  <trd lng="tr">c&uuml;zdan</trd>
</drv>

<drv mrk="mon.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="mon.sen0a.KOMUNE">
    <dif>
      Ne havanta <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_z;&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_y;</trd>
  <trd lng="br">diarc'hant</trd>
  <trdgrp lng="de">
    <trd>abgebrannt</trd>,
    <trd>pleite</trd>
  </trdgrp>
  <trd lng="fa">&a_b;&f_y;&zwnj;&f_p;&a_w;&a_l;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;sargent&eacute;</trd>,
    <trd>sans-le-sou</trd>
  </trdgrp>
  <trd lng="hu">p&eacute;nztelen</trd>
  <trd 
lng="ru">&c_b;&c_je;&c_z;&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">bez peňazí</trd>
</drv>

<drv mrk="mon.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc mrk="mon.sen0igi.KOMUNE">
    <dif>
      Senigi je <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="sucx.el0i">elsu&ccirc;i</ref>,
      <ref cel="hav.sen0igi">senhavigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>um sein Geld <ind>bringen</ind></trd>,
    <trd>das Geld aus der Tasche <ind>ziehen</ind></trd>,
    <trd>abzocken</trd>,
    <trd>berauben</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;pouiller</trd>,
    <trd>d&eacute;trousser</trd>
  </trdgrp>
  <trd lng="hu">p&eacute;nz&eacute;t&odblac;l megfoszt</trd>
  <trd lng="nl">van zijn/haar geld <ind>beroven</ind></trd>
</drv>

<drv mrk="mon.elacxeta0o">
  <kap>ela&ccirc;eta <tld/>o</kap>
  <snc mrk="mon.ela&ccirc;eta0o.JUR">
    <dif>
      <tld lit="M"/>sumo donata kiel
      kompenso por liberigo de osta&gcirc;o.
    </dif>
  </snc>
  <trd lng="bg">&c_o;&c_t;&c_k;&c_u;&c_p;</trd>
  <trd lng="de">L&ouml;segeld</trd>
  <trd lng="en">ransom</trd>
  <trd lng="es">rescate</trd>
  <trd lng="fa">&a_f;&a_d;&f_y;&a_h;</trd>
  <trd lng="fr">ran&ccedil;on</trd>
  <trd lng="ru">&c_v;&c_y;&c_k;&c_u;&c_p;</trd>
</drv>

<drv mrk="mon.internacia0afonduso">
  <kap>Internacia <tld lit="M"/>a Fonduso</kap>
  <mlg>IMF</mlg>
  <fnt><bib>ManPol</bib></fnt>
  <snc mrk="mon.internacia0afonduso.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Speciala
      <ref tip="lst" cel="organi2.0ajxo"
      lst="voko:organiza&jcirc;oj">organiza&jcirc;o</ref> de
      <ref tip="malprt" cel="naci.Unuigxintaj_0oj">Unui&gcirc;intaj
      Nacioj</ref>, kiu kontrolas la internacian valutosistemon
      kaj provas ekvilibrigi la kreskadon de la monda komerco:
      <ekz>
        en Prago okazis la 55a
        jarkunsido de Internacia <tld lit="M"/>a Fonduso kaj de
        Monda Banko (MB), kun kunvenoj de ministroj pri financo,
        fakaj seminarioj, prelegoj, konferencoj, ktp.
        <fnt>
          <aut>J. Patera</aut>,
          <vrk><url ref="&Monato;/007258.html">
          Post la kunsido de financistoj en Prago</url></vrk>,
          <bib>Monato</bib>,
          <lok>2001:01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_M;&c_ib;&c_zh;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y; 
&c_v;&c_a;&c_l;&c_ju;&c_t;&c_n;&c_y; &c_f;&c_o;&c_n;&c_d;</trd>,
    <trd>&c_M;&c_V;&c_F;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>Font Moneizel Etrevroadel</trd>,
    <trd>FME</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Internationale <ind>W&auml;hrungsfonds</ind></trd>,
    <trd>IWF</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>International <ind>Monetary</ind> Fund</trd>,
    <trd>IMF</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>Fondo <ind>Monetario</ind> Internacional</trd>,
    <trd>FMI</trd>
  </trdgrp>
  <trd lng="fa">&a_S;&a_n;&a_d;&a_w;&a_q; 
&a_b;&f_y;&a_n;&zwnj;&a_A;&a_l;&a_m;&a_l;&a_l;&f_y; &f_p;&a_w;&a_l;</trd>
  <trdgrp lng="fr">
    <trd>Fonds <ind>Mon&eacute;taire</ind> International</trd>,
    <trd>FMI</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Nemzetk&ouml;zi <ind>Valutaalap</ind></trd>,
    <trd>IMF</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>Dana Moneter Internasional</trd>,
    <trd>IMF</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Internationaal <ind>Monetair</ind> Fonds</trd>,
    <trd>IMF</trd>
  </trdgrp>
  <trd lng="pl">Mi&eogonek;dzynarodowy Fundusz Walutowy</trd>
  <trdgrp lng="pt">
    <trd>Fundo <ind>Monet&aacute;rio</ind> Internacional</trd>,
    <trd>FMI</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_M;&c_je;&c_zh;&c_d;&c_u;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j; 
<ind>&c_V;&c_a;&c_l;&c_ju;&c_t;&c_n;&c_y;&c_j; 
&c_F;&c_o;&c_n;&c_d;</ind></trd>,
    <trd>&c_M;&c_V;&c_F;</trd>
  </trdgrp>
</drv>

<drv mrk="mon.posx0o">
  <kap>po&scirc;<tld/>o</kap>
  <snc mrk="mon.posx0o.KOMUNE">
    <dif>
      <tld lit="M"/>o donata al iu,
      ekzemple al infano, por &ccirc;iutagaj
      malgrandaj elspezoj:
      <ekz>
        mi e&ccirc; ricevis &ccirc;iusemajne po&scirc;<tld/>on
        <fnt>
          <aut>Emil</aut>,
          <vrk>Decidema mi &ccirc;iam estas</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">argent de poche</trd>
</drv>

<drv mrk="mon.0avida">
  <kap><tld/>avida</kap>
  <snc mrk="mon.0avida.KOMUNE">
  <dif>
    Tre deziranta <tld/>on en &ccirc;iuj
    cirkonstancoj.
  </dif>
  <refgrp tip="vid">
    <ref cel="avar.0a">avara</ref>,
    <ref cel="mamon.0o">mamono</ref>.
  </refgrp>
  </snc>
  <trd lng="de">geldgierisch</trd>
  <trdgrp lng="fa">
    <trd>&f_p;&a_w;&a_l;&zwnj;&f_p;&a_r;&a_s;&a_t;</trd>,
    <trd>&f_p;&a_w;&a_l;&zwnj;&a_d;&a_w;&a_s;&a_t;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cupide</trd>,
    <trd>int&eacute;ress&eacute;<klr> (par l'argent)</klr></trd>
  </trdgrp>
  <trd lng="hu">p&eacute;nz&eacute;hs&eacute;g</trd>
  <trd lng="nl">gierig</trd>
</drv>

<drv mrk="mon.0falsisto">
  <kap><tld/>falsisto</kap>
  <snc mrk="mon.0falsisto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Tiu kiu fabrikas kontra&ubreve;le&gcirc;e senvaloran <tld/>on, kiu
      trompe imitas la &scirc;tatan.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_a;&c_m;&c_a;&c_n;&c_e;&c_t;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_r;&c_o;&c_b;&c_sh;&c_ch;&c_y;&c_k; 
&c_g;&c_r;&c_o;&c_sh;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="br">falsvoneizer</trd>
  <trdgrp lng="de">
    <trd>Geldf&auml;lscher</trd>,
    <trd>F&auml;lscher</trd>
  </trdgrp>
  <trd lng="es">falsificador</trd>
  <trd lng="fa">&a_j;&a_A;&a_ayn;&a_l;<klr tip="amb"> (&a_s;&f_k;&a_h;&a_komo;
      &a_A;&a_s;&f_k;&a_n;&a_A;&a_s;)</klr></trd>
  <trd lng="fr">faux-monnayeur</trd>
  <trd lng="hu">p&eacute;nzhamis&iacute;t&oacute;</trd>
  <trd lng="nl">valsmunter</trd>
  <trd 
lng="ru">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_o;&c_m;&c_o;&c_n;&c_je;&c_t;&c_ch;&c_i;&c_k;</trd>
  <trd lng="sk">peňazokaz</trd>
</drv>
</art>
