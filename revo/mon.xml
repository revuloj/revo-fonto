    <trd>munt</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bilon</trd>,
    <trd>moneta</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>bucat&abreve;</trd>,
    <trd>moned&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_je;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>minca</trd>,
    <trd>peniaz</trd>
  </trdgrp>
  <trd lng="sv">mynt</trd>
</drv>

<drv mrk="mon.0ereto">
  <kap><tld/>ereto</kap>
  <snc mrk="mon.0ereto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Malgrandvalora <tld/>ero:
