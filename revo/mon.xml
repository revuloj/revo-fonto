<drv mrk="mon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <tezrad fak="EKON"/>
  <snc mrk="mon.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      &Ccirc;ia rimedo, &gcirc;enerale agnoskata kiel inter&scirc;an&gcirc;ilo a&ubreve; pagilo:
      <ekz>
        a&ccirc;etita per <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 17</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi donas <tld/>on por la kampo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 23:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        plena sumo da <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 23:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi donos al vi panon pro viaj brutoj, se vi jam ne havas
        <tld/>on 
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 47:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        tridek sikloj da <tld/>o devas esti pagitaj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 21:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        li perdis sian dungo<tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 22:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pagu tiom da <tld/>o, kiom virgulinoj ordinare ricevas dote
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 22:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        donante al li <tld/>on, ne postulu de li procentojn
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 25:37</lok>
        </fnt>;
      </ekz>
      <ekz>
        por la <tld/>o pastra pre&gcirc;o, por la <tld/>o romp' de
        le&gcirc;o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        investi <tld/>on en ventomuelilaj projektoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007052.html">Verda
          nature, science, socie</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kolekti <tld/>on por helpi malsanajn infanojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006882.html">Pollando:
          Kiam orkestro fari&gcirc;as orkesto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li malavare disdonis <tld/>on el nigraj kasoj kaj kontoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006835.html">Germanio:
          Pekoj de patriarko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la grandaj partioj <klr>[&#x2026;]</klr> tro facile <tld/>on kaj ne
        povis imagi, ke korupteco estus io malbona
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9308-alarmsignalo.txt">El
          mia vidpunkto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        nek la &scirc;tato nek la asekuraj kompanioj havas sufi&ccirc;e da <tld/>o
        por pagi tiom da pensioj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007101.html">Pensoj
          pensiaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kest.0o">kesto</ref>,
      <ref cel="auxtom.bank0o"><tld/>a&ubreve;tomato</ref>
      <ref cel="or.0o">oro</ref>,
      <ref cel="sak.0o">sako</ref>
    </refgrp>
  </snc>
  <trd lng="af">geld</trd>
