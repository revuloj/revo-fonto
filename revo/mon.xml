<drv mrk="mon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <tezrad fak="EKON"/>
  <snc mrk="mon.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      &Ccirc;ia rimedo, &gcirc;enerale agnoskata kiel inter&scirc;an&gcirc;ilo a&ubreve; pagilo:
      <ekz>
        a&ccirc;etita per <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 17</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi donas <tld/>on por la kampo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 23:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        plena sumo da <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 23:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi donos al vi panon pro viaj brutoj, se vi jam ne havas
        <tld/>on 
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 47:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        tridek sikloj da <tld/>o devas esti pagitaj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 21:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        li perdis sian dungo<tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 22:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        li pagu tiom da <tld/>o, kiom virgulinoj ordinare ricevas dote
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 22:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        donante al li <tld/>on, ne postulu de li procentojn
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 25:37</lok>
        </fnt>;
      </ekz>
      <ekz>
        por la <tld/>o pastra pre&gcirc;o, por la <tld/>o romp' de
        le&gcirc;o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        investi <tld/>on en ventomuelilaj projektoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007052.html">Verda
          nature, science, socie</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kolekti <tld/>on por helpi malsanajn infanojn
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/006882.html">Pollando:
          Kiam orkestro fari&gcirc;as orkesto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li malavare disdonis <tld/>on el nigraj kasoj kaj kontoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/006835.html">Germanio:
          Pekoj de patriarko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la grandaj partioj <klr>[&#8230;]</klr> tro facile <tld/>on kaj ne
        povis imagi, ke korupteco estus io malbona
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="http://steloj.de/esperanto/monato1/monato-9308-alarmsignalo.txt">El
          mia vidpunkto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        nek la &scirc;tato nek la asekuraj kompanioj havas sufi&ccirc;e da <tld/>o
        por pagi tiom da pensioj
        <fnt>
          <bib>Monato</bib>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007101.html">Pensoj
          pensiaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kest.0o">kesto</ref>,
      <ref cel="or.0o">oro</ref>,
      <ref cel="sak.0o">sako</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_o;&c_sh;&c_y;</trd>
  <trd lng="bg">&c_p;&c_a;&c_r;&c_i;</trd>
  <trdgrp lng="br">
    <trd>arc'hant</trd>,
    <trd>moneiz</trd>
  </trdgrp>
  <trd lng="ca">diners</trd>
  <trd lng="cs">pen&iacute;ze</trd>
  <trd lng="de">Geld</trd>
  <trd lng="en">money</trd>
  <trd lng="es">dinero</trd>
  <trd lng="fa">&f_p;&a_w;&a_l;</trd>
  <trdgrp lng="fr">
    <trd>argent</trd>,
    <trd>monnaie</trd>
  </trdgrp>
  <trd lng="hu">p&eacute;nz</trd>
  <trd lng="id">uang</trd>
  <trdgrp lng="ku">
    <trd>pere</trd>,
    <trd>draf</trd>
  </trdgrp>
  <trd lng="nl">geld</trd>
  <trd lng="oc">argent</trd>
  <trd lng="pl">pieni&aogonek;dze</trd>
  <trd lng="pt">dinheiro</trd>
  <trd lng="ro">ban</trd>
  <trd lng="ru">&c_d;&c_je;&c_n;&c_mol;&c_g;&c_i;</trd>
  <trd lng="sk">peniaze</trd>
  <trd lng="sv">pengar</trd>
  <trd lng="tp">mani</trd>
  <trd lng="tr">para</trd>
  <trd lng="vo">mon</trd>
  <trdgrp lng="zh">
    <trd>&#38065; [<ind>qi&aacute;n</ind>]</trd>
    <trd>&#37329;&#38065; [<ind>j&#299;n qi&aacute;n</ind>]</trd>
  </trdgrp>
</drv>

<drv mrk="mon.0a">
  <kap><tld/>a</kap>
  <snc mrk="mon.0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a unuo;
      </ekz><ekz>
        <tld/>aj aferoj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz><ekz>
       <tld/>aj interesoj;
      </ekz>
      <ekz>
        &ccirc;i tiu ora &scirc;ildo, inkrustita per multekostaj &scirc;tonoj, havis, krom
        granda <tld/>a valoro, forton de l' amuleto
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        komercistoj &#8208; por ili la tuta vivo estas bazita sur la <tld/>a
        profito
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> &ccirc;iam sin trovadis en <tld/>a embaraso
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        al miaj oficistoj mankas <tld/>aj rimedoj, kiujn ne povas plu
        liveri mia malri&ccirc;i&gcirc;inta trezorejo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu nivelo de kreskado eblos nur pro &gcirc;ia libera <tld/>a politiko
        <fnt>
          <bib>Monato</bib>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="http://www.monato.net/2013/011053.php">Ekonomia
          fato</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="financ.0a">financa</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="br">
    <trd>-<ind>arc'hant</ind></trd>,
    <trd>moneizel</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&ecaron;nov&yacute;</trd>,
    <trd>pen&ecaron;&zcaron;it&yacute;</trd>,
    <trd>pen&ecaron;&zcaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Geld-</trd>
  <trdgrp lng="en">
    <trd>monetary</trd>,
    <trd>pecuniary</trd>
  </trdgrp>
  <trd lng="es">monetario</trd>
  <trd lng="fa">&f_p;&a_w;&a_l;&f_y;</trd>
  <trd lng="fr">mon&eacute;taire</trd>
  <trdgrp lng="hu">
    <trd>p&eacute;nz-</trd>,
    <trd>p&eacute;nz&uuml;gyi</trd>,
    <trd>monet&aacute;ris</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>geld-</trd>,
    <trd>monetair</trd>
  </trdgrp>
  <trd lng="pl">pieni&eogonek;&zdot;ny</trd>
  <trd lng="ru">&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">pe&ncaron;a&zcaron;n&yacute;</trd>
</drv>

<drv mrk="mon.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="mon.0ero.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Aparta peco de metala <tld/>o:
      <ekz>
        centimo, pfenigo kaj kopeko estas <tld/>eroj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 41</lok>
        </fnt>;
      </ekz>
      <ekz>
        dudek ar&gcirc;entaj <tld/>eroj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 37:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        punpago de cent ar&gcirc;entaj <tld/>eroj
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 22:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        en sia dekstra po&scirc;o li portis la paper<tld/>on <klr>[&#8230;]</klr>
        kaj en
        leda saketo sur la brusto li havis kelke da alkudritaj oraj
        <tld/>eroj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        sen la kupraj <tld/>eroj, kiujn oni &jcirc;etas al li, <klr>[&#8230;]</klr>
        la kantisto de Luziado devus morti de malsato
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        du flankoj de la sama <tld/>ero
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/006810.html">Diskutige
          kaj diskutinde</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        unu <tld/>ero ne estas <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz><ekz>
        oni lin konas kiel malbonan <tld/>eron
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        havi provizojn de e&ubreve;ro-<tld/>eroj
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url
          ref="http://www.esperanto.be/fel/2004/008259.html">Planoj de
          la litova monerstampejo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li en&jcirc;etis &gcirc;ustan <tld/>eron, sed la a&ubreve;tomato ne reagis, e&ccirc; ne
        redonis la monon
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url
          ref="http://www.esperanto.be/fel/2004/008357.html">La unua
          nokto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        antikvaj <tld/>eroj
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url
          ref="http://www.esperanto.be/fel/2004/008445.html">Kolekti
          antikva&jcirc;ojn</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        eldono de <tld/>eroj kaj <ref
        cel="bilet.mon0o"><tld/>biletoj</ref>
        <fnt>
          <bib>Monato</bib>
          <aut>Lawrence Mee</aut>
          <vrk><url ref="http://www.monato.net/2005/008283.php">La
          stelo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>erstampejo
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url
          ref="http://www.esperanto.be/fel/2004/008259.html">Planoj de
          la litova monerstampejo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>erigi &ccirc;ekon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_e;&c_t;&c_a;</trd>
  <trd lng="bg">&c_m;&c_o;&c_n;&c_je;&c_t;&c_a;</trd>
  <trd lng="br">pezh moneiz</trd>
  <trdgrp lng="cs">
    <trd>drobn&eacute; mince</trd>,
    <trd>mince</trd>,
    <trd>pen&iacute;z</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>M&uuml;nze</trd>,
    <trd>Geldst&uuml;ck</trd>
  </trdgrp>
  <trd lng="en">coin</trd>
  <trd lng="es">moneda</trd>
  <trd lng="fa">&a_s;&f_k;&a_h;</trd>
  <trd lng="fr">pi&egrave;ce de monnaie</trd>
  <trd lng="hu">p&eacute;nz&eacute;rme</trd>
  <trd lng="id"><ind>uang</ind> receh</trd>
  <trdgrp lng="nl">
    <trd>muntstuk</trd>,
    <trd>munt</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_je;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>minca</trd>,
    <trd>peniaz</trd>
  </trdgrp>
  <trd lng="sv">mynt</trd>
</drv>

<drv mrk="mon.0ereto">
  <kap><tld/>ereto</kap>
  <snc mrk="mon.0ereto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Malgrandvalora <tld/>ero:
      <ekz>
        malferminte denove sian <tld/>ujon, &scirc;i kalkulis la
        <tld/>eretojn, kiuj restis en &gcirc;i post la faritaj a&ccirc;etoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ereto de feli&ccirc;o
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_o;&c_b;&c_n;&c_a;&c_ja; 
&c_m;&c_a;&c_n;&c_e;&c_t;&c_a;</trd>
  <trd lng="br">moneiz bihan</trd>
  <trdgrp lng="de">
    <trd>kleine <ind>M&uuml;nze</ind></trd>,
    <trd>kleines <ind>Geldst&uuml;ck</ind></trd>
  </trdgrp>
  <trd lng="fa">&f_p;&a_w;&a_l; &a_kh;&a_r;&a_d;</trd>
  <trdgrp lng="fr">
    <trd>petite monnaie</trd>,
    <trd>pi&egrave;cette</trd>
  </trdgrp>
  <trd lng="hu">apr&oacute;p&eacute;nz</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_k;&c_a;&c_ja; 
&c_m;&c_o;&c_n;&c_je;&c_t;&c_a;</trd>
</drv>

<drv mrk="mon.0ujo">
  <kap><ofc>*</ofc><tld/>ujo</kap>
  <snc mrk="mon.0ujo.KOMUNE">
    <dif>
      Po&scirc;o a&ubreve; saketo destinita por kunporti <tld/>erojn:
      <ekz>
        en la po&scirc;o de mia pantalono mi portas <tld/>ujon
        <fnt><bib>F</bib>, <lok>&FE;40</lok></fnt>;
      </ekz><ekz>
        la vidvino metis la manon en la po&scirc;on kaj eltiris
        <tld/>ujon, kiu enhavis kelke da <tld/>paperetoj
        <fnt><bib>Marta</bib></fnt>;
      </ekz><ekz>
        Tolstoj haltis kaj donis el sia <tld/>ujo du ar&gcirc;entajn
        <tld/>erojn
        <fnt>
          <aut>V. Langlet</aut>,
          <vrk>Voja&gcirc;impresoj</vrk>,
          <lok>1895</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_ib;&c_t;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_a;&c_l;&c_jo;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_r;&c_t;&c_m;&c_o;&c_n;&c_je;</trd>
  <trd lng="br">doug-moneiz</trd>
  <trd lng="cs">pen&ecaron;&zcaron;enka</trd>
  <trdgrp lng="de">
    <trd>Geldbeutel</trd>,
    <trd>Geldtasche</trd>,
    <trd>Brieftasche</trd>,
    <trd>Portemonnaie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>wallet</trd>,
    <trd>handbag</trd>,
    <trd>purse</trd>
  </trdgrp>
  <trd lng="es">monedero</trd>
  <trd lng="fa">&f_k;&f_y;&a_f; &f_p;&a_w;&a_l;</trd>
  <trd lng="fr">porte-monnaie</trd>
  <trdgrp lng="hu">
    <trd>ersz&eacute;ny</trd>,
    <trd>p&eacute;nzt&aacute;rca</trd>
  </trdgrp>
  <trd lng="id">dompet</trd>
  <trd lng="nl">portemonnee</trd>
  <trdgrp lng="pl">
    <trd>portfel</trd>,
    <trd>portmonetka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_sh;&c_je;&c_l;&c_jo;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_t;&c_m;&c_o;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">pe&ncaron;a&zcaron;enka</trd>
  <trd lng="tr">c&uuml;zdan</trd>
  <trd lng="zh">&#38065;&#21253;</trd>
</drv>




<drv mrk="mon.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="mon.sen0a.KOMUNE">
    <dif>
      Ne havanta <tld/>on:
      <ekz>
        kiam la poeto Paul Verlaine estis sen<tld/>a kaj tamen soifa, la
        kelneroj en la latina kvartalo akceptis porpage &jcirc;us verkitan
        versa&jcirc;on
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga
          Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        meditoj pri sen<tld/>aj tempoj
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga
          Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        liaj lastaj vivojaroj pasis en instituto por sen<tld/>aj
        maljunuloj
        <fnt>
          <bib>Monato</bib>
          <aut>W. M. A. de Smet</aut>
          <vrk><url
          ref="http://www.esperanto.be/fel/2004/008418.html">Edward
          Croegaert, la forgesita lingvokreanto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>aj studentoj kelkfoje &scirc;tele enbusi&gcirc;as por eviti a&ccirc;eti
        bileton
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="http://www.monato.net/2007/009247.php">Busaj
          servoj plibonigendaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_z;&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_y;</trd>
  <trd lng="br">diarc'hant</trd>
  <trd lng="cs">nesolventn&iacute;</trd>
  <trdgrp lng="de">
    <trd>abgebrannt</trd>,
    <trd>pleite</trd>
  </trdgrp>
  <trd lng="en">broke</trd>
  <trd lng="fa">&a_b;&f_y;&zwnj;&f_p;&a_w;&a_l;</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;sargent&eacute;</trd>,
