<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mon</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="mon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <tezrad fak="EKON"/>
  <snc mrk="mon.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Pagilo la&ubreve;le&gcirc;e fabrikata kaj konsistanta el stampita
      metaldisketo
      a&ubreve; surpresita paperfolio kun difinita valoro por servi kiel
      a&ccirc;etrimedo:
      <ekz>
        <tld/>o plenumas &ccirc;iujn oficojn
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        por la <tld/>o pastra pre&gcirc;o, por la <tld/>o romp' de
        le&gcirc;o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kest.0o">kesto</ref>,
      <ref cel="or.0o">oro</ref>,
      <ref cel="sak.0o">sako</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_o;&c_sh;&c_y;</trd>
  <trd lng="bg">&c_p;&c_a;&c_r;&c_i;</trd>
  <trdgrp lng="br">
    <trd>arc'hant</trd>,
    <trd>moneiz</trd>
  </trdgrp>
  <trd lng="cs">pen&iacute;ze</trd>
  <trd lng="de">Geld</trd>
  <trd lng="en">money</trd>
  <trd lng="es">dinero</trd>
  <trdgrp lng="fr">
    <trd>monnaie</trd>,
    <trd>argent</trd>
  </trdgrp>
  <trd lng="hu">p&eacute;nz</trd>
  <trd lng="nl">geld</trd>
  <trd lng="pl">pieni&aogonek;dze</trd>
  <trd lng="pt">dinheiro</trd>
  <trd lng="ro">ban</trd>
  <trd lng="ru">&c_d;&c_je;&c_n;&c_mol;&c_g;&c_i;</trd>
  <trd lng="sv">pengar</trd>
  <trd lng="tp">mani</trd>
  <trd lng="vo">mon</trd>
</drv>

<drv mrk="mon.0a">
  <kap><tld/>a</kap>
  <snc mrk="mon.0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a unuo;
      </ekz>
      <ekz>
        <tld/>aj aferoj, interesoj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="financ.0a">financa</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_y;</trd>
  <trdgrp lng="br">
    <trd>-<ind>arc'hant</ind></trd>,
    <trd>moneizel</trd>
  </trdgrp>
  <trd lng="es">monetario</trd>
  <trdgrp lng="hu">
    <trd>p&eacute;nz-</trd>,
    <trd>p&eacute;nz&uuml;gyi</trd>,
    <trd>monet&aacute;ris</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>geld-</trd>,
    <trd>monetair</trd>
  </trdgrp>
  <trd lng="pl">pieni&eogonek;&zdot;ny</trd>
  <trd lng="ru">&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="mon.internacia0afonduso">
  <kap>Internacia <tld lit="M"/>a Fonduso</kap>
  <mlg>IMF</mlg>
  <fnt><bib>ManPol</bib></fnt>
  <snc mrk="mon.internacia0afonduso.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Speciala <ref tip="super" cel="organi2.0ajxo">organiza&jcirc;o</ref> de
      <ref tip="malprt" cel="naci.Unuigxintaj_0oj">Unui&gcirc;intaj
      Nacioj</ref>,
      kiu kontrolas la internacian valutosistemon kaj provas ekvilibrigi
      la kreskadon de la monda komerco:
      <ekz>
        en Prago okazis la 55a
        jarkunsido de Internacia <tld lit="M"/>a Fonduso kaj de Monda
        Banko (MB), kun kunvenoj de ministroj pri financo, fakaj
        seminarioj, prelegoj, konferencoj, ktp.
        <fnt>
        <aut>J. Patera</aut>,
        <vrk><url ref="&Monato;/007258.html">
        Post la kunsido de financistoj en Prago</url></vrk>,
        <bib>Monato</bib>,
        <lok>numero 2001-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_M;&c_ib;&c_zh;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y; &c_v;&c_a;&c_l;&c_ju;&c_t;&c_n;&c_y; &c_f;&c_o;&c_n;&c_d;</trd>,
    <trd>&c_M;&c_V;&c_F;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>Font Moneizel Etrevroadel</trd>,
    <trd>FME</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Internationale <ind>W&auml;hrungsfonds</ind></trd>,
    <trd>IWF</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>International <ind>Monetary</ind> Fund</trd>,
    <trd>IMF</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>Fondo <ind>Monetario</ind> Internacional</trd>,
    <trd>FMI</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Fonds <ind>Mon&eacute;taire</ind> International</trd>,
    <trd>FMI</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Nemzetk&ouml;zi <ind>Valutaalap</ind></trd>,
    <trd>IMF</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Internationaal <ind>Monetair</ind> Fonds</trd>,
    <trd>IMF</trd>
  </trdgrp>
  <trd lng="pl">Mi&eogonek;dzynarodowy Fundusz Walutowy</trd>
  <trdgrp lng="pt">
    <trd>Fundo <ind>Monet&aacute;rio</ind> Internacional</trd>,
    <trd>FMI</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_M;&c_je;&c_zh;&c_d;&c_u;&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y;&c_j; <ind>&c_V;&c_a;&c_l;&c_ju;&c_t;&c_n;&c_y;&c_j;</ind>
    &c_F;&c_o;&c_n;&c_d;</trd>,
    <trd>&c_M;&c_V;&c_F;</trd>
  </trdgrp>
</drv>

<drv mrk="mon.0ero">
  <kap><tld/>ero</kap>
  <snc mrk="mon.0ero.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Aparta peco de metala <tld/>o:
      <ekz>
        ora <tld/>ero;
      </ekz>
      <ekz>
        unu <tld/>ero ne estos <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni lin konas kiel malbonan <tld/>eron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>erigi &ccirc;ekon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_n;&c_e;&c_t;&c_a;</trd>
  <trd lng="bg">&c_m;&c_o;&c_n;&c_je;&c_t;&c_a;</trd>
  <trd lng="br">pezh moneiz</trd>
  <trdgrp lng="de">
    <trd>M&uuml;nze</trd>,
    <trd>Geldst&uuml;ck</trd>
  </trdgrp>
  <trd lng="fr">pi&egrave;ce de monnaie</trd>
  <trd lng="hu">p&eacute;nz&eacute;rme</trd>
  <trd lng="nl">muntstuk</trd>
  <trd lng="ru">&c_m;&c_o;&c_n;&c_je;&c_t;&c_a;</trd>
  <trd lng="sv">mynt</trd>
</drv>

<drv mrk="mon.0ereto">
  <kap><tld/>ereto</kap>
  <snc mrk="mon.0ereto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Malgrandvalora <tld/>ero.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_o;&c_b;&c_n;&c_a;&c_ja; &c_m;&c_a;&c_n;&c_e;&c_t;&c_a;</trd>
  <trd lng="br">moneiz bihan</trd>
  <trdgrp lng="de">
    <trd>kleine M&uuml;nze</trd>,
    <trd>kleines Geldst&uuml;ck</trd>
  </trdgrp>
  <trd lng="fr">petite monnaie</trd>
  <trd lng="hu">apr&oacute;p&eacute;nz</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_n;&c_je;&c_t;&c_a;</trd>
</drv>

<drv mrk="mon.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="mon.0ujo.KOMUNE">
    <dif>
      Saketo destinita por enteni <tld/>erojn, kiun oni portas ordinare
      en po&scirc;o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_l;&c_ib;&c_t;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_sh;&c_a;&c_l;&c_jo;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_o;&c_r;&c_t;&c_m;&c_o;&c_n;&c_je;</trd>
  <trd lng="br">doug-moneiz</trd>
  <trdgrp lng="de">
    <trd>Geldbeutel</trd>,
    <trd>Geldtasche</trd>,
    <trd>Brieftasche</trd>,
    <trd>Portemonnaie</trd>
  </trdgrp>
  <trd lng="es">monedero</trd>
  <trd lng="fr">porte-monnaie</trd>
  <trdgrp lng="hu">
    <trd>ersz&eacute;ny</trd>,
    <trd>p&eacute;nzt&aacute;rca</trd>
  </trdgrp>
  <trd lng="nl">portemonnee</trd>
  <trdgrp lng="pl">
    <trd>portfel</trd>,
    <trd>portmonetka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_sh;&c_je;&c_l;&c_jo;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_t;&c_m;&c_o;&c_n;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="mon.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="mon.sen0a.KOMUNE">
    <dif>
      Ne havanta <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_z;&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_y;</trd>
  <trd lng="br">diarc'hant</trd>
  <trdgrp lng="de">
    <trd>abgebrannt</trd>,
    <trd>pleite</trd>
  </trdgrp>
  <trd lng="hu">p&eacute;nztelen</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="mon.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc mrk="mon.sen0igi.KOMUNE">
    <dif>
      Senigi iun je <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="sucx.el0i">elsu&ccirc;i</ref>,
      <ref cel="hav.sen0igi">senhavigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>um sein Geld <ind>bringen</ind></trd>,
    <trd>das Geld aus der Tasche <ind>ziehen</ind></trd>,
    <trd>abzocken</trd>,
    <trd>berauben</trd>
  </trdgrp>
  <trd lng="hu">p&eacute;nz&eacute;t&odblac;l megfoszt</trd>
  <trd lng="nl">van zijn/haar geld <ind>beroven</ind></trd>
</drv>

<drv mrk="mon.0avida">
  <kap><tld/>avida</kap>
  <snc mrk="mon.0avida.KOMUNE">
    <refgrp tip="sin">
      <ref cel="avar.0a">avara</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="mamon.0o">mamono</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Geldgier</trd>
  </trdgrp>
  <trd lng="hu">p&eacute;nz&eacute;hs&eacute;g</trd>
  <trd lng="nl">gierigheid</trd>
</drv>

<drv mrk="mon.0falsisto">
  <kap><tld/>falsisto</kap>
  <snc mrk="mon.0falsisto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Tiu kiu fabrikas kontra&ubreve;le&gcirc;e senvaloran <tld/>on, kiu
      trompe imitas la &scirc;tatan.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_a;&c_m;&c_a;&c_n;&c_e;&c_t;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_r;&c_o;&c_b;&c_sh;&c_ch;&c_y;&c_k; &c_g;&c_r;&c_o;&c_sh;&c_a;&c_w;</trd>
  </trdgrp>
  <trd lng="br">falsvoneizer</trd>
  <trdgrp lng="de">
    <trd>Geldf&auml;lscher</trd>,
    <trd>F&auml;lscher</trd>
  </trdgrp>
  <trd lng="es">falsificador</trd>
  <trd lng="fr">faux-monnayeur</trd>
  <trd lng="hu">p&eacute;nzhamis&iacute;t&oacute;</trd>
  <trd lng="nl">valsmunter</trd>
  <trd lng="ru">&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_i;&c_v;&c_o;&c_m;&c_o;&c_n;&c_je;&c_t;&c_ch;&c_i;&c_k;</trd>
</drv>

<drv mrk="mon.0puno">
  <kap><tld/>puno</kap>
  <snc mrk="mon.0puno.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Puno konsistanta en pago de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_r;&c_a;&c_f;</trd>
  <trd lng="br">tell-gastiz</trd>
  <trdgrp lng="de">
    <trd>Geldbu&szlig;e</trd>,
    <trd>Geldstrafe</trd>
  </trdgrp>
  <trd lng="es">multa</trd>
  <trdgrp lng="hu">
    <trd>p&eacute;nzb&uuml;ntet&eacute;s</trd>,
    <trd>b&iacute;rs&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">geldboete</trd>
  <trd lng="pl">kara pieni&eogonek;&zdot;na</trd>
  <trd lng="pt">multa</trd>
  <trd lng="ru">&c_sh;&c_t;&c_r;&c_a;&c_f;</trd>
  <trd lng="sv">b&ouml;ter</trd>
</drv>
</art>
