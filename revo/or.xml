<drv mrk="or.0a">
  <kap><tld/>a</kap>
  <snc mrk="or.0a.eloro">
  <dif>
    El <tld/>o:
  </dif>
  <subsnc>
    <ekz>
      <tld/>a ringo
      <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 11:22</lok>
        </fnt>;
    </ekz>
    <ekz>
      montri grandan <tld/>an medalon sur la kolo
      <fnt>
        <bib>Far1</bib>
        <lok>&Ccirc;apitro XXV</lok>
      </fnt>.
    </ekz>
    <trd lng="de">g&uuml;lden</trd>
  </subsnc>
  <subsnc>
    <ekz>
      <tld/>a &scirc;losilo &ccirc;iun pordon malfermas
      <fnt><bib>PrV</bib></fnt>;
    </ekz><ekz>
      <tld/>a posteno.
    </ekz>
  </subsnc>
  <subsnc>
    <ekz>
      &scirc;i estas tre bona, <ind><tld/>a koro</ind>
      <fnt>
        <bib>IK</bib>
        <lok>&Ccirc;aptiro IV</lok>
      </fnt>;
      <trd lng="de">goldiges Herz</trd>
    </ekz>
    <ekz>
      <klr>[voli]</klr> revenigi la oran tempa&gcirc;on
      <klr>(tempoj idealigitaj)</klr>
      <fnt>
        <bib>Rabistoj</bib>
        <lok>Akto Unua</lok>
      </fnt>;
    </ekz>
    <ekz>
      tiu dol&ccirc;a <tld/>a tempo estas frukto de fantazio neniam
      efektivigebla
      <fnt>
        <bib>OV</bib>
        <vrk><url ref="https://eo.wikisource.org/wiki/Originala_Verkaro/III/1"
            >Esperanto kaj Volap&uuml;k</url></vrk>
        <lok>La Esperantisto&rightquot; 1889/90. (1889, pa&gcirc;. 2&mdash;6; pa&gcirc;. 9&mdash;12. &mdash; 1890, pa&gcirc;. 24&mdash;27; pa&gcirc;. 37&mdash;38.</lok>
      </fnt>;
    </ekz>
    <ekz>
      promesi <tld/>ajn montojn
      (<ref tip="vid" cel="sxmir.0i">&scirc;miri</ref>)
      <fnt><bib>PrV</bib></fnt>;
    </ekz>
    <ekz>
      La <tld lit="O"/>aj Versoj de Pitagoro
      <fnt>
          <vrk><url ref="https://esperantaretradio.blogspot.com/2020/02/nova-akropolo-2.html"
              >Nova Akropolo</url></vrk>
          <lok>Esperanta Retaradio, 202-02-18</lok>
        </fnt>.
    </ekz>
  <trd lng="de">goldig</trd>
  </subsnc>
  <trd lng="en">gold</trd>
    <trdgrp lng="fr">
    <trd>d'or</trd>,
    <trd>en or</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>emas</baz>... emas</trd>,
    <trd>dari emas</trd>
  </trdgrp>
  </snc>
  <snc mrk="or.0a.kolore">
    <dif>
      Havanta <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloron</ref> de <tld/>o.
     </dif>
     <ref tip="sub" cel="blond.0a">blonda</ref>
     <trd lng="de">goldfarben</trd>
     <trd lng="en">golden</trd>
    <trd lng="fr">dor&eacute;</trd>
    <trdgrp lng="id">
      <trd><baz>emas</baz>keemasan</trd>,
      <trd><baz>emas</baz>keemas-emasan</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_l;&c_a;&c_t;&c_y;</trd>
  <trd lng="bg">&c_z;&c_l;&c_a;&c_t;&c_o;</trd>
  <trdgrp lng="br">
    <trd>alaouret</trd>,
    <trd>aour<klr tip="ind"> (en aour)</klr></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>daurat</trd>,
    <trd>d'or</trd>,
    <trd>auri</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gold-</trd>,
    <trd>golden</trd>
  </trdgrp>
  <trd lng="el">&chi;&rho;&ypsilon;&sigma;&alfa;&phi;
  &epsilon_ton;&ny;&jota;&omikron;&sigma_fina;</trd>
