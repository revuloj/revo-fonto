      <trd>&c_b;&c_l;&c_ib;&c_sh;&c_ch;&c_e;&c_c;&c_mol; &c_ja;&c_k; &c_z;&c_o;&c_l;&c_a;&c_t;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>resplendir</trd>,
      <trd>excel&middot;lir</trd>
    </trdgrp>
    <trd lng="de">golden <ind>scheinen</ind></trd>
    <trd lng="en"><ind>shine</ind> like gold</trd>
    <trd lng="id"><baz>emas</baz>mengemas</trd>
    <trd lng="pl">z&lstroke;oci&cacute; si&eogonek;</trd>
    <trd lng="ro">auri</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>pozlatit</trd>,
    <trd>zlatit</trd>
  </trdgrp>
  <trd lng="es">chapar<klr tip="amb"> (en oro)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x91D1;&#x3092;&#x304B;&#x3076;&#x305B;&#x308B;
    <pr>&#x304B;&#x306D;&#x3092;&#x304B;&#x3076;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x91D1;&#x7B94;&#x3092;&#x5F35;&#x308B;  <pr>&#x304D;&#x3093;&#x3071;&#x304F;&#x3092;&#x306F;&#x308B;</pr></trd>,
    <trd>&#x91D1;&#x30E1;&#x30C3;&#x30AD;&#x3059;&#x308B;
    <pr>&#x304B;&#x306D;&#x3081;&#x3063;&#x304D;&#x3059;&#x308B;</pr></trd>
