<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>or</rad>/o
</kap>
<drv mrk="or.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg>Au</mlg>
  <snc>
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 79, atompezo 196,967, simbolo Au. Flava metalo, tre
      densa, facile fadenigebla multvalora, kiun oni uzas por juveloj kaj
      moneroj:
      <ekz>
        vazo el <tld/>o;
      </ekz>
      <ekz>
        <tld/>o la&ubreve; folioj, la&ubreve; fanda&jcirc;oj;
      </ekz>
      <ekz>
        temp' estas valoro simile al <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne &ccirc;io estas <tld/>o, kio brilas<fnt>B</fnt>
        <klr>(ne fidu brilan eksteron)</klr>;
      </ekz>
      <ekz>
        pagi per <tld/>o
        <klr>(ne per valorpaperoj)</klr>;
      </ekz>
      <ekz>
        vi devas &ccirc;i tie <tld/>velki (i&gcirc;i <tld/>kolora pro 
velki&gcirc;o),
        kompatinda floreto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="de">Gold</trd>
    <trd lng="en">gold</trd>
    <trd lng="es">oro</trd>
    <trd lng="fr">or</trd>
    <trd lng="it">oro</trd>
    <trd lng="nl">goud</trd>
  </snc>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Mono, ri&ccirc;eco:
      <ekz>
        kiu havas <tld/>on, havas honoron
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        por sia <tld/>o &ccirc;iu estas sinjoro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ne defendas <tld/>o kontra&ubreve; doloro
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ilo el <tld/>o ta&ubreve;gas por &ccirc;iu laboro
        <klr>(per mono oni faras &ccirc;ion)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Geld</trd>,
      <trd>Gold</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>geld</trd>,
      <trd>goud</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Multvalora, bonega afero:
      <ekz>
        &gcirc;entila kaj trankvila, kun koro el <tld/>o
        (<refgrp tip="vid">
          <ref cel="ole.0a">olea</ref>,
          <ref cel="vaks.0o">vakso</ref>
        </refgrp>)
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        legu tiun poemon: &gcirc;i estas vere el <tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Gold</trd>
    <trd lng="nl">goud</trd>
  </snc>                                 
  <trd lng="be">&c_z;&c_o;&c_l;&c_a;&c_t;&c_a;</trd>
  <trd lng="br">aour</trd>
  <trd lng="cs">zlato</trd>
  <trdgrp lng="el">
    <trd>&chi;&rho;&ypsilon;&sigma;&alfa_ton;&phi;&jota;</trd>,
    <trd>&chi;&rho;&ypsilon;&sigma;&omikron_ton;&sigma_fina;</trd>
  </trdgrp>
  <trd lng="en">gold</trd>
  <trd lng="hu">arany</trd>
  <trd lng="pl">z&lstroke;oto</trd>
  <trdgrp lng="pt">
    <trd>oiro</trd>,
    <trd>ouro<klr> (bras.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_o;&c_l;&c_o;&c_t;&c_o;</trd>
  <trd lng="sv">guld</trd>
</drv>

<drv mrk="or.0a">
  <kap><tld/>a</kap>
  <dif>
    El <tld/>o:
  </dif>
  <snc>
    <ekz>
      <tld/>a ringo, medalo;
    </ekz>
    <ekz>
      <tld/>aj minejoj.
    </ekz>
    <trdgrp lng="de">
      <trd>golden</trd>,
      <trd>g&uuml;lden [arkaika]</trd>
    </trdgrp>
  </snc>
  <snc>
    <ekz>
      <tld/>a &scirc;losilo &ccirc;iun pordon malfermas
      <fnt><bib>PrV</bib></fnt>;
    </ekz>
    <ekz>
      <tld/>a posteno.
    </ekz>
    <trd lng="de">golden</trd>
  </snc>
  <snc>
    <ekz>
      Tiu dol&ccirc;a <tld/>a tempo estas frukto de fantazio neniam
      efektivigebla<fnt>Z</fnt>;
    </ekz>
    <ekz>
      tiu utopiulo esperas revenigi la <tld/>an
      tempa&gcirc;on<fnt>Z</fnt>
      <klr>(feli&ccirc;an praan tempa&gcirc;on, je kiu kredis 
antikvuloj)</klr>;
    </ekz>
    <ekz>
      promesi <tld/>ajn montojn
      (<ref tip="vid" cel="sxmir.0i">&scirc;miri</ref>)
      <fnt><bib>PrV</bib></fnt>;
    </ekz>
    <ekz>
      <tld/>a koro;
    </ekz>
    <ekz>
      <tld/>aj versoj de Pitagoro.
    </ekz>
    <trd lng="de">golden</trd>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_l;&c_a;&c_t;&c_y;</trd>      
  <trdgrp lng="br">
    <trd>alaouret</trd>,   
    <trd>aour<klr tip="ind"> (en aour)</klr></trd>
  </trdgrp>
  <trd 
lng="el">&chi;&rho;&ypsilon;&sigma;&alfa;&phi;&epsilon_ton;&ny;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="hu">arany-</trd>
  <trd lng="nl">gouden</trd>
  <trdgrp lng="pt">
    <trd>de oiro</trd>,
    <trd>de ouro<klr> (bras.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_o;&c_l;&c_o;&c_t;&c_o;&c_j;</trd>
  <trd lng="sv">gyllene</trd>
</drv>

<drv mrk="or.0igi">
  <kap><tld/>(ig)i</kap>
  <snc>
    <dif>
      Kovri per maldika tavolo el <tld/>o:
      <ekz>
        <tld/>ita kadro<fnt>Z</fnt>, braceleto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la suno <tld/>igas <klr>(<tld/>e briligas)</klr>
        per siaj radioj la fenestrajn vitra&jcirc;ojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        pensoj <tld/>itaj per la lasta rebrilo de l'
        amo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_z;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>     
    <trdgrp lng="br">
      <trd>alaouri&ntilde;</trd>,   
      <trd>aouri&ntilde;</trd>
    </trdgrp>
    <trd lng="de">vergolden</trd>
    <trd lng="fr">dorer</trd>
    <trd lng="hu">aranyoz</trd>
    <trd lng="nl">vergulden</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">f&ouml;rgyll</trd>
  </snc>
</drv>

<drv mrk="or.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Verka&jcirc;o el <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_l;&c_a;&c_t;&c_y; 
&c_v;&c_y;&c_r;&c_a;&c_b;</trd>    
  <trd lng="br">aouraj</trd>
  <trd lng="hu">aranyt&aacute;rgy</trd>
  <trd lng="ru">&c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je; &c_i;&c_z; 
<ind>&c_z;&c_o;&c_l;&c_o;&c_t;&c_a;</ind></trd>
</drv>

<drv mrk="or.0ajxisto">
  <kap><tld/>a&jcirc;isto</kap>
  <snc>
    <dif>
      Tiu, kiu profesie laboras super <tld/>a&jcirc;oj.
    </dif>
  </snc>
  <trd lng="be">&c_ju;&c_v;&c_e;&c_l;&c_ib;&c_r; <klr 
tip="amb">(&c_p;&c_a; &c_z;&c_o;&c_l;&c_a;&c_t;&c_u;)</klr></trd>     
  <trdgrp lng="br">
    <trd>aourer</trd>,   
    <trd>orfebour</trd>
  </trdgrp>
  <trd lng="de">Goldschmied</trd>
  <trd lng="fr">orf&egrave;vre</trd>
  <trd lng="hu">aranym&udblac;ves</trd>
  <trd lng="nl">goudsmit</trd>
  <trd lng="ru">&c_ju;&c_v;&c_je;&c_l;&c_i;&c_r;<klr tip="amb"> 
(&c_p;&c_o; &c_z;&c_o;&c_l;&c_o;&c_t;&c_u;)</klr></trd>
  <trd lng="sv">guldsmed</trd>
</drv>

<drv mrk="or.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Regiono, en kiu oni trovas <tld/>masojn a&ubreve; <tld/>vejnojn.
    </dif>
  </snc>                               
  <trd lng="br">aoureg</trd>
  <trd lng="hu">aranylel&odblac;hely</trd>
  <trd lng="nl">goudveld</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_a;&c_ja; 
&c_r;&c_o;&c_s;&c_s;&c_y;&c_p;&c_mol;</trd>,
    <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_o;&c_j; 
&c_p;&c_r;&c_i;&c_i;&c_s;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="or.0igisto">
  <kap><tld/>igisto</kap>
  <snc>
    <dif>
      Tiu, kiu profesie <tld/>as.
    </dif>
  </snc>                                  
  <trd lng="br">alaourer</trd>       
  <trd lng="fr">doreur</trd>
  <trd 
lng="el">&chi;&rho;&ypsilon;&sigma;&omikron;&chi;&omikron_ton;&omikron;&sigma_fina;</trd>
  <trd lng="hu">aranyoz&oacute;</trd>
  <trd lng="pt">ourives</trd>
</drv>
</art>
