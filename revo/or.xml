    <drv mrk="or.0i">
      <kap><tld/>i</kap>
      <snc mrk="or.0i.tr">
        <gra><vspec>tr</vspec></gra>
        <dif>
          Kovri per maldika tavolo el <tld/>o:
          <ekz>
            <tld/>ita kadro<fnt>Z</fnt>, braceleto;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            pensoj <tld/>itaj per la lasta rebrilo de l'
            amo<fnt>B</fnt>.
          </ekz>
        </dif>
        <ref tip="sin" cel="or.0umi"><tld/>umi</ref>
        <trdgrp lng="be">
          <trd>&c_z;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>,
          <trd>&c_p;&c_a;&c_z;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>
        </trdgrp>
        <trdgrp lng="br">
          <trd>alaouri&ntilde;</trd>,
          <trd>aouri&ntilde;</trd>
        </trdgrp>
        <trd lng="ca">daurar</trd>
        <trd lng="de">vergolden</trd>
        <trd lng="en">gild</trd>
        <trd lng="fr">dorer</trd>
        <trd lng="hu">aranyoz</trd>
        <trd lng="nl">vergulden</trd>
        <trdgrp lng="pl">
          <trd>z&lstroke;oci&cacute;</trd>,
          <trd>oz&lstroke;aca&cacute;</trd>,
          <trd>poz&lstroke;aca&cacute;</trd>
        </trdgrp>
        <trd lng="ro">auri</trd>
        <trdgrp lng="ru">
          <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_p;&c_o;&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>
        </trdgrp>
        <trd lng="sv">f&ouml;rgyll</trd>
        <trdgrp lng="zh">
    <trd>&#27877;&#37329; <pr>n&iacute;j&imacron;n</pr></trd>,
    <trd>&#38002;&#37329; <pr>xi&amacron;ngj&imacron;n</pr></trd>,
    <trd>&#38262;&#37329; <pr>xi&amacron;ngj&imacron;n</pr></trd>
  </trdgrp>
</snc>
      <snc mrk="or.0i.ntr">
        <gra><vspec>ntr</vspec></gra>
        <dif>
          Impresi kiel <tld/>o, aspekti kiel <tld/>o:
          <ekz>
            la duono de Forumo, ku&scirc;anta senpere sub la kastelaj
            abrupta&jcirc;oj, estis jam dronanta en ombro, dum la
            kolonoj de la pli alte starantaj temploj <tld/>is ankora&ubreve;
            en lumo kaj bluo
            <fnt>
              <aut>H. Sienkewicz, tr. Lidia Zamenhof</aut>:
              <vrk>Quo vadis</vrk>, <lok>&Ccirc;ap. 2a</lok>
            </fnt>;
          </ekz><ekz>
            la vivo tiel mirinde belis, la suno tiel <tld/>is kaj
            varmis, la lando tiel fabelece &ccirc;armis <klr>...</klr>
            <fnt>
              <aut>B. Traven, tr. Hans-Georg Kaiser</aut>:
              <vrk>La Mortula &scirc;ipo</vrk>. <lok>&Ccirc;ap. 17a</lok>
            </fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_z;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>,
          <trd>&c_b;&c_l;&c_ib;&c_sh;&c_ch;&c_e;&c_c;&c_mol; &c_ja;&c_k; &c_z;&c_o;&c_l;&c_a;&c_t;&c_a;</trd>
        </trdgrp>
        <trdgrp lng="ca">
          <trd>resplendir</trd>,
          <trd>excel&middot;lir</trd>
        </trdgrp>
        <trd lng="en"><ind>shine</ind> like gold</trd>
        <trd lng="pl">z&lstroke;oci&cacute; si&eogonek;</trd>
        <trd lng="ro">auri</trd>
        <trdgrp lng="ru">
          <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_je;&c_t;&c_mol;</trd>,
          <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
        </trdgrp>
      </snc>
