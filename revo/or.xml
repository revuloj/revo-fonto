    <drv mrk="or.0a">
      <kap><tld/>a</kap>
      <snc mrk="or.0a.eloro">
      <dif>
        El <tld/>o:
      </dif>
      <subsnc>
        <ekz>
          <tld/>a ringo, medalo;
        </ekz><ekz>
          <tld/>aj minejoj.
        </ekz>
        <trdgrp lng="de">
          <trd>golden</trd>,
          <trd>g&uuml;lden [arkaika]</trd>
        </trdgrp>
      </subsnc>
      <subsnc>
        <ekz>
          <tld/>a &scirc;losilo &ccirc;iun pordon malfermas
          <fnt><bib>PrV</bib></fnt>;
        </ekz><ekz>
          <tld/>a posteno.
        </ekz>
        <trd lng="de">golden</trd>
      </subsnc>
      <subsnc>
        <ekz>
          Tiu dol&ccirc;a <tld/>a tempo estas frukto de fantazio neniam
          efektivigebla<fnt>Z</fnt>;
        </ekz><ekz>
          tiu utopiulo esperas revenigi la <tld/>an
          tempa&gcirc;on<fnt>Z</fnt>
          <klr>(feli&ccirc;an praan tempa&gcirc;on, je kiu kredis
            antikvuloj)</klr>;
        </ekz><ekz>
          promesi <tld/>ajn montojn
          (<ref tip="vid" cel="sxmir.0i">&scirc;miri</ref>)
          <fnt><bib>PrV</bib></fnt>;
        </ekz><ekz>
          <tld/>a koro;
        </ekz><ekz>
          <tld/>aj versoj de Pitagoro.
        </ekz>
        <trd lng="de">golden</trd>
      </subsnc>
      <trd lng="en">gold</trd>
        <trdgrp lng="fr">
        <trd>d'or</trd>,
        <trd>en or</trd>
      </trdgrp>
      </snc>
      <snc mrk="or.0a.kolore">
        <dif>
          Havanta <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloron</ref> de <tld/>o.
         </dif>
         <ref tip="sub" cel="blond.0a">blonda</ref>
         <trd lng="en">golden</trd>
        <trd lng="fr">dor&eacute;</trd>
      </snc>
      <trd lng="be">&c_z;&c_a;&c_l;&c_a;&c_t;&c_y;</trd>
      <trd lng="bg">&c_z;&c_l;&c_a;&c_t;&c_o;</trd>
      <trdgrp lng="br">
        <trd>alaouret</trd>,
        <trd>aour<klr tip="ind"> (en aour)</klr></trd>
      </trdgrp>
      <trdgrp lng="ca">
        <trd>daurat</trd>,
        <trd>d'or</trd>,
        <trd>auri</trd>
      </trdgrp>
      <trd lng="de">Gold</trd>
      <trd lng="el">&chi;&rho;&ypsilon;&sigma;&alfa;&phi;
      &epsilon_ton;&ny;&jota;&omikron;&sigma_fina;</trd>
      <trd lng="es">de oro</trd>
      <trd lng="hu">arany-</trd>
