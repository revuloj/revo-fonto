    <drv mrk="or.0igi">
      <kap><tld/>igi</kap>
      <snc>
        <dif>
          Fari <tld/>a (a&ubreve; kvaza&ubreve; <tld/>a):
          <ekz>
            <tld/>igi siajn &scirc;para&jcirc;ojn
            <klr>(transformi ilin el mona formo en <tld/>on)</klr>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            la suno <tld/>igas <klr>(<tld/>e briligas)</klr>
            per siaj radioj la fenestrajn vitra&jcirc;ojn.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="ca">
        <trd><ind>fer tornar</ind> daurat</trd>,
        <trd><ind>convertir</ind> en or</trd>
      </trdgrp>
  <trdgrp lng="cs">
