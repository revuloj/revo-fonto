    <drv mrk="or.0a">
      <kap><tld/>a</kap>
      <snc mrk="or.0a.eloro">
      <dif>
        El <tld/>o:
      </dif>
      <subsnc>
        <ekz>
          <tld/>a ringo, medalo;
        </ekz><ekz>
          <tld/>aj minejoj.
        </ekz>
        <trdgrp lng="de">
          <trd>golden</trd>,
          <trd>g&uuml;lden [arkaika]</trd>
        </trdgrp>
      </subsnc>
      <subsnc>
        <ekz>
          <tld/>a &scirc;losilo &ccirc;iun pordon malfermas
          <fnt><bib>PrV</bib></fnt>;
        </ekz><ekz>
          <tld/>a posteno.
        </ekz>
        <trd lng="de">golden</trd>
      </subsnc>
      <subsnc>
        <ekz>
          Tiu dol&ccirc;a <tld/>a tempo estas frukto de fantazio neniam
          efektivigebla<fnt>Z</fnt>;
        </ekz><ekz>
          tiu utopiulo esperas revenigi la <tld/>an
          tempa&gcirc;on<fnt>Z</fnt>
          <klr>(feli&ccirc;an praan tempa&gcirc;on, je kiu kredis 
            antikvuloj)</klr>;
        </ekz><ekz>
          promesi <tld/>ajn montojn
          (<ref tip="vid" cel="sxmir.0i">&scirc;miri</ref>)
          <fnt><bib>PrV</bib></fnt>;
        </ekz><ekz>
          <tld/>a koro;
        </ekz><ekz>
          <tld/>aj versoj de Pitagoro.
        </ekz>
        <trd lng="de">golden</trd>
      </subsnc>
      <trdgrp lng="fr">
        <trd>d'or</trd>,
        <trd>en or</trd>
      </trdgrp>
      </snc>
      <snc mrk="or.0a.kolore">
        <dif>
          Havanta <ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloron</ref> de <tld/>o.
         </dif>
         <ref tip="sub" cel="blond.0a">blonda</ref>
         <trd lng="fr">dor&eacute;</trd>
      </snc>
      <trd lng="be">&c_z;&c_a;&c_l;&c_a;&c_t;&c_y;</trd>  
      <trd lng="bg">&c_z;&c_l;&c_a;&c_t;&c_o;</trd>    
      <trdgrp lng="br">
        <trd>alaouret</trd>,   
        <trd>aour<klr tip="ind"> (en aour)</klr></trd>
      </trdgrp>
      <trdgrp lng="ca">
        <trd>daurat</trd>,
        <trd>d'or</trd>,
        <trd>auri</trd>
      </trdgrp>
      <trd lng="de">Gold</trd>
      <trd lng="el">&chi;&rho;&ypsilon;&sigma;&alfa;&phi;
      &epsilon_ton;&ny;&jota;&omikron;&sigma_fina;</trd>
      <trd lng="es">de oro</trd>
      <trd lng="hu">arany-</trd>
      <trd lng="nl">gouden</trd>
      <trd lng="pl">z&lstroke;oty</trd>
      <trdgrp lng="pt">
        <trd>de oiro</trd>,
        <trd>de ouro<klr> (bras.)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_z;&c_o;&c_l;&c_o;&c_t;&c_o;&c_j;</trd>
      <trd lng="sv">gyllene</trd>
    </drv>

    <drv mrk="or.0i">
      <kap><tld/>i</kap>
      <snc mrk="or.0i.tr">
        <gra><vspec>tr</vspec></gra>
        <dif>
          Kovri per maldika tavolo el <tld/>o:
          <ekz>
            <tld/>ita kadro<fnt>Z</fnt>, braceleto;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            pensoj <tld/>itaj per la lasta rebrilo de l'
            amo<fnt>B</fnt>.
          </ekz>
        </dif>
        <ref tip="sin" cel="or.0umi"><tld/>umi</ref>
        <trdgrp lng="be">
          <trd>&c_z;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>,
          <trd>&c_p;&c_a;&c_z;&c_a;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>
        </trdgrp>     
        <trdgrp lng="br">
          <trd>alaouri&ntilde;</trd>,   
          <trd>aouri&ntilde;</trd>
        </trdgrp>
        <trd lng="ca">daurar</trd>
        <trd lng="de">vergolden</trd>
        <trd lng="fr">dorer</trd>
        <trd lng="hu">aranyoz</trd>
        <trd lng="nl">vergulden</trd>
        <trdgrp lng="pl">
          <trd>z&lstroke;oci&cacute;</trd>,
          <trd>oz&lstroke;aca&cacute;</trd>,
          <trd>poz&lstroke;aca&cacute;</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>,
          <trd>&c_p;&c_o;&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>
        </trdgrp>
        <trd lng="sv">f&ouml;rgyll</trd>
      </snc>
      <snc mrk="or.0i.ntr">
        <gra><vspec>ntr</vspec></gra>
        <dif>
          Impresi kiel <tld/>o, aspekti kiel <tld/>o:
          <ekz>
            la duono de Forumo, ku&scirc;anta senpere sub la kastelaj
            abrupta&jcirc;oj, estis jam dronanta en ombro, dum la
            kolonoj de la pli alte starantaj temploj <tld/>is ankora&ubreve;
            en lumo kaj bluo
            <fnt>
              <aut>H. Sienkewicz, tr. Lidia Zamenhof</aut>:
              <vrk>Quo vadis</vrk>, <lok>&Ccirc;ap. 2a</lok>
            </fnt>;
          </ekz><ekz>
            la vivo tiel mirinde belis, la suno tiel <tld/>is kaj
            varmis, la lando tiel fabelece &ccirc;armis <klr>...</klr>
            <fnt>
              <aut>B. Traven, tr. Hans-Georg Kaiser</aut>:
              <vrk>La Mortula &scirc;ipo</vrk>. <lok>&Ccirc;ap. 17a</lok>
            </fnt>.
          </ekz>
        </dif>
        <trdgrp lng="ca">
          <trd>resplendir</trd>,
          <trd>excel&middot;lir</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_je;&c_t;&c_mol;</trd>,
          <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
        </trdgrp>
        <trd lng="pl">z&lstroke;oci&cacute; si&eogonek;</trd>
      </snc>
  <trdgrp lng="cs">
    <trd>pozlatit</trd>,
    <trd>zlatit</trd>
  </trdgrp>
  <trd lng="es">chapar<klr tip="amb"> (en oro)</klr></trd>
  <trd lng="sk">pozl&aacute;ti&tcaron;</trd>
</drv>

    <drv mrk="or.0igi">
      <kap><tld/>igi</kap>
      <snc>
        <dif>
          Fari <tld/>a (a&ubreve; kvaza&ubreve; <tld/>a):
          <ekz>
            <tld/>igi siajn &scirc;para&jcirc;ojn
            <klr>(transformi ilin el mona formo en <tld/>on)</klr>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            la suno <tld/>igas <klr>(<tld/>e briligas)</klr>
            per siaj radioj la fenestrajn vitra&jcirc;ojn.
          </ekz>
        </dif>
      </snc>
  <trdgrp lng="ca">
        <trd><ind>fer tornar</ind> daurat</trd>,
        <trd><ind>convertir</ind> en or</trd>
      </trdgrp>
  <trdgrp lng="cs">
    <trd>pozlatit</trd>,
    <trd>zlatit</trd>
  </trdgrp>
  <trd lng="es">dorar</trd>
  <trd lng="pl">z&lstroke;oci&cacute;</trd>
  <trdgrp lng="ru">
        <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol; &c_v;
          &c_z;&c_o;&c_l;&c_o;&c_t;&c_o;</trd>
      </trdgrp>
  <trd lng="sk">pozl&aacute;ti&tcaron;</trd>
</drv>

    <drv mrk="or.0umi">
      <kap><tld/>umi</kap>
      <snc>
        <dif>
          <ref tip="dif" cel="or.0i.tr"><tld/>i<sncref/></ref>:
          <ekz>
            &ccirc;e proksima stratangulo oni povas vidi la por pend&scirc;ildo
            uzatan &leftquot;<tld lit="O"/>an &Scirc;ipon&rightquot;, belege cizelitan,
            <tld/>umitan vel&scirc;ipon el kupro, objekton de
            entuziasmo por &ccirc;iuj infanoj
	    <fnt>
	      <aut>Jozefo HORV&Aacute;TH</aut>:
	      <vrk>Gyoer a&ubreve; kie ekkokerikis ventokoko</vrk>.
	      <bib>Monato</bib>.
	    </fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            morga&ubreve;, kiam birdoj kantos sur bran&ccirc;oj kaj la matensuno
            <tld/>umos arbopintojn kaj disgutos sennombrajn brilajn
            perlojn sur la akvon, &scirc;i jam estos ferminta la 
            okulojn por &ccirc;iam
            <fnt>
              <aut>Bakin, tr. Vejdo</aut>: <vrk>La Familio</vrk>.
              <lok><url ref="http://www.elerno.net/elibro/familio26.htm">&ccirc;ap. 26a</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="ca">durar <klr>(comunicar un color semblant al de
      l'or)</klr></trd>
  <trdgrp lng="cs">
    <trd>pozlatit aplikac&iacute; zlat&eacute; f&oacute;lie</trd>,
    <trd>zabalit do pozl&aacute;tka</trd>
  </trdgrp>
  <trd lng="pl">poz&lstroke;oci&cacute;</trd>
  <trdgrp lng="ru">
        <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_p;&c_o;&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>
      </trdgrp>
  <trd lng="sk">pozl&aacute;ti&tcaron;</trd>
</drv>

    <drv mrk="or.0umo">
      <kap><tld/>umo</kap>
      <snc mrk="or.0umo.ado">
        <dif>
          La ago <tld/>umi, <tld/>umado
          <ekz>
            Al &ccirc;ia land' la gloron de l' falsaj rev-<tld/>umoj
            <fnt>
              <aut>Mallarm&eacute;, tr. G. Waringhien</aut>:
              <vrk>Pafos, sur via nomo ferminte la volumojn</vrk>.
              <lok>La Nica literatura revuo, 1:4</lok>
            </fnt>.
          </ekz>
        </dif>
        <trd lng="ca">daurador</trd>
      </snc>
      <snc mrk="or.0umo.ajxo">
        <dif>
          Rezulto de la ago <tld/>umi: la <tld/>tavolo a&ubreve; la
          objekto <tld/>umita:
          <ekz>
            karakterizaj pri la renesanca lignogravurado estas la
            pompaj vegeta&jcirc;formaj relief-ornama&jcirc;oj,
            simbolaj bestofiguroj, <tld/>umo k.a.
            <fnt>
              <aut>Evgeni GEORGIEV</aut>: <vrk>Vidinda mona&hcirc;ejo</vrk>.
              <bib>Monato</bib>
            </fnt>;
          </ekz><ekz>
            La epoletoj. La galonoj... La <tld/>umo... &Ccirc;io
            brilanta...
            <fnt>
              <vrk><url ref="http://satesperanto.free.fr/satkulturo/article.php3?id_article=208">Vo&ccirc;oj</url></vrk>
            </fnt>;
          </ekz><ekz>
            inter la binda&jcirc;oj, rimarkindan valoron havas la
            Veneciaj <klr>...</klr> kun <tld/>uma&jcirc;oj kaj
            pentra&jcirc;oj sur ledo, ligno, veluro kaj anka&ubreve; sur
            perlamoto
            <fnt>
              <vrk>Venecio (gvidlibro)</vrk>. <lok>Venecio, 1923</lok>
            </fnt>;
          </ekz><ekz>
            Tiel <tld/>umo, mebloj, gem', koliero
            Adaptis sin al &scirc;ia belo rara,
            Sen noci al &scirc;ia bril' perfekte klara,
            Kaj &scirc;ajnis servi &scirc;in kiel bordero
            <fnt>
              <aut>Ch. Baudelaire</aut>:
              <vrk>La floroj de l' malbono</vrk>.
              <lok>Stafeto</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="ca">dauradura</trd>
  <trd lng="cs">pozl&aacute;tko</trd>
  <trd lng="sk">pozl&aacute;tka</trd>
</drv>

    <drv mrk="or.0ajxo">
      <kap><tld/>a&jcirc;o</kap>
      <snc>
        <dif>
          Verka&jcirc;o el <tld/>o.
        </dif>
      </snc>
      <trd lng="be">&c_z;&c_a;&c_l;&c_a;&c_t;&c_y; &c_v;&c_y;&c_r;&c_a;&c_b;</trd>    
      <trd lng="br">aouraj</trd>
      <trdgrp lng="ca">
        <trd>dauradura</trd>,
        <trd>pe&ccedil;a treballada en or</trd>
      </trdgrp>
      <trd lng="hu">aranyt&aacute;rgy</trd>
      <trd lng="ru">&c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je; &c_i;&c_z; <ind>&c_z;&c_o;&c_l;&c_o;&c_t;&c_a;</ind></trd>
    </drv>

    <drv mrk="or.0ajxisto">
      <kap><tld/>a&jcirc;isto</kap>
      <snc>
        <dif>
          Tiu, kiu profesie laboras super <tld/>a&jcirc;oj.
        </dif>
      </snc>
      <trd lng="be">&c_ju;&c_v;&c_e;&c_l;&c_ib;&c_r; <klr tip="amb">(&c_p;&c_a;
