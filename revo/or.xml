  <trdgrp lng="ru">
        <trd>&c_z;&c_o;&c_l;&c_o;&c_t;&c_i;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_b;&c_r;&c_a;&c_shch;&c_a;&c_t;&c_mol; &c_v;
