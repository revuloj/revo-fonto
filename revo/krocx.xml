<drv mrk="krocx.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="krocx.al0i.alfiksi">
    <dif>
      Alfiksi, aligi al:
      <ekz>
        <nom>Clemency</nom> <klr>[&#x2026;]</klr> sin al<tld/>is al la mantelo de sinjoro <nom>Warden</nom>
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la Eternulo al<tld/>os al vi la peston
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 28:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sin firme al<tld/>is per amba&ubreve; manoj sur la tegmentoj de du vagonoj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i sin al<tld/>is al tiu konsolanta penso
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la Egipta pastraro firme sin al<tld/>is al sia nova kulto
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estis devigita preni la fini&gcirc;ojn &leftquot;e&rightquot; kaj &leftquot;u&rightquot;, kiuj kiel sendependaj fini&gcirc;oj
        havis jam alian sencon <klr>[kaj do]</klr> al<tld/>i ilin nedisigeble al la
        radiko
        <fnt>
          <bib>LR</bib>
          <lok>Dua Parto &ndash; Gramatiko</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_y;&c_ch;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>anklammern</trd>,
      <trd>anheften</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>unir</trd>
    </trdgrp>
    <trd lng="nl">vasthaken</trd>
    <trd lng="ru">&c_p;&c_r;&c_i;&c_c;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="krocx.al0i.pendigi">
    <dif>
      Pendigi ion de <tld/>ilo:
      <ekz>
        &scirc;i demetis la pleton, al<tld/>is sian mantekon al la se&gcirc;odorso kaj sidi&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2006/008905.php"
              >Negro i&gcirc;is nigrulo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Anka&ubreve; &ccirc;i tiun sencon<sncref ref="krocx.al0i.pendigi"/>
      mi opinias franca&jcirc;o (se temas pri simpla pendigo).
      <aut>Sergio Pokrovskij</aut>
    </rim>
    <trd lng="be">&c_p;&c_a;&c_v;&c_je;&c_s;&c_ib;&c_c;&c_mol; &c_n;&c_a; &c_k;&c_r;&c_u;&c_k;</trd>
    <trd lng="de">aufh&auml;ngen</trd>
    <trdgrp lng="es">
      <trd>colgar</trd>,
      <trd>enganchar</trd>,
      <trd>aferrar</trd>
    </trdgrp>
  </snc>
  <trd lng="en">attach</trd>
  <trd lng="es">enganchar</trd>
  <trd lng="fr">accrocher</trd>
  <trdgrp lng="hu">
    <trd>r&aacute;akaszt</trd>,
    <trd>hozz&aacute;kapcsol</trd>
  </trdgrp>
  <trd lng="nl">ophangen</trd>
  <trdgrp lng="pl">
    <trd>czepia&cacute;</trd>,
    <trd>powiesi&cacute;</trd>,
    <trd>przy&lstroke;&aogonek;cza&cacute;</trd>,
    <trd>przymocowywa&cacute;</trd>,
    <trd>przytwierdza&cacute;</trd>,
    <trd>zahacza&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ad&abreve;uga</trd>,
    <trd>ag&abreve;&tcomma;a&tcomma;i</trd>,
    <trd>ata&scomma;a</trd>,
    <trd>sp&acirc;nzura</trd>
  </trdgrp>
  <trd lng="sv">kroka fast</trd>
  <trdgrp lng="zh">
    <trd>&#x6302;&#x94A9; <pr>gu&agrave;g&omacron;u</pr></trd>,
    <trd>&#x639B;&#x9264; <pr>gu&agrave;g&omacron;u</pr></trd>,
    <trd>&#x6302; <pr>gu&agrave;</pr></trd>,
    <trd>&#x639B; <pr>gu&agrave;</pr></trd>,
    <trd>&#x6263;&#x4F4F; <pr>k&ograve;uzh&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="krocx.al0igxi">
  <kap>al<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      <ref tip="super" cel="ten.al0igxi">Alteni&gcirc;i</ref>:
      <ekz>
        kvarjara knabineto al<tld/>i&gcirc;is al &scirc;iaj manoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li fortike al<tld/>i&gcirc;is al trunketa&jcirc;oj kaj radikoj kaj grimpis supren
        <fnt>
          <bib>Fab2</bib>
          <lok>Sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vipuro <klr>[&#x2026;]</klr> al<tld/>i&gcirc;is al lia mano
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 28:3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Alligi&gcirc;i al:
      <ekz>
        mi al<tld/>i&gcirc;is al Viaj ordonoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 119:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maljunulo per &ccirc;iuj siaj pensoj kaj meditoj al<tld/>i&gcirc;is al sia mono
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi jam al<tld/>i&gcirc;is al mia kandidato kiel lapo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        fatalismo <klr>[&#x2026;]</klr> al<tld/>i&gcirc;as al la pa&scirc;oj <klr>(neniel forlasi ilin)</klr>
        kaj premegas la kapojn de virinoj malri&ccirc;aj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_ch;&c_a;&c_p;&c_ib;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">sich <ind>anklammern</ind></trd>
  <trdgrp lng="en">
    <trd>clutch</trd>,
    <trd><ind>hold</ind> tight</trd>,
    <trd><ind>grasp</ind> tightly</trd>
  </trdgrp>
  <trd lng="es"><ind>aferrar</ind>se</trd>
  <trd lng="fr">s'<ind>accrocher</ind></trd>
  <trdgrp lng="hu">
    <trd>r&aacute;akaszkodik</trd>,
    <trd>hozz&aacute;kapcsol&oacute;dik</trd>
  </trdgrp>
  <trd lng="ja">&#x3057;&#x304C;&#x307F;&#x3064;&#x304F;</trd>
  <trd lng="nl">zich <ind>vasthaken</ind></trd>
  <trdgrp lng="pl">
    <trd>czepia&cacute; si&eogonek;</trd>,
    <trd>przyczepia&cacute; si&eogonek;</trd>,
    <trd>zawisa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ag&abreve;&tcomma;a&tcomma;i-v&abreve;</trd>,
    <trd>lipi&tcomma;i-v&abreve;</trd>,
    <trd>at&acirc;rna&tcomma;i</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_c;&c_je;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trd lng="sv">haka sig fast</trd>
  <trdgrp lng="zh">
    <trd>&#x6349;&#x4F4F; <pr>zhu&omacron;zh&ugrave;</pr></trd>,
    <trd>&#x6432; <pr>w&amacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="krocx.de0imal0i">
  <kap>de<tld/>i, <var><kap>mal<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malfiksi, <ref cel="implik.mal0i">malimpliki</ref>,
      liberigi iun a&ubreve; ion for de io:
      <ekz>
        mal<tld/>i veston de la <tld/>ilo;
      </ekz>
      <ekz>
        mi <tld/>is a&ubreve; mal<tld/>is la kru&ccirc;ojn, portis ilin arde varmegaj <klr>[&#x2026;]</klr>
        trans la ferdekvojon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 28</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vidvino <tld/>i&gcirc;is per nigraj puntoj de sia nigra
        pelerino al &ccirc;iza&jcirc;oj de la tablo, <nom>Pjotr Ivanovi&ccirc;</nom>
        levi&gcirc;is por mal<tld/>i
        <fnt>
        <aut>L. Tolstoj, tr. V. Melnikov</aut>:
          <vrk>La morto de Ivan Ilji&ccirc;</vrk>.
          <lok>Jekaterinburg: Sezonoj, 1997, &ccirc;ap. 1.</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>ante la agrafon de &scirc;ia brakringo
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        timulinetoj, &jcirc;us de<tld/>itaj de la patrina jupo
        <klr>(tute senspertaj)</klr>
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_ch;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_o;&c_t;&c_k;&c_a;&c_ch;&c_a;&c_m;</trd>
  <trd lng="de">vom Haken <ind>l&ouml;sen</ind></trd>
  <trd lng="es">desenganchar</trd>
  <trd lng="fr">d&eacute;crocher</trd>
  <trdgrp lng="hu">
    <trd>leakaszt</trd>,
    <trd>lekapcsol</trd>
  </trdgrp>
  <trd lng="ja">&#x306F;&#x305A;&#x3059;</trd>
  <trdgrp lng="nl">
    <trd>losmaken<klr tip="ind"> (afhaken)</klr></trd>,
    <trd>afhaken</trd>
  </trdgrp>
  <trd lng="pl">odczepia&cacute;</trd>
  <trd lng="ro">deta&scomma;are</trd>
  <trd lng="ru">&c_o;&c_t;&c_c;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_n;&c_ib;&c_m;&c_a;&c_t;&c_i; &c_z; &c_g;&c_a;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_sh;&c_a;&c_l;&c_k;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_ch;&c_ib;&c_p;&c_l;&c_ja;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
