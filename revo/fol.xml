<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fol</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fol.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      En freneze <ref tip="super" cel="gaj.0a">gaja</ref>, petolema humoro:
      <ekz>
        regis etoso bola kaj <tld/>a
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 62a, p. 343a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>fou<klr> (d&eacute;brid&eacute;)</klr></trd>,
    <trd>fou-fou</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_u;&c_d;&c_je;&c_r;&c_zh;&c_n;&c_o; &c_v;&c_je;&c_s;&c_jo;&c_l;&c_y;&c_j;</trd>
</drv>

<drv mrk="fol.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">POE</uzo>
  <snc>
    <dif>
      Malprudente gajadi kaj petoli.
    </dif>
  </snc>
  <trd lng="fr">faire le <ind>fou</ind><klr> (s'amuser)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;<klr tip="ind"> (&c_v;&c_je;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;)</klr></trd>,
    <trd>&c_b;&c_je;&c_z;&c_u;&c_d;&c_je;&c_r;&c_zh;&c_n;&c_o; &c_v;&c_je;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>

