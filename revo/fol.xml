<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fol.xml,v 1.9 2021/07/09 11:24:15 revo Exp $">
<kap>
   <rad>fol</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fol.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frenez.0a">Frenezeta</ref>;
      troige <ref tip="super" cel="gaj.0a">gaja</ref>, 
      <ref tip="sin" cel="petol.0ema">petolema</ref>:
      <ekz>
        ni provu <klr>[&#8230;]</klr>, unuavide, <tld/>an kontra&ubreve;a&jcirc;on
        <fnt>
          <bib>KFl</bib>
        </fnt>;
      </ekz>
      <ekz>
        regis etoso bola kaj <tld/>a
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 62a, p. 343a</lok></fnt>;
      </ekz>
      <ekz>
        anstata&ubreve;igi &#8218;freneza&#8216; per &#8218;<tld/>a&#8216; a&ubreve; &#8218;mal&gcirc;oja&#8216; per &#8218;trista&#8216; ne sufi&ccirc;as por igi
        frazon literatura
        <fnt>
          <bib>BonaLingvo</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ekstra1.0a.iu">ekstravaganca<sncref/></ref>,
       <ref cel="ekzalt.0i">ekzaltita</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>bl&aacute;hov&yacute;</trd>,
    <trd>bl&aacute;zniv&ecaron; &scaron;a&scaron;kovsk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>verr&uuml;ckt</trd>,
    <trd>albern</trd>,
    <trd>&uuml;berm&uuml;tig</trd>,
    <trd>&uuml;berdreht</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>silly</trd>,
    <trd>giggly</trd>
  </trdgrp>
  <trdgrp lng="fr">
     <trd>fou<klr> (d&eacute;brid&eacute;)</klr></trd>,
     <trd>fou-fou</trd>
   </trdgrp>
  <trd lng="hu">bolondos</trd>
  <trd lng="pl">figlarny</trd>
  <trdgrp lng="pt">
    <trd>adoidado</trd>,
    <trd>amalucado</trd>,
    <trd>atoleimado</trd>
  </trdgrp>
  <trd lng="ro">zglobiu</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_u;&c_d;&c_je;&c_r;&c_zh;&c_n;&c_o; &c_v;&c_je;&c_s;&c_jo;&c_l;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>bl&aacute;zniv&yacute;</trd>,
    <trd>zja&scaron;en&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="fol.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      Malprudente gajadi kaj <ref tip="sin" cel="petol.0i">petoli</ref>:
      <ekz>
        mi vin amas &ndash; tial vivas &ndash; tial <tld/>as
        <fnt>
          <bib>LOdE</bib>
          <aut>Grigori Arosev</aut>
          <vrk><url ref="https://tekstaro.com/t?nomo=ondo-de-esperanto">Cindra Soneto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="kaprio.0i">kaprioli</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>flachsen</trd>,
    <trd>herumalbern</trd>
  </trdgrp>
  <trd lng="fr">faire le <ind>fou</ind><klr> (s'amuser)</klr></trd>
  <trd lng="hu">bolondozik</trd>
  <trd lng="pl">figlowa&cacute;</trd>
  <trdgrp lng="ru">
     <trd>&c_b;&c_je;&c_s;&c_i;&c_t;&c_mol;&c_s;&c_ja;<klr tip="ind"> 
(&c_v;&c_je;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;)</klr></trd>,
     <trd>&c_b;&c_je;&c_z;&c_u;&c_d;&c_je;&c_r;&c_zh;&c_n;&c_o; 
&c_v;&c_je;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
