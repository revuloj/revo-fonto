<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <ofc>1</ofc> <ofc>7</ofc>
  <rad>literatur</rad>/o
</kap>
<drv mrk="litera.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tuto de la spiritaj verkoj, de iu kulturo, nacio, epoko a&ubreve; pri iu
      temo:
      <ekz>
        nia &gcirc;isnuna <tld/>o
        <fnt><bib>F</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        beletro estas nur parto de la <tld/>o;
      </ekz>
      <ekz>
        la <tld/>o pri Goethe, pri Faust, pri iu malsano.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="el">&lambda;&omikron;&gamma;&omikron;&tau;&epsilon;&chi;&ny;&jota_ton;&alfa;</trd>
  <trdgrp lng="de">
    <trd>Literatur</trd>,
    <trd>Schrifttum</trd>
  </trdgrp>
  <trd lng="en">literature</trd>
  <trd lng="es">literatura</trd>
  <trd lng="fr">litt&eacute;rature</trd>
  <trd lng="he">&samekh;&pe;&resh;&vav;&tav;</trd>
  <trd lng="hu">irodalom</trd>
  <trd lng="nl">literatuur</trd>
  <trd lng="pt">literatura</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
</drv>

<drv mrk="litera.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="litera.0isto.BELE">
    <dif>
      Fakulo pri literaturo, precipe pri beletro a&ubreve; gazetaro:
      <ekz>
        vi, <tld/>istoj, povas facile rezonadi tiamaniere, sidante super
        libro kaj plumo
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la plej elstaraj <tld/>istoj (verkistoj, kritikistoj kaj multaj
        aliaj) pridebatis la temon
        <fnt>
        <aut>W. H. Simcock</aut>,
        <vrk>Esti homa kaj literaturema</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2003a, numero 7-8a, p. 28a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="fr">
    <trd>homme de lettres</trd>,
    <trd>litt&eacute;rateur</trd>
  </trdgrp>
  <trd lng="hu">irodalm&aacute;r</trd>  
  <trd lng="ru">&c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_o;&c_r;</trd>
</drv>

<drv mrk="litera.bela0o">
  <kap>bela <tld/>o <fnt><bib>VdE</bib>, <lok>artikolo
  beletristiko</lok></fnt></kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="litera.bela0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <ref tip="dif" cel="beletr.0o">beletro</ref>
    <ekz>
      bel<tld/>a<fnt>Z</fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_k;&c_a;&c_ja; &c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="de">
    <trd>sch&ouml;ngeistige Literatur</trd>,
    <trd>Belletristik</trd>
  </trdgrp>
  <trd lng="fr">belles lettres</trd>
  <trd lng="he">&samekh;&pe;&resh;&vav;&tav; &yod;&pe;&he;</trd>
  <trd lng="hu">sz&eacute;pirodalom</trd>
  <trd lng="nl">bellettrie</trd>
  <trd lng="ru">&c_h;&c_u;&c_d;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
</drv>

<drv mrk="litera.griza0o">
  <kap>griza <tld/>o</kap>
  <snc mrk="litera.griza0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Skriba literaturo ne aperanta &ccirc;e grandaj eldonistoj, sed rekte
      farita &ccirc;e asocioj, fakultatoj kaj similaj, kaj sekve malfacile
      trovebla:
      <ekz>
        nia tuta hava&jcirc;o de jenaj kategorioj: libroj, bro&scirc;uroj, &leftquot;griza
        <tld/>o&rightquot;
        <fnt>
        <aut>D. Rodriguez</aut>,
        <vrk>Internacia E-Muzeo, Vieno</vrk>,
        <lok>soc.culture.esperanto, 2002-11-23</lok></fnt>.
      </ekz>
    </dif>
    <rim mrk="litera.griza0o.RIM_ETIM">
      Sian nomon griza <tld/>o &scirc;uldas al la iamaj ofte grizaj kovriloj de
      amatore faritaj bro&scirc;uroj.
    </rim>
  </snc>
  <trd lng="fr">litt&eacute;rature grise</trd>
  <trd lng="hu">sz&uuml;rke <ind>irodalom</ind></trd>  
</drv>
</art>
