<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <ofc>1</ofc> <ofc>7</ofc>
  <rad>literatur</rad>/o
</kap>
<drv mrk="litera.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Tuto de la spiritaj verkoj, koncernantaj la vivon kaj kulturon de iu
      nacio, epoko:
      <ekz>
        la beletro estas nur parto de la <tld/>o;
      </ekz>
      <ekz>
        lingvo jam praktike ekzistanta, havanta jam sufi&ccirc;e
        grandan <tld/>on &ccirc;iam kreskantan<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Tuto de la verkoj, presa&jcirc;oj ktp pri iu temo:
      <ekz>
        la <tld/>o pri
        Goethe, pri Faust, pri iu malsano.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="el">&lambda;&omikron;&gamma;&omikron;&tau;&epsilon;&chi;&ny;&jota_ton;&alfa;</trd>
  <trdgrp lng="de">
     <trd>Literatur</trd>,
     <trd>Schrifttum</trd>
  </trdgrp>
  <trd lng="en">literature</trd>
  <trd lng="es">literatura</trd>
  <trd lng="he">&samekh;&pe;&resh;&vav;&tav;</trd>
  <trd lng="hu">irodalom</trd>
  <trd lng="nl">literatuur</trd>
  <trd lng="pt">literatura</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
</drv>

<drv mrk="litera.bela0o">
  <kap>
    bela <tld/>o
    <fnt><bib>VdE</bib>, <lok>art. Beletristiko</lok></fnt>
  </kap>
  <uzo tip="fak">BELE</uzo>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="beletr.0o">Beletro</ref>:
      <ekz>
        bel<tld/>a<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_k;&c_a;&c_ja; &c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="de">
     <trd>sch&ouml;ngeistige Literatur</trd>,
     <trd>Belletristik</trd>
  </trdgrp>
  <trd lng="he">&samekh;&pe;&resh;&vav;&tav; &yod;&pe;&he;</trd>
  <trd lng="hu">sz&eacute;pirodalom</trd>
  <trd lng="nl">bellettrie</trd>
  <trd lng="ru">&c_h;&c_u;&c_d;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
</drv>
</art>
