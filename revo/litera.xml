<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: litera.xml,v 1.38 2024/01/15 19:02:15 revo Exp $">
<kap>
  <ofc>*</ofc> <ofc>1</ofc> <ofc>7</ofc>
  <rad>literatur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="litera.0o">
  <kap><tld/>o</kap>
  <snc mrk="litera.0o.SCI">
    <dif>
      Tuto da spiritaj verkoj de iu kulturo, nacio, epoko a&ubreve;
      pri iu temo:
      <ekz>
        nia &gcirc;isnuna <tld/>o
        <fnt><bib>F</bib>, <lok>anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        beletro estas nur parto de la <tld/>o;
      </ekz>
      <ekz>
        la <tld/>o pri Goethe, pri Faust, pri iu malsano.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="bg">&c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="cs">literatura</trd>
  <trdgrp lng="de">
    <trd>Literatur</trd>,
    <trd>Schrifttum</trd>
  </trdgrp>
  <trd lng="el">&lambda;&omikron;&gamma;&omikron;&tau;&epsilon;&chi;&ny;&jota_ton;&alfa;</trd>
  <trd lng="en">literature</trd>
  <trd lng="es">literatura</trd>
  <trd lng="fr">litt&eacute;rature</trd>
  <trd lng="he">&samekh;&pe;&resh;&vav;&tav;</trd>
  <trd lng="hu">irodalom</trd>
  <trd lng="it">letteratura</trd>
  <trdgrp lng="ja">
    <trd>&#x6587;&#x5B66;  <pr>&#x3076;&#x3093;&#x304C;&#x304F;</pr></trd>,
    <trd>&#x6587;&#x82B8;  <pr>&#x3076;&#x3093;&#x3052;&#x3044;</pr></trd>,
    <trd>&#x6587;&#x732E;  <pr>&#x3076;&#x3093;&#x3051;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">literatuur</trd>
  <trdgrp lng="pl">
    <trd>literatura</trd>,
    <trd>pi&sacute;miennictwo</trd>
  </trdgrp>
  <trd lng="pt">literatura</trd>
  <trd lng="ro">literatur&abreve;</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>literat&uacute;ra</trd>,
    <trd>slovesnos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_ib;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>,
    <trd>&#x43F;&c_i;&c_s;&#x44C;&#x43C;&c_je;&#x43D;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd>&c_s;&#x43B;&#x43E;&c_v;&c_je;&c_s;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6587;&#x5B66; <pr>w&#xE9;nxu&#xE9;</pr></trd>,
    <trd>&#x6587;&#x5B78; <pr>w&#xE9;nxu&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="litera.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="litera.0isto.BELE">
    <dif>
      Fakulo pri literaturo, precipe pri beletro a&ubreve; gazetaro:
      <ekz>
        vi, <tld/>istoj, povas facile rezonadi tiamaniere, sidante
        super libro kaj plumo
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la plej elstaraj <tld/>istoj (verkistoj, kritikistoj kaj
        multaj
        aliaj) pridebatis la temon
        <fnt>
          <aut>W. H. Simcock</aut>,
          <vrk>Esti homa kaj literaturema</vrk>,
          <bib>Monato</bib>,
          <lok>2003:7-8, p. 28a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_m;&c_malmol;&c_zh; &c_n;&c_a; &c_p;&c_i;&c_s;&c_m;&c_a;</trd>
  <trd lng="cs">liter&aacute;t</trd>
  <trd lng="de">Literat</trd>
  <trd lng="en">man of letters</trd>
  <trdgrp lng="fr">
    <trd>homme de lettres</trd>,
    <trd>litt&eacute;rateur</trd>
  </trdgrp>
  <trd lng="hu">irodalm&aacute;r</trd>
  <trd lng="it">letterato</trd>
  <trdgrp lng="ja">
    <trd>&#x6587;&#x5B66;&#x8005;  <pr>&#x3076;&#x3093;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x4F5C;&#x5BB6;  <pr>&#x3055;&#x3063;&#x304B;</pr></trd>
  </trdgrp>
  <trd lng="nl">letterkundige</trd>
  <trdgrp lng="pl">
    <trd>literat</trd>,
    <trd>pisarz</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>literat</trd>,
    <trd>scriitor</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">spisovate&lcaron;</trd>
  <trd lng="uk">&#x43B;&c_ib;&c_t;&c_je;&c_r;&c_a;&c_t;&#x43E;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x6587;&#x5B66;&#x5BB6; <pr>w&#xE9;nxu&#xE9;ji&#x101;</pr></trd>,
    <trd>&#x6587;&#x5B78;&#x5BB6; <pr>w&#xE9;nxu&#xE9;ji&#x101;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="litera.bela0o">
  <kap>bela <tld/>o <fnt><bib>VdE</bib>, <lok>artikolo
        beletristiko</lok></fnt></kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="litera.bela0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      <ref tip="dif" cel="beletr.0o">Beletro</ref>:
      <ekz>
        &scirc;i laboris kiel eldonredaktoro, unue &ccirc;e belliteratura eldonejo
        <fnt>
          <vrk><url ref="http://www.ipernity.com/doc/esperanto-muzeo-svitavy/43036158">Forpasis
          Magda &Scaron;aturov&aacute; (1929-2016)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_k;&c_a;&c_ja; &c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="de">
    <trd>sch&ouml;ngeistige Literatur</trd>,
    <trd>Belletristik</trd>
  </trdgrp>
  <trd lng="fr">belles lettres</trd>
  <trd lng="he">&samekh;&pe;&resh;&vav;&tav; &yod;&pe;&he;</trd>
  <trd lng="hu">sz&eacute;pirodalom</trd>
  <trdgrp lng="it">
     <trd>lettere<klr> (letteratura)</klr></trd>,
     <trd>belle lettere</trd>
  </trdgrp>
  <trd lng="nl">bellettrie</trd>
  <trdgrp lng="pl">
    <trd>beletrystyka</trd>,
    <trd>literatura pi&eogonek;kna</trd>
  </trdgrp>
  <trd lng="ro">beletristic&abreve;</trd>
  <trd lng="ru">&c_h;&c_u;&c_d;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;
  &c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x865A;&#x6784;&#x5C0F;&#x8BF4; <pr>x&#x16B;g&#xF2;uxi&#x1CE;oshu&#x14D;</pr></trd>,
    <trd>&#x865B;&#x69CB;&#x5C0F;&#x8AAA; <pr>x&#x16B;g&#xF2;uxi&#x1CE;oshu&#x14D;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="litera.griza0o">
  <kap>griza <tld/>o</kap>
  <snc mrk="litera.griza0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <uzo tip="stl">FRAZ</uzo>
    <dif>
      Skriba literaturo ne aperanta &ccirc;e grandaj eldonistoj, sed
      rekte
      farita &ccirc;e asocioj, fakultatoj kaj similaj, kaj sekve
      malfacile
      trovebla:
      <ekz>
        nia tuta hava&jcirc;o de jenaj kategorioj: libroj,
        bro&scirc;uroj, &leftquot;griza <tld/>o&rightquot;
        <fnt>
          <aut>D. Rodriguez</aut>,
          <vrk>Internacia E-Muzeo, Vieno</vrk>,
          <lok>soc.culture.esperanto, 2002-11-23</lok></fnt>.
      </ekz>
    </dif>
    <rim mrk="litera.griza0o.RIM_ETIM">
      Sian nomon griza <tld/>o &scirc;uldas al la iamaj ofte grizaj
      kovriloj de amatore faritaj bro&scirc;uroj.
    </rim>
  </snc>
  <trd lng="de">graue <ind>Literatur</ind></trd>
  <trd lng="fr">litt&eacute;rature grise</trd>
  <trd lng="hu">sz&uuml;rke <ind>irodalom</ind></trd> 
  <trd lng="it">letteratura grigia</trd>
  <trd lng="nl">grijze <ind>literatuur</ind></trd>
  <trd lng="pl">literatura szara</trd>     
  <trd lng="ro">literatura gri</trd>
</drv>

<drv mrk="litera.0scienco">
  <kap><tld/>scienco</kap>
  <snc mrk="litera.0scienco.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Fako de <ref tip="malprt" cel="filolo1.0o">filologio</ref>,
      scienco pri <tld/>o el la vidpunktoj estetika, historia,
      funkcia ktp:
      <ekz>
        poste, jam forgesinte pri intimeco, Declerck meditas &#x2010;
        kio necesas por ke iu verko apartenu al beletro kaj kial
        necesas recenzado kaj <tld/>scienco
        <fnt>
          <aut>V. Melnikov</aut>,
          <vrk>Tamen pensostimula</vrk>,
          <bib>LOdE</bib>, <lok>2006:3 (137)</lok>
        </fnt>;
      </ekz><ekz>
        reveninte al &Ccirc;ernovcy, Rose ekstudis <tld/>sciencon
        kaj filozofion
        <fnt>
          <aut>F.-G. Roessler</aut>,
          <vrk>Vivo en vortoj</vrk>,
          <bib>Monato</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_t;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;&c_z;&c_n;&c_a;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="cs">liter&aacute;rn&iacute; v&ecaron;da</trd>
  <trd lng="de">Literaturwissenschaft</trd>
  <trd lng="it">storia della letteratura moderna</trd>
  <trd lng="nl">literatuurwetenschap</trd>
  <trd lng="pl">literaturoznawstwo</trd>
  <trd lng="ro">&#x219;tiin&#x21B;&abreve; literar&abreve;</trd>
  <trd lng="ru">&c_l;&c_i;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">liter&aacute;rna veda</trd>
  <trd lng="uk">&#x43B;&c_ib;&c_t;&c_je;&c_r;&c_a;&c_t;&c_u;&c_r;&#x43E;&c_z;&#x43D;&c_a;&c_v;&c_s;&c_t;&c_v;&#x43E;</trd>
</drv>

</art>
