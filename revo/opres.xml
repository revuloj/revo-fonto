<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: opres.xml,v 1.9 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>opres</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="opres.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="opres.0i.premi">
    <dif>
      <ref tip="dif" cel="prem.0i.FIG">Premi <sncref/></ref>:
      <ekz>
        sendis min mia patro al mia fratino, vidvino post Henriko, la
        Bavaria duko, kiun <tld/>is Ludoviko, pretekstante esti
        imperiestro
        <fnt>
        <aut>Karolo la kvara, trad. J. Vondrou&scaron;ek</aut>,
        <vrk><url ref="http://mujweb.atlas.cz/kultura/malovec/KARLO.HTM">La
        propra biografio</url></vrk>,
        <lok>1995</lok>
        </fnt>;
      </ekz>
      <ekz>
        julie de 1921 <klr>[Arkadij Gajdar-on]</klr> oni nomumis regimentestro por opresi Ribelon de
        Tambov
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=35355">Arkadij Gajdar</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estas kolosa grando, &gcirc;i <tld/>as imagon
        <fnt>
        <aut>J. I. Perelman</aut>,
        <vrk>Interplanedaj Voja&gcirc;oj</vrk>,
        <lok><url ref="http://kosmonauxtiko.atspace.com/Libroj/Interplanedaj_vojagxoj/06.htm">&ccirc;apitro
        6a</url>
        [vidita je 2006-08-11]</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="prem.sub0i.tirane">subpremi<sncref/></ref>
    <trd lng="de">unterdr&uuml;cken</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_g;&c_n;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_c;&c_ib;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>krut&ecaron; potla&ccaron;it</trd>,
    <trd>su&zcaron;ovat</trd>,
    <trd>tyranizovat</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>accabler</trd>,
    <trd>opprimer</trd>,
    <trd>pers&eacute;cuter</trd>
  </trdgrp>
  <trd lng="nl">onderdrukken<klr tip="ind"> (zeldz.)</klr></trd>
  <trdgrp lng="pl">
    <trd>uciska&cacute;</trd>,
    <trd>represjonowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>merge</trd>,
    <trd>represiune</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kruto utl&aacute;&ccaron;a&tcaron;</trd>,
    <trd>tyranizova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43D;&#x43E;&c_b;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_g;&#x43D;&#x43E;&c_b;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_g;&#x43D;&c_ib;&c_t;&c_i;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x538B;&#x8FEB; <pr>y&#x101;p&#xF2;</pr></trd>,
    <trd>&#x58D3;&#x8FEB; <pr>y&#x101;p&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="opres.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="opres.0o.premo">
    <dif>
      <ref tip="dif" cel="prem.0o.FIG">Premo <sncref/></ref>:
      <ekz>
        tie &scirc;i ne sentis l' <tld/>on spertitan juna&gcirc;e
        <fnt>
        <aut>Paula Gunn Allen, trad. E. Galvin</aut>,
        <vrk><url ref="">Koraka vojo</url></vrk>,
        <lok>[1990?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        87 &jcirc;urnalistoj estis arestitaj vendrede, kiam ili preparis por
        surstrata kunveno kontra&ubreve; la re&gcirc;a <tld/>o al la &jcirc;urnalismo
        <fnt>
        <aut>R. Manandhar</aut>,
        <vrk>Nepalo: 87 &jcirc;urnalistoj arestitaj...</vrk>,
        <lok><url ref="http://raporto.info">Raporto.info</url>,
        2005-09-16</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Unterdr&uuml;ckung</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_c;&c_ib;&c_s;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_g;&c_n;&c_jo;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>oprese</trd>,
    <trd>&uacute;tlak</trd>
  </trdgrp>
  <trd lng="fr">oppression</trd>
  <trd lng="nl">onderdrukking<klr tip="ind"> (zeldz.)</klr></trd>
  <trdgrp lng="pl">
    <trd>opresja</trd>,
    <trd>ucisk</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>opresiune</trd>,
    <trd>constr&acirc;ngere</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>opresia</trd>,
    <trd>&uacute;tlak</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43D;&#x43E;&c_b;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_u;&c_t;&c_i;&c_s;&#x43A;</trd>,
    <trd>&c_g;&#x43D;&c_ib;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x538B;&#x8FEB; <pr>y&#x101;p&#xF2;</pr></trd>,
    <trd>&#x58D3;&#x8FEB; <pr>y&#x101;p&#xF2;</pr></trd>,
    <trd>&#x8FEB;&#x5BB3; <pr>p&#xF2;h&#xE0;i</pr></trd>,
    <trd>&#x538B;&#x529B; <pr>y&#x101;l&#xEC;</pr></trd>,
    <trd>&#x58D3;&#x529B; <pr>y&#x101;l&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>

