<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: triton.xml,v 1.7 2024/05/10 10:43:47 revo Exp $">
    <kap>
      <rad>triton</rad>/o
    </kap>

<drv mrk="triton.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Intervalo de tri grandaj sekundoj, t.e. troa kvarto:
      <ekz>
        akra dissonanta&jcirc;o estas la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=260153">Dissona&jcirc;o</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la efiko de strebotonoj bazi&gcirc;as origine en la malstabileco de la
        <tld/>o konata ekde la mezepoko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=397464">Strebotono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Ne konfuzu kun <ref cel="trit.0o">trito</ref>.
    </rim>
    <trd lng="de">Tritonus</trd>
    <trd lng="fr">triton</trd>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_t;&c_o;&c_n; <klr tip="amb">(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; &c_ib;&c_n;&c_t;&c_e;&c_r;&c_v;&c_a;&c_l;)</klr></trd>
  <trd lng="ja">&#x30C8;&#x30EA;&#x30C8;&#x30F3;</trd>
  <trdgrp lng="pl">
    <trd>tryt  <klr>(izotop wodoru)</klr></trd>,
    <trd>tryton</trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_r;&c_i;&c_t;&#x43E;&#x43D;</trd>
  <trd lng="zh">&#x7630;&#x8788; <pr>lu&#x1D2;yu&#xE1;n</pr></trd>
</drv>
  </art>

