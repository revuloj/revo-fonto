<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  -<rad>id</rad>
</kap>
<subart>
  <dif>
    Sufikso esprimanta &gcirc;eneralan ideon de naskiteco:
  </dif>
  <snc>
    <ekz>
      kio estas hom<tld/>o, ke Vi lin atentas
      <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
      <lok>Psalmaro 144:3</lok>)</fnt>?
    </ekz>
    <ekz>
      princ<tld/>o
      <klr>(filo de princo)</klr>;
    </ekz>
    <ekz>
      <ind>hund<tld/>o</ind>
      <trd lng="ru">&c_shch;&c_je;&c_n;&c_o;&c_k;</trd>
      <klr>(naskito de hundino)</klr>;
    </ekz>
    <ekz>
      <ind>&ccirc;eval<tld/>o</ind>
      <trd lng="ru">&c_zh;&c_je;&c_r;&c_je;&c_b;&c_jo;&c_n;&c_o;&c_k;</trd>
      estas nematura &ccirc;evalo
      <fnt>(<aut>Zamenhof</aut>: <vrk>Fundamento de Esperanto</vrk>,
      <lok>Ekzercaro, &para; 36</lok>)</fnt>;
    </ekz>
    <ekz>
      la filoj, nepoj kaj pranepoj de re&gcirc;o estas re&gcirc;<tld/>oj
      <fnt>(<aut>Zamenhof</aut>: <vrk>Fundamento de Esperanto</vrk>,
      <lok>Ekzercaro, &para; 36</lok>)</fnt>;
    </ekz>
    <ekz>
      la Hebreoj estas Izrael<tld/>oj, &ccirc;ar ili devenas de
      Izraelo
      <fnt>(<aut>Zamenhof</aut>: <vrk>Fundamento de Esperanto</vrk>,
      <lok>Ekzercaro, &para; 36</lok>)</fnt>;
    </ekz>
    <ekz>
      la Napoleon<tld/>oj esperis longe ricevi denove la tronon de
      Franeujo<fnt>Z</fnt>;
    </ekz>
    <ekz>
      brut<tld/>o<fnt>Z</fnt>;
    </ekz>
    <ekz>
      kreska&jcirc;<tld/>o
      <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
      <lok>Psalmaro 80:14</lok>)</fnt>,
    </ekz>
    <ekz>
      <ind>plant<tld/>o</ind>
      <trd lng="de">Spr&ouml;ssling</trd>
      <trd lng="ru">&c_p;&c_o;&c_b;&c_je;&c_g;</trd>.
    </ekz>
  </snc>
</subart>
<subart>
  <dif>
    Memstara samsignifa vortero.
  </dif>
  <drv mrk="id.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        Naskito, infano, filo:
        <ekz>
          geedzoj kun multaj <tld/>oj.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Baby</trd>,
        <trd>Kind</trd>,
        <trd>Spr&ouml;ssling</trd>
      </trdgrp>
    </snc>
    <snc>
      <dif>
        Posteulo; postnepo post multaj generacioj:
        <ekz>
          ni estas &ccirc;iuj la <tld/>oj de Adam.
        </ekz>
      </dif>
      <trd lng="de">Nachkomme</trd>
    </snc>
    <snc>
      <uzo tip="stl">FIG</uzo>
      <dif>
        Dis&ccirc;iplo:
        <ekz>
          la <tld/>oj de Apolono, de Orfeo, de Platono, de Lutero, de sankta
          Bruno.
        </ekz>
      </dif>
    </snc>
    <trd lng="en">young</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_i;&c_t;&c_ja;</trd>,
      <trd>&c_p;&c_o;&c_t;&c_o;&c_m;&c_o;&c_k;</trd>
    </trdgrp>
  </drv>
  <drv mrk="id.0aro">
    <kap><tld/>aro</kap>
    <snc>
      <dif>
        Aro de la homoj devenantaj de sama praavo.
      </dif>
      <trd lng="de">Geschlecht <klr tip="ind">(Nachkommen)</klr></trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_o;&c_t;&c_o;&c_m;&c_k;&c_i;</trd>,
        <trd>&c_p;&c_o;&c_t;&c_o;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
      </trdgrp>
    </snc>
    <snc>
      <dif>
        Posteularo.
      </dif>
      <trd lng="de">Nachkommen</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_o;&c_t;&c_o;&c_m;&c_k;&c_i;</trd>,
        <trd>&c_p;&c_o;&c_t;&c_o;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>
      </trdgrp>
    </snc>
    <snc>
      <dif>
        Dis&ccirc;iplaro.
      </dif>
    </snc>
  </drv>
</subart>
</art>

