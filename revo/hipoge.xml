<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: hipoge.xml,v 1.8 2015/07/04 08:37:35 revo Exp $">
<kap>
  <rad>hipoge</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="hipoge.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKE</uzo>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Subtera <ref tip="super" cel="konstr.0ajxo">konstrua&jcirc;o</ref>
      funkcianta kiel <ref tip="super" cel="tomb.0ejo">tombejo</ref>:
      <ekz>
        la freskoj en la <tld/>oj de princoj de Tang-dinastio (618&ndash;907)
        estas <klr>...</klr> enhavori&ccirc;aj
        <fnt><bib>EncCxi</bib> <lok>&ccirc;ap. &leftquot;Freskoj&rightquot;, p. 203a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">podzemní stavba</trd>
  <trd lng="de">Hypog&auml;um</trd>
  <trd lng="fr">hypog&eacute;e<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>s&iacute;rkamra<klr tip="ind"> (f&oacute;ld alatti)</klr></trd>,
    <trd>s&iacute;rbolt<klr tip="ind"> (f&oacute;ld alatti)</klr></trd>,
    <trd>h&uuml;pogeosz</trd>
  </trdgrp>
  <trd lng="pt">hipogeu</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_z;&c_je;&c_m;&c_n;&c_a;&c_ja; &c_g;&c_r;&c_o;&c_b;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">podzemná stavba</trd>
</drv>
</art>

