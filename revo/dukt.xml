<drv mrk="dukt.ovo0o">
  <kap>ovo<tld/>o</kap>
  <snc mrk="dukt.ovo0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;iu el la du
      <ref tip="super" cel="dukt.0o.ANA"><tld/>oj<sncref/></ref>
      kiuj elkondukas la <ref tip="vid" cel="cxel.ovo0o.FIZL">ovo&ccirc;elojn</ref>
      el la ovario (&ccirc;e inoj de homo kaj de diversaj bestoj):
      <ekz>
        parazitaj vermoj, trematodoj el la familio
        <em>Prosthogonimidae</em>
        afekcias la ovo<tld/>ojn de kokinoj kaj aliaj birdoj kaj ka&ubreve;zas
        tie
        inflamojn
        <fnt>
          <aut>O. A. Fischer</aut>
          <vrk><url ref="">
          Libeloj kiel interaj gastigantoj de parazitaj vermoj
          (trematodoj kaj cestodoj)</url></vrk>
          <lok>Scienca Revuo, 2008:3, p. 141a-156a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_j;&c_c;&c_a;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_m;&c_a;&c_c;&c_ib;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_t;&c_r;&c_u;&c_b;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="de">Eileiter</trd>
  <trd lng="fr">oviducte</trd>
  <trd lng="ja">&#21365;&#31649;  <pr>&#12425;&#12435;&#12363;&#12435;</pr></trd>
  <trd lng="uk">&#1103;&c_j;&c_c;&c_je;&#1087;&c_r;&#1086;&c_v;&c_ib;&c_d;</trd>
  <trd lng="zh">&#36664;&#21365;&#31649; <pr>sh&#363;lu&#462;ngu&#462;n</pr></trd>
</drv>

<drv mrk="dukt.spermo0o">
  <kap>spermo<tld/>o</kap>
  <snc mrk="dukt.spermo0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;iu el la du
      <ref tip="super" cel="dukt.0o.ANA"><tld/>oj<sncref/></ref>
      kiuj kondukas <ref tip="vid" cel="sperm.0o">spermon</ref>
      de epididimo &gcirc;is uretro (&ccirc;e viroj de homo kaj de diversaj bestoj):
      <ekz>
        el la tubetoj a&ubreve; saketoj eli&gcirc;as spermatozooj tra spermo<tld/>o
        <fnt>
          <aut>C. St&oslash;p-Bowitz, B. M&eacute;sz&aacute;ros</aut>
          <vrk><url ref="&STEB;biologio/enkond-fokuso/f874-p08.htm">
          Enkonduko en la Biologion</url></vrk>
          <lok>Fokuso, 1987:4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_m;&c_ja;&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;</trd>
  <trdgrp lng="fr">
    <trd>canal d&eacute;f&eacute;rent</trd>,
    <trd>spermiducte</trd>
  </trdgrp>
  <trd lng="ja">&#31934;&#31649;  <pr>&#12379;&#12356;&#12363;&#12435;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_ib;&#1084;&#8217;&#1103;&c_v;&c_i;&#1085;&#1086;&c_s;&#1085;&c_a; &#1087;&c_r;&#1086;&c_t;&#1086;&#1082;&c_a;</trd>,
    <trd>&c_s;&c_ib;&#1084;&#8217;&#1103;&#1085;&c_i;&c_j; &#1082;&c_a;&#1085;&c_a;&#1083;</trd>
  </trdgrp>
</drv>
</art>

