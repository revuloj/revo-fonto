<drv mrk="dukt.0o">
  <kap><tld/>o</kap>
  <snc mrk="dukt.0o.KOMUNE">
    <dif>
      Iom granda <ref tip="super" cel="tub.0o">tubo</ref>, kiu portadas
      flua&jcirc;on de devena loko al cela loko:
    </dif>
    <subsnc mrk="dukt.0o.ANA">
      <fnt><bib>SPV</bib></fnt>
      <uzo tip="fak">ANA</uzo>
      <uzo tip="fak">FIZL</uzo>
      <dif>
        de organo a&ubreve; al organo:
        <ekz>
          glandoj, kiuj <klr>...</klr> havas nur unu <tld/>on
          <fnt>
          <aut>P. Jelenc</aut>,
          <vrk>intersekseco</vrk>,
          <lok>soc.culture.esperanto, 1994-02-20</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="ca">via</trd>
      <trdgrp lng="de">
        <trd>Leiter</trd>,
        <trd>R&ouml;hre</trd>
      </trdgrp>
      <trdgrp lng="en"><trd>duct</trd>,<trd>tube</trd></trdgrp>
      <trd lng="fa">&a_m;&a_j;&a_r;&a_A;</trd>
      <trd lng="fr">conduit<klr tip="amb"> (tube)</klr></trd>
      <trd lng="hu">vezet&eacute;k <klr tip="amb">(anat&oacute;mia)</klr></trd>
      <trdgrp lng="it">
        <trd>dotto</trd>,
        <trd>canale<klr> (anat.)</klr></trd>,
        <trd>condotto</trd>
      </trdgrp>
      <trd lng="nl">leider<klr tip="ind"> (anatomie)</klr></trd>
      <trd lng="pl">przew&oacute;d</trd>
      <trd lng="pt">duto</trd>
      <trd lng="ru">&c_k;&c_a;&c_n;&c_a;&c_l;</trd>
    </subsnc>
    <subsnc mrk="dukt.0o.TEK">
      <fnt><bib>PIV1</bib></fnt>
      <uzo tip="fak">TEK</uzo>
      <dif>
        de produkta industria loko al konsuma loko:
        <ekz>
          konstrui novan petrolan kaj/a&ubreve; gasan <tld/>on
          Rusio-Belarusio-Polio.
          <fnt>
          <aut>Z. Lapcionak</aut>,
          <vrk>Re: politika geografio</vrk>,
          <lok>soc.culture.esperanto, 2002-07-23</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="ca">
        <trd>tub</trd>,
        <trd>canonada</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Leitung</trd>,
        <trd>Pipeline</trd>,
        <trd>Trasse</trd>
      </trdgrp>
      <trd lng="en">pipeline</trd>
      <trd lng="fa">&a_kh;&a_T; &a_l;&a_w;&a_l;&a_h;</trd>
      <trd lng="fr">conduit<klr tip="amb"> (tube)</klr></trd>
      <trd lng="hu">cs&odblac;vezet&eacute;k</trd>
      <trdgrp lng="it">
        <trd>dotto</trd>,
        <trd>condotta<klr> (tubo)</klr></trd>,
        <trd>condotto</trd>
      </trdgrp>
      <trd lng="nl">leiding<klr tip="ind"> (buis)</klr></trd>
      <trdgrp lng="pl">
        <trd>-ci&aogonek;g<klr> (np. ropoci&aogonek;g)</klr></trd>,
        <trd>kana&lstroke;</trd>
      </trdgrp>
      <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;</trd>
    </subsnc>
    <subsnc mrk="dukt.0o.KOMP">
      <fnt><bib>KompLeks</bib></fnt>
      <uzo tip="fak">KOMP</uzo>
      <dif>
        de procezo al procezo, sen intera dosiero:
        <ekz>
          &leftquot;tubo&rightquot;, fakvorte
          &leftquot;<tld/>o&rightquot;, signifas, ke eligon de unu
          programo oni metas kiel enigon de la sekva programo
          <fnt>
          <aut>M. Minich</aut>,
          <vrk>rekonstrui signojn</vrk>,
          <lok>soc.culture.esperanto, 2004-01-28</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="ca">canal</trd>
      <trd lng="de">Pipe</trd>
      <trd lng="fa">&a_kh;&a_T; &a_l;&a_w;&a_l;&a_h;</trd>
      <trdgrp lng="fr">
        <trd>pipe<klr tip="amb"> (informatique)</klr></trd>,
        <trd>tube<klr tip="amb"> (pipe, informatique)</klr></trd>
      </trdgrp>
      <trd lng="hu">cs&odblac; <klr tip="amb">(programl&aacute;ncol&aacute;sra)</klr></trd>
      <trd lng="it">pipe<klr> (inf.)</klr></trd>
      <trd lng="nl">pipe<klr tip="ind"> (informatica)</klr></trd>
      <trd lng="pl">potok<klr> (informat.)</klr></trd>
      <trd lng="ru">&c_k;&c_a;&c_n;&c_a;&c_l;</trd>
    </subsnc>
    <trd lng="ca">conducte</trd>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>konduktubo</ke>; (en kelkaj okazoj, &#265;efe en medicino) prefere diru <ke>tubeto</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_t;&c_o;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ductus</trd>,
    <trd>kan&aacute;lek</trd>,
    <trd>pr&uring;chod</trd>,
    <trd>trubice</trd>,
    <trd>v&yacute;vod</trd>,
    <trd>v&yacute;vod &zcaron;l&aacute;z se zevn&iacute; sekrec&iacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#31649;  <pr>&#12363;&#12435;</pr></trd>,
    <trd>&#23566;&#31649;  <pr>&#12393;&#12358;&#12363;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kan&aacute;lik</trd>,
    <trd>trubica</trd>,
    <trd>v&yacute;vod</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1082;&c_a;&#1085;&c_a;&#1083;</trd>,
    <trd>&#1087;&c_r;&#1086;&c_h;&c_ib;&c_d;</trd>,
    <trd>&c_v;&#1086;&c_d;&#1086;&#1087;&c_r;&#1086;&c_v;&c_ib;&c_d;</trd>,
    <trd>&#1085;&c_a;&c_f;&c_t;&#1086;&#1087;&c_r;&#1086;&c_v;&c_ib;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#36664;&#27833;&#31649; <pr>sh&#363;y&#243;ugu&#462;n</pr></trd>,
    <trd>&#31649;&#36947; <pr>gu&#462;nd&#224;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dukt.0a">
  <kap><tld/>a</kap>
  <snc mrk="dukt.0a.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <uzo tip="klr">(pri organo)</uzo>
    <dif>
      Eliganta substancojn tra <tld/>o.
    </dif>
    <ref tip="vid" cel="ekzokrin.0a.FIZL">ekzokrina <sncref/></ref>
  </snc>
<trd lng="be">&c_ja;&c_k;&c_ib; &c_m;&c_a;&c_je; <ind>&c_k;&c_a;&c_n;&c_a;&c_l;</ind></trd>
</drv>

<drv mrk="dukt.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="dukt.sen0a.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <uzo tip="klr">(pri organo)</uzo>
    <dif>
      Eliganta substancojn, sen <tld/>o:
      <ekz>
        aliaj <klr>[glandoj]</klr> estas tute sen<tld/>aj kaj ties
        sekrecia&jcirc;oj enfluas rekte en la sangofluon
        <fnt>
          <aut>H. Schicke</aut>,
          <vrk><url ref="http://www.naturkuracado.info/51088195f60b7be04/51088195f60ce1426/index.html">
          Metabolaj kaj endokrinaj malsanoj</url></vrk>,
          <lok>Naturkuracado.info, 2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="endokrin.0a.FIZL">endokrina <sncref/></ref>
  </snc>
  <trd lng="be">&c_ja;&c_k;&c_ib; &c_n;&c_ja; &c_m;&c_a;&c_je; <ind>&c_k;&c_a;&c_n;&c_a;&c_l;&c_a;&c_w;</ind></trd>
  <trd lng="ja">&#20869;&#20998;&#27852;&#12398;  <pr>&#12394;&#12356;&#12406;&#12435;&#12404;&#12388;&#12398;</pr></trd>
</drv>

<drv mrk="dukt.akvo0o">
  <kap>akvo<tld/>o</kap>
  <snc mrk="dukt.akvo0o.TEK">
    <fnt><bib>EBV</bib></fnt>
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="dukt.0o.TEK"><tld lit="D"/>o<sncref/></ref>
      kaj kunaj instala&jcirc;oj, kiuj provizas akvon al urbo a&ubreve; ajna
      loko, kie &gcirc;i estas bezonata:
      <ekz>
        Londono ne havis akvo<tld/>on, tiam
        <fnt><bib>SatirRak</bib>, <lok>la barbaro</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="akvedu.0o">akvedukto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_k;&c_v;&c_e;&c_d;&c_u;&c_k;</trd>,
    <trd>&c_v;&c_a;&c_d;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="ca">aq&uuml;educte</trd>
  <trdgrp lng="cs">
    <trd>akvadukt</trd>,
    <trd>kan&aacute;lek obsahuj&iacute;c&iacute; vodu</trd>,
    <trd>mokovod</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wasserleitung</trd>,
    <trd>Aqu&auml;dukt</trd>
  </trdgrp>
  <trd lng="en">aqueduct</trd>
  <trd lng="es">acueducto</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b;&#8204;&a_b;&a_fatha;&a_r;</trd>,
    <trd>&a_A_madda;&a_b;&a_A;&a_r;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">aqueduc</trd>
  <trd lng="he">&#1488;&#1463;&#1502;&#1463;&#1514; &#1502;&#1463;&#1497;&#1460;&#1501;</trd>
  <trd lng="hu">v&iacute;zvezet&eacute;k<klr tip="ind">
  (aquaeductus)</klr></trd>
  <trd lng="it">acquedotto</trd>
  <trd lng="ja">&#27700;&#36947;&#31649;  <pr>&#12377;&#12356;&#12393;&#12358;&#12363;&#12435;</pr></trd>
  <trd lng="nl">aquaduct</trd>
  <trdgrp lng="pl">
    <trd>wodoci&aogonek;g</trd>,
    <trd>akwedukt<klr> (hist.)</klr></trd>
  </trdgrp>
  <trd lng="pt">aqueduto</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_v;&c_je;&c_d;&c_u;&c_k;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>akvadukt</trd>,
    <trd>vodovod</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&#1086;&c_d;&#1086;&#1087;&c_r;&#1086;&c_v;&c_ib;&c_d;</trd>,
    <trd>&c_v;&#1086;&c_d;&#1086;&c_g;&c_ib;&#1085;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#27700;&#31649; <pr>shu&#464;gu&#462;n</pr></trd>,
    <trd>&#36664;&#27700;&#31649; <pr>sh&#363;shu&#464;gu&#462;n</pr></trd>,
    <trd>&#33258;&#20358;&#27700;&#31649;&#36947;
    <pr>z&#236;l&#225;ishu&#464;gu&#462;nd&#224;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dukt.gas0o">
