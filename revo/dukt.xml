<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dukt.xml,v 1.29 2023/11/04 18:50:52 revo Exp $">
<kap>
  <rad>dukt</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="dukt.0o">
  <kap><tld/>o</kap>
  <snc mrk="dukt.0o.KOMUNE">
    <dif>
      Iom granda <ref tip="super" cel="tub.0o">tubo</ref>, kiu portadas
      flua&jcirc;on de devena loko al cela loko:
    </dif>
    <subsnc mrk="dukt.0o.ANA">
      <fnt><bib>SPV</bib></fnt>
      <uzo tip="fak">ANA</uzo>
      <uzo tip="fak">FIZL</uzo>
      <dif>
        de organo a&ubreve; al organo:
        <ekz>
          glandoj, kiuj <klr>...</klr> havas nur unu <tld/>on
          <fnt>
          <aut>P. Jelenc</aut>,
          <vrk>intersekseco</vrk>,
          <lok>soc.culture.esperanto, 1994-02-20</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="ca">via</trd>
      <trdgrp lng="de">
        <trd>Leiter</trd>,
        <trd>R&ouml;hre</trd>
      </trdgrp>
      <trdgrp lng="en"><trd>duct</trd>,<trd>tube</trd></trdgrp>
      <trd lng="fa">&a_m;&a_j;&a_r;&a_A;</trd>
      <trd lng="fr">conduit<klr tip="amb"> (tube)</klr></trd>
      <trd lng="hu">vezet&eacute;k <klr tip="amb">(anat&oacute;mia)</klr></trd>
      <trdgrp lng="it">
        <trd>dotto</trd>,
        <trd>canale<klr> (anat.)</klr></trd>,
        <trd>condotto</trd>
      </trdgrp>
      <trd lng="nl">leider<klr tip="ind"> (anatomie)</klr></trd>
      <trd lng="pl">przew&oacute;d</trd>
      <trd lng="pt">duto</trd>
      <trd lng="ru">&c_k;&c_a;&c_n;&c_a;&c_l;</trd>
    </subsnc>
    <subsnc mrk="dukt.0o.TEK">
      <fnt><bib>PIV1</bib></fnt>
      <uzo tip="fak">TEK</uzo>
      <dif>
        de produkta industria loko al konsuma loko:
        <ekz>
          konstrui novan petrolan kaj/a&ubreve; gasan <tld/>on
          Rusio-Belarusio-Polio.
          <fnt>
          <aut>Z. Lapcionak</aut>,
          <vrk>Re: politika geografio</vrk>,
          <lok>soc.culture.esperanto, 2002-07-23</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="ca">
        <trd>tub</trd>,
        <trd>canonada</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Leitung</trd>,
        <trd>Pipeline</trd>,
        <trd>Trasse</trd>
      </trdgrp>
      <trd lng="en">pipeline</trd>
      <trd lng="fa">&a_kh;&a_T; &a_l;&a_w;&a_l;&a_h;</trd>
      <trd lng="fr">conduit<klr tip="amb"> (tube)</klr></trd>
      <trd lng="hu">cs&odblac;vezet&eacute;k</trd>
      <trdgrp lng="it">
        <trd>dotto</trd>,
        <trd>condotta<klr> (tubo)</klr></trd>,
        <trd>condotto</trd>
      </trdgrp>
      <trd lng="nl">leiding<klr tip="ind"> (buis)</klr></trd>
      <trdgrp lng="pl">
        <trd>-ci&aogonek;g<klr> (np. ropoci&aogonek;g)</klr></trd>,
        <trd>kana&lstroke;</trd>
      </trdgrp>
      <trd lng="ru">&c_t;&c_r;&c_u;&c_b;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;</trd>
    </subsnc>
    <subsnc mrk="dukt.0o.KOMP">
      <fnt><bib>KompLeks</bib></fnt>
      <uzo tip="fak">KOMP</uzo>
      <dif>
        de procezo al procezo, sen intera dosiero:
        <ekz>
          &leftquot;tubo&rightquot;, fakvorte
          &leftquot;<tld/>o&rightquot;, signifas, ke eligon de unu
          programo oni metas kiel enigon de la sekva programo
          <fnt>
          <aut>M. Minich</aut>,
          <vrk>rekonstrui signojn</vrk>,
          <lok>soc.culture.esperanto, 2004-01-28</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="ca">canal</trd>
      <trd lng="de">Pipe</trd>
      <trd lng="fa">&a_kh;&a_T; &a_l;&a_w;&a_l;&a_h;</trd>
      <trdgrp lng="fr">
        <trd>pipe<klr tip="amb"> (informatique)</klr></trd>,
        <trd>tube<klr tip="amb"> (pipe, informatique)</klr></trd>
      </trdgrp>
      <trd lng="hu">cs&odblac; <klr tip="amb">(programl&aacute;ncol&aacute;sra)</klr></trd>
      <trd lng="it">pipe<klr> (inf.)</klr></trd>
      <trd lng="nl">pipe<klr tip="ind"> (informatica)</klr></trd>
      <trd lng="pl">potok<klr> (informat.)</klr></trd>
      <trd lng="ru">&c_k;&c_a;&c_n;&c_a;&c_l;</trd>
    </subsnc>
    <trd lng="ca">conducte</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_n;&c_a;&c_l;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_t;&c_o;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ductus</trd>,
    <trd>kan&aacute;lek</trd>,
    <trd>pr&uring;chod</trd>,
    <trd>trubice</trd>,
    <trd>v&yacute;vod</trd>,
    <trd>v&yacute;vod &zcaron;l&aacute;z se zevn&iacute; sekrec&iacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7BA1;  <pr>&#x304B;&#x3093;</pr></trd>,
    <trd>&#x5C0E;&#x7BA1;  <pr>&#x3069;&#x3046;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kan&aacute;lik</trd>,
    <trd>trubica</trd>,
    <trd>v&yacute;vod</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43D;&c_a;&#x43B;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_h;&c_ib;&c_d;</trd>,
    <trd>&c_v;&#x43E;&c_d;&#x43E;&#x43F;&c_r;&#x43E;&c_v;&c_ib;&c_d;</trd>,
    <trd>&#x43D;&c_a;&c_f;&c_t;&#x43E;&#x43F;&c_r;&#x43E;&c_v;&c_ib;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8F38;&#x6CB9;&#x7BA1; <pr>sh&#x16B;y&#xF3;ugu&#x1CE;n</pr></trd>,
    <trd>&#x7BA1;&#x9053; <pr>gu&#x1CE;nd&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dukt.0a">
  <kap><tld/>a</kap>
  <snc mrk="dukt.0a.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <uzo tip="klr">(pri organo)</uzo>
    <dif>
      Eliganta substancojn tra <tld/>o.
    </dif>
    <ref tip="vid" cel="ekzokrin.0a.FIZL">ekzokrina <sncref/></ref>
  </snc>
</drv>

<drv mrk="dukt.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="dukt.sen0a.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <uzo tip="klr">(pri organo)</uzo>
    <dif>
      Eliganta substancojn, sen <tld/>o:
      <ekz>
        aliaj <klr>[glandoj]</klr> estas tute sen<tld/>aj kaj ties 
        sekrecia&jcirc;oj enfluas rekte en la sangofluon
        <fnt>
          <aut>H. Schicke</aut>,
          <vrk><url ref="http://www.naturkuracado.info/51088195f60b7be04/51088195f60ce1426/index.html">
          Metabolaj kaj endokrinaj malsanoj</url></vrk>,
          <lok>Naturkuracado.info, 2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="endokrin.0a.FIZL">endokrina <sncref/></ref>
  </snc>
  <trd lng="ja">&#x5185;&#x5206;&#x6CCC;&#x306E;  <pr>&#x306A;&#x3044;&#x3076;&#x3093;&#x3074;&#x3064;&#x306E;</pr></trd>
</drv>

<drv mrk="dukt.akvo0o">
  <kap>akvo<tld/>o</kap>
  <snc mrk="dukt.akvo0o.TEK">
    <fnt><bib>EBV</bib></fnt>
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="dukt.0o.TEK"><tld lit="D"/>o<sncref/></ref>
      kaj kunaj instala&jcirc;oj, kiuj provizas akvon al urbo a&ubreve; ajna
      loko, kie &gcirc;i estas bezonata:
      <ekz>
        Londono ne havis akvo<tld/>on, tiam
        <fnt><bib>SatirRak</bib>, <lok>la barbaro</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="akvedu.0o">akvedukto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_k;&c_v;&c_e;&c_d;&c_u;&c_k;</trd>,
    <trd>&c_v;&c_a;&c_d;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trd lng="ca">aq&uuml;educte</trd>
  <trdgrp lng="cs">
    <trd>akvadukt</trd>,
    <trd>kan&aacute;lek obsahuj&iacute;c&iacute; vodu</trd>,
    <trd>mokovod</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wasserleitung</trd>,
    <trd>Aqu&auml;dukt</trd>
  </trdgrp>
  <trd lng="en">aqueduct</trd>
  <trd lng="es">acueducto</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b;&zwnj;&a_b;&a_fatha;&a_r;</trd>,
    <trd>&a_A_madda;&a_b;&a_A;&a_r;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">aqueduc</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5DE;&#x5B7;&#x5EA; &#x5DE;&#x5B7;&#x5D9;&#x5B4;&#x5DD;</trd>
  <trd lng="hu">v&iacute;zvezet&eacute;k<klr tip="ind">
  (aquaeductus)</klr></trd>
  <trd lng="it">acquedotto</trd>
  <trd lng="ja">&#x6C34;&#x9053;&#x7BA1;  <pr>&#x3059;&#x3044;&#x3069;&#x3046;&#x304B;&#x3093;</pr></trd>
  <trd lng="nl">aquaduct</trd>
  <trdgrp lng="pl">
    <trd>wodoci&aogonek;g</trd>,
    <trd>akwedukt<klr> (hist.)</klr></trd>
  </trdgrp>
  <trd lng="pt">aqueduto</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_k;&c_v;&c_je;&c_d;&c_u;&c_k;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>akvadukt</trd>,
    <trd>vodovod</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&#x43E;&c_d;&#x43E;&#x43F;&c_r;&#x43E;&c_v;&c_ib;&c_d;</trd>,
    <trd>&c_v;&#x43E;&c_d;&#x43E;&c_g;&c_ib;&#x43D;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C34;&#x7BA1; <pr>shu&#x1D0;gu&#x1CE;n</pr></trd>,
    <trd>&#x8F38;&#x6C34;&#x7BA1; <pr>sh&#x16B;shu&#x1D0;gu&#x1CE;n</pr></trd>,
    <trd>&#x81EA;&#x4F86;&#x6C34;&#x7BA1;&#x9053; <pr>z&#xEC;l&#xE1;ishu&#x1D0;gu&#x1CE;nd&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dukt.gas0o">
  <kap>gas<tld/>o</kap>
  <fnt><bib>EBV</bib></fnt>
  <snc mrk="dukt.gas0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="dukt.0o.TEK"><tld lit="D"/>o<sncref/></ref>,
      kiu portas gason, ordinare brulgason.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;</trd>
  <trd lng="ca">gasoducte</trd>
  <trd lng="de">Gaspipeline</trd>
  <trd lng="en">gas <ind>pipeline</ind></trd>
  <trd lng="fa">&a_kh;&a_T; &a_l;&a_w;&a_l;&a_h;&a_hamza_sure; &f_g;&a_A;&a_z;</trd>
  <trd lng="fr">gazoduc</trd>
  <trd lng="he">&#x5E6;&#x5D9;&#x5E0;&#x5D5;&#x5E8; &#x5D2;&#x5D6;</trd>
  <trd lng="hu">g&aacute;zvezet&eacute;k</trd>
  <trd lng="it">gasdotto</trd>
  <trd lng="ja">&#x30AC;&#x30B9;&#x7BA1;  <pr>&#x30AC;&#x30B9;&#x304B;&#x3093;</pr></trd>
  <trd lng="nl">gaspijpleiding</trd>
  <trd lng="pl">gazoci&aogonek;g</trd>
  <trd lng="ru">&c_g;&c_a;&c_z;&c_o;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;</trd>
  <trd lng="zh">&#x8F38;&#x6C23;&#x7BA1;&#x9053; <pr>sh&#x16B;q&#xEC;gu&#x1CE;nd&#xE0;o</pr></trd>
</drv>

<drv mrk="dukt.nafto0o">
  <kap>nafto<tld/>o</kap>
  <fnt><bib>EBV</bib></fnt>
  <snc mrk="dukt.nafto0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="dukt.0o.TEK"><tld lit="D"/>o<sncref/></ref>,
      kiu portas nafton.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_f;&c_t;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;</trd>
  <trd lng="ca">oleoducte</trd>
  <trdgrp lng="cs">
    <trd>produktovod</trd>,
    <trd>ropovod</trd>
  </trdgrp>
  <trd lng="de">&Ouml;lpipeline</trd>
  <trd lng="en">oil <ind>pipeline</ind></trd>
  <trd lng="fa">&a_kh;&a_T; &a_l;&a_w;&a_l;&a_h;&a_hamza_sure; &a_n;&a_f;&a_t;</trd>
  <trd lng="fr">pipe-line</trd>
  <trd lng="he">&#x5E6;&#x5D9;&#x5E0;&#x5D5;&#x5E8; &#x5E0;&#x5E4;&#x5D8;</trd>
  <trd lng="hu">k&odblac;olajvezet&eacute;k</trd>
  <trd lng="it">oleodotto</trd>
  <trd lng="nl">oliepijpleiding</trd>
  <trdgrp lng="pl">
    <trd>naftoci&aogonek;g</trd>,
    <trd>ropoci&aogonek;g</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_f;&c_t;&c_je;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;</trd>
  <trd lng="sk">ropovod</trd>
  <trd lng="uk">&#x43D;&c_a;&c_f;&c_t;&#x43E;&#x43F;&c_r;&#x43E;&c_v;&c_ib;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#x8F38;&#x6CB9;&#x7BA1;&#x9053; <pr>sh&#x16B;y&#xF3;ugu&#x1CE;nd&#xE0;o</pr></trd>,
    <trd>&#x6CB9;&#x7BA1; <pr>y&#xF3;ugu&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dukt.ovo0o">
  <kap>ovo<tld/>o</kap>
  <snc mrk="dukt.ovo0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;iu el la du
      <ref tip="super" cel="dukt.0o.ANA"><tld/>oj<sncref/></ref>
      kiuj elkondukas la <ref tip="vid" cel="cxel.ovo0o.FIZL">ovo&ccirc;elojn</ref>
      el la ovario (&ccirc;e inoj de homo kaj de diversaj bestoj):
      <ekz>
        parazitaj vermoj, trematodoj el la familio
        <em>Prosthogonimidae</em>
        afekcias la ovo<tld/>ojn de kokinoj kaj aliaj birdoj kaj ka&ubreve;zas
        tie
        inflamojn
        <fnt>
          <aut>O. A. Fischer</aut>
          <vrk><url ref="">
          Libeloj kiel interaj gastigantoj de parazitaj vermoj
          (trematodoj kaj cestodoj)</url></vrk>
          <lok>Scienca Revuo, 2008:3, p. 141a-156a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_j;&c_c;&c_a;&c_v;&c_o;&c_d;</trd>,
    <trd>&c_m;&c_a;&c_c;&c_ib;&c_ch;&c_n;&c_a;&c_ja; <ind>&c_t;&c_r;&c_u;&c_b;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="de">Eileiter</trd>
  <trd lng="fr">oviducte</trd>
  <trd lng="ja">&#x5375;&#x7BA1;  <pr>&#x3089;&#x3093;&#x304B;&#x3093;</pr></trd>
  <trd lng="uk">&#x44F;&c_j;&c_c;&c_je;&#x43F;&c_r;&#x43E;&c_v;&c_ib;&c_d;</trd>
  <trd lng="zh">&#x8F38;&#x5375;&#x7BA1; <pr>sh&#x16B;lu&#x1CE;ngu&#x1CE;n</pr></trd>
</drv>

<drv mrk="dukt.spermo0o">
  <kap>spermo<tld/>o</kap>
  <snc mrk="dukt.spermo0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;iu el la du
      <ref tip="super" cel="dukt.0o.ANA"><tld/>oj<sncref/></ref>
      kiuj kondukas <ref tip="vid" cel="sperm.0o">spermon</ref>
      de epididimo &gcirc;is uretro (&ccirc;e viroj de homo kaj de diversaj bestoj):
      <ekz>
        el la tubetoj a&ubreve; saketoj eli&gcirc;as spermatozooj tra spermo<tld/>o
        <fnt>
          <aut>C. St&oslash;p-Bowitz, B. M&eacute;sz&aacute;ros</aut>
          <vrk><url ref="&STEB;biologio/enkond-fokuso/f874-p08.htm">
          Enkonduko en la Biologion</url></vrk>
          <lok>Fokuso, 1987:4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_m;&c_ja;&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;</trd>
  <trdgrp lng="fr">
    <trd>canal d&eacute;f&eacute;rent</trd>,
    <trd>spermiducte</trd>
  </trdgrp>
  <trd lng="ja">&#x7CBE;&#x7BA1;  <pr>&#x305B;&#x3044;&#x304B;&#x3093;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_ib;&#x43C;&#x2019;&#x44F;&c_v;&c_i;&#x43D;&#x43E;&c_s;&#x43D;&c_a; &#x43F;&c_r;&#x43E;&c_t;&#x43E;&#x43A;&c_a;</trd>,
    <trd>&c_s;&c_ib;&#x43C;&#x2019;&#x44F;&#x43D;&c_i;&c_j; &#x43A;&c_a;&#x43D;&c_a;&#x43B;</trd>
  </trdgrp>
</drv>
</art>

