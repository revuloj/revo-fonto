    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="dukt.0o.TEK"><tld lit="D"/>o<sncref/></ref>,
      kiu portas gason, ordinare brulgason.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;</trd>
  <trd lng="ca">gasoducte</trd>
  <trd lng="de">Gaspipeline</trd>
  <trd lng="en">gas <ind>pipeline</ind></trd>
  <trd lng="fa">&a_kh;&a_T; &a_l;&a_w;&a_l;&a_h;&a_hamza_sure; &f_g;&a_A;&a_z;</trd>
