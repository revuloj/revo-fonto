<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hasxisx.xml,v 1.17 2023/11/15 16:45:17 revo Exp $">
<kap>
  <rad>ha&scirc;i&scirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="hasxisx.0o">
  <kap><tld/>o</kap>
  <snc mrk="hasxisx.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="drog.0o.drogacxo">Drogo</ref>
      preparata el la rezino de <ref tip="vid" cel="kanab.0o.BOT">kanabo</ref>;
      ma&ccirc;ata a&ubreve; fumata:
      <ekz>
        dum la du unuaj horoj mi sentis ion kvaza&ubreve; mi estus
        fuminta <tld/>on. Mi estis tre kvieta kaj serena
        <fnt>
          <aut>trad. &Eacute;. Coffinet</aut>,
          <vrk><url ref="http://perso.wanadoo.fr/eric.coffinet/Iboga/lotsof-esp.html">
          La protokolo de Lotsof</url></vrk>,
          <lok>1998-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="marihxuan.0o">mari&hcirc;uano</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_sh;&c_y;&c_sh;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Haschisch</trd>
  </trdgrp>
  <trd lng="en">hashish</trd>
  <trd lng="fa">&a_H;&a_sh;&f_y;&a_sh;</trd>
  <trdgrp lng="fr">
    <trd>hachich</trd>
  </trdgrp>
  <trd lng="it">hashish</trd>
  <trdgrp lng="ja">
    <trd>&#x30CF;&#x30B7;&#x30B7;</trd>,
    <trd>&#x30CF;&#x30C3;&#x30B7;&#x30C3;&#x30B7;</trd>,
    <trd>&#x5927;&#x9EBB;  <pr>&#x305F;&#x3044;&#x307E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>hasj</trd>,
    <trd>hasjiesj</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>haszysz</trd>
  </trdgrp>
  <trd lng="pt">haxixe</trd>
  <trd lng="uk">&c_g;&c_a;&c_sh;&c_i;&c_sh;</trd>
  <trd lng="zh">&#x5927;&#x9EBB; <pr>d&#xE0;m&#xE1;</pr></trd>
</drv>
</art>

