<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nikotian</rad>/o
</kap>
<drv mrk="nikoti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="solana.0oj">solanacoj</ref>
      (<trd lng="la">Nicotiana</trd>), el kies sekigitaj folioj oni
      faras <ref tip="vid" cel="tabak.0o.fumado">tabakon</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_t;&c_u;&c_n;&c_mol;
  <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr>
  </trd>
  <trd lng="de">Tabakpflanze</trd>
  <trd lng="hu">doh&aacute;ny<klr tip="ind">
(n&ouml;v&eacute;ny)</klr></trd>
  <trd lng="nl">tabaksplant</trd>
  <trd lng="pl">tyto&nacute;</trd>
  <trd lng="pt">nicociana</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_k;&c_o;&c_t;&c_i;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_b;&c_a;&c_k;
    <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr>
    </trd>
  </trdgrp>
  <trd lng="tr">t&uuml;t&uuml;n</trd>
</drv>
</art>
