<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nikoti.xml,v 1.23 2024/11/05 21:26:20 revo Exp $">
<kap>
  <rad>nikotian</rad>/o
</kap>
<drv mrk="nikoti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="nikoti.0o.genro">
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="solana.0oj">solanacoj</ref>
      (<trd lng="la">Nicotiana</trd>),
      ordinare unujara <ref tip="lst" cel="herb.0o" lst="voko:solanoparencoj">herbo</ref> a&ubreve; molligna arbusto,
      el kies sekigitaj folioj oni
      faras <ref tip="vid" cel="tabak.0o.fumado">tabakon</ref>:
      <ekz>
        la esplorteamo publikigis siajn pozitivajn rezultojn pri la modifo de <tld/>o
        <fnt>
          <vrk><url ref="http://muzaiko.info/programdetaloj?id=899">Muzaiko, elsendo 47, Interalie</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/8/8e/Patch_of_Tobacco_(Nicotiana_tabacum_)_in_a_field_in_Intercourse%2C_Pennsylvania..jpg/299px-Patch_of_Tobacco_(Nicotiana_tabacum_)_in_a_field_in_Intercourse%2C_Pennsylvania..jpg"
      prm="CC BY-SA 4.0"
      >kampo de <tld/>o
        <fnt>
          <aut>Derek Ramsey, propra foto</aut>
          <lok><url ref="&WCW;/File:Patch_of_Tobacco_(Nicotiana_tabacum_)_in_a_field_in_Intercourse,_Pennsylvania..jpg"
              >https://commons.wikimedia.org/wiki/File:Patch_of_Tobacco_(Nicotiana_tabacum_)_in_a_field_in_Intercourse,_Pennsylvania..jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_t;&c_u;&c_n;&c_mol;
  <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr>
  </trd>
  <trd lng="ca">nicotiana</trd>
  <trd lng="cs">tab&aacute;k <klr>(rostlina)</klr></trd>
  <trd lng="de">Tabakpflanze</trd>
  <trd lng="hu">doh&aacute;ny<klr tip="ind">
(n&ouml;v&eacute;ny)</klr></trd>
  <trd lng="ja">&#12479;&#12496;&#12467;</trd>
  <trd lng="nl">tabaksplant</trd>
  <trd lng="pl">tyto&nacute;</trd>
  <trd lng="pt">nicociana</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_k;&c_o;&c_t;&c_i;&c_a;&c_n;&c_a;</trd>,
    <trd>&c_t;&c_a;&c_b;&c_a;&c_k;
    <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr>
    </trd>
  </trdgrp>
  <trd lng="sk">tabak <klr>(rastlina)</klr></trd>
  <trd lng="tr">t&uuml;t&uuml;n</trd>
  <trd lng="uk">&c_t;&c_ju;&c_t;&c_ju;&c_n;</trd>
  <trdgrp lng="zh">
    <trd>&#28895;&#33609;&#21046;&#21697; <pr>y&amacron;nc&#462;ozh&igrave;p&#464;n</pr></trd>,
    <trd>&#29017;&#33609;&#35069;&#21697; <pr>y&amacron;nc&#462;ozh&igrave;p&#464;n</pr></trd>
  </trdgrp>
</drv>
</art>
