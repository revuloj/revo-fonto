        <bib>SatirRak</bib>,
        <lok>la fluidumo de la &ccirc;iovido</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_o;&c_r;&c_y;&c_n;&c_t;</trd>
