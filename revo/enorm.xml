<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: enorm.xml,v 1.16 2022/04/27 14:38:54 revo Exp $">
<kap>
  <rad>enorm</rad>/a
</kap>
<drv mrk="enorm.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="enorm.0a.ega">
    <dif>
      <ref tip="dif" cel="eg.0a">Ega</ref>:
      <ekz>
        <nom>Jia Zheng</nom> stari&gcirc;is kaj falis sur la genuojn por danki la
        <tld/>an
        indulgon de Lia Majesto
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 106a, volumo 3a, p.
        375a</lok></fnt>;
      </ekz>
      <ekz>
        koncerne planlingvojn jen elefante <tld/>a korpuso
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/006813.html">Sa&gcirc;a kapo duonvorton
          komprenas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en la hejmo regis <tld/>a tumulto
        <fnt>
          <bib>Monato</bib>
          <aut>CZAR</aut>
          <vrk><url ref="&Monato;/2003/008152.html">La fino de Vincent van
          Gogh</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        telefonfirmaoj kiel <nom>NTL</nom> havas <tld/>ajn rimedojn
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="&Steloj;monato2/007132.html">Interreto: Interreto
          senpage</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la elspezoj <tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009387.php">Koridoroj kien?</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
      <ref tip="sin" cel="ordina.ekster0a.lau_grado">eksterordinara</ref>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_l;&c_ib;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>enormn&iacute;</trd>,
    <trd>enormn&iacute;</trd>,
    <trd>nadm&ecaron;rn&yacute;</trd>,
    <trd>nesm&iacute;rn&ecaron; velk&yacute;</trd>,
    <trd>obrovsk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>enorm</trd>,
    <trd>au&szlig;ergew&ouml;hnlich</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>enormous</trd>, <trd>immense</trd></trdgrp>
  <trd lng="fr">&eacute;norme</trd>
  <trd lng="he">&#x5E2;&#x5E0;&#x5E7;&#x5D9;</trd>
  <trd lng="nl">enorm</trd>
  <trdgrp lng="pl">
    <trd>wielki</trd>,
    <trd>ogromny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>enorme</trd>,
    <trd>imenso</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>enormn&yacute;</trd>,
    <trd>nadmern&yacute;</trd>,
    <trd>obrovsk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x435;&#x43B;&#x438;&#x447;&#x435;&#x437;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x432;&#x435;&#x43B;&#x435;&#x442;&#x435;&#x43D;&#x441;&#x44C;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x43D;&#x435;&#x43F;&#x43E;&#x43C;&#x456;&#x440;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="enorm.0e">
  <kap>
    <tld/>e
  </kap>
  <snc mrk="enorm.0e.ege">
    <dif>
      Ege, eksterordinare:
      <ekz>
        tio kostas <tld/>e
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008521.php">Pledo por mondcivitana
          partio</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        foruzado de papero kreskis <tld/>e
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <lok><url ref="&Steloj;monato2/007176.html">&Steloj;monato2/007176.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        iu ajn listo de anglaj sonoj povas esti <tld/>e plilongigita per aldono de la
        vasta nombro da dialektoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Steloj;monato2/007159.html">Flugilhavaj
          vortoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_r;&c_n;&c_a;</trd>
  <trd lng="de">enorm</trd>
</drv>


</art>
