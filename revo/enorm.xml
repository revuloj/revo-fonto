<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: enorm.xml,v 1.20 2023/11/07 20:34:10 revo Exp $">
<kap>
  <rad>enorm</rad>/a
</kap>
<drv mrk="enorm.0a">
  <kap>
    <tld/>a
    <fnt><bib>PIV1</bib></fnt>
  </kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="enorm.0a.ega">
    <dif>
      Supermezure granda; <ref tip="dif" cel="eg.0a">ega</ref>:
      <ekz>
        tiu tuta <tld/>a laboro, kiu postulis precizan planadon, firman organizitecon
        kaj multe da persona sindedi&ccirc;o, povis esti realigita dank&#x2019; al Esperanto
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        koncerne planlingvojn jen elefante <tld/>a korpuso
        <fnt>
  	<aut>Pejno Simono</aut>
  	<vrk><url ref="&Monato;/2000/006813.php">Plansprache und Phraseologie</url></vrk>
  	<bib>Monato</bib>
  	<lok>2000/06, p. 33</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jia Zheng</nom> stari&gcirc;is kaj falis sur la genuojn por danki la
        <tld/>an
        indulgon de Lia Majesto
        <fnt>
          <bib>RugxDom</bib>,
          <lok>&ccirc;apitro 106a, volumo 3a, p. 375a</lok>
          </fnt>;
      </ekz>
      <ekz>
        en la hejmo regis <tld/>a tumulto
        <fnt>
          <bib>Monato</bib>
          <aut>CZAR</aut>
          <vrk><url ref="&Monato;/2003/008152.html">La fino de Vincent van
          Gogh</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        telefonfirmaoj kiel <nom>NTL</nom> havas <tld/>ajn rimedojn
        <fnt>
          <aut>Edmund Grimley Evans</aut>
            <vrk><url ref="&Monato;/2000/007132.html">Interreto senpage</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 23</lok>
          </fnt>;
      </ekz>
      <ekz>
        estas <klr>[&#x2026;]</klr> <tld/>a svarmo de kurieroj tie &ccirc;i
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Fantaziujo en Dan&gcirc;ero</lok>
        </fnt>;
      </ekz>
      <ekz>
        la elspezoj <tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009387.php">Koridoroj kien?</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ordina.ekster0a.lau_grado">eksterordinara</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_l;&c_ib;&c_z;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>enormn&iacute;</trd>,
    <trd>enormn&iacute;</trd>,
    <trd>nadm&ecaron;rn&yacute;</trd>,
    <trd>nesm&iacute;rn&ecaron; velk&yacute;</trd>,
    <trd>obrovsk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>enorm</trd>,
    <trd>au&szlig;ergew&ouml;hnlich</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>enormous</trd>, <trd>immense</trd></trdgrp>
  <trd lng="fr">&eacute;norme</trd>
  <trd lng="he">&ayin;&nun;&qof;&yod;</trd>
  <trd lng="nl">enorm</trd>
  <trdgrp lng="pl">
    <trd>wielki</trd>,
    <trd>ogromny</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>enorme</trd>,
    <trd>imenso</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>enormn&yacute;</trd>,
    <trd>nadmern&yacute;</trd>,
    <trd>obrovsk&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_je;&c_l;&c_i;&c_ch;&c_je;&c_z;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_je;&c_l;&c_je;&c_t;&c_je;&c_n;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_o;&c_m;&c_ib;&c_r;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x975E;&#x5E38;&#x5927; <pr>f&#x113;ich&#xE1;ngd&#xE0;i</pr></trd>,
    <trd>&#x6781;&#x5927; <pr>j&#xED;d&#xE0;</pr></trd>,
    <trd>&#x866F;&#x87E0; <pr>qi&#xFA;p&#xE1;n</pr></trd>,
    <trd>&#x5927;&#x5927; <pr>d&#xE0;d&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="enorm.0e">
  <kap>
    <tld/>e
  </kap>
  <snc mrk="enorm.0e.ege">
    <dif>
      Ege, eksterordinare:
      <ekz>
        tio kostas <tld/>e
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008521.php">Pledo por mondcivitana
          partio</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        foruzado de papero kreskis <tld/>e
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007176.php">Estimataj gelegantoj,</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 3</lok>
          </fnt>;
      </ekz>
      <ekz>
        iu ajn listo de anglaj sonoj povas esti <tld/>e plilongigita per aldono de la
        vasta nombro da dialektoj
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007159.php">Flugilhavaj vortoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 22</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_z;&c_mol;&c_m;&c_je;&c_r;&c_n;&c_a;</trd>
  <trd lng="de">enorm</trd>
</drv>

</art>
