<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>bergamot</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="bergam.0o">
  <kap><tld/>o</kap>
  <snc mrk="bergam.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Frukto de varia&jcirc;o de
      <ref tip="super" cel="orangx.0arbo.BOT">oran&gcirc;arbo</ref>
      (<trd lng="la">Citrus aurantium bergamia</trd>),
      el kies &scirc;elo oni ekstraktas bonodoran oleon.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_r;&c_g;&c_a;&c_m;&c_o;&c_t;</trd>
  <trd lng="ca">bergamota</trd>
  <trd lng="de">Bergamotte</trd>
  <trd lng="en">bergamot</trd>
  <trd lng="es">bergamota</trd>
  <trd lng="fr">bergamote</trd>
  <trd lng="hu">bergamott</trd>
  <trd lng="nl">bergamot</trd>
  <trd lng="pl">bergamota</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_g;&c_a;&c_m;&c_o;&c_t;</trd>
</drv>
<adm>
  Enkonduki ~ujo. (MB)
</adm>
</art>
