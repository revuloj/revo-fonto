<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bergam.xml,v 1.29 2023/10/29 19:11:01 revo Exp $">
<kap>
<rad>bergamot</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="bergam.0o">
  <kap><tld/>o</kap>
  <snc mrk="bergam.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT">Frukto</ref>
      de <ref tip="malprt" cel="bergam.0ujo"><tld/>ujo</ref>,
      el kies &scirc;elo oni ekstraktas bonodoran oleon.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_r;&c_g;&c_a;&c_m;&c_o;&c_t;</trd>
  <trd lng="ca">bergamota</trd>
  <trdgrp lng="cs">
    <trd>Sevillsk&yacute; pomeran&ccaron;</trd>,
    <trd>ho&rcaron;k&yacute; pomeran&ccaron;</trd>,
    <trd>mo&rcaron;sk&yacute; pomeran&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Bergamottefrucht</trd>
  <trd lng="en">bergamot orange</trd>
  <trd lng="es">bergamota</trd>
  <trd lng="fr">bergamote</trd>
  <trd lng="he">&#x5D4;&#x5D3;&#x5E8; &#x5D4;&#x5DC;&#x5D9;&#x5DE;&#x5D5;&#x5E0;&#x5D9;&#x5EA;</trd>
  <trd lng="hu">bergamott</trd>
  <trd lng="ja">&#x30D9;&#x30EB;&#x30AC;&#x30E2;&#x30C3;&#x30C8;</trd>
  <trdgrp lng="nl">
    <trd>bergamot</trd>,
    <trd>maatjespeer</trd>
  </trdgrp>
  <trd lng="pl">bergamota</trd>
  <trd lng="pt">bergamota</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_g;&c_a;&c_m;&c_o;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>bergamota</trd>,
    <trd>druh citr&oacute;novn&iacute;ka</trd>
  </trdgrp>
  <trd lng="sv">bergamott</trd>
  <trd lng="uk">&#x431;&#x435;&#x440;&#x491;&#x430;&#x43C;&#x43E;&#x442;</trd>
</drv>

<drv mrk="bergam.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="bergam.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Varia&jcirc;o de <ref tip="super" cel="citrus.0o">citruso</ref>
      (<trd lng="la">Citrus x aurantium bergamia</trd>):
      <ekz>
        <tld/>ujo <klr>[&#x2026;]</klr>, arbo kun fruktoj malpli grandaj
        ol oran&gcirc;o, pir- a&ubreve; glob-formaj (<tld/>o), kun tre acida,
        verdete flava karno kaj amara, bonodora flava &scirc;elo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=188573">Bergamotujo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/f2/Bergamottepflanze.jpg/360px-Bergamottepflanze.jpg" prm="CC-BY-SA-3.0"><tld/>oj kaj folioj
        <fnt>
          <aut>K reger</aut>
          <lok><url ref="&WCW;/File:Bergamottepflanze.jpg">&WCW;/File:Bergamottepflanze.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="zh">&#x9999;&#x67E0;&#x6AAC; <pr>xi&#x101;ngn&#xED;ngm&#xE9;ng</pr></trd>
</snc>
  <trd lng="cs">bergamot strom</trd>
  <trd lng="de">Bergamotte</trd>
  <trd lng="en">bergamot tree</trd>
  <trdgrp lng="nl">
    <trd>bergamotboom</trd>,
    <trd>maatjesperenboom</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bergamota</trd>,
    <trd>bergamotka</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bergamot</trd>,
    <trd>strom</trd>
  </trdgrp>
</drv>
</art>
