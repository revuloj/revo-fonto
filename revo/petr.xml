<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>* 7</ofc><rad>Petr</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="petr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="petr.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        sinjoro <tld/>o kaj lia edzino tre amas miajn infanojn
        <fnt><bib>F</bib>, <lok>&FE; 18</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o, Anno kaj Elizabeto estas miaj gefratoj
        <fnt><bib>F</bib>, <lok>&FE; 36</lok></fnt>;
      </ekz>
      <ekz>
        kaj mi diras al vi, ke vi estas <tld/>o
        <fnt><bib>NT</bib>, <lok>Mat 16:18</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">Pere</trd>
    <trd lng="de">Peter</trd>
    <trd lng="en">Peter</trd>
    <trd lng="fr">Pierre<klr> (pr&eacute;nom)</klr></trd>
    <trdgrp lng="nl">
      <trd>Pieter</trd>,
      <trd>Peter</trd>
    </trdgrp>
    <trd lng="no">Petter</trd>
    <trd lng="pl">Piotr</trd>
  </snc>
</drv>
</art>

