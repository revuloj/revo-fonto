<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap><ofc>*</ofc>
      <rad>fremd</rad>/a</kap>

    <drv mrk="fremd.0a">
      <kap><ofc>*</ofc><tld/>a</kap>
      <snc mrk="fremd.0a.aliula">
	<dif>
	  Ekstera kaj senrilata al la koncerna persono a&ubreve; afero;
	  malpropra, aliula:
	  <ekz>
	    tiuj fratoj fari&gcirc;is tute <tld/>aj unuj al
	    aliaj<fnt>Z</fnt>;
	  </ekz><ekz>
	    <tld/>a malfeli&ccirc;o instruas
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    diskuto tute <tld/>a je la temo;
	  </ekz><ekz>
	    li estas <tld/>a al nia societo;
	  </ekz><ekz>
	    al homo nenio homa estu <tld/>a;
	  </ekz><ekz>
	    &gcirc;entileco estas al li <tld/>a;
	  </ekz><ekz>
	    ne doloras frapo sur <tld/>a kapo
	    (<ref tip="vid" cel="najbar.0a">najbara</ref>)
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    el <tld/>a po&scirc;o oni pagas facile
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    reciproka <tld/>eco dividas inter si la naturajn
	    filojn de sama lando<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<trd lng="be">&c_ch;&c_u;&c_zh;&c_y;</trd>
	<trdgrp lng="de">
	  <trd>fremd</trd>,
	  <trd>au&szlig;enstehend</trd>,
	  <trd>au&szlig;erhalb</trd>
	</trdgrp>
	<trd lng="nl">vreemd</trd>
	<trdgrp lng="ru">
	  <trd>&c_ch;&c_u;&c_zh;&c_o;&c_j;</trd>,
	  <trd>&c_ch;&c_u;&c_zh;&c_d;&c_y;&c_j;</trd>,
	  <trd>&c_p;&c_o;&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_n;&c_i;&c_j;</trd>
	</trdgrp>
      </snc>

      <snc mrk="fremd.0a.alilanda">
	<dif>
	  Aliloka, alilanda, alietna, eksterlanda:
	  <ekz>
	    la <tld/>aj lingvoj, homoj, landoj, kutimoj.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="ekzot">ekzota</ref>,
	  <ref cel="mar">transmara</ref>.
	</refgrp>
	<trdgrp lng="be">
	  <trd>&c_z;&c_a;&c_m;&c_je;&c_zh;&c_n;&c_y;</trd>,
	  <trd>&c_ib;&c_n;&c_sh;&c_a;&c_z;&c_je;&c_m;&c_n;&c_y;</trd>
	  <trd>&c_ch;&c_u;&c_zh;&c_a;&c_z;&c_je;&c_m;&c_n;&c_y;</trd>
	</trdgrp>
	<trdgrp lng="de">
	  <trd>fremd<klr tip="ind"> (nicht von hier)</klr></trd>,
	  <trd>ausl&auml;ndisch</trd>,
	  <trd>ausw&auml;rtig</trd>
	</trdgrp>
	<trd lng="es">extranjero</trd>
	<trdgrp lng="nl">
	  <trd>vreemd<klr tip="ind"> (niet van hier)</klr></trd>,
	  <trd>buitenlands</trd>
	</trdgrp>
	<trdgrp lng="ru">
	  <trd>&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
	  <trd>&c_ch;&c_u;&c_zh;&c_je;&c_z;&c_je;&c_m;&c_n;&c_y;&c_j;</trd>,
	  <trd>&c_ch;&c_u;&c_zh;&c_je;&c_s;&c_t;&c_r;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
	</trdgrp>
      </snc>
      <adm>
	Stranga angla traduko. <aut>MB</aut>
      </adm>
      <trd lng="en">strange</trd>
      <trd lng="fr">&eacute;tranger<klr tip="ind"> (adj.)</klr></trd>
    </drv>

    <drv mrk="fremd.0ulo">
      <kap><tld/>ulo</kap>
      <snc>
	<dif>
	  Ulo <tld/>a al la koncerna lando, etno, urbo, familio,
	  homgrupo:
	  <ekz>
	    kial do mi trovis favoron en viaj okuloj, ke vi
	    atentas min, kvankam mi estas <tld/>ulino?
	    <fnt><bib>MT</bib>, <lok>&Rut; 2:10</lok></fnt>;
	  </ekz><ekz>
	    ekster esperanta medio esperantisto sentus sin sola, inter
	    <tld/>uloj; male, en esperantista medio li sentus sin inter
	    fratoj, kvaza&ubreve; inter sangofratoj
	    <fnt>
	      <aut>Jorge Camacho</aut>:
	      <vrk>La liturgio de l' foiro (Elementoj por ekzegezo)</vrk>
	    </fnt>;
	  </ekz><ekz>
	    se geedzoj sin batas, <tld/>ulo restu flanke
	    <fnt><bib>PrV</bib></fnt>;
	  </ekz><ekz>
	    Jakob vivis <tld/>ule en la lando Kanaana
	    <fnt><bib>MT</bib>, <lok>&Gen; 37:1</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <ref tip="sin" cel="ekster.0ulo.fremdulo">eksterulo<sncref/></ref>
      <trdgrp lng="be">
        <trd>&c_ch;&c_u;&c_zh;&c_a;&c_k;</trd>,
        <trd>&c_ch;&c_u;&c_zh;&c_y;&c_n;&c_je;&c_c;</trd>,
        <trd>&c_ib;&c_n;&c_sh;&c_a;&c_z;&c_je;&c_m;&c_je;&c_c;</trd>,
        <trd>&c_ch;&c_u;&c_zh;&c_a;&c_z;&c_je;&c_m;&c_je;&c_c;</trd>,
        <trd>&c_z;&c_a;&c_m;&c_je;&c_zh;&c_n;&c_ib;&c_k;</trd>
      </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_ch;&c_u;&c_zh;&c_a;&c_k;</trd>,
	<trd>&c_ch;&c_u;&c_zh;&c_o;&c_j;<klr tip="amb"> (&c_v; &c_z;&c_n;&c_a;&c_ch;. &c_s;&c_u;&c_shch;.)</klr></trd>,
	<trd>&c_p;&c_o;&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_n;&c_i;&c_j;<klr tip="amb"> (&c_v; &c_z;&c_n;&c_a;&c_ch;. &c_s;&c_u;&c_shch;.)</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="fremd.0igxo">
      <kap><tld/>i&gcirc;o</kap>
      <snc mrk="fremd.0igxo.malintimigxo">
	<dif>
	  &Ccirc;eso de amikaj, kamaradaj a&ubreve; intimaj rilatoj:
	  <ekz>
	    pli kaj pli lar&gcirc;i&gcirc;as la <tld/>i&gcirc;o inter la
	    patro kaj liaj infanoj.
	  </ekz><ekz>
	    alia afero estas, ke kiu ajn minoritata movado, e&ccirc; la plej
	    serioza, riskas la dan&gcirc;eron esti rigardata kiel sekteca
	    kun &ccirc;iuj psikologiaj kaj aliaj malavanta&gcirc;oj de tiu
	    pozicio: mal&scirc;ato kaj malestimo, primokoj kaj ridindigoj,
	    kelkfoje e&ccirc; kompleta <tld/>i&gcirc;o kaj ekskomuniki&gcirc;o el la
	    socio
	    <fnt>
	      <aut>Ivo Lapenna</aut>:
	      <vrk>Por pli efika informado</vrk>,
	      <lok>UEA, Rotterdam, 1974</lok>
	    </fnt>.
	  </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_a;&c_d;&c_ch;&c_u;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
      <trd lng="ru">&c_o;&c_t;&c_ch;&c_u;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
    </drv>

  </art>
