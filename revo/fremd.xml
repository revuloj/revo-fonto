<drv mrk="fremd.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="fremd.0a.aliula">
    <dif>
      Ekstera kaj senrilata al la koncerna persono a&ubreve; afero;
      malpropra, aliula:
      <ekz>
        tiuj fratoj fari&gcirc;is tute <tld/>aj unuj al
        aliaj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a malfeli&ccirc;o instruas</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">m&aacute;s k&aacute;r&aacute;n tanul az okos</trd>
      </ekz>
      <ekz>
        diskuto tute <tld/>a je la temo;
      </ekz>
      <ekz>
        li estas <tld/>a al nia societo;
      </ekz>
      <ekz>
        al homo nenio homa estu <tld/>a;
      </ekz>
      <ekz>
        &gcirc;entileco estas al li <tld/>a;
      </ekz>
      <ekz>
        <ind>ne doloras frapo sur <tld/>a kapo</ind>
        (<ref tip="vid" cel="najbar.0a">najbara</ref>)
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">nem az &eacute;n fejelm f&aacute;j, ha a
        m&aacute;s&eacute;t &uuml;tik</trd>
      </ekz>
      <ekz>
        <ind>el <tld/>a po&scirc;o oni pagas facile</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">a kibicnek semmi sem dr&aacute;ga</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_ch;&c_u;&c_zh;&c_y;</trd>
    <trdgrp lng="de">
      <trd>fremd</trd>,
      <trd>au&szlig;enstehend</trd>,
      <trd>au&szlig;erhalb</trd>
    </trdgrp>
    <trd lng="nl">vreemd</trd>
    <trdgrp lng="pt">
      <trd>estranho</trd>,
      <trd>alheio</trd>,
      <trd>estrangeiro</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>obcy</trd>,
      <trd>postronny</trd>,
      <trd>cudzy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_u;&c_zh;&c_o;&c_j;</trd>,
      <trd>&c_ch;&c_u;&c_zh;&c_d;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_o;&c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_n;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fremd.0a.alilanda">
    <dif>
      Aliloka, alilanda, alietna, eksterlanda:
      <ekz>
        la <tld/>aj lingvoj, homoj, landoj, kutimoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekzot">ekzota</ref>,
      <ref cel="mar">transmara</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_m;&c_je;&c_zh;&c_n;&c_y;</trd>,
      <trd>&c_ib;&c_n;&c_sh;&c_a;&c_z;&c_je;&c_m;&c_n;&c_y;</trd>,
      <trd>&c_ch;&c_u;&c_zh;&c_a;&c_z;&c_je;&c_m;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>fremd<klr tip="ind"> (nicht von hier)</klr></trd>,
      <trd>ausl&auml;ndisch</trd>,
      <trd>ausw&auml;rtig</trd>
    </trdgrp>
    <trd lng="es">extranjero</trd>
    <trdgrp lng="nl">
      <trd>vreemd<klr tip="ind"> (niet van hier)</klr></trd>,
      <trd>buitenlands</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>obcy</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>estranho</trd>,
      <trd>alheio</trd>,
      <trd>estrangeiro</trd>,
      <trd>alien&iacute;gena</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_n;&c_y;&c_j;
      </trd>,
      <trd>&c_ch;&c_u;&c_zh;&c_je;&c_z;&c_je;&c_m;&c_n;&c_y;&c_j;</trd>,
      <trd>
      &c_ch;&c_u;&c_zh;&c_je;&c_s;&c_t;&c_r;&c_a;&c_n;&c_n;&c_y;&c_j;
      </trd>
    </trdgrp>
  </snc>
