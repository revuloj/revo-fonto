      <trd>alheio</trd>,
      <trd>estrangeiro</trd>,
      <trd>alien&iacute;gena</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;&c_a;&c_n;&c_n;&c_y;&c_j;
      </trd>,
      <trd>&c_ch;&c_u;&c_zh;&c_je;&c_z;&c_je;&c_m;&c_n;&c_y;&c_j;</trd>,
