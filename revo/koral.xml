<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: koral.xml,v 1.25 2024/02/19 09:24:08 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>koral</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="koral.0o">
  <kap><tld/>o</kap>
  <snc mrk="koral.0o.JUV">
    <dif>
      Kalka arboforma produkta&jcirc;o de iuj <ref cel="knid.0uloj">kniduloj</ref>
      formantaj koloniojn, ru&gcirc;a a&ubreve; blanka, ordinare
      fiksita al submaraj rokoj:
      <ekz>
         la palaco de la re&gcirc;o de la maro <klr>[&#8230;]</klr> estas el <tld/>oj,
         kaj la altaj fenestroj el la plej travidebla sukceno
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj kaj kristalo ne estas atentataj <klr>[&#8230;]</klr>, posedo de sa&gcirc;o estas
        pli valora ol perloj
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 28:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        ilia viza&gcirc;o estis pli ru&gcirc;a ol <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Plorkanto 4:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bu&scirc;o havis koloron de <tld/>oj, la mallumaj okuloj havis brilon junan kaj
        viglan
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la regiono similis al tuta arbaro da blankaj <tld/>oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_l;</trd>
  <trd lng="bg">&c_k;&c_o;&c_r;&c_a;&c_l;</trd>
  <trd lng="bo">&t_ba;&t_yata;&t_shabchu;&t_tsheg;&t_ra;&t_shabchu;&t_tsheg;</trd>
  <trd lng="cs">kor&aacute;l</trd>
  <trd lng="de">Koralle</trd>
  <trd lng="en">coral</trd>
  <trd lng="es">coral</trd>
  <trd lng="fa">&a_m;&a_r;&a_j;&a_A;&a_n;</trd>
  <trd lng="fr">corail</trd>
  <trdgrp lng="hu">
    <trd>korall</trd>,
    <trd>korallgy&ouml;ngy</trd>,
    <trd>kal&aacute;ris</trd>
  </trdgrp>
  <trd lng="ja">&#x30B5;&#x30F3;&#x30B4;</trd>
  <trd lng="nl">koraal<klr tip="ind"> (zn.)</klr></trd>
  <trd lng="pl">koral</trd>
  <trd lng="pt">coral</trd>
  <trd lng="ro">coral</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_a;&c_l;&c_l;</trd>
  <trd lng="sk">koral</trd>
  <trdgrp lng="zh">
    <trd>&#x73CA; <pr>sh&amacron;n</pr></trd>,
    <trd>&#x73CA;&#x745A; <pr>sh&amacron;nh&#xFA;</pr></trd>,
    <trd>&#x745A; <pr>h&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="koral.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="koral.0a.materiala">
    <dif>
      El <tld/>o:
      <ekz>
        &scirc;i demetis sian <tld/>an &ccirc;irka&ubreve;kolon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        fi&scirc;etoj eliras el siaj <tld/>aj hejmoj kaj min rigardas
        <fnt>
          <aut>B. Kostinek</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr22/inter_koraloj.html">
          Inter la koraloj</url></vrk>,
          <lok>Nica Literatura Revuo, 1956-11 &gcirc;is 12 (2/2), p.
          62-64a</lok>
        </fnt>;
      </ekz>
      <ekz>
        la naturo de Nov-Kaledonio proponas ravajn kamparajn pejza&gcirc;ojn, <klr>[&#8230;]</klr>
        <tld/>ajn insulojn
        <fnt>
          <bib>Monato</bib>
          <aut>G&eacute;rard Mansel</aut>
          <vrk><url ref="https://www.monato.be/2014/011366.php">&Ccirc;arma insulo en Pacifiko</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Korallen-</trd>
    <trd lng="hu">korall-</trd>
  </snc>
  <snc mrk="koral.0a.kolora">
    <dif>
      <tld lit="K"/>kolora:
      <ekz>
        <tld/>aj lipoj;
      </ekz>
      <ekz>
        &ccirc;u ne estas pli bone <klr>[&#8230;]</klr> klinadi la bu&scirc;on al &scirc;ia <tld/>a bu&scirc;o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLIX</lok>
        </fnt>?
      </ekz>
    </dif>
    <trd lng="de">korallenrot</trd>
    <trd lng="hu">korallpiros</trd>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_l;&c_a;&c_v;&c_y;</trd>
  <trd lng="en">coral</trd>
  <trd lng="fa">&a_m;&a_r;&a_j;&a_A;&a_n;&f_y;</trd>
  <trd lng="fr">corail</trd>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30F3;&#x30B4;&#x306E;  <pr>&#12373;&#12435;&#12372;&#x306E;</pr></trd>,
    <trd>&#x30B5;&#x30F3;&#x30B4;&#x88FD;&#x306E; 
    <pr>&#x30B5;&#x30F3;&#x30B4;&#x305B;&#12356;&#x306E;</pr></trd>,
    <trd>&#x30B5;&#x30F3;&#x30B4;&#33394;&#x306E;  <pr>&#x30B5;&#x30F3;&#x30B4;&#12356;&#x308D;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">koraal<klr tip="ind"> (bn.)</klr></trd>
  <trd lng="pl">koralowy</trd>
  <trd lng="ro">coraliu</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_a;&c_l;&c_l;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>
</art>
