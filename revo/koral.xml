<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>koral</rad>/o
</kap>
<drv mrk="koral.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Kalka arboforma produkta&jcirc;o de iuj
      <ref tip="vid" cel="polip.0o">polipoj</ref>,
      pli malpli hele ru&gcirc;a, ordinare fiksita al submaraj rokoj,
      kaj el kiu oni faras juvelojn:
      <ekz>
        braceleto el <tld/>o;
      </ekz>
      <ekz>
        la&ccirc;o de <tld/>etoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_a;&c_l;</trd>
  <trd lng="de">Koralle</trd>
  <trd lng="en">coral</trd>
  <trd lng="fa">&a_m;&a_r;&a_j;&a_A;&a_n;</trd>
  <trd lng="fr">corail</trd>
  <trdgrp lng="hu">
    <trd>korall</trd>,
    <trd>korallgy&ouml;ngy</trd>,
    <trd>kal&aacute;ris</trd>    
  </trdgrp>
  <trd lng="pt">coral</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_a;&c_l;&c_l;</trd>
</drv>

<drv mrk="koral.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      El <tld/>o:
      <ekz>
        <tld/>aj insuletoj, juveloj.
      </ekz>
    </dif>
    <trd lng="de">Korallen-</trd>
    <trd lng="hu">korall-</trd>    
  </snc>
  <snc>
    <dif>
      <tld/>kolora:
      <ekz>
        <tld/>a bu&scirc;o, <tld/>aj lipoj.
      </ekz>
    </dif>
    <trd lng="de">korallenrot</trd>
    <trd lng="hu">korallpiros</trd>    
  </snc>
  <trd lng="en">coral</trd>
  <trd lng="fa">&a_m;&a_r;&a_j;&a_A;&a_n;&f_y;</trd>
  <trd lng="fr">corail</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_a;&c_l;&c_l;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>
</art>
