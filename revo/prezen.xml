<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>prezenc</rad>/o
</kap>
<drv mrk="prezen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <dif>
      Verba <ref tip="sub" cel="temp.0o.GRA">tempo</ref>
      montranta la estantecon; as-tempo, nun-tempo<sup>Z</sup>:
      <ekz>
        li rigardas la aferon per okcidentaj okuloj, &ccirc;ar apliko de terminoj kiel <tld/>o
        kaj preterito estas diskutinda, kiel anka&ubreve; la rifuzo de futuro al la lingvo
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="http://www.monato.net/2011/010166.php">Klara kaj amuza ha&scirc;io</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="preter1.0o">preterito</ref>.
      <ref cel="futur.0o">futuro</ref>,
      <ref cel="imperf.0o">imperfekto</ref>,
      <ref cel="perfek.0o">perfekto</ref>,
      <ref cel="pluskv.0o">pluskvamperfekto</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_ja;&c_p;&c_je;&c_r;&c_a;&c_sh;&c_n;&c_ib;
    &c_ch;&c_a;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>pr&eacute;zens</trd>,
    <trd>p&rcaron;&iacute;tomn&yacute; &ccaron;as</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Gegenwart</trd>,
    <trd>Pr&auml;sens</trd>
  </trdgrp>
  <trd lng="en">present tense</trd>
  <trd lng="es">presente</trd>
  <trd lng="fr">pr&eacute;sent<klr tip="ind"> (conjugaison)</klr></trd>
  <trd lng="hu">jelen <ind>id&odblac;</ind></trd>
  <trdgrp lng="nl">
    <trd>praesens</trd>,
    <trd>onvoltooid tegenwoordige <ind>tijd</ind></trd>
  </trdgrp>
  <trd lng="pl">czas tera&zacute;niejszy</trd>
  <trd lng="pt">presente <klr>(tempo verbal)</klr></trd>
  <trd lng="ro">prezen&tcedil;&abreve;</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_je;&c_je; 
&c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
  <trd lng="sk">pr&iacute;tomn&yacute; &ccaron;as</trd>
</drv>
</art>
