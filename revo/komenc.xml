<drv mrk="komenc.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="komenc.0igxi.KOMUNE">
    <dif>
      Fari&gcirc;i <tld/>ata; eni&gcirc;i en sian unuan parton:
      <ekz>
        tuj post la alveno <tld/>i&gcirc;as la disportado de la po&scirc;ta&jcirc;oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        balda&ubreve; <tld/>i&gcirc;os la laboroj de nia kongreso
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en 
          Boulogne sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        matene <tld/>i&gcirc;is tondroj kaj fulmoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 19:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is vespero
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:63</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is denove milito
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 19:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;is la sep jaroj de malsato
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 41:54</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>kio <tld/>i&gcirc;is, tio anka&ubreve; fini&gcirc;os</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">was einen <ind>Anfang</ind> hat, hat auch ein Ende</trd>
      </ekz>
      <ekz>
        <tld/>i&gcirc;is proceso, mono fluas sen &ccirc;eso
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>putrado de fi&scirc;o <tld/>i&gcirc;as de l' kapo</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">der <ind>Fisch</ind> stinkt vom Kopf her</trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
       <ref cel="est.ek0i">ekesti</ref>
    </refgrp>
    <refgrp tip="vid">    
      <ref cel="nask.0igxi">naski&gcirc;i</ref>,
      <ref cel="eksplo.0i">eksplodi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_ch;&c_y;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_a;&c_ch;&c_y;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_s;&c_ch;&c_y;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anfangen</trd>,
    <trd>beginnen</trd>,
  </trdgrp>
  <trdgrp lng="es">
    <trd>comenzar<klr tip="ind"> (intr.)</klr></trd>,
    <trd><ind>empezar</ind><klr tip="ind"> (debutar)</klr></trd>,
    <trd>debutar<klr tip="ind"> (intr.)</klr></trd>,
    <trd><ind>comprometer</ind>se<klr tip="ind"> (debutar)</klr></trd>,
    <trd><ind>inicializar</ind>se</trd>,
    <trd><ind>abrir</ind>se<klr tip="ind"> (debutar)</klr></trd>,
    <trd>partir<klr tip="ind"> (comenzar)</klr></trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commencer<klr tip="ind"> (intr.)</klr></trd>,
    <trd>s'<ind>amorcer</ind><klr tip="ind"> (d&eacute;buter)</klr></trd>,
    <trd>se <ind>commencer</ind></trd>,
    <trd>d&eacute;buter<klr tip="ind"> (intr.)</klr></trd>,
    <trd>se <ind>d&eacute;clencher</ind><klr tip="ind"> 
(d&eacute;buter)</klr></trd>,
    <trd>d&eacute;marrer<klr tip="ind"> (intr., d&eacute;buter)</klr></trd>,
    <trd>s'<ind>engager</ind><klr tip="ind"> (d&eacute;buter)</klr></trd>,
    <trd>s'<ind>initialiser</ind></trd>,
    <trd>s'<ind>ouvrir</ind><klr tip="ind"> (d&eacute;buter)</klr></trd>,
    <trd>partir<klr tip="ind"> (commencer)</klr></trd>,
