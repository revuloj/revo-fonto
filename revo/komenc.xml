  </trdgrp>
  <trd lng="sk">za&ccaron;iato&ccaron;n&iacute;k</trd>
  <trd lng="th">&#xE1C;&#xE39;&#xE49;&#xE17;&#xE35;&#xE48;&#xE40;&#xE1E;&#xE34;&#xE48;&#xE07;&#xE40;&#xE23;&#xE34;&#xE48;&#xE21;&#xE40;&#xE23;&#xE35;&#xE22;&#xE19;</trd>
  <trd lng="uk">&c_p;&c_o;&c_ch;&c_a;&c_t;&c_k;&c_ib;&c_v;&c_je;&c_c;&c_mol;</trd>
