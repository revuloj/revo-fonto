<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: regent.xml,v 1.15 2022/08/25 22:31:45 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>regent</rad>/o
</kap>
<drv mrk="regent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Provizora <ref tip="super" cel="regn.0estro">regnestro</ref>, kiu regas dum neplena&gcirc;o a&ubreve;
      foresto de la
      re&gcirc;o:
      <ekz>
        Filipo de <nom>Orl&eacute;ans</nom>, Duko de <nom>Orl&eacute;ans</nom>, <tld lit="R"/>o de Francio
        <klr>[&#8230;]</klr>
        plenumis la <tld/>econ de Francio ekde la morto de Ludoviko la 14-a (1715)
        &gcirc;is la plena&gcirc;eco de Ludoviko la 15-a
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=126691">Filipo de Orl&eacute;ans</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_g;&c_je;&c_n;&c_t; <klr>(&c_k;&c_ib;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_k;)</klr></trd>
  <trdgrp lng="cs">
    <trd>m&iacute;stodr&zcaron;&iacute;c&iacute;</trd>,
    <trd>regent</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Regent</trd>,
    <trd>Reichsverweser</trd>
  </trdgrp>
  <trd lng="en">regent</trd>
  <trdgrp lng="hu">
    <trd>korm&aacute;nyz&oacute;</trd>,
    <trd>r&eacute;gens</trd>
  </trdgrp>
  <trd lng="nl">regent</trd>
  <trd lng="pl">regent</trd>
  <trd lng="ro">regent</trd>
  <trd lng="ru">&c_r;&c_je;&c_g;&c_je;&c_n;&c_t; <klr tip="amb">(&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_je;&c_l;&c_mol;)</klr></trd>
  <trdgrp lng="sk">
    <trd>miestodr&zcaron;iaci</trd>,
    <trd>regent</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_je;&c_gu;&c_je;&#x43D;&c_t; (&#x43F;&c_r;&c_a;&c_v;&c_i;&c_t;&c_je;&#x43B;&#x44C;)</trd>
</drv>
</art>
