<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: alcion1.xml,v 1.3 2024/08/20 21:27:33 revo Exp $">
  <kap>
    <rad>alcioned</rad>/oj
  </kap>

<drv mrk="alcion1.0oj">
  <kap><tld/>oj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       <ref tip="lst" lst="voko:zoologiaj_familioj" cel="famili.0o.BIO">Familio</ref>
       de birdoj (<trd lng="la">Alcedinidae</trd>),
       al kiu apartenas la <ref tip="prt" cel="alcion.0o">alciono</ref>:
      <ekz>
        la kukabarao &ccirc;asas kiel aliaj <tld/>oj: per gvatado sur ta&ubreve;ga bran&ccirc;o a&ubreve; kablo
        kie &gcirc;i atendas pacience preterpasantan predon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=244316"
              >Ridema kukabarao</url></lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_z;&c_ib;&c_m;&c_a;&c_r;&c_o;&c_d;&c_k;&c_ib; <klr
   tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
   <trd lng="ca">alced&iacute;nids</trd>
   <trd lng="de">Eisv&ouml;gel</trd>
   <trd lng="fr">alc&eacute;dinid&eacute;s</trd>
   <trd lng="hu">j&eacute;gmad&aacute;rf&eacute;l&eacute;k</trd>
   <trd lng="nl">ijsvogels</trd>
   <trd lng="pl">zimorodkowate</trd>
   <trd lng="ru">&c_z;&c_i;&c_m;&c_o;&c_r;&c_o;&c_d;&c_k;&c_i;<klr tip="amb">
   (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

