<drv mrk="felicx.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari iun <tld/>a:
      <ekz>
        lia edzi&gcirc;o, liaj infanoj, liaj sukcesoj
        <ind>tute <tld/>igi</ind>s lin;
        <trd lng="fr">combler<klr tip="ind"> (rendre pleinement
        heureux)</klr></trd>
      </ekz>
      <ekz>
        via amo tiel <tld/>igis min
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne povas <tld/>igi unu el miaj infanecaj amatinoj kaj samtempe ne
        mal<tld/>igi la alian
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi deziras, <klr>[&#8230;]</klr> ke iam &scirc;i <tld/>igu vin, sed pri tio mi tre dubas
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        lasu la mortintojn dormi kaj <tld/>igu la vivantojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>!
      </ekz>
      <ekz>
        bonaj infanoj gepatrojn <tld/>igas, malbonaj ilin entombigas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_sh;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_sh;&c_ch;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_ib;&c_c;&c_mol;</trd>
