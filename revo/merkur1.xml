<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: merkur1.xml,v 1.20 2017/06/08 13:10:16 revo Exp $">
<kap><rad>Merkur</rad>/o <fnt><bib>PIV1</bib></fnt></kap>
<drv mrk="merkur1.0o">
  <kap><tld/>o</kap>
  <snc mrk="merkur1.0o.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
     Romia <ref tip="super" cel="di.0o.politeisma">dio</ref> de komerco.
      Analogo de helena <ref tip="vid" cel="hermes.0o">Hermeso</ref>:
      <ekz>
        <klr>[li]</klr> ekkuris tiel vigle, kvaza&ubreve; li havus &ccirc;e la
        maleoloj la flugiletojn de <tld/>o
        <fnt>
          <aut>H. Sienkiewicz trad. L. Zamenhof</aut>
          <vrk>Quo vadis?</vrk>
          <lok>&ccirc;ap. 21</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">Mercurius</trd>
    <trd lng="pt">Merc&uacute;rio <klr>(mit.)</klr></trd>
  </snc>
  <snc mrk="merkur1.0o.AST">
    <uzo tip="fak">AST</uzo>
    <mlg>&#9791;</mlg>
    <dif>
      La plej proksima al la <ref tip="vid" cel="sun.0o.nia">Suno</ref>
      <ref tip="lst" cel="planed.0o" lst="voko:planedoj">planedo</ref>
      de la <ref tip="malprt" cel="sun.0sistemo">sunsistemo</ref>:
      <ekz>
        <tld/>o estas malgranda mondo, nur iom pli granda ol la Luno
        <fnt>
          <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
          <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
          universo</vrk>.
          <lok>&ccirc;apitro 4a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="fa">
      <trd>&a_t;&f_y;&a_r;</trd>
      <trd>&a_ayn;&a_T;&a_A;&a_r;&a_d;</trd>
    </trdgrp>
    <trd lng="hu">Merc&uacute;r</trd>
    <trd lng="pt">Merc&uacute;rio <klr>(astr.)</klr></trd>
    <trd lng="zh">&#27700;&#26143;<klr> [Shu&#464;x&#299;ng]</klr></trd>
  </snc>
  <trd lng="be">&c_M;&c_e;&c_r;&c_k;&c_u;&c_r;&c_y;&c_j;</trd>
  <trd lng="bg">&c_M;&c_je;&c_r;&c_k;&c_u;&c_r;&c_i;&c_j;</trd>
  <trd lng="cs">Merkur</trd>
  <trd lng="de">Merkur</trd>
  <trd lng="el">&Epsilon;&rho;&my;&eta_ton;&sigma_fina;</trd>
  <trd lng="en">Mercury</trd>
  <trd lng="es">Mercurio</trd>
  <trd lng="fr">Mercure</trd>
  <trd lng="id">Merkurius</trd>
  <trd lng="it">Mercurio</trd>
  <trd lng="nl">Mercurius</trd>
  <trd lng="pl">Merkury</trd>
  <trd lng="pt">Merc&uacute;rio (astr.)</trd>
  <trd lng="ru">&c_M;&c_je;&c_r;&c_k;&c_u;&c_r;&c_i;&c_j;</trd>
  <trd lng="sk">Merk&uacute;r</trd>
</drv>

<drv mrk="merkur1.0a">
  <kap><tld/>a</kap>
  <snc mrk="merkur1.0a.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Rilata al <tld/>o <sncref ref="merkur1.0o.mita"/>.
    </dif>
    <trd lng="pl">merkurego</trd>
    <trd lng="pt">mercuriano <klr>(mit.)</klr></trd>
    <trd lng="ru">&c_m;&c_je;&c_r;&c_k;&c_u;&c_r;&c_i;&c_je;&c_v;</trd>
  </snc>
  <snc mrk="merkur1.0a.planeda">
    <uzo tip="fak">AST</uzo>
    <dif>
      Rilata al <tld/>o <sncref ref="merkur1.0o.AST"/>:
      <ekz>la merkura nokto, tamen, estas aparte malvarma
        <fnt>
          <aut>Vikipedio</aut>
          <vrk><url
          ref="http://eo.wikipedia.org/wiki/Merkuro_(planedo)">Merkuro</url></vrk>
        </fnt>
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>merkurego</trd>,
      <trd>merkuria&nacute;ski</trd>
    </trdgrp>
    <trd lng="pt">mercuriano <klr>(mit.)</klr></trd>
    <trd lng="ru">
    &c_m;&c_je;&c_r;&c_k;&c_u;&c_r;&c_i;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;
    </trd>
  </snc>
  <trd lng="en">Mercurial</trd>
  <trd lng="fr">mercurien</trd>
  <trd lng="nl">van <ind>Mercurius</ind></trd>
</drv>
</art>
