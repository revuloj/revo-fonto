<vortaro>
  <art mrk="$Id: madrepor.xml,v 1.2 2025/10/09 10:35:56 afido Exp $">
    <kap>
      <rad>madrepor</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="madrepor.0o">
  <kap><tld/>o</kap>
  <snc mrk="madrepor.0o.koralo">
    <dif>
      Genro de &scirc;tonecaj <ref tip="super" cel="koral.0o">koralo</ref>j (Madrepora), formanta rifojn
      kaj insulojn en tropikaj regionoj:
      <ekz>
        ni atendis tremantaj, ka&scirc;itaj meze de <tld/>aj rifoj, kiuj &ccirc;irka&ubreve;as la bordon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/c/ca/Zigzag_coral_(Madrepora_oculata).jpg/601px-Zigzag_coral_(Madrepora_oculata).jpg"
      prm="PD"
      ><tld/>o (Madrepora oculata)
        <fnt>
          <aut>NURC/UNCW and NOAA/FGBNMS, NOAA Photo Library</aut>
          <lok><url ref="&WCW;/File:Zigzag_coral_(Madrepora_oculata).jpg"
              >https://commons.wikimedia.org/wiki/File:Zigzag_coral_(Madrepora_oculata).jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Wei&szlig;e Koralle</trd>
    <trd lng="en">Madrepora</trd>
    <trd lng="es">Madrepora</trd>
    <trd lng="fr">Madrepora</trd>
  </snc>
<trd lng="be">&c_m;&c_a;&c_d;&c_r;&c_e;&c_p;&c_o;&c_r;&c_a;</trd>
</drv>

  </art>

