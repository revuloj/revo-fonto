<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kalvari</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kalvar.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Monteto apud Jerusalem, sur kiu Jesuo estis krucumata. 
    </dif>
    <trd lng="de">Kalvarienberg</trd>
    <trd lng="en">Calvary</trd>
    <trd lng="es">Calvario</trd>
    <trdgrp lng="fr">
      <trd>Calvaire</trd>,
      <trd>Mont du <ind>Calvaire</ind></trd>,
      <trd>Golgotha</trd>
    </trdgrp>
    <trd lng="nl">Kalvarieberg</trd>
    <trd lng="ru">&c_G;&c_o;&c_l;&c_g;&c_o;&c_f;&c_a;</trd>
  </snc>
  <snc num="2">
    <uzo tip="fak">KAT</uzo>
    <dif>
      En la pre&gcirc;ejoj, serio da pentra&jcirc;oj a&ubreve; skulpta&jcirc;oj, prezentanta la
      <tld/>an vojon, kiun Jesuo sekvis de la Tribunalo &gcirc;is la <tld/>o,
      kaj kiun la popola pieco dividis la&ubreve; 14 momentoj, nomitaj stacioj.
    </dif>
    <trd lng="de">Kreuzweg</trd>
    <trd lng="en">stations of the cross</trd>
    <trdgrp lng="fr">
      <trd>calvaire</trd>,
      <trd>chemin de croix</trd>
    </trdgrp>
    <trd lng="nl">kruisweg</trd>
    <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_t;&c_n;&c_y;&c_je; &c_m;&c_u;&c_k;&c_i;</trd>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre malfeli&ccirc;a kaj turmenta vivo.  
    </dif>
    <trd lng="de">Leidensweg</trd>
    <trd lng="en">Calvary</trd>
    <trd lng="fr">calvaire</trd>
    <trd lng="nl">lijdensweg</trd>
    <trd lng="ru">&c_t;&c_je;&c_r;&c_n;&c_i;&c_s;&c_t;&c_y;&c_j; &c_p;&c_u;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="es">calvario</trd>
  <trd lng="hu">k&aacute;lv&aacute;ria</trd>
</drv>
<adm>
  Reverkenda.
  <aut>MB</aut>
</adm>
</art>
