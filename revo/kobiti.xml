<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kobitid</rad>/o
</kap>
<drv mrk="kobiti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kobiti.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Cobitis</trd>), e&ubreve;ropa fi&scirc;o nutranta sin per
      ma&ccirc;ado de sablo retenante nutra&jcirc;on.
    </dif>
  </snc>
  <trd lng="de">Steinbei&szlig;er</trd>
</drv>
<drv mrk="kobiti.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="ciprin.0oformaj">ciprinoformaj
      fi&scirc;oj</ref> (<trd lng="la">Cobitidae</trd>), de nesalakvaj fi&scirc;oj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schmerlen</trd>,
    <trd>Grundeln</trd>
  </trdgrp>
</drv>
</art>
