<art mrk="$Id: kobiti.xml,v 1.20 2023/12/03 09:58:51 revo Exp $">
<kap>
  <rad>kobitid</rad>/o
</kap>
<drv mrk="kobiti.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      E&ubreve;ropa <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref> 
      nutranta sin per ma&ccirc;ado de sablo retenante nutra&jcirc;on;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref tip="malprt" cel="kobiti1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cobitis</trd>):
      <ekz>
        la gepatraj birdoj nutras siajn idojn per fi&scirc;etoj, <tld/>oj, ranoj, krustuloj
        kaj aliaj insektoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=48992">Ru&gcirc;a ibiso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/d/d1/Cobitis_sp.2_subsp.4_(Shiga%2C_Japan).jpg/640px-Cobitis_sp.2_subsp.4_(Shiga%2C_Japan).jpg" prm="CC BY-SA 3.0"><tld/>o
        <fnt>
          <aut>Seotaro, propra verko</aut>
          <lok><url ref="&WCW;/File:Cobitis_sp.2_subsp.4_(Shiga,_Japan).jpg">&WCW;/File:Cobitis_sp.2_subsp.4_(Shiga,_Japan).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_u;&c_ju;&c_n;</trd>
  <trd lng="de">Steinbei&szlig;er</trd>
  <trd lng="hu">cs&iacute;k</trd>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30DE;&#x30C9;&#x30B8;&#x30E7;&#x30A6;</trd>,
    <trd>&#x30C9;&#x30B8;&#x30E7;&#x30A6;</trd>
  </trdgrp>
  <trd lng="nl">modderkruiper</trd>
  <trd lng="pl">koza  <klr>(ryba)</klr></trd>     
  <trd lng="ro">zv&acirc;rlug&abreve;</trd>
  <trd lng="ru">&c_v;&c_mol;&c_ju;&c_n;</trd>
  <trd lng="uk">&#x432;&#x2019;&#x44E;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x9C3C; <pr>x&#xED;</pr></trd>,
    <trd>&#x9C0D; <pr>qi&#x16B;</pr></trd>
  </trdgrp>
</drv>

