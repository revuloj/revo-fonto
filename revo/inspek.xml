<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>inspekt</rad>/i
</kap>
<drv mrk="inspek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kontroli la&ubreve; ricevita komisio la funkciadon de io a&ubreve; la agadon de iu:
      <ekz><tld/>i lernejon, &scirc;ipon, armeon, fabrikon;</ekz>
      <ekz><tld/>i laboristojn, oficistojn.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ekzame.0i">ekzameni</ref>,
      <ref cel="kontro.0i">kontroli</ref>,
      <ref cel="revizi.0i">revizii</ref>.
    </refgrp>
  </snc>
  <trd lng="de">inspizieren</trd>
  <trd lng="en">inspect</trd>
  <trd lng="nl">inspecteren</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_s;&c_p;&c_je;&c_k;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_z;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="inspek.0anto0isto">
  <kap><tld/>anto, <tld/>isto<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Oficisto komisiita por <tld/>i.  
    </dif>
  </snc>
  <trd lng="nl">inspecteur</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_s;&c_p;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_z;&c_i;&c_r;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
