<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: liknid.xml,v 1.22 2023/09/01 19:10:59 revo Exp $">
<kap>
  <rad>liknid</rad>/o
</kap>
<drv mrk="liknid.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="kariof1.0oj">kariofilacoj</ref>
      (<trd lng="la">Lychnis</trd>):
      <ekz>
        kvankam la grundo de <nom>Ailsa Craig</nom> estas sala, humida
        kaj ofte 
        malvarma, diversaj specioj de plantoj kreskas sur la insulo:
        ekzemple
        sova&gcirc;a hiacinto, <tld/>o, sileno kaj koleario somere kaj eriko,
        filiko kaj urtiko a&ubreve;tune
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008289.html">Mistera
          insulo Ailsa Craig</url></vrk>
          <bib>Monato</bib>
          <lok>2004/07, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">kakukkszegf&udblac;szeg</trd>
  </snc>
  <bld lok="&WCU;/thumb/2/20/Lychnis_flos-cuculi_sl2.jpg/432px-Lychnis_flos-cuculi_sl2.jpg" prm="CC   BY-SA 3.0"><ind>kukola <tld/>o</ind> (Lychnis flos-cuculi) Rohrwald, A&ubreve;strujo
       <fnt>
         <aut>Stefan.lefnaer, propra verko</aut>
         <lok><url ref="&WCW;/File:Lychnis_flos-cuculi_sl2.jpg">&WCW;/File:Lychnis_flos-cuculi_sl2.jpg</url></lok>
       </fnt>
    <trdgrp lng="hu">
      <trd>r&eacute;ti kakukkszegf&udblac;szeg</trd>,
      <trd>sz&uacute;nyogszegf&udblac;szeg</trd>,
      <trd>sz&uacute;nyogvir&aacute;g</trd>
    </trdgrp>
    <trd lng="la">Lychnis flos-cuculi</trd>
    <trd lng="pt">flor-de-cuco</trd>
  </bld>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_k;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pechnelke</trd>,
    <trd>Lichtnelke</trd>
  </trdgrp>
  <trd lng="en">ragged robin</trd>
  <trd lng="ja">&#x30BB;&#x30F3;&#x30CE;&#x30A6;</trd>
  <trd lng="nl">koekoeksbloem</trd>
  <trd lng="pl">firletka poszarpana</trd>
  <trd lng="pt">l&iacute;quinis</trd>
  <trd lng="ro">floarea-cucului</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_h;&c_n;&c_i;&c_s;</trd>,
    <trd>&c_z;&c_o;&c_r;&c_mol;&c_k;&c_a; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_ib;&c_r;&#x43A;&c_i;</trd>
</drv>
</art>
