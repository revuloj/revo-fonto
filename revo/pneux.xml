<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>pne&ubreve;</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="pneux.0o">
  <kap><tld/>o</kap>
  <snc mrk="pneux.0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Elasta tubo &ccirc;irka&ubreve; veturila <ref tip="malprt"
      cel="rad.0o.TEK">rado</ref>, &gcirc;in &scirc;irmanta, skuojn
mildiganta:
      <ekz>
        bonvolu plenigi la benzinujon, kaj poste kontrolu la <tld/>ojn
        <fnt>
          <aut>Ailanto red.</aut>,
          <vrk><url
          ref="http://www.kafejo.com/humuro/03.htm">&Scirc;ercaro
3a</url></vrk>,
          <lok>2001-10-02</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estis la unua metrosistemo de la mondo kiu kuras entute sur
        <tld/>oj anstata&ubreve; sur &scirc;talaj radoj
        <fnt>
          <vrk><url
          ref="http://www.metrodemontreal.com/languages/esperanto/index.html">La
          Metroo de Montrealo</url></vrk>,
          <lok>[vidita je 2006-09-15]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_n;&c_a;
<klr>(&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_a;&c_g;&c_a;
&c_s;&c_r;&c_o;&c_d;&c_k;&c_a;)</klr></trd>
  <trd lng="bg">&c_g;&c_u;&c_m;&c_a;</trd>
  <trd lng="de">Reifen</trd>
  <trd lng="en">tire</trd>
  <trd lng="fr">pneu</trd>
  <trd lng="hu">gumiabroncs</trd>
  <trd lng="nl">band<klr tip="ind"> (v.auto, fiets...)</klr></trd>
  <trdgrp lng="pt">
    <trd>pneu</trd>,
    <trd>pneum&aacute;tico</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_i;&c_n;&c_a;</trd>
</drv>
</art>

