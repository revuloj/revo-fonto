  <trd lng="hu">tamariskaf&eacute;l&eacute;k</trd>
  <trd lng="nl">tamariskfamilie</trd>
  <trdgrp lng="pt">
   <trd>tamaric&aacute;ceas</trd>
  </trdgrp>
  <trdgrp lng="ru">
 
<trd>&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;&c_shch;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je;
<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_i;&c_s;&c_k;&c_o;&c_v;&c_y;&c_je;<klr
tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_i;&c_k;&c_s;&c_o;&c_v;&c_y;&c_je;<klr
tip="amb"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sv">tamariskv&auml;xter</trd>
</drv>
</art>
