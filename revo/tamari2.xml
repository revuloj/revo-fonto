<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tamarikac</rad>/oj
</kap>
<drv mrk="tamari2.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      malgrandaj arboj kaj arbedoj
      (<trd lng="la">Tamaricaceae</trd>), kun skvamaj folioj eligantaj
      salon, precipe kreskantaj en stepoj kaj dezertoj.
    </dif>
  </snc>
  <trd lng="de">Tamariskengew&auml;chse</trd>
  <trd lng="nl">tamariskfamilie</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;&c_shch;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_i;&c_s;&c_k;&c_o;&c_v;&c_y;&c_je;</trd>,
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_i;&c_k;&c_s;&c_o;&c_v;&c_y;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">tamariskv&auml;xter</trd>
</drv>
</art>
