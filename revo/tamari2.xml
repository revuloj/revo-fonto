<vortaro>
<art mrk="$Id$">
<kap><rad>tamarikac</rad>/oj</kap>
<drv mrk="tamari2.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      malgrandaj arboj kaj arbedoj
      (<trd lng="la">Tamaricaceae</trd>), kun skvamaj folioj eligantaj
      salon, precipe kreskantaj en stepoj kaj dezertoj.
    </dif>
  </snc>
  <trd lng="de">Tamariskengew&auml;chse</trd>
  <trd lng="sv">tamariskv&auml;xter</trd>
</drv>
