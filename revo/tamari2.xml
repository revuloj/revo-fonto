<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tamari2.xml,v 1.14 2018/09/22 10:10:13 revo Exp $">
<kap>
  <rad>tamarikac</rad>/oj
</kap>
<drv mrk="tamari2.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de 
      <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      malgrandaj arboj kaj arbedoj
      (<trd lng="la">Tamaricaceae</trd>), kun skvamaj folioj eligantaj
      salon, precipe kreskantaj en stepoj kaj dezertoj.
      <ekz>
        <tld/>oj <klr>[vivas&#8230;]</klr> e&ccirc; 
        sur salaj grundoj, ili havas malgrandajn, skvamecajn foliojn, alterne
        starantajn <klr>[&#8230;]</klr> ofte <klr>[kun]</klr> saloglandoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=33747">Tamarikacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en"><trd>Tamaricaceae</trd>, <trd>tamarisk
  family</trd></trdgrp>
  <trd lng="be">&c_t;&c_a;&c_m;&c_a;&c_r;&c_y;&c_s;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">tamaric&agrave;cies</trd>
  <trd lng="de">Tamariskengew&auml;chse</trd>
  <trd lng="fr">Tamaricac&eacute;es</trd>
  <trd lng="hu">tamariskaf&eacute;l&eacute;k</trd>
  <trd lng="nl">tamariskfamilie</trd>
  <trd lng="pl">tamaryszkowate</trd>
  <trdgrp lng="pt">
   <trd>tamaric&aacute;ceas</trd>
  </trdgrp>
  <trdgrp lng="ru">
 
<trd>&c_g;&c_r;&c_je;&c_b;&c_je;&c_n;&c_shch;&c_i;&c_k;&c_o;&c_v;&c_y;&c_je;
<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_i;&c_s;&c_k;&c_o;&c_v;&c_y;&c_je;<klr
tip="ind"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_t;&c_a;&c_m;&c_a;&c_r;&c_i;&c_k;&c_s;&c_o;&c_v;&c_y;&c_je;<klr
tip="amb"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sv">tamariskv&auml;xter</trd>
</drv>
</art>
