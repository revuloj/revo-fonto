    <trd>&#x547C;&#x3073;&#x58F0;  <pr>&#x3088;&#x3073;&#x3054;&#x3048;</pr></trd>,
    <trd>&#x62DB;&#x304D;  <pr>&#x307E;&#x306D;&#x304D;</pr></trd>,
    <trd>&#x547C;&#x51FA;&#x97F3;  <pr>&#x3088;&#x3073;&#x3060;&#x3057;&#x304A;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>in&scaron;trukcia pre vyvolanie programu</trd>,
    <trd>volanie</trd>,
    <trd>vyvolanie</trd>
  </trdgrp>
