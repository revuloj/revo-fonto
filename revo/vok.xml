<drv mrk="vok.0o">
<kap><tld/>o</kap>
  <snc mrk="vok.0o.vokado">
    <dif>
      Ago <tld/>i, rezulto de tiu ago:
      <ekz>
        li ne a&ubreve;dis vian <tld/>on;
      </ekz>
      <ekz>
        post la subiro de la suno tra la militistaro ekkuris <tld/>o: &ccirc;iu en sian urbon,
        &ccirc;iu en sian landon
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 22:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        de &ccirc;ie a&ubreve;di&gcirc;is <tld/>oj pri kompato, <tld/>oj pasiaj, obstinaj, kiuj balda&ubreve;
        &scirc;an&gcirc;i&gcirc;is en unu &gcirc;eneralan krion
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sanskritaj pre&gcirc;oj kaj sanktaj <tld/>oj estas murmurataj de pastroj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o al la dioj
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        atendu la <tld/>on de la re&gcirc;o
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        li sentis la potencan <tld/>on al tiu tasko
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        sekvi la <tld/>on de siaj deziroj
        <fnt>
          <bib>Tor</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">crida</trd>
    <trdgrp lng="de">
      <trd>Ruf</trd>,
      <trd>Rufen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>call</trd>,
      <trd>calling</trd>
    </trdgrp>
   
    <trd lng="es">llamada</trd>
    <trd lng="hu">h&iacute;v&aacute;s</trd>
    <trdgrp lng="pl">
      <trd>zawo&lstroke;anie</trd>,
      <trd>wezwanie</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_o;&c_v;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_y;&c_v;</trd>,
      <trd>&c_k;&c_l;&c_i;&c_ch;</trd>,
      <trd>&c_v;&c_y;&c_z;&c_o;&c_v;</trd>
    </trdgrp>
    <trd lng="vo">vok</trd>
  </snc>
  <trdgrp lng="be">
      <trd>&c_k;&c_l;&c_ib;&c_ch;</trd>,
      <trd>&c_p;&c_o;&c_k;&c_l;&c_ib;&c_ch;</trd>,
      <trd>&c_z;&c_a;&c_k;&c_l;&c_ib;&c_k;</trd>,
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>instrukce pro vyvol&aacute;n&iacute; programu</trd>,
    <trd>vol&aacute;n&iacute;</trd>,
    <trd>vyvol&aacute;n&iacute;</trd>
  </trdgrp>
  <trd lng="fr">appel</trd>
