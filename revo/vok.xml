<drv mrk="vok.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vok.el0i.KOMUNE">
    <dif>
      <tld lit="V"/>i por elirigi:
      <ekz>
        ne el<tld/>u lupon el la arbaro
        <fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        el<tld/>i iun al duelo;
      </ekz>
      <ekz>
         li el<tld/>is Lazaron el la tombo kaj levis lin el la mortintoj
           
          <fnt><bib>NT</bib>, <lok>Johano 12:17</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>to call forth</trd>,
      <trd>summon</trd>
    </trdgrp>
    <trd lng="hu">kih&iacute;v</trd>
    <trd lng="nl">te <ind>voorschijn</ind> roepen</trd>
    <trd lng="pl">wywo&lstroke;ywa&cacute;</trd>
  </snc>
  <snc mrk="vok.el0i.FIG_kauxzi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Okazigi, estigi, ka&ubreve;zi:
      <ekz>
        tiu &ccirc;i demando per sia naiveco el<tld/>os
        ridon<fnt><bib>EE</bib>, 
        <lok>&ccirc;ap. 2</lok></fnt>;
      </ekz>
      <ekz>
        tiu neologismo el<tld/>as konfuzojn kun aliaj
        vortoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu propono celas nur el<tld/>i skandalon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu artikolo el<tld/>is malpacon<fnt>Z</fnt>,
      </ekz>
      <ekz>
        multon da intereso;
      </ekz>
      <ekz>
        konflikto, kiu povas el<tld/>i militon;
      </ekz>
      <ekz>
        feli&ccirc;o kaj ri&ccirc;o envion el<tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la propono el<tld/>is &ccirc;e ili la timon, ke...<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vek.0i">veki</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_a;&c_b;&c_u;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>to evoke</trd>,
      <trd>elicit</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kiv&aacute;lt</trd>,
      <trd>el&odblac;id&eacute;z</trd>
    </trdgrp>
    <trd lng="nl">ten <ind>gevolge</ind> hebben</trd>
    <trd lng="pl">wywo&lstroke;ywa&cacute;</trd>
  </snc>
  <snc mrk="vok.el0i.FIG_anime">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Prezenti al la spirito vivan vidbildon:
      <ekz>
        la propraj nomoj el<tld/>as en ni bildojn pli konkretajn ol la
        komunaj substantivoj;
      </ekz>
      <ekz>
        en poezio oni devas precipe atenti la el<tld/>i-povon de la
        vortoj.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="pl">wywo&lstroke;ywa&cacute;</trd>
  </snc>
  <trd lng="bo">&t_sa;&t_kata;&t_da;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>cridar (per fer sortir)</trd>,
    <trd>evocar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vyvolat</trd>,
    <trd>vyzvat</trd>
  </trdgrp>
  <trd lng="en">to evoke</trd>
  <trd lng="es">evocar</trd>
  <trdgrp lng="fr">
    <trd>&eacute;voquer</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&he;&ayin;&lamed;&vav;&tav;</trd>,
    <trd>&lamed;&ayin;&vav;&resh;&resh;</trd>,
    <trd>&lamed;&zayin;&mem;&fnun;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&odblac;id&eacute;z</trd>,
    <trd>megjelen&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vyvola&tcaron;</trd>
</drv>

<drv mrk="vok.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vok.kun0i.KOMUNE">
    <dif>
      <tld lit="V"/>i al kunveno:
      <ekz>
        Moseo kun<tld/>is &ccirc;iujn Izraelidojn, kaj diris al ili: a&ubreve;skultu, ho <nom>Izrael</nom>, la
        le&gcirc;ojn kaj la regulojn <klr>[&#8230;]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 5:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi kun<tld/>is kontra&ubreve; ili grandan kunvenon
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 5:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mastrino <klr>[&#8230;]</klr> kun<tld/>as la tutan familion, por a&ubreve;skulti, kiel
        sovetia in&gcirc;eniero muzikas List&#8217;on
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        noktmeze oni kun<tld/>is ur&gcirc;an partikunvenon kune kun la aktivistoj
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>5. La publika malla&ubreve;do.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Zamenhof</nom> revis kun<tld/>i &leftquot;kongreson por ne&ubreve;trale-homa religio&rightquot;
        <fnt>
          <bib>Zam</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">convocar</trd>
  <trd lng="cs">svolat</trd>
  <trd lng="de">zusammenrufen</trd>
  <trdgrp lng="en">
    <trd>to convoke</trd>,
    <trd>convene</trd>
  </trdgrp>
  <trd lng="el">&sigma;&ypsilon;&gamma;&kappa;&alfa;&lambda;&omega_ton;</trd>
  <trd lng="es">convocar</trd>
  <trdgrp lng="fr">
    <trd>convoquer</trd>
  </trdgrp>
  <trd lng="he">&lamed;&zayin;&mem;&fnun;</trd>
  <trd lng="hu">&ouml;sszeh&iacute;v</trd>
  <trd lng="nl">samenroepen</trd>
  <trd lng="pl">zwo&lstroke;ywa&cacute;</trd>
  <trd lng="pt">convocar</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zvola&tcaron;</trd>
</drv>

