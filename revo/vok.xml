<drv mrk="vok.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vok.0i.KOMUNE">
    <dif>
      La&ubreve;tigi la vo&ccirc;on por komprenigi al iu, ke li devas veni:
      <ekz>
        <tld/>i helpanton kontra&ubreve; rabisto<fnt>B</fnt>;
      </ekz>
      <ekz>
        al vi, ho viroj, mi <tld/>as
        <fnt><bib>MT</bib>, <lok>Sentencoj 8:4</lok></fnt>;
      </ekz>
      <ekz>
        ne <tld/>u diablon, &ccirc;ar li povas aperi
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        oni <tld/>as la bovojn ne festeni, sed treni
         <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>i por helpo, helpon, al batalo;
      </ekz>
      <ekz>
        kaj en tiu tago la Eternulo <tld/>is, ke oni ploru kaj
        kriu
        <fnt><bib>MT</bib>, <lok>Jesaja 22:12</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i sian hundon fajfante;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sonorilo <tld/>as al pre&gcirc;ejo kaj mem neniam
        eniras<fnt><bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="zh">&#x547C;&#x53EB; [<ind>h&#x16B;ji&agrave;o</ind>]</trd>
  </snc>
  <snc mrk="vok.0i.alvoki">
    <dif>
      Sciigi al iu, ke li devas veni,
      <ref tip="vid" cel="vok.al0i.iun">al<tld/>i</ref>,
      inviti:
      <ekz>
        <tld/>i iun per gesto, letero;
      </ekz>
      <ekz>
        venas kvaza&ubreve; vokita kaj petita
        <fnt><bib>PrV</bib></fnt>
        <klr>(&gcirc;ustatempe)</klr>;
      </ekz>
      <ekz>
        oni <tld/>is min al la kortego;
      </ekz>
      <ekz>
        <klr>[li]</klr> sendis voki lian konstantan kuraciston
        <fnt><bib>Fab2</bib>, <lok>La najtingalo</lok></fnt>;
      </ekz>
      <ekz>
        mia sorto min <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la tasko <tld/>as min;
      </ekz>
      <ekz>
        multaj <tld/>itoj, sed ne multaj elektitoj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_r;&c_a;&c_sh;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_p;&c_r;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="zh">&#x53EC;&#x5524; [<ind>zh&agrave;ohu&agrave;n</ind>]</trd>
  </snc>
  <snc mrk="vok.0i.elvoki">
    <ref tip="dif" cel="vok.el0i.FIG_kauxzi">el<tld/>i</ref>
  </snc>
  <trd lng="af">oproep</trd>
  <trd lng="am">&#x12ED;&#x12F0;&#x12CD;&#x1209;</trd>
  <trd lng="ar">&#x62F;&#x639;&#x648;&#x629;</trd>
  <trd lng="az">z&#x259;ng edin</trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_o;&c_v;&c_i;&c_k;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_i;&c_k;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="bn">&#x995;&#x9B2;</trd>
  <trd lng="bo">&#xF66;&#xF90;&#xF51;&#xF0B;&#xF42;&#xF4F;&#xF7C;&#xF44;&#xF0B;</trd>
  <trd lng="ca">cridar</trd>
  <trd lng="co">d</trd>
  <trdgrp lng="cs">
    <trd>k&rcaron;i&ccaron;et</trd>,
    <trd>obeslat</trd>,
    <trd>volat</trd>,
    <trd>vyzvat</trd>,
    <trd>zavolat</trd>
  </trdgrp>
  <trd lng="da">opkald</trd>
  <trd lng="de">rufen</trd>
  <trd lng="el">&phi;&omega;&ny;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="en">call</trd>
  <trd lng="es">llamar</trd>
  <trd lng="et">&#xFC;leskutse</trd>
  <trd lng="eu">deia</trd>
  <trd lng="fr">appeler</trd>
  <trd lng="fy">belje</trd>
  <trd lng="gl">chamada</trd>
  <trd lng="gu">&#xA95;&#xAC9;&#xAB2;</trd>
  <trd lng="ha">kira</trd>
  <trd lng="he">&lamed;&qof;&resh;&vav;&alef; &lamed;-</trd>
  <trd lng="hi">&#x915;&#x949;&#x932;</trd>
  <trd lng="hr">poziv</trd>
  <trd lng="hu">h&iacute;v</trd>
  <trd lng="hy">&#x56F;&#x578;&#x579;</trd>
  <trd lng="is">kalla</trd>
  <trd lng="ja">&#x30B3;&#x30FC;&#x30EB;</trd>
  <trd lng="jw">telpon</trd>
  <trd lng="ka">&#x10D3;&#x10D0;&#x10E0;&#x10D4;&#x10D9;&#x10D4;&#x10D7;</trd>
  <trd lng="km">&#x1780;&#x17B6;&#x179A;&#x17A0;&#x17C5;</trd>
  <trd lng="ko">&#xD1B5;&#xD654;</trd>
  <trd lng="lo">&#xEC2;&#xE97;</trd>
  <trd lng="ml">&#xD35;&#xD3F;&#xD33;&#xD3F;</trd>
  <trd lng="mr">&#x915;&#x949;&#x932;</trd>
  <trd lng="my">&#x1001;&#x1031;&#x102B;&#x103A;&#x101D;&#x1031;&#x102B;&#x103A;</trd>
  <trd lng="ne">&#x915;&#x932;</trd>
  <trd lng="nl">roepen</trd>
  <trd lng="ny">kuitana</trd>
  <trd lng="oc">apelar</trd>
  <trd lng="pa">&#xA15;&#xA3E;&#xA32; &#xA26;&#xA3E;</trd>
  <trdgrp lng="pl">
    <trd>wo&lstroke;a&cacute;</trd>,
    <trd>wzywa&cacute;</trd>
  </trdgrp>
  <trd lng="ps">&#x63A;&#x648;&#x69A;&#x62A;&#x646;&#x6D0;</trd>
