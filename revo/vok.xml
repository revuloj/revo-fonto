    <trd>&c_z;&c_a;&#x43A;&#x43B;&c_i;&#x43A;</trd>,
    <trd>&#x43F;&#x43E;&#x43A;&#x43B;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>

<drv mrk="vok.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vok.al0i.iun">
    <dif>
      <tld lit="V"/>i al si:
      <ekz>
        <nom>Isaak</nom> al<tld/>is Jakobon kaj benis lin
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 28:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        al<tld/>u la laboristojn kaj donu al ili la salajron 
        <fnt><bib>NT</bib>, <lok>Mateo 20:8</lok></fnt>;
      </ekz>
      <ekz>
        Pa&ubreve;lo al<tld/>is unu el la centestroj, kaj diris: konduku 
        &ccirc;i tiun junulon al la
        &ccirc;efkapitano, &ccirc;ar li havas ion por diri
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 23:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi al<tld/>is vin, por ke vi helpu min kun<tld/>i la reprezentantojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        vin mi al<tld/>as kiel atestantojn, ke mi metos finon al &ccirc;i tiuj            
        fripona&jcirc;oj
         <fnt><bib>Far3</bib>, <lok>&ccirc;ap. 6</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i tuj interrompis sin vidante taksion, kiun &scirc;i vigle al<tld/>is
        <fnt>
          <bib>CKv</bib>
        </fnt>;
      </ekz>
      <ekz>
        homo, kiu agas tiel, al<tld/>as <klr>(<ref cel="tir.al0i.logi">altiras</ref>)</klr>
        murdon al si
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vok.kun0i">kunvoki</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>zu sich <ind>rufen</ind></trd>,
      <trd>einberufen</trd>
    </trdgrp>
    <trd lng="en">to summon</trd>
    <trd lng="hu">(mag&aacute;hoz) <ind>h&iacute;v</ind></trd>
    <trd lng="pl">przywo&lstroke;ywa&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="vok.al0i.ion">
    <dif>
      <ref tip="super" cel="pet.0i">Peti</ref> ion <tld/>ante:
      <ekz>
        la &gcirc;enerala kunveno al<tld/>as &ccirc;iujn kunlaborantojn <klr>[&#x2026;]</klr>, &ccirc;u
        partianojn, &ccirc;u senpartiajn kamaradojn &ndash; <klr>[&#x2026;]</klr> helpi al la
        administracio
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fakaj organiza&jcirc;oj estis al<tld/>itaj prepari proprajn konkretajn programojn
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[per]</klr> la Rezolucio 1 <klr>[&#x2026;]</klr> la 53-a UK en <nom>Madrid</nom> (1968)
        al<tld/>is la Esperanto-organiza&jcirc;ojn instigi siajn registarojn akcepti &ccirc;iujn
        konvenciojn rilatantajn al la Homaj Rajtoj
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili do ekmilitis kaj al<tld/>is la ceterajn kantonojn al helpo
        <fnt>
          <bib>FSp</bib>
        </fnt>;
      </ekz>
      <ekz>
        li al<tld/>is la junularon sin dedi&ccirc;i al internacia kunlaboro
        <fnt>
          <bib>Kon11</bib>
          <lok>Pa&gcirc;oj El La Pasinteco</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="admon.0i">admoni</ref>,
      <ref cel="apelac.0i.JUR">apelacii</ref>,
      <ref cel="instig.0i">instigi</ref>,
      <ref cel="invit.0i">inviti</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
