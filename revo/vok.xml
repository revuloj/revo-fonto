  <trd lng="si">&#xD87;&#xDB8;&#xDAD;&#xDD4;&#xDB8;&#xDCA;</trd>
  <trdgrp lng="sk">
    <trd>vykr&iacute;knu&tcaron;</trd>,
    <trd>zavola&tcaron;</trd>,
