  <snc mrk="vok.0i.elvoki">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="vok.el0i.FIG_kauxzi">El<tld/>i<sncref/></ref>:
      <ekz>
        ekde kiam mi ekkonis Rikon, mi sentis ion neordinaran en li, ion, kio povus
        <tld/>i krimon
        <fnt>
          <bib>ChV</bib>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto <tld/>is la atenton de la internaciaj amas-komunikiloj
        <fnt>
          <bib>LOdE</bib>
          <lok> 2002, &#x2116;3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dua plena albumo de Eterne Rima tuj <tld/>as nian atenton ekde la titolo
        <fnt>
          <bib>Kon11</bib>
          <lok>Recenzo</lok>
        </fnt>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="tir.post0i.sekvigi">posttiri<sncref/></ref>,
      <ref cel="provok.0i.kauxzi">provoki<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="de">hervorrufen</trd>
  </snc>

