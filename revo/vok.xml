<drv mrk="vok.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vok.el0i.KOMUNE">
    <dif>
      <tld lit="V"/>i por elirigi:
      <ekz>
         li el<tld/>is Lazaron el la tombo kaj levis lin el la mortintoj           
          <fnt><bib>NT</bib>, <lok>Johano 12:17</lok></fnt>;
      </ekz>
      <ekz>
        unu post la alia <klr>[li]</klr> el<tld/>is la infanojn
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kapitano pensis el<tld/>i la markizon al duelo
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        per malla&ubreve;ta &leftquot;<esc>bhai! bhai!</esc>&rightquot; <klr>[li]</klr> el<tld/>is ilin el la arbaro
        <fnt>
          <bib>IKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne el<tld/>u lupon el la arbaro
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">heruasrufen</trd>
    <trdgrp lng="en">
      <trd>to call forth</trd>,
      <trd>summon</trd>
    </trdgrp>
    <trd lng="hu">kih&iacute;v</trd>
    <trd lng="nl">te <ind>voorschijn</ind> roepen</trd>
    <trd lng="pl">wywo&lstroke;ywa&cacute;</trd>
  </snc>
  <snc mrk="vok.el0i.FIG_kauxzi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Okazigi, estigi, ka&ubreve;zi:
      <ekz>
        tiu &ccirc;i demando per sia naiveco el<tld/>os
        ridon<fnt><bib>EE</bib>, 
        <lok>&ccirc;ap. 2</lok></fnt>;
      </ekz>
      <ekz>
        trafaj formuloj el<tld/>as entuziasmon kaj apla&ubreve;dojn
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        modelo, por el<tld/>i imitadon
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        jen Cezaro la granda falis! Antonio el<tld/>as kompaton por Cezaro kaj koleron
        kontra&ubreve; la murdintojn
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        aran&gcirc;adi cirkludojn, el<tld/>antajn miron pro sia lukso kaj malbona gusto
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi klopodis el<tld/>i lian amon, por malfeli&ccirc;igi lin, kaj per tio servi al mia
        diino
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiu apena&ubreve; rimarkebla aludo el<tld/>is fortan ru&gcirc;on sur la vangojn de la bela
        virino
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        vorto <klr>[&#x2026;]</klr> el<tld/>anta konfuzon kun aliaj vortoj
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vek.0i">veki</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_r;&c_y;&c_ch;&c_y;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_a;&c_b;&c_u;&c_d;&c_zh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">hervorrufen</trd>
    <trdgrp lng="en">
      <trd>to evoke</trd>,
      <trd>elicit</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kiv&aacute;lt</trd>,
      <trd>el&odblac;id&eacute;z</trd>
    </trdgrp>
    <trd lng="nl">ten <ind>gevolge</ind> hebben</trd>
    <trd lng="pl">wywo&lstroke;ywa&cacute;</trd>
  </snc>
  <snc mrk="vok.el0i.FIG_anime">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Prezenti al la spirito vivan vidbildon:
      <ekz>
        lia fantazio devas el<tld/>i en lia spirito la tutan okazinta&jcirc;on kun &ccirc;iuj
        <klr>[&#x2026;]</klr> detaloj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni el<tld/>as la bildon de &scirc;ipo batata de la ondoj kaj vipata de ventego kaj
        pere de tiu bildo oni prezentas la dan&gcirc;eron, kiu minacas
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        bona oratoro, al kiu &scirc;ajnas, ke la preparita fino ne el<tld/>us pli fortan
        efikon, rezignas pri &gcirc;i
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Alice</nom>, spiranta per la ribela aero, ne devus el<tld/>i en mi tiajn idekunrilatojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la sprita penado mallongigi sian vojon el<tld/>is, kiel &scirc;ajnas, ankora&ubreve; pli
        eksterordinaran elpenson
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_b;&c_u;&c_zh;&c_d;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">hervorrufen</trd>
    <trd lng="pl">wywo&lstroke;ywa&cacute;</trd>
  </snc>
  <trd lng="bo">&t_sa;&t_kata;&t_da;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>cridar (per fer sortir)</trd>,
    <trd>evocar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vyvolat</trd>,
    <trd>vyzvat</trd>
  </trdgrp>
  <trd lng="en">to evoke</trd>
  <trd lng="es">evocar</trd>
  <trdgrp lng="fr">
    <trd>&eacute;voquer</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&he;&ayin;&lamed;&vav;&tav;</trd>,
    <trd>&lamed;&ayin;&vav;&resh;&resh;</trd>,
    <trd>&lamed;&zayin;&mem;&fnun;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&odblac;id&eacute;z</trd>,
    <trd>megjelen&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x547C;&#x3073;&#x51FA;&#x3059;  <pr>&#x3088;&#x3073;&#x3060;&#x3059;</pr></trd>,
    <trd>&#x3072;&#x304D;&#x8D77;&#x3053;&#x3059;  <pr>&#x3072;&#x304D;&#x304A;&#x3053;&#x3059;</pr></trd>,
    <trd>&#x60F3;&#x8D77;&#x3055;&#x305B;&#x308B;  <pr>&#x305D;&#x3046;&#x304D;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vyvola&tcaron;</trd>
  <trd lng="uk">&c_v;&c_i;&#x43A;&#x43B;&c_i;&#x43A;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="vok.kun0i">
  <kap>kun<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vok.kun0i.KOMUNE">
    <dif>
      <tld lit="V"/>i al kunveno:
      <ekz>
        Moseo kun<tld/>is &ccirc;iujn Izraelidojn, kaj diris al ili: a&ubreve;skultu, ho <nom>Izrael</nom>, la
        le&gcirc;ojn kaj la regulojn <klr>[&#x2026;]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 5:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi kun<tld/>is kontra&ubreve; ili grandan kunvenon
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 5:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mastrino <klr>[&#x2026;]</klr> kun<tld/>as la tutan familion, por a&ubreve;skulti, kiel
        sovetia in&gcirc;eniero muzikas List&#x2019;on
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        noktmeze oni kun<tld/>is ur&gcirc;an partikunvenon kune kun la aktivistoj
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>5. La publika malla&ubreve;do.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Zamenhof</nom> revis kun<tld/>i &leftquot;kongreson por ne&ubreve;trale-homa religio&rightquot;
        <fnt>
          <bib>Zam</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">convocar</trd>
  <trd lng="cs">svolat</trd>
  <trd lng="de">zusammenrufen</trd>
  <trdgrp lng="en">
    <trd>to convoke</trd>,
    <trd>convene</trd>
  </trdgrp>
  <trd lng="el">&sigma;&ypsilon;&gamma;&kappa;&alfa;&lambda;&omega_ton;</trd>
  <trd lng="es">convocar</trd>
  <trdgrp lng="fr">
