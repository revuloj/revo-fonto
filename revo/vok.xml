    </trdgrp>
    <trd lng="pl">przyzywa&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_i;&c_z;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ca">
