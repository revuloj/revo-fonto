    <trd>klarifikasi</trd>,
    <trd>ke<ind>terang</ind>an</trd>
  </trdgrp>
  <trd lng="ja">&#35500;&#26126;  <pr>&#12379;&#12388;&#12417;&#12356;</pr></trd>
  <trd lng="pl">wyja&sacute;nienie</trd>
  <trdgrp lng="uk">
