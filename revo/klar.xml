<drv mrk="klar.0igi">
  <kap><tld/>igi</kap>
  <dif>
    Igi <tld/>a; malkonfuzi:
  </dif>
  <snc mrk="klar.0igi.malkonfuzi" ref="klar.0a.malkonfuza">
    <ekz>
      <nom>Marta</nom> vi&scirc;is per la mano siajn okulojn, kvaza&ubreve; &scirc;i volus <tld/>igi sian rigardon
      <fnt>
        <bib>Marta</bib>
      </fnt>;
    </ekz>
    <ekz>
      la azeno mal<tld/>igas &gcirc;in per siaj piedoj, tamen la akvo fluas pluen kaj
      fari&gcirc;as denove <tld/>a
      <fnt>
        <bib>Fab1</bib>
        <lok>Ligo de amikeco</lok>
      </fnt>.
    </ekz>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_ja;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_ja;&c_s;&c_mol;&c_n;&c_ja;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="en">to clarify</trd>
    <trd lng="de">kl&auml;ren</trd>
    <trd lng="en">clarify</trd>
    <trd lng="es">esclarecer</trd>
    <trd lng="he">&lamed;&nun;&qof;&vav;&tav; \<ind>&nun;&qof;&he;</ind>\</trd>
    <trdgrp lng="hu">
      <trd>der&iacute;t</trd>,
      <trd>&uuml;lep&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>rozja&sacute;nia&cacute;</trd>,
      <trd>klarowa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_ja;&c_s;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_s;&c_v;&c_je;&c_t;&c_l;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="klar.0igi.SON" ref="klar.0a.malrauxka">
    <ekz>
      tusetante, por <tld/>igi la vo&ccirc;on, Kruko komencas
      <fnt>
        <bib>KrB</bib>
      </fnt>;
    </ekz>
    <ekz>
      li <tld/>igis sian vo&ccirc;on kaj jene rakontis <klr>[&#x2026;]</klr>
      <fnt>
        <aut>J. Ribillard</aut>,
        <vrk>Vagado sub palmoj</vrk>,
        <lok>Rotterdam, 1956</lok>
      </fnt>.
    </ekz>
    <trd lng="be">&c_p;&c_r;&c_a;&c_ch;&c_y;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>
    <trd lng="en">to clear from impediment</trd>
    <trd lng="de">kl&auml;ren</trd>
    <trd lng="en">clear from impediment</trd>
    <trd lng="es">aclarar</trd>
    <trdgrp lng="he">
      <trd>&lamed;<ind>&kaf;&ayin;&kaf;&ayin;</ind></trd>,
      <trd>&lamed;<ind>&kaf;&het;&kaf;&het;</ind></trd>,
      <trd>&lamed;<ind>&het;&kaf;&fkaf;</ind></trd>
    </trdgrp>
    <trd lng="hu">k&ouml;sz&ouml;r&uuml;li <klr tip="ind">(a
    tork&aacute;t)</klr></trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="klar.0igi.komprenebligi" ref="klar.0a.komprenebla">
    <ekz>
      <tld/>igi la uzadon de la vortaro
      <fnt>
        <bib>FK</bib>
      </fnt>;
    </ekz>
    <ekz>
      mi <tld/>igos tion &ccirc;i per ekzemplo
      <fnt>
        <bib>FK</bib>
      </fnt>;
    </ekz>
    <ekz>
      mi turnas la atenton <klr>[&#x2026;]</klr> al la <tld/>igita punkto
      <fnt>
        <bib>FK</bib>
      </fnt>;
    </ekz>
    <ekz>
      li signifo<tld/>igis al ni niajn son&gcirc;ojn, al &ccirc;iu li <tld/>igis la&ubreve; lia son&gcirc;o
      <fnt>
        <bib>MT</bib>
        <lok>Genezo 41:12</lok>
      </fnt>;
    </ekz>
    <ekz>
      ne<tld/>igebla malamikeco
      <fnt>
        <bib>FK</bib>
      </fnt>;
    </ekz>
    <ekz>
      ni reciproke nin konas, <tld/>igon ne bezonas
      <fnt>
        <bib>PrV</bib>
      </fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="ekspli.0i">ekspliki</ref>,
      <ref cel="koment.0i">komenti</ref>,
      <ref cel="kompre.0igi">komprenigi</ref>,
