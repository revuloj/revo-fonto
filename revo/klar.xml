<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>klar</rad>/a
</kap>
<drv mrk="klar.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Malkonfuza, pura; tra kiu oni povas distinge vidi:
      <ekz>
        <tld/>a akvo, rivero, vino, lumo;
      </ekz>
      <ekz>
        trankvilaj kaj <tld/>aj okuloj;
      </ekz>
      <ekz>
        <ind>el <tld/>a <klr>(sennuba)</klr> &ccirc;ielo tondro ekbatis</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">un coup de <ind>tonnerre</ind> dans un ciel serein</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hel.0a.lumplena">hela <sncref/></ref>,
      <ref cel="diafan.0a">diafana</ref>,
      <ref cel="preciz.0a">preciza</ref>,
      <ref cel="net.0a">neta</ref>,
      <ref cel="krista">kristala</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>tiszta</trd>,
      <trd>vil&aacute;gos</trd>,
      <trd>der&uuml;lt</trd>
    </trdgrp>
    <trd lng="nl">klaar<klr tip="ind"> (helder)</klr>
    </trd>
  </snc>
  <snc>
    <uzo tip="klr">(p.p. sono)</uzo>
    <dif>
      Ne obtuza, malra&ubreve;ka; distingeble sonanta en la orelo:
      <ekz>
        <tld/>a vo&ccirc;o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hel.0a.sonora">hela <sncref/></ref>.
    </refgrp>
    <trd lng="hu">cseng&odblac;</trd>
    <trd lng="nl">helder</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Facile komprenebla; distingebla por la spirito:
      <ekz>
        <tld/>a stilo, spirito, esprimo, penso;
      </ekz>
      <ekz>
        ne tute <tld/>aj instinktoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>ne<tld/>a</ind> demando;
        <trdgrp lng="fr">
          <trd>pas <ind>clair</ind></trd>,
          <trd>manquant de <ind>clart&eacute;</ind></trd>,
          <trd>obscur</trd>
          <trd>confus</trd>,
          <trd>flou</trd>,
          <trd>vague<klr tip="ind"> (adj.)</klr></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>konservu <tld/>econ de la kapo</ind><fnt>Z</fnt>;
        <trdgrp lng="fr">
          <trd>conservez votre <ind>clairvoyance</ind></trd>
          <trd>gardez votre <ind>lucidit&eacute;</ind></trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>havi <tld/>econ pri io</ind><fnt>Z</fnt>.
        <trd lng="fr">y voir <ind>clair</ind> au sujet de qc</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eviden.0a">evidenta</ref>,
      <ref cel="preciz.0a">preciza</ref>.
    </refgrp>
    <rim>
      <em>klara</em> ne povas kvalifiki personon.
    </rim>
    <trdgrp lng="hu">
      <trd>vil&aacute;gos</trd>,
      <trd>szabatos</trd>,
      <trd>&eacute;rthet&odblac;</trd>
    </trdgrp>
    <trd lng="nl">duidelijk</trd>
  </snc>
  <trd lng="be">&c_ja;&c_s;&c_n;&c_y;</trd>
  <trd lng="cs">jasn&yacute;</trd>
  <trd lng="ca">clar</trd>
  <trd lng="de">klar</trd>
  <trd lng="en">clear</trd>
  <trd lng="es">claro</trd>
  <trdgrp lng="fr">
    <trd>clair</trd>,
    <trd>compr&eacute;hensible</trd>,
    <trd>intelligible</trd>,
    <trd>limpide</trd>,
    <trd>net</trd>,
  </trdgrp>
  <trd lng="he">&bet;&resh;&vav;&resh;</trd>
  <trd lng="ru">&c_ja;&c_s;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="klar.0e">
  <kap><tld/>e</kap>
  <dif>
    Distingeble:
  </dif>
  <snc>
    <dif>
      por la okulo:
      <ekz>
        tra maldensa pinaro, oni povis vidi
        tute <tld/>e flavajn murojn.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>tiszt&aacute;n</trd>,
      <trd>vil&aacute;gosan</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      por la orelo:
      <ekz>
        <tld/>e akcenti la vortojn<fnt>Z</fnt>;
      </ekz>
    </dif>
    <trd lng="hu">tiszt&aacute;n</trd>
  </snc>
  <snc>
    <dif>
      por la spirito:
      <ekz>
        <tld/>e rememori ion<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiuj sentoj ne<tld/>e aperis en lia konscio<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>&eacute;rthet&odblac;en</trd>,
      <trd>szabatosan</trd>,
      <trd>vil&aacute;gosan</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_ja;&c_s;&c_n;&c_a;</trd>
  <trd lng="ca">clarament</trd>
  <trd lng="de">klar</trd>
  <trd lng="es">claramente</trd>
  <trdgrp lng="fr">
    <trd>clairement</trd>,
    <trd>distinctement</trd>,
    <trd>nettement</trd>
  </trdgrp>
  <trd lng="he">&bet;&bet;&resh;&vav;&resh;</trd>
  <trd lng="nl">duidelijk</trd>
  <trd lng="ru">&c_ja;&c_s;&c_n;&c_o;</trd>
</drv>

<drv mrk="klar.0igi">
  <kap><tld/>igi</kap>
  <dif>
    Igi <tld/>a; malkonfuzi:
  </dif>
  <snc>
    <ekz>
      <tld/>igi fe&ccirc;an vinon;
    </ekz>
    <ekz>
      kun okuloj mal<tld/>igitaj de larmoj;
    </ekz>
    <trd lng="de">kl&auml;ren</trd>
    <trd lng="he">&lamed;&nun;&qof;&vav;&tav; \<ind>&nun;&qof;&he;</ind>\</trd>
    <trdgrp lng="hu">
      <trd>der&iacute;t</trd>,
      <trd>&uuml;lep&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_ja;&c_s;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_s;&c_v;&c_je;&c_t;&c_l;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <ekz>
      tusi por <tld/>igi sian vo&ccirc;on;
    </ekz>
    <trd lng="de">kl&auml;ren</trd>
    <trdgrp lng="he">
      <trd>&lamed;<ind>&kaf;&ayin;&kaf;&ayin;</ind></trd>,
      <trd>&lamed;<ind>&kaf;&het;&kaf;&het;</ind></trd>,
      <trd>&lamed;<ind>&het;&kaf;&fkaf;</ind></trd>
    </trdgrp>
    <trd lng="hu">k&ouml;sz&ouml;r&uuml;li <klr tip="ind">(a tork&aacute;t)</klr></trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <ekz>
      <tld/>igi malfacilan tekston, enigmon, problemon, proponon;
    </ekz>
    <ekz>
      &gcirc;is la afero <tld/>i&gcirc;os<fnt>Z</fnt>;
    </ekz>
    <refgrp tip="vid">
      <ref cel="kompre.0igi">komprenigi</ref>,
      <ref cel="koment1.0i">komentarii</ref>,
      <ref cel="ekspli.0i">ekspliki</ref>,
      <ref cel="volv.mal0i">malvolvi</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_t;&c_l;&c_u;&c_m;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_t;&c_l;&c_u;&c_m;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>erkl&auml;ren</trd>,
      <trd>erl&auml;utern</trd>
    </trdgrp>
    <trdgrp lng="he">
      <trd>&lamed;&he;&samekh;&bet;&yod;&resh; \<ind>&samekh;&bet;&resh;</ind>\</trd>,
      <trd>&lamed;&he;&bet;&he;&yod;&resh; \<ind>&bet;&he;&resh;</ind>\</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tiszt&aacute;z</trd>,
      <trd>magyar&aacute;z</trd>,
      <trd>felvil&aacute;gos&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>verduidelijken</trd>,
      <trd>duidelijk maken</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_s;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_ja;&c_s;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_ja;&c_s;&c_n;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">aclarir</trd>
  <trd lng="es">aclarar</trd>
  <trdgrp lng="fr">
    <trd>clarifier</trd>,
    <trd>&eacute;claircir</trd>,
    <trd>&eacute;clairer<klr tip="ind"> (expliquer)</klr></trd>,
    <trd>expliquer</trd>
  </trdgrp>
</drv>

<drv mrk="klar.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Konfuza por la okulo, orelo a&ubreve; spirito:
      <ekz>
        en akvo mal<tld/>a oni fi&scirc;kaptas facile
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        mal<tld/>aj okuloj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        mal<tld/>aj konturoj de montaro;
      </ekz>
      <ekz>
        mal<tld/>a ideo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        rifuzo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        esprimo;
      </ekz>
      <ekz>
        ties viza&gcirc;on kaj vestojn mal<tld/>e lumigis la flamo;
      </ekz>
      <ekz>
        <ind>mal<tld/>igi</ind> rivereton<fnt>Z</fnt>;
        <trdgrp lng="fr">
          <trd>troubler<klr tip="ind"> (rendre trouble)</klr></trd>,
          <trd>brouiller</trd>
        </trdgrp>
      </ekz>
      <ekz>
        movmal<tld/>igi la akvon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind>mal<tld/>igi la prudenton de iu</ind><fnt>B</fnt>.
        <trd lng="fr">obscurcir le <ind>discernement</ind> de qn</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nebul.0a">nebula</ref>,
      <ref cel="nub.0a">nuba</ref>,
      <ref cel="enigm.0a">enigma</ref>,
      <ref cel="sfinks.0a">sfinksa</ref>,
      <ref cel="mister.0a">mistera</ref>,
      <ref cel="hierog.0o">hieroglifa</ref>,
      <ref cel="apokal.0a">apokalipsa</ref>,
      <ref cel="kabal.0o">kabala</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_ja;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_c;&c_mol;&c_m;&c_ja;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_m;&c_u;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">fosc</trd>
  <trd lng="de">unklar</trd>
  <trdgrp lng="fr">
    <trd>trouble<klr tip="ind"> (adj.)</klr></trd>,
    <trd>brumeux<klr tip="ind"> (embrouill&eacute;)</klr></trd>,
    <trd>confus</trd>,
    <trd>embrouill&eacute;</trd>,
    <trd>flou<klr tip="ind"> (adj.)</klr></trd>,
    <trd>vague<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="es">oscuro</trd>
  <trdgrp lng="he">
    <trd>&mem;&ayin;&vav;&resh;&pe;&lamed;</trd>,
    <trd>&ayin;&kaf;&vav;&resh;</trd>,
    <trd>&samekh;&tav;&vav;&fmem;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>zavaros</trd>,
    <trd>tiszt&aacute;zatlan</trd>,
    <trd>&eacute;rthetetlen</trd>
  </trdgrp>
  <trd lng="nl">onduidelijk</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_ja;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_u;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_m;&c_u;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
</art>
