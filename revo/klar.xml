  <trd lng="gd">soilleir</trd>
  <trd lng="gl">claro</trd>
  <trd lng="gu">&#2730;&#2750;&#2736;&#2726;&#2736;&#2765;&#2742;&#2709;</trd>
  <trd lng="he">&bet;&bet;&resh;&vav;&resh;</trd>
  <trd lng="hi">&#2360;&#2381;&#2346;&#2359;&#2381;&#2335;</trd>
  <trd lng="hmn">ntshiab</trd>
  <trd lng="hr">jasno</trd>
  <trd lng="ht">kl&egrave;</trd>
  <trd lng="hy">&#1402;&#1377;&#1408;&#1382;</trd>
  <trd lng="ig">doro anya</trd>
  <trd lng="is">lj&oacute;st</trd>
  <trd lng="ja">&#26126;&#30906;&#12394;</trd>
  <trd lng="jw">cetha</trd>
  <trd lng="ka">&#4316;&#4304;&#4311;&#4308;&#4314;&#4312;</trd>
  <trd lng="kk">&c_a;&c_j;&#1179;&c_y;&c_n;</trd>
  <trd lng="km">&#6021;&#6098;&#6036;&#6070;&#6047;&#6091;&#6043;&#6070;&#6047;&#6091;</trd>
  <trd lng="kn">&#3256;&#3277;&#3242;&#3255;&#3277;&#3231;</trd>
  <trd lng="ko">&#47749;&#54869;&#54620;</trd>
  <trd lng="ku">zelal</trd>
  <trd lng="ky">&c_a;&c_ch;&c_y;&c_k;</trd>
  <trd lng="lat">patet</trd>
  <trd lng="lo">&#3720;&#3760;&#3777;&#3720;&#3785;&#3719;</trd>
  <trd lng="lt">ai&scaron;ku,</trd>
  <trd lng="lv">skaidrs</trd>
  <trd lng="mg">mazava</trd>
