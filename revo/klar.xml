  <ofc>*</ofc>
  <rad>klar</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="klar.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="klar.0a.malkonfuza">
    <dif>
      Tra kiu oni povas distinge vidi, malkonfuza, pura:
      <ekz>
        <tld/>a, fre&scirc;a akvo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a vetero
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a, kiel la plej pura vitro
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        farita el blanka <tld/>a marmoro
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sidas sola en la <tld/>a lumo de la luno
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        glacia malvarmo iris tra la naturo, <klr>[&#x2026;]</klr> sed &ccirc;irka&ubreve;e la montoj ricevis
        belegan verdan tonon, tiel <tld/>an, tiel brilantan
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj <klr>(<ref cel="seren.0a">serena</ref>j)</klr> mallongaj someraj vesperoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        li vidis inter la folietoj <klr>[&#x2026;]</klr> malgrandan knabinon, rigardantan per
        siaj <tld/>aj okuloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i estis tiel delikata kaj <tld/>a, kiel lunlumo
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>el <tld/>a &ccirc;ielo tondro ekbatis</ind>
        <klr>(t.e. tute neatendite)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="de">ein Blitz aus heiterem Himmel</trd>
        <trd lng="fr">un coup de <ind>tonnerre</ind> dans un ciel
        serein</trd>
        <trd lng="ru">&c_g;&c_r;&c_o;&c_m; &c_s;&c_r;&c_je;&c_d;&c_i; &c_ja;&c_s;&c_n;&c_o;&c_g;&c_o;
        &c_n;&c_je;&c_b;&c_a;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diafan.0a">diafana</ref>,
      <ref cel="hel.0a.lumplena">hela <sncref/></ref>,
      <ref tip="super" cel="lum.0a.hela">luma</ref>,
      <ref cel="net.0a">neta</ref>,
      <ref cel="preciz.0a">preciza</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>n&iacute;tid</trd>,
      <trd>di&agrave;fan</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>n&iacute;tido</trd>,
      <trd>di&aacute;fano</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tiszta</trd>,
      <trd>vil&aacute;gos</trd>,
      <trd>der&uuml;lt</trd>
    </trdgrp>
    <trd lng="nl">klaar<klr tip="ind"> (helder)</klr></trd>
    <trdgrp lng="pl">
      <trd>klarowny</trd>,
      <trd>czysty</trd>,
      <trd>wyra&zacute;ny</trd>
    </trdgrp>
  </snc>
  <snc mrk="klar.0a.malrauxka">
    <uzo tip="klr">(pri sono)</uzo>
    <dif>
      Ne obtuza, mal<ref tip="ant" cel="rauxk.0a.SON">ra&ubreve;ka</ref>;
      sonanta distingeble por la orelo:
      <ekz>
        eksonis neforta, sed <tld/>a vo&ccirc;o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        vo&ccirc;o <tld/>a kaj forta
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li a&ubreve;dis la <tld/>an sonon de martelo kontra&ubreve; amboso
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        Niko havis violonon <klr>[&#x2026;]</klr> kaj <klr>[li]</klr> e&ccirc; sukcesis, kvankam iom
        palpe, aperigi kelkajn sufi&ccirc;e <tld/>ajn tonojn
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        sonorilo ar&gcirc;entosona, <tld/>a kaj la&ubreve;ta eksonis en la aero
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hel.0a.sonora">hela<sncref/></ref>.
    </refgrp>
    <trdgrp lng="ca">
      <trd>prec&iacute;s</trd>,
      <trd>pur</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>preciso</trd>,
      <trd>puro</trd>
    </trdgrp>
    <trd lng="hu">cseng&odblac;</trd>
    <trd lng="nl">helder</trd>
    <trdgrp lng="pl">
      <trd>czysty</trd>,
      <trd>wyra&zacute;ny</trd>
    </trdgrp>
  </snc>
  <snc mrk="klar.0a.komprenebla">
    <dif>
      Facile <ref tip="super" cel="kompre.0ebla">komprenebla</ref>; distingebla
      por la spirito:
      <ekz>
        &ccirc;iu penso en nia animo estis <tld/>a por ni amba&ubreve;
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen estas mia <tld/>a volo, kiun mi ne forlasos
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne tute <tld/>aj instinktoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio estis]</klr> <tld/>a <klr>(<ref cel="dub.sen0a">senduba</ref>)</klr> rifuzo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne<tld/>a koncepto
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>ne<tld/>a</ind> demando
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
        <trdgrp lng="ca">
          <trd>ambigu</trd>,
          <trd>incert</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>Unklarheit</trd>,
          <trd><ind>offen</ind>e Frage</trd>
        </trdgrp>
        <trdgrp lng="es">
          <trd>ambiguo</trd>,
          <trd>confuso</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>pas <ind>clair</ind></trd>,
          <trd>manquant de <ind>clart&eacute;</ind></trd>,
          <trd>obscur</trd>
          <trd>confus</trd>,
          <trd>flou</trd>,
          <trd>vague<klr tip="ind"> (adj.)</klr></trd>
        </trdgrp>
      </ekz><ekz>
        <ind>konservu <tld/>econ de la kapo</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 3:21</lok>
        </fnt>;
        <trd lng="de">einen <ind>k&uuml;hlen</ind> Kopf bewahren</trd>
        <trdgrp lng="es">
          <trd>conserva tu <ind>clarividencia</ind></trd>
          <trd>guarda tu <ind>lucidez</ind></trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>conservez votre <ind>clairvoyance</ind></trd>
          <trd>gardez votre <ind>lucidit&eacute;</ind></trd>
        </trdgrp>
      </ekz>
      <ekz>
        pri <klr>[ties tago]</klr> graveco &scirc;i <klr>[&#x2026;]</klr>
        mem ne havis plenan <tld/>econ
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eviden.0a">evidenta</ref>,
      <ref cel="preciz.0a">preciza</ref>
    </refgrp>
    <rim>
      PV asertas, ke
      <ctl><em>klara</em> ne povas kvalifiki personon</ctl>.
      Tio estas ne tute &gcirc;usta &dash; almena&ubreve; ja ekzistas la
      esprimo
      &leftquot;<tld/>a kapo&rightquot;, kiu karakterizas personon:
      <ekz>
        vi havos tian <tld/>an kapon, ke &ccirc;ion, kio en via &ccirc;eesto
        estos rakontata a&ubreve; legata, vi &ccirc;ion vidos anta&ubreve; vi kiel
        vivantan
        <fnt><bib>Fab2</bib>, <lok>Malnova strata
        lanterno</lok></fnt>;
      </ekz><ekz>
        Ivano konstatis, <klr>[&#x2026;]</klr> ke <nom>Stravinskij</nom> estas
        <tld/>a kapo kaj famulo
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 11a</lok></fnt>.
      </ekz>
      <aut>Sergio Pokrovskij</aut>
    </rim>
    <trdgrp lng="ca">
      <trd>senzill</trd>,
      <trd>obvi</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>sencillo</trd>,
      <trd>obvio</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vil&aacute;gos</trd>,
      <trd>szabatos</trd>,
      <trd>&eacute;rthet&odblac;</trd>
    </trdgrp>
    <trd lng="nl">duidelijk</trd>
    <trdgrp lng="pl">
      <trd>jasny</trd>,
      <trd>zrozumia&lstroke;y</trd>,
      <trd>wyra&zacute;ny</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_ja;&c_s;&c_n;&c_y;</trd>
  <trd lng="bo">&t_ga;&t_sa;&t_la;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trd lng="ca">clar</trd>
  <trdgrp lng="cs">
    <trd>jasn&yacute;</trd>,
    <trd>pr&uring;hledn&yacute;</trd>,
    <trd>srozumiteln&yacute;</trd>,
    <trd>z&rcaron;eteln&yacute;</trd>,
    <trd>&ccaron;ir&yacute;</trd>
  </trdgrp>
  <trd lng="de">klar</trd>
  <trd lng="en">clear<klr> (adj)</klr></trd>
  <trd lng="es">claro</trd>
  <trdgrp lng="fr">
    <trd>clair</trd>,
    <trd>compr&eacute;hensible</trd>,
    <trd>intelligible</trd>,
    <trd>limpide</trd>,
    <trd>net</trd>,
  </trdgrp>
  <trd lng="he">&bet;&resh;&vav;&resh;</trd>
  <trdgrp lng="ja">
    <trd>&#x6F84;&#x307F;&#x304D;&#x3063;&#x305F;  [<ind>&#x3059;&#x307F;&#x304D;&#x3063;&#x305F;</ind>]</trd>,
    <trd>&#x660E;&#x308B;&#x3044;  [<ind>&#x3042;&#x304B;&#x308B;&#x3044;</ind>]</trd>,
    <trd>&#x660E;&#x77AD;&#x306A;  [<ind>&#x3081;&#x3044;&#x308A;&#x3087;&#x3046;&#x306A;</ind>]</trd>,
    <trd>&#x660E;&#x767D;&#x306A;  [<ind>&#x3081;&#x3044;&#x306F;&#x304F;&#x306A;</ind>]</trd>
  </trdgrp>
  <trd lng="ne">&#x938;&#x94D;&#x92A;&#x937;&#x94D;&#x91F;</trd>
  <trd lng="oc">clar</trd>
  <trdgrp lng="ro">
    <trd>clar</trd>,
    <trd>limpede</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ja;&c_s;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_ch;&c_jo;&c_t;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_o;&c_t;&c_ch;&c_jo;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>jasn&yacute;</trd>,
    <trd>zrete&lcaron;n&yacute;</trd>,
    <trd>&ccaron;&iacute;ry</trd>
  </trdgrp>
  <trd lng="sv">klar</trd>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_s;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_v;&c_ib;&c_t;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_ib;&c_t;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_r;&c_a;&c_z;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_r;&c_o;&c_z;&c_u;&c_m;&c_ib;&c_l;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="klar.0e">
  <kap><tld/>e</kap>
  <dif>
    Distingeble:
  </dif>
  <snc mrk="klar.0e.malkonfuze">
    <dif>
      por la okulo a&ubreve; imago:
      <ekz>
        skribu tre <tld/>e sur la &scirc;tonoj &ccirc;iujn vortojn de &ccirc;i tiu instruo
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 27:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        la jura konsilisto ekvidis tute <tld/>e anta&ubreve; si brulantan lanternon
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[en la]</klr> lumo de la steloj <klr>[&#x2026;]</klr> ni vidis <tld/>e la
        konturojn de &ccirc;iuj montoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        ankora&ubreve; unu aferon &scirc;i vidis <tld/>e kaj precize: la blankan viza&gcirc;eton de Janjo
        kaj la grandajn nigrajn okulojn <klr>[&#x2026;]</klr> kiel sen&ccirc;esa, trudi&gcirc;a rememoro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi preska&ubreve; tute <tld/>e povas tion prezenti al mi
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>!
      </ekz>
      <ekz>
        mi memoras ankora&ubreve; <tld/>e lian viza&gcirc;on
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">clearly</trd>
    <trdgrp lng="hu">
      <trd>tiszt&aacute;n</trd>,
      <trd>vil&aacute;gosan</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>klarownie</trd>,
      <trd>czysto</trd>,
      <trd>wyra&zacute;nie</trd>
    </trdgrp>
  </snc>
  <snc mrk="klar.0e.SON">
    <dif>
      por la orelo:
      <ekz>
        oni <tld/>e a&ubreve;dis sonon de rompi&gcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili kantis kaj Johano rekonis tre <tld/>e &ccirc;iujn belajn melodiojn, kiujn li lernis
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ion, kion &gcirc;i sentis silente, la malgranda ala&ubreve;do tiel <tld/>e kaj bele kantis
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="en">clearly</trd>
    <trd lng="hu">tiszt&aacute;n</trd>
    <trdgrp lng="pl">
      <trd>czysto</trd>,
      <trd>wyra&zacute;nie</trd>
    </trdgrp>
  </snc>
  <snc mrk="klar.0e.kompreneble">
    <dif>
      por la spirito:
      <ekz>
        mi <tld/>e avertis viajn patrojn
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 11:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        hipokritulo, eligu unue la trabon el via okulo, kaj tiam vi <tld/>e vidos, por
        eltiri la lignereton el la okulo de via frato
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 7:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        li povoscias konservi la ideojn kaj sentojn, &gcirc;is ili <tld/>e kaj precize ricevas
        la formon de vortoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi povos <tld/>e kaj precize rememori <klr>[&ccirc;]</klr>ion,
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unua sento, kiu <tld/>e veki&gcirc;is en &scirc;i, estis &dash; envio
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cert.0e">certe</ref>,
      <ref cel="kompre.0eble.maniero">kompreneble</ref>,
      <ref cel="eviden.0e">evidente</ref>,
      <ref cel="preciz.0e">precize</ref>,
      <ref cel="dub.sen0e">sendube</ref>
    </refgrp>
    <trdgrp lng="en">
      <trd>clearly</trd>,
      <trd>plainly</trd>,
      <trd>with <ind>clarity</ind></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&eacute;rthet&odblac;en</trd>,
      <trd>szabatosan</trd>,
      <trd>vil&aacute;gosan</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>jasny</trd>,
      <trd>zrozumia&lstroke;y</trd>,
      <trd>wyra&zacute;ny</trd>
    </trdgrp>
  </snc>
  <trd lng="af">duidelik</trd>
  <trd lng="am">&#x130D;&#x120D;&#x1345;</trd>
  <trd lng="ar">&a_w;&a_A;&a_D;&a_H;</trd>
  <trd lng="az">ayd&inodot;n</trd>
  <trd lng="be">&c_ja;&c_s;&c_n;&c_a;</trd>
  <trd lng="bn">&#x9B8;&#x9CD;&#x9AA;&#x9B7;&#x9CD;&#x99F;</trd>
  <trd lng="bs">jasno</trd>
  <trdgrp lng="ca">
    <trd>clarament</trd>,
    <trd>manifestament</trd>
  </trdgrp>
  <trd lng="co">chjaru</trd>
  <trd lng="cy">glir</trd>
  <trd lng="da">klar</trd>
  <trdgrp lng="de">
    <trd>klar</trd>,
    <trd>eindeutig</trd>
  </trdgrp>
  <trd lng="es">claramente</trd>
  <trd lng="et">selge</trd>
  <trd lng="eu">argi</trd>
  <trdgrp lng="fr">
    <trd>clairement</trd>,
    <trd>distinctement</trd>,
    <trd>nettement</trd>
  </trdgrp>
  <trd lng="fy">opklearje</trd>
  <trd lng="ga">soil&eacute;ir</trd>
  <trd lng="gd">soilleir</trd>
  <trd lng="gl">claro</trd>
  <trd lng="gu">&#xAAA;&#xABE;&#xAB0;&#xAA6;&#xAB0;&#xACD;&#xAB6;&#xA95;</trd>
  <trd lng="he">&bet;&bet;&resh;&vav;&resh;</trd>
