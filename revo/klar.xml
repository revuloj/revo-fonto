<drv mrk="klar.mem0a">
  <kap>mem<tld/>a</kap>
  <snc mrk="klar.mem0a.KOMUNE">
    <dif>
      <ref tip="vid" cel="eviden.0a">Evidenta</ref>,
      <ref tip="vid" cel="kompre.mem0ebla">memkomprenebla</ref>:
      <ekz>
        kelkaj el tiuj sintagmoj estas mem<tld/>aj,
        aliaj estas iom idiotismaj
        <fnt>
          <aut>J. Henry</aut>,
          <vrk><url ref="http://jimhenry.conlang.org/conlang/tokipona/lecionoj/lecion03.htm">
          Baza Lecionaro de Tokipono</url></vrk>,
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        se la sekso de persono estas mem<tld/>a, la in-sufikso superfluas
        <fnt>
          <aut>Vil&ccirc;jo Simcock</aut>
          <vrk><url ref="&Monato;/2006/008876.php">Angie kaj Bush</url></vrk>
          <bib>Monato</bib>
          <lok>2006/03, p. 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial grandskale okazas tia seksa aberacio inter pastroj? la kialo estas
        mem<tld/>a, kaj la problemon povas solvi la papo
        <fnt>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="&Monato;/2007/009363.php">Kial pastroj ne edzi&gcirc;u?</url></vrk>
          <bib>Monato</bib>
          <lok>2007/12, p. 6</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>selbsterkl&auml;rend</trd>,
      <trd>offensichtlich</trd>,
      <trd>evident</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_u;&c_m;&c_n;&c_je;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_ja;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>manifest</trd>,
    <trd>palmari</trd>
  </trdgrp>
  <trd lng="en">self-evident</trd>
  <trdgrp lng="es">
    <trd>evidente</trd>,
    <trd>palmario</trd>,
    <trd>patente</trd>,
    <trd>manifiesto</trd>
  </trdgrp>
