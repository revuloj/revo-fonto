      <trd>&c_p;&c_r;&c_o;&c_ja;&c_s;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_ja;&c_s;&c_n;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">aclarir</trd>
