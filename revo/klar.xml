<drv mrk="klar.0igi">
  <kap><tld/>igi</kap>
  <dif>
    Igi <tld/>a; malkonfuzi:
  </dif>
  <snc mrk="klar.0igi.malkonfuzi" ref="klar.0a.malkonfuza">
    <ekz>
      <nom>Marta</nom> vi&scirc;is per la mano siajn okulojn, kvaza&ubreve; &scirc;i volus <tld/>igi sian rigardon
      <fnt>
        <bib>Marta</bib>
      </fnt>;
    </ekz>
    <ekz>
      la azeno mal<tld/>igas &gcirc;in per siaj piedoj, tamen la akvo fluas pluen kaj
      fari&gcirc;as denove <tld/>a
      <fnt>
        <bib>Fab1</bib>
        <lok>Ligo de amikeco</lok>
      </fnt>.
    </ekz>
    <trd lng="en">to clarify</trd>
    <trd lng="de">kl&auml;ren</trd>
    <trd lng="es">esclarecer</trd>
    <trd lng="he">&lamed;&nun;&qof;&vav;&tav; \<ind>&nun;&qof;&he;</ind>\</trd>
    <trdgrp lng="hu">
      <trd>der&iacute;t</trd>,
      <trd>&uuml;lep&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>rozja&sacute;nia&cacute;</trd>,
      <trd>klarowa&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_ja;&c_s;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_s;&c_v;&c_je;&c_t;&c_l;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="klar.0igi.SON" ref="klar.0a.malrauxka">
    <ekz>
      tusetante, por <tld/>igi la vo&ccirc;on, Kruko komencas
      <fnt>
        <bib>KrB</bib>
      </fnt>;
    </ekz>
    <ekz>
      li <tld/>igis sian vo&ccirc;on kaj jene rakontis <klr>[&#8230;]</klr>
      <fnt>
        <aut>J. Ribillard</aut>,
        <vrk>Vagado sub palmoj</vrk>,
        <lok>Rotterdam, 1956</lok>
      </fnt>.
    </ekz>
    <trd lng="en">to clear from impediment</trd>
    <trd lng="de">kl&auml;ren</trd>
    <trd lng="es">aclarar</trd>
    <trdgrp lng="he">
      <trd>&lamed;<ind>&kaf;&ayin;&kaf;&ayin;</ind></trd>,
      <trd>&lamed;<ind>&kaf;&het;&kaf;&het;</ind></trd>,
      <trd>&lamed;<ind>&het;&kaf;&fkaf;</ind></trd>
    </trdgrp>
    <trd lng="hu">k&ouml;sz&ouml;r&uuml;li <klr tip="ind">(a
    tork&aacute;t)</klr></trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="klar.0igi.komprenebligi" ref="klar.0a.komprenebla">
    <ekz>
      <tld/>igi la uzadon de la vortaro
      <fnt>
        <bib>FK</bib>
      </fnt>;
    </ekz>
    <ekz>
      mi <tld/>igos tion &ccirc;i per ekzemplo
      <fnt>
        <bib>FK</bib>
      </fnt>;
    </ekz>
    <ekz>
      mi turnas la atenton <klr>[&#8230;]</klr> al la <tld/>igita punkto
      <fnt>
        <bib>FK</bib>
      </fnt>;
    </ekz>
    <ekz>
      li signifo<tld/>igis al ni niajn son&gcirc;ojn, al &ccirc;iu li <tld/>igis la&ubreve; lia son&gcirc;o
      <fnt>
        <bib>MT</bib>
        <lok>Genezo 41:12</lok>
      </fnt>;
    </ekz>
    <ekz>
      ne<tld/>igebla malamikeco
      <fnt>
        <bib>FK</bib>
      </fnt>;
    </ekz>
    <ekz>
      ni reciproke nin konas, <tld/>igon ne bezonas
      <fnt>
        <bib>PrV</bib>
      </fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="ekspli.0i">ekspliki</ref>,
      <ref cel="koment.0i">komenti</ref>,
      <ref cel="kompre.0igi">komprenigi</ref>,
      <ref cel="volv.mal0i">malvolvi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_l;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_t;&c_l;&c_u;&c_m;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_t;&c_l;&c_u;&c_m;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>erkl&auml;ren</trd>,
      <trd>erl&auml;utern</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>to explain</trd>,
      <trd>to clarify</trd>,
      <trd>to make evident</trd>,
      <trd>to elucidate</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>explicar</trd>,
      <trd>clarificar</trd>,
      <trd>aclarar</trd>
    </trdgrp>
    <trdgrp lng="he">
      <trd>&lamed;&he;&samekh;&bet;&yod;&resh; \<ind>&samekh;&bet;&resh;</ind>\</trd>,
      <trd>&lamed;&he;&bet;&he;&yod;&resh; \<ind>&bet;&he;&resh;</ind>\</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>tiszt&aacute;z</trd>,
      <trd>magyar&aacute;z</trd>,
      <trd>felvil&aacute;gos&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>verduidelijken</trd>,
      <trd>duidelijk maken</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wyja&sacute;nia&cacute;</trd>,
      <trd>obja&sacute;nia&cacute;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_malmol;&c_ja;&c_s;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_ja;&c_s;&c_n;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_ja;&c_s;&c_n;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">aclarir</trd>
  <trdgrp lng="cs">
    <trd>klarifikovat</trd>,
    <trd>objas&ncaron;ovat</trd>,
    <trd>ujas&ncaron;ovat</trd>,
    <trd>vykl&aacute;dat</trd>,
    <trd>vysv&ecaron;tlovat</trd>
  </trdgrp>
  <trd lng="es">aclarar</trd>
  <trdgrp lng="fr">
    <trd>clarifier</trd>,
    <trd>&eacute;claircir</trd>,
    <trd>&eacute;clairer<klr tip="ind"> (expliquer)</klr></trd>,
    <trd>expliquer</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>objas&ncaron;ova&tcaron;</trd>,
    <trd>vysvet&lcaron;ova&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rklara</trd>
</drv>

<drv mrk="klar.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="klar.mal0a.konfuza">
    <dif>
      <ref cel="konfuz.0a">Konfuza</ref> por la okulo, 
      orelo a&ubreve; spirito:
      <ekz>
        mal<tld/>aj okuloj
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 23:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> vidante lian palan viza&gcirc;on kaj mal<tld/>an rigardon, ekkriis kun
        timo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a fonto
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 25:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne volis trinki la vinon, &ccirc;ar &gcirc;i enhavis en si ian suspektan mal<tld/>a&jcirc;on
        <fnt>
          <bib>F</bib>
          <lok>&FE; 35</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; vino li enver&scirc;is en mian glason ian malagrablan acida&jcirc;on, kaj tiun &ccirc;i
        mal<tld/>an fluida&jcirc;on li devigis min eltrinki
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        akvaj fluegoj <klr>[&#8230;]</klr> mal<tld/>aj pro glacio
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 6:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        iliaj oreloj a&ubreve;das mal<tld/>e
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 13:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        lingvo mal<tld/>a kaj nedistingebla
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 33:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maljunulino <klr>[&#8230;]</klr> havis mal<tld/>an vidon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ha&ccirc;jo kaj Grenjo</lok>
        </fnt>;
      </ekz>
      <ekz>
        de la centro de la urbo flugis mal<tld/>a bruo, mutigata de la interspaco
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la vo&ccirc;oj en la komenco mal<tld/>aj, fari&gcirc;is pli kaj pli fortaj, kaj oni povis
        jam a&ubreve;di &ccirc;iun vorton
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;agreno mal<tld/>igis la prudenton de la elpelitaj soldatoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        leteroj <klr>[&#8230;]</klr> kun sufi&ccirc;e mal<tld/>aj adresoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en akvo mal<tld/>a oni fi&scirc;kaptas facile
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>mal<tld/>igi</ind> rivereton.
        <trdgrp lng="ca">
          <trd>atorrollar</trd>,
          <trd>contorbar</trd>
        </trdgrp>
        <trdgrp lng="en">
          <trd>to obscure</trd>,
          <trd>confound</trd>
        </trdgrp>
        <trdgrp lng="es">
          <trd>enturbiar</trd>,
          <trd>oscurecer</trd>,
          <trd>obscurecer</trd>,
          <trd>enredar</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>troubler<klr tip="ind"> (rendre trouble)</klr></trd>,
          <trd>brouiller</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="enigm.0a">enigma</ref>,
      <ref cel="hierog.0o">hieroglifa</ref>,
      <ref cel="mister.0a">mistera</ref>,
      <ref cel="nebul.0a">nebula</ref>,
      <ref cel="nub.0a">nuba</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_ja;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_c;&c_mol;&c_m;&c_ja;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_m;&c_u;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>fosc</trd>,
    <trd>brut</trd>,
    <trd>dif&uacute;s</trd>,
    <trd>misteri&oacute;s</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kaln&yacute;</trd>,
    <trd>nejasn&yacute;</trd>,
    <trd>nez&rcaron;eteln&yacute;</trd>,
    <trd>temn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unklar</trd>,
    <trd>tr&uuml;be</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>obscure</trd>,
    <trd>unclear</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>oscuro</trd>,
    <trd>turbio</trd>,
    <trd>confuso</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>trouble<klr tip="ind"> (adj.)</klr></trd>,
    <trd>brumeux<klr tip="ind"> (embrouill&eacute;)</klr></trd>,
    <trd>confus</trd>,
    <trd>embrouill&eacute;</trd>,
    <trd>flou<klr tip="ind"> (adj.)</klr></trd>,
    <trd>vague<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&mem;&ayin;&vav;&resh;&pe;&lamed;</trd>,
    <trd>&ayin;&kaf;&vav;&resh;</trd>,
    <trd>&samekh;&tav;&vav;&fmem;</trd>
  </trdgrp>
  <trdgrp lng="hu">
