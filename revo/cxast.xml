<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&ccirc;ast</rad>/a
</kap>
<drv mrk="cxast.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="cxast.0a.more">
    <dif>
      Pura, senpeka pri seksaj aferoj:
      <ekz>
        mi estas <tld/>a knabino, ne virino malpura
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 14a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a konduto.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>keusch</trd>,
      <trd>z&uuml;chtig</trd>,
      <trd>unbefleckt</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>sz&udblac;zies</trd>,
      <trd>romlatlan</trd>,
      <trd>er&eacute;nyes</trd>
    </trdgrp>    
    <trdgrp lng="pl">
      <trd>czysty</trd>,
      <trd>niewinny</trd>,
      <trd>dziewiczy</trd>
    </trdgrp>
  </snc>
  <snc mrk="cxast.0a.cxastiga">
    <dif>
      Ne ekscitanta voluptajn dezirojn:
      <ekz>
        <tld/>a romano.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dec.0a">deca</ref>,
      <ref cel="virg.0a">virga</ref>,
      <ref cel="virt.0a">virta</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>rein</trd>,
      <trd>sittsam</trd>
    </trdgrp>
    <trd lng="hu">erk&ouml;lcs&ouml;s</trd>    
    <trdgrp lng="pl">
      <trd>przyzwoity</trd>,
      <trd>pruderyjny</trd>
    </trdgrp>
  </snc>
  <trd lng="en">chaste</trd>
  <trd lng="es">casto</trd>
  <trd lng="fr">chaste</trd>
  <trd lng="it">casto</trd>
  <trd lng="pt">casto</trd>
  <trd lng="nl">kuis</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_l;&c_o;&c_m;&c_u;&c_d;&c_r;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_i;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_o;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">kysk</trd>
</drv>

<drv mrk="cxast.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Virto, sinteno de iu evitanta seksan dibo&ccirc;on a&ubreve;
      agadon:
      <ekz>
        ni promesis al ni reciproke fidelecon en <tld/>eco kaj honesteco
        <fnt><bib>Fab3</bib>, <lok>&scirc;i estis tute
        senta&ubreve;ga</lok>
</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pudor.0o">pudoro</ref>
    </refgrp>
  </snc>
  <trd
  lng="bg">&c_c;&c_je;&c_l;&c_o;&c_m;&c_malmol;&c_d;&c_r;&c_i;&c_je;</trd>
  <trd lng="de">Keuschheit</trd>
  <trd lng="fr">chastet&eacute;</trd>
  <trdgrp lng="hu">
    <trd>sz&udblac;zess&eacute;g</trd>,
    <trd>sz&udblac;ziess&eacute;g</trd>
    <trd>romlatlans&aacute;g</trd>
  </trdgrp>
  <trd lng="it">castit&agrave;</trd>
  <trdgrp lng="pl">
    <trd>czysto&sacute;&cacute;</trd>,
    <trd>niewinno&sacute;&cacute;</trd>,
    <trd>przyzwoito&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_l;&c_o;&c_m;&c_u;&c_d;&c_r;&c_i;&c_je;</trd>,
    <trd>&c_c;&c_je;&c_l;&c_o;&c_m;&c_u;&c_d;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_i;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_o;&c_r;&c_o;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="cxast.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Voluptama, <ref tip="super" cel="dibocx.0a">dibo&ccirc;a</ref>
      a&ubreve;
      malpudora pri seksaj aferoj:
      <ekz>
        virinon mal<tld/>an a&ubreve; senhonorigitan ili ne prenu
        <fnt><bib>MT</bib>, <lok>&Lev; 21:7</lok></fnt>;
      </ekz>
      <ekz>
        iliaj blasfemaj kaj mal<tld/>aj paroloj en la gastejo
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 1a,
        renkonto</lok>
</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;bauch&eacute;</trd>,
    <trd>dissolu<klr tip="amb"> (d&eacute;bauch&eacute;)</klr></trd>,
    <trd>lascif</trd>,
    <trd>libertin<klr tip="amb"> (adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>par&aacute;zna</trd>,
    <trd>feslett</trd>,
    <trd>romlott</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>impudico</trd>,
    <trd>dissoluto</trd>,
    <trd>impuro<klr> (moralmente)</klr></trd>,
    <trd>immorale</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>nieczysty</trd>,
    <trd>nieprzywoity</trd>,
    <trd>wyuzdany</trd>,
    <trd>rozpustny</trd>,
    <trd>bezpruderyjny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_shch;&c_jo;&c_n;&c_n;&c_y;&c_j;
</trd>,
    <trd>&c_p;&c_o;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_t;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="cxast.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Mal<tld/>a ago, kontra&ubreve;mora seksumado:
      <ekz>
        de interne, el la koro de homoj, eliras malvirtaj pensoj,
        mal<tld/>a&jcirc;oj
        <fnt><bib>NT</bib>, <lok>&Mar; 7:21</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;bauche<klr tip="amb"> (acte)</klr></trd>,
    <trd>d&eacute;pravation<klr tip="amb"> (acte)</klr></trd>,
    <trd>impudeur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>par&aacute;znas&aacute;g</trd>,
    <trd>bujas&aacute;g</trd>
    <trd>kicsapong&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>atto <ind>immorale</ind><klr> (sessuale)</klr></trd>, 
    <trd>impudicizia<klr> (atto)</klr></trd>,
    <trd>sconcezza<klr> (atto)</klr></trd>    
  </trdgrp>  
  <trdgrp lng="pl">
    <trd>rozpusta</trd>,
    <trd>wyuzdanie</trd>,
    <trd>deprawacja</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_t;</trd>
</drv>

<drv mrk="cxast.mal0eco">
  <kap>mal<tld/>eco</kap>
  <snc>
    <dif>
      Vivado, sinteno sekse dibo&ccirc;a:
      <ekz>
        la piecon anstata&ubreve;is la lukso, la sa&gcirc;on la
        mal<tld/>eco
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;bauche<klr tip="amb"> (qualit&eacute;)</klr></trd>,
    <trd>d&eacute;pravation<klr tip="amb"> (qualit&eacute;)</klr></trd>,
    <trd>impudicit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fesletts&eacute;g</trd>,
    <trd>romlotts&aacute;g</trd>,
    <trd>erk&ouml;lcstelens&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>immoralit&agrave;</trd>, 
    <trd>impudicizia</trd>,
    <trd>dissolutezza</trd>    
  </trdgrp> 
  <trdgrp lng="pl">
    <trd>rozpusta</trd>,
    <trd>wyuzdanie</trd>,
    <trd>deprawacja</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_t;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_shch;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="cxast.mal0ejo">
  <kap>mal<tld/>ejo</kap>
  <snc>
    <ref tip="dif" cel="bordel.0o">bordelo</ref>
    <ekz>
      ili adultas kaj iras amase en mal<tld/>ejojn
      <fnt><bib>MT</bib>, <lok>&Jer; 5:7</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="de">
    <trd>Puff</trd>,
    <trd>Bordell</trd>,
    <trd>Lasterh&ouml;hle</trd>
  </trdgrp>
  <trd lng="es">burdel</trd>
  <trdgrp lng="fr">
    <trd>bordel<klr tip="ind"> (maison close)</klr></trd>,
    <trd>claque<klr tip="ind"> (maison close)</klr></trd>,
    <trd>maison close</trd>,
    <trd>maison de prostitution</trd>,
    <trd>lupanar</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>bord&eacute;lyh&aacute;z</trd>,
    <trd>nyilv&aacute;nosh&aacute;z</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>lupanare</trd>, 
    <trd>postribolo</trd>, 
    <trd>casa di piacere</trd>, 
    <trd>casa chiusa</trd>,
    <trd>bordello</trd>
  </trdgrp>
  <trd lng="nl">bordeel</trd>
  <trdgrp lng="pl">
    <trd>burdel</trd>,
    <trd>dom publiczny</trd>,
    <trd>dom schadzek</trd>,
    <trd>miejsce <ind>rozpusty</ind><klr> (obraz.)</klr></trd>
  </trdgrp>
  <trd lng="pt">bordel</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_b;&c_l;&c_i;&c_ch;&c_n;&c_y;&c_j;
    &c_d;&c_o;&c_m;</trd>,
    <trd>&c_b;&c_o;&c_r;&c_d;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_g;&c_n;&c_je;&c_z;&c_d;&c_o;
    <ind>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_t;&c_a;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="cxast.mal0i">
  <kap>mal<tld/>i</kap>
  <snc>
    <dif>
      Sekse dibo&ccirc;i, mal<tld/>e konduti.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>unz&uuml;chtig</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>d&eacute;baucher</ind></trd>,
    <trd>se <ind>d&eacute;vergonder</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>par&aacute;zn&aacute;lkodik</trd>,
    <trd>buj&aacute;lkodik</trd>
  </trdgrp>
  <trd lng="it">fornicare</trd>
  <trd lng="nl">onkuisheid bedrijven</trd>
  <trdgrp lng="pl">
    <trd>by&cacute; <ind>rozpustnym</ind></trd>,
    <trd>by&cacute; <ind>wyuzdanym</ind></trd>,
    <trd>by&cacute; <ind>zdeprawowanym</ind></trd>,
    <trd>by&cacute; <ind>nieprzywoitym</ind></trd>,
    <trd>by&cacute; <ind>bezpruderyjnym</ind></trd>,
    <trd>prostytuowa&cacute; si&eogonek;</trd>,
    <trd>kurwi&cacute; si&eogonek;<klr> (obra&zacute;l.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_t;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_t;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    &c_p;&c_o;&c_r;&c_o;&c_k;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="cxast.mal0isto">
  <kap>mal<tld/>isto</kap>
  <snc>
    <dif>
      Viro, kiu vivtenas sin amorante kontra&ubreve; pago:
      <ekz>
        ne devas esti mal<tld/>isto inter la filoj de Izrael
        <fnt><bib>MT</bib>, <lok>&Rea; 23:17</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>prostitu&eacute;</trd>,
    <trd>gigolo</trd>,
    <trd>tapin<klr tip="ind"> (prostitu&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dzsigol&oacute;</trd>,
    <trd>selyemfi&uacute;</trd>,
    <trd>prostitu&aacute;lt</trd>
  </trdgrp>
  <trd lng="it">gigol&ograve;</trd>
  <trdgrp lng="pl">
    <trd>&zdot;igolo</trd>,
    <trd>m&eogonek;ska dziwka<klr> (obra&zacute;l.)</klr></trd>,
    <trd>m&eogonek;ska prostytutka</trd>,
    <trd>m&eogonek;ska kurwa<klr> (obra&zacute;l.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_g;&c_o;&c_l;&c_o;</trd>,
    <trd>&c_a;&c_l;&c_mol;&c_f;&c_o;&c_n;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="cxast.mal0istino">
  <kap>mal<tld/>istino</kap>
  <snc>
    <dif>
      Virino, kiu vivtenas sin amorante kontra&ubreve; pago:
      <ekz>
    la impostistoj kaj mal<tld/>istinoj iras anta&ubreve; vi en la
    regnon de 
    Dio
        <fnt><bib>NT</bib>, <lok>&Mat; 21:31</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd><ind>prostitu&eacute;</ind>e</trd>,
    <trd>fille publique</trd>,
    <trd>fille de <ind>joie</ind></trd>,
    <trd>grue</trd>,
    <trd>p&eacute;ripat&eacute;ticienne</trd>,
    <trd>putain</trd>,
    <trd>tapineuse</trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>utcal&aacute;ny</trd>,
    <trd>&ouml;r&ouml;ml&aacute;ny</trd>,
    <trd>k&eacute;jn&odblac;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dziwka<klr> (obra&zacute;l.)</klr></trd>,
    <trd>prostytutka</trd>,
    <trd>kurwa<klr> (obra&zacute;l.)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>prostituta</trd>,
    <trd>puttana</trd>,
    <trd>meretrice</trd>,
    <trd>marchettara</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_i;&c_t;&c_u;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_u;&c_t;&c_a;&c_n;&c_a;</trd>
  </trdgrp>
</drv>
</art>
