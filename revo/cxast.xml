<drv mrk="cxast.mal0eco">
  <kap>mal<tld/>eco</kap>
  <snc mrk="cxast.mal0eco.sinteno">
    <dif>
      Vivado, sinteno sekse dibo&ccirc;a:
      <ekz>
        via bofilino, mal<tld/>i&gcirc;is, kaj &scirc;i e&ccirc; gravedi&gcirc;is de
        mal<tld/>eco
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 38:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        la piecon anstata&ubreve;is la lukso, la sa&gcirc;on la
        mal<tld/>eco
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Unz&uuml;chtigkeit</trd>,
    <trd>Unkeuschheit</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>inmoralidad</trd>,
    <trd>impudicia</trd>,
    <trd>lujuria</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;bauche<klr tip="amb"> (qualit&eacute;)</klr></trd>,
    <trd>d&eacute;pravation<klr tip="amb"> (qualit&eacute;)</klr></trd>,
    <trd>impudicit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fesletts&eacute;g</trd>,
    <trd>romlotts&aacute;g</trd>,
    <trd>erk&ouml;lcstelens&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>immoralit&agrave;</trd>,
    <trd>impudicizia</trd>,
    <trd>dissolutezza</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozpusta</trd>,
    <trd>wyuzdanie</trd>,
    <trd>deprawacja</trd>
  </trdgrp>
  <trdgrp lng="pt">
  <trd>deprava&ccedil;&atilde;o</trd>
  <trd>imoralidade</trd>
  <trd>impudic&iacute;cia</trd>
  <trd>indec&ecirc;ncia</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_t;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_shch;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_z;&#x43F;&c_u;&c_s;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&c_u;&c_t;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
  <trd lng="zh">&#x597D;&#x8272; <pr>h&#x1CE;os&#xE8;</pr></trd>
</drv>

<drv mrk="cxast.mal0ejo">
  <kap>mal<tld/>ejo</kap>
  <snc mrk="cxast.mal0ejo.bordelo">
    <dif>
      <ref tip="dif" cel="bordel.0o">Bordelo</ref>:
      <ekz>
        ili adultas kaj iras amase en mal<tld/>ejojn
        <fnt><bib>MT</bib>, <lok>&Jer; 5:7</lok></fnt>;
      </ekz>
      <ekz>
        vi aran&gcirc;is viajn mal<tld/>ejojn &ccirc;e la komenco de &ccirc;iu vojo kaj
        faris viajn
        fialta&jcirc;ojn sur &ccirc;iu strato, vi estis ne kiel mal<tld/>istino, kiu
        &scirc;atas
        donacojn
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 16:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la mal<tld/>ejo, tio estas en la domo de la cezaro
        <fnt>
          <vrk><url ref="&EduInf;lern/librejo/0002/0006.php">Quo
          Vadis</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Puff</trd>,
    <trd>Bordell</trd>,
    <trd>Lasterh&ouml;hle</trd>
  </trdgrp>
