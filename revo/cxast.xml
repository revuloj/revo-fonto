<drv mrk="cxast.mal0ajxo">
  <kap>mal<tld/>a&jcirc;o</kap>
  <snc mrk="cxast.mal0ajxo.seksumado">
    <dif>
      Mal<tld/>a ago, kontra&ubreve;mora seksumado:
      <ekz>
        ili faris mal<tld/>a&jcirc;on kaj malnobla&jcirc;on
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 20:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        de interne, el la koro de homoj, eliras malvirtaj pensoj,
        mal<tld/>a&jcirc;oj
        <fnt><bib>NT</bib>, <lok>&Mar; 7:21</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Unzucht</trd>
  <trdgrp lng="es">
    <trd>inmoralidad</trd>,
    <trd>depravaci&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;bauche<klr tip="amb"> (acte)</klr></trd>,
    <trd>d&eacute;pravation<klr tip="amb"> (acte)</klr></trd>,
    <trd>impudeur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>par&aacute;znas&aacute;g</trd>,
    <trd>bujas&aacute;g</trd>,
    <trd>kicsapong&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>atto <ind>immorale</ind><klr> (sessuale)</klr></trd>,
    <trd>impudicizia<klr> (atto)</klr></trd>,
    <trd>sconcezza<klr> (atto)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x307F;&#x3060;&#x3089;&#x306A;&#x884C;&#x70BA;  <pr>&#x307F;&#x3060;&#x3089;&#x306A;&#x3053;&#x3046;&#x3044;</pr></trd>
  <trdgrp lng="pl">
    <trd>rozpusta</trd>,
    <trd>wyuzdanie</trd>,
    <trd>deprawacja</trd>
  </trdgrp>
  <trdgrp lng="pt">
  <trd>deprava&ccedil;&atilde;o</trd>
  <trd>imoralidade</trd>
  <trd>impudic&iacute;cia</trd>
  <trd>indec&ecirc;ncia</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_z;&#x43F;&c_u;&c_s;&#x43D;&c_i;&c_j; &c_v;&c_ch;&c_i;&#x43D;&#x43E;&#x43A;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&c_u;&c_t;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&c_u;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
