<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: simi.xml,v 1.30 2021/01/03 08:10:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>simi</rad>/o
</kap>
<drv mrk="simi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Iu el <ref tip="malprt" cel="simi.0oj"><tld/>oj</ref>,
      <ref tip="lst" cel="mam.0ulo" lst="voko:simioj">mamulo</ref> 
      kun pli malpli rekta teni&gcirc;o, nudaj man- kaj piedplatoj,
      viza&gcirc;o kaj oreloj kaj prenkapablaj 
      <ref tip="prt" cel="man.0o">manoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_p;&c_a;</trd>
  <trd lng="bg">&c_m;&c_a;&c_j;&c_m;&c_u;&c_n;&c_a;</trd>
  <trd lng="bo">&#3942;&#4004;&#4018;&#3962;&#3939;&#3851;</trd>
  <trd lng="br">marmouz</trd>
  <trd lng="ms">beruk</trd>
  <trdgrp lng="ca">
    <trd>simi</trd>,
    <trd>mona, mico</trd>
  </trdgrp>
  <trd lng="cs">opice</trd>
  <trd lng="de">Affe</trd>
  <trdgrp lng="en">
     <trd>monkey</trd>,
     <trd>ape</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>simio</trd>,
    <trd>mono</trd>
  </trdgrp>
  <trd lng="fr">singe</trd>
  <trd lng="hu">majom</trd>
  <trdgrp lng="id">
    <trd>monyet</trd>,
    <trd>kera</trd>
  </trdgrp>
  <trd lng="it">scimmia</trd>
  <trd lng="nl">aap</trd>
  <trd lng="pl">ma&lstroke;pa</trd>
  <trd lng="ru">&c_o;&c_b;&c_je;&c_z;&c_mol;&c_ja;&c_n;&c_a;</trd>
  <trd lng="sk">opica</trd>
</drv>

<drv mrk="simi.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="malprt" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Primates</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_m;&c_a;&c_t;&c_y;</trd>
  <trd lng="ca">primats</trd>
  <trd lng="cs">vy&scaron;&scaron;&iacute; prim&aacute;ti</trd>
  <trd lng="de">Herrentiere</trd>
  <trd lng="en">primates</trd>
  <trd lng="fr">primates</trd>
  <trd lng="hu">f&odblac;eml&odblac;s&ouml;k</trd>
  <trd lng="id">primata</trd>
  <trd lng="it">primati</trd>
  <trd lng="pl">naczelne<klr tip="amb"> (rz&aogonek;d)</klr></trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_m;&c_a;&c_t;&c_y;<klr tip="amb"> (&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trdgrp lng="sk">
    <trd>prim&aacute;ty</trd>,
    <trd>vy&scaron;&scaron;&iacute; prim&aacute;ti</trd>
  </trdgrp>
</drv>

<drv mrk="simi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kvaza&ubreve; <tld/>o imiti:
      <ekz>
        mallerte <tld/>i nobelan sintenon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_p;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">imitar</trd>
  <trdgrp lng="cs">
    <trd>napodobovat</trd>,
    <trd>opi&ccaron;it se</trd>
  </trdgrp>
  <trd lng="de">nach&auml;ffen</trd>
  <trd lng="en">ape</trd>
  <trd lng="fr">singer</trd>
  <trd lng="hu">majmol</trd>
  <trd lng="it">scimmiottare</trd>
  <trd lng="nl">na-apen</trd>
  <trd lng="pl">ma&lstroke;powa&cacute;</trd>
  <trd lng="ru">&c_o;&c_b;&c_je;&c_z;&c_mol;&c_ja;&c_n;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>napodob&ncaron;ova&tcaron;</trd>,
    <trd>opi&ccaron;i&tcaron; sa</trd>
  </trdgrp>
</drv>
</art>
