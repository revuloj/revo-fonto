<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>simi</rad>/o
</kap>
<drv mrk="simi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Iu el <ref tip="malprt" cel="simi.0oj"><tld/>oj</ref>,
      mamulo kun pli malpli rekta teni&gcirc;o, nudaj man- kaj piedplatoj,
      viza&gcirc;o kaj oreloj kaj prenkapablaj manoj.     
    </dif>
  </snc>
  <trd lng="de">Affe</trd>
  <trd lng="en">monkey</trd>
  <trd lng="nl">aap</trd>
</drv>
<drv mrk="simi.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Primates</trd>).
    </dif>
  </snc>
  <trd lng="de">Herrentiere</trd>
</drv>
<drv mrk="simi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kvaza&ubreve; <tld/>o imiti:
      <ekz>
        mallerte <tld/>i nobelan sintenon.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">naapen</trd>
</drv>
</art>

