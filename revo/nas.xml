<kap>
  <ofc>4</ofc>
  <rad>nas</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="nas.0o">
  <kap><tld/>o</kap>
  <snc mrk="nas.0o.CXAS">
    <dif>
      Konusforma <ref tip="super" cel="korb.0o">korbo</ref> el vergetoj a&ubreve; aliaj stangetoj
      por kapti fi&scirc;ojn:
      <ekz>
        ni iru kaj metu niajn <tld/>etojn en la lagon, kaj
        morga&ubreve; ni havos bongustan matenman&gcirc;on
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="sak.ret0o.kaptilo">retsako<sncref/></ref>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_ch;</trd>
  <trd lng="br">kavell<klr tip="ind"> (pesketa)</klr></trd>
  <trd lng="ca">nansa</trd>
  <trd lng="cs">keser</trd>
  <trdgrp lng="de">
     <trd>Reuse</trd>,
     <trd>Fischreuse</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>eel trap</trd>,
    <trd>fish trap</trd>,
    <trd>lobster trap</trd>,
    <trd>fishing basket</trd>,
    <trd>eel buck</trd>,
    <trd>eel pot</trd>,
    <trd>crayfish net</trd>,
    <trd>hoop net</trd>
  </trdgrp>
  <trd lng="fr">nasse</trd>
  <trd lng="hu">varsa</trd>
  <trd lng="ja">&#x3046;&#x3051;</trd>
  <trd lng="nl">fuik</trd>
  <trdgrp lng="pl">
    <trd>wi&eogonek;cierz</trd>,
    <trd>were&zdot;ka</trd>
  </trdgrp>
  <trd lng="pt">nassa</trd>
  <trd lng="ro">v&acirc;r&scomma;e </trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_sh;&c_a; <klr>(&c_r;&c_y;&c_b;&c_o;&c_l;&c_o;&c_v;&c_n;&c_o;&c_je;
  &c_o;&c_r;&c_u;&c_d;&c_i;&c_je;)</klr></trd>
  <trd lng="sk">vr&scaron;a <klr>(ryba)</klr></trd>
  <trd lng="sv">ryssja</trd>
  <trd lng="uk">&c_v;&c_je;&c_r;&c_sh;&c_a; (&c_r;&c_i;&c_b;&c_a;&c_l;&c_mol;&c_s;&c_mol;&c_k;&c_a;)</trd>
  <trdgrp lng="zh">
    <trd>&#x518D;&#x6B21;&#x4F7F;&#x7528; <pr>z&#xE0;ic&#xEC;sh&#x1D0;y&#xF2;ng</pr></trd>,
    <trd>&#x6C34;&#x67F5; <pr>shu&#x1D0;zh&#xE0;</pr></trd>,
    <trd>&#x6C34;&#x6805; <pr>shu&#x1D0;zh&#xE0;</pr></trd>,
    <trd>&#x6C55; <pr>sh&#xE0;n</pr></trd>,
    <trd>&#x9B5A;&#x7C43; <pr>y&#xFA;l&#xE1;n</pr></trd>,
    <trd>&#x9C7C;&#x7BEE; <pr>y&#xFA;l&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>
