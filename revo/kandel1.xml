<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>kandelabr</rad>/o
</kap>
<drv mrk="kandel1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Luksa plurbran&ccirc;a <ref tip="super"
      cel="kandel.0ingo">kandelingo</ref>:
      <ekz>
        sur bronzaj postamentoj orbrilantaj <tld/>oj etendis siajn
        bran&ccirc;formajn brakojn
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_d;&c_e;&c_l;&c_ja;&c_b;&c_r;</trd>
  <trd lng="de">Kronleuchter</trd>
  <trd lng="en">candelabrum</trd>
  <trd lng="es">candelabro</trd>
  <trd lng="fa">&fa_sxin;&fa_mim;&fa_ein;&fa_dal;&fa_alef;&fa_nun;<klr
  tip="amb">
  (&fa_cxe;&fa_nun;&fa_dal;&fa_sxin;&fa_alef;&fa_hxe;&fa_heh;)</klr></trd>
  <trd lng="fr">cand&eacute;labre</trd>
  <trdgrp lng="hu">
    <trd>kandel&aacute;ber</trd>,
    <trd>karos <ind>gyertyatart&oacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kandelaber</trd>,
    <trd>armkandelaar</trd>,
    <trd>kroonluchter</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_d;&c_je;&c_l;&c_ja;&c_b;&c_r;</trd>
</drv>
</art>
