  <trd lng="es">queja</trd>
  <trd lng="fr">plainte</trd>
  <trd lng="hu">panasz</trd>
  <trd lng="id"><baz>keluh</baz>keluhan</trd>
  <trdgrp lng="ja">
    <trd>&#x82E6;&#x75DB;&#x306E;&#x8A34;&#x3048;  <pr>&#x304F;&#x3064;&#x3046;&#x306E;&#x3046;&#x3063;&#x305F;&#x3048;</pr></trd>,
    <trd>&#x4E0D;&#x5E73;  <pr>&#x3075;&#x3078;&#x3044;</pr></trd>,
