  </trdgrp>
  <trd lng="ru">&c_zh;&c_a;&c_l;&c_o;&c_b;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>ponosa</trd>,
    <trd>s&tcaron;a&zcaron;nos&tcaron;</trd>,
    <trd>&zcaron;aloba</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_r;&c_ib;&c_k;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_r;&c_g;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6295;&#x8A34; <pr>t&oacute;us&ugrave;</pr></trd>,
    <trd>&#x6295;&#x8BC9; <pr>t&oacute;us&ugrave;</pr></trd>,
    <trd>&#x6028;&#x8A00; <pr>yu&agrave;ny&aacute;n</pr></trd>,
    <trd>&#x8036;&#x5229;&#x7C73;&#x54C0;&#x6B4C; <pr>y&emacron;l&igrave;m&#x1D0;&amacron;ig&emacron;</pr></trd>,
    <trd>&#x7533;&#x8A34; <pr>sh&emacron;ns&ugrave;</pr></trd>,
    <trd>&#x7533;&#x8BC9; <pr>sh&emacron;ns&ugrave;</pr></trd>,
