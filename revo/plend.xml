    <trd>&c_s;&c_k;&c_a;&c_r;&c_zh;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_s;&c_k;&c_a;&c_r;&c_g;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x55D4;&#x6028; <pr>ch&emacron;nyu&agrave;n</pr></trd>,
