<art mrk="$Id: babiru.xml,v 1.23 2022/04/26 15:00:32 revo Exp $">
<kap><rad>babirus</rad>/o</kap>
<drv mrk="babiru.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="babiru.0o.genro">
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> kun sola specio el familio      
      <ref tip="malprt" cel="porked.0oj">porkedoj</ref>
      (<trd lng="la">Babyrousa</trd>), el Celebeso kaj najbaraj insuloj,
      kun longaj kurbaj kojnodentoj trairantaj la rostran nazon:
      <ekz>
        la <tld/>o vivas en densaj &gcirc;angaloj, &ccirc;asas nokte, kuras rapide,
        na&gcirc;as facile, &gcirc;i nazfosas en grundo de riverbordoj kaj mar&ccirc;oj 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=228538">Babiruso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/c/cd/Hirscheber1a.jpg/320px-Hirscheber1a.jpg" prm="CC BY-SA 2.0 de"><tld/>o (Babyrousa celebensis)
        <fnt>
          <aut>Masteraah, propra verko</aut>
          <lok><url ref="&WCW;/File:Hirscheber1a.jpg">&WCW;/File:Hirscheber1a.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_b;&c_ib;&c_r;&c_u;&c_s;&c_a;</trd>
  <trd lng="ca">babirussa</trd>
  <trd lng="de">Hirscheber</trd>
  <trdgrp lng="en">
    <trd>babiroussa</trd>,
    <trd>babirussa</trd>,
    <trd>babirusa</trd>
  </trdgrp>
  <trd lng="fa">&a_kh;&a_w;&f_k;&zwnj;&a_A_madda;&a_h;&a_w;</trd>
  <trd lng="fr">babiroussa</trd>
  <trd lng="he">&#x5DE;&#x5D9;&#x5DF; &#x5D7;&#x5D6;&#x5D9;&#x5E8;</trd>
  <trd lng="hu">babirussza</trd>
  <trd lng="it">babirussa</trd>
  <trdgrp lng="nl">
    <trd>hertzwijn</trd>,
    <trd>babiroesa</trd>
  </trdgrp>
  <trd lng="pl">babirusa</trd>
  <trd lng="ru">&c_b;&c_a;&c_b;&c_i;&c_r;&c_u;&c_s;&c_s;&c_a;</trd>
  <trdgrp lng="sv">
    <trd>hjortsvin</trd>,
    <trd>babyrousa</trd>,
    <trd>babirusa</trd>
  </trdgrp>
  <trd lng="uk">&#x431;&#x430;&#x431;&#x456;&#x440;&#x443;&#x441;&#x430;</trd>
</drv>
</art>
