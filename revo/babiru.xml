<art mrk="$Id$">
<kap><rad>babirus</rad>/o</kap>
<drv mrk="babiru.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="porked.0oj">porkedoj</ref>
      (<trd lng="la">Babyrousa</trd>), el Celebeso kaj najbaraj insuloj.
    </dif>
  </snc>
  <trd lng="de">Hirscheber</trd>
  <trdgrp lng="en">
    <trd>babiroussa</trd>,
    <trd>babirussa</trd>,
    <trd>babirusa</trd>
  </trdgrp>
  <trd lng="fr">babiroussa</trd>
  <trd lng="hu">babirussza</trd>
  <trdgrp lng="nl">
    <trd>hertzwijn</trd>,
    <trd>babiroesa</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_b;&c_i;&c_r;&c_u;&c_s;&c_s;&c_a;</trd>
</drv>
</art>
