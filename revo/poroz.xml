<drv mrk="poroz.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Porori&ccirc;a, <ref tip="ant" cel="por1.0a">poreca</ref>:
      <ekz>
        gisa&jcirc;o, <tld/>a, plej ofte fendita kaj etvalora
        <fnt>
          <bib>VRA</bib>
          <lok>Erik Asklund</lok>
        </fnt>;
      </ekz>
      <ekz>
        pumiko estas tre trua, <tld/>a, &scirc;a&ubreve;msimila vulkana vitro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=85236">Pumiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">por&ouml;s</trd>
    <trd lng="en">porous</trd>
    <trd lng="fr">poreux</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_ib;&c_t;&c_a;&c_v;&c_a;&c_t;&c_y;</trd>,
    <trd>&c_p;&c_o;&c_r;&c_y;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x80FD;&#x6E17;&#x900F; <pr>n&#xE9;ngsh&#xE8;nt&#xF2;u</pr></trd>,
    <trd>&#x80FD;&#x6EF2;&#x900F; <pr>n&#xE9;ngsh&#xE8;nt&#xF2;u</pr></trd>
  </trdgrp>
</drv>

