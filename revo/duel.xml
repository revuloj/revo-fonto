<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>duel</rad>/o
</kap>
<drv mrk="duel.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Anta&ubreve;konsentita batalo inter du personoj pro privata malpaco:
      <ekz>
        inviti iun al <tld/>o;
      </ekz>
      <ekz>
        <ind><tld/>atestantoj</ind>.
        <trd lng="ru">&c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_a;&c_n;&c_t;&c_y;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_u;&c_e;&c_l;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>Duell</trd>,
    <trd>Zweikampf</trd>
  </trdgrp>
  <trd lng="en">duel</trd>
  <trd lng="es">duelo</trd>
  <trd lng="fr">duel</trd>
  <trd lng="hu">p&aacute;rbaj</trd>
  <trd lng="ru">&c_d;&c_u;&c_e;&c_l;&c_mol;</trd>
</drv>

<drv mrk="duel.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Regule batali duope &gcirc;is vundado a&ubreve; mortigo:
      <ekz>
        mi decidis <tld/>i kun vi
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 24a, p. 154a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>sich <ind>duellieren</ind></trd>,
    <trd>einen <ind>Zweikampf</ind> austragen</trd>,
    <trd>einen <ind>Zweikampf</ind> ausfechten</trd>
  </trdgrp>
  <trd lng="fr">se battre en <ind>duel</ind></trd>
  <trd lng="hu">p&aacute;rbajozik</trd>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_n;&c_a; <ind>&c_d;&c_u;&c_e;&c_l;&c_i;</ind></trd>
</drv>

<drv mrk="duel.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <dif>
      Iu el la batalantoj en <tld/>o:
      <ekz>
        nun mi ne estas plu <tld/>anto, sed kuracisto
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 24a, p. 160a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_u;&c_e;&c_l;&c_ja;&c_n;&c_t;</trd>
  <trd lng="de">Duellant</trd>
  <trdgrp lng="en">
    <trd>dueler</trd>,
    <trd>dueller</trd>,
    <trd>duelist</trd>,
    <trd>duellist</trd>
  </trdgrp>
  <trd lng="fr">duelliste</trd>
  <trd lng="hu">p&aacute;rbajoz&oacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_e;&c_l;&c_ja;&c_n;&c_t;</trd>,
    <trd>&c_u;&c_ch;&c_a;&c_s;&c_t;&c_n;&c_i;&c_k; <ind>&c_d;&c_u;&c_e;&c_l;&c_i;</ind></trd>
  </trdgrp>
</drv>
</art>
