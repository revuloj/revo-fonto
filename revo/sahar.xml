<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>Sahar</rad>/o <fnt><bib>Far1</bib></fnt>
</kap>
<drv mrk="sahar.0o">
  <kap><tld/>o</kap>
  <snc mrk="sahar.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Granda, tre varma, sabla kaj &scirc;tona <ref tip="lst" lst="voko:regionoj" cel="dezert.0o">dezerto</ref>
      en norda <ref tip="malprt" cel="afrik.A0o">Afriko</ref>:
      <ekz>
        dezerta intermonto, kiaj ekzistas &ccirc;ie en <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la afrika regiono Sahelo, sude de la dezerto <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2005/008698.php">Evitenda evoluo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_a;&c_h;&c_a;&c_r;&c_a;</trd>
  <trd lng="bg">&c_S;&c_a;&c_h;&c_a;&c_r;&c_a;</trd>
  <trd lng="ca">el <ind>S&agrave;hara</ind></trd>
  <trd lng="cs">Sahara</trd>
  <trd lng="de">Sahara</trd>
  <trd lng="en">Sahara</trd>
  <trd lng="es">Sahara</trd>
  <trd lng="fr">Sahara</trd>
  <trd lng="it">Sahara</trd>
  <trd lng="nl">Sahara</trd>
  <trd lng="pl">Sahara</trd>
  <trd lng="ru">&c_S;&c_a;&c_h;&c_a;&c_r;&c_a;</trd>
  <trd lng="sk">Sahara</trd>
</drv>
</art>

