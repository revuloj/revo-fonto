<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vakcini</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vakcin1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vakcin1.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Sova&gcirc;a arbedo kun man&gcirc;eblaj nigraj
      (<ref tip="sub" cel="mirtel.0o">mirtelo</ref>, k.a.)
      a&ubreve; ru&gcirc;aj beroj,
      genro el familio <ref tip="super" cel="erikac.0oj">erikacoj</ref>
      (<trd lng="la">Vaccinium</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_u;&c_s;&c_mol;&c_n;&c_ib;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_u;&c_s;&c_mol;&c_n;&c_ib;&c_c;&c_y;</trd>
  </trdgrp>
  <trd lng="br">gwezenn-lus</trd>
  <trdgrp lng="de">
    <trd>Heidelbeere <klr>(nigra)</klr></trd>,
    <trd>Preiselbeere <klr>(ru&gcirc;a)</klr></trd>, k.a.
  </trdgrp>
  <trdgrp lng="en">
    <trd>vaccinium</trd>,
    <trd>blueberry</trd>,
    <trd>cranberry</trd>,
    <trd>huckleberry</trd>,
    <trd>bilberry</trd>
  </trdgrp>
  <trd lng="fr">airelle rouge</trd>
  <trd lng="hu">&aacute;fonya</trd>
  <trd lng="nl">bosbes</trd>
  <trd lng="no">tytteb&aelig;r</trd>
  <trd lng="pl">bor&oacute;wka</trd>
  <trdgrp lng="pt">
   <trd>airela</trd>,
   <trd>arando</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_u;&c_s;&c_n;&c_i;&c_k;&c_a;</trd>
  <trd lng="sv">lingon</trd>
</drv>
</art>
