<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vakcini</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vakcin1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vakcin1.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Sovaĝa arbedo kun manĝeblaj nigraj
      (<ref tip="sub" cel="mirtel.0o">mirtelo</ref>, k.a.)
      aŭ ruĝaj beroj,
      genro el familio <ref tip="super" cel="erikac.0oj">erikacoj</ref>
      (<trd lng="la">Vaccinium</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>брусьніка</trd>,
    <trd>брусьніцы</trd>
  </trdgrp>
  <trd lng="br">gwezenn-lus</trd>
  <trdgrp lng="de">
    <trd>Heidelbeere <klr>(nigra)</klr></trd>,
    <trd>Preiselbeere <klr>(ruĝa)</klr></trd>, k.a.
  </trdgrp>
  <trdgrp lng="en">
    <trd>vaccinium</trd>,
    <trd>blueberry</trd>,
    <trd>cranberry</trd>,
    <trd>huckleberry</trd>,
    <trd>bilberry</trd>
  </trdgrp>
  <trd lng="fr">airelle rouge</trd>
  <trd lng="hu">áfonya</trd>
  <trd lng="nl">bosbes</trd>
  <trd lng="no">tyttebær</trd>
  <trd lng="pl">borówka</trd>
  <trdgrp lng="pt">
   <trd>airela</trd>,
   <trd>arando</trd>
  </trdgrp>
  <trd lng="ru">брусника</trd>
  <trd lng="sv">lingon</trd>
</drv>
</art>
