<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vakcin1.xml,v 1.26 2023/09/05 09:43:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vakcini</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vakcin1.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vakcin1.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Sova&gcirc;a arbedo de malvarmaj landoj kun man&gcirc;eblaj nigraj
      (<ref tip="sub" cel="mirtel.0o">mirtelo</ref>, k.a.)
      a&ubreve; ru&gcirc;aj beroj,
      genro el familio <ref tip="super" cel="erikac.0oj">erikacoj</ref>
      (<trd lng="la">Vaccinium</trd>).
    </dif>
  </snc>
  <snc mrk="vakcin1.0o.KUI">
    <dif>
      Bero de <tld/>a arbedo:
      <ekz>
        hiera&ubreve; devis la patrino kaj la filino iri al 
        parencoj en Tikkala por <klr>...</klr> kolekti 
        <tld/>ojn
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_u;&c_s;&c_mol;&c_n;&c_ib;&c_k;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_u;&c_s;&c_mol;&c_n;&c_ib;&c_c;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_o;&c_r;&c_o;&c_v;&c_i;&c_n;&c_k;&c_a;</trd>
  <trd lng="br">gwezenn-lus</trd>
  <trdgrp lng="ca">
    <trd>nabiu</trd>,
    <trd>g&egrave;nere Vaccinium</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Heidelbeere <klr>(nigra)</klr></trd>,
    <trd>Preiselbeere <klr>(ru&gcirc;a)</klr></trd>, k.a.
  </trdgrp>
  <trdgrp lng="en">
    <trd>vaccinium</trd>,
    <trd>blueberry</trd>,
    <trd>cranberry</trd>,
    <trd>huckleberry</trd>,
    <trd>bilberry</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&f_y;&a_A;&a_h;&zwnj;&f_g;&f_y;&a_l;&a_h;</trd>
  <trd lng="fr">airelle</trd>
  <trd lng="hu">&aacute;fonya</trd>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30CE;&#x30AD;</trd>,
    <trd>&#x30B3;&#x30B1;&#x30E2;&#x30E2;</trd>
  </trdgrp>
  <trd lng="nl">bosbes</trd>
  <trd lng="no">tytteb&aelig;r</trd>
  <trd lng="pl">bor&oacute;wka</trd>
  <trdgrp lng="pt">
   <trd>airela</trd>,
   <trd>arando</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_u;&c_s;&c_n;&c_i;&c_k;&c_a;</trd>
  <trd lng="sv">lingon</trd>
  <trd lng="uk">&#x431;&#x440;&#x443;&#x441;&#x43D;&#x438;&#x446;&#x44F;</trd>
</drv>
</art>
