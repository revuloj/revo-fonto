<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fregat1.xml,v 1.4 2018/12/27 09:10:13 revo Exp $">
<kap>
  <rad>fregat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fregat1.0o">
  <kap><tld/>o</kap>
  <snc mrk="fregat1.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mara palmopieda <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref>, 
      kies flugo estas tre rapida; <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref>
      el familio fregatedoj (<trd lng="la">Fregata</trd>) kun malnova nomo
      <ref tip="sin" cel="takipet.0o.ZOO">takipeto</ref>:
      <ekz>
        la Andrea <tld/>o estas granda marbirdo kun longeco de 95&#8239;cm kaj enverguro de
        250&#8239;cm, masklo estas nekonfuzebla: lia supra korpo estas nigra kun verdaj
        briloj, la gor&gcirc;o, kolo kaj brusto nigraj kun purpuraj briloj, la ventro
        blanka, la beko nigra kaj la gor&gcirc;a sako ru&gcirc;a
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=48994"
              >Andrea fregato</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/7/78/Magnificent_Frigate_Bird%2C_Female%2C_Colon_Harbour%2C_Panama.jpg/640px-Magnificent_Frigate_Bird%2C_Female%2C_Colon_Harbour%2C_Panama.jpg"
      prm="CC BY-SA 4.0"
      >ina <tld/>o &ccirc;e Colon Harbor, Panamo
        <fnt>
          <aut>ImagePerson, propra verko</aut>
          <lok><url ref="&WCW;/File:Magnificent_Frigate_Bird,_Female,_Colon_Harbour,_Panama.jpg"
              >https://commons.wikimedia.org/wiki/File:Magnificent_Frigate_Bird,_Female,_Colon_Harbour,_Panama.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <rim>
    <ref tip="hom" cel="fregat.0o">Fregato</ref>
    anka&ubreve; estas speco de vel&scirc;ipo. 
  </rim>
  <trd lng="be">&c_f;&c_r;&c_e;&c_g;&c_a;&c_t;</trd>
  <trd lng="de">Fregattvogel</trd>
  <trdgrp lng="en">
    <trd>frigate bird</trd>,
    <trd>frigatebird</trd>
  </trdgrp>
  <trd lng="fr">fr&eacute;gate<klr tip="ind"> (zool.)</klr></trd>
  <trd lng="nl">fregatvogel</trd>
  <trdgrp lng="hu">
    <trd>fregattmad&aacute;r</trd>,
    <trd>haj&oacute;smad&aacute;r</trd>
  </trdgrp>
  <trd lng="it">fregata<klr> (zool.)</klr></trd>
  <trd lng="la">fregata</trd>
  <trd lng="nl">fregat</trd>
  <trd lng="pl">fregata</trd>
  <trdgrp lng="pt">
    <trd>fragata <klr tip="amb">(zool.)</klr></trd>,
    <trd>tesour&atilde;o <klr tip="amb">(zool.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_g;&c_a;&c_t;</trd>
  <trd lng="sk">fregata vzne&scaron;en&aacute;</trd>
  <trd lng="cs">fregatka z&aacute;padoindick&aacute;</trd>
</drv>
</art>

