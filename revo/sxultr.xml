<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ultr</rad>/o
</kap>
<drv mrk="sxultr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Parto de la korpo kuniganta kun kolo la supron de brakoj de homo
      a&ubreve; de anta&ubreve;aj kruroj de besto:
      <ekz>
        levi moke a&ubreve; mal&scirc;ate la <tld/>ojn;
      </ekz>
      <ekz>
        lia &scirc;ar&gcirc;o estu deprenita de sur liaj <tld/>oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li prenis sur siajn <tld/>ojn <klr>(f. sur sin)</klr> nian tutan
        aferon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        agi <ind><tld/>on &ccirc;e <tld/>o</ind>
        <klr>(f: tute akorde kunlabori)</klr>.
        <trd lng="de">Schulter an Schulter</trd>
        <trd lng="es">hombro a hombro</trd>
        <trd lng="hu">v&aacute;llvetve</trd>
        <trd lng="pl">rami&eogonek; w rami&eogonek;</trd>
        <trd lng="ru">&c_p;&c_l;&c_je;&c_ch;&c_o;&c_m; &c_k; &c_p;&c_l;&c_je;&c_ch;&c_u;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxuld.0o">&scirc;uldo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_ch;&c_o;</trd>
  <trd lng="bg">&c_r;&c_a;&c_m;&c_o;</trd>
  <trd lng="de">Schulter</trd>
  <trd lng="el">&omega_ton;&my;&omikron;&sigma_fina;</trd>
  <trd lng="en">shoulder</trd>
  <trd lng="es">hombro</trd>
  <trd lng="fr">&eacute;paule</trd>
  <trd lng="hr">rame</trd>
  <trd lng="hu">v&aacute;ll</trd>
  <trd lng="it">spalla</trd>
  <trd lng="nl">schouder</trd>
  <trd lng="pl">bark</trd>
  <trd lng="pt">ombro</trd>
  <trd lng="ru">&c_p;&c_l;&c_je;&c_ch;&c_o;</trd>
  <trdgrp lng="sv">
    <trd>skuldra</trd>,
    <trd>axel</trd>
  </trdgrp>
  <trd lng="tr">omuz</trd>
</drv>

<drv mrk="sxultr.0ozono">
  <kap><tld/>ozono</kap>
  <snc>
    <dif>
      Rimeno oblikve metita de sur <tld/>o al subbrako.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_t;&c_u;&c_p;&c_e;&c_ja;</trd>, 
    <trd>&c_p;&c_a;&c_p;&c_r;&c_u;&c_g;&c_a;</trd>
  </trdgrp>  
  <trdgrp lng="hu">
    <trd>v&aacute;llsz&iacute;j</trd>, 
    <trd>antantsz&iacute;j</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>tracolla</trd>,
    <trd>spallaccio</trd>
  </trdgrp>
  <trd lng="nl">schouderriem</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_r;&c_t;&c_u;&c_p;&c_je;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_v;&c_ja;&c_z;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
