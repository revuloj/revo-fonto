<vortaro>
<art mrk="$Id$">
<kap>
  <rad>helikriz</rad>/o
</kap>
<drv mrk="helikr.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Helichrysum</trd>), el kiu pluraj specioj
      seke konservi&gcirc;as sen perdi la koloron.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_y;&c_m;&c_o;&c_t;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_c;&c_mol;&c_m;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Strohblume</trd>
  <trd lng="fr">immortelle<klr tip="ind"> (fleur)</klr></trd>
  <trdgrp lng="nl">
    <trd>strobloem</trd>,
    <trd>immortel</trd>
  </trdgrp>
  <trd lng="hu">szalmagyop&aacute;r</trd>
  <trd
lng="ru">&c_b;&c_je;&c_s;&c_s;&c_m;&c_je;&c_r;&c_t;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
