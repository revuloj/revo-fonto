<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: morter.xml,v 1.21 2024/01/26 19:05:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>morter</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="morter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="morter.0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Miksa&jcirc;o el sablo, akvo kaj glua&jcirc;o, ekz-e <ref tip="prt" cel="kalk.0oestingita">kalko estingita</ref> a&ubreve; 
      <ref tip="prt" cel="cement.0o.KON">cemento</ref>, uzata
      por kunigi &scirc;tonojn a&ubreve; brikojn de konstrua&jcirc;o:
      <ekz>
        diru al tiuj, kiuj &scirc;miras per malta&ubreve;ga <tld/>o, ke &gcirc;i defalos
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 13:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tempo malaperigis &ccirc;iun postsignon de <tld/>o, kaj la &scirc;tonoj restis unu sur la
        alia nur pro sia pezo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter la traboj estis pajloargila <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Elisabeth Le Dru</aut>
          <vrk><url ref="&Monato;/2010/009948.php">Vizito al Vannes</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni &ccirc;iuj malsupreniris misa&ubreve;guran &scirc;tuparon <klr>[&#x2026;]</klr> kiu 
        sen&ccirc;ese rondiris <klr>[&#x2026;]</klr>, preter 
        monotonaj muroj el gutantaj &scirc;tonaj blokoj kaj dispeceti&gcirc;anta 
        <tld/>o
        <fnt>
          <aut>H. P. Lovecraft, trad. C. Kisa</aut>,
          <vrk>La Soleno</vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gips.0ajxo">gipsa&jcirc;o</ref>,
      <ref cel="kalk.0ajxo">kalka&jcirc;o</ref>,
      <ref cel="stuk.0o">stuko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_d;&c_a;&c_w;&c_n;&c_ib;&c_ch;&c_y; <ind>&c_r;&c_a;&c_s;&c_t;&c_v;&c_o;&c_r;</ind></trd>
  <trd lng="cs">malta</trd>
  <trd lng="de">M&ouml;rtel</trd>
  <trd lng="en">mortar</trd>
  <trd lng="es">mortero</trd>
  <trd lng="fr">mortier</trd>
  <trdgrp lng="hu">
    <trd>habarcs</trd>,
    <trd>malter</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30E2;&#x30EB;&#x30BF;&#x30EB;</trd>,
    <trd>&#x3057;&#x3063;&#x304F;&#x3044;</trd>
  </trdgrp>
  <trd lng="nl">mortel</trd>
  <trd lng="pl">zaprawa <klr tip="amb">murarska</klr></trd>
  <trd lng="pt">argamassa</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_v;&c_o;&c_r; <klr tip="amb">(&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;)</klr></trd>
  <trd lng="sk">malta</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_p;&c_n;&c_ja;&c_n;&c_i;&c_j; &c_r;&c_o;&c_z;&c_ch;&c_i;&c_n;</trd>,
    <trd>&c_b;&c_u;&c_d;&c_ib;&c_v;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_r;&c_o;&c_z;&c_ch;&c_i;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7070;&#x6D46; <pr>hu&#x12B;ji&#x101;ng</pr></trd>,
    <trd>&#x7070;&#x6F3F; <pr>hu&#x12B;ji&#x101;ng</pr></trd>,
    <trd>&#x7802;&#x6D46; <pr>sh&#x101;ji&#x101;ng</pr></trd>,
    <trd>&#x7802;&#x6F3F; <pr>sh&#x101;ji&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>
