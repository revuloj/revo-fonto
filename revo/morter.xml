<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>morter</rad>/o
</kap>
<drv mrk="morter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Miksa&jcirc;o el kalko kaj sablo por kunigi &scirc;tonojn a&ubreve; brikojn de
      konstrua&jcirc;o
    </dif>
    <refgrp tip="sin">
      <ref cel="cement.0o">cemento</ref>,
      <ref cel="glu.0o">gluo</ref>,
      <ref cel="lut">lutilo</ref>,
      <ref cel="stuk.0o">stuko</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_d;&c_a;&c_w;&c_n;&c_ib;&c_ch;&c_y; <ind>&c_r;&c_a;&c_s;&c_t;&c_v;&c_o;&c_r;</ind></trd>
  <trd lng="de">M&ouml;rtel</trd>
  <trd lng="es">mortero</trd>
  <trdgrp lng="hu">
    <trd>habarcs</trd>,
    <trd>malter</trd>
  </trdgrp>
  <trd lng="nl">mortel</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_v;&c_o;&c_r; <klr tip="amb">(&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;)</klr></trd>
</drv>
</art>
