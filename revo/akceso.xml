<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akceso.xml,v 1.26 2023/10/12 16:57:43 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>akcesor</rad>/a
</kap>
<drv mrk="akceso.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ne<ref tip="ant" cel="cxef.0a.KOMUNE">&ccirc;efa</ref>, 
      mal<ref tip="ant" cel="precip.0a">precipa</ref>, 
      <ref tip="sin" cel="flank.0a.FIG">flanka<sncref/></ref>:
      <ekz>
        oni disigas <klr>[&#x2026;]</klr> tion, kio estas <ref tip="ant" cel="esenc.0a">esenca</ref>, 
        de tio, kio estas <tld/>a
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        konscio pri la esenca egaleco de la homa naturo, super &ccirc;iuj <tld/>aj diferencoj
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kapdoloro kaj akuta malsatsento estas nur <tld/>aj simptomoj
        <fnt>
          <bib>SatirRak</bib>
          <lok>Enciclopeditis</lok>
        </fnt>;
      </ekz>
      <ekz>
        po&scirc;telefonoj estas nun plen&scirc;topitaj per <tld/>aj programoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2010/009945.php">Kiom da elektroniko?</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial lia filo lernu <tld/>an, sensignifan lingvon
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2006/008965.php">Lingvaj problemoj</url></vrk>
          <lok>2006</lok>
        </fnt>?
      </ekz>
    </dif>    
    <refgrp tip="vid">
      <ref cel="bagate.0a">bagatela</ref>,
      <ref cel="ekstr.0a.aldona">ekstra<sncref/></ref>,
      <ref cel="krom.0a">kroma</ref>,
      <ref cel="grav.mal0a">malgrava</ref>,
      <ref cel="pli.0a.aldona">plia<sncref/></ref>,
      <ref cel="suroga.0a">surogata</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_b;&c_o;&c_ch;&c_n;&c_y;</trd>,
      <trd>&c_d;&c_r;&c_u;&c_g;&c_a;&c_s;&c_n;&c_y;</trd>,
      <trd>&c_d;&c_a;&c_p;&c_a;&c_m;&c_o;&c_zh;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="bg">&c_v;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_n;</trd>
    <trdgrp lng="de">
      <trd>unwesentlich</trd>,
      <trd>nebens&auml;chlich</trd>,
      <trd>Zusatz-</trd>,
      <trd>Ersatz-</trd>,
      <trd>Neben-</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>accessory</trd>,
      <trd>secondary</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>accesorio</trd>,
      <trd>secundario</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>accessoire</trd>,
      <trd>secondaire</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bijkomstig</trd>,
      <trd>secundair</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>dodatkowy</trd>,
      <trd>uboczny</trd>,
      <trd>poboczny</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>acess&oacute;rio</trd>,
      <trd>secund&aacute;rio</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_b;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_t;&c_o;&c_r;&c_o;&c_s;&c_t;&c_je;&c_p;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_d;&c_o;&c_p;&c_o;&c_l;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_v;&c_s;&c_p;&c_o;&c_m;&c_o;&c_g;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="sk">ved&lcaron;aj&scaron;&iacute;</trd>
    <trdgrp lng="sv">
      <trd>bi-</trd>,
      <trd>till&auml;ggs-</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>akcesorick&yacute;</trd>,
    <trd>akcesorn&iacute;</trd>,
    <trd>nepodstatn&yacute;</trd>,
    <trd>pr&uring;vodn&iacute;</trd>,
    <trd>vedlej&scaron;&iacute; <klr>(stoj&iacute;c&iacute; vedle hlavn&iacute;ho)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&shin;&tet;&lamed;&yod;</trd>,
    <trd>&mem;&shin;&nun;&yod;</trd>,
    <trd>&tet;&pe;&lamed;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x526F;&#x306E;  <pr>&#x3075;&#x304F;&#x306E;</pr></trd>,
    <trd>&#x526F;&#x6B21;&#x7684;&#x306A;  <pr>&#x3075;&#x304F;&#x3058;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x4ED8;&#x968F;&#x7684;&#x306A;  <pr>&#x3075;&#x305A;&#x3044;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nepodstatn&yacute;</trd>,
    <trd>nepodstatn&yacute;, ved&lcaron;aj&scaron;&iacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_b;&c_ib;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_r;&c_u;&c_g;&c_o;&c_r;&c_ja;&c_d;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_o;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_o;&c_p;&c_o;&c_m;&c_ib;&c_zh;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
