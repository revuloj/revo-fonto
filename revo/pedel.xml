<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pedel.xml,v 1.20 2025/01/17 20:26:01 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>pedel</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="pedel.0o">
   <kap><tld/>o</kap>
   <snc mrk="pedel.0o.KOMUNE">
     <dif>
       Malsupera oficisto de pre&gcirc;ejo, universitato a&ubreve;
       ju&gcirc;ejo, kiu zorgas pri malgravaj materialaj taskoj:
       <ekz>
         tiuvespere, post la man&gcirc;o, li kaj la ju&gcirc;<tld/>o
         kartludis
         <fnt>
           <aut>E. de Bonaero</aut>,
           <vrk>La Friponeto de Tormes</vrk>,
           <lok>1973</lok>
         </fnt>;
       </ekz>
       <ekz>
        li estis akolito, pre&gcirc;eja <tld/>o, <klr>[&#8230;]</klr> mona&hcirc;o
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la pre&gcirc;ejo min apudestis kiel atestanto
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estis portempe dungita kiel <tld/>o por internacia konferenco en Alma-Ato
        <fnt>
          <bib>CKv</bib>
          <lok>2</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="ekonom.0o">ekonomo</ref>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_d;&c_e;&c_l;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>pedel</trd>,
    <trd>univerzitn&iacute; a soudn&iacute; sluha</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pedell</trd>,
    <trd>Hausmeister <klr>(Schule, Universit&auml;t)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
     <trd>beadle</trd>,
     <trd>apparitor</trd>,
     <trd>verger</trd>,
     <trd>usher <klr tip="amb">(court)</klr></trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>bedeau <klr>(&eacute;glise)</klr></trd>,
     <trd>appariteur <klr>(universit&eacute;)</klr></trd>,
     <trd>huissier <klr>(justice)</klr></trd>
   </trdgrp>
  <trdgrp lng="hu">
    <trd>pedellus</trd>,
    <trd>iskolaszolga</trd>,
    <trd>t&ouml;rv&eacute;nyszolga</trd>,
    <trd>sekresty&eacute;s</trd>,
    <trd>egyh&aacute;zfi</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#29992;&#21209;&#21729;  <pr>&#12424;&#12358;&#12416;&#12356;&#12435;</pr></trd>,
    <trd>&#32854;&#22530;&#30058;  <pr>&#12379;&#12356;&#12393;&#12358;&#12400;&#12435;</pr></trd>,
    <trd>&#24311;&#21519;  <pr>&#12390;&#12356;&#12426;</pr></trd>,
    <trd>&#29694;&#26989;&#32887;&#21729;  <pr>&#12370;&#12435;&#12366;&#12423;&#12358;&#12375;&#12423;&#12367;&#12356;&#12435;</pr></trd>,
    <trd>&#23432;&#34907;  <pr>&#12375;&#12421;&#12360;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>pedel</trd>,
     <trd>koster</trd>,
     <trd>concierge</trd>
   </trdgrp>
  <trdgrp lng="pl">
    <trd>pedel</trd>,
    <trd>wo&zacute;ny <klr>(uniwersytecki, s&aogonek;dowy)</klr></trd>,
    <trd>zakrystian</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>bedel <klr>(de igreja)</klr></trd>,
    <trd>bedel <klr>(de col&eacute;gio, universidade)</klr></trd>,
    <trd>esbirro <klr>(de tribunal)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_d;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">univerzitn&yacute; a s&uacute;dny sluha</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_d;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_d;&c_r;&c_ib;&c_b;&c_n;&c_i;&c_j; &c_s;&c_l;&c_u;&c_zh;&c_b;&c_o;&c_v;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_r;&c_o;&c_zh; (&c_u; &c_s;&c_u;&c_d;&c_ib;</trd>,
    <trd>&c_c;&c_je;&c_r;&c_k;&c_v;&c_ib;</trd>,
    <trd>&c_u;&c_n;&c_ib;&c_v;&c_je;&c_r;&c_s;&c_i;&c_t;&c_je;&c_t;&c_ib;)</trd>
  </trdgrp>
</drv>
