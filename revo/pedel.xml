<vortaro>
<art mrk="$Id: pedel.xml,v 1.15 2015/07/04 08:37:41 revo Exp $">
<kap>
   <ofc>1</ofc>
   <rad>pedel</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pedel.0o">
   <kap><tld/>o</kap>
   <snc mrk="pedel.0o.KOMUNE">
     <dif>
       Malsupera oficisto de pre&gcirc;ejo, universitato a&ubreve; 
       ju&gcirc;ejo, kiu zorgas pri malgravaj materialaj taskoj:
       <ekz>
         tiuvespere, post la man&gcirc;o, li kaj la ju&gcirc;<tld/>o
         kartludis
         <fnt>
           <aut>E. de Bonaero</aut>,
           <vrk>La Friponeto de Tormes</vrk>,
           <lok>1973</lok>
         </fnt>.       
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="cs">
    <trd>pedel</trd>,
    <trd>univerzitní a soudní sluha</trd>
  </trdgrp>
  <trd lng="de">Pedell</trd>
  <trdgrp lng="en">
     <trd>beadle</trd>,
     <trd>apparitor</trd>,
     <trd>verger</trd>,
     <trd>usher <klr tip="amb">(court)</klr></trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>bedeau <klr>(&eacute;glise)</klr></trd>,
     <trd>appariteur <klr>(universit&eacute;)</klr></trd>,
     <trd>huissier <klr>(justice)</klr></trd>
   </trdgrp>
  <trdgrp lng="hu">
    <trd>pedellus</trd>,
    <trd>iskolaszolga</trd>,
    <trd>t&ouml;rv&eacute;nyszolga</trd>,
    <trd>sekresty&eacute;s</trd>,
    <trd>egyh&aacute;zfi</trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>pedel</trd>,
     <trd>koster</trd>,
     <trd>concierge</trd>
   </trdgrp>
  <trdgrp lng="pl">
    <trd>pedel</trd>,
    <trd>wo&zacute;ny <klr>(uniwersytecki, s&aogonek;dowy)</klr></trd>,
    <trd>zakrystian</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>bedel <klr>(de igreja)</klr></trd>,
    <trd>bedel <klr>(de col&eacute;gio, universidade)</klr></trd>,
    <trd>esbirro <klr>(de tribunal)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_d;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">univerzitný a súdny sluha</trd>
</drv>
</art>
