<drv mrk="dolcx.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="dolcx.mal0a.gusto">
    <dif>
      Havanta akran, mordan guston, kiel galo a&ubreve; pipro:
      <ekz>
        mal<tld/>a kiel absinto
        <fnt><bib>MT</bib>, <lok>&Sen; 5:4</lok>)</fnt>;
      </ekz>
      <ekz>
        mal<tld/>a migdalo
        <fnt><bib>Fab3</bib>, <lok>Sub la saliko</lok></fnt>;
      </ekz>
      <ekz>
        mal<tld/>a akvo
        <fnt>
          <bib>MT</bib>
          <lok>&Nom; 5:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        macojn kun mal<tld/>aj herboj ili man&gcirc;u
        <fnt>
          <bib>MT</bib>
          <lok>&Eli; 12:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a por la lango, sed saniga por la sango
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="amar.0a.gusto">amara</ref>
    <ref tip="vid" cel="acid.0a">acida</ref>
    <trd lng="en">bitter</trd>
  <trd lng="es">amargo</trd>
    <trd lng="fa">&a_t;&a_l;&a_kh;</trd>
    <trd lng="pt">amargo</trd>
  </snc>
  <snc mrk="dolcx.mal0a.sento">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Doloriga, prema, peza por la psi&hcirc;o a&ubreve; rilata al
      tia sento:
      <ekz>
         kaj ili faros al si pro vi kalva&jcirc;on,
         &ccirc;irka&ubreve;zonos sin per saka&jcirc;o, kaj
         ploros pri vi kun mal<tld/>a koro mal<tld/>an ploron
        <fnt><bib>MT</bib> <lok>&Jeh; 27:31</lok>)</fnt>;
      </ekz>
      <ekz>
         &scirc;i ver&scirc;is mal<tld/>ajn larmojn
         <fnt><bib>BonaS-ino</bib></fnt>;
      </ekz>
      <ekz>
         tiel denove pasis iom da tempo, longa kaj mal<tld/>a
         <fnt><bib>Fab3</bib> <lok>Knabino, kiu pa&scirc;is sur
         panon</lok></fnt>;
      </ekz>
      <ekz>
         neniu pokalo de la vivo enhavas ekskluzive nur vermuton,
         neniu bona homo povus regali homon nur per la mal<tld/>a
         kaliko da suferoj
         <fnt><bib>Fab4</bib> <lok>Historio el la
         dunoj</lok></fnt>;
      </ekz>
      <ekz>
         ia mal<tld/>a ironio sonis en la vo&ccirc;o de la nejuna
         virino
         <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        mal<tld/>a sento de neplenumita espero
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="amar.0a.sento">amara</ref>
    <trd lng="es">amargo</trd>
    <trdgrp lng="fa">
      <trd>&a_t;&a_l;&a_kh;</trd>,
      <trd>&a_n;&a_A;&f_g;&a_w;&a_A;&a_r;</trd>,
      <trd>&a_n;&a_A;&a_kh;&a_w;&a_sh;&a_A;&f_y;&a_n;&a_d;</trd>,
      <trd>&a_A;&a_n;&a_d;&a_w;&a_h;&a_b;&a_A;&a_r;</trd>
    </trdgrp>
    <trd lng="pt">amargo</trd>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_r;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_g;&c_o;&c_r;&c_ch;&c_i;&c_v;</trd>
  <trd lng="de">bitter</trd>
  <trd lng="es">amargo</trd>
  <trd lng="fr">amer</trd>
  <trd lng="he">&tet;&ayin;&fmem; &mem;&resh;</trd>
  <trd lng="id">pahit</trd>
  <trd lng="it">amaro</trd>
  <trdgrp lng="ja">
    <trd>&#x82E6;&#x3044;  <pr>&#x306B;&#x304C;&#x3044;</pr></trd>,
    <trd>&#x3064;&#x3089;&#x3044;</trd>,
    <trd>&#x304D;&#x3073;&#x3057;&#x3044;</trd>
  </trdgrp>
  <trd lng="nl">bitter</trd>
  <trd lng="pl">gorzki</trd>
  <trd lng="ro">amar</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_mol;&c_k;&c_i;&c_j;</trd>
  <trd lng="uk">&c_g;&c_ib;&c_r;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x60B2;&#x75DB;&#x5730; <pr>b&#x113;it&#xF2;ngde</pr></trd>,
    <trd>&#x8F9B;&#x9178; <pr>x&#x12B;nsu&#x101;n</pr></trd>,
    <trd>&#x8F9B;&#x8FA3; <pr>x&#x12B;nl&#xE0;</pr></trd>,
    <trd>&#x8F9B; <pr>x&#x12B;n</pr></trd>
  </trdgrp>
</drv>

