  <rad>dol&ccirc;</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="dolcx.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="dolcx.0a.gusto">
    <dif>
      Agrable impresanta
      <ref tip="lst" cel="gust.0o.eco" lst="voko:gustoj">guston</ref>,
      kiel sukero a&ubreve; mielo:
      <ekz>
        la patro donis al mi <tld/>an pomon
        <fnt>
          <bib>F</bib>
          <lok>&FE; 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli <tld/>a ol mielo
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 14:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a nektaro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj fruktoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> kuiris por li <tld/>an vinon
        <fnt>
          <bib>Fab1</bib>
          <lok>Malbonkonduta knabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        glacia&jcirc;o estas <tld/>a glaciigita franda&jcirc;o
        <fnt>
          <bib>F</bib>
          <lok>&FE; 35</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; per <tld/>a kuko vi min ne allogos
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        post <tld/>a vino restas acida vinagro
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        akvoj &scirc;telitaj estas <tld/>aj
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 9:17</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ba">siass</trd>
    <trd lng="ca">dol&ccedil;</trd>
    <trdgrp lng="es">
      <trd>dulce</trd>,
      <trd>azucarado</trd>
    </trdgrp>
    <trd lng="fa">&a_sh;&f_y;&a_r;&f_y;&a_n;</trd>
    <trdgrp lng="fr">
      <trd>doux</trd>,
      <trd>sucr&eacute;</trd>
    </trdgrp>
    <trd lng="he">&mem;&tav;&vav;&qof;</trd>
    <trd lng="hr">sladak</trd>
    <trd lng="ka">&#x10E2;&#x10D9;&#x10D1;&#x10D8;&#x10DA;&#x10D8;</trd>
    <trd lng="lt">saldus</trd>
    <trd lng="oc">do&ccedil;</trd>
    <trd lng="pt">doce</trd>
    <trd lng="ro">dulce</trd>
    <trd lng="ru">&c_s;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;</trd>
    <trd lng="sl">sladek</trd>
    <trd lng="th">&#xE2B;&#xE27;&#xE32;&#xE19;</trd>
  </snc>
  <snc mrk="dolcx.0a.gxuiga">
    <dif>
      Agrabla por la sentumoj:
      <ekz>
        <tld/>a kantado
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a vo&ccirc;o, agrablega sono
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a odora&jcirc;o
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxarm.0a">&ccirc;arma</ref>,
      <ref cel="glat.0a">glata</ref>,
      <ref cel="gxu">&gcirc;uiga</ref>,
      <ref cel="placx.0a">pla&ccirc;a</ref>,
      <ref cel="velur">velura</ref>.
    </refgrp>
    <trdgrp lng="es">
      <trd>agradable</trd>,
      <trd>dulce</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_sh;&f_y;&a_r;&f_y;&a_n;</trd>,
      <trd>&a_kh;&a_w;&a_sh;&a_A;&f_y;&a_n;&a_d;</trd>,
      <trd>&a_d;&a_l;&a_n;&a_sh;&f_y;&a_n;</trd>,
      <trd>&a_d;&a_l;&f_p;&a_dh;&f_y;&a_r;</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>agr&eacute;able</trd>,
      <trd>doux</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_l;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>doce</trd>,
      <trd>suave</trd>,
      <trd>ameno</trd>,
      <trd>fagueiro</trd>,
      <trd>meigo</trd>,
      <trd>agrad&aacute;vel</trd>
    </trdgrp>
  </snc>
  <snc mrk="dolcx.0a.milda">
    <dif>
      Agrabla por la korsento, milda, malsevera, karesa:
      <ekz>
        <tld/>a homo, viza&gcirc;o;
      </ekz>
      <ekz>
        <tld/>aj an&gcirc;elaj infanoj
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj son&gcirc;oj
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a estas la dormo de laboranto
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 5:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        en plej <tld/>aj tremoj tuj ekbatis homa koro
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a kora &gcirc;ojo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a la morto por la maljunulo, la laca de vivo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        pri <tld/>a <klr>(karesa)</klr> vorto ne fieru, mal<tld/>an ne koleru
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a melankolia rideto
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        el <klr>[lia]</klr> bu&scirc;o fluadis <tld/>amiela parolo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la malbono estas <tld/>a en lia bu&scirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 20:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>animeco de Kristo
        <fnt>
          <bib>NT</bib>
          <lok>II. Korintanoj 10:1</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="delika.0a">delikata 2</ref>,
      <ref cel="facil.0a">facila</ref>,
      <ref cel="glat.0a">glata</ref>,
      <ref cel="kviet.0a">kvieta</ref>,
      <ref cel="akr">malakra</ref>,
      <ref cel="miel.0a">miela</ref>,
      <ref cel="moder.0a">modera</ref>
    </refgrp>
    <trdgrp lng="es">
      <trd>suave</trd>,
      <trd>dulce</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_sh;&f_y;&a_r;&f_y;&a_n;</trd>,
      <trd>&a_m;&a_l;&a_A;&f_y;&a_m;</trd>,
      <trd>&a_n;&a_r;&a_m;</trd>,
      <trd>&a_l;&a_T;&f_y;&a_f;</trd>
    </trdgrp>
    <trd lng="fr">doux</trd>
    <trdgrp lng="pt">
    <trd>doce</trd>,
    <trd>suave</trd>,
    <trd>ameno</trd>,
    <trd>fagueiro</trd>,
    <trd>meigo</trd>,
    <trd>agrad&aacute;vel</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_l;&c_a;&c_d;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_l;&c_o;&c_d;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_s;&c_l;&c_a;&c_d;&c_malmol;&c_k;</trd>
  <trd lng="bo">&t_ma;&t_nga;&t_ra;&t_tsheg;&t_ma;&t_naro;&t_tsheg;</trd>
  <trd lng="cs">sladk&yacute;</trd>
  <trdgrp lng="de">
    <trd>s&uuml;&szlig;</trd>, <trd>lieblich</trd>
  </trdgrp>
  <trd lng="en">sweet</trd>
  <trd lng="es">dulce</trd>
  <trd lng="id">manis</trd>
  <trd lng="it">dolce</trd>
  <trdgrp lng="ja">
    <trd>&#x7518;&#x3044;  <pr>&#x3046;&#x307E;&#x3044;</pr></trd>,
    <trd>&#x5FC3;&#x5730;&#x597D;&#x3044;  <pr>&#x3053;&#x3053;&#x3061;&#x3088;&#x3044;</pr></trd>,
    <trd>&#x3084;&#x3055;&#x3057;&#x3044;</trd>
  </trdgrp>
  <trd lng="nl">zoet</trd>
  <trd lng="pl">s&lstroke;odki</trd>
  <trd lng="sw">-tamu</trd>
  <trd lng="tok">suwi</trd>
  <trdgrp lng="uk">
    <trd> &c_s;&c_o;&c_l;&c_o;&c_d;&c_k;&c_i;&c_j; (&c_t;&c_zh; &c_p;&c_je;&c_r;&c_je;&c_n;.)</trd>,
    <trd>&c_p;&c_r;&c_ib;&c_s;&c_n;&c_i;&c_j; (&c_p;&c_r;&c_o; &c_v;&c_o;&c_d;&c_u;)</trd>
  </trdgrp>
  <trd lng="zh">&#x751C;&#x7F8E; <pr>ti&#xE1;nm&#x11B;i</pr></trd>
</drv>

<drv mrk="dolcx.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc>
    <dif>
      En <tld/>a maniero:
      <ekz>
        li parolas, kaj lia parolo fluas <tld/>e kaj agrable
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>e dormadi
        <fnt><bib>Fab2</bib>, <lok>Elfo de la rozo</lok></fnt>;
      </ekz>
      <ekz>
        la &ccirc;armaj malgrandaj infanoj dormas kaj son&gcirc;as tiel <tld/>e
        <fnt>
          <bib>Fab1</bib>
          <lok>Cikonioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> najtingalo <klr>[&#x2026;]</klr> kantadis tiel <tld/>e
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e &scirc;miri al iu la lipojn <klr>(<ref cel="flat.0i">flati</ref>)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        paroli <tld/>e en la orelon
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_l;&c_o;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_o;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="en">sweetly</trd>
  <trd lng="fr">doucement</trd>
  <trd lng="id">dengan <ind>manis</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x7518;&#x304F;  <pr>&#x3046;&#x307E;&#x304F;</pr></trd>,
    <trd>&#x5FC3;&#x5730;&#x597D;&#x304F;  <pr>&#x3053;&#x3053;&#x3061;&#x3088;&#x304F;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dolcx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="dolcx.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;a&jcirc;o kun precipe <tld/>a gusto
      pro mielo, sukero a&ubreve;
      aliaj <tld/>aj konsista&jcirc;oj:
      <ekz>
        la ursoj amas <tld/>a&jcirc;ojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ursa Felo</lok>
        </fnt>;
      </ekz>
      <ekz>
        man&gcirc;u grasa&jcirc;on kaj trinku <tld/>a&jcirc;on, kaj sendu porciojn al tiuj, kiuj nenion
        pretigis por si
        <fnt>
          <bib>MT</bib>
          <lok>&Neh; 8:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Nebukadnecar</nom>, re&gcirc;o de <nom>Babel</nom>, <klr>[&#x2026;]</klr>
        plenigis sian ventron per miaj <tld/>a&jcirc;oj,
        kaj forpelis min
        <fnt><bib>MT</bib>, <lok>&Jer; 51:34</lok></fnt>;
      </ekz>
      <ekz>
        la konsilo de amiko estas <tld/>a&jcirc;o por la animo
        <fnt>
          <bib>MT</bib>
          <lok>&Sen; 27:9</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="frand.0ajxo">franda&jcirc;o</ref>
    <trd lng="de">S&uuml;&szlig;igkeit</trd>
    <trdgrp lng="zh">
    <trd>&#x53EF;&#x53E3; <pr>k&#x11B;k&#x1D2;u</pr></trd>,
    <trd>&#x7CD6;&#x679C; <pr>t&#xE1;nggu&#x1D2;</pr></trd>,
    <trd>&#x751C;&#x98DF; <pr>ti&#xE1;nsh&#xED;</pr></trd>,
    <trd>&#x559C;&#x7CD6; <pr>x&#x1D0;t&#xE1;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_l;&c_o;&c_d;&c_k;&c_a;&c_je;</trd>,
    <trd>&c_s;&c_l;&c_o;&c_d;&c_y;&c_ch;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_d;&c_y;&c_t;&c_a;&c_r;&c_s;&c_k;&c_ib; &c_v;&c_y;&c_r;&c_a;&c_b;</trd>
  </trdgrp>
  <trd lng="bo">&t_ba;&t_yata;&t_drengbu;&t_tsheg;&t_ra;&t_gigu;&t_la;&t_tsheg;</trd>
  <trdgrp lng="en">
    <trd>piece of <ind>confectionery</ind></trd>,
    <trd>sweets</trd>,
    <trd>candy</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pastel<klr> (producto)</klr></trd>,
    <trd>dulce</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>patisserie<klr> (produit)</klr></trd>,
    <trd>sucrerie</trd>
  </trdgrp>
  <trd lng="he">&mem;&mem;&tav;&qof;</trd>
  <trdgrp lng="id">
    <trd><ind>gula</ind>-gula</trd>,
    <trd><ind>manis</ind>an</trd>,
    <trd>permen</trd>
  </trdgrp>
  <trd lng="it">dolce<klr> (cibo)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x7518;&#x3044;&#x3082;&#x306E;  <pr>&#x3042;&#x307E;&#x3044;&#x3082;&#x306E;</pr></trd>,
    <trd>&#x304A;&#x83D3;&#x5B50;  <pr>&#x304A;&#x304B;&#x3057;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zoetigheid</trd>,
    <trd>zoetwaar</trd>,
    <trd>snoep</trd>,
    <trd>suikergoed</trd>
  </trdgrp>
  <trd lng="pl">s&lstroke;odycze<klr> {pl}</klr></trd>
  <trdgrp lng="tok">
    <trd>suwi<klr tip="ind"> (subst.)</klr></trd>,
    <trd>ijo suwi</trd>
  </trdgrp>
</drv>

<drv mrk="dolcx.0ajxejo">
  <kap><tld/>a&jcirc;ejo</kap>
  <snc mrk="dolcx.0ajxejo.vendejo">
    <dif>
      <ref tip="super" cel="vend.0ejo">Vendejo</ref> kie oni vendas <tld/>a&jcirc;ojn:
      <ekz>
        posttagmeze ili iris en <tld/>a&jcirc;ejon por man&gcirc;i torton
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2007/009024.php">Bela son&gcirc;o</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        okaze de la sendependi&gcirc;o de Kosovo la <tld/>a&jcirc;ejo <klr>[&#x2026;]</klr> pretigis
        eksterordinaran kukon
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009440.php">Dol&ccirc;a sendependi&gcirc;o</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="suker.0ajxejo">sukera&jcirc;ejo</ref>
    <trdgrp lng="de">
      <trd>Konditorei</trd>,
      <trd>S&uuml;&szlig;warenladen</trd>
    </trdgrp>
    <trd lng="zh">&#x7CD5;&#x70B9;&#x5E97; <pr>g&#x101;odi&#x1CE;ndi&#xE0;n</pr></trd>
</snc>
    <trdgrp lng="be">
    <trd>&c_c;&c_u;&c_k;&c_je;&c_r;&c_n;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_d;&c_y;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_d;&c_y;&c_t;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sweet shop</trd>,
    <trd>candy store</trd>,
    <trd>confectioner's<klr> (shop)</klr></trd>
  </trdgrp>
  <trd lng="es">pasteler&iacute;a<klr> (tienda)</klr></trd>
  <trd lng="fr">patisserie<klr> (lieu)</klr></trd>
  <trdgrp lng="id">
    <trd><ind>toko</ind> gula-gula</trd>,
    <trd><ind>toko</ind> manisan</trd>,
    <trd><ind>toko</ind> permen</trd>
  </trdgrp>
  <trd lng="it">pasticceria<klr> (negozio)</klr></trd>
  <trd lng="nl">snoepwinkel</trd>
  <trd lng="pl">cukiernia</trd>
  <trd lng="ro">cofet&abreve;rie</trd>
</drv>

<drv mrk="dolcx.0igi">
  <kap>
    <tld/>igi
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dolcx.0igi.gusto">
    <dif>
      Igi <tld/>a (per mielo, sukero kc):
      <ekz>
        akvo <tld/>igita per mielo
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>igi la adia&ubreve;on per bombonoj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2004/008358.html">Kuracbaneje</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="suker.0i">sukeri</ref>
    <trd lng="de">s&uuml;&szlig;en</trd>
  </snc>
  <snc mrk="dolcx.0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Igi agrabla, tenta, &gcirc;ua:
      <ekz>
        via kolero nur <tld/>igos al li la triumfon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u ne ekzistas rimedo, kiu ebligus <tld/>igi la sorton de tiuj kompatinduloj
        <fnt>
          <bib>ChL</bib>
        </fnt>?
      </ekz>
      <ekz>
        a&ubreve;skultu, <klr>[&#x2026;]</klr> li diris per pli <tld/>igita tono, kiu strange
        kontrastis kun tiu, kiun li estis uzinta <klr>[anta&ubreve;e]</klr>
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        per ruzaj vortoj vi min ne <tld/>igos!
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        la bona servisto sin demandis per kia <tld/>igita rimedo li povos anonci al sia
        mastro tiu pereon
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        reciproka amo kaj harmonio <tld/>igas al ni la vivon
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">vers&uuml;&szlig;en</trd>
    <trd lng="zh">&#x52A0;&#x751C; <pr>ji&#x101;ti&#xE1;n</pr></trd>
</snc>
  <trdgrp lng="be">
