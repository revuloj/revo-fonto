<drv mrk="cxen.el0igi">
  <kap>de<tld/>igi, <var><kap>el<tld/>igi</kap></var>, <var><kap>sen<tld/>igi</kap></var></kap>
  <snc mrk="cxen.el0igi.TEK">
    <dif>
      Liberigi de <tld/>o:
      <ekz>
        &ccirc;iuj hundoj devas esti de<tld/>igitaj kaj pelitaj en ilian
        mezon, ke ili disi&gcirc;u, distri&gcirc;u
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        malfeli&ccirc;ulo, mi vin sen<tld/>igas
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="it">
     <trd><ind>liberare</ind> dalle catene</trd>,
     <trd>sguinzagliare</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>spuszcza&cacute; z <ind>&lstroke;a&nacute;cucha</ind></trd>,
      <trd>oswobadza&cacute; z <ind>&lstroke;a&nacute;cucha</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="cxen.el0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="brid.sen0igi.FIG">Senbridigi<sncref/></ref>:
      <ekz>
        de<tld/>igita kolero;
      </ekz>
      <ekz>
        la veron, kiun vi ser&ccirc;as, vi neniam trovos per mallar&gcirc;animaj, sencelaj
        diskutadoj <klr>[&#8230;]</klr> esploradu kun el<tld/>igita animo, kaj poste
        decidi&gcirc;u
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum tiu fina kaj senespera provo ili tamen ka&ubreve;zis potenc-pinton:
        &ccirc;i-lasta do de<tld/>igis eksplodon, kiu dissaltigis la supran protektoplaton
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011522.php">La
          akcidento de &Ccirc;ernobilo 30 jarojn poste</url></vrk>
        </fnt>.
      </ekz>
    </dif>
   <trd lng="de">entfesseln</trd>
    <trdgrp lng="it">
     <trd>scatenare</trd>,
     <trd>sguinzagliare<klr> (fig.)</klr></trd>
    </trdgrp>
    <trd lng="zh">&#x6FC0;&#x8D77; <pr>j&#x12B;q&#x1D0;</pr></trd>
</snc>
  <trd lng="be">&c_s;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;
  &c_z; <ind>&c_l;&c_a;&c_n;&c_c;&c_u;&c_g;&c_a;</ind></trd>

  <trd lng="es">desencadenar</trd>
  <trd lng="fr">d&eacute;chainer</trd>
  <trd lng="ja">&#37782;&#x304B;&#12425;&#x89E3;&#x304D;&#x653E;&#12377; 
  <pr>&#x304F;&#12373;&#x308A;&#x304B;&#12425;&#12392;&#x304D;&#x306F;&#x306A;&#12377;</pr></trd>
  <trd lng="nl">ontketenen</trd>
  <trdgrp lng="pt">
    <trd>desacorrentar</trd>,
    <trd>desencadear</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol; &c_s;
  <ind>&c_c;&c_je;&c_p;&c_i;</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_p;&c_u;&c_s;&c_k;&c_a;&c_t;&c_i; (&c_z; &c_l;&c_a;&c_n;&c_c;&c_ju;&c_g;&c_a;)</trd>,
    <trd>&c_z;&c_b;&c_u;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_d;&c_ib;&c_j;&c_m;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_l;&c_i;&c_k;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

