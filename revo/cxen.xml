<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;en</rad>/o
</kap>
<drv mrk="cxen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Ligilo, plej ofte metala, el interkunigitaj ringoj:
      <ekz>
        <tld/>o de hundo, de po&scirc;horlo&gcirc;o;
      </ekz>
      <ekz>
        ora kol<tld/>o kun ornampenda&jcirc;o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="katen.0o">kateno</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Sinsekvo da samspecaj objektoj a&ubreve; aferoj:
      <ekz>
        <tld/>o da montoj
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fabeloj de Andersen,
        Ligo de amikeco</vrk>)</fnt>;
      </ekz>
      <ekz>
        <tld/>o da insuloj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>o da ideoj;
      </ekz>
      <ekz>
        tio estos <tld/>o de zorgoj k suferoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        seninterrompa <tld/>o de komerca&jcirc;oj
        <fnt>(<aut>Kabe</aut>: <vrk>La Faraono, 1a vol.</vrk>,
        <lok>&ccirc;ap. 19</lok>)</fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la generacioj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kette</trd>
  <trd lng="nl">ketting</trd>
  <trd lng="en">chain</trd>
  <trdgrp lng="ru">
    <trd>&c_c;&c_je;&c_p;&c_mol;</trd>,
    <trd>&c_c;&c_je;&c_p;&c_o;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="cxen.0ero">
  <kap><tld/>ero</kap>
  <snc>
    <dif>
      &Ccirc;iu aparta ringo de <tld/>o 
    </dif>
    <refgrp tip="sin">
      <ref cel="masx.0o">ma&scirc;o</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Kettenglied</trd>
  <trd lng="nl">schakel</trd>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_n;&c_o; <klr>(&c_c;&c_je;&c_p;&c_i;)</klr></trd>
</drv>
<drv mrk="cxen.0i">
  <kap><tld/>i<fnt>Z</fnt>, en<tld/>igi<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ligi per <tld/>o. 
    </dif>
  </snc>
  <trd lng="de">anketten</trd>
  <trd lng="nl">vastketenen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_c;&c_je;&c_p;&c_mol;&c_ju;)</klr></trd>,
    <trd>&c_p;&c_o;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol; &c_n;&c_a; &c_c;&c_je;&c_p;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="cxen.el0igi">
  <kap>el<tld/>igi<fnt>Z</fnt></kap>
  <snc num="1">
    <dif>
      Liberigi de <tld/>o:
      <ekz>
        el<tld/>igita hundo.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Senbridigi:
      <ekz>
        el<tld/>igita kolero;
      </ekz>
      <ekz>
        milito el<tld/>igis &ccirc;iujn malvirtojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">entfesselt</trd>
  <trd lng="nl">ontketenen</trd>
</drv>
</art>
