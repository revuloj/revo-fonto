<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;am</rad>/o
</kap>
<drv mrk="sxam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Delikata ledo de &ccirc;amo preparita por servi kiel frotpurigilo.
    </dif>
    <trdgrp lng="de">
      <trd>Gamsleder</trd>,
      <trd>S&auml;mischleder</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      &Scirc;afledo preparita por anstata&ubreve;i tian &ccirc;amledon.
    </dif>
  </snc>
  <trd lng="fr">peau de chamois</trd>
  <trdgrp lng="nl">
    <trd>gemzeleer</trd>,
    <trd>zeem</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_m;&c_sh;&c_a;</trd>
  <trd lng="sv">s&auml;mskskinn</trd>
</drv>
</art>
