<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lin&ccirc;</rad>/i
</kap>
<drv mrk="lincx.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Punmortigi iun sen ju&gcirc;ista regula verdikto, la&ubreve; maniero de la usona
      popolmaso.
    </dif>
  </snc>
  <trd lng="en">lynch</trd>
  <trd lng="es">linchar</trd>
  <trd lng="nl">lynchen</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_n;&c_ch;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_je;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol; <ind>&c_s;&c_a;&c_m;&c_o;&c_s;&c_u;&c_d;&c_u;</ind></trd>
  </trdgrp>
</drv>
</art>
