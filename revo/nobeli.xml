<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nobeli</rad>/o
</kap>
<drv mrk="nobeli.0o">
  <kap><tld/>o</kap>
  <mlg>No</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 102, atompezo 259,1009, simbolo <frm>No</frm>. 
      <ref tip="super" cel="urani.trans0o">Transuranio</ref>. 
      &Ccirc;iuj <ref tip="super" cel="izotop.0o">izotopoj</ref> 
      de tiu elemento estas radioaktivaj. Aktinoido.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_o;&c_b;&c_e;&c_l;&c_ib;&c_j;</trd>      
  <trd lng="br">nobeliom</trd>
  <trd lng="ca">nobeli</trd>
  <trd lng="de">Nobelium</trd>
  <trd lng="en">nobelium</trd>
  <trd lng="es">nobelio</trd>
  <trd lng="fr">nob&eacute;lium</trd>
  <trd lng="hu">nob&eacute;lium</trd>
  <trd lng="it">nobelio</trd>
  <trd lng="la">Nobelium</trd>
  <trd lng="nl">nobelium</trd>
  <trd lng="pl">nobel</trd>
  <trd lng="pt">nob&eacute;lio</trd>
  <trd lng="ru">&c_n;&c_o;&c_b;&c_je;&c_l;&c_i;&c_j;</trd>
</drv>
</art>
