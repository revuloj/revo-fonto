<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nobeli</rad>/o
</kap>
<drv mrk="nobeli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 102, atompezo 259,1009, simbolo No. <ref tip="super"
      cel="urani.trans0o">Transuranio</ref>. &Ccirc;iuj izotopoj de tiu elemento
      estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="de">Nobelium</trd>
  <trd lng="en">nobelium</trd>
  <trd lng="nl">nobelium</trd>
  <trd lng="ru">&c_n;&c_o;&c_b;&c_je;&c_l;&c_i;&c_j;</trd>
</drv>
</art>
