  <snc mrk="kiam.0.demanda">
    <dif>
       Demanda <ref tip="super" cel="pronom.0o.gxenerala">pronomo</ref>
       adverba, signifanta &leftquot;en kiu tempo?&rightquot;:
       <ekz>
          <tld/>  mi ekiru
          <fnt>
             <aut>Michael Ende, trad. Wolfram Diestel</aut>
             <vrk>La Sen&ccirc;esa Rakonto</vrk>
             <lok>La Destino de Atrejo</lok>
          </fnt>?
       </ekz>
       <ekz>
          &gcirc;is <tld/>
          <fnt>
             <bib>Monato</bib>
             <aut>Donald Broadribb</aut>
             <vrk><url ref="&Monato;/2003/007837.html">Ne al &ccirc;iuj pla&ccirc;os &ccirc;i literatura
                   gemeto</url></vrk>
             <lok>2003</lok>
          </fnt>?
       </ekz>
    </dif>
    <trd lng="ca">quan</trd>
    <trd lng="de">wann</trd>
    <trd lng="es">cuando</trd>
    <trd lng="he">&mem;&tav;&yod;</trd>
    <trd lng="hu">mikor</trd>
    <trd lng="id">kapan</trd>
    <trd lng="oc">quand</trd>
    <trd lng="pl">kiedy</trd>
    <trd lng="pt">quando</trd>
    <trd lng="ro">c&acirc;nd</trd>
    <trdgrp lng="th">
