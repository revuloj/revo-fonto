    <trd>&#20511;&#12426;&#12427;  <pr>&#12363;&#12426;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">huren</trd>
  <trdgrp lng="pl">
    <trd>wynaj&aogonek;&cacute;</trd>,
    <trd>wynajmowa&cacute;</trd>,
    <trd>najmowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">alugar</trd>
  <trdgrp lng="ro">
    <trd>&icirc;nchiria</trd>,
    <trd>angaja</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_t;&c_mol; &c_v;&c_n;&c_a;&c_jo;&c_m;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_mol;
    &c_n;&c_a;&c_p;&c_r;&c_o;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_a;&c_r;&c_je;&c_n;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;
    <klr>(&c_p;&c_o;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je; &c_i; &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prenaja&tcaron; si</trd>,
    <trd>vzia&tcaron; do n&aacute;jmu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_j;&c_m;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_i; &c_v;&c_n;&c_a;&c_j;&c_m;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_r;&c_o;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_o;&c_r;&c_je;&c_n;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_i; &c_v; &c_o;&c_r;&c_je;&c_n;&c_d;&c_u; (&c_p;&c_r;&c_i;&c_m;&c_ib;&c_shch;&c_je;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_m;&c_je;&c_b;&c_l;&c_ib; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#36035; <pr>l&igrave;n</pr></trd>,
