<drv mrk="lu.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Ricevi ion por &gcirc;in uzi dum difinita tempo, pagante
      interkonsentitan prezon:
      <ekz>
        <tld/>i lo&gcirc;ejon, a&ubreve;ton, balvestojn;
      </ekz>
      <ekz>
        la mizeraj mebla&jcirc;oj <klr>[&#8230;]</klr> ne estis &scirc;ia propra&jcirc;o, &scirc;i <tld/>is ilin kune
        kun la &ccirc;ambro
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is &ccirc;ambron de la &scirc;ipo je &gcirc;usta prezo
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is boateton kaj kun helpo de lerta remisto balda&ubreve; staris sur la &scirc;ipo
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la filo sekve <tld/>is apartamentojn en pli bela kvartalo de la urbo
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni rekondukos la bestojn tien, kie ni <tld/>is ilin
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vitalij</nom> <tld/>is fiakriston
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la taktiko de tiu firmao estis <tld/>i standon apud nia kaj, anstata&ubreve; eksponadi
        <fnt>
          <bib>KrM</bib>
          <lok>38. Du Botoj Faras Paron.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> <tld/>is taksion por la dudek-minuta voja&gcirc;o
       <fnt>
       <aut>Thomas Eccardt</aut>
          <vrk><url ref="&Monato;/2000/006859.php">Urbeto a&ubreve; sendependa lando?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni povas anka&ubreve; <tld/>i kaj provi rakedojn, sketilojn, ekipa&jcirc;ojn por pilkoludoj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2009/009812.php">&Gcirc;ui simplajn &scirc;atokupojn sub&ccirc;iele</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        en jam 119 urboj en E&ubreve;ropo eblas <tld/>i biciklojn
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2012/010599.php">Nova malnova trafikilo</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pren.lu0i"><tld/>preni</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="farm.0i">farmi</ref>,
      <ref cel="uz.frukt0o">fruktuzo</ref>,
      <ref cel="prunt.0i">prunti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_c;&c_mol; &c_u;
    <ind>&c_a;&c_r;&c_e;&c_n;&c_d;&c_u;</ind></trd>,
    <trd>&c_b;&c_r;&c_a;&c_c;&c_mol;
    <ind>&c_u;&c_n;&c_a;&c_jo;&c_m;</ind></trd>,
    <trd>&c_a;&c_r;&c_e;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_c;&c_mol;
    <ind>&c_n;&c_a;&c_p;&c_r;&c_a;&c_k;&c_a;&c_t;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_a;&c_je;&c_m;&c_a;&c_m;</trd>
  <trd lng="bo">&t_ga;&t_ya;&t_ra;&t_tsheg;</trd>
