<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lu</rad>/i
</kap>
<drv mrk="lu.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Ricevi ion por &gcirc;in uzi dum difinita tempo, pagante interkonsentitan
      prezon:
      <ekz>
        <tld/>i meblojn, balvestojn;
      </ekz>
      <ekz>
        <tld/>ebla &ccirc;ambro;
      </ekz>
      <ekz>
        Aleksandro kaj Dorina <tld/>is etan &ccirc;ambron, malproksime de la
        gepatroj de Dorina<fnt><aut>Julian Modest</aut>: <vrk>En la ombro
        de la silento</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 3a,
        p. 14a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="pren.lu0i"><tld/>preni</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="farm.0i">farmi</ref>,
      <ref cel="prunt.0i">prunti</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_c;&c_mol; &c_u; <ind>&c_a;&c_r;&c_e;&c_n;&c_d;&c_u;</ind></trd>,
    <trd>&c_b;&c_r;&c_a;&c_c;&c_mol; <ind>&c_u;&c_n;&c_a;&c_jo;&c_m;</ind></trd>,
    <trd>&c_a;&c_r;&c_e;&c_n;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_c;&c_mol; <ind>&c_n;&c_a;&c_p;&c_r;&c_a;&c_k;&c_a;&c_t;</ind></trd>
  </trdgrp>
  <trd lng="br">feurmi&ntilde;<klr> (kemer e feurm)</klr></trd>
  <trd lng="de">mieten</trd>
  <trd lng="el">&epsilon;&ny;&omikron;&jota;&kappa;&jota;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="en">hire</trd>
  <trdgrp lng="es">
    <trd>alquilar</trd>,
    <trd>arrendar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>louer<klr> (prendre en location)</klr></trd>
  </trdgrp>
  <trd lng="nl">huren</trd>
  <trd lng="pt">alugar</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_a;&c_t;&c_mol; &c_v;&c_n;&c_a;&c_jo;&c_m;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_mol; &c_n;&c_a;&c_p;&c_r;&c_o;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_a;&c_r;&c_je;&c_n;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_o;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je; &c_i;
    &c_t;.&c_p;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="lu.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Doni ion al iu, por lia uzo dum difinita tempo, ricevante
      interkonsentitan prezon:
      <ekz>
        alia kompanio <klr>...</klr>, kiu respondecas pri trakoj, signaloj
        kaj kelkaj stacidomoj, luigas siajn trakojn al la aliaj
        kompanioj
        <fnt><aut>Paul Gubbins</aut>: <vrk>Politiko
        elreligita</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 2a, p.
        6a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="don.lu0i"><tld/>doni</ref>
    </refgrp>
  </snc>                 
  <trdgrp lng="be">
    <trd>&c_z;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_u; <ind>&c_a;&c_r;&c_e;&c_n;&c_d;&c_u;</ind></trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_n;&c_a;&c_p;&c_r;&c_a;&c_k;&c_a;&c_t;</ind></trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_u;&c_n;&c_a;&c_jo;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="br">feurmi&ntilde;<klr> (lezel e feurm)</klr></trd>
  <trd lng="de">vermieten</trd>
  <trd lng="el">&epsilon;&ny;&omikron;&jota;&kappa;&jota;&alfa_ton;&zeta;&omega;</trd>
  <trdgrp lng="es">
    <trd>alquilar</trd>,
    <trd>arrendar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>louer<klr> (donner en location)</klr></trd>
  </trdgrp>
  <trd lng="nl">verhuren</trd>
  <trd lng="pt">alugar</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_v;&c_n;&c_a;&c_jo;&c_m;</trd>,
    <trd>&c_s;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_n;&c_a;&c_p;&c_r;&c_o;&c_k;&c_a;&c_t;</trd>,
    <trd>&c_s;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_v; &c_a;&c_r;&c_je;&c_n;&c_d;&c_u;</trd>
  </trdgrp>
</drv>

<drv mrk="lu.vic0igi">
  <kap>vic<tld/>igi</kap>
  <uzo tip="fak">EKON</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="L"/>igi parton de domo a&ubreve; lo&gcirc;ejo, kiun oni mem luas.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_u; <ind>&c_s;&c_u;&c_b;&c_a;&c_r;&c_e;&c_n;&c_d;&c_u;</ind></trd>            
  <trd lng="br">eilfeurmi&ntilde;</trd>
  <trd lng="de">untervermieten</trd>
  <trd lng="el">&ypsilon;&pi;&omikron;&my;&jota;&sigma;&theta;&omega_ton;&ny;&omega;</trd>
  <trdgrp lng="fr">
    <trd>sous-louer</trd>
  </trdgrp>
  <trd lng="nl">onderverhuren</trd>
  <trd lng="pt">sublocar</trd>
</drv>
</art>
