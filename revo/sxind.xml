<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ind</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxind.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxind.0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      Ligno<ref tip="vid" cel="tegol.0o">tegolo</ref>:
      <ekz>
        tiujare nia domo &scirc;an&gcirc;is sian tegmenton el pajla al <tld/>a
        <fnt>
          <aut>I. Szabo</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/32699/284324">
          1956 &mdash; Liberbatalo</url></vrk>,
          <lok>2010-10-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_o;&c_n;&c_t;&c_a;</trd>, 
    <trd>&c_d;&c_r;&c_a;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="en"><klr>(wooden) </klr>shingle</trd>
  <trd lng="fr">bardeau</trd>
  <trd lng="hu">zsindely</trd>
  <trdgrp lng="it">
    <trd>scandola</trd>, 
    <trd>tegola<klr> (di legno)</klr></trd>
  </trdgrp>
  <trd lng="nl">dakplankje</trd>
  <trd lng="pt">taco</trd>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
