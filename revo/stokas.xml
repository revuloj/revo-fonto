<vortaro>
<art mrk="$Id$">
<kap><rad>stokast</rad>/a</kap>
<drv mrk="stokas.0a">
   <kap><tld/>a</kap>
   <fnt><bib>PIV1</bib></fnt>
   <snc>
     <dif>
       Rilata al hazardaj fenomenoj.
     </dif>
     <refgrp tip="vid">
       <ref cel="variab.hazarda0o">hazarda variablo</ref>.
     </refgrp>
   </snc>
   <trd lng="de">Stochastik</trd>
   <trd lng="fr">stochastique</trd>
   <trd lng="es">estoc&aacute;stico</trd>
   <trd lng="nl">stochastisch</trd>
   <trd 
lng="ru">&c_s;&c_t;&c_o;&c_h;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="stokas.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">MAT</uzo>
   <fnt><bib>MatStokTerm</bib><lok>p. 51</lok></fnt>
   <snc>
     <ref tip="dif" cel="variab.hazarda0o">hazarda variablo</ref>
   </snc>
   <trd lng="de">Zufallsvariable</trd>
   <trd lng="nl">stochast</trd>
   <trd lng="ru">&c_s;&c_l;&c_u;&c_ch;&c_a;&c_j;&c_n;&c_a;&c_ja; 
&c_v;&c_je;&c_l;&c_i;&c_ch;&c_i;&c_n;&c_a;</trd>
</drv>
</art>
