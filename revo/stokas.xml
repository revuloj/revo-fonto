<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stokas.xml,v 1.11 2018/11/18 19:10:20 revo Exp $">
<kap><rad>stokast</rad>/a</kap>
<drv mrk="stokas.0a">
  <kap><tld/>a</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
       Rilata al hazardaj fenomenoj:
      <ekz>
        <tld/>aj fraktaloj rilatas kun la kaosa teorio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1283">Fraktalo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la konverto de <nom>Fourier</nom> de hazarda (alivorte <tld/>a) ondformo estas anka&ubreve;e
        hazarda
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=105765">Frekvenca spektro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="variab.hazarda0o">hazarda variablo</ref>.
    </refgrp>
  </snc>
   <trd lng="be">&c_s;&c_t;&c_a;&c_h;&c_a;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
   <trd lng="de">stochastisch</trd>
   <trd lng="fr">stochastique</trd>
   <trd lng="es">estoc&aacute;stico</trd>
   <trd lng="hu">sztochasztikus</trd>   
   <trd lng="nl">stochastisch</trd>
   <trd lng="ru">&c_s;&c_t;&c_o;&c_h;&c_a;&c_s;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="stokas.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">MAT</uzo>
   <fnt><bib>MatStokTerm</bib><lok>p. 51</lok></fnt>
   <snc>
     <ref tip="dif" cel="variab.hazarda0o">hazarda variablo</ref>
   </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_a;&c_d;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_v;&c_je;&c_l;&c_ib;&c_ch;&c_y;&c_n;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>n&aacute;hodn&aacute; prom&ecaron;nn&aacute;</trd>,
    <trd>n&aacute;hodn&eacute; &ccaron;&iacute;slo</trd>
  </trdgrp>
  <trd lng="de">Zufallsvariable</trd>
  <trd lng="hu">val&oacute;sz&iacute;n&udblac;s&eacute;gi v&aacute;ltoz&oacute;</trd>
  <trd lng="nl">stochast</trd>
  <trd lng="ru">&c_s;&c_l;&c_u;&c_ch;&c_a;&c_j;&c_n;&c_a;&c_ja; &c_v;&c_je;&c_l;&c_i;&c_ch;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>n&aacute;hodn&aacute; premenn&aacute;</trd>,
    <trd>n&aacute;hodn&eacute; &ccaron;&iacute;slo</trd>
  </trdgrp>
</drv>
</art>
