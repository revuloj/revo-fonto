<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>stokast</rad>/a</kap>
<drv mrk="stokas.0a">
  <kap><tld/>a</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Rilata al hazardaj fenomenoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="variab.hazarda0o">hazarda variablo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Stochastik</trd>
  <trd lng="nl">stochastisch</trd>
</drv>

<drv mrk="stokas.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatStokTerm</bib><lok>p. 51</lok></fnt>
  <snc>
    <ref tip="dif" cel="variab.hazarda0o">hazarda variablo</ref>
  </snc>
  <trd lng="de">Zufallsvariable</trd>
  <trd lng="nl">stochast</trd>
</drv>
</art>
