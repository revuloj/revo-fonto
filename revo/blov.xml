<?xml version="1.0"?>

<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: blov.xml,v 1.30 2017/12/20 23:10:24 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>blov</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="blov.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="blov.0i.ntr">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Pu&scirc;i, ekmovi la aeron iamaniere:
      <ekz>
        fortega vento <tld/>as
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 11a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i sur la varman supon;
      </ekz>
      <ekz>
        <tld/>i sur la fajron por &gcirc;in eksciti;
      </ekz>
      <ekz>
        <tld/>i en fluton;
      </ekz>
      <ekz>
        <tld/>u per trumpeto inter la nacioj
        <fnt><bib>MT</bib>, <lok>&Jer; 51:27</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i per la korno de ribelo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>dmucha&cacute;</trd>,
      <trd>d&aogonek;&cacute;</trd>,
      <trd>wia&cacute;</trd>
    </trdgrp>
    <trd lng="sk">via&tcaron;</trd>
  </snc>
  <snc mrk="blov.0i.tr">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Pu&scirc;i la aeron sur a&ubreve; en ion:
      <ekz>
        <tld/>i la supon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i la teon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i trumpeton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu espero <tld/>as fre&scirc;an forton en niajn brustojn;
      </ekz>
      <ekz>
        re<tld/>i en iu la fajron de l' ven&gcirc;emo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>dmucha&cacute;<klr> na co&sacute;, w co&sacute;</klr></trd>,
      <trd>d&aogonek;&cacute;<klr> w co&sacute;</klr></trd>
    </trdgrp>
    <trd lng="sk">f&uacute;ka&tcaron;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_mol;&c_m;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_z;&c_mol;&c_m;&c_u;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">c'hwezha&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>fouknout</trd>,
    <trd>vanout</trd>,
    <trd>zafoukat</trd>,
    <trd>zavanout</trd>,
    <trd>za&ccaron;&iacute;t foukat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>blasen</trd>,
    <trd>wehen</trd>,
    <trd>pusten</trd>
  </trdgrp>
  <trd lng="en">blow</trd>
  <trd lng="es">soplar</trd>
  <trd lng="fr">souffler</trd>
  <trd lng="hu">f&uacute;j</trd>
  <trd lng="pt">soprar</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pref&uacute;ka&tcaron; <klr>(r&uacute;ru)</klr></trd>,
    <trd>tiahnu&tcaron; (o prievane)</trd>
  </trdgrp>
</drv>

<drv mrk="blov.0o">
  <kap><tld/>o</kap>
  <snc mrk="blov.0o.KOMUNE">
    <dif>
      Pu&scirc;o de aero:
      <ekz>
        diru al mi, &ccirc;u ni parolos pri la vetero kaj <tld/>o,
        a&ubreve; &ccirc;u ni estos sinceraj
        <fnt>
          <aut>L. Dilling, trad. E. Hall</aut>,
          <vrk>La Porcio da Glacia&jcirc;o</vrk>,
          <bib>FK</bib>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>proud vzduchu</trd>,
    <trd>z&aacute;van</trd>
  </trdgrp>
  <trd lng="fr">souffle</trd>
  <trdgrp lng="hu">
    <trd>f&uacute;j&aacute;s</trd>,
    <trd>fuvallat</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podmuch</trd>,
    <trd>powiew</trd>,
    <trd>dmuchni&eogonek;cie</trd>,
    <trd>tchnienie</trd>
  </trdgrp>
  <trd lng="ro">sufla</trd>
  <trd lng="ru">&c_d;&c_u;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">z&aacute;van</trd>
</drv>

<drv mrk="blov.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="blov.0ado.KOMUNE">
    <dif>
      Granda a&ubreve; da&ubreve;ra pu&scirc;o de aero:
      <ekz>
        <ind>ne&gcirc;a <tld/>ado</ind>
        <klr>(ne&gcirc;a ventego)</klr>
        <trdgrp lng="be">
          <trd>&c_z;&c_a;&c_v;&c_je;&c_ja;</trd>,
          <trd>&c_z;&c_a;&c_v;&c_ib;&c_r;&c_u;&c_h;&c_a;</trd>
        </trdgrp>
        <trd lng="hu">h&oacute;f&uacute;v&aacute;s</trd>
        <trdgrp lng="pl">
          <trd>zamie&cacute;</trd>,
          <trd>zawieja</trd>,
          <trd>zadymka</trd>,
          <trd>zawierucha</trd>
        </trdgrp>
        <trd lng="ru">&c_m;&c_je;&c_t;&c_je;&c_l;&c_mol;</trd>
        <fnt><bib>Fab2</bib>, <lok>ne&gcirc;a re&gcirc;ino</lok></fnt>;
      </ekz>
      <ekz>
        a&ubreve;di&gcirc;is <tld/>ado de delikata venteto
        <fnt><bib>MT</bib>, <lok>&Re1; 19:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_d;&c_y;&c_h;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fouk&aacute;n&iacute;</trd>,
    <trd>vanut&iacute;</trd>
  </trdgrp>
  <trd lng="fr">souffle</trd>
  <trd lng="hu">f&uacute;v&aacute;s</trd>
  <trdgrp lng="pl">
    <trd>wicher</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_u;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>f&uacute;kanie</trd>,
    <trd>vanutie</trd>
  </trdgrp>
</drv>

<drv mrk="blov.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="blov.0ajxo.KOMUNE">
    <dif>
      La aero, spira&jcirc;o, odoro, kiu estas eligata a&ubreve;
      alportata de la <tld/>ado:
      <ekz>
        kovrita de tuko el blanka ne&gcirc;a <tld/>a&jcirc;o
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arom.0o">aromo</ref>,
      <ref cel="haladz.0o">haladzo</ref>,
      <ref cel="parfum.0o">parfumo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_d;&c_y;&c_h;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_w;</trd>
  </trdgrp>
  <trd lng="br">c'hwezhadenn</trd>
  <trdgrp lng="de">
    <trd>Brise</trd>,
    <trd>Hauch</trd>,
    <trd>Puste</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bouff&eacute;e</trd>,
    <trd>exhalaison</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fuvallat</trd>,
    <trd>fuvalom</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podmuch</trd>,
    <trd>powiew</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_u;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="blov.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="blov.0ilo.KOMUNE">
    <dif>
      Speciala ilo por <tld/>i:
      <ekz>
        li revigligis la fajron per <tld/>ilo.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="balg.0o">balgo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_h; <klr
  tip="amb">(&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;)</klr></trd>
  <trdgrp lng="br">
    <trd>megin</trd>,
    <trd>c'hwezherez</trd>
  </trdgrp>
  <trd lng="cs">m&ecaron;ch <klr>(varhan)</klr></trd>
  <trd lng="de">Blasebalg</trd>
  <trd lng="fr">soufflet<klr tip="ind"> (instrument)</klr></trd>
  <trd lng="hu">f&uacute;jtat&oacute;</trd>
  <trd lng="pl">dmuchawa</trd>
  <trd lng="ru">&c_m;&c_je;&c_h;&c_i; <klr
  tip="amb">(&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
  <trd lng="sk">f&uacute;ka&ccaron; <klr>(n&aacute;stroj)</klr></trd>
</drv>

<drv mrk="blov.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="blov.cxirkaux0i.KOMUNE">
    <dif>
      &Ccirc;irka&ubreve;i <tld/>ante,
      <tld/>i al la &ccirc;irka&ubreve;o de:
      <ekz>
        la akra vento &ccirc;irka&ubreve;<tld/>is lian korpon
        <fnt><bib>Fab1</bib>, <lok>&gcirc;ardeno de la
        paradizo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">k&ouml;rbef&uacute;j</trd>  
  <trd lng="pl">owiewa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_d;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="blov.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="blov.dis0i.KOMUNE">
    <dif>
      Disigi per <tld/>o:
      <ekz>
        ili estas kiel grenventuma&jcirc;o, kiun dis<tld/>as la vento
        <fnt><bib>MT</bib>, <lok>&Psa; 1:4</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dis<tld/>i la iluziojn de iu.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_je;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="cs">rozfoukat <klr>(od sebe)</klr></trd>
  <trdgrp lng="de">
    <trd>wegblasen</trd>,
    <trd>verwehen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>souffler <klr tip="ind">(disperser en soufflant)</klr></trd>,
    <trd>dissiper <klr tip="ind">(fig.)</klr></trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;tf&uacute;j</trd>
  <trdgrp lng="pl">
    <trd>rozwiewa&cacute;</trd>,
    <trd>rozdmuchiwa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">rozf&uacute;ka&tcaron;</trd>
</drv>

<drv mrk="blov.dis0igxi">
  <kap>dis<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="blov.dis0igxi.KOMUNE">
    <dif>
      Disi&gcirc;i a&ubreve; diseri&gcirc;i en la &ccirc;irka&ubreve;a aero;
      <ref tip="vid" cel="aper.mal0i">malaperi</ref>:
      <ekz>
        Dis<tld/>i&gcirc;as la&ubreve; la vento /
        &Scirc;iaj densaj brunaj haroj...
        <fnt>
          <aut>V. N. Devjatnin</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Poezio/velga.html">
          Velga</url></vrk>,
          <lok>[19??]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>dissiper</ind></trd>,
    <trd>s'<ind>&eacute;vanouir</ind><klr> (disparaitre)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="blov.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="blov.en0i.KOMUNE">
    <dif>
      <tld lit="B"/>i al la interno de...:
      <ekz>
        en<tld/>is vento tiel glacia, ke &scirc;ajnis, ke &gcirc;i venas 
        rekte de la suda poluso
        <fnt>
          <aut>C. Gates</aut>,
          <vrk>La Vidvino kaj la Profesoro</vrk>,
          <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="blov.en0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Inspiri, sugeste komuniki:
      <ekz>
        tra pluraj generacioj, vjetnamaj artistoj scipovis en<tld/>i 
        novan spiriton per elpensado de novaj teknikoj
        <fnt>
          <aut>Nguyen Xuan Thu</aut>,
          <vrk><url
          ref="http://www.ipernity.com/blog/xuanautuno32/427884">
          La vjetnama tradicia silko-pentrarto</url></vrk>,
          <lok>Ipernity, 2012-11-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">insuffler</trd>
</drv>

<drv mrk="blov.for0i">
  <kap><ofc>*</ofc>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="blov.for0i.KOMUNE">
    <dif>
      Forigi per <tld/>o:
      <ekz>
        la vento for<tld/>is de mia kapo la &ccirc;apon
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
      </ekz>
      <ekz>
        la vento for<tld/>is la nubojn<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_je;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>odfouknout</trd>,
    <trd>odv&aacute;ti</trd>,
    <trd>sv&aacute;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wegblasen</trd>,
    <trd>verwehen</trd>
  </trdgrp>
  <trd lng="fr">souffler <klr tip="ind">(enlever en
  soufflant)</klr></trd>
  <trd lng="hu">elf&uacute;j</trd>
  <trdgrp lng="pl">
    <trd>wydmuchiwa&cacute;</trd>,
    <trd>wywiewa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_ja;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>odf&uacute;knu&tcaron;</trd>,
    <trd>odvia&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="blov.tra0o">
  <kap>tra<tld/>o</kap>
  <snc mrk="blov.tra0o.KOMUNE">
    <dif>
      Fluo da aero, kiu trairas ejon a&ubreve; lokon de iu flanko al
      alia kontra&ubreve;a:
      <ekz>
        la tra<tld/>oj estas &ccirc;iam dan&gcirc;eraj por la sano.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_r;&c_a;&c_z;&c_mol;&c_n;&c_ja;&c_k;</trd>
  <trd lng="br">avel-sil</trd>
  <trdgrp lng="cs">
    <trd>pr&uring;van</trd>,
    <trd>tah</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zug</trd>, <trd>Durchzug</trd>
  </trdgrp>
  <trd lng="fr">courant d'air</trd>
  <trd lng="hu">huzat</trd>
  <trdgrp lng="pl">
    <trd>przeci&aogonek;g</trd>,
    <trd>przewiew</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_v;&c_o;&c_z;&c_n;&c_ja;&c_k;</trd>
  <trd lng="sk">prievan</trd>
</drv>

<drv mrk="blov.plen0i">
  <kap>plen<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="blov.plen0i.KOMUNE">
    <dif>
      &Scirc;veligi, pufigi per <tld/>o:
      <ekz>
        plen<tld/>i sapan vezikon;
      </ekz>
      <ekz>
        plen<tld/>ita velo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <ind>plen<tld/>i la lipojn</ind>
        <klr>(mal&scirc;ate)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">lebiggyeszti az ajk&aacute;t</trd>
      </ekz>
      <ekz>
        plen<tld/>i sin de malhumileco
        <fnt><bib>Fab2</bib>, <lok>poligono</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">c'hweza&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>nafouknout</trd>,
    <trd>nafukovat</trd>,
    <trd>plnit vzduchem</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufblasen</trd>, <trd>aufpusten</trd>
  </trdgrp>
  <trd lng="fr">gonfler <klr tip="ind">(tr.)</klr></trd>
  <trd lng="hu">felf&uacute;j</trd>
  <trdgrp lng="pl">
    <trd>nadmuchiwa&cacute;</trd>,
    <trd>nadyma&cacute;</trd>,
    <trd>rozdyma&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>naf&uacute;knu&tcaron;</trd>,
    <trd>plni&tcaron; vzduchom</trd>
  </trdgrp>
</drv>

<drv mrk="blov.spir0i">
  <kap>spir<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="blov.spir0i.KOMUNE">
    <dif>
      <tld lit="B"/>i elspirante:
      <ekz>
        la maljuna &ccirc;evalo <klr>...</klr> svingadis la voston,
