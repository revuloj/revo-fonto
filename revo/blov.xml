<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>blov</rad>/i
</kap>
<drv mrk="blov.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="blov.0i.ntr">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Pu&scirc;i, ekmovi la aeron iamaniere:
      <ekz>
        fortega vento <tld/>as
        <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 11</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>i sur la varman supon;
      </ekz>
      <ekz>
        <tld/>i sur la fajron por &gcirc;in eksciti;
      </ekz>
      <ekz>
        <tld/>i en fluton;
      </ekz>
      <ekz>
        <tld/>i per trumpeto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i per la korno de ribelo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="sk">via&tcaron;</trd>
  </snc>
  <snc mrk="blov.0i.tr">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Pu&scirc;i la aeron sur a&ubreve; en ion:
      <ekz>
        <tld/>i la supon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i la teon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i trumpeton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu espero <tld/>as fre&scirc;an forton en niajn brustojn;
      </ekz>
      <ekz>
        re<tld/>i en iu la fajron de l' ven&gcirc;emo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="sk">f&uacute;ka&tcaron;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_mol;&c_m;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_z;&c_mol;&c_m;&c_u;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">vanout</trd>
  <trdgrp lng="de">
    <trd>blasen</trd>,
    <trd>wehen</trd>,
    <trd>pusten</trd>
  </trdgrp>
  <trd lng="en">blow</trd>
  <trd lng="es">soplar</trd>
  <trd lng="fr">souffler</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="blov.0o0ado">
  <kap><tld/>o, <tld/>ado</kap>
  <snc>
    <dif>
      Pu&scirc;o de aero:
      <ekz>
        <ind>ne&gcirc;a <tld/>ado</ind>
        <fnt><bib>Fab2</bib>, <lok>Ne&gcirc;a re&gcirc;ino</lok></fnt>
        <klr>(ne&gcirc;a ventego)</klr>.
        <trdgrp lng="be">
          <trd>&c_z;&c_a;&c_v;&c_je;&c_ja;</trd>,
          <trd>&c_z;&c_a;&c_v;&c_ib;&c_r;&c_u;&c_h;&c_a;</trd>
        </trdgrp>
        <trd lng="ru">&c_m;&c_je;&c_t;&c_je;&c_l;&c_mol;</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_d;&c_y;&c_h;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_w;</trd>
  </trdgrp>
  <trd lng="fr">souffle</trd>
  <trd lng="ru">&c_d;&c_u;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="blov.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      La aero, spira&jcirc;o, odoro, kiu estas eligata a&ubreve; alportata
      de la <tld/>ado
    </dif>
    <refgrp tip="sin">
      <ref cel="parfum.0o">parfumo</ref>,
      <ref cel="haladz.0o">haladzo</ref>,
      <ref cel="arom.0o">aromo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_d;&c_y;&c_h;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brise</trd>,
    <trd>Hauch</trd>,
    <trd>Puste</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bouff&eacute;e</trd>,
    <trd>exhalaison</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_u;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="blov.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Speciala ilo por <tld/>i:
      <ekz>
        li revigligis la fajron per <tld/>ilo
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="balg.0o">balgo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_h; <klr tip="amb">(&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;)</klr></trd>
  <trd lng="de">Blasebalg</trd>
  <trd lng="fr">soufflet<klr tip="ind"> (instrument)</klr></trd>
  <trd lng="ru">&c_m;&c_je;&c_h;&c_i; <klr tip="amb">(&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
</drv>

<drv mrk="blov.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Ccirc;irka&ubreve;igi per <tld/>o,
      <tld/>i al la &ccirc;irka&ubreve;o de:
      <ekz>
        la akra vento &ccirc;irka&ubreve;<tld/>is lian korpon
        <fnt><bib>Fab1</bib><lok>&Gcirc;ardeno de la paradizo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="blov.dis0ifor0i">
  <kap>dis<tld/>i, for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Forigi ion per <tld/>o:
      <ekz>
        la vento for<tld/>is la nubojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la vento for<tld/>is mian &ccirc;apelon;
      </ekz>
      <ekz>
        dis<tld/>i la iluziojn de iu.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_je;&c_ja;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>wegblasen</trd>, <trd>verwehen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>souffler <klr tip="ind">(disperser en soufflant)</klr></trd>,
    <trd>dissiper <klr tip="ind">(fig.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_ja;&c_t;&c_mol;</trd>
</drv>

<drv mrk="blov.tra0o">
  <kap>tra<tld/>o</kap>
  <snc>
    <dif>
      Fluo da aero, kiu trairas ejon a&ubreve; lokon de iu flanko al alia
      kontra&ubreve;a:
      <ekz>
        la tra<tld/>oj estas &ccirc;iam dan&gcirc;eraj por la sano.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_r;&c_a;&c_z;&c_mol;&c_n;&c_ja;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Zug</trd>, <trd>Durchzug</trd>
  </trdgrp>
  <trd lng="fr">courant d'air</trd>
  <trd lng="ru">&c_s;&c_k;&c_v;&c_o;&c_z;&c_n;&c_ja;&c_k;</trd>
</drv>

<drv mrk="blov.plen0i">
  <kap>plen<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;veligi, pufigi per <tld/>o:
      <ekz>
        plen<tld/>i sapan vezikon;
      </ekz>
      <ekz>
        plen<tld/>ita velo<fnt>B</fnt>;
      </ekz>
      <ekz>
        plen<tld/>i la lipojn
        <klr>(mal&scirc;ate)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        plen<tld/>i sin de malhumileco
        <fnt><bib>Fab2</bib>, <lok>Poligono</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufblasen</trd>, <trd>aufpusten</trd>
  </trdgrp>
  <trd lng="fr">gonfler <klr tip="ind">(tr.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="blov.spir0i">
  <kap>spir<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="B"/>i elspirante.
    </dif>
  </snc>
  <trd lng="de">pusten</trd>
  <trd lng="fr">expirer <klr tip="ind">(l'air des poumons)</klr></trd>
</drv>

<drv mrk="blov.0eksciti">
  <kap><tld/>eksciti</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Eksciti <tld/>ante:
      <ekz>
        <tld/>eksciti fajron.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">anfachen</trd>
  <trd lng="fr">attiser</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_d;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="blov.0estingi">
  <kap><tld/>estingi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Estingi <tld/>ante:
      <ekz>
        <tld/>estingi kandelon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_d;&c_z;&c_mol;&c_m;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_a;&c_s;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ausblasen</trd>, <trd>auspusten</trd>
  </trdgrp>
  <trd lng="fr">souffler <klr tip="ind">(qc pour l'&eacute;teindre)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_u;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_g;&c_a;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
