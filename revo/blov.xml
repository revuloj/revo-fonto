<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: blov.xml,v 1.42 2023/08/31 08:48:52 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>blov</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="blov.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="blov.0i.ntr">
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Pu&scirc;i, ekmovi la aeron iamaniere:
      <ekz>
        fortega vento <tld/>as
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 11a</lok></fnt>;
      </ekz>
      <ekz>
        falis pluvo, kaj <klr>[&#x2026;]</klr> <tld/>is ventoj
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 7:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;tuna vento jam <tld/>is tra la truoj de la tegmento
        <fnt>
          <bib>Fab3</bib>
          <lok>Koboldo &ccirc;e la butikisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vento <tld/>is en &scirc;ian mantelon, kiu etendi&gcirc;adis &ccirc;iuflanken simile al granda
        velo
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ek<tld/>is per la trumpetoj
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 7:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is sur la ardantajn karbojn, ili prilumis lian krete blankan viza&gcirc;on,
        kiu de tio iom ru&gcirc;eti&gcirc;is
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malgranda knabo devis longe <klr>[&#x2026;]</klr> vapor<tld/>i sur la fenestrojn,
        por ricevi truon, tra kiu li povus rigardi
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is al ili en la fa&ubreve;kon tiel, ke ili lernis teni la bekon fermita
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i per la korno de ribelo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="instru2.blov0o">blovinstrumento</ref>,
       <ref cel="spir.0i">spiri</ref>
    </refgrp>
    <trdgrp lng="pl">
      <trd>dmucha&cacute;</trd>,
      <trd>d&aogonek;&cacute;</trd>,
      <trd>wia&cacute;</trd>
    </trdgrp>
    <trd lng="sk">via&tcaron;</trd>
  </snc>
  <snc mrk="blov.0i.tr">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Pu&scirc;i la aeron sur a&ubreve; en ion:
      <ekz>
        <klr>[li]</klr> <tld/>is sian teon, por &gcirc;in iom malvarmigi
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la vento <tld/>is la velojn kaj la &scirc;ipo glitis facile
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi la]</klr> sablon turno<tld/>is alten en formo de grandaj kolonoj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i re<tld/>as en li la fajron de l&#x2019; ven&gcirc;emo
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        el<tld/>i
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        trans<tld/>i
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve;<tld/>i
        <fnt>
          <bib>Fab3</bib>
          <lok>&Ccirc;e la plej ekstrema maro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="ekscit.0i">eksciti</ref>,
       <ref cel="flam.0igi">flamigi</ref>
    </refgrp>
    <trdgrp lng="pl">
      <trd>dmucha&cacute;<klr> na co&sacute;, w co&sacute;</klr></trd>,
      <trd>d&aogonek;&cacute;<klr> w co&sacute;</klr></trd>
    </trdgrp>
    <trd lng="sk">f&uacute;ka&tcaron;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_mol;&c_m;&c_u;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_z;&c_mol;&c_m;&c_u;&c_h;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_z;&c_ib;&c_m;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF55;&#xF74;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF42;&#xF0B;</trd>
  <trd lng="br">c'hwezha&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>fouknout</trd>,
    <trd>vanout</trd>,
    <trd>zafoukat</trd>,
    <trd>zavanout</trd>,
    <trd>za&ccaron;&iacute;t foukat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>blasen</trd>,
    <trd>wehen</trd>,
    <trd>pusten</trd>
  </trdgrp>
  <trd lng="en">blow</trd>
  <trd lng="es">soplar</trd>
  <trd lng="fr">souffler</trd>
  <trdgrp lng="he">
    <trd>&#x5DC;&#x5E0;&#x5E9;&#x5D5;&#x5E3;</trd>,
    <trd>&#x5DC;&#x5E0;&#x5E9;&#x5D5;&#x5D1;</trd>
  </trdgrp>
  <trd lng="hu">f&uacute;j</trd>
  <trdgrp lng="ja">
    <trd>&#x5439;&#x304F;  [<ind>&#x3075;&#x304F;</ind>]</trd>,
    <trd>&#x606F;&#x3092;&#x5410;&#x304F;  [<ind>&#x3044;&#x304D;&#x3092;&#x306F;&#x304F;</ind>]</trd>,
    <trd>&#x5439;&#x304D;&#x9CF4;&#x3089;&#x3059;  [<ind>&#x3075;&#x304D;&#x306A;&#x3089;&#x3059;</ind>]</trd>
  </trdgrp>
  <trd lng="ne">&#x91D;&#x91F;&#x915;&#x93E;</trd>
  <trd lng="pt">soprar</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pref&uacute;ka&tcaron; <klr>(r&uacute;ru)</klr></trd>,
    <trd>tiahnu&tcaron; (o prievane)</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_u;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&#x44F;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i; <klr>(&c_shch;&#x43E;-&#x43D;.)</klr></trd>,
    <trd>&c_u;&#x43B;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="blov.0o">
  <kap><tld/>o</kap>
  <snc mrk="blov.0o.KOMUNE">
    <dif>
      Pu&scirc;o de aero:
      <ekz>
        diru al mi, &ccirc;u ni parolos pri la vetero kaj <tld/>o,
        a&ubreve; &ccirc;u ni estos sinceraj
        <fnt>
          <aut>L. Dilling, trad. E. Hall</aut>,
          <vrk>La Porcio da Glacia&jcirc;o</vrk>,
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>proud vzduchu</trd>,
    <trd>z&aacute;van</trd>
  </trdgrp>
  <trd lng="fr">souffle</trd>
  <trd lng="he">&#x5DE;&#x5E9;&#x5D1;</trd>
  <trdgrp lng="hu">
    <trd>f&uacute;j&aacute;s</trd>,
    <trd>fuvallat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x98A8;  [<ind>&#x3075;&#x3046;</ind>]</trd>,
    <trd>&#x4E00;&#x5439;&#x304D;  [<ind>&#x3072;&#x3068;&#x3075;&#x304D;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podmuch</trd>,
    <trd>powiew</trd>,
    <trd>dmuchni&eogonek;cie</trd>,
    <trd>tchnienie</trd>
  </trdgrp>
  <trd lng="ro">sufla</trd>
  <trd lng="ru">&c_d;&c_u;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">z&aacute;van</trd>
  <trd lng="uk">&#x43F;&#x43E;&c_d;&c_u;&c_v;</trd>
</drv>

<drv mrk="blov.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="blov.0ado.KOMUNE">
    <dif>
      Granda a&ubreve; da&ubreve;ra pu&scirc;o de aero:
      <ekz>
        <ind>ne&gcirc;a <tld/>ado</ind>
        <klr>(ne&gcirc;a ventego)</klr>
        <trdgrp lng="be">
          <trd>&c_z;&c_a;&c_v;&c_je;&c_ja;</trd>,
          <trd>&c_z;&c_a;&c_v;&c_ib;&c_r;&c_u;&c_h;&c_a;</trd>
        </trdgrp>
        <trd lng="hu">h&oacute;f&uacute;v&aacute;s</trd>
        <trdgrp lng="pl">
          <trd>zamie&cacute;</trd>,
          <trd>zawieja</trd>,
          <trd>zadymka</trd>,
          <trd>zawierucha</trd>
        </trdgrp>
        <trd lng="ru">&c_m;&c_je;&c_t;&c_je;&c_l;&c_mol;</trd>
        <fnt><bib>Fab2</bib>, <lok>ne&gcirc;a re&gcirc;ino</lok></fnt>;
      </ekz>
      <ekz>
        a&ubreve;di&gcirc;is <tld/>ado de delikata venteto
        <fnt><bib>MT</bib>, <lok>&Re1; 19:12</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_d;&c_y;&c_h;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_w;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>fouk&aacute;n&iacute;</trd>,
    <trd>vanut&iacute;</trd>
  </trdgrp>
  <trd lng="fr">souffle</trd>
  <trd lng="he">&#x5DE;&#x5B7;&#x5E9;&#x5C1;&#x5B8;&#x5D1;</trd>
  <trd lng="hu">f&uacute;v&aacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#x5D50;  [<ind>&#x3042;&#x3089;&#x3057;</ind>]</trd>,
    <trd>&#x9001;&#x98A8;  [<ind>&#x305D;&#x3046;&#x3075;&#x3046;</ind>]</trd>,
    <trd>&#x5439;&#x594F;  [<ind>&#x3059;&#x3044;&#x305D;&#x3046;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wicher</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_u;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>f&uacute;kanie</trd>,
    <trd>vanutie</trd>
  </trdgrp>
</drv>

<drv mrk="blov.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="blov.0ajxo.KOMUNE">
    <dif>
      La aero, spira&jcirc;o, odoro, kiu estas eligata a&ubreve;
      alportata de la <tld/>ado:
      <ekz>
        kovrita de tuko el blanka ne&gcirc;a <tld/>a&jcirc;o
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arom.0o">aromo</ref>,
      <ref cel="haladz.0o">haladzo</ref>,
      <ref cel="parfum.0o">parfumo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_d;&c_y;&c_h;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_je;&c_w;</trd>
  </trdgrp>
  <trd lng="br">c'hwezhadenn</trd>
  <trdgrp lng="de">
    <trd>Brise</trd>,
    <trd>Hauch</trd>,
    <trd>Puste</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bouff&eacute;e</trd>,
    <trd>exhalaison</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5B7;&#x5E9;&#x5C1;&#x5B8;&#x5D1;</trd>
  <trdgrp lng="hu">
    <trd>fuvallat</trd>,
    <trd>fuvalom</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7A7A;&#x6C17;  [<ind>&#x304F;&#x3046;&#x304D;</ind>]</trd>,
    <trd>&#x306B;&#x304A;&#x3044;</trd>,
    <trd>&#x606F;  [<ind>&#x305D;&#x304F;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podmuch</trd>,
    <trd>powiew</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_u;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="blov.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="blov.0ilo.KOMUNE">
    <dif>
      Speciala ilo por <tld/>i:
      <ekz>
        li revigligis la fajron per <tld/>ilo;
      </ekz>
      <ekz>
        la <tld/>ilo bruldifekti&gcirc;is, la plumbo malaperis en la fajro
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 6:29</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="balg.0o">balgo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_h; <klr tip="amb">(&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;)</klr></trd>
  <trdgrp lng="br">
    <trd>megin</trd>,
    <trd>c'hwezherez</trd>
  </trdgrp>
  <trd lng="cs">m&ecaron;ch <klr>(varhan)</klr></trd>
  <trd lng="de">Blasebalg</trd>
  <trd lng="fr">soufflet<klr tip="ind"> (instrument)</klr></trd>
  <trd lng="he">&#x5DE;&#x5B7;&#x5E4;&#x5BC;&#x5D5;&#x5BC;&#x5D7;&#x5B7;</trd>
  <trd lng="hu">f&uacute;jtat&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x3075;&#x3044;&#x3054;</trd>,
    <trd>&#x9001;&#x98A8;&#x6A5F;  [<ind>&#x305D;&#x3046;&#x3075;&#x3046;&#x304D;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">dmuchawa</trd>
  <trd lng="ru">&c_m;&c_je;&c_h;&c_i; <klr tip="amb">(&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
  <trd lng="sk">f&uacute;ka&ccaron; <klr>(n&aacute;stroj)</klr></trd>
  <trd lng="uk">&#x43A;&#x43E;&c_v;&c_a;&#x43B;&#x44C;&c_s;&#x44C;&#x43A;&c_i;&c_j; &#x43C;&c_ib;&c_h;</trd>
</drv>

<drv mrk="blov.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="blov.cxirkaux0i.KOMUNE">
    <dif>
      &Ccirc;irka&ubreve;i <tld/>ante,
      <tld/>i al la &ccirc;irka&ubreve;o de:
      <ekz>
        la akra vento &ccirc;irka&ubreve;<tld/>is lian korpon
        <fnt><bib>Fab1</bib>, <lok>&gcirc;ardeno de la
        paradizo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">umwehen</trd>
  <trd lng="hu">k&ouml;rbef&uacute;j</trd>  
  <trd lng="pl">owiewa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_d;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_d;&c_u;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="blov.dis0i">
  <kap>dis<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="blov.dis0i.KOMUNE">
    <dif>
      Disigi per <tld/>o:
      <ekz>
        ili estas kiel grenventuma&jcirc;o, kiun dis<tld/>as la vento
        <fnt><bib>MT</bib>, <lok>&Psa; 1:4</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Saul</nom> dis<tld/>igis per trumpeto <klr>(<ref cel="sci.0igi">sciigis</ref>)</klr>
        tra la tuta lando <klr>[&#x2026;]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 13:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dis<tld/>i la iluziojn de iu.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_mol;&c_v;&c_je;&c_ja;&c_c;&c_mol;</trd>
  <trd lng="cs">rozfoukat <klr>(od sebe)</klr></trd>
  <trdgrp lng="de">
    <trd>wegblasen</trd>,
    <trd>verwehen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>souffler <klr tip="ind">(disperser en soufflant)</klr></trd>,
    <trd>dissiper <klr tip="ind">(fig.)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5E0;&#x5E9;&#x5D5;&#x5D1; &#x5D5;&#x5DC;&#x5E4;&#x5D6;&#x5E8;</trd>
  <trd lng="hu">sz&eacute;tf&uacute;j</trd>
  <trd lng="ja">&#x5439;&#x304D;&#x6563;&#x3089;&#x3059;  [<ind>&#x3075;&#x304D;&#x3061;&#x3089;&#x3059;</ind>]</trd>
  <trdgrp lng="pl">
    <trd>rozwiewa&cacute;</trd>,
    <trd>rozdmuchiwa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_ja;&c_t;&c_mol;</trd>
  <trd lng="sk">rozf&uacute;ka&tcaron;</trd>
  <trd lng="uk">&#x440;&#x43E;&#x437;&#x432;&#x456;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="blov.dis0igxi">
  <kap>dis<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="blov.dis0igxi.KOMUNE">
    <dif>
      Disi&gcirc;i a&ubreve; diseri&gcirc;i en la &ccirc;irka&ubreve;a aero;
      <ref tip="vid" cel="aper.mal0i">malaperi</ref>:
      <ekz>
        Dis<tld/>i&gcirc;as la&ubreve; la vento /
        &Scirc;iaj densaj brunaj haroj...
        <fnt>
          <aut>V. N. Devjatnin</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Poezio/velga.html">
          Velga</url></vrk>,
          <lok>[19??]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>dissiper</ind></trd>,
    <trd>s'<ind>&eacute;vanouir</ind><klr> (disparaitre)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="blov.en0i">
  <kap>en<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="blov.en0i.KOMUNE">
