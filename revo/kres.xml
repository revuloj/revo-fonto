<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kres.xml,v 1.21 2022/03/22 15:44:13 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kres</rad>/o
</kap>
<drv mrk="kres.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Iu el pluraj specioj el familio 
      <ref tip="malprt" cel="brasik1.0oj">brasikacoj</ref>,
      <ref tip="lst" cel="herb.0o.BOT" lst="voko:brasikoparencoj">herbo</ref>
      kun akreta gusto, uzata kiel <ref tip="lst" lst="voko:spicoj" cel="spic.0o">spico</ref>
      kaj salato:
      <ekz>
        la &gcirc;arden<tld/>o (<nom>Lepidium sativum</nom>) <klr>[&#x2026;]</klr>
        havanta milde akrajn foliojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=97118">&Gcirc;ardenkreso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        al la frankfurta verda sa&ubreve;co tradicie apartenas almena&ubreve; sep
        herboj:
        borago, cerefolio, <tld/>o, petroselo, pimpinelo, okzalo kaj
        &scirc;enoprazo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=97120">Verda
          sa&ubreve;co</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        skorbutherbo gustas kiel <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=557842">Skorbutherbo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">morritort</trd>
  <trd lng="en">cress</trd>
  <trdgrp lng="es">
    <trd>mastuerzo</trd>,
    <trd>berro</trd>
  </trdgrp>
  <trd lng="de">Kresse</trd>
  <trd lng="fr">cresson</trd>
  <trd lng="hu">zs&aacute;zsa</trd>
  <trd lng="nl">tuinkers</trd>
  <trd lng="no">karse</trd>
  <trdgrp lng="pl">
    <trd>rze&zdot;ucha</trd>,
    <trd>pieprzyca</trd>
  </trdgrp>
  <trd lng="pt">agri&atilde;o</trd>
  <trd lng="ro">creson</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_s;&c_s;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_s;&c_s;-&c_s;&c_a;&c_l;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="uk">&#x445;&#x440;&#x456;&#x43D;&#x43D;&#x438;&#x446;&#x44F;</trd>
</drv>

<drv mrk="kres.akvo0o">
  <kap>
    akvo<tld/>o
  </kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">Specio</ref>
      el familio <ref tip="malprt" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Nasturtium officinale</trd>),
      man&gcirc;ebla <ref tip="lst" cel="herb.0o.BOT" lst="voko:brasikoparencoj">herbo</ref>, kiu kreskas en
      subakvaj lokoj:
      <ekz>
        la akvo<tld/>o estas plurjara, moltiga planto, <klr>[&#x2026;]</klr>
        vivas
        apud fluakvoj, kiuj ne frostas e&ccirc; vintre <klr>[&#x2026;]</klr>, en
        Francio, oni 
        kultivas akvo<tld/>on ekde la la 14-a jarcento
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=97119">Akvokreso</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">creixen</trd>
  <trd lng="en">watercress</trd>
  <trd lng="es">berro</trd>
  <trd lng="de">Brunnenkresse</trd>
  <trd lng="fr">cresson de fontaine</trd>
  <trd lng="hu">v&iacute;zitorma</trd>
  <trd lng="nl">waterkers</trd>
  <trd lng="no">br&oslash;nnkarse</trd>
  <trd lng="pl">rukiew wodna</trd>
  <trd lng="pt">mastru&ccedil;o-dos-rios</trd>
  <trd lng="ro">n&abreve;sturel</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_r;&c_u;&c_n;&c_k;&c_r;&c_je;&c_s;&c_s;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; &c_k;&c_r;&c_je;&c_s;&c_s;</trd>,
    <trd>&c_zh;&c_je;&c_r;&c_u;&c_h;&c_a; &c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
