<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kres</rad>/o
</kap>
<drv mrk="kres.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Iu el pluraj specioj el familio <ref tip="super"
      cel="brasik1.0oj">brasikacoj</ref> kun
      akreta gusto, uzata kiel spica&jcirc;o kaj
      salato.
    </dif>
  </snc>
  <trd lng="en">cress</trd>
  <trd lng="de">Kresse</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_s;&c_s;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_s;&c_s;-&c_s;&c_a;&c_l;&c_a;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="kres.akvo0o">
  <kap>
    akvo<tld/>o
  </kap>
  <snc>
    <dif>
      Specio el familio <ref tip="super" cel="brasik1.0oj">brasikacoj</ref>
      (<trd lng="la">Nasturtium officinale</trd>),
      man&gcirc;ebla vegeta&jcirc;o, kiu kreskas en
      subakvaj lokoj.
    </dif>
  </snc>
  <trd lng="en">watercress</trd>
  <trd lng="de">Brunnenkresse</trd>
  <trdgrp lng="ru">
    <trd lng="ru">&c_b;&c_r;&c_u;&c_n;&c_k;&c_r;&c_je;&c_s;&c_s;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_ja;&c_n;&c_o;&c_j; &c_k;&c_r;&c_je;&c_s;&c_s;</trd>,
    <trd>&c_zh;&c_je;&c_r;&c_u;&c_h;&c_a; &c_l;&c_je;&c_k;&c_a;&c_r;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
