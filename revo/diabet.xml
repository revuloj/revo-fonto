<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>diabet</rad>/o
</kap>
<drv mrk="diabet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="san.mal0o.MED"
      lst="voko:malsanoj">Malsano</ref> karakterizata per neasimilado
      kaj perurina ellasado de
      sukero.   
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_ja;&c_b;&c_e;&c_t;</trd>
  <trdgrp lng="de">
     <trd>Zuckerkrankheit</trd>,
     <trd>Zucker</trd>,
     <trd>Diabetes</trd>,
     <trd>Diabetes mellitus</trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_i;&c_a;&c_b;&c_je;&c_t;</trd>
  <trd lng="ca">diabetis</trd>
  <trd lng="en">diabetes</trd>
  <trd lng="es">diabetes</trd>
  <trd lng="fr">diab&egrave;te</trd>
  <trdgrp lng="hu">
    <trd>cukorbetegs&eacute;g</trd>,
    <trd>diabetesz</trd>
  </trdgrp>
  <trd lng="id">diabetes</trd>
  <trdgrp lng="nl">
    <trd>diabetes</trd>,
    <trd>suikerziekte</trd>
  </trdgrp>
  <trd lng="pl">cukrzyca</trd>
  <trdgrp lng="pt">
    <trd>diabetes</trd>,
    <trd>diabete</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_a;&c_b;&c_je;&c_t;</trd>
  <trd lng="sk">cukrovka</trd>
  <trd lng="sv">diabetes</trd>
</drv>
</art>
