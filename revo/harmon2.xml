<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>harmonik</rad>/o
</kap>
<drv mrk="harmon2.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="harmon2.0o.SCI">
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref> pri la harmonio.
    </dif>
    <trd lng="de">Harmonielehre</trd>
    <trd lng="nl">harmonie</trd>
    <trd lng="ru">&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="harmon2.0o.muzikilo">
    <dif>
      <ref tip="dif" cel="harmon2.busx0o">Bu&scirc;harmoniko</ref>.
    </dif>
  </snc>
  <snc>
    <dif>
      <ref tip="dif" cel="akordi.0o">Akordiono</ref>.
    </dif>
  </snc>
</drv>

<drv mrk="harmon2.blov0o">
  <kap>
    blov<tld/>o        
  </kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="harmon2.busx0o">Bu&scirc;harmoniko</ref>.
    </dif>
  </snc>
</drv>

<drv mrk="harmon2.busx0o">
  <kap>
    bu&scirc;<tld/>o        
  </kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.blov0o">Blovinstrumento</ref>
      konsistanta el klavaro da metalaj a&ubreve; vitraj lamenoj
      (frap<tld/>o) a&ubreve; el vico da truoj garnitaj per vibrantaj
      lamenetoj, tra kiu oni blovas.
      <ref tip="sin" cel="harmon2.0o.muzikilo">Harmoniko</ref>.
      <ref tip="sin" cel="harmon2.blov0o">Blovharmoniko</ref>.
    </dif>
  </snc>
  <trd lng="de">Mundharmonika</trd>
  <trd lng="fr">harmonica</trd>
  <trd lng="nl">mondharmonica</trd>
  <trd lng="ru">&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_k;&c_a;
  <klr>(&c_g;&c_u;&c_b;&c_n;&c_a;&c_ja; &c_i; &c_t;.&c_p;.)</klr></trd>
</drv>

<drv mrk="harmon2.tir0o">
  <kap>
    tir<tld/>o
  </kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="akordi.0o">Akordiono</ref>.
    </dif>
  </snc>
</drv>
</art>
