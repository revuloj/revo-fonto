<vortaro>
<art mrk="$Id: harmon2.xml,v 1.31 2019/06/30 07:42:48 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>harmonik</rad>/o
</kap>
<drv mrk="harmon2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="harmon2.0o.muzikilo">
    <dif>
      <ref tip="lst" lst="voko:muzikiloj" cel="instru2.muzik0o">Muzikinstrumento</ref>, 
      &ccirc;e kiu la sono esti&gcirc;as per langetoj fermantaj kameretojn, kiuj 
      svingi&gcirc;as per aero elfluanta la kamereton preter ili:
      <ekz>
        dum 13 jaroj kiel posedo de <nom>Hohner</nom>, novaj teknologioj 
        estis aldonitaj kaj plibonigis la jam bonajn <tld/>ojn de <nom>Hering</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=213876">Hering</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        klavaroj havas blankajn kaj nigrajn klavojn en difinita ordo, 
        aliajn ordojn oni trovas <klr>[&ccirc;e]</klr> la <tld/>o a&ubreve; la bandonio,
        kiuj<klr>[n]</klr> oni ludas per butonoj anstata&ubreve; klavoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=224438">Klavarinstrumento</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="harmon2.busx0o">bu&scirc;<tld/>o</ref>,
      <ref cel="harmon2.tir0o">tirharmoniko</ref>
    </refgrp>
    <trd lng="de">Hamonika</trd>
    <trdgrp lng="en">
      <trd>harmonica</trd>,
      <trd><klr>(mouth) </klr>harp</trd>
    </trdgrp>        
    <trd lng="br">harmonika</trd>
    <trd lng="ca">harm&ograve;nica</trd>
    <trdgrp lng="es">
      <trd>arm&oacute;nica</trd>,
      <trd>harm&oacute;nica</trd>
    </trdgrp>        
    <trd lng="fr">harmonica</trd>
    <trd lng="hu">harmonika</trd>
    <trd lng="id">harmonika</trd>
    <trd lng="it">armonica<klr> (sost.)</klr></trd>
    <trd lng="pl">harminijka<klr> ustna</klr></trd>
    <trdgrp lng="pt">
     <trd>gaita<klr tip="amb"> (de boca)</klr></trd>, 
     <trd>harm&ocirc;nica<klr tip="amb"> (de boca)</klr></trd> 
    </trdgrp>
    <trd lng="ru">&c_g;&c_u;&c_b;&c_n;&c_a;&c_ja;
    <ind>&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_k;&c_a;</ind></trd>
  </snc>
