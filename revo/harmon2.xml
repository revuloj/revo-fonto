<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>harmonik</rad>/o
</kap>
<drv mrk="harmon2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="harmon2.0o.SCI">
    <dif>
      <ref tip="super" cel="scienc.0o.fako">Scienco</ref> pri la
      harmonio.
    </dif>
    <adm>
      Tiu senco mankas en PIV1. Gxin anstatauxas "~scienco".
      <aut>MB</aut>
    </adm>                            
    <trd lng="br">klotadouriezh</trd>
    <trd lng="de">Harmonielehre</trd>
    <trd lng="en">harmonics</trd>
    <trd lng="es">armon&iacute;a</trd>
    <trd lng="fr">harmonie<klr tip="ind"> (science musicale)</klr></trd>
    <trd lng="hu">&ouml;sszhangzattan</trd>
    <trd lng="nl">harmonie</trd>
    <trd lng="pl">harmonika</trd>
    <trd lng="pt">harmonia<klr tip="amb"> (ci&ecirc;ncia)</klr></trd>
    <trd lng="ru">&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="harmon2.0o.muzikilo">
    <ref tip="dif" cel="harmon2.busx0o">bu&scirc;<tld/>o.</ref>
    <trdgrp lng="en">
      <trd>harmonica</trd>,
      <trd><klr>(mouth) </klr>harp</trd>
    </trdgrp>        
    <trd lng="br">harmonika</trd>
    <trd lng="es">harm&oacute;nica</trd>
    <trd lng="fr">harmonica</trd>
    <trd lng="hu">harmonika</trd>
    <trd lng="pl">harminijka<klr> ustna</klr></trd>
    <trdgrp lng="pt">
     <trd>gaita<klr tip="amb"> (de boca)</klr></trd>, 
     <trd>harm&ocirc;nica<klr tip="amb"> (de boca)</klr></trd> 
    </trdgrp>
    <trd lng="ru">&c_g;&c_u;&c_b;&c_n;&c_a;&c_ja;
    <ind>&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_k;&c_a;</ind></trd>
  </snc>
  <snc mrk="harmon2.0o.akordiono">
    <ref tip="dif" cel="akordi.0o">akordiono.</ref>  
    <trd lng="br">akordeon</trd>
    <trd lng="en">accordion</trd>
    <trd lng="es">acorde&oacute;n</trd>
    <trd lng="hu">tang&oacute;harmonika</trd>
    <trd lng="nl">harmonica</trd>
    <trd lng="pl">harmonia</trd>
    <trdgrp lng="pt">
     <trd>concertina</trd>, 
     <trd>acorde&atilde;o</trd>,
     <trd>cordeona</trd>, 
     <trd>sanfona</trd>, 
     <trd>gaita<klr tip="amb"> (concertina)</klr></trd>, 
     <trd>harm&ocirc;nica<klr tip="amb"> (concertina)</klr></trd> 
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_mol;</trd>,
      <trd>&c_b;&c_a;&c_ja;&c_n;</trd>,
      <trd>&c_a;&c_k;&c_k;&c_o;&c_r;&c_d;&c_je;&c_o;&c_n;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="harmon2.blov0o">
  <kap>blov<tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <ref tip="dif" cel="harmon2.busx0o">bu&scirc;harmoniko.</ref>
    <trdgrp lng="en">
      <trd>harmonica</trd>,
      <trd><klr>(mouth) </klr>harp</trd>
    </trdgrp>    
    <trd lng="br">harmonika</trd>
    <trd lng="es">herm&oacute;nica</trd>
    <trd lng="fr">harmonica</trd>
    <trd lng="hu">sz&aacute;jharmonika</trd>
    <trd lng="nl">mondharmonica</trd>
    <trd lng="pl">harmonijka<klr> ustna</klr></trd>
    <trdgrp lng="pt">
     <trd>gaita<klr tip="amb"> (de boca)</klr></trd>,
     <trd>harm&ocirc;nica<klr tip="amb"> (de boca)</klr></trd> 
    </trdgrp>
    <trd lng="ru">&c_g;&c_u;&c_b;&c_n;&c_a;&c_ja;
    <ind>&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_k;&c_a;</ind></trd>
  </snc>
</drv>

<drv mrk="harmon2.busx0o">
  <kap>bu&scirc;<tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.blov0o">Blovinstrumento</ref>,
      konsistanta el vico da truoj garnitaj per vibrantaj
      lamenetoj, tra kiu oni blovas.
    </dif>
    <refgrp tip="sin">
      <ref cel="harmon2.0o.muzikilo"><tld/>o
      <sncref ref="harmon2.0o.muzikilo"/></ref>,
      <ref cel="harmon2.blov0o">blov<tld/>o</ref>.
    </refgrp>
  </snc>        
  <trd lng="br">harmonika</trd>
  <trd lng="de">Mundharmonika</trd> 
  <trdgrp lng="en">
     <trd>harmonica</trd>,
     <trd><klr>(mouth) </klr>harp</trd>
  </trdgrp>
  <trd lng="es">harm&oacute;nica</trd>
  <trd lng="fr">harmonica</trd>
  <trd lng="hu">sz&aacute;jharmonika</trd>
  <trd lng="nl">mondharmonica</trd>
  <trd lng="pl">harmonijka<klr> ustna</klr></trd>
  <trdgrp lng="pt">
   <trd>gaita<klr tip="amb"> (de boca)</klr></trd>,
   <trd>harm&ocirc;nica<klr tip="amb"> (de boca)</klr></trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_u;&c_b;&c_n;&c_a;&c_ja;
  <ind>&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_i;&c_k;&c_a;</ind></trd>
</drv>

<drv mrk="harmon2.frap0o">
  <kap>frap<tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="instru2.frap0o">Frapinstrumento</ref>,
      konsistanta el vico da metalaj a&ubreve; lignaj lamenoj, sur kiujn
      oni frapas.
    </dif>
  </snc>
  <adm>
  La "difino", kiun eblas cxerpi en PV, estas misvojiga, cxar
  gxi parolas pri "klavaro". Entute mi ne scias, cxu la termino
  havas utilon: sxajnas, ke la nuraj muzikiloj de tiu speco
  estas ksilofono kaj "metalofono" (ne-PIVa). <aut>MB</aut>
  </adm>             
  <trd lng="br">zilofon</trd>
  <trd lng="fr">xylophone</trd>
  <trd lng="pt">xilofone</trd>
  <trd lng="ru">&c_k;&c_s;&c_i;&c_l;&c_o;&c_f;&c_o;&c_n;</trd>
</drv>

<drv mrk="harmon2.tir0o">
  <kap>tir<tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <ref tip="dif" cel="akordi.0o">akordiono.</ref>
  </snc>       
  <trd lng="br">akordeon</trd>
  <trd lng="en">accordion</trd>
  <trd lng="es">acorde&oacute;n</trd>
  <trd lng="fr">accord&eacute;on</trd>
  <trdgrp lng="hu">
    <trd>tang&oacute;harmonika</trd>,
    <trd>akordeon</trd>
  </trdgrp>
  <trd lng="nl">trekharmonica</trd>
  <trdgrp lng="pl">
    <trd>harminia</trd>,
    <trd>akordeon</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>concertina</trd>, 
   <trd>acorde&atilde;o</trd>,
   <trd>cordeona</trd>, 
   <trd>sanfona</trd>, 
   <trd>gaita<klr tip="amb"> (concertina)</klr></trd>, 
   <trd>harm&ocirc;nica<klr tip="amb"> (concertina)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_r;&c_m;&c_o;&c_n;&c_mol;</trd>,
    <trd>&c_a;&c_k;&c_k;&c_o;&c_r;&c_d;&c_je;&c_o;&c_n;</trd>,
    <trd>&c_b;&c_a;&c_ja;&c_n;</trd>
  </trdgrp>
</drv>
</art>
