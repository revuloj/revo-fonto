<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kis.xml,v 1.39 2023/12/03 09:58:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kis</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kis.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kis.0i.FIZL">
    <dif>
      <ref tip="super" cel="tusx.0i.kontakti">Tu&scirc;i</ref> per la lipoj por 
      esprimi amon a&ubreve; respekton:
      <ekz>
        <nom>Jakob</nom> <tld/>is <nom>Ra&hcirc;el</nom>on kaj la&ubreve;te ekploris
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 29:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel moderna perfektigita servisto <klr>[li]</klr> anstata&ubreve; veni <tld/>i la
        manon de la juna sinjoro, nur salutis lin de malproksime
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>II</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni permesis al la civilaj kaj militistaj altranguloj <tld/>i la malsupran
        &scirc;tupon de l' trono
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ida</nom> <tld/>is la florojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &scirc;in <tld/>is sur &scirc;ia ru&gcirc;a bu&scirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        nevidate &scirc;i <tld/>is la frunton de la fian&ccirc;ino
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is la larmojn de &scirc;iaj okuloj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Gerda</nom> <tld/>is al li la vangojn, kaj ili fari&gcirc;is rozokoloraj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> genui&gcirc;is por <tld/>i la teron
        <fnt>
          <bib>Rob</bib>
          <lok>Parto IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        batu malbonulon, li vin flatos, <tld/>u, li vin batos
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">besar</trd>
    <trdgrp lng="es">
      <trd>dar un beso</trd>,
      <trd>besar</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>embrasser<klr tip="ind"> (avec les l&egrave;vres)</klr></trd>,
      <trd>donner un <ind>baiser</ind> (&agrave;)</trd>,
      <trd>faire la <ind>bise</ind> (&agrave;)</trd>,
      <trd>baiser<klr tip="ind"> (embrasser)</klr></trd>,
    </trdgrp>
    <trd lng="he">&lamed;<ind>&nun;&shin;&qof;</ind></trd>
    <trd lng="tr">&ouml;pmek</trd>
  </snc>
  <snc mrk="kis.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Milde, delikate <ref tip="super" cel="tusx.0eti">tu&scirc;eti</ref>:
      <ekz>
        trankvilaj margolfoj, kie la bran&ccirc;oj <tld/>as la klaran spegulon de la akvo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la radio de la suno <klr>[&#x2026;]</klr> <tld/>is la florantan pomarban bran&ccirc;on
        <fnt>
          <bib>Fab3</bib>
          <lok>Ekzistas diferenco</lok>
        </fnt>;
      </ekz>
      <ekz>
        alloga regiono, <tld/>ata de la maro kaj de la suno
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010686.php">Mizeriga petrolo</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>acaronar</trd>,
      <trd>amanyagar</trd>
    </trdgrp>
    <trd lng="es">acariciar<klr> (fig.)</klr></trd>
    <trdgrp lng="fr">
      <trd>effleurer</trd>,
      <trd>caresser<klr tip="ind"> (effleurer)</klr></trd>,
      <trd>l&eacute;cher<klr tip="ind"> (effleurer)</klr></trd>
    </trdgrp>
    <trd lng="he">&lamed;&nun;&shin;&vav;&qof; &lamed;- \<ind>&nun;&shin;&qof;</ind>\</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_a;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_c;&c_a;&c_l;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_c;&c_je;&c_l;&c_u;&c_v;&c_a;&c_m;</trd>
  <trd lng="bo">&t_-a;&t_naro;&t_tsheg;&t_ba;&t_yata;&t_drengbu;&t_da;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>hubi&ccaron;kovat</trd>,
    <trd>l&iacute;bat</trd>
  </trdgrp>
  <trd lng="de">k&uuml;ssen</trd>
  <trd lng="en">kiss</trd>
  <trd lng="fa">&a_b;&a_w;&a_s;&f_y;&a_d;&a_n;</trd>
  <trd lng="hu">cs&oacute;kol</trd>
  <trd lng="id">men<ind>cium</ind></trd>
  <trd lng="io">kisar</trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30B9;&#x3059;&#x308B;  <pr>&#x304D;&#x3059;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x53E3;&#x3065;&#x3051;&#x3059;&#x308B;  <pr>&#x304F;&#x3061;&#x3065;&#x3051;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x305D;&#x3063;&#x3068;&#x89E6;&#x308C;&#x308B;  <pr>&#x305D;&#x3063;&#x3068;&#x3075;&#x308C;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kussen<klr tip="ind"> (ww.)</klr></trd>,
    <trd>zoenen<klr tip="ind"> (ww.)</klr></trd>
  </trdgrp>
  <trd lng="pl">ca&lstroke;owa&cacute;</trd>     
  <trd lng="ro">s&abreve;ruta</trd>
  <trd lng="ru">&c_c;&c_je;&c_l;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">bozka&tcaron;</trd>
  <trd lng="uk">&c_c;&c_ib;&c_l;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trd lng="vo">kid&ouml;n</trd>
  <trdgrp lng="zh">
    <trd>&#x543B; <pr>w&#x11B;n</pr></trd>,
    <trd>&#x4EB2;&#x543B; <pr>q&#x12B;nw&#x11B;n</pr></trd>,
    <trd>&#x63A5;&#x543B; <pr>ji&#x113;w&#x11B;n</pr></trd>,
    <trd>&#x4EB2;&#x5634; <pr>q&#x12B;nzu&#x1D0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kis.0o">
  <kap><tld/>o</kap>
  <snc mrk="kis.0o.FIZL">
    <dif>
      Tu&scirc;ado per la lipoj:
      <ekz>
        &gcirc;uante la <tld/>ojn de la amatino
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kovradis ilin per <tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Elfo de la rozo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ojn mi ne donos
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>!
      </ekz>
      <ekz>
        li rompis la rozon, metis <tld/>on sur &gcirc;ian fre&scirc;an bu&scirc;on
        <fnt>
          <bib>Fab2</bib>
          <lok>Rozo de la tombo de Homero</lok>
        </fnt>;
      </ekz>
      <ekz>
        Klaro fermis al li la bu&scirc;on per <tld/>o
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i respondis per apena&ubreve; tu&scirc;anta, silenta <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        lia &gcirc;ojo estis tiel granda kiel la via, kiam vi premis la unuan kison sur la
        rozajn lipojn de via unua karulino
        <fnt>
          <bib>TAA</bib>
          <lok>&Ccirc;apitro II.</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni inter&scirc;an&gcirc;as <tld/>on kaj &ccirc;irka&ubreve;prenon kun &ccirc;iu el niaj amikoj
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>El Odeso &gcirc;is Konstantinopolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>perfingra <tld/>o</ind>
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
        <trd lng="de">Kusshand</trd>
        <trd lng="nl">kushandje</trd>
      </ekz>
      <ekz>
        Johano <klr>[&#x2026;]</klr> sendis al li per la fingroj multe da <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        salutu &ccirc;iujn fratojn per sankta <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>I. Tesalonikanoj 5:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        ordonita <tld/>o havas guston maldol&ccirc;an
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <tld/>o anta&ubreve; amaso estas <ind><tld/>o de Judaso</ind>
        <klr>(amika&jcirc;o de perfidonto)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="nl">judaskus</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="kis.0o.FIG">
    <dif>
      Delikata tu&scirc;eto:
      <ekz>
        la arbaron jam tu&scirc;is la flava <tld/>o
        de a&ubreve;tuno
        <fnt><bib>PV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_c;&c_a;&c_l;&c_u;&c_n;&c_a;&c_k;</trd>
  <trd lng="bg">&c_c;&c_je;&c_l;&c_u;&c_v;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>bes</trd>,
    <trd>pet&oacute;</trd>,
    <trd>besada</trd>,
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hubi&ccaron;ka</trd>,
    <trd>polibek</trd>,
    <trd>pol&iacute;ben&iacute;</trd>,
    <trd>pusa</trd>
  </trdgrp>
  <trd lng="de">Kuss</trd>
  <trd lng="en">kiss</trd>
  <trd lng="es">beso</trd>
  <trd lng="fa">&a_b;&a_w;&a_s;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>baiser<klr tip="ind"> (subst.)</klr></trd>,
    <trd>b&eacute;cot</trd>,
    <trd>bise<klr tip="ind"> (baiser)</klr></trd>,
    <trd>caresse<klr tip="ind"> (effleurement)</klr></trd>,
    <trd>effleurement</trd>
  </trdgrp>
  <trd lng="fy">t&uacute;t</trd>
  <trd lng="he">&nun;&shin;&yod;&qof;&he;</trd>
  <trd lng="hu">cs&oacute;k</trd>
  <trd lng="id"><ind>cium</ind>an</trd>
  <trd lng="io">kiso</trd>
  <trdgrp lng="ja">
    <trd>&#x30AD;&#x30B9;  <pr>&#x304D;&#x3059;</pr></trd>,
    <trd>&#x53E3;&#x3065;&#x3051;  <pr>&#x304F;&#x3061;&#x3065;&#x3051;</pr></trd>,
    <trd>&#x63A5;&#x543B;  <pr>&#x305B;&#x3063;&#x3077;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kus</trd>,
    <trd>zoen</trd>
  </trdgrp>
  <trd lng="oc">poton</trd>
  <trd lng="pl">poca&lstroke;unek</trd>     
  <trd lng="ro">s&abreve;rut</trd>
  <trd lng="ru">&c_p;&c_o;&c_c;&c_je;&c_l;&c_u;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>bozk</trd>,
    <trd>pusa</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_o;&c_c;&c_ib;&c_l;&c_u;&c_n;&c_o;&c_k;</trd>
  <trd lng="vo">kid</trd>
  <trdgrp lng="zh">
    <trd>&#x4EB2;&#x543B; <pr>q&#x12B;nw&#x11B;n</pr></trd>,
    <trd>&#x543B; <pr>w&#x11B;n</pr></trd>,
    <trd>&#x63A5;&#x543B; <pr>ji&#x113;w&#x11B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kis.inter0i">
  <kap>inter<tld/>i,
  <var><kap>inter<tld/>i&gcirc;i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kis.inter0i.FIZL">
    <dif>
      Reciproke <tld/>i:
      <ekz>
        la pubereco estas tiu tempo, en kiu knaboj kaj
        knabinoj ne scias, &ccirc;u ili interbatu a&ubreve; inter<tld/>u
        <fnt>
          <aut>Tatoeba</aut>,
          <vrk><url ref="http://tatoeba.org/eng/sentences/show/2272373">
          Frazo 2272373a</url></vrk>,
          <lok>[vidita en 2015]</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ne&hcirc;ludov</nom> inter<tld/>i&gcirc;is kun la kampulo
        <fnt>
          <aut>L. Tolstoj, trad. V. Sapo&jcirc;nikov</aut>,
          <vrk><url ref="&Sezonoj;2015/04/tolstoj/">
          Resurekto</url></vrk>,
          <lok>2000</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_a;&c_l;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">K&uuml;sse austauschen</trd>
  <trd lng="fr">s'<ind>embrasser</ind></trd>
  <trd lng="id">ber<ind>cium</ind>an</trd>
  <trd lng="pl">poca&lstroke;owa&cacute; si&eogonek;</trd>     
  <trd lng="ro">s&abreve; te s&abreve;rut</trd>
  <trd lng="uk">&c_c;&c_ib;&c_l;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
</drv>
</art>
