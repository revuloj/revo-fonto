        <trd lng="de">Kollimator</trd>
        <trd lng="en">Collimator</trd>
        <trd lng="fr">Collimateur</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="ja">&#x8996;&#x6E96;&#x3059;&#x308B;  <pr>&#x3057;&#x3058;&#x3085;&#x3093;&#x3059;&#x308B;</pr></trd>
