<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vaku.xml,v 1.25 2023/09/05 09:43:53 revo Exp $">
<kap>
  <ofc>9</ofc><rad>vaku</rad>/o
</kap>
<drv mrk="vaku.0o">
  <kap>
    <ofc>9</ofc><tld/>o
  </kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      Malplena spaco, okupita de neniu materio:
      <ekz>
        <nom>Albert Einstein</nom> mem, por prezenti sian teorion pri 
        relativeco, parolis pri lifto falanta en la <tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011597.php">&Scirc;ton&jcirc;eto sen cenzuro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <uzo tip="fak">POL</uzo>
        sekvojn de tiu &ccirc;i potenc-<tld/>o ni ne povas prognozi
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2006/008849.php">Katastrofo en Irako</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        meze de somerferia <tld/>o li lan&ccirc;is pa&scirc;on post pa&scirc;o sian planon
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2009/009891.php">Planoj kaj prognozoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="plen.mal0o">malpleno</ref>,
      <ref cel="neni.0o">nenio</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_k;&c_u;&c_m;</trd>,
    <trd>&c_p;&c_u;&c_s;&c_t;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">buit<klr> (subst.)</klr></trd>
  <trdgrp lng="de">
     <trd>Leere</trd>,
     <trd>Leerraum</trd>,
     <trd>Vakuum</trd>
  </trdgrp>
  <trd lng="el">&kappa;&epsilon;&ny;&omikron_ton;</trd>
  <trd lng="en">vacuum</trd>
  <trd lng="es">vac&iacute;o</trd>
  <trd lng="fa">&a_kh;&a_l;&a_A_hamza_sure;</trd>
  <trdgrp lng="fr">
    <trd>vide<klr> (physique)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&resh;&yod;&qof;<klr tip="ind"> (&vav;&alef;&qof;&vav;&fmem;)</klr></trd>,
    <trd>&het;&lamed;&lamed; &resh;&yod;&qof;</trd>,
    <trd>&vav;&alef;&qof;&vav;&fmem;</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>v&aacute;kuum</trd>,
     <trd>l&eacute;g&uuml;res t&eacute;r</trd>
  </trdgrp>
  <trd lng="ja">&#x771F;&#x7A7A;  [<ind>&#x3057;&#x3093;&#x304F;&#x3046;</ind>]</trd>
  <trd lng="nl">vacu&uuml;m</trd>
  <trd lng="pl">pr&oacute;&zdot;nia</trd>
  <trd lng="pt">v&aacute;cuo</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_k;&c_u;&c_u;&c_m;</trd>,
    <trd>&c_p;&c_u;&c_s;&c_t;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&#x43A;&c_u;&c_u;&#x43C;</trd>,
    <trd>&#x43F;&#x43E;&c_r;&#x43E;&c_zh;&#x43D;&c_je;&c_ch;&c_a;</trd>,
    <trd>&#x43F;&c_u;&c_s;&c_t;&#x43E;&c_t;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="vaku.0igi">
  <kap>
    <tld/>igi
  </kap>
  <snc>
    <dif>
      Eligi &ccirc;ian materion el ia ujo.
    </dif>
    <ref tip="vid" cel="evaku.0i">evakui</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_u;&c_s;&c_t;&c_a;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_u;&c_s;&c_t;&c_o;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_r;&c_a;&c_zh;&c_n;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">buidar</trd>
  <trdgrp lng="de">
     <trd>leeren</trd>,
     <trd>entleeren</trd>,
     <trd>leer machen</trd>,
     <trd>leerpumpen</trd>,
     <trd>evakuieren</trd>
  </trdgrp>
  <trd lng="en">make a vacuum</trd>
  <trd lng="es">vaciar</trd>
  <trdgrp lng="fr">
    <trd>faire le <ind>vide</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&he;<ind>&resh;&yod;&qof;</ind></trd>,
    <trd>&lamed;&resh;&vav;&qof;&fnun; \<ind>&resh;&qof;&fnun;</ind>\</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>v&aacute;kuumot hoz l&eacute;tre</trd>,
     <trd>l&eacute;g&uuml;res teret hoz l&eacute;tre</trd>
  </trdgrp>
  <trd lng="ja">&#x771F;&#x7A7A;&#x306B;&#x3059;&#x308B;  [<ind>&#x3057;&#x3093;&#x304F;&#x3046;&#x306B;&#x3059;&#x308B;</ind>]</trd>
  <trd lng="nl">vacu&uuml;m trekken</trd>
  <trd lng="pl">opr&oacute;&zdot;nia&cacute;</trd>
  <trdgrp lng="pt">
   <trd>fazer v&aacute;cuo</trd>, 
   <trd>esvaziar</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_t;&c_v;&#x43E;&c_r;&c_i;&c_t;&c_i; &c_v;&c_a;&#x43A;&c_u;&c_u;&#x43C;</trd>
</drv>
</art>
