<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: matado.xml,v 1.16 2013/09/05 16:10:11 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>matador</rad>/o
</kap>
<drv mrk="matado.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Homo, kies profesio estas mortigi per spado virbovon en virbovaj
      batalludoj.
    </dif>
    <trd lng="ca">matador</trd>
    <trd lng="pl">matador</trd>
    <trd lng="pt">matador <klr tip="amb">(tauromaquia)</klr></trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo tre influa, ludanta &ccirc;efan rolon en iu afero:
      <ekz>
        la <tld/>oj de la financo, de la administracio
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="korife.0o">korifeo</ref>.
    </refgrp>
    <trd lng="ca">manaia</trd>
  <trdgrp lng="pt">
    <trd>matador <klr tip="ind">(fig.)</klr></trd>,
    <trd>manda-chuva</trd>,
    <trd>chef&atilde;o</trd>,
    <trd>figur&atilde;o</trd>,
    <trd>magnata</trd>    
  </trdgrp>
  </snc>                       
  <trd lng="be">&c_m;&c_a;&c_t;&c_a;&c_d;&c_o;&c_r;</trd>
  <trd lng="bg">&c_m;&c_a;&c_t;&c_a;&c_d;&c_o;&c_r;</trd>
  <trd lng="br">matador</trd>
  <trd lng="de">Matador</trd>
  <trd lng="es">matador</trd>  
  <trd lng="fr">matador</trd>
  <trd lng="hu">matador</trd>
  <trd lng="nl">matador</trd>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_a;&c_d;&c_o;&c_r;</trd>
</drv>
</art>
