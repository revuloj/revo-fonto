<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>invalid</rad>/o
</kap>
<drv mrk="invali.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo, kiu ne plu kapablas plenumi sian laboron, servon, oficon
      pro kripleco a&ubreve; malforteco:
      <ekz>
        laborista, militista <tld/>o;
      </ekz>
    <ekz>
      internaj dokumentoj de la Administracio por Veteranaj Aferoj
      taksas, ke la kosto pro militistaj <tld/>oj post 25 jaroj kreskos
      de
      nunaj 29 miliardoj da dolaroj &gcirc;is 59 miliardoj.
      <fnt><aut>Roland THIBAULT</aut>, <vrk><url
      ref="http://www.esperanto.be/fel/2008/009538.php">Nombro de
      militistaj invalidoj kreskas</url></vrk>, <bib>Monato</bib>,
      <lok>2008/10, p. 16</lok></fnt>.
    </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="impote.0a">impotenta</ref>,
      <ref cel="kaduk.0a">kaduka</ref>,
      <ref cel="stump">stumpigita</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_v;&c_a;&c_l;&c_ib;&c_d;</trd>
  <trd lng="de">Invalide</trd>
  <trd lng="es">inv&aacute;lido</trd>
  <trd lng="fr">invalide</trd>
  <trd lng="hu">rokkant</trd>
  <trd lng="nl">invalide</trd>
  <trd lng="pt">inv&aacute;lido<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_v;&c_a;&c_l;&c_i;&c_d;</trd>
</drv>

<drv mrk="invali.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Stato de <tld/>o:
      <ekz>
        asekuro kontra&ubreve; <tld/>eco.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_v;&c_a;&c_l;&c_ib;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Invalidit&auml;t</trd>
  <trd lng="es">invalidez</trd>
  <trd lng="fr">invalidit&eacute;</trd>
  <trd lng="hu">rokkants&aacute;g</trd>
  <trd lng="nl">invaliditeit</trd>
  <trd lng="pt">invalidez</trd>
  <trd lng="ru">&c_i;&c_n;&c_v;&c_a;&c_l;&c_i;&c_d;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
