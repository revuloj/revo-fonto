<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: musx.xml,v 1.53 2025/11/27 20:30:27 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mu&scirc;</rad>/o <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="musx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="musx.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda duflugila harkovrita
      <ref tip="lst" cel="insekt.0o" lst="voko:mu&scirc;oj">insekto</ref>,
      kies diversaj specioj estas tre komunaj
      kaj vivas en lokoj lo&gcirc;ataj de homoj kaj bestoj;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="malprt" cel="musxed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Musca</trd>):
      <ekz>
        malgrandaj verdaj <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la zumo de l' <tld/>o kolerigis lin
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tajloreto <klr>[&#x2026;]</klr> senkompate batis la mu&scirc;ojn <klr>[&#x2026;]</klr>, sep per
        unu bato
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ricevi <tld/>on en la cerbon
        <fnt><bib>PrV</bib></fnt>
        <klr>(havi strangajn ideojn)</klr>;
      </ekz>
      <ekz>
        kalkuli <tld/>ojn <klr>(vane peni pri
        bagateloj)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/1/12/Housefly_musca_domestica_cropped_(2).png/320px-Housefly_musca_domestica_cropped_(2).png"
      prm="GFDL 1.2"
      >hejma <tld/>o (Musca domestica)
        <fnt>
          <aut>Muhammad Mahdi Karim, prilaborita de B. Kimmel</aut>
          <lok><url ref="&WCW;/File:Housefly_musca_domestica_cropped_(2).png"
              >https://commons.wikimedia.org/wiki/File:Housefly_musca_domestica_cropped_(2).png</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Stubenfliege</trd>,
      <trd>Mucke<klr> [sudgermana]</klr></trd>
    </trdgrp>
    <trd lng="en">housefly</trd>
    <trd lng="nl">paardenvlieg</trd>
  </snc>
  <snc mrk="musx.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Similforma insekto el la ordo
      <ref tip="malprt" cel="dipter.0oj">dipteroj</ref>:
      <ekz>
        la <tld/>oj ne &ccirc;esis pikadi
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unutaga <tld/>o
        <klr>(<ref cel="efemer3.0o">efemero</ref>)</klr>
        dancis kaj suprenflugis en la aeron
        <fnt>
          <bib>Fab4</bib>
          <lok>Lasta son&gcirc;o de maljuna kverko</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsani&gcirc;o, ka&ubreve;zita de infekto fare de la cecea <tld/>o
        <klr>(vd <ref cel="cece.0o">ceceo</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Andreas K&uuml;nzli</aut>
          <vrk><url ref="&Monato;/2013/011020.php"
              >Atentinda memuaro</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kul.0o">kulo</ref>,
      <ref cel="moskit.0o">moskito</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/d/d9/Mouche_verte_dos.jpg/408px-Mouche_verte_dos.jpg"
      prm="CC BY-SA 3.0"
      ><ind>or<tld/>o</ind> (Lucilia caesar)
        <fnt>
          <aut>KoS, propra verko</aut>
          <lok><url ref="&WCW;/File:Mouche_verte_dos.jpg"
              >https://commons.wikimedia.org/wiki/File:Mouche_verte_dos.jpg</url></lok>
        </fnt>
      <trd lng="ca">mosca verda</trd>
      <trd lng="de">Goldfliege</trd>
      <trd lng="en">common greenbottle</trd>
      <trd lng="hu">f&eacute;mz&ouml;ld <ind>d&ouml;gl&eacute;gy</ind></trd>
      <trd lng="id"><ind>lalat</ind> hijau</trd>
      <trd lng="la">Lucilia caesar</trd>
    </bld>
    <trd lng="de">Fliege</trd>
    <trd lng="en">fly <klr>(true fly)</klr></trd>
    <trd lng="nl">vlieg</trd>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_h;&c_a;</trd>
  <trd lng="bg">&c_m;&c_u;&c_h;&c_a;</trd>
  <trd lng="ca">mosca</trd>
  <trd lng="de">Fliege</trd>
  <trd lng="es">mosca</trd>
  <trd lng="fr">mouche</trd>
  <trd lng="hu">l&eacute;gy</trd>
  <trd lng="id">lalat</trd>
  <trd lng="pl">mucha</trd>
  <trd lng="pt">mosca<klr tip="amb">(zool.)</klr></trd>
  <trd lng="ro">musc&abreve;</trd>
  <trd lng="ru">&c_m;&c_u;&c_h;&c_a;</trd>
  <trd lng="tr">sinek</trd>
  <trdgrp lng="zh">
    <trd>&#x8747; <pr>y&iacute;ng</pr></trd>,
    <trd>&#x8805; <pr>y&iacute;ng</pr></trd>,
    <trd>&#x82CD;&#x8747; <pr>c&amacron;ngying</pr></trd>,
    <trd>&#x84BC;&#x8805; <pr>c&amacron;ngying</pr></trd>
  </trdgrp>
</drv>
<drv mrk="musx.M0o">
  <kap><tld lit="M"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Mus</mlg>
  <snc mrk="musx.M0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Musca</trd>):
      <ekz>
        NGC 4372 estas globa stelamaso en konstelacio <tld lit="M"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=117205"
              >NGC 4372</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_M;&c_u;&c_h;&c_a; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="en">Musca</trd>
  <trd lng="fr">Mouche<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="id">Musca<klr> (konstelasi)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x30CF;&#x30A8;</trd>,
    <trd>&#x3061;&#x3087;&#x3073;&#x3072;&#x3052;</trd>
  </trdgrp>
  <trd lng="pl">Mucha  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Musc&abreve;  <klr>(constela&tcomma;ie)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_u;&c_h;&c_a;</trd>,
    <trd>&c_m;&c_u;&c_sh;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x82CD;&#x8747;&#x5EA7; <pr>C&amacron;ngyingzu&ograve;</pr></trd>,
    <trd>&#x84BC;&#x8805;&#x5EA7; <pr>C&amacron;ngyingzu&ograve;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="musx.banan0o">
  <kap>banan<tld/>o, <var><kap>vinagro<tld/>o</kap></var></kap>
  <snc mrk="musx.banan0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Kelkmilimetra <ref tip="lst" cel="musx.0o.KOMUNE" lst="voko:mu&scirc;oj">mu&scirc;eto</ref> &scirc;atanta
      fruktojn;
      multespecia <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el
      familio drozofiledoj
      (<trd lng="la">Drosophila</trd>):
      <ekz>
        la tiel nomata vinagro<tld/>o allogi&gcirc;as al fermentantaj
        vinberoj
        <fnt>
          <aut>D. Fielding</aut>,
          <vrk>Drozofilo</vrk>,
          <lok><url ref="http://verdadrako.50g.com/bulteno10.html">
          La Verda Drako, 2005-05</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        grandan signifon por biologio akiris la banan<tld/>o
        <klr>[&#x2026;]</klr>, &scirc;atata esplorobjekto de genetiko pro siaj bone
        observeblaj kromosomoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Mu%C5%9Do">mu&scirc;o</url>,
          2010-02-10</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="drozofil.0o.ZOO">drozofilo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_a;&c_z;&c_a;&c_f;&c_ib;&c_l;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_d;&c_o;&c_v;&c_a;&c_ja; <ind>&c_m;&c_u;&c_sh;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>dros&ograve;fila</trd>,
    <trd>mosca del vinagre</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Taufliege</trd>,
    <trd>Obstfliege</trd>,
    <trd>Essigfliege</trd>,
    <trd>Fruchtfliege</trd>
  </trdgrp>
  <trd lng="en">fruit fly</trd>
  <trd lng="fr">drosophile</trd>
  <trd lng="id"><ind>lalat</ind> buah</trd>
  <trd lng="ja">&#x30B7;&#x30E7;&#x30A6;&#x30B8;&#x30E7;&#x30A6;&#x30D0;&#x30A8;</trd>
  <trdgrp lng="pl">
    <trd>muszka owocowa</trd>,
    <trd>muszka owoc&oacute;wka<klr> (pot.)</klr></trd>,
    <trd>drozofila</trd>
  </trdgrp>
  <trd lng="ro">drosophila melanogaster</trd>
  <trd lng="uk">&c_d;&c_r;&c_o;&c_z;&c_o;&c_f;&c_ib;&c_l;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x679C;&#x8747; <pr>gu&#x1D2;y&iacute;ng</pr></trd>,
    <trd>&#x679C;&#x8805; <pr>gu&#x1D2;y&iacute;ng</pr></trd>
  </trdgrp>
</drv>
</art>
