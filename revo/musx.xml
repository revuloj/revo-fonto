<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mu&scirc;</rad>/o
</kap>
<drv mrk="musx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="musx.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="musxed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Musca</trd>),
      malgranda duflugila insekto, kies diversaj specioj estas tre komunaj
      kaj vivas en lokoj lo&gcirc;ataj de homoj kaj bestoj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        havi <tld/>on en la cerbo<fnt>Z</fnt>
        <klr>(esti iomete freneza a&ubreve; almena&ubreve; tre stranga, havi
        strangajn ideojn)</klr>;
      </ekz>
      <ekz>
        <tld/>o zumas en lia kapo <klr>(samsenca esprimo.
        <ref tip="sin" cel="gril.0o">grilo</ref>)</klr>;
      </ekz>
      <ekz>
        kalkuli <tld/>ojn <klr>(vane peni pri
        bagateloj)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        bati du <tld/>ojn per unu bato<fnt>Z</fnt> <klr>(f.
        <ref tip="vid" cel="sxton.0o">&scirc;tono</ref>)</klr>.
      </ekz>
    </dif>
    <trd lng="de">Stubenfliege</trd>
    <trd lng="en">housefly</trd>
    <trd lng="nl">paardenvlieg</trd>
  </snc>
  <snc mrk="musx.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Similforma, ne pikanta, insekto el la ordo
      <ref tip="malprt" cel="dipter.0oj">dipteroj</ref>:
      <ekz>
        vinagro<tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kul.0o">kulo</ref>,
      <ref cel="moskit.0o">moskito</ref>
    </refgrp>
    <bld lok="../bld/musx.jpg">
      <ind>or<tld/>o</ind>
      <trd lng="hu">f&eacute;mz&ouml;ld <ind>d&ouml;gl&eacute;gy</ind></trd>
      <trd lng="la">Lucilia caesar</trd>
    </bld>
    <trd lng="de">Fliege</trd>
    <trd lng="nl">vlieg</trd>
  </snc>
  <trd lng="pl">mucha</trd>
  <trd lng="hu">l&eacute;gy</trd>
  <trd lng="ru">&c_m;&c_u;&c_h;&c_a;</trd>
  <trd lng="tr">sinek</trd>
</drv>
</art>
