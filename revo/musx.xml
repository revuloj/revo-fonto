<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mu&scirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="musx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="musx.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super"
      cel="musxed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Musca</trd>),
      malgranda duflugila insekto, kies diversaj specioj estas tre
      komunaj
      kaj vivas en lokoj lo&gcirc;ataj de homoj kaj bestoj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        havi <tld/>on en la cerbo<fnt>Z</fnt>
        <klr>(esti iomete freneza a&ubreve; almena&ubreve; tre stranga, havi
        strangajn ideojn)</klr>;
      </ekz>
      <ekz>
        <tld/>o zumas en lia kapo <klr>(samsenca esprimo.
        <ref tip="sin" cel="gril.0o">grilo</ref>)</klr>;
      </ekz>
      <ekz>
        kalkuli <tld/>ojn <klr>(vane peni pri
        bagateloj)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        bati du <tld/>ojn per unu bato<fnt>Z</fnt> <klr>(f.
        <ref tip="vid" cel="sxton.0o">&scirc;tono</ref>)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Stubenfliege</trd>,
      <trd>Mucke<klr> [sudgermana]</klr></trd>
    </trdgrp>
    <trd lng="en">housefly</trd>
    <trd lng="nl">paardenvlieg</trd>
  </snc>
  <snc mrk="musx.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Similforma, ne pikanta, insekto el la ordo
      <ref tip="malprt" cel="dipter.0oj">dipteroj</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="kul.0o">kulo</ref>,
      <ref cel="moskit.0o">moskito</ref>
    </refgrp>
    <bld lok="../bld/musx.jpg">
      <ind>or<tld/>o</ind>
      <trd lng="hu">f&eacute;mz&ouml;ld <ind>d&ouml;gl&eacute;gy</ind></trd>
      <trd lng="la">Lucilia caesar</trd>
    </bld>
    <trd lng="de">Fliege</trd>
    <trd lng="nl">vlieg</trd>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_h;&c_a;</trd>
  <trd lng="bg">&c_m;&c_u;&c_h;&c_a;</trd>
  <trd lng="de">Fliege</trd>
  <trd lng="fr">mouche</trd>
  <trd lng="hu">l&eacute;gy</trd>
  <trd lng="pl">mucha</trd>
  <trd lng="pt">mosca<klr tip="amb">(zool.)</klr></trd>
  <trd lng="es">mosca</trd>
  <trd lng="ru">&c_m;&c_u;&c_h;&c_a;</trd>
  <trd lng="tr">sinek</trd>
</drv>

<drv mrk="musx.banan0o">
  <kap>banan<tld/>o, <var><kap>vinagro<tld/>o</kap></var></kap>
  <snc mrk="musx.banan0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Kelkmimiletra mu&scirc;eto &scirc;atanta fruktojn;
      multespecia genro el familio drozofiledoj
      (<trd lng="la">Drosophila</trd>):
      <ekz>
        la tiel nomata vinagro<tld/>o allogi&gcirc;as al fermentantaj
        vinberoj
        <fnt>
          <aut>D. Fielding</aut>,
          <vrk>Drozofilo</vrk>,
          <lok><url ref="http://verdadrako.50g.com/bulteno10.html">
          La Verda Drako, 2005-05</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        grandan signifon por biologio akiris la banan<tld/>o 
        <klr>...</klr>, &scirc;atata esplorobjekto de genetiko pro siaj bone 
        observeblaj kromosomoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="http://eo.wikipedia.org/wiki/Mu%C5%9Do">mu&scirc;o</url>, 
          2010-02-10</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="drozofil.0o.ZOO">drozofilo</ref>
    </refgrp>
  </snc>
  <trd lng="fr">drosophile</trd>
  <trdgrp lng="pl">
    <trd>muszka owocowa</trd>,
    <trd>muszka owoc&oacute;wka<klr> (pot.)</klr></trd>,
    <trd>drozofila</trd>
  </trdgrp>
</drv>
</art>
