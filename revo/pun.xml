<drv mrk="pun.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="pun.0inda.KOMUNE">
    <dif>
      Inda ricevi <tld/>on:
    </dif>
    <subsnc mrk="pun.0inda.malbonaginto" ref="pun.0i.malbonaginton">
      <ekz>
        vivis <tld/>inde, mortis ridinde
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        dungantoj evitantaj impostpagon sentas sin ne<tld/>indaj
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2006/008995.php">Salajro en koverto</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
    </subsnc>
    <subsnc mrk="pun.0inda.malbonago" ref="pun.0i.malbonagon">
      <ekz>
        viaj <tld/>indaj malbonegaj faroj estos pardonitaj al vi
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz><ekz>
        mi konfesas ke mi mensogis kaj vivis duan vivon,
        ka&scirc;itan disde la homoj, sed tio ne estas ja tiom kruele
        <tld/>inda
        <fnt><bib>MkM</bib> <lok>&ccirc;ap. 19a</lok></fnt>;
      </ekz><ekz>
        <tld/>inda mensogo
        <fnt><bib>Revadoj</bib> <lok>Kvara promenado</lok></fnt>.
      </ekz>
    </subsnc>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_ja;&c_k;&c_ib; &c_z;&c_a;&c_s;&c_l;&c_u;&c_g;&c_o;&c_w;&c_v;&c_a;&c_je; <ind>&c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_ja;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>strafbar</trd>,
    <trd>str&auml;flich</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>punishable</trd>,
    <trd>worthy of punishment</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>punible</trd>,
    <trd>castigable</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;ritant une <ind>punition</ind></trd>
  <trdgrp lng="hu">
    <trd>b&uuml;ntetend&odblac;</trd>,
    <trd>b&uuml;ntet&eacute;sre &eacute;rdemes</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>layak</ind> dihukum</trd>,
    <trd><ind>patut</ind> dihukum</trd>
  </trdgrp>
  <trd lng="ja">&#x51E6;&#x7F70;&#12377;&#12409;&#x304D;  <pr>&#12375;&#12423;&#12400;&#12388;&#12377;&#12409;&#x304D;</pr></trd>
  <trd lng="nl">strafbaar</trd>
  <trd lng="pl">karygodny</trd>
  <trdgrp lng="pt">
    <trd>pun&iacute;vel</trd>,
    <trd>castig&aacute;vel</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j; <ind>&c_n;&c_a;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_ja;</ind></trd>,
    <trd>&c_n;&c_a;&c_k;&c_a;&c_z;&c_u;&c_je;&c_m;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="uk">&c_ja;&c_k;&c_i;&c_j; &c_z;&c_a;&c_s;&c_l;&c_u;&c_g;&c_o;&c_v;&c_u;&c_jeu; &c_k;&c_a;&c_r;&c_i;/&c_p;&c_o;&c_k;&c_a;&c_r;&c_a;&c_n;&c_n;&c_ja;</trd>
  <trdgrp lng="zh">
