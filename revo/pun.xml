<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pun</rad>/i
</kap>
<drv mrk="pun.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi al iu suferon kompense de ties malbonfaro a&ubreve; kulpo:
      <ekz>
        <tld/>i iun, &ccirc;ar li malobeis, pro ke li malobeis;
      </ekz>
      <ekz>
        <tld/>i ies malobeon;
      </ekz>
      <ekz>
        <tld/>i iun je malliberejo, knabon per vergo pro malobeo;
      </ekz>
      <ekz>
        kiun Dio amas, tiun li <tld/>as<fnt>Z</fnt>;
      </ekz>
      <ekz>per mono, ne per mano, <tld/>u vila&gcirc;anon
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>kune kaptite, kune <tld/>ite
<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <tld/>inda maliculo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pag.0i">pagi</ref>,
      <ref cel="ricev.0i">ricevi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">strafen</trd>
  <trd lng="en">punish</trd>
  <trd lng="fr">punir</trd>
  <trd lng="hu">b&uuml;ntet</trd>
  <trd lng="nl">straffen</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_a;&c_r;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">straffa</trd>
</drv>

<drv mrk="pun.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sufero per kiu oni <tld/>as:
      <ekz>
        <ind>mon<tld/>o</ind>;
        <trd lng="hu">p&eacute;nzb&uuml;ntet&eacute;s</trd>
        <trd lng="ru">&c_sh;&c_t;&c_r;&c_a;&c_f;</trd>
      </ekz>
      <ekz>
        <ind><tld/>laboro</ind>;
        <trd lng="hu">k&eacute;nyszermunka</trd>
        <trd lng="ru">&c_k;&c_a;&c_t;&c_o;&c_r;&c_g;&c_a;</trd>
      </ekz>
      <ekz>
        esti devigita fari ion je <tld/>o de morto<fnt>B</fnt>;
      </ekz>
      <ekz>
        porti la <tld/>on por io<fnt>Z</fnt>;
      </ekz>
      <ekz>
        malvirtulo ne restos <ind>sen<tld/>a</ind><fnt>Z</fnt>.
        <trd lng="ru">&c_b;&c_je;&c_z;&c_n;&c_a;&c_k;&c_a;&c_z;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Strafe</trd>
  <trd lng="hu">b&uuml;ntet&eacute;s</trd>
  <trd lng="nl">straf</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_a;</trd>,
    <trd>&c_v;&c_z;&c_y;&c_s;&c_k;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">straff</trd>
</drv>
</art>
