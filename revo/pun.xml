    <trd>potresta&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">straffa</trd>
  <trd lng="uk">&c_k;&c_a;&c_r;&c_a;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x60E9; <pr>ch&#xE9;ng</pr></trd>,
    <trd>&#x61F2; <pr>ch&#xE9;ng</pr></trd>,
    <trd>&#x60E9;&#x7F5A; <pr>ch&#xE9;ngf&#xE1;</pr></trd>,
    <trd>&#x61F2;&#x7F70; <pr>ch&#xE9;ngf&#xE1;</pr></trd>,
    <trd>&#22788; <pr>ch&#x1D4;</pr></trd>,
    <trd>&#34389; <pr>ch&#x1D4;</pr></trd>,
    <trd>&#x8A85; <pr>zh&#x16B;</pr></trd>,
    <trd>&#x8BDB; <pr>zh&#x16B;</pr></trd>,
    <trd>&#x60E9;&#x6CBB; <pr>ch&#xE9;ngzh&#xEC;</pr></trd>,
    <trd>&#x61F2;&#x6CBB; <pr>ch&#xE9;ngzh&#xEC;</pr></trd>,
    <trd>&#x7F5A; <pr>f&#xE1;</pr></trd>,
    <trd>&#x7F70; <pr>f&#xE1;</pr></trd>,
    <trd>&#21046;&#x88C1; <pr>zh&#xEC;c&#xE1;i</pr></trd>,
