<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: okcipi.xml,v 1.22 2018/10/29 20:10:21 revo Exp $">
<kap>
  <rad>okcipit</rad>/o
</kap>
<drv mrk="okcipi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Malanta&ubreve;a malsupra parto de la <ref tip="malprt" cel="kap.0o.ANA">kapo</ref>.
    </dif>
    <ref tip="sin" cel="krani.post0o">postkranio</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_y;&c_l;&c_ib;&c_c;&c_a;</trd>
  <trd lng="ca">occ&iacute;pit</trd>
  <trd lng="de">Hinterhaupt</trd>
  <trd lng="el">&jota;&ny;&jota_ton;&omikron;</trd>
  <trd lng="en">occiput</trd>
  <trd lng="fa">&f_p;&a_s;&zwnj;&a_s;&a_r;</trd>
  <trd lng="fr">occiput</trd>
  <trdgrp lng="hu">
    <trd>nyakszirt</trd>,
    <trd>tark&oacute;</trd>
  </trdgrp>
  <trd lng="it">occipite</trd>
  <trd lng="nl">achterhoofd</trd>
  <trd lng="pl">potylica</trd>
  <trd lng="pt">occip&iacute;cio</trd>
  <trd lng="ru">&c_z;&c_a;&c_t;&c_y;&c_l;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>tylo</trd>,
    <trd>z&aacute;hlavie</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>t&yacute;l</trd>,
    <trd>z&aacute;tyl&iacute;</trd>
  </trdgrp>
</drv>

<drv mrk="okcipi.0osto">
  <kap><tld/>osto,
    <var><kap><tld/>a osto</kap></var>
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="ost.0o">Osto</ref> de la homa skeleto, kiu
      formas la malanta&ubreve;an malsupran parton de la
      <ref tip="malprt" cel="krani.0o">kranio</ref>:
      <ekz>
        nur la unua spina nervo, kiu eliras la vertebran kanalon inter
        la <tld/>a osto de la kranio kaj la plej supra kola vertebro, 
        nomata &leftquot;atlaso&rightquot;, ne havas kaj motorajn kaj sentajn nervoradikojn,
        sed nur motorajn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=312842">Spina nervo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="okcipital.0o">okcipitalo</ref>
    <refgrp tip="vid">
      <ref cel="frunt.0osto">fruntosto</ref>,
      <ref cel="pariet.0osto">parietosto</ref>,
      <ref cel="temporal.0o">temporalo</ref>
    </refgrp>
  </snc>
  <trd lng="en">occipital bone</trd>
  <trd lng="be">&c_p;&c_a;&c_t;&c_y;&c_l;&c_ib;&c_ch;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="ca">occipital</trd>
  <trd lng="cs">t&yacute;ln&iacute; kost</trd>
  <trd lng="de">Hinterhauptbein</trd>
  <trd lng="el">&jota;&ny;&jota;&alfa;&kappa;&omikron_ton;&sigma_fina;<klr> (&omikron;&sigma;&tau;&omikron_ton;&ypsilon;&ny;)</klr></trd>
  <trd lng="fa">&a_A;&a_s;&a_t;&a_kh;&a_w;&a_A;&a_n; &f_p;&a_s;&zwnj;&a_s;&a_r;&f_y;</trd>
  <trd lng="fr">occipital</trd>
  <trd lng="hu">nyakszirtcsont</trd>
  <trd lng="it">osso <ind>occipitale</ind></trd>
  <trd lng="nl">achterhoofdsbeen</trd>
  <trd lng="pl">ko&sacute;&cacute; potyliczna</trd>
  <trd lng="pt"><klr>(osso) </klr>occipital</trd>
  <trd lng="ru">&c_z;&c_a;&c_t;&c_y;&c_l;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">z&aacute;hlavn&aacute; kos&tcaron;</trd>
</drv>
</art>
