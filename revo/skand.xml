<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skand</rad>/i
  <fnt>B</fnt>
</kap>
<drv mrk="skand.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Dividi verson la&ubreve; silaboj a&ubreve; la&ubreve; verseroj.
    </dif>
  </snc>
  <snc>
    <dif>
      Prononci apartigante la silabojn.
    </dif>
  </snc>
  <trd lng="cs">skandovat</trd>
  <trd lng="de">skandieren</trd>
  <trd lng="en">scan</trd>
  <trd lng="fr">scander</trd>
  <trd lng="nl">scanderen</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_n;&c_d;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
