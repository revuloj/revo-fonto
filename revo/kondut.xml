<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kondut</rad>/i
</kap>
<drv mrk="kondut.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Agi iamaniere en siaj rilatoj kun aliaj homoj:
      <ekz>
        <tld/>i bone, malbone, neripro&ccirc;inde, glore;
      </ekz>
      <ekz>
        <tld/>i kolere, afable, malame kontra&ubreve; iu;
      </ekz>
      <ekz>
        li <tld/>is &ccirc;iam &gcirc;entile al mi.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="trakt.0i">trakti</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_s;&c_ja;&c_b;&c_je;</trd>
  <trd lng="cs">chovat se</trd>
  <trdgrp lng="de">
    <trd><klr>sich </klr>benehmen</trd>,
    <trd><klr>sich </klr>f&uuml;hren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conduct <klr>oneself</klr></trd>,
    <trd>behave</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>conduire</ind><klr tip="ind"> (se comporter)</klr></trd>,
    <trd>se <ind>comporter</ind></trd>,
    <trd>agir<klr tip="ind"> (se comporter)</klr></trd>
  </trdgrp>
  <trd lng="nl">zich <ind>gedragen</ind></trd>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_t;&c_i; &c_s;&c_je;&c_b;&c_ja;</trd>
</drv>

<drv mrk="kondut.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Maniero, la&ubreve; kiu oni <tld/>as:
      <ekz>
        havi bonan <tld/>on;
      </ekz>
      <ekz>
        &scirc;an&gcirc;i sian <tld/>on;
      </ekz>
      <ekz>
        malbon<tld/>a<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ten.sin0o">sinteno</ref>,
      <ref cel="ten.0igxo">teni&gcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>Benehmen</trd>,
    <trd>F&uuml;hrung</trd>,
    <trd>Manieren</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>behavior</trd>,
    <trd>conduct</trd>,
    <trd>deportment</trd>,
    <trd>manners</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>conduite<klr tip="ind"> (comportement)</klr></trd>,
    <trd>attitude<klr tip="ind"> (comportement)</klr></trd>,
    <trd>comportement</trd>,
    <trd><ind>fa&ccedil;on</ind>s<klr tip="ind"> (comportement)</klr></trd>,
    <trd><ind>mani&egrave;re</ind>s<klr tip="ind"> (comportement)</klr></trd>,
  </trdgrp>
  <trd lng="nl">gedrag</trd>
  <trd lng="ru">&c_p;&c_o;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
</art>
