<vortaro>
<art mrk="$Id: rekviz.xml,v 1.16 2017/10/31 23:11:46 revo Exp $">
<kap>
  <rad>rekvizici</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rekviz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rekviz.0i.JUR">
    <dif>
      Oficiale ordoni disponigon de personoj a&ubreve; a&jcirc;oj
      por ia publika servo:
      <ekz>
        damne! tiuj fi-hundidoj en la komunuma oficejo <tld/>as jen 
        du tegolistojn unu tagon jen du &ccirc;arpentistojn alian tagon, kaj 
        jam disigis la laborforton de nia vila&gcirc;o  
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_k;&c_v;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_r;&c_je;&c_k;&c_v;&c_i;&c_z;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>rekv&iacute;rovat</trd>,
    <trd>zabrat</trd>,
    <trd>&uacute;&rcaron;edn&ecaron; zabavit</trd>
  </trdgrp>
  <trd lng="de">requirieren</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_S;&a_A;&a_d;&a_r;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_t;&a_w;&a_q;&f_y;&a_f; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;quisitionner</trd>
  <trdgrp lng="hu">
    <trd><klr>(hat&oacute;s&aacute;gilag)</klr> lefoglal</trd>,
    <trd><klr>(hat&oacute;s&aacute;gilag)</klr> ig&eacute;nybe vesz</trd>,
    <trd>rekvir&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>rekwireren</trd>,
    <trd>opvorderen</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_v;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>rekvirova&tcaron;</trd>,
    <trd>zhaba&tcaron;</trd>,
    <trd>&uacute;radne zabera&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="rekviz.0o">
  <kap><tld/>o</kap>
  <snc mrk="rekviz.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Decido, ago <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_k;&c_v;&c_ib;&c_z;&c_ib;&c_c;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_r;&c_je;&c_k;&c_v;&c_i;&c_z;&c_i;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>rekvizice</trd>,
    <trd>z&aacute;bor</trd>
  </trdgrp>
  <trd lng="de">Requisition</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_S;&a_A;&a_d;&a_r;&a_h;</trd>,
    <trd>&a_t;&a_w;&a_q;&f_y;&a_f;</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;quisition</trd>
  <trdgrp lng="hu">
    <trd><klr>(hat&oacute;s&aacute;gi)</klr> lefoglal&aacute;s</trd>,
    <trd><klr>(hat&oacute;s&aacute;gi)</klr> ig&eacute;nybev&eacute;tel</trd>,
    <trd>rekvir&aacute;l&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>rekwisitie</trd>,
    <trd>opvordering</trd>
  </trdgrp>
  <trd lng="pt">requisi&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_v;&c_i;&c_z;&c_i;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">rekviz&iacute;cia</trd>
</drv>
</art>
