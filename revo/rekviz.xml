<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rekvizici</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="rekviz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ordono de iu publika a&ubreve;toritato, ke oni disponigu al &gcirc;i personojn a&ubreve;
      a&jcirc;ojn por ia publika servo.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_k;&c_v;&c_ib;&c_z;&c_ib;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Requisition</trd>
  <trdgrp lng="nl">
    <trd>rekwisitie</trd>,
    <trd>opvordering</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_v;&c_i;&c_z;&c_i;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="rekviz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Preni <tld/>e.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_k;&c_v;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">requirieren</trd>
  <trdgrp lng="nl">
    <trd>rekwireren</trd>,
    <trd>opvorderen</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_k;&c_v;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
