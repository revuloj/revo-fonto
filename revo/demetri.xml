<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: demetri.xml,v 1.13 2023/11/04 13:16:27 revo Exp $">
    <kap>
      <rad>Demetri</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>  
    <drv mrk="demetri.0o">
      <kap>
        <tld/>o
      </kap>
      <snc mrk="demetri.0o.NOM">
        <uzo tip="fak">NOM</uzo>
        <dif>
           Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
           <ekz>
              unu viro, nomata <tld/>o, ar&gcirc;enta&jcirc;isto, kiu faradis
              ar&gcirc;entajn templetojn de Artemis, liveris al la metiistoj
              multe da okupo
              <fnt><bib>NT</bib>, <lok>&Ago; 24:19.</lok></fnt>;
           </ekz>
           <ekz>
              <uzo tip="fak">PERS</uzo>
              <ind><tld/>o la Tesalonika</ind> estis
              populara sanktulo en la orienta kristanismo;
              <trd lng="ru">&c_D;&c_i;&c_m;&c_i;&c_t;&c_r;&c_i;&c_j; &c_S;&c_o;&c_l;&c_u;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
           </ekz>
           <ekz>
              <uzo tip="fak">PERS</uzo>
              <ind><tld/>o la Dona</ind>;
              <trd lng="ru">&c_D;&c_m;&c_i;&c_t;&c_r;&c_i;&c_j; &c_D;&c_o;&c_n;&c_s;&c_k;&c_o;&c_j;</trd>
           </ekz>
           <ekz>
              <uzo tip="fak">PERS</uzo>
              la <ind>Falsa <tld/>o</ind> okupis la moskvan tronon en
              1605&ndash;1606.
              <trd lng="hu">&Aacute;l-Dmitrij</trd>
              <trd lng="ru">&c_L;&c_zh;&c_je;&c_d;&c_m;&c_i;&c_t;&c_r;&c_i;&c_j; I</trd>
           </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_Z;&c_mol;&c_m;&c_ib;&c_c;&c_je;&c_r;</trd>,
        <trd>&c_D;&c_z;&c_mol;&c_m;&c_ib;&c_t;&c_r;&c_y;</trd>
      </trdgrp>
      <trd lng="ca">Demetri</trd>
      <trdgrp lng="de">
         <trd>Demeter<klr> [griech. Gottheit]</klr></trd>,
         <trd>Demetrius<klr> [antiker Name]</klr></trd>,
         <trd>Dimitri<klr> [russ. Name]</klr></trd>
      </trdgrp>
      <trd lng="es">Demetrio</trd>
      <trd lng="fr">Dimitri</trd>
      <trdgrp lng="hu">
         <trd>Demeter</trd>,
         <trd>D&ouml;m&ouml;t&ouml;r</trd>,
      </trdgrp>
      <trd lng="it">Demetrio</trd>
      <trd lng="grc">&Delta;&eta;&my;&eta_ton;&tau;&rho;&jota;&omikron;&sigma_fina;</trd>
      <trd lng="pt">Dem&eacute;trio</trd>
      <trd lng="ru">&c_D;&c_m;&c_i;&c_t;&c_r;&c_i;&c_j;</trd>
      <trd lng="uk">&#x414;&#x43C;&#x438;&#x442;&#x440;&#x43E;</trd>
  <trdgrp lng="zh">
    <trd>&#x5FB7;&#x7C73;&#x7279;&#x5C14; <pr>d&#xE9;m&#x1D0;t&#xE8;&#x11B;r</pr></trd>,
    <trd>&#x5F97;&#x58A8;&#x5FD2;&#x8033; <pr>d&#xE9;m&#xF2;t&#xE8;&#x11B;r</pr></trd>,
    <trd>&#x5F97;&#x58A8;&#x5FD2;&#x5C14; <pr>d&#xE9;m&#xF2;t&#xE8;&#x11B;r</pr></trd>
  </trdgrp>
</drv>

  </art>

