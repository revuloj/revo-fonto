<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: klemat.xml,v 1.16 2023/09/01 15:07:13 revo Exp $">
<kap>
  <rad>klemat</rad>/o <fnt>Z</fnt>
  <var><kap><rad var="id">klematid</rad>/o <fnt><bib>PV</bib></fnt></kap></var>,
  <var><kap><rad var="it">klematit</rad>/o <fnt><bib>PIV2</bib></fnt></kap></var>
</kap>
<drv mrk="klemat.0o">
  <kap><tld/>o,
    <var><kap><tld var="id"/>o</kap></var>,
    <var><kap><tld var="it"/>o</kap></var>
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="ranunk1.0oj">ranunkolacoj</ref>
      (<trd lng="la">Clematis</trd>), volve grimpanta vegeta&jcirc;o:
      <ekz>
        jam kreskis tie &leftquot;grimpantaj&rightquot; plantoj, ekzemple tri <tld/>oj, rozo, pasifloro,
        planto de la solana familio kaj flava lupolo
       <fnt>
          <aut>M&aacute;ire NicAoidh</aut>
          <vrk><url ref="&Monato;/2000/006878.php">&Gcirc;ardenumi</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 20</lok>
          </fnt>;
      </ekz>
      <ekz>
        sova&gcirc;a <tld/>o
        <fnt>
          <vrk><url ref="https://www.scribd.com/document/389857956/bulteno-139-finredaktite-la-30an-de-septembro-2018">La
          infana&gcirc;o, bulteno 139</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
         <tld var="id"/>o a&ubreve; <tld var="it"/>o <klr>[&#x2026;]</klr> entenas 200 speciojn da sta&ubreve;doj,
         herbecaj
        plantoj kaj arbedoj, grimpemaj plantoj, &ccirc;iamverdaj a&ubreve; falfoliaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30363">Klematido</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/a8/Clematis_vitalba_flowers_230702.jpg/360px-Clematis_vitalba_flowers_230702.jpg" prm="CC-BY-SA-3.0">floraro de <tld/>o (Clematis vitalba)
        <fnt>
          <aut>foto de BerndH</aut>
          <lok><url ref="&WCW;/File:Clematis_vitalba_flowers_230702.jpg">&WCW;/File:Clematis_vitalba_flowers_230702.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_v;&c_o;&c_j;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Clematis</trd>,
    <trd>Waldrebe</trd>
  </trdgrp>
  <trd lng="fr">cl&eacute;matite</trd>
  <trd lng="en">clematis</trd>
  <trdgrp lng="hu">
    <trd>iszalag</trd>,
    <trd>b&eacute;rcse</trd>,
    <trd>klem&aacute;tisz</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AF;&#x30EC;&#x30DE;&#x30C1;&#x30B9;</trd>,
    <trd>&#x30BB;&#x30F3;&#x30CB;&#x30F3;&#x30BD;&#x30A6;</trd>
  </trdgrp>
  <trd lng="nl">clematis</trd>
  <trd lng="pl">powojnik</trd>     
  <trd lng="ro">clematis</trd>
  <trd lng="uk">&#x43B;&#x43E;&#x43C;&c_i;&#x43D;&c_ib;&c_s;</trd>
</drv>
</art>
