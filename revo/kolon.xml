<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kolon</rad>/o
</kap>
<drv mrk="kolon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kolon.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Vertikala peco el ligno, &scirc;tono, fero a&ubreve; alia substanco, 
      &gcirc;enerale fini&gcirc;anta
      per kapitelo, kaj servanta kiel subtenilo a&ubreve; kiel ornamo de
      konstrua&jcirc;o:
      <ekz>
        cilindra, kvarangula, dorika, korinta, ionia <tld/>o;
      </ekz>
      <ekz>
        lanterna <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
      <uzo tip="stl">FIG</uzo>
        la <tld/>oj de la &scirc;tato.
      </ekz>
    </dif>
    <trd lng="de">S&auml;ule</trd>
    <trdgrp lng="en">
      <trd>column</trd>,
      <trd>pillar</trd>
    </trdgrp>
    <trd lng="hu">oszlop</trd>
    <trd lng="nl">zuil</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_l;&c_o;&c_n;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_t;&c_o;&c_l;&c_b;</trd>,
      <trd>&c_s;&c_t;&c_o;&c_l;&c_p;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kolon.0o.monumento">
    <dif>
      Monumento, konsistanta el izolita <tld/>o el marmoro, bronzo ktp:
      <ekz>
        la <ind>Trajana <tld/>o</ind>;
        <trd lng="fr">Colonne Trajane</trd>
      </ekz>
      <ekz>
        <uzo tip="fak">GEOG</uzo>
        la <ind>Herkulaj <tld/>oj</ind>
        <klr>(&ccirc;e la antikvuloj, amba&ubreve; promontoroj de markolo de
        Gibraltar)</klr>;
        <trd lng="fr"><ind>Colonne</ind>s d'Hercule</trd>
      </ekz>
      <ekz>
        <ind><tld/>o de malhonoro</ind>
        <klr>(al kiu oni ligis la kondamnitojn)</klr>.
        <trd lng="fr">pilori</trd>
      </ekz>
    </dif>
    <trd lng="de">S&auml;ule</trd>
    <trd lng="en">pillar</trd>
    <trd lng="hu">eml&eacute;koszlop</trd>
    <trd lng="nl">zuil</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_l;&c_o;&c_n;&c_n;&c_a;</trd>,
      <trd>&c_s;&c_t;&c_o;&c_l;&c_b;</trd>,
      <trd>&c_s;&c_t;&c_o;&c_l;&c_p;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kolon.0o.formo">
    <dif>
      &Ccirc;iu ajn a&jcirc;o &ccirc;u solida, a&ubreve; flueca, 
      &ccirc;u gasa, kun formo de <tld/>o:
      <ekz>
        <tld/>o de akvo, de hidrargo, de fumo supreniranta.
      </ekz>
    </dif>
    <trd lng="de">S&auml;ule</trd>
    <trd lng="en">column</trd>
    <trd lng="hu">oszlop</trd>
    <trd lng="nl">zuil</trd>
    <trd lng="ru">&c_s;&c_t;&c_o;&c_l;&c_b;</trd>
  </snc>
  <snc mrk="kolon.0o.staplo">
    <ref tip="dif" cel="stapl.0o">staplo</ref>
    <ekz>
      <tld/>o da libroj, da froma&gcirc;oj.
    </ekz>
    <trd lng="de">Stapel</trd>
    <trd lng="en">stack</trd>
    <trdgrp lng="hu">
      <trd>rak&aacute;s</trd>,
      <trd>oszlop</trd></trdgrp>
    <trd lng="nl">stapel</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_o;&c_p;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_o;&c_l;&c_o;&c_n;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_t;&c_o;&c_l;&c_b;&c_i;&c_k;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kolon.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <uzo tip="stl">RAR</uzo>
    <ref tip="dif" cel="kolumn.0o.TIP">kolumno</ref>
    <trd lng="de">Spalte</trd>
    <trd lng="en">column</trd>
    <trd lng="hu">has&aacute;b</trd>
    <trd lng="nl">kolom</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_o;&c_l;&c_o;&c_n;&c_k;&c_a; <klr>(&c_v; &c_g;&c_a;&c_z;&c_je;&c_t;&c_je;, &c_k;&c_n;&c_i;&c_g;&c_je;)</klr></trd>,
      <trd>&c_s;&c_t;&c_o;&c_l;&c_b;&c_je;&c_c;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kolon.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Trupoj la&ubreve;longe ordigitaj dum mar&scirc;ado.
    </dif>
    <ref tip="sin" cel="kolumn.0o.2">kolumno 2</ref>
    <trdgrp lng="de">
      <trd>Kolonne</trd>,
      <trd>Marschs&auml;ule</trd>
    </trdgrp>
    <trd lng="de">Kolonne</trd>
    <trdgrp lng="en">
      <trd>column</trd>,
      <trd>file</trd>
    </trdgrp>
    <trd lng="hu">menetoszlop</trd>
    <trd lng="nl">colonne</trd>
    <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_n;&c_n;&c_a; <klr>(&c_v;&c_o;&c_je;&c_n;&c_n;&c_y;&c_j; &c_s;&c_t;&c_r;&c_o;&c_j;)</klr></trd>
  </snc>
  <trdgrp lng="fr">
    <trd>colonne</trd>,
    <trd>file</trd>,
    <trd>pilier</trd>,
    <trd>pile<klr tip="ind"> (tas)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="kolon.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Vico da <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>S&auml;ulenreihe</trd>,
    <trd>Kolonnade</trd>
  </trdgrp>
  <trd lng="en">colonnade</trd>
  <trdgrp lng="fr">
    <trd>colonnade</trd>
  </trdgrp>
  <trd lng="hu">oszlopsor</trd>
  <trdgrp lng="nl">
    <trd>zuilenrij</trd>,
    <trd>colonnade</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_n;&c_n;&c_a;&c_d;&c_a;</trd>
</drv>

<drv mrk="kolon.sur0ajxo">
  <kap>sur<tld/>a&jcirc;o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <ref tip="dif" cel="entabl.0o">entablemento</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Hauptgesims</trd>,
    <trd>Geb&auml;lk</trd>
  </trdgrp>
  <trd lng="en">entablature</trd>
  <trd lng="fr">entablement</trd>
  <trdgrp lng="hu">
    <trd>f&odblac;p&aacute;rk&aacute;ny</trd>,
    <trd>koronap&aacute;rk&aacute;ny</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_a;&c_b;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;</trd>
</drv>
</art>
