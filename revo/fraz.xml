<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: fraz.xml,v 1.31 2020/10/14 06:36:59 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>fraz</rad>/o
</kap>
<drv mrk="fraz.0o">
  <kap><tld/>o</kap>
  <snc mrk="fraz.0o.GRA">
    <tezrad fak="GRA"/>
    <uzo tip="fak">GRA</uzo>
    <dif>
      Kuna&jcirc;o da <ref tip="prt" cel="vort.0o.LIN">vortoj</ref>
      prezentanta kompletan sencon:
      <ekz>
        tial ofte <klr>[...]</klr> pli bona estas <tld/>o ne tute logika kaj ne
        perfekte preciza, sed mallonga kaj viva, ol <tld/>o perfekte
        preciza, sed enuige peza kaj tro kabineta<fnt><bib>LR</bib>,
        <lok>Respondo 25, La Revuo, 1907, Oktobro</lok></fnt>;
      </ekz>
      <ekz>
        simpla <tld/>o
        <klr>(konsistanta el ununura <ref tip="prt"
        cel="propoz.0o.GRA">propozicio</ref>)</klr>;
      </ekz>
      <ekz>
        kompleksa <tld/>o <klr>(plurpropozicia)</klr>;
      </ekz>
      <ekz>
        la lasta <tld/>o de <ref tip="malprt"
        cel="aline.0o.tekstopeco">alineo</ref>;
      </ekz>
      <ekz>
        tiu <tld/>o estas tro longa kaj ne klara;
      </ekz>
      <ekz>
        tiu vorto estas subjekto de la <tld/>o;
      </ekz>
      <ekz>
        indikativa, ordona, nega <tld/>o.
      </ekz>
    </dif>
    <trd lng="cs">v&ecaron;ta</trd>
    <trd lng="es">oraci&oacute;n</trd>
    <trd lng="hu">mondat</trd>
    <trd lng="de">Satz</trd>
    <trd lng="nl">zin</trd>
    <trd lng="pt">frase</trd>
    <trd 
lng="ru">&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
    <trd lng="sk">veta</trd>
  </snc>
  <snc mrk="fraz.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Afektaj, belsonaj, senkernaj, malmultsignifaj paroloj:
      <ekz>
        grandaj <tld/>oj, malgrandaj agoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="afekt.0o">afekto</ref>,
      <ref cel="bombas.0o">bombasto</ref>,
      <ref cel="emfaz.0o">emfazo</ref>,
      <ref cel="patos.0o">patoso</ref>
    </refgrp>
    <trd lng="de">Reden</trd>
    <trd lng="hu">fr&aacute;zis</trd>
    <trd lng="nl">frase</trd>
    <trd lng="pt">frase</trd>
  </snc>
  <snc mrk="fraz.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Nelonga kaj pli-malpli kompleta parto de muzika
      <ref tip="malprt" cel="tem.0o">temo</ref>,
      <ref tip="malprt" cel="melodi.0o">melodio</ref>.
    </dif>
    <trd lng="en">phrase</trd>
    <trdgrp lng="hu">
      <trd>(zenei) <ind>mondat</ind></trd>,
      <trd>fr&aacute;zis</trd>
    </trdgrp>
    <trd lng="pt">frase</trd>
    <trd lng="ru">&c_f;&c_r;&c_a;&c_z;&c_a;<klr tip="amb"> 
(&c_m;&c_u;&c_z;.)</klr></trd>
  </snc>
  <snc mrk="fraz.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      En <ref tip="malprt" cel="lingv.program0o">programlingvo</ref>,
      meznivela unuo de programteksto, prezentanta relative
      a&ubreve;tonoman kaj kompletan informon:
      <ref tip="sub" cel="deklar.0o.KOMP">deklaron</ref> de programa
      objekto, <ref tip="sub" cel="ordon.0o.KOMP">ordonon</ref>
      a&ubreve;
      komandon, <ref tip="sub" cel="esprim.0o.KOMP">esprimon</ref>:
      <ekz>
        deklara, plenumebla, ordona, valorhava <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>statement</trd>, <trd>expression</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>mondat</trd>,
      <trd>utas&iacute;t&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="zh">
      <trd>&#35821;&#21477; [<ind>y&#468; j&ugrave;</ind>]</trd>
      <trd>&#34920;&#36798;&#24335; [<ind>bi&#462;o d&aacute;
      sh&igrave;</ind>]</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>instrukce</trd>,
    <trd>povel</trd>,
    <trd>p&rcaron;&iacute;kaz</trd>,
    <trd>v&yacute;raz</trd>,
    <trd>v&yacute;rok</trd>
  </trdgrp>
  <trd lng="de">Anweisung</trd>
  <trd lng="en">sentence</trd>
  <trdgrp lng="fr">
    <trd>phrase</trd>,
    <trd>discours<klr tip="ind"> (pompeux)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_r;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>in&scaron;trukcia</trd>,
    <trd>povel</trd>,
    <trd>pr&iacute;kaz</trd>,
    <trd>vyraz</trd>
  </trdgrp>
</drv>

<drv mrk="fraz.0aro">
  <kap><tld/>aro</kap>
  <snc>
    <dif>
      Kuna&jcirc;o da <tld/>oj.
    </dif>
    <trd lng="ru">&c_n;&c_a;&c_b;&c_o;&c_r; &c_f;&c_r;&c_a;&c_z;</trd>
  </snc>
  <snc>
    <dif>
      Kolekto da modelaj anta&ubreve;pretaj <tld/>oj.
    </dif>
    <trd lng="hu">sz&oacute;l&aacute;sgy&udblac;jtem&eacute;ny</trd>
    <trd 
lng="ru">&c_r;&c_a;&c_z;&c_g;&c_o;&c_v;&c_o;&c_r;&c_n;&c_i;&c_k;</trd>
  </snc>
  <trd lng="cs">konverza&ccaron;n&iacute;
  p&rcaron;&iacute;ru&ccaron;ka</trd>
  <trd lng="sk">konverza&ccaron;n&aacute; pr&iacute;ru&ccaron;ka</trd>
</drv>

<drv mrk="fraz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Paroli per afektaj, malmultsignifaj <tld/>oj.
    </dif>
  </snc>
  <trd lng="cs">vyjad&rcaron;ovat se ve fr&aacute;z&iacute;ch</trd>
  <trdgrp lng="fr">
    <trd>discourir<klr tip="ind"> (pontifier)</klr></trd>,
    <trd>pontifier</trd>
  </trdgrp>
  <trd lng="hu">sz&oacute;nokol <klr tip="amb">(pej.)</klr></trd>
  <trd 
lng="ru">&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_g;&c_o;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">vyjadrova&tcaron; sa vo fr&aacute;zach</trd>
</drv>

<drv mrk="fraz.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu kutimas <tld/>i:
      <ekz>
        <tld/>ista sensenca&jcirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>fr&aacute;zista</trd>,
    <trd>fr&aacute;zovit&yacute; &rcaron;e&ccaron;n&iacute;k</trd>,
    <trd>mluvka</trd>
  </trdgrp>
  <trd lng="de">Schw&auml;tzer</trd>
  <trd lng="fr">phraseur</trd>
  <trd lng="hu">fraz&odblac;r</trd>
  <trd lng="ru">&c_f;&c_r;&c_a;&c_z;&c_jo;&c_r;</trd>
  <trd lng="sk">fr&aacute;zista</trd>
</drv>

<drv mrk="fraz.cxirkaux0o">
  <kap>&ccirc;irka&ubreve;<tld/>o</kap>
  <snc mrk="fraz.cxirkaux0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="super" cel="figur.stil0o">Stilfiguro</ref>,
      <ref tip="super" cel="trop.0o">tropo</ref> indikanta ideon,
      personon a&ubreve; objekton ne rekte per la propra vorto, sed per
      priskribo a&ubreve; karakteriza detalo, ekzemple
      <ctl>la re&gcirc;o de Olimpo</ctl> anstata&ubreve;
      <ctl>Ze&ubreve;so</ctl>; <ctl>la Eterna Urbo</ctl>
      anstata&ubreve; <ctl>Romo</ctl>, <ctl>la a&ubreve;toro de
      &ccirc;i tiuj linioj</ctl>
      (anstata&ubreve; <ctl>mi</ctl> en a&ubreve;tora teksto):
      <ekz>
        sufi&ccirc;e da &ccirc;irka&ubreve;<tld/>oj!
        <fnt><bib>Lanti</bib>, <lok>Vivu la
        ne&ubreve;tralismo!</lok></fnt>;
      </ekz>
      <ekz>
        la supersti&ccirc;a <nom>Anfisa</nom> sen &ccirc;irka&ubreve;<tld/>o
        deklaris al sia &ccirc;agrenita mastrino, ke tio estas
        sor&ccirc;o
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="sub" cel="euxfem.0o">e&ubreve;femismo</ref>
  <trd lng="de">Umschreibung</trd>
  <trd lng="en">periphrasis</trd>
  <trd lng="fr">p&eacute;riphrase</trd>
  <trd lng="hu">k&ouml;r&uuml;l&iacute;r&aacute;s</trd>
  <trd lng="nl">omschrijving</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_o;&c_s;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_i;&c_f;&c_r;&c_a;&c_z;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="fraz.frap0o">
  <kap>frap<tld/>o</kap>
  <snc mrk="fraz.frap0o.REK">
    <dif>
      Facile memorebla frazo reklamanta principon, politikan celon, 
      institucion, venda&jcirc;on:
      <ekz>
        ni starigas konkurson por trovi trafan frap<tld/>on de TEJO
        <fnt>
          <aut>Tejo</aut>,
          <vrk><url ref="http://www.tejo.org/frapfrazo/index.php">
          Frapfrazoj</url></vrk>,
          <lok>[vidita en 2008]</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mot.0o">moto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>heslo</trd>,
    <trd>slogan</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>devise</trd>,
    <trd>slogan</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>heslo</trd>,
    <trd>slogan</trd>
  </trdgrp>
</drv>

<drv mrk="fraz.0parto">
  <kap><tld/>parto <fnt><bib>PMEG</bib></fnt></kap>
  <snc mrk="fraz.0parto.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Vorto a&ubreve; grupo da vortoj, rolantaj kiel unuo en <tld/>o.
    </dif>
  </snc>
  <rim>
    PIV2 nomas tion <ctl>sintagmo</ctl> &#8208; ne tre oportune,
    &ccirc;ar tiu
    vorto estas tro &scirc;ar&gcirc;ita je sencoj lingvosciencaj
    (internacie
    &gcirc;i havas almena&ubreve; 5 aliajn sencojn, iuj el kiuj estas
    pli 
oftaj).
    <aut>Sergio Pokrovskij</aut>
  </rim>
  <trd lng="hu">mondatr&eacute;sz</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_v;&c_o;&c_s;&c_o;&c_ch;&c_je;&c_t;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_ch;&c_l;&c_je;&c_n; 
&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="fraz.0rolo">
  <kap><tld/>rolo <fnt><bib>PMEG</bib></fnt></kap>
  <snc mrk="fraz.0rolo.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      Funkcio de <tld/>parto en la tuta <tld/>o.
    </dif>
  </snc>
  <refgrp tip="sub">
    <ref cel="subjek.0o.GRA">subjekto<sncref/></ref>,
    <ref cel="predik1.0o.GRA">predikato<sncref/></ref>,
    <ref cel="objekt.0o.GRA">objekto<sncref/></ref>,
    <ref cel="komple1.0o.GRA">komplemento<sncref/></ref>
  </refgrp>
  <rim>
    PIV1 kaj PIV2 nomas tion <ctl>frazelemento</ctl> &#8208; tio ne
    harmonias kun la sencoj de la vorto <ctl>elemento</ctl>.
    <aut>Sergio Pokrovskij</aut>
  </rim>
  <trd lng="hu">mondatfunkci&oacute;</trd>
  <trd lng="ru">&c_ch;&c_l;&c_je;&c_n; 
&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_ja;</trd>
</drv>
</art>
