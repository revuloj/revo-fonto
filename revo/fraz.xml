<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>fraz</rad>/o
    </kap>
    <drv mrk="fraz.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Kuna&jcirc;o da vortoj prezentanta kompletan sencon:
          <ekz>
            tiu <tld/>o estas tro longa kaj ne klara;
          </ekz><ekz>
            tiu vorto estas subjekto de la <tld/>o
            <klr>(f)</klr>;
          </ekz><ekz>
            muzika <tld/>o.
          </ekz>
        </dif>
      </snc>
      <snc>
        <dif>
          Afektaj, belsonaj, senkernaj, malmultsignifaj paroloj:
          <ekz>
            grandaj <tld/>oj, malgrandaj agoj.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="emfaz.0o">emfazo</ref>,
          <ref cel="bombas.0o">bombasto</ref>,
          <ref cel="afekt">afekto</ref>,
          <ref cel="patos.0o">patoso</ref>.
        </refgrp>
      </snc>
      <snc mrk="fraz.0o.KOMP">
        <uzo tip="fak">KOMP</uzo>
        <dif>
          En <ref tip="malprt" cel="lingv.0o.program">programlingvo</ref>,
          meznivela unuo de programteksto, prezentanta relative
          a&ubreve;tonoman kaj kompletan informon:
          <ref tip="sub" cel="deklar.0o.KOMP">deklaron</ref> de programa
          objekto, <ref tip="sub" cel="ordon.0o.KOMP">ordonon</ref> a&ubreve;
          komandon, <ref tip="sub" cel="esprim.0o.KOMP">esprimon</ref>:
          <ekz>deklara, plenumebla, ordona, valorhava <tld/>o</ekz>.
        </dif>
        <trdgrp lng="en"><trd>phrase</trd>, <trd>statement</trd></trdgrp>
      </snc>
      <trd lng="en">phrase</trd>
      <trdgrp lng="ru"><trd>&c_f;&c_r;&c_a;&c_z;&c_a;</trd>,
        <trd>&c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd></trdgrp>
    </drv>
    <drv mrk="fraz.0aro">
      <kap><tld/>aro</kap>
      <snc>
        <dif>
          Kuna&jcirc;o da <tld/>oj. 
        </dif>
      </snc>
      <snc>
        <dif>
          Kolekto da modelaj anta&ubreve;pretaj <tld/>oj. 
        </dif>
      </snc>
    </drv>
    <drv mrk="fraz.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
        <dif>
          Paroli per afektaj, malmultsignifaj <tld/>oj. 
        </dif>
      </snc>
    </drv>
    <drv mrk="fraz.0isto">
      <kap><tld/>isto</kap>
      <snc>
        <dif>
          Tiu, kiu kutimas <tld/>i:
          <ekz>
            <tld/>ista sensenca&jcirc;o<fnt>Z</fnt>.
          </ekz> 
        </dif>
      </snc>
      <trd lng="ru">&c_f;&c_r;&c_a;&c_z;&#x451;&c_r;</trd>
    </drv>
    <drv mrk="fraz.cxirkaux0o">
      <kap>&ccirc;irka&ubreve;<tld/>o</kap>
      <snc>
        <dif>
          Dirmaniero, la&ubreve; kiu oni esprimas ideon, personon a&ubreve; objekton
          ne rekte per la propra vorto, sed per aludo a&ubreve; karakteriza
          detalo, ekz. "la re&gcirc;o de Olimpo" anst. "Jupitero"; "la
          &ccirc;efurbo de Italujo" anst. "Romo".
        </dif>
      </snc>
      <trdgrp lng="ru"><trd>&c_i;&c_n;&c_o;&c_s;&c_k;&c_a;&c_z;&c_a;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_p;&c_je;&c_r;&c_i;&c_f;&c_r;&c_a;&c_z;&c_a;</trd></trdgrp>
    </drv>
  </art>
<!--
$Log$
-->
</vortaro>
