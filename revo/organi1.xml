<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>organism</rad>/o
    </kap>
    <drv mrk="organi1.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          Tuto de la organoj, kiuj konsistigas vivan esta&jcirc;on:
          <ekz>
            ili apartenas al la <tld/>oj de la plej malalta
            grado<fnt>Z</fnt>.
          </ekz> 
        </dif>
        <refgrp tip="sin">
          <ref cel="korp.0o">korpo</ref>.
        </refgrp>
      </snc>
      <trd lng="nl">organisme</trd>
      <trd lng="sv">organism</trd>
      <trd lng="ru">&c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_m;</trd>
    </drv>
    <drv mrk="organi1.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>o:
          <ekz>
            la <tld/>a skolo en sociologio
            <klr>(kiu prezentis la socion analogie al
              <tld/>o biologia)</klr>.
          </ekz>
        </dif>
      </snc>
    </drv>
  </art>
