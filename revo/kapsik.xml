<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kapsik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kapsik.0o">
  <kap><tld/>o</kap>
  <snc mrk="kapsik.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="legom.0o.KUI">Legomo</ref> kies fruktoj 
      havas sukan karnon, ru&gcirc;an verdan flavan a&ubreve; oran&gcirc;an eksteron, kavan 
      internon, kaj kiuj dol&ccirc;as &ccirc;e grandaj specoj, akras &ccirc;e malgrandaj, 
      genro el familio <ref tip="super"
      cel="solana.0oj">solanacoj</ref> (<trd lng="la">Capsicum</trd>).
    </dif>
    <refgrp tip="vid">
      <ref cel="paprik.0o">papriko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_c;
  <klr>(&c_s;&c_t;&c_r;&c_u;&c_ch;&c_k;&c_o;&c_v;&c_y;)</klr></trd>
  <trd lng="de">Paprika</trd>
  <trd lng="en">pepper</trd>
  <trdgrp lng="fr">
    <trd>piment</trd>,
    <trd>poivron</trd>
  </trdgrp>
  <trd lng="hu">paprika</trd>
  <trd lng="nl">paprika</trd>
  <trd lng="pl">papryka</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_j;
  &c_p;&c_je;&c_r;&c_je;&c_c;</trd>
</drv>
</art>
