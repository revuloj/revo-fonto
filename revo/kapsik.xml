  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="solana.0oj">solanacoj</ref>
      (<trd lng="la">Capsicum</trd>), kies frukto estas uzata kiel
      spica&jcirc;o.
    </dif>
    <refgrp tip="vid">
      <ref cel="paprik.0o">papriko</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Paprika</trd>
  <trd lng="en">pepper</trd>
  <trd lng="nl">paprika</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_j; &c_p;&c_je;&c_r;&c_je;&c_c;</trd>
</drv>
