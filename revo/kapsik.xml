<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>9</ofc> <rad>kapsik</rad>/o <fnt>Z</fnt></kap>

<drv mrk="kapsik.0o">
  <kap><tld/>o</kap>
  <snc mrk="kapsik.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Kreska&jcirc;o, kiu produktas <tld/>on<sncref ref="kapsik.0o.KUI"/>.
      Genro el familio <ref tip="super" cel="solana.0oj">solanacoj</ref>
      (<trd lng="la">Capsicum</trd>).
    </dif>
  </snc>
  <snc mrk="kapsik.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Verda, ru&gcirc;a a&ubreve; flava longforma frukto de
<tld/>o<sncref ref="kapsik.0o.BOT"/>,
      kun multaj semoj pendantaj interne,
      uzata kiel <ref tip="super" cel="legom.0o.KUI">legomo</ref> kaj
kiel spico:
      <ekz>akra kapsiko</ekz>,
      <ekz>neakra kapsiko</ekz>,
      <ekz>el kapsiko oni faras <ref cel="paprik.0o">paprikon</ref></ekz>.
      <refgrp tip="vid">
        <ref cel="paprik.0o">papriko</ref>
      </refgrp>
    </dif>
  </snc>

  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_c;
  <klr>(&c_s;&c_t;&c_r;&c_u;&c_ch;&c_k;&c_o;&c_v;&c_y;)</klr></trd>
  <trd lng="de">Paprika</trd>
  <trd lng="en">pepper</trd>
  <trdgrp lng="fr">
    <trd>piment</trd>,
    <trd>poivron</trd>
  </trdgrp>
  <trd lng="hu">paprika</trd>
  <trd lng="nl">paprika</trd>
  <trd lng="pl">papryka</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_j;
  &c_p;&c_je;&c_r;&c_je;&c_c;</trd>
</drv>

<drv mrk="kapsik.0eto">
  <kap><ofc>9</ofc> <tld/>eto</kap>
  <uzo tip="fak">BOT</uzo>
  <dif>
    Malgranda ordinare tre akra <tld/>o<sncref ref="kapsik.0o.KUI"/>.
  </dif>
  <trdgrp lng="en">
    <trd>chili</trd>,
    <trd>chilli</trd>
  </trdgrp>
</drv>

</art>
