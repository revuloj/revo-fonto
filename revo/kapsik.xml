<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kapsik.xml,v 1.34 2023/09/01 13:58:55 revo Exp $">
<kap>
  <ofc>9</ofc> <rad>kapsik</rad>/o <fnt>Z</fnt> 
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kapsik.0o">
  <kap><tld/>o</kap>
  <snc mrk="kapsik.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="herb.0o" lst="voko:solanoparencoj">herbo</ref>
      &ccirc;e la bazo ofte ligneca, kun longaj ovalaj glatrandaj folioj
      sur tigetoj,  kvinnombraj blankaj, violkoloraj a&ubreve; verdetaj 
      floroj kaj grandaj, diversformaj, ene kavaj beroj 
      (<ref tip="prt" cel="kapsik.0o.KUI"><tld/>o<sncref ref="kapsik.0o.KUI"/></ref>);
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="solana.0oj">solanacoj</ref>
      (<trd lng="la">Capsicum</trd>).
    </dif>
    <trdgrp lng="ca">
      <trd>pebrotera</trd>,
      <trd>pebrer</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>pimentera</trd>,
      <trd>capsicum</trd>
    </trdgrp>
  </snc>
  <snc mrk="kapsik.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Verda, ru&gcirc;a a&ubreve; flava glob- a&ubreve; longforma frukto de <tld/>o<sncref ref="kapsik.0o.BOT"/>, kun
      multaj semoj pendantaj interne,
      uzata kiel <ref tip="super" cel="legom.0o.KUI">legomo</ref> kaj
      kiel spico:
      <ekz>
        akra kapsiko;
      </ekz>
      <ekz>
        neakra kapsiko;
      </ekz>
      <ekz>
        el kapsiko oni faras <ref cel="paprik.0o">paprikon</ref>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>pebrot</trd>,
      <trd>pebrera</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>pimiento</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_c; <klr>(&c_s;&c_t;&c_r;&c_u;&c_ch;&c_k;&c_o;&c_v;&c_y;)</klr></trd>
  <trd lng="bo">&#xF66;&#xF7C;&#xF0B;&#xF63;&#xF7C;&#xF0B;&#xF66;&#xF94;&#xF7C;&#xF53;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trd lng="cs">paprika ro&ccaron;n&iacute;</trd>
  <trd lng="de">Paprika</trd>
  <trd lng="en">pepper</trd>
  <trd lng="es">pimiento</trd>
  <trd lng="fr">poivron</trd>
  <trd lng="hu">paprika</trd>
  <trd lng="ja">&#x30C8;&#x30A6;&#x30AC;&#x30E9;&#x30B7;</trd>
  <trdgrp lng="nl">
    <trd>peper<klr> (akra gusto)</klr><klr tip="ind"> (vrucht of
    plant)</klr></trd>,
    <trd>paprika<klr> (neakra gusto)</klr></trd>
  </trdgrp>
  <trd lng="pl">papryka</trd>
  <trd lng="ro">ardei</trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_u;&c_ch;&c_k;&c_o;&c_v;&c_y;&c_j; &c_p;&c_je;&c_r;&c_je;&c_c;</trd>
  <trd lng="sk">paprika <klr>(rastlina)</klr></trd>
  <trd lng="tr">dolmal&inodot;k biber</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_r;&c_u;&c_ch;&#x43A;&#x43E;&c_v;&c_i;&c_j; &#x43F;&c_je;&c_r;&c_je;&c_c;&#x44C;</trd>,
    <trd>&#x43E;&c_v;&#x43E;&c_ch;&c_je;&c_v;&c_i;&c_j; &#x43F;&c_je;&c_r;&c_je;&c_c;&#x44C; (&c_r;&#x43E;&c_s;&#x43B;&c_i;&#x43D;&c_a; &c_ib;
    &#x43F;&#x43B;&c_ib;&c_d;)</trd>
  </trdgrp>
</drv>

<drv mrk="kapsik.0eto">
  <kap><ofc>9</ofc> <tld/>eto</kap>
  <snc mrk="kapsik.0eto.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Malgranda ordinare tre akra <tld/>o<sncref ref="kapsik.0o.KUI"/>:
      <ekz>
        brezu la ru&gcirc;an kaj verdan neakrajn kapsikojn, la kukurbon kaj
        la akrajn <tld/>etojn dum 5 minutoj
        <fnt>
        <aut>-</aut>,
        <vrk>Fajreca kokida&jcirc;o kun legomoj</vrk>,
        <lok><url ref="http://eo.wikibooks.org/wiki/Kuirlibro">
        Vikimedia Kuirlibro</url>, 2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>vitet</trd>,
      <trd>bitxo</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_ch;&c_y;&c_k; <klr>(&c_p;&c_ja;&c_k;&c_u;&c_ch;&c_y;)</klr></trd>
  <trd lng="bo">&#xF56;&#xF66;&#xF72;&#xF63;&#xF0B;&#xF66;&#xFA8;&#xF53;&#xF0B;</trd>
  <trdgrp lng="en">
    <trd>chili</trd>,
    <trd>chilli</trd>
  </trdgrp>
  <trd lng="es">chile</trd>
  <trd lng="fr">piment</trd>
  <trdgrp lng="nl">
    <trd>lombok</trd>,
    <trd>Spaanse peper</trd>
  </trdgrp>
  <trd lng="tr">biber</trd>
</drv>
</art>
