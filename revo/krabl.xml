<art mrk="$Id: krabl.xml,v 1.9 2023/09/01 15:46:44 revo Exp $">
<kap>
  <rad>krabl</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="krabl.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="krabl.0i.KOMUNE">
    <dif>
      Iri, rampi, vigle movante la korpon kaj la krurojn, kiel iuj
      bestetoj:
      <ekz>
        &scirc;telisto <klr>...</klr> scias <tld/>i sur muro kiel mu&scirc;o
        <fnt><bib>RabistFab</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>gatejar</trd>,
    <trd>anar de quatre grapes</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>l&eacute;zt</trd>,
    <trd>sunout se <klr>(housenka)</klr></trd>,
    <trd>&scaron;ourat se</trd>
  </trdgrp>
  <trd lng="fr">carapater</trd>
  <trdgrp lng="ja">
    <trd>&#x306F;&#x3046;</trd>,
    <trd>&#x306F;&#x3044;&#x6B69;&#x304F;  [<ind>&#x306F;&#x3044;&#x3042;&#x308B;&#x304F;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">pe&lstroke;za&cacute; <klr>(o zwierz&eogonek;tach)</klr></trd>
  <trdgrp lng="pt">
    <trd>andar de <ind>gatinhas</ind></trd>,
    <trd>andar de <ind>gatas</ind></trd>,
    <trd>andar de <ind>quatro</ind></trd>,
    <trd>andar de quatro <ind>p&eacute;s</ind></trd>
  </trdgrp>
  <trd lng="ro">t&acirc;r&icirc;</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_z;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_o; &c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_y;&c_h;, &c_ch;&c_l;&c_je;&c_n;&c_i;&c_s;&c_t;&c_o;&c_n;&c_o;&c_g;&c_i;&c_h;
  &c_i; &c_t;.&c_p;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>liez&tcaron;</trd>,
    <trd>sun&uacute;&tcaron; sa <klr>(h&uacute;senica)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x432;&#x437;&#x430;&#x442;&#x438; &#x44F;&#x43A; &#x43A;&#x440;&#x430;&#x431;&#x438;</trd>,
    <trd>&#x43F;&#x430;&#x432;&#x443;&#x43A;&#x438;</trd>,
    <trd>&#x43A;&#x43E;&#x43F;&#x43E;&#x448;&#x438;&#x442;&#x438;&#x441;&#x44F;</trd>
  </trdgrp>
</drv>
