<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krabl.xml,v 1.11 2023/12/01 18:29:33 revo Exp $">
<kap>
  <rad>krabl</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="krabl.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="krabl.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="ir.0i.deAl">Iri</ref>, <ref cel="ramp.0i">rampeti</ref>,
      vigle movante la korpon kaj la krurojn, kiel iuj bestetoj:
      <ekz>
        &scirc;telisto <klr>[&#8230;]</klr> scias <tld/>i sur muro kiel mu&scirc;o
        <fnt><bib>RabistFab</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi ricevis la unuan &ccirc;i-jaran Ondon, mi rapidas sciigi vin, ke &gcirc;i <tld/>is tiujn
        &ccirc;. 300&#8239;km dum plenaj 3 semajnoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 3 (101)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la universo estis koncentrinta sin en sendimensian punkton kaj eksplodis
        <klr>[&#8230;]</klr> neeviteble <tld/>as en mia menso la penso: kaj anta&ubreve;e
        <fnt>
          <bib>Monato</bib>
          <aut>Alberto Garc&iacute;a Fumero</aut>
          <vrk>Scienco kaj religio kunfluu, ne kverelu</vrk>
          <lok>2014</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>gatejar</trd>,
    <trd>anar de quatre grapes</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>l&eacute;zt</trd>,
    <trd>sunout se <klr>(housenka)</klr></trd>,
    <trd>&scaron;ourat se</trd>
  </trdgrp>
  <trd lng="de">krabbeln</trd>
  <trd lng="fr">carapater</trd>
  <trdgrp lng="ja">
    <trd>&#12399;&#12358;</trd>,
    <trd>&#12399;&#12356;&#27497;&#12367;  <pr>&#12399;&#12356;&#12354;&#12427;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="pl">pe&lstroke;za&cacute; <klr>(o zwierz&eogonek;tach)</klr></trd>
  <trdgrp lng="pt">
    <trd>andar de <ind>gatinhas</ind></trd>,
    <trd>andar de <ind>gatas</ind></trd>,
    <trd>andar de <ind>quatro</ind></trd>,
    <trd>andar de quatro <ind>p&eacute;s</ind></trd>
  </trdgrp>
  <trd lng="ro">t&acirc;r&icirc;</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_z;&c_a;&c_t;&c_mol;<klr tip="amb"> (&c_o; &c_n;&c_a;&c_s;&c_je;&c_k;&c_o;&c_m;&c_y;&c_h;, &c_ch;&c_l;&c_je;&c_n;&c_i;&c_s;&c_t;&c_o;&c_n;&c_o;&c_g;&c_i;&c_h;
  &c_i; &c_t;.&c_p;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>liez&tcaron;</trd>,
    <trd>sun&uacute;&tcaron; sa <klr>(h&uacute;senica)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_v;&c_z;&c_a;&c_t;&c_i; &c_ja;&c_k; &c_k;&c_r;&c_a;&c_b;&c_i;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_u;&c_k;&c_i;</trd>,
    <trd>&c_k;&c_o;&c_p;&c_o;&c_sh;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>

