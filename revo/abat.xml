<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: abat.xml,v 1.56 2023/10/12 16:57:42 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>abat</rad>/o
</kap>

<drv mrk="abat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="monahx.0ejestro">Mona&hcirc;ejestro</ref>
      de <tld/>ejo:
      <ekz>
        en tiu &ccirc;i minuto preteriras la <tld/>o de la mona&hcirc;ejo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ekfloris okcidente &leftquot;<nom>Volap&uuml;k</nom>&rightquot; de l' <tld/>o
        <nom>&Scirc;lejer</nom> el Konstanco
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>MacRegol</nom> estis <tld/>o de la mona&hcirc;ejo de
        <nom>Birr</nom>, kiu mortis en 822
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2007/009180.php">Ornamitaj
          manuskriptoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      La titolon plej ofte uzas la <ref tip="vid" cel="benedikt.0ano">benediktanoj</ref>
      kaj aliaj ordenoj samdevenaj; en aliaj romkatolikaj ordenoj la
      sama funkcio ofte nomi&gcirc;as
      alie, ekz-e <ref cel="prior.0o">prioro</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_t;</trd>
  <trd lng="br">abad</trd>
  <trd lng="ca">abat</trd>
  <trd lng="cs">opat</trd>
  <trd lng="da">abbed</trd>
  <trd lng="de">Abt</trd>
  <trd lng="en">abbott</trd>
  <trd lng="es">abad</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_A;&a_h;&a_b; &a_b;&a_z;&a_r;&f_g;</trd>,
    <trd>&a_r;&a_y_hamza;&f_y;&a_s; &a_d;&a_kasra;&f_y;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">abb&eacute;</trd>
  <trd lng="he">&#x5E8;&#x5B9;&#x5D0;&#x5E9;&#x5C1; &#x5DE;&#x5B4;&#x5E0;&#x5D6;&#x5B8;&#x5E8;</trd>
  <trd lng="hu">ap&aacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#x4FEE;&#x9053;&#x9662;&#x9577;  <pr>&#x3057;&#x3085;&#x3046;&#x3069;&#x3046;&#x3044;&#x3093;&#x3061;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x5927;&#x4FEE;&#x9053;&#x9662;&#x9577;  <pr>&#x3060;&#x3044;&#x3057;&#x3085;&#x3046;&#x3069;&#x3046;&#x3044;&#x3093;&#x3061;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">abt</trd>
  <trd lng="oc">abat</trd>
  <trdgrp lng="pl">
    <trd>opat<klr> (tytu&lstroke; wy&zdot;szych prze&lstroke;o&zdot;onych wklasztorach
    katolickich)
    </klr></trd>
        <trd>archimandryta</trd>
        <trd>
    humen<klr> (w ko&sacute;ciele bizantyjskim)
    </klr></trd>
        <trd>
    humion<klr> (w ko&sacute;ciele bizantyjskim)
    </klr></trd>
        <trd>
    ihumen<klr> (w ko&sacute;ciele bizantyjskim)
    </klr></trd>
  </trdgrp>
  <trd lng="pt">abade</trd>
  <trd lng="ru">&c_a;&c_b;&c_b;&c_a;&c_t;</trd>
  <trd lng="sk">op&aacute;t</trd>
  <trd lng="sl">opat</trd>
  <trd lng="sv">abbot</trd>
  <trd lng="it">abate</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_b;&c_a;&c_t;</trd>,
    <trd>&c_ib;&c_g;&c_u;&#x43C;&c_je;&#x43D;</trd>
  </trdgrp>
</drv>

<drv mrk="abat.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="monahx.0inejestrino">Mona&hcirc;inejestrino</ref> 
      de <tld/>inejo:
      <ekz>
        en la jaro 1136 la <tld/>ino de la mona&hcirc;inejo mortis kaj
        <nom>Hildegard</nom> transprenis ties taskojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=6667">Hildegard
          de Bingen</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
&c_a;&c_b;&c_a;&c_t;&c_y;&c_s;&c_a;</trd>
  <trd lng="br">abadez</trd>
  <trd lng="ca">abadessa</trd>
  <trd lng="cs">abaty&scaron;e</trd>
  <trd lng="da">abedisse</trd>
  <trd lng="de">&Auml;btissin</trd>
  <trd lng="en">abbess</trd>
  <trd lng="es">abadesa</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_A;&a_h;&a_b;&a_h;&a_hamza_sure; &a_b;&a_z;&a_r;&f_g;</trd>,
    <trd>&a_r;&a_y_hamza;&f_y;&a_s;&a_h;&a_hamza_sure; &a_d;&a_kasra;&f_y;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">abbesse</trd>
  <trd lng="he">&#x5E0;&#x5D6;&#x5D9;&#x5E8;&#x5D4; &#x5E8;&#x5D0;&#x5E9;&#x5D9;&#x5EA;</trd>
  <trd lng="hu">ap&aacute;tn&odblac;</trd>
  <trd lng="ja">&#x5973;&#x5B50;&#x4FEE;&#x9053;&#x9662;&#x9577;  <pr>&#x3058;&#x3087;&#x3057;&#x3057;&#x3085;&#x3046;&#x3069;&#x3046;&#x3044;&#x3093;&#x3061;&#x3087;&#x3046;</pr></trd>
  <trd lng="nl">abdis</trd>
  <trd lng="oc">abadessa</trd>
  <trdgrp lng="pl">
    <trd>ksieni<klr> (tytu&lstroke; prze&lstroke;o&zdot;onych w klasztorach
    mniszek)</klr></trd>
    <trd>
    humenia<klr> (w ko&sacute;ciele bizantyjskim)</klr></trd>
    <trd>
    ihumenia<klr> (w ko&sacute;ciele bizantyjskim)</klr></trd>
    <trd>
    humenica<klr> (w ko&sacute;ciele bizantyjskim)</klr></trd>
  </trdgrp>
  <trd lng="pt">abadessa</trd>
  <trd lng="sk">abati&scaron;a</trd>
  <trd lng="sl">opatinja</trd>
<trd lng="ru">
&c_a;&c_b;&c_b;&c_a;&c_t;&c_i;&c_s;&c_a;</trd>
  <trd lng="it">badessa</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_b;&c_a;&c_t;&c_i;&c_s;&c_a;</trd>,
    <trd>&c_ib;&c_g;&c_u;&#x43C;&c_je;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="abat.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      <ref cel="cenobit.0o">Cenobita</ref> <ref tip="super" cel="monahx.0ejo">mona&hcirc;ejo</ref>,
      a&ubreve;tonoma, la&ubreve;kanone fondita kaj oficialigita, en kiu
      membras almena&ubreve; 12 mona&hcirc;oj:
      <ekz>
        ekzistas nur 6 <tld/>ejoj, kiuj solaj en la mondo rajtas
        produkti trapistan bieron
        <fnt>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2001/007144.php">Bierlando Belgio</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        vizitindas deko da cisterciaj kaj benediktaj <tld/>ejoj, kiujn
        la historio postlasis
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Gassion</aut>
          <vrk><url ref="&Monato;/2003/007934.html">Aude</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la koron de la forpasinto oni entombigis en Hungario, en la
        <tld/>ejo <nom>Pannonhalma</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2011/010463.php">Forpasas
          granda e&ubreve;ropano</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_c;&c_t;&c_v;&c_a;<klr tip="amb">
(&c_m;&c_u;&c_zh;&c_ch;&c_y;&c_n;&c_s;&c_k;&c_a;&c_je;)</klr></trd>
  <trd lng="br">
abati<klr tip="ind"> (menec'h)</klr></trd>
  <trd lng="ca">abadia</trd>
  <trd lng="cs">opatstv&iacute;</trd>
  <trdgrp lng="da">
    <trd>abbedi</trd>,
    <trd>kloster</trd>
  </trdgrp>
  <trd lng="de">Abtei</trd>
  <trd lng="en">abbey</trd>
  <trd lng="es">abad&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_kasra;&f_y;&a_r;</trd>,
    <trd>&a_S;&a_w;&a_m;&a_ayn;&a_h;</trd>,
    <trd>&a_r;&a_damma;&a_h;&a_b;&a_A;&a_n;&f_g;&a_A;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">
abbaye<klr tip="ind"> (de moines)</klr></trd>
  <trd lng="he">&#x5DE;&#x5B4;&#x5E0;&#x5D6;&#x5B8;&#x5E8;</trd>
  <trd lng="hu">ap&aacute;ts&aacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x4FEE;&#x9053;&#x9662;  <pr>&#x3057;&#x3085;&#x3046;&#x3069;&#x3046;&#x3044;&#x3093;</pr></trd>,
    <trd>&#x5927;&#x4FEE;&#x9053;&#x9662;  <pr>&#x3060;&#x3044;&#x3057;&#x3085;&#x3046;&#x3069;&#x3046;&#x3044;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">abdij</trd>
  <trd lng="oc">abadi&aacute;</trd>
  <trd lng="pl">opactwo</trd>
  <trd lng="pt">abadia</trd>
  <trd lng="ru">
&c_a;&c_b;&c_b;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;
<klr tip="amb">
  (&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_je;)
</klr></trd>
  <trd lng="sk">op&aacute;tstvo</trd>
  <trd lng="sl">opatija</trd>
  <trd lng="it">abbazia</trd>
  <trd lng="uk">&c_a;&c_b;&c_a;&c_t;&c_s;&c_t;&c_v;&#x43E;</trd>
</drv>

<drv mrk="abat.0inejo">
  <kap><tld/>inejo</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      <ref cel="cenobit.0o">Cenobita</ref> <ref tip="super" cel="monahx.0inejo">mona&hcirc;inejo</ref>,
      a&ubreve;tonoma, la&ubreve;kanone fondita kaj oficialigita, en kiu
      membras almena&ubreve; 12 mona&hcirc;inoj:
      <ekz>
        la Pre&gcirc;ejo de Sankta Mikaelo, de la 12-a jarcento, kun proksima
        benedikta <tld/>inejo, rekonstrui&gcirc;is en la 17-a jarcento la&ubreve; la
        baroka stilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=75655">Mazara
          del Vallo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_c;&c_t;&c_v;&c_a;
  <klr tip="amb">
  (&c_zh;&c_a;&c_n;&c_o;&c_ch;&c_a;&c_je;)
  </klr></trd>
  <trd lng="br">abati<klr tip="amb"> (leanezed)</klr></trd>
  <trd lng="ca">abadia<klr tip="amb"> (de monjes)</klr></trd>
  <trd lng="da">nonnekloster</trd>
  <trd lng="de">Abtei<klr tip="amb"> (von Nonnen)</klr></trd>
  <trd lng="en">abbey</trd>
  <trd lng="es">abad&iacute;a<klr> (de monjas)</klr></trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_kasra;&f_y;&a_r;</trd>,
    <trd>&a_S;&a_w;&a_m;&a_ayn;&a_h;</trd>,
    <trd>&a_r;&a_damma;&a_h;&a_b;&a_A;&a_n;&f_g;&a_A;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">abbaye<klr tip="amb"> (de nonnes)</klr></trd>
  <trd lng="hu">ap&aacute;caz&aacute;rda</trd>
  <trd lng="nl">abdij<klr tip="amb"> (vrouwenklooster)</klr></trd>
  <trd lng="oc">abadi&aacute;<klr tip="amb"> (de monjas)</klr></trd>
  <trd lng="ru">
    &c_a;&c_b;&c_b;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;
    <klr tip="amb">
    (&c_zh;&c_je;&c_n;&c_s;&c_k;&c_o;&c_je;)
    </klr></trd>
  <trd lng="it">convento</trd>
</drv>
</art>
