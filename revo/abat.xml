<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>abat</rad>/o
</kap>
<drv mrk="abat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="monahx.0ejestro">Mona&hcirc;ejestro</ref>
      de <tld/>ejo.
    </dif>
    <rim>
      La titolon plej ofte uzas la <ref cel="benedi.0o">benediktanoj</ref>
      kaj aliaj ordenoj samdevenaj;
      en aliaj romkatolikaj ordenoj la sama funkcio ofte nomi&gcirc;as
      alie, ekz-e <ref cel="prior.0o">prioro</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_t;</trd>
  <trd lng="cs">opat</trd>
  <trd lng="de">Abt</trd>
  <trd lng="en">abbott</trd>
  <trd lng="es">abad</trd>
  <trd lng="fr">abb&eacute;</trd>
  <trd lng="hu">ap&aacute;t</trd>
  <trd lng="nl">abt</trd>
  <trd lng="pt">abade</trd>
  <trd lng="ru">&c_a;&c_b;&c_b;&c_a;&c_t;</trd>
  <trd lng="sk">op&aacute;t</trd>
  <trd lng="sv">abbot</trd>
</drv>

<drv mrk="abat.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="monahx.0inejestrino">Mona&hcirc;inejestrino</ref>
      de <tld/>inejo.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_t;&c_y;&c_s;&c_a;</trd>
  <trd lng="cs">abaty&scaron;e</trd>
  <trd lng="de">&Auml;btissin</trd>
  <trd lng="en">abbess</trd>
  <trd lng="fr">abbesse</trd>
  <trd lng="nl">abdis</trd>
  <trd lng="sk">abati&scaron;a</trd>
  <trd lng="ru">&c_a;&c_b;&c_b;&c_a;&c_t;&c_i;&c_s;&c_a;</trd>
</drv>

<drv mrk="abat.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Cenobita <ref tip="super" cel="monahx.0ejo">mona&hcirc;ejo</ref>,
      a&ubreve;tonoma, la&ubreve;kanone fondita kaj oficialigita, en kiu
      membras
      almena&ubreve; 12 mona&hcirc;oj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_c;&c_t;&c_v;&c_a;<klr tip="amb"> (&c_m;&c_u;&c_zh;&c_ch;&c_y;&c_n;&c_s;&c_k;&c_a;&c_je;)</klr></trd>
  <trd lng="cs">opatstv&iacute;</trd>
  <trd lng="fr">abbaye<klr tip="ind"> (de moines)</klr></trd>
  <trd lng="nl">abdij</trd>
  <trd lng="ru">&c_a;&c_b;&c_b;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;<klr tip="amb">
  (&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_je;)</klr></trd>
  <trd lng="sk">op&aacute;tstvo</trd>
</drv>

<drv mrk="abat.0inejo">
  <kap><tld/>inejo</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Cenobita
      <ref tip="super" cel="monahx.0inejo">mona&hcirc;inejo</ref>,
      a&ubreve;tonoma, la&ubreve;kanone fondita kaj oficialigita, en kiu
      membras
      almena&ubreve; 12 mona&hcirc;inoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_c;&c_t;&c_v;&c_a;<klr tip="amb"> (&c_zh;&c_a;&c_n;&c_o;&c_ch;&c_a;&c_je;)</klr></trd>
  <trd lng="fr">abbaye<klr tip="ind"> (de nonnes)</klr></trd>
  <trd lng="nl">abdij<klr tip="ind"> (vrouwenklooster)</klr></trd>
  <trd lng="ru">&c_a;&c_b;&c_b;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;<klr tip="amb">
  (&c_zh;&c_je;&c_n;&c_s;&c_k;&c_o;&c_je;)</klr></trd>
</drv>
</art>
