<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>abat</rad>/o
</kap>

<drv mrk="abat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
<ref tip="super" cel="monahx.0ejestro">Mona&hcirc;ejestro</ref>
      de <tld/>ejo.
    </dif>
    <rim>
      La titolon plej ofte uzas la 
<ref cel="benedi.0o">benediktanoj</ref>
      kaj aliaj ordenoj samdevenaj;
      en aliaj romkatolikaj ordenoj la sama funkcio ofte nomi&gcirc;as
      alie, ekz-e <ref cel="prior.0o">prioro</ref>.
    </rim>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_t;</trd>
  <trd lng="br">abad</trd>
  <trd lng="ca">abat</trd>
  <trd lng="cs">opat</trd>
  <trd lng="de">Abt</trd>
  <trd lng="en">abbott</trd>
  <trd lng="es">abad</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_A;&a_h;&a_b; &a_b;&a_z;&a_r;&f_g;</trd>,
    <trd>&a_r;&a_y_hamza;&f_y;&a_s; &a_d;&a_kasra;&f_y;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">abb&eacute;</trd>
  <trd lng="hu">ap&aacute;t</trd>
  <trd lng="nl">abt</trd>
  <trd lng="oc">abat</trd>
  <trdgrp lng="pl">
<trd>opat<klr> (tytu&lstroke; wy&zdot;szych prze&lstroke;o&zdot;onych wklasztorach
katolickich)
</klr></trd>
    <trd>archimandryta</trd>
    <trd>
humen<klr> (w ko&sacute;ciele bizantyjskim)
</klr></trd>
    <trd>
humion<klr> (w ko&sacute;ciele bizantyjskim)
</klr></trd>
    <trd>
ihumen<klr> (w ko&sacute;ciele bizantyjskim)
</klr></trd>
  </trdgrp>
  <trd lng="pt">abade</trd>
<trd lng="ru">&c_a;&c_b;&c_b;&c_a;&c_t;</trd>
  <trd lng="sk">op&aacute;t</trd>
  <trd lng="sv">abbot</trd>
  <trd lng="it">abate</trd>
</drv>

<drv mrk="abat.0ino">
  <kap><tld/>ino</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
<ref tip="super" cel="monahx.0inejestrino">Mona&hcirc;inejestrino</ref> 
de <tld/>inejo.
    </dif>
  </snc>
  <trd lng="be">
&c_a;&c_b;&c_a;&c_t;&c_y;&c_s;&c_a;</trd>
  <trd lng="br">abadez</trd>
  <trd lng="ca">abadessa</trd>
  <trd lng="cs">abaty&scaron;e</trd>
  <trd lng="de">&Auml;btissin</trd>
  <trd lng="en">abbess</trd>
  <trd lng="es">abadesa</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_A;&a_h;&a_b;&a_h;&a_hamza_sure; &a_b;&a_z;&a_r;&f_g;</trd>,
    <trd>&a_r;&a_y_hamza;&f_y;&a_s;&a_h;&a_hamza_sure; &a_d;&a_kasra;&f_y;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">abbesse</trd>
  <trd lng="hu">
ap&aacute;tn&odblac;</trd>
  <trd lng="nl">abdis</trd>
  <trd lng="oc">abadessa</trd>
  <trdgrp lng="pl">
<trd>ksieni<klr> (tytu&lstroke; prze&lstroke;o&zdot;onych w klasztorach
mniszek)</klr></trd>
<trd>
humenia<klr> (w ko&sacute;ciele bizantyjskim)</klr></trd>
<trd>
ihumenia<klr> (w ko&sacute;ciele bizantyjskim)</klr></trd>
<trd>
humenica<klr> (w ko&sacute;ciele bizantyjskim)</klr></trd>
  </trdgrp>
  <trd lng="pt">abadessa</trd>
  <trd lng="sk">abati&scaron;a</trd>
<trd lng="ru">
&c_a;&c_b;&c_b;&c_a;&c_t;&c_i;&c_s;&c_a;</trd>
  <trd lng="it">badessa</trd>
</drv>

<drv mrk="abat.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Cenobita 
<ref tip="super" cel="monahx.0ejo">mona&hcirc;ejo</ref>,
      a&ubreve;tonoma, la&ubreve;kanone fondita kaj oficialigita, en kiu
      membras
      almena&ubreve; 12 mona&hcirc;oj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_c;&c_t;&c_v;&c_a;<klr tip="amb">
(&c_m;&c_u;&c_zh;&c_ch;&c_y;&c_n;&c_s;&c_k;&c_a;&c_je;)</klr></trd>
  <trd lng="br">
abati<klr tip="ind"> (menec'h)</klr></trd>
  <trd lng="ca">abadia</trd>
  <trd lng="cs">opatstv&iacute;</trd>
  <trd lng="de">Abtei</trd>
  <trd lng="en">abbey</trd>
  <trd lng="es">abad&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_kasra;&f_y;&a_r;</trd>,
    <trd>&a_S;&a_w;&a_m;&a_ayn;&a_h;</trd>,
    <trd>&a_r;&a_damma;&a_h;&a_b;&a_A;&a_n;&f_g;&a_A;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">
abbaye<klr tip="ind"> (de moines)</klr></trd>
  <trd lng="hu">ap&aacute;ts&aacute;g</trd>
  <trd lng="nl">abdij</trd>
  <trd lng="oc">abadi&aacute;</trd>
  <trd lng="pl">opactwo</trd>
  <trd lng="pt">abadia</trd>
  <trd lng="ru">
&c_a;&c_b;&c_b;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;
<klr tip="amb">
  (&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_je;)
</klr></trd>
  <trd lng="sk">op&aacute;tstvo</trd>
  <trd lng="it">abbazia</trd>
</drv>

<drv mrk="abat.0inejo">
  <kap><tld/>inejo</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Cenobita 
<ref tip="super" cel="monahx.0inejo">mona&hcirc;inejo</ref>,
      a&ubreve;tonoma, la&ubreve;kanone fondita kaj oficialigita, en kiu
      membras almena&ubreve; 12 mona&hcirc;inoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_c;&c_t;&c_v;&c_a;
<klr tip="amb">
(&c_zh;&c_a;&c_n;&c_o;&c_ch;&c_a;&c_je;)
</klr></trd>
  <trd lng="br">
abati<klr tip="amb"> (leanezed)</klr></trd>
  <trd lng="ca">
abadia<klr tip="amb"> (de monjes)</klr></trd>
  <trd lng="de">
Abtei<klr tip="amb"> (von Nonnen)</klr></trd>
  <trd lng="en">abbey</trd>
  <trd lng="es">abad&iacute;a<klr> (de monjas)</klr></trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_kasra;&f_y;&a_r;</trd>,
    <trd>&a_S;&a_w;&a_m;&a_ayn;&a_h;</trd>,
    <trd>&a_r;&a_damma;&a_h;&a_b;&a_A;&a_n;&f_g;&a_A;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">
abbaye<klr tip="amb"> (de nonnes)</klr></trd>
  <trd lng="hu">ap&aacute;caz&aacute;rda</trd>
  <trd lng="nl">
abdij<klr tip="amb"> (vrouwenklooster)</klr></trd>
  <trd lng="oc">
abadi&aacute;<klr tip="amb"> (de monjas)</klr></trd>
  <trd lng="ru">
&c_a;&c_b;&c_b;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;
<klr tip="amb">
  (&c_zh;&c_je;&c_n;&c_s;&c_k;&c_o;&c_je;)
</klr></trd>
  <trd lng="it">convento</trd>
</drv>
</art>
