<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>brik</rad>/o
    </kap>

    <drv mrk="brik.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="brik.0o.bakita">
        <uzo tip="fak">KON</uzo>
        <dif>
          Negranda ortangula paralelipeda bloko, plej ofte el argilo (bakita,
          kunpremita a&ubreve; sunsekigita) a&ubreve; el miksa&jcirc;o de kvarca sablo kaj
          kalko, uzata por konstrui:
          <ekz>
            dronanto domon proponas, savito e&ccirc; <tld/>on ne donas   
            <klr>(post la dan&gcirc;ero oni forgesas la
              promesojn)</klr><fnt><bib>PrV</bib></fnt>;
          </ekz><ekz>
            la domo estis &ccirc;irka&ubreve;ita de muro el palaj <tld/>oj
	    <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 9a.</lok></fnt>
          </ekz><ekz>
            iru en kalkon, knedu argilon, faru fortajn <tld/>ojn
            <fnt><bib>MT</bib>. <lok>&Nah; 3:14</lok></fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_c;&c_a;&c_g;&c_l;&c_ib;&c_n;&c_a;</trd>
        <trd lng="de">Ziegel</trd>
        <trd lng="fa">&a_A_madda;&a_j;&a_r;</trd>
        <trd lng="hu">t&eacute;gla</trd>
        <trdgrp lng="pl">
          <trd>ceg&lstroke;a</trd>,
          <trd>ceg&lstroke;&oacute;wka</trd>
        </trdgrp>
        <trd lng="ru">&c_k;&c_i;&c_r;&c_p;&c_i;&c_ch;</trd>
      </snc>
      <snc mrk="brik.0o.formo">
        <dif>
          &Ccirc;iu <tld/>forma a&jcirc;o:
          <ekz>
            <tld/>o de sapo.
          </ekz>
        </dif>
        <refgrp tip="vid">
          <ref cel="sxton.0o">&scirc;tono</ref>,
          <ref cel="tegol.0o">tegolo</ref>.
        </refgrp>
        <trdgrp lng="be">
          <trd>&c_k;&c_a;&c_v;&c_a;&c_l;&c_a;&c_k;</trd>,
          <trd>&c_b;&c_r;&c_u;&c_s;&c_o;&c_k;</trd>
        </trdgrp>
        <trd lng="en">block</trd>
        <trd lng="fa">&a_q;&a_A;&a_l;&a_b;</trd>
        <trd lng="hu">darab <klr tip="ind"> (t&eacute;gla form&aacute;j&uacute;)</klr></trd>
        <trdgrp lng="pl">
          <trd>ceg&lstroke;a</trd>,
          <trd>bloczek</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_k;&c_u;&c_s;&c_o;&c_k;</trd>,
          <trd>&c_b;&c_r;&c_u;&c_s;&c_o;&c_k;</trd>
        </trdgrp>
      </snc>
      <trd lng="en">brick</trd>
      <trd lng="es">ladrillo</trd>
      <trd lng="fr">brique</trd>
      <trd lng="sk">tehla</trd>
    </drv>

    <drv mrk="brik.0eto">
      <kap><tld/>eto<fnt>Z</fnt></kap>
      <snc>
        <dif>
          Malgranda <tld/>o <klr>(speciale el kunpremita pulvoro de
            <ref cel="karb.ter0o">terkarbo</ref>)</klr>.
        </dif>
        <trd lng="be">&c_b;&c_r;&c_y;&c_k;&c_je;&c_t;</trd>
        <trd lng="de">Brikett</trd>
        <trdgrp lng="fa">
          <trd>&a_b;&a_r;&f_y;&f_k;&a_t;</trd>,
          <trd>&a_z;&a_gh;&a_A;&a_l; &a_q;&a_A;&a_l;&a_b;&f_y;</trd>
        </trdgrp>
        <trd lng="fr">briquette</trd>
        <trd lng="hu">brikett</trd>
        <trd lng="pl">brykiet</trd>
        <trdgrp lng="ru">
          <trd>&c_b;&c_r;&c_i;&c_k;&c_je;&c_t;</trd>,
          <trd>&c_k;&c_i;&c_r;&c_p;&c_i;&c_ch;&c_i;&c_k;</trd>
        </trdgrp>
        <trd lng="sk">briketa</trd>
      </snc>
    </drv>
  </art>
