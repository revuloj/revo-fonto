<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: flik.xml,v 1.28 2023/11/09 20:48:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>flik</rad>/i
</kap>
<drv mrk="flik.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="flik.0i.ripari">
    <dif>
      <ref tip="super" cel="ripar.0i">Ripari</ref> almetante pecon por
      kovri truon, ekzemple en vesto:
      <ekz>
	<tld/>i &scirc;iritan robon, difektitan &scirc;uon;
      </ekz>
      <ekz>
	<tld/>i truon en tegmento<fnt>Z</fnt>;
      </ekz>
      <ekz>
	multpece <tld/>ita pantalono;
      </ekz>
      <ekz>
	surkudri pecon por <tld/>i mantelon.
      </ekz>
    </dif>
    <trd lng="de">flicken</trd>
    <trd lng="hu">foltoz</trd>	  
    <trdgrp lng="zh">
    <trd>&#x4FEE;&#x8865;&#x8FC7; <pr>xi&#x16B;b&#x1D4;gu&#xF2;</pr></trd>,
    <trd>&#x6253;&#x4E86;&#x8865;&#x4E01; <pr>d&#x1CE;li&#x1CE;ob&#x1D4;d&#x12B;ng</pr></trd>,
    <trd>&#x4FEE;&#x8865;&#x5320; <pr>xi&#x16B;b&#x1D4;ji&#xE0;ng</pr></trd>,
    <trd>&#x7D29; <pr>zh&#xEC;</pr></trd>,
    <trd>&#x976A; <pr>d&#x12B;ng</pr></trd>,
    <trd>&#x88F0; <pr>du&#x14D;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="flik.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mallerte, nur parte ripari ion:
      <ekz>
	<tld/>i rompitan amikecon;
      </ekz>
      <ekz>
	<tld/>i la karton de E&ubreve;ropo, la pacon.
      </ekz>
    </dif>
    <trd lng="de">zusammenflicken</trd>
    <trd lng="hu">&ouml;sszefoltoz</trd>	
    <trdgrp lng="zh">
    <trd>&#x8E9C; <pr>zu&#x101;n</pr></trd>,
    <trd>&#x62FC;&#x8D77;&#x4F86; <pr>p&#x12B;nqilai</pr></trd>,
    <trd>&#x534F;&#x540C; <pr>xi&#xE9;t&#xF3;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_y;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&#xF63;&#xFB7;&#xF53;&#xF0B;&#xF54;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF42;&#xF0B;</trd>
  <trdgrp lng="ca">
    <trd>adobar</trd>,
    <trd>aparracar</trd>,
    <trd>apeda&ccedil;ar</trd>,
    <trd>reparar <klr>(apeda&ccedil;ar)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>flikovat</trd>,
    <trd>l&aacute;tat</trd>,
    <trd>spravit</trd>,
    <trd>za&scaron;&iacute;t</trd>,
    <trd>za&scaron;&iacute;vat</trd>,
    <trd>z&aacute;platovat</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>patch</trd>,
    <trd>darn</trd>,
    <trd>mend</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>emparchar</trd>,
    <trd>parchear</trd>,
    <trd>parchar</trd>
  </trdgrp>
  <trd lng="fa">&a_w;&a_S;&a_l;&a_h; &a_z;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>racommoder</trd>,
    <trd>rapi&eacute;cer</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3064;&#x304E;&#x3092;&#x5F53;&#x3066;&#x308B;  <pr>&#x3064;&#x304E;&#x3092;&#x3042;&#x3066;&#x308B;</pr></trd>,
    <trd>&#x53D6;&#x308A;&#x7E55;&#x3046;  <pr>&#x3068;&#x308A;&#x3064;&#x304F;&#x308D;&#x3046;</pr></trd>,
    <trd>&#x30D1;&#x30C3;&#x30C1;&#x3092;&#x5F53;&#x3066;&#x308B;  <pr>&#x3071;&#x3063;&#x3061;&#x3092;&#x3042;&#x3066;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">oplappen</trd>
  <trd lng="pl">&lstroke;ata&cacute;</trd>
  <trdgrp lng="pt">
    <trd>remendar</trd>,
    <trd>consertar</trd>,
    <trd>reparar</trd>,
    <trd>atamancar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_z;&c_a;&c_p;&c_l;&c_a;&c_t;&c_u;</trd>
  </trdgrp>
  <trd lng="sk">pl&aacute;ta&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x430;&#x442;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x448;&#x442;&#x43E;&#x43F;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="flik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Riparo farita per surkudrita a&ubreve; surmetita peco:
      <ekz>
	neniu alkudras <tld/>a&jcirc;on el nefulita drapo sur malnovan veston
	<fnt><bib>NT</bib>, <lok>&Mar;, 2:21</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="ja">
    <trd>&#x3064;&#x304E;</trd>,
    <trd>&#x30D1;&#x30C3;&#x30C1;</trd>
  </trdgrp>
  <trd lng="pt">peda&ccedil;</trd>
  <trd lng="de">Flicken</trd>
  <trd lng="en">patch</trd>
  <trdgrp lng="es">
    <trd>parche</trd>,
    <trd>remiendo</trd>
  </trdgrp>
  <trd lng="fa">&a_w;&a_S;&a_l;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>pi&egrave;ce<klr tip="ind"> (pour r&eacute;parer un trou)</klr></trd>,
    <trd>rustine</trd>
  </trdgrp>
  <trd lng="hu">folt</trd>  
  <trd lng="nl">oplapping</trd>
  <trd lng="pl">&lstroke;ata</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  <trd lng="uk">&#x43B;&#x430;&#x442;&#x43A;&#x430;</trd>
  <trd lng="zh">&#x8865;&#x4E01; <pr>b&#x1D4;ding</pr></trd>
</drv>

<drv mrk="flik.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu metie <tld/>as.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>spr&aacute;vka&rcaron;</trd>,
    <trd>z&aacute;plat&aacute;&rcaron;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>remendador</trd>,
    <trd>parcheador</trd>
  </trdgrp>
  <trd lng="fr">rapi&eacute;ceur</trd>
  <trd lng="hu">foltoz&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x7E55;&#x3044;&#x8077;&#x4EBA;  <pr>&#x3064;&#x304F;&#x308D;&#x3044;&#x3057;&#x3087;&#x304F;&#x306B;&#x3093;</pr></trd>,
    <trd>&#x639B;&#x3051;&#x306F;&#x304E;&#x5C4B;  <pr>&#x304B;&#x3051;&#x306F;&#x304E;&#x3084;</pr></trd>
  </trdgrp>
  <trd lng="nl">oplapper</trd>
  <trd lng="sk">z&aacute;plat&aacute;r</trd>
</drv>

<drv mrk="flik.0acxi">
  <kap><tld/>a&ccirc;i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senzorge, mallerte <tld/>i.
    </dif>
  </snc>
  <trdgrp lng="de">
  <trd>zusammenmurksen</trd>,
  <trd>pfuschen</trd>
  </trdgrp>
  <trd lng="es">remendar</trd>
  <trd lng="fr">rapetasser</trd>
  <trd lng="hu">&ouml;sszetoldoz</trd>  
  <trd lng="ja">&#x4E0D;&#x7D30;&#x5DE5;&#x306B;&#x3064;&#x304E;&#x3092;&#x5F53;&#x3066;&#x308B;  <pr>&#x3076;&#x3055;&#x3044;&#x304F;&#x306B;&#x3064;&#x304E;&#x3092;&#x3042;&#x3066;&#x308B;</pr></trd>
</drv>
</art>
