<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: flik.xml,v 1.32 2025/12/18 10:39:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>flik</rad>/i
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="flik.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="flik.0i.ripari">
    <dif>
      <ref tip="super" cel="ripar.0i">Ripari</ref> almetante pecon por
      kovri truon, ekzemple en vesto:
      <ekz>
        <tld/>i <klr>[&#8230;s]</klr>iajn &scirc;trumpojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur siajn azenojn <klr>[ili metis]</klr> malnovajn dis&scirc;iritajn kaj <tld/>itajn
        vinsakojn
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 9:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        fu&scirc;a kunkudra&jcirc;o, kie oni al<tld/>is kroman strion
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        strateto kun &gcirc;iaj ruini&gcirc;antaj kaj re<tld/>itaj fasadoj
        <fnt>
          <bib>MkM</bib>
          <lok>21</lok>
        </fnt>;
      </ekz>
      <ekz>
        knabo, finu la kamizolon kaj <tld/>u la pantalonon, a&ubreve; vi eksentos la ulnon sur
        viaj oreloj
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en fremda tegmento li <tld/>as truon kaj en propra ne vidas la fluon
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">flicken</trd>
    <trd lng="hu">foltoz</trd>
    <trdgrp lng="zh">
    <trd>&#x4FEE;&#34917;&#x8FC7; <pr>xi&umacron;b&#x1D4;gu&ograve;</pr></trd>,
    <trd>&#25171;&#x4E86;&#34917;&#x4E01; <pr>d&#x1CE;li&#x1CE;ob&#x1D4;d&imacron;ng</pr></trd>,
    <trd>&#x4FEE;&#34917;&#21280; <pr>xi&umacron;b&#x1D4;ji&agrave;ng</pr></trd>,
    <trd>&#x7D29; <pr>zh&igrave;</pr></trd>,
    <trd>&#x976A; <pr>d&imacron;ng</pr></trd>,
    <trd>&#x88F0; <pr>du&omacron;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="flik.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Mallerte, nur parte <ref tip="super" cel="ripar.0i">ripari</ref> ion:
      <ekz>
        <tld/>i la kreskantan fendon inter la bezono kaj oferto per abrupta malalti&gcirc;o de
        la kvalita nivelo
        <fnt>
          <bib>KFl</bib>
          <lok>I. Enkonduko</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni komencis kun<tld/>i la informerojn, kiujn &ccirc;iu el ni povis alporti
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i siajn konsciencojn
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2007/009272.php"
              >Malfavore fervoje</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        truon de l' honoro <tld/>os neniu tajloro
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">zusammenflicken</trd>
    <trd lng="hu">&ouml;sszefoltoz</trd>
    <trdgrp lng="zh">
    <trd>&#x8E9C; <pr>zu&amacron;n</pr></trd>,
    <trd>&#x62FC;&#x8D77;&#x4F86; <pr>p&imacron;nqilai</pr></trd>,
    <trd>&#x534F;&#x540C; <pr>xi&eacute;t&oacute;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_c;&c_y;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bo">&t_la;&t_hata;&t_na;&t_tsheg;&t_pa;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_ga;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>adobar</trd>,
    <trd>aparracar</trd>,
    <trd>apeda&ccedil;ar</trd>,
    <trd>reparar <klr>(apeda&ccedil;ar)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>flikovat</trd>,
    <trd>l&aacute;tat</trd>,
    <trd>spravit</trd>,
    <trd>za&scaron;&iacute;t</trd>,
    <trd>za&scaron;&iacute;vat</trd>,
    <trd>z&aacute;platovat</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>patch</trd>,
    <trd>darn</trd>,
    <trd>mend</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>emparchar</trd>,
    <trd>parchear</trd>,
    <trd>parchar</trd>
  </trdgrp>
  <trd lng="fa">&a_w;&a_S;&a_l;&a_h; &a_z;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>racommoder</trd>,
    <trd>rapi&eacute;cer</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>tambal</trd>,
    <trd><baz>tambal</baz>menambal</trd>,
    <trd><baz>tampal</baz>menampal</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12388;&#x304E;&#12434;&#x5F53;&#12390;&#x308B;  <pr>&#12388;&#x304E;&#12434;&#12354;&#12390;&#x308B;</pr></trd>,
    <trd>&#x53D6;&#x308A;&#x7E55;&#12358;  <pr>&#12392;&#x308A;&#12388;&#x304F;&#x308D;&#12358;</pr></trd>,
    <trd>&#x30D1;&#x30C3;&#x30C1;&#12434;&#x5F53;&#12390;&#x308B;  <pr>&#12401;&#12387;&#12385;&#12434;&#12354;&#12390;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">oplappen</trd>
  <trd lng="pl">&lstroke;ata&cacute;</trd>
  <trdgrp lng="pt">
    <trd>remendar</trd>,
    <trd>consertar</trd>,
    <trd>reparar</trd>,
    <trd>atamancar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_z;&c_a;&c_p;&c_l;&c_a;&c_t;&c_u;</trd>
  </trdgrp>
  <trd lng="sk">pl&aacute;ta&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_sh;&c_t;&c_o;&c_p;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="flik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Riparo farita per surkudrita a&ubreve; surmetita peco:
      <ekz>
	neniu alkudras <tld/>a&jcirc;on el nefulita drapo sur malnovan veston
	<fnt><bib>NT</bib>, <lok>&Mar;, 2:21</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="ja">
    <trd>&#12388;&#x304E;</trd>,
    <trd>&#x30D1;&#x30C3;&#x30C1;</trd>
  </trdgrp>
  <trd lng="pt">peda&ccedil;</trd>
  <trd lng="de">Flicken</trd>
  <trd lng="en">patch</trd>
  <trdgrp lng="es">
    <trd>parche</trd>,
    <trd>remiendo</trd>
  </trdgrp>
  <trd lng="fa">&a_w;&a_S;&a_l;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>pi&egrave;ce<klr tip="ind"> (pour r&eacute;parer un trou)</klr></trd>,
    <trd>rustine</trd>
  </trdgrp>
  <trd lng="hu">folt</trd>
  <trdgrp lng="id">
    <trd>tambalan</trd>,
    <trd>tampal</trd>,
    <trd>tampalan</trd>
  </trdgrp>
  <trd lng="nl">oplapping</trd>
  <trd lng="pl">&lstroke;ata</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  <trd lng="uk">&c_l;&c_a;&c_t;&c_k;&c_a;</trd>
  <trd lng="zh">&#34917;&#x4E01; <pr>b&#x1D4;ding</pr></trd>
</drv>

<drv mrk="flik.0ilo">
  <kap>
    <tld/>ilo
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="pingl.0o.tajlora">Pinglo</ref> a&ubreve; alia ilo por <tld/>ado:
      <ekz>
        &scirc;trump<tld/>ilo
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fingroj direktis la <tld/>ilon rekte al la pantoflo
        <fnt>
          <bib>Fab2</bib>
          <lok>Flikilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        <tld/>ilo <klr>[&#8230;]</klr> samfunkcia kiel la Uniksa <nom>patch</nom>, t.e. destinita por
        a&ubreve;tomate
        apliki diferencodosieron
        <fnt>
          <vrk><url ref="https://bertilow.com/div/komputada_leksikono/F.html#FLIKILO"
              >Komputada Leksikono, flikilo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Stopfnadel</trd>,
    <trd>Repassiernadel</trd>
  </trdgrp>
  <trd lng="en">darning needle</trd>
  <trd lng="fr">aiguille &agrave; repriser</trd>
<trdgrp lng="id">
  <trd><baz>tambal</baz>penambal</trd>,
  <trd><baz>tampal</baz>penampal</trd>,
  <trd>alat tambal</trd>,
  <trd>alat tampal</trd>
</trdgrp>
</drv>

<drv mrk="flik.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu metie <tld/>as:
      <ekz>
        je via humila servo, sinjoraj mo&scirc;toj, la tre malmultekosta &scirc;u<tld/>isto
        <fnt>
          <aut>Julio Baghy</aut>
          <vrk><url
          ref="http://literaturo.org/HARLOW-Don/Esperanto/Literaturo/migranta/flikisto.html"
              >La &scirc;uflikisto</url></vrk>
          <lok>Migranta plumo pa&gcirc;oj 80-87</lok>
        </fnt>;
      </ekz>
      <ekz>
        en &ccirc;i tiuj malfacilaj tempoj la &ccirc;evalo faras bonan negocon, pli bonan ol la
        <tld/>isto
        <fnt>
          <aut>trad. Kabe</aut>
          <vrk><url ref="http://esperanto.davidgsimpson.com/inko/i2s-pola-antologio.pdf"
              >La Flikisto</url></vrk>
          <lok>p. 27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_t;&c_a;&c_ch;&c_n;&c_ib;&c_k; <klr>(&c_k;&c_r;&c_a;&c_v;&c_je;&c_c;)</klr></trd>
  <trdgrp lng="cs">
    <trd>spr&aacute;vka&rcaron;</trd>,
    <trd>z&aacute;plat&aacute;&rcaron;</trd>
  </trdgrp>
  <trd lng="de">Flicker</trd>
  <trdgrp lng="es">
    <trd>remendador</trd>,
    <trd>parcheador</trd>
  </trdgrp>
  <trd lng="fr">rapi&eacute;ceur</trd>
  <trd lng="hu">foltoz&oacute;</trd>
  <trdgrp lng="id">
    <trd><baz>tambal</baz>penambal</trd>,
    <trd><baz>tampal</baz>penampal</trd>,
    <trd>tukang tambal</trd>,
    <trd>tukang tampal</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7E55;&#12356;&#32887;&#x4EBA;  <pr>&#12388;&#x304F;&#x308D;&#12356;&#12375;&#12423;&#x304F;&#x306B;&#12435;</pr></trd>,
    <trd>&#x639B;&#12369;&#x306F;&#x304E;&#x5C4B;  <pr>&#x304B;&#12369;&#x306F;&#x304E;&#12420;</pr></trd>
  </trdgrp>
  <trd lng="nl">oplapper</trd>
  <trd lng="sk">z&aacute;plat&aacute;r</trd>
</drv>

<drv mrk="flik.0acxi">
  <kap><tld/>a&ccirc;i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senzorge, mallerte <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_p;&c_ib;&c_c;&c_mol; <klr>(&c_a;&c_b;&c_y; &c_ja;&c_k;, &c_n;&c_a; &c_s;&c_k;&c_o;&c_r;&c_u;&c_ju; &c_r;&c_u;&c_k;&c_u;)</klr></trd>
  <trdgrp lng="de">
  <trd>zusammenmurksen</trd>,
  <trd>pfuschen</trd>
  </trdgrp>
  <trd lng="es">remendar</trd>
  <trd lng="fr">rapetasser</trd>
  <trd lng="hu">&ouml;sszetoldoz</trd>
  <trdgrp lng="id">
    <trd><baz>tambal</baz>menambal asal-asalan</trd>,
    <trd><baz>tampal</baz>menampal asal-asalan</trd>
  </trdgrp>
  <trd lng="ja">&#x4E0D;&#x7D30;&#x5DE5;&#x306B;&#12388;&#x304E;&#12434;&#x5F53;&#12390;&#x308B;
  <pr>&#12406;&#12373;&#12356;&#x304F;&#x306B;&#12388;&#x304E;&#12434;&#12354;&#12390;&#x308B;</pr></trd>
</drv>
</art>
