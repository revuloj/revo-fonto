<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>flik</rad>/i
</kap>
<drv mrk="flik.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Ripari almetante pecon por kovri truon (precipe pri vestoj):
      <ekz><tld/>i &scirc;iritan robon, difektitan &scirc;uon;</ekz>
      <ekz><tld/>i truon en tegmento<fnt>Z</fnt>;</ekz>
      <ekz>multpece <tld/>ita pantalono;</ekz>
      <ekz>surkudri pecon por <tld/>i mantelon.</ekz>
    </dif>
    <trd lng="de">flicken</trd>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fu&scirc;e, mallerte ripari ion:
      <ekz><tld/>i rompitan amikecon;</ekz>
      <ekz><tld/>i la karton de E&ubreve;ropo, la pacon.</ekz>
    </dif>
    <trd lng="de">zusammenflicken</trd>
  </snc>
  <trd lng="en">patch</trd>
  <trd lng="fr">rapi&eacute;cer</trd>
  <trd lng="nl">oplappen</trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_t;&c_o;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_z;&c_a;&c_p;&c_l;&c_a;&c_t;&c_u;</trd>
  </trdgrp>
</drv>
<drv mrk="flik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Surkudrita (a&ubreve; surmetita) peco kaj riparita parto. 
    </dif>
  </snc>
  <trd lng="de">Flicken</trd>
  <trdgrp lng="fr">
    <trd>pi&egrave;ce<klr tip="ind"> (pour r&eacute;parer un trou)</klr></trd>,
    <trd>rustine</trd></trdgrp> 
  <trd lng="nl">oplapping</trd>
  <trd lng="ru">&c_z;&c_a;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
</drv>
<drv mrk="flik.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu metie <tld/>as. 
    </dif>
  </snc>
  <trd lng="fr">rapi&eacute;ceur</trd>
  <trd lng="nl">oplapper</trd>
</drv>
<drv mrk="flik.0acxi">
  <kap><tld/>a&ccirc;i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Senzorge, mallerte <tld/>i.  
    </dif>
  </snc>
  <trd lng="de">zusammenmurksen</trd>
  <trd lng="fr">rapetasser</trd>
</drv>
</art>
