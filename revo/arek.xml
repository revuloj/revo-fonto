<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arek.xml,v 1.13 2023/10/28 08:33:08 revo Exp $">
<kap>
  <rad>arek</rad>/o
</kap>

<drv mrk="arek.0o">
  <kap><tld/>o</kap>
  <snc mrk="arek.0o.arbo">
    <fnt><bib>PIV1</bib></fnt>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> de 
      <ref tip="lst" lst="voko:arekoparencoj" cel="palm.0oj">palmoj</ref>
      el Sudorientazio (<trd lng="la">Areca</trd>),
      el familio 
      <ref tip="super" cel="arekac.0oj">arekacoj</ref>; 
      unu specio (<trd lng="la">Areca catechu</trd>) 
      produktas man&gcirc;eblajn nuksojn.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_r;&c_e;&c_k;&c_a;</trd>,
      <trd>&c_a;&c_r;&c_e;&c_k;&c_a;&c_v;&c_a;&c_ja; &c_p;&c_a;&c_l;&c_mol;&c_m;&c_a;</trd>
    </trdgrp>
    <trd lng="ca">areca</trd>
    <trdgrp lng="de">
      <trd>Betelpalme</trd>,
      <trd>Arekapalme</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ar&eacute;quier</trd>,
      <trd>arec</trd>
    </trdgrp>
    <trd lng="pl">areka</trd>
    <trdgrp lng="zh">
    <trd>&#x69DF;&#x6994; <pr>b&#x12B;nglang</pr></trd>,
    <trd>&#x6AB3;&#x6994; <pr>b&#x12B;nglang</pr></trd>
  </trdgrp>
</snc>
  
  <snc mrk="arek.0o.nukso">
    <dif>
      <ref tip="dif" cel="arek.0nukso"><tld lit="A"/>nukso</ref>:
      <ekz>
        mi forgesis kunporti <tld/>an saketon, franjo, &ccirc;u vi havas
        <tld/>ojn
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 64a, p.
        397a</lok></fnt>?
      </ekz>
      <ekz>
        betelo grimpanta &ccirc;irka&ubreve; la trunko de <tld/>o
        <fnt>
          <vrk><url ref="http://e.vea.vn/view/1629_historiodebetelokajareknukso.htm">Historio
          de Betelo kaj areknukso</url></vrk>
        </fnt>.
      </ekz> 
    </dif>
    <trd lng="be">&c_a;&c_r;&c_e;&c_k;&c_a;&c_v;&c_y; &c_a;&c_r;&c_e;&c_h;</trd>
    <trd lng="ca">nou d'areca</trd>
    <trdgrp lng="de">
      <trd>Betelnuss</trd>,
      <trd>Arekanuss</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>noix d'arec</trd>,
      <trd>noix de b&eacute;tel</trd>
    </trdgrp>
    <trd lng="pl">orzech arekowy</trd>
    <trd lng="zh">&#x68B9; <pr>b&#x12B;ng</pr></trd>
</snc>
  <trdgrp lng="cs">
    <trd>areka</trd>,
    <trd>palma arekov&aacute;</trd>
  </trdgrp>
  <trd lng="sk">palma arekov&aacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_r;&c_je;&#x43A;&c_a;</trd>,
    <trd>&c_a;&c_r;&c_je;&#x43A;&#x43E;&c_v;&c_a; &#x43F;&c_a;&#x43B;&#x44C;&#x43C;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="arek.0nukso">
  <kap><tld/>nukso</kap>
  <snc mrk="arek.0nukso.BOT">
    <dif>
      <ref tip="super" cel="kern.0o.BOT">Kerno</ref> de la frukto de
      <tld/>arbo kun man&gcirc;ebla grajno:
      <ekz>
        kutimo ma&ccirc;i betelon kaj <tld/>nukson
        <fnt>
          <vrk><url ref="http://e.vea.vn/view/1629_historiodebetelokajareknukso.htm">Historio
          de Betelo kaj areknukso</url></vrk>
        </fnt>
      </ekz>
    </dif>
    <trd lng="zh">&#x68B9; <pr>b&#x12B;ng</pr></trd>
</snc>
  <trd lng="be">&c_a;&c_r;&c_e;&c_k;&c_a;&c_v;&c_y; &c_a;&c_r;&c_e;&c_h;</trd>
  <trd lng="ca">nou d'areca</trd>
  <trdgrp lng="de">
    <trd>Betelnuss</trd>,
    <trd>Arekanuss</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>noix d'arec</trd>,
    <trd>noix de b&eacute;tel</trd>
  </trdgrp>
   <trd lng="uk">&c_a;&c_r;&c_je;&#x43A;&#x43E;&c_v;&c_i;&c_j; &c_g;&#x43E;&c_r;&c_ib;&c_h;</trd>
</drv>
</art>

