<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arek.xml,v 1.11 2022/03/12 11:35:08 revo Exp $">
<kap>
  <rad>arek</rad>/o
</kap>

<drv mrk="arek.0o">
  <kap><tld/>o</kap>
  <snc mrk="arek.0o.arbo">
    <fnt><bib>PIV1</bib></fnt>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> de 
      <ref tip="lst" lst="voko:arekoparencoj" cel="palm.0oj">palmoj</ref>
      el Sudorientazio (<trd lng="la">Areca</trd>),
      el familio 
      <ref tip="super" cel="arekac.0oj">arekacoj</ref>; 
      unu specio (<trd lng="la">Areca catechu</trd>) 
      produktas man&gcirc;eblajn nuksojn.
    </dif>
    <trd lng="ca">areca</trd>
    <trdgrp lng="de">
      <trd>Betelpalme</trd>,
      <trd>Arekapalme</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>ar&eacute;quier</trd>,
      <trd>arec</trd>
    </trdgrp>
    <trd lng="pl">areka</trd>
  </snc>
  
  <snc mrk="arek.0o.nukso">
    <dif>
      <ref tip="dif" cel="arek.0nukso"><tld lit="A"/>nukso</ref>:
      <ekz>
        mi forgesis kunporti <tld/>an saketon, franjo, &ccirc;u vi havas
        <tld/>ojn
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 64a, p.
        397a</lok></fnt>?
      </ekz>
      <ekz>
        betelo grimpanta &ccirc;irka&ubreve; la trunko de <tld/>o
        <fnt>
          <vrk><url ref="http://e.vea.vn/view/1629_historiodebetelokajareknukso.htm">Historio
          de Betelo kaj areknukso</url></vrk>
        </fnt>.
      </ekz> 
    </dif>
    <trd lng="ca">nou d'areca</trd>
    <trdgrp lng="de">
      <trd>Betelnuss</trd>,
      <trd>Arekanuss</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>noix d'arec</trd>,
      <trd>noix de b&eacute;tel</trd>
    </trdgrp>
    <trd lng="pl">orzech arekowy</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>areka</trd>,
    <trd>palma arekov&aacute;</trd>
  </trdgrp>
  <trd lng="sk">palma arekov&aacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x430;&#x440;&#x435;&#x43A;&#x430;</trd>,
    <trd>&#x430;&#x440;&#x435;&#x43A;&#x43E;&#x432;&#x430; &#x43F;&#x430;&#x43B;&#x44C;&#x43C;&#x430;</trd>
  </trdgrp>
</drv>

<drv mrk="arek.0nukso">
  <kap><tld/>nukso</kap>
  <snc mrk="arek.0nukso.BOT">
    <dif>
      <ref tip="super" cel="kern.0o.BOT">Kerno</ref> de la frukto de
      <tld/>arbo kun man&gcirc;ebla grajno:
      <ekz>
        kutimo ma&ccirc;i betelon kaj <tld/>nukson
        <fnt>
          <vrk><url ref="http://e.vea.vn/view/1629_historiodebetelokajareknukso.htm">Historio
          de Betelo kaj areknukso</url></vrk>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">nou d'areca</trd>
  <trdgrp lng="de">
    <trd>Betelnuss</trd>,
    <trd>Arekanuss</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>noix d'arec</trd>,
    <trd>noix de b&eacute;tel</trd>
  </trdgrp>
   <trd lng="uk">&#x430;&#x440;&#x435;&#x43A;&#x43E;&#x432;&#x438;&#x439; &#x433;&#x43E;&#x440;&#x456;&#x445;</trd>
</drv>
</art>

