<art mrk="$Id: ap.xml,v 1.6 2020/03/21 15:10:24 revo Exp $">
<kap>
  <rad>ap</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="ap.0o">
  <kap><tld/>o</kap>
  <snc mrk="ap.0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Mallongigo por <ref tip="dif" cel="aplik.0ajxo.KOMP">aplika&jcirc;o</ref>, precipe po&scirc;telefona:
      <ekz>
        ni programas telefon-aplika&jcirc;on (<tld/>on) por
        <nom>iOS</nom> kaj <nom>Android</nom>,
        kiu nomi&gcirc;as Amikumu
        <fnt>
          <aut>R. Delamore</aut>
          <vrk>en:<url ref="https://www.liberafolio.org/2016/10/18/amikumu-helpos-trovi-proksimajn-esperantistojn/">
          Amikumu helpos trovi proksimajn esperantistojn</url></vrk>
          <lok>Libera Folio, 2016-10-18</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="cs">
      <trd>aplikace</trd>,
      <trd>apka</trd>
    </trdgrp>
  </snc>
  <trd lng="en">app</trd>
  <trd lng="de">App</trd>
  <trd lng="fr">appli<klr> (informatique)</klr></trd>
  <trd lng="fy">app</trd>
  <trd lng="nl">app</trd>
  <trd lng="no">app</trd>
  <trd lng="ms">aplikasi</trd>
</drv>
</art>

