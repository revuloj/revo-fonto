<drv mrk="mol.0o">
  <kap>
    <tld/>o, <var><kap><tld/>eco</kap></var>
  </kap>
  <snc mrk="mol.0o.fizika">
    <dif>
      Cedemo al premo:
      <ekz>
        mi sentis ilian tu&scirc;on, la glacian malsekecon de iliaj korpoj, abomenan
        <tld/>econ de iliaj haroj
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
      <ekz>
        ekfalo en la se&gcirc;an <tld/>on, malstre&ccirc;igo, varmo, silento, agrablo
        <fnt>
          <bib>Monato</bib>
          <aut>Bernhard Pabst</aut>
          <vrk><url ref="https://www.monato.be/2008/009376.php"
              >Inter libroj posttagmeze</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Weichheit</trd>,
      <trd>Nachgiebigkeit</trd>
    </trdgrp>
  </snc>
  <snc mrk="mol.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Milda, malsevera konduto a&ubreve; impreso:
      <ekz>
        tiu &ccirc;i <tld/>eco de la sentoj, kiu lin &ccirc;e &ccirc;iu sufero fandigas en plorantan
        simpation
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;i <tld/>eco de la trajtoj fari&gcirc;is indigno, tiu &ccirc;i rideto fari&gcirc;is malespero
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        tono, en kiu abrupte alternis tirata <tld/>eco kaj ra&ubreve;ka mal<tld/>eco, kiel oni
        tion trovas &ccirc;e ftizuloj
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili manifestis raran <tld/>on kaj bonkorecon volonte absolvante murdistojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2004, &#8470; 4 (114)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Milde</trd>,
      <trd>Sanftheit</trd>
    </trdgrp>
  </snc>
  <ref tip="hom" cel="mol1.0o">molo (kvanto)</ref>
</drv>









<drv mrk="mol.0acxa">
  <kap>
    <tld/>a&ccirc;a
  </kap>
  <snc mrk="mol.0acxa.KOMUNE">
    <dif>
      Kontra&ubreve;nature, malagrable <tld/>a:
      <ekz>
        karno <tld/>a&ccirc;a sen elasteco
        <fnt>
          <aut>Okada H., trad. Konisi G.</aut>
          <vrk>Notoj pri la Delto</vrk>
          <lok>1992</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a&ccirc;aj manoj, kurioze gantitaj
        <fnt>
          <bib>Sol</bib>
        </fnt>;
      </ekz>
      <ekz>
        la tero estis <tld/>a&ccirc;a kaj glita pro falintaj folioj
        <fnt>
          <bib>StU</bib>
          <lok>Britujo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>matschig</trd>,
    <trd>breiig</trd>,
    <trd>schwammig</trd>,
    <trd>weichlich</trd>
  </trdgrp>
  <trd lng="fr">mollasson</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_l;&c_ja;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_v;&#8217;&c_ja;&c_l;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="mol.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -a&jcirc;'</lok></fnt></kap>
  <snc mrk="mol.0ajxo.KOMUNE">
    <dif>
      Io <tld/>a a&ubreve; <tld/>a parto de io:
      <ekz>
        la <ind><tld/>a&jcirc;o de pano</ind>
        <fnt>
          <aut>Kazimierz Bein (Kabe)</aut>
          <vrk><url ref="https://dlibra.kul.pl/dlibra/publication/51791/edition/46340"
              >Vortaro de Esperanto, 1911</url></vrk>
        </fnt>;
        <trd lng="de">Krume</trd>
        <trd lng="ru">&c_m;&c_ja;&c_k;&c_i;&c_sh;</trd>       
      </ekz>
      <ekz>
        lanug&#8217;-<tld/>a&jcirc;o
        <fnt>
          <bib>SrT</bib>
          <lok>Libro XI.</lok>
        </fnt>;
      </ekz>
      <ekz>
        kapo, premegita de pezega rokpeco <klr>[&#8230;]</klr> aliformigita en duberu&gcirc;an
        <tld/>a&jcirc;on
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        fali en <tld/>a&jcirc;on de fotelo <klr>[&#8230;]</klr> forgesante &ccirc;ion
        <fnt>
          <bib>KrM</bib>
          <lok>24. Per Flugiloj De Facila Vento.</lok>
        </fnt>;
      </ekz>
      <ekz>
        maldensa vaporo ku&scirc;as kvaza&ubreve; ia <tld/>a&jcirc;o sur la suna akvovasto
        <fnt>
          <bib>ViV</bib>
          <lok>IV.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a&jcirc;o de frukto</ind>
        <klr>(vd <ref tip="sin" cel="karn.0o.BOT">karno<sncref/></ref>)</klr>
        <fnt><bib>PV</bib></fnt>.
        <trd lng="de">Fruchtfleisch</trd>
        <trd lng="en">pulp</trd>
        <trd lng="pl">mi&aogonek;&zdot;sz</trd>
        <trd lng="ru">&c_m;&c_ja;&c_k;&c_o;&c_t;&c_mol;</trd>       
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>Wabbel</trd>,
     <trd>Labber</trd>
  </trdgrp>
  <trd lng="fr">mou<klr tip="amb"> (subst., partie molle)</klr></trd>
  <trdgrp lng="pl">
    <trd>mi&aogonek;&zdot;sz</trd>,
    <trd>mi&eogonek;kisz</trd>,
    <trd>s&lstroke;abizna</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_m;&c_ja;&c_g;&c_k;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_m;&c_ja;&c_k;&c_o;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&#8217;&c_ja;&c_k;&c_u;&c_sh;</trd>,
    <trd>&c_m;&#8217;&c_ja;&c_k;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_m;&#8217;&c_ja;&c_k;&c_u;&c_sh;&c_k;&c_a; (&c_h;&c_l;&c_ib;&c_b;&c_a;)</trd>
  </trdgrp>
</drv>

<drv mrk="mol.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <dif>
    Igi <tld/>a:
  </dif>
  <snc ref="mol.0a.cedema" mrk="mol.0igi.KOMUNE">
    <ekz>
      vakso <tld/>igita per fajro
      <fnt>
        <bib>Marta</bib>
      </fnt>;
    </ekz>
    <ekz>
      ulceroj <klr>[&#8230;]</klr> <tld/>igitaj per oleo
      <fnt>
        <bib>MT</bib>
        <lok>Jesaja 1:6</lok>
      </fnt>;
    </ekz>
    <ekz>
      li pli volonte uzas la ardantan ka&ubreve;terizon ol la <tld/>igan &scirc;mira&jcirc;on
      <fnt>
        <bib>DKM</bib>
        <lok>&Ccirc;apitro LXV</lok>
      </fnt>;
    </ekz>
  </snc>
  <snc ref="mol.0a.milda" mrk="mol.0igi.mildigi">
    <uzo tip="stl">FIG</uzo>
    <ekz>
      la pezeco kaj la severeco de la konstrua&jcirc;o estis <tld/>igita per multaj subtilaj
      ornama&jcirc;oj
      <fnt>
        <bib>I&scirc;tar</bib>
        <lok>&Ccirc;apitro V</lok>
      </fnt>;
    </ekz>
    <ekz>
      vi lin volu per vortoj delikataj <tld/>igi
      <fnt>
        <bib>FK</bib>
        <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
      </fnt>;
    </ekz>
    <ekz>
      Kristo <tld/>igadis korojn ankora&ubreve; pli malmolajn
      <fnt>
        <bib>QuV</bib>
        <lok>&Ccirc;apitro XXVII</lok>
      </fnt>;
    </ekz>
    <ekz>
      blua duonlumo <tld/>igas ru&gcirc;on sur la rozkoloraj vangoj
      <fnt>
        <bib>Metrop</bib>
      </fnt>;
    </ekz>
    <ekz>
      blueta vualo de la nebuleca aero <tld/>igis konturojn de la novaj ok- a&ubreve;
      deketa&gcirc;aj konstrua&jcirc;egoj
      <fnt>
        <bib>Metrop</bib>
      </fnt>;
    </ekz>
    <ekz>
      subite <tld/>iginte la tonon, li demandis <klr>[&#8230;]</klr>
      <fnt>
        <bib>Metrop</bib>
      </fnt>.
    </ekz>
  </snc>
  <snc ref="mol.0a.FON" mrk="mol.0igi.FON">
    <uzo tip="fak">FON</uzo>
    <ekz>
      la sono &leftquot;j&rightquot; ordinare <tld/>igas la konsonanton, kiu
      staras anta&ubreve; &gcirc;i
      <fnt>
        <bib>LR</bib>
        <lok>Dua Parto &ndash; Gramatiko</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_m;&c_ja;&c_k;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
    <trd>boukaat</trd>,
    <trd>gwakaat</trd>
  </trdgrp>
