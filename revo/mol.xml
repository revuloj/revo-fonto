<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>mol</rad>/a
</kap>
<drv mrk="mol.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="mol.0a.cedema">
    <dif>
      Ne kontra&ubreve;staranta al premo, facile cedanta:
      <ekz>
        <tld/>a kiel pasto;
      </ekz>
      <ekz>
        <tld/>a karno, pano, argilo, lano, kuseno, kanapeto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        karaktero <tld/>a kiel vakso<fnt>Z</fnt>
        (<ref tip="sin" cel="ole.0a">olea</ref>);
      </ekz>
      <ekz>
        &scirc;vel<tld/>a tapi&scirc;o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fleks.0ebla">fleksebla</ref>,
      <ref cel="kned">knedebla</ref>.
    </refgrp>
  </snc>
  <snc mrk="mol.0a.milda">
    <dif>
      Ne akra, milda:
      <ekz>
        <tld/>a karaktero, vo&#x109;o;
      </ekz>
      <ekz>
        <tld/>aj (neprecizaj) konturoj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="mol.0a.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <klr>(Parolsono)</klr> karakterizata per levo de la langodorso
      al la palato; <ref tip="sin" cel="palat.0a">palata</ref>.
    </dif>
  </snc>
  <trd lng="cs">m&ecaron;kk&yacute;</trd>
  <trd lng="de">weich</trd>
  <trd lng="en">soft</trd>
  <trd lng="es">suave</trd>
  <trd lng="nl">zacht</trd>
  <trd lng="pl">mi&eogonek;kky</trd>
  <trd lng="ru">&c_m;&c_ja;&c_g;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="mol.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io <tld/>a a&ubreve; <tld/>a parto de io:
      <ekz>
        la <tld/>a&jcirc;o de pano<fnt>B</fnt>,
      </ekz>
      <ekz>
        de frukto.
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_m;&c_ja;&c_k;&c_o;&c_t;&c_mol;</trd>
</drv>

<drv mrk="mol.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi vakson per varmo;
      </ekz>
      <ekz>
        <tld/>igi (kompatigi, mildigi) la koron de iu;
      </ekz>
      <ekz>
        lia koro <tld/>i&gcirc;is<fnt>Z</fnt>;
      </ekz><ekz>
        <uzo tip="fak">FON</uzo>
        la sono j ordinare <tld/>igas la konsonanton, kiu
        staras anta&#x16D; &#x11D;i
        <fnt><bib>LR</bib>, <lok></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_ja;&c_g;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_ja;&c_g;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="mol.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Forte kontra&ubreve;staranta al premo, ne facile cedanta:
      <ekz>
        mal<tld/>a viando, pano, ligno, &scirc;tono;
      </ekz>
      <ekz>
        mal<tld/>e kuirita ovo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mal<tld/>a koro;
      </ekz>
      <ekz><uzo tip="stl">FIG</uzo>
        neceseco kontra&ubreve;vola estas le&gcirc;o mal<tld/>a
<fnt><bib>PrV</bib></fnt></ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bronz.0a">bronza</ref>,
      <ref cel="fer.0a">fera</ref>,
      <ref cel="sxton.0a">&scirc;tona</ref>,
      <ref cel="rigid.0a">rigida</ref>,
      <ref cel="sever.0a">severa</ref>,
      <ref cel="kruel.0a">kruela</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">hard</trd>
  <trd lng="pl">twardy</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_v;&c_jo;&c_r;&c_d;&c_y;&c_j;</trd>,
    <trd>&c_zh;&c_jo;&c_s;&c_t;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="mol.0matura">
  <kap><tld/>matura</kap>
  <uzo tip="klr">(pp frukto)</uzo>
  <snc>
    <dif>
      <tld lit="M"/>i&gcirc;inta kaj duone putri&gcirc;anta pro tromatureco.
    </dif>
  </snc>
  <trd lng="nl">overrijp</trd>
</drv>

<drv mrk="mol.0anaso">
  <kap><ofc>*</ofc><tld/>anaso</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="anased.0oj">anasedoj</ref>
      (<trd lng="la">Somateria</trd>), vivante &ccirc;e nordaj
      maroj kaj kies helbrunan, <tld/>an lanugon oni uzas por
      kusenoj.
    </dif>
  </snc>
  <trd lng="cs">kajka</trd>
  <trd lng="de">Eiderente</trd>
  <trd lng="en">Eider-duck</trd>
  <trd lng="hu">pehelyr&eacute;ce</trd>
  <trd lng="nl">eidereend</trd>
  <trd lng="ru">&c_g;&c_a;&c_g;&c_a;</trd>
</drv>

</art>
