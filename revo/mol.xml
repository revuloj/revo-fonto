<drv mrk="mol.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o
  <fnt><bib>F</bib>, <lok>Universala Vortaro, -a&jcirc;'</lok></fnt></kap>
  <snc mrk="mol.0ajxo.KOMUNE">
    <dif>
      Io <tld/>a a&ubreve; <tld/>a parto de io:
      <ekz>
        la <ind><tld/>a&jcirc;o de pano</ind>
        <fnt>
          <aut>Kazimierz Bein (Kabe)</aut>
          <vrk><url ref="https://dlibra.kul.pl/dlibra/publication/51791/edition/46340">Vortaro de
          Esperanto, 1911</url></vrk>
        </fnt>;
        <trd lng="de">Krume</trd>
        <trd lng="ru">&c_m;&c_ja;&c_k;&c_i;&c_sh;</trd>
        <trd lng="id"><ind>remah</ind>-remah roti</trd>
      </ekz>
      <ekz>
        lanug&#8217;-<tld/>a&jcirc;o
        <fnt>
          <bib>SrT</bib>
          <lok>Libro XI.</lok>
        </fnt>;
      </ekz>
      <ekz>
        kapo, premegita de pezega rokpeco <klr>[&#8230;]</klr> aliformigita en duberu&gcirc;an
        <tld/>a&jcirc;on
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        fali en <tld/>a&jcirc;on de fotelo <klr>[&#8230;]</klr> forgesante &ccirc;ion
        <fnt>
          <bib>KrM</bib>
          <lok>24. Per Flugiloj De Facila Vento.</lok>
        </fnt>;
      </ekz>
      <ekz>
        maldensa vaporo ku&scirc;as kvaza&ubreve; ia <tld/>a&jcirc;o sur la suna akvovasto
        <fnt>
          <bib>ViV</bib>
          <lok>IV.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>a&jcirc;o de frukto</ind>
        <klr>(vd <ref tip="sin" cel="karn.0o.BOT">karno<sncref/></ref>)</klr>
        <fnt><bib>PV</bib></fnt>.
        <trd lng="de">Fruchtfleisch</trd>
        <trd lng="en">pulp</trd>
        <trd lng="id"><ind>daging</ind> buah</trd>
        <trd lng="pl">mi&aogonek;&zdot;sz</trd>
        <trd lng="ru">&c_m;&c_ja;&c_k;&c_o;&c_t;&c_mol;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>Wabbel</trd>,
     <trd>Labber</trd>
  </trdgrp>
  <trd lng="fr">mou<klr tip="amb"> (subst., partie molle)</klr></trd>
  <trd lng="ja">&#12420;&#x308F;&#12425;&#x304B;&#12356;&#12418;&#x306E;</trd>
  <trdgrp lng="pl">
    <trd>mi&aogonek;&zdot;sz</trd>,
    <trd>mi&eogonek;kisz</trd>,
    <trd>s&lstroke;abizna</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_m;&c_ja;&c_g;&c_k;&c_a;&c_ja; &c_ch;&c_a;&c_s;&c_t;&c_mol;</trd>,
     <trd>&c_m;&c_ja;&c_k;&c_o;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&#8217;&c_ja;&c_k;&c_u;&c_sh;</trd>,
    <trd>&c_m;&#8217;&c_ja;&c_k;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_m;&#8217;&c_ja;&c_k;&c_u;&c_sh;&c_k;&c_a; (&c_h;&c_l;&c_ib;&c_b;&c_a;)</trd>
  </trdgrp>
</drv>

<drv mrk="mol.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <dif>
    Igi <tld/>a:
  </dif>
  <snc ref="mol.0a.cedema" mrk="mol.0igi.KOMUNE">
    <ekz>
      vakso <tld/>igita per fajro
      <fnt>
        <bib>Marta</bib>
      </fnt>;
    </ekz>
    <ekz>
      ulceroj <klr>[&#8230;]</klr> <tld/>igitaj per oleo
      <fnt>
        <bib>MT</bib>
        <lok>Jesaja 1:6</lok>
      </fnt>;
    </ekz>
    <ekz>
      li pli volonte uzas la ardantan ka&ubreve;terizon ol la <tld/>igan &scirc;mira&jcirc;on
      <fnt>
        <bib>DKM</bib>
        <lok>&Ccirc;apitro LXV</lok>
      </fnt>;
    </ekz>
  <trdgrp lng="id">
    <trd><baz>empuk</baz>mengempukkan</trd>,
    <trd><baz>halus</baz>menghaluskan</trd>,
    <trd><baz>lembut</baz>melembutkan</trd>,
    <trd><baz>lunak</baz>melunakkan</trd>
  </trdgrp>
  </snc>
  <snc ref="mol.0a.milda" mrk="mol.0igi.mildigi">
    <uzo tip="stl">FIG</uzo>
    <ekz>
      la pezeco kaj la severeco de la konstrua&jcirc;o estis <tld/>igita per multaj subtilaj
      ornama&jcirc;oj
      <fnt>
        <bib>I&scirc;tar</bib>
        <lok>&Ccirc;apitro V</lok>
      </fnt>;
    </ekz>
    <ekz>
      vi lin volu per vortoj delikataj <tld/>igi
      <fnt>
        <bib>FK</bib>
        <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
      </fnt>;
    </ekz>
    <ekz>
      Kristo <tld/>igadis korojn ankora&ubreve; pli malmolajn
      <fnt>
        <bib>QuV</bib>
        <lok>&Ccirc;apitro XXVII</lok>
      </fnt>;
    </ekz>
    <ekz>
      blua duonlumo <tld/>igas ru&gcirc;on sur la rozkoloraj vangoj
      <fnt>
        <bib>Metrop</bib>
      </fnt>;
    </ekz>
    <ekz>
      blueta vualo de la nebuleca aero <tld/>igis konturojn de la novaj ok- a&ubreve;
      deketa&gcirc;aj konstrua&jcirc;egoj
      <fnt>
        <bib>Metrop</bib>
      </fnt>;
    </ekz>
    <ekz>
      subite <tld/>iginte la tonon, li demandis <klr>[&#8230;]</klr>
      <fnt>
        <bib>Metrop</bib>
      </fnt>.
    </ekz>
  <trdgrp lng="id">
    <trd><baz>halus</baz>menghaluskan<klr> (sikap, suara, dsb.)</klr></trd>,
    <trd><baz>lembut</baz>melembutkan<klr> (sikap, suara, dsb.)</klr></trd>,
    <trd><baz>lemah</baz>melemahlembutkan</trd>
  </trdgrp>
  </snc>
  <snc ref="mol.0a.FON" mrk="mol.0igi.FON">
    <uzo tip="fak">FON</uzo>
    <ekz>
      la sono &leftquot;j&rightquot; ordinare <tld/>igas la konsonanton, kiu
      staras anta&ubreve; &gcirc;i
      <fnt>
        <bib>LR</bib>
        <lok>Dua Parto &ndash; Gramatiko</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_m;&c_ja;&c_k;&c_ch;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
    <trd>boukaat</trd>,
    <trd>gwakaat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&ecaron;k&ccaron;it</trd>,
    <trd>obm&ecaron;k&ccaron;ovat</trd>
  </trdgrp>
  <trd lng="es">ablandar</trd>
  <trd lng="fa">&a_n;&a_r;&a_m; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">amollir</trd>
  <trdgrp lng="ja">
    <trd>&#12420;&#x308F;&#12425;&#x304B;&#x306B;&#12377;&#x308B;</trd>,
    <trd>&#12420;&#x308F;&#12425;&#12370;&#x308B;</trd>,
    <trd>&#x8EDF;&#21270;&#12373;&#x305B;&#x308B;  <pr>&#x306A;&#12435;&#x304B;&#12373;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">zmi&eogonek;kcza&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_m;&c_ja;&c_g;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_ja;&c_g;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obm&auml;k&ccaron;i&tcaron;</trd>,
    <trd>zm&auml;k&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&#8217;&c_ja;&c_k;&c_sh;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_m;&#8217;&c_ja;&c_k;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_o;&c_m;&#8217;&c_ja;&c_k;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_m;&#8217;&c_ja;&c_k;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="mol.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <dif>
    I&gcirc;i <tld/>a:
  </dif>
  <snc ref="mol.0a.cedema" mrk="mol.0igxi.KOMUNE">
    <ekz>
      de la figarbo <klr>[&#8230;]</klr> bran&ccirc;o jam <tld/>i&gcirc;as kaj aperigas foliojn,
      <klr>[&#8230;]</klr> la somero estas proksima
      <fnt>
        <bib>NT</bib>
        <lok>Marko 13:28</lok>
      </fnt>;
    </ekz>
    <ekz>
      la pordisto sentis siajn genuojn <tld/>i&gcirc;i
      <fnt>
        <bib>MkM</bib>
        <lok>5</lok>
      </fnt>;
    </ekz>
    <ekz>
      <nom>Pluriven</nom> sentis &ccirc;iujn muskolojn <tld/>i&gcirc;i, estis preska&ubreve; agrable
      <fnt>
        <bib>ChR</bib>
        <lok>ABZ de murdo</lok>
      </fnt>.
    </ekz>
  <trdgrp lng="id">
    <trd><baz>empuk</baz>mengempuk</trd>,
    <trd><baz>halus</baz>menghalus</trd>,
    <trd><baz>lembut</baz>melembut</trd>,
    <trd><baz>lunak</baz>melunak</trd>
  </trdgrp>
  </snc>
  <snc ref="mol.0a.milda" mrk="mol.0igxi.FIG">
    <ekz>
      via koro moli&gcirc;is kaj vi humili&gcirc;is anta&ubreve; la Eternulo
      <fnt>
        <bib>MT</bib>
        <lok>II. Re&gcirc;oj 22:19</lok>
      </fnt>;
    </ekz>
    <ekz>
      <nom>Sara</nom> <klr>[&#8230;]</klr> rigardis tiel plende, ke la kronprinco <tld/>i&gcirc;is kaj
      karesis &scirc;in
      <fnt>
        <bib>Far1</bib>
        <lok>&Ccirc;apitro XVII</lok>
      </fnt>;
    </ekz>
    <ekz>
      la esploranta, penetra rigardo <tld/>i&gcirc;is la&ubreve; la varmi&gcirc;o de la printempo
      <fnt>
        <bib>HsT</bib>
        <lok>Heredanto</lok>
      </fnt>;
    </ekz>
    <ekz>
      la nokta krepusko ankora&ubreve; ne venis, sed koloroj kaj konturoj komencis pali&gcirc;i kaj
      <tld/>i&gcirc;i
      <fnt>
        <bib>InfanTorent2</bib>
        <lok>Dua Parto</lok>
      </fnt>.
    </ekz>
  <trd lng="id"><baz>lemah</baz>melemah lembut</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_m;&c_ja;&c_k;&c_ch;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_m;&c_ja;&c_k;&c_ch;&c_e;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>amollir</ind></trd>,
    <trd>se <ind>ramollir</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12420;&#x308F;&#12425;&#x304B;&#x306B;&#x306A;&#x308B;</trd>,
    <trd>&#12420;&#x308F;&#12425;&#12368;</trd>,
    <trd>&#x8EDF;&#21270;&#12377;&#x308B;  <pr>&#x306A;&#12435;&#x304B;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">mi&eogonek;kn&aogonek;&cacute;</trd>
</drv>

<drv mrk="mol.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="mol.mal0a.fizike">
    <dif>
      Kiu senti&gcirc;as &ccirc;e palpado kiel kompakta, <ref cel="ced.mal0ema">malcedema</ref>,
      <ref cel="star.kontraux0i">kontra&ubreve;staranta</ref> al premo:
      <ekz>
        tiu &ccirc;i malfre&scirc;a pano estas mal<tld/>a, kiel &scirc;tono
        <fnt>
          <bib>F</bib>
          <lok>&FE; 33</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi konas vian malobeemecon kaj vian mal<tld/>an nukon
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 31:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        li endormi&gcirc;is, tenante la kapon sur la mal<tld/>a rando de la lito
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ku&scirc;is sur io mal<tld/>a, kaj mia korpo pro tio fari&gcirc;is blua kaj bruna
        <fnt>
          <bib>Fab1</bib>
          <lok>Re&gcirc;idino sur pizo</lok>
        </fnt>!
      </ekz>
      <ekz>
        spiconuksoj estis <klr>[&#8230;]</klr> tro mal<tld/>aj por li
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ovoj, ju pli longe ili estas kuirataj, fari&gcirc;as des pli mal<tld/>aj
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        viandon li donas al mi tiel mal<tld/>an kiel trabo
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[en vintro]</klr> la tero estas mal<tld/>a kaj kovrita de ne&gcirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la mal<tld/>a argila planko de la stalo
        <fnt>
          <bib>IdO</bib>
          <lok>Kvina Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="mol.0a.cedema">mola <sncref/></ref>,
      <ref cel="fleks.0ebla">fleksebla</ref>,
      <ref cel="kned">knedebla</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="firm.0a.malmola">firma<sncref/></ref>,
      <ref cel="rigid.0a.neflekseble">rigida<sncref/></ref>,
      <ref cel="sxton.0a.FIG_fortika">&scirc;tona<sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="mol.mal0a.morale">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="indulg.sen0a">Senindulga</ref>, <ref cel="kompat.sen0a">senkompata</ref>,
      <ref cel="delika.mal0a">maldelikata</ref>, rezista:
      <ekz>
        &scirc;ia longa kaj sulkoplena viza&gcirc;o kovri&gcirc;is
        efektive per esprimo mal<tld/>a kaj severa
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;in tu&scirc;is la mal<tld/>a mano de la sorto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        koro <klr>[&#8230;]</klr> mal<tld/>a kiel &scirc;tono
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 41:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>a vorto ekscitas koleron
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 15:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        io ekmovi&gcirc;is en la mal<tld/>a animo de Doroteo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la malfeli&ccirc;a virino subite refalis el la supro de sia revo, kaj sin rompegis sur
        la mal<tld/>an realecon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        jugo propravola ne estas mal<tld/>a
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        neceseco kontra&ubreve;vola estas le&gcirc;o mal<tld/>a
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fer.0a.FIG">fera<sncref/></ref>,
      <ref cel="kruel.0a">kruela</ref>,
      <ref cel="rigid.0a.necedeme">rigida<sncref/></ref>,
      <ref cel="sever.0a">severa</ref>,
      <ref cel="sxton.0a.FIG_senindulga">&scirc;tona<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_d;&c_y;</trd>
  <trd lng="bg">&c_t;&c_v;&c_malmol;&c_r;&c_d;</trd>
  <trd lng="bo">&t_ma;&t_kha;&t_rata;&t_drengbu;&t_ga;&t_sa;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>
  <trd lng="br">kalet</trd>
  <trd lng="cs">tvrd&yacute;</trd>
  <trd lng="de">hart</trd>
  <trd lng="en">hard</trd>
  <trd lng="es">duro</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_kh;&a_t;</trd>,
    <trd>&a_s;&a_f;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">dur</trd>
  <trd lng="id">keras</trd>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x305F;&#12356;</trd>,
    <trd>&#x53B3;&#12375;&#12356;  <pr>&#x304D;&#12403;&#12375;&#12356;</pr></trd>,
    <trd>&#28961;&#x60C5;&#x306A;  <pr>&#12416;&#12376;&#12423;&#12358;&#x306A;</pr></trd>,
    <trd>&#x786C;&#x97F3;&#x306E;  <pr>&#12371;&#12358;&#x304A;&#12435;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="ne">&#2325;&#2337;&#x93E;</trd>
  <trd lng="nl">hard</trd>
  <trd lng="pl">twardy</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_v;&c_jo;&c_r;&c_d;&c_y;&c_j;</trd>,
    <trd>&c_zh;&c_jo;&c_s;&c_t;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_jo;&c_r;&c_s;&c_t;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">tvrd&yacute;</trd>
  <trd lng="tok">kiwen</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_v;&c_je;&c_r;&c_d;&c_i;&c_j;</trd>,
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_i;&c_j;</trd>
