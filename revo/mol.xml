<drv mrk="mol.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="mol.0a.cedema">
    <dif>
      Ne kontra&ubreve;staranta al premo, facile <ref tip="ant" cel="ced.0i.malrezisti">cedanta</ref>:
      <ekz>
        <klr>[ili]</klr> portas <tld/>ajn vestojn
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 11:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie malsupre kreskas la plej delikata <tld/>a herbo
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub la flankojn de la birdo &scirc;i metis <tld/>an kotonon
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ku&scirc;i&gcirc;is sur la <tld/>a musko
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la konsilistoj sidis en siaj se&gcirc;oj kun la <tld/>aj kapkusenoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        galo&scirc;oj <klr>[&#8230;]</klr> el tiel <tld/>a ledo <klr>[&#8230;]</klr>, kvaza&ubreve; fanditaj sur
        mia piedo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ekkaresis per sia <tld/>a mano lian varmegan frunton
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        malrapide ni mar&scirc;is en la <tld/>a sablo
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj ventoj venis el Sudokcidento
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> per <tld/>aj pa&scirc;oj glitis tra la kuirejo
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        konscienco senmakula estas kuseno plej <tld/>a
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="fleks.0ebla">fleksebla</ref>,
      <ref cel="kned">knedebla</ref>
    </refgrp>
    <trdgrp lng="br">
      <trd>blot</trd>,
      <trd>bouk</trd>,
      <trd>gwak</trd>
    </trdgrp>
    <trd lng="es">blando</trd>
  </snc>
  <snc mrk="mol.0a.milda">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne <ref tip="ant" cel="akr.0a.FIG">akra</ref>, <ref tip="sin" cel="mild.0a">milda</ref>:
      <ekz>
        homo kun <tld/>a kaj varma koro
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li konati&gcirc;is kun mizero kaj kun malfacilaj tagoj, kaj ili faras la spiriton
        mal<tld/>a a&ubreve; <tld/>a, ofte tro <tld/>a
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniam &gcirc;is nun mi vidis tiel gracian, tiel delikatan, tiel velure <tld/>an kaj
        tiel &ccirc;arman personon
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolumo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> parolis per vo&ccirc;o vere tre agrabla, <tld/>a kiel veluro
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vo&ccirc;oj <klr>[&#8230;]</klr> imitis <tld/>an susuradon de ondi&gcirc;antaj &scirc;tofoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj linioj de iliaj korpoj mistere altiris liajn sentojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vartistinoj <klr>[&#8230;]</klr> difektas nian fantazion per teruraj fabeloj kaj
        enpresas en nian <tld/>an cerbon terurajn bildojn de ia lasta ju&gcirc;o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a krepuskolumo
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        printempe <tld/>blua &ccirc;ielo
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; &ccirc;io vidi la vivon en helaj kaj <tld/>aj koloroj
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i salutis min kun <tld/>a, pura akcento
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        venis <tld/>a brileto en la okulojn
        <fnt>
          <bib>HsT</bib>
          <lok>Heredanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        per pacienco oni altiras al si potenculon, kaj <tld/>a parolo rompas oston
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 25:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[karaktero]</klr> <tld/>a kiel vakso
        <klr>(kp <ref cel="ole.0a">olea</ref>)</klr>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="velur.0a.FIG">velura<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>sanft</trd>,
      <trd>gef&uuml;gig</trd>
    </trdgrp>
    <trd lng="es">suave</trd>
  </snc>
  <snc mrk="mol.0a.FON">
    <uzo tip="fak">FON</uzo>
    <dif>
      <klr>(Parolsono)</klr> karakterizata per levo de la langodorso
      al la palato; <ref tip="sin" cel="palat.0a">palata</ref>.
    </dif>
