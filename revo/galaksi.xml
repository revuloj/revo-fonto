<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: galaksi.xml,v 1.22 2022/04/30 08:42:40 revo Exp $">
    <kap>
      <ofc>9</ofc><rad>galaksi</rad>/o <fnt><bib>SPV</bib></fnt>
    </kap>

    <drv mrk="galaksi.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">AST</uzo>
      <snc mrk="galaksi.0o.stelamaso">
        <dif>
          &Ccirc;iu el la grandaj stelamasoj, kiuj konsistigas la unuojn
          de la <ref tip="malprt" cel="univer.0o">universo</ref>:
          <ekz>
            la <ref tip="sub" cel="lakt.0avojo">Lakta Vojo</ref>
            estas nur unu <tld/>o inter miliardoj da <tld/>oj,
            kiuj ekzistas tra la kosmo;
            la homaro logas sur etete malgranda rokri&ccirc;a planedo
            (Tero) de tiu <tld/>o
            <fnt>
              <lok><url ref="&Viki;/Lakta_Vojo">Vikipedio</url></lok>
            </fnt>;
          </ekz><ekz>
            la kolosaj skaloj de la <tld/>oj
            <fnt><aut>Garbhan Macaoidh</aut>:
              <vrk>Per ardua ad astra - Tra kruta&jcirc;oj al la astroj
              (David
                Galadi-Enriquez kaj Amri Wandel: La kosmo kaj ni)</vrk>,
              <bib>Monato</bib><lok>jaro 2001a, numero 6a, p. 17a</lok>
            </fnt>.
          </ekz><ekz>
            usonaj astronomoj malkovris ke du konataj planedsistemoj
            ekster
            nia <tld/>o havas ne po nur unu sed po du planedojn
            <fnt>
              <aut>OH</aut>: <vrk>Du strangaj planedsistemoj</vrk>,
              <bib>Monato</bib><lok>jaro 2001a, numero 7a, p. 20a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <snc mrk="galaksi.0o.laktovojo">
        <uzo tip="stl">EVI</uzo>
        <ref tip="dif" cel="lakt.0avojo">la Lakta Vojo</ref>
        <trdgrp lng="fr">
          <trd>voie lact&eacute;e</trd>
        </trdgrp>
      </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_a;&c_k;&c_t;&c_y;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>galaxie</trd>,
    <trd>ml&eacute;&ccaron;n&aacute; dr&aacute;ha</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Galaxis</trd>,
        <trd>Milchstra&szlig;e</trd>,
        <trd>Milchstra&szlig;ensystem</trd>
      </trdgrp>
  <trd lng="el">&gamma;&alfa;&lambda;&alfa;&xi;&jota_ton;&alfa;&sigma_fina;</trd>
  <trd lng="en">galaxy</trd>
  <trd lng="es">galaxia</trd>
  <trdgrp lng="fr">
        <trd>galaxie</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>galaxis</trd>,
        <trd>csillagrendszer</trd>
      </trdgrp>
  <trd lng="id">galaksi</trd>
  <trd lng="nl">melkweg</trd>
  <trd lng="pt">gal&aacute;xia</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_a;&c_k;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">galaxia</trd>
  <trd lng="sv">galax</trd>
  <trd lng="uk">&#x433;&#x430;&#x43B;&#x430;&#x43A;&#x442;&#x438;&#x43A;&#x430;</trd>
</drv>
  </art>
