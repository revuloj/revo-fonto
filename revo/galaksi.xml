<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>galaksi</rad>/o
</kap>
<drv mrk="galaksi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc num="1">
    <dif>
      La amaso de &ccirc;irka&ubreve; 100 miliardoj da
      <ref tip="prt" cel="stel.0o">steloj</ref>, interne de kiu
      trovi&gcirc;as nia <ref tip="prt" cel="sun.0o">suno</ref>,
      kaj kiu havas formon de ovala plata kuko,
      kun diametro de &ccirc;irka&ubreve; 100.000 lumjaroj k diko de &ccirc;irka&ubreve;
      10.000 je la centro.
    </dif>
  </snc>
  <snc num="2">
    <dif>
      &Ccirc;iu el la analogaj stelamasoj, kiuj konsistigas la kosmajn unuojn.
    </dif>
  </snc>
  <refgrp tip="malprt">
    <ref cel="univer.0o">universo</ref>,
    <ref cel="kosm.0o">kosmo</ref>
  </refgrp>
  <trd lng="en">galaxy</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_a;&c_k;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>
</art>

