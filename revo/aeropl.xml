<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>aeroplan</rad>/o
</kap>
<drv mrk="aeropl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AVI</uzo>
  <snc>
    <dif>
      Aparato kapabla sin levi kaj movi en la aero, ne estante pli malpeza
      ol &gcirc;i, dank' al la premo de la vento sur &gcirc;iaj surfacoj.
    </dif>
  </snc>
  <refgrp tip="super">
    <ref cel="aviad.0ilo">aviadilo</ref>,
    <ref cel="avi.0o">avio</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_a;&c_e;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_n;</trd>,
    <trd>&c_s;&c_a;&c_m;&c_a;&c_l;&c_jo;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>airplane</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>aeroplano</trd>,
    <trd>avi&oacute;n</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>a&eacute;roplane</trd>,
    <trd>avion</trd>
  </trdgrp>
  <trd lng="he">&alef;&vav;&yod;&resh;&vav;&fnun;</trd>
  <trdgrp lng="it">
    <trd>aeroplano</trd>,
    <trd>aereo</trd>
  </trdgrp>
  <trd lng="nl">vliegtuig</trd>
  <trd lng="pl">samolot</trd>
  <trdgrp lng="pt">
    <trd>aeroplano</trd>,
    <trd>avi&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_m;&c_o;&c_l;&c_jo;&c_t;</trd>,
    <trd>&c_a;&c_e;&c_r;&c_o;&c_p;&c_l;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">lietadlo</trd>
  <trd lng="sv">flygplan</trd>
  <trd lng="tr">u&ccedil;ak</trd>
</drv>
</art>
