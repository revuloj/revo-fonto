<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>trofe</rad>/o
</kap>
<drv mrk="trofe.0o">
  <kap><tld/>o</kap>
  <snc mrk="trofe.0o.MIL">
  <uzo tip="fak">MIL</uzo>
    <dif>
      Armoj, standardoj a&ubreve; iaspecaj militakira&jcirc;oj elmontrataj
kiel signo
      de venko.
    </dif>
  </snc>
  <snc mrk="trofe.0o.BELA">
  <uzo tip="fak">BELA</uzo>
    <dif>
      Skulptita a&ubreve; pentrita <ref tip="super"
cel="dekora.0o.ornamajxo">dekoracio</ref>
      prezentanta trofeon.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_f;&c_e;&c_j;</trd>
  <trdgrp lng="de">
    <trd>Troph&auml;e</trd>,
    <trd>Siegeszeichen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>troph&eacute;e</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>gy&odblac;zelmi jelv&eacute;ny</trd>,
    <trd>tr&oacute;fea</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>trofee</trd>,
    <trd>zegeteken</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_r;&c_o;&c_f;&c_je;&c_j;</trd>
</drv>
</art>
