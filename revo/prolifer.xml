<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prolifer.xml,v 1.10 2022/06/12 20:31:11 revo Exp $">
<kap>
  <rad>prolifer</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="prolifer.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="prolifer.0i.BIO">
    <uzo tip="fak">BIO</uzo><uzo tip="fak">BAK</uzo>
    <uzo tip="klr">(pri &ccirc;eloj)</uzo>
    <dif>
      Rapide
      <ref tip="super" cel="mult.0igxi">multi&gcirc;i</ref>,
      <ref tip="super" cel="prospe1.0i.ntr">prosperi</ref>:
      <ekz>
        sur &ccirc;i tiu tubo oni &scirc;miras ian materialon sur kiu facile 
        kolekti&gcirc;as kaj <tld/>as &ccirc;elo
        <fnt>
          <aut>Degu&ccirc;i I.</aut>,
          <vrk>&Ccirc;u ni povas akcepti la studon de l'homa ET-&ccirc;elo?</vrk>,
          <bib>Oomoto</bib>,
          <lok>2001 2002, numeroj 447a 448a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>as la demagogio       
        <fnt>
          <aut>G. Moya</aut>,
          <vrk>Socia evoluo</vrk>,
          <lok><url ref="http://storm.prohosting.com/jesuo/k45.htm">
          Kajeroj el la Sudo, 2000-01, numero 45a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>sich <ind>vermehren</ind></trd>,
      <trd>sich <ind>ausbreiten</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>bujet</trd>,
    <trd>d&ecaron;lit se</trd>,
    <trd>proliferovat</trd>,
    <trd>pu&ccaron;et</trd>,
    <trd>rozmno&zcaron;ovat se <klr>(bu&ncaron;ky)</klr></trd>,
    <trd>tvo&rcaron;it v&yacute;honky</trd>
  </trdgrp>
  <trd lng="en">proliferate</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&f_k;&a_th;&f_y;&a_r; &a_sh;&a_d;&a_n;</trd>,
    <trd>&a_A;&a_f;&a_z;&a_A;&f_y;&a_sh; &f_y;&a_A;&a_f;&a_t;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">prolif&eacute;rer</trd>
  <trdgrp lng="it">
    <trd>proliferare</trd>,
    <trd>prolificare</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zich <ind>vermenigvuldigen</ind><klr tip="ind">
    (arch.)</klr></trd>,
    <trd>voortwoekeren<klr tip="ind"> (arch.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rozmna&zdot;a&cacute; si&eogonek;</trd>,
    <trd>mno&zdot;y&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>multiplica</trd>,
    <trd>c&abreve; se &icirc;nmul&tcedil;esc</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>deli&tcaron; sa</trd>,
    <trd>rozmno&zcaron;ova&tcaron; sa</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&#x43E;&c_z;&#x43C;&#x43D;&#x43E;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;
  &#x43F;&#x43E;&c_d;&c_ib;&#x43B;&#x43E;&#x43C;</trd>
</drv>

<drv mrk="prolifer.0o">
  <kap><tld/>o, <var><kap><tld/>ado</kap></var></kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="prolifer.0o.BIO">
    <uzo tip="fak">BIO</uzo><uzo tip="fak">BAK</uzo>
    <uzo tip="klr">(pri &ccirc;eloj)</uzo>
    <dif>
      Multi&gcirc;o, prosperado:
      <ekz>
        anka&ubreve; ekzistas kontra&ubreve;-kancera efiko de &gcirc;ia pigmento, sed tio okazas &ccirc;efe anta&ubreve;
        <tld/>ado kaj diferenci&gcirc;o de &ccirc;eloj, do dum periodo anta&ubreve; la DNA-sintezo
        <fnt>
          <bib>Monato</bib>
          <aut>Peng Jianjun</aut>
          <vrk><url ref="&Monato;/2005/008444.php">Teo por preventi
          kancerojn</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &ccirc;ina estas multe pli minacata ol la kvin aliaj lingvoj pro la <tld/>ado de la
        angla
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2005/008616.php">Lingva egaleco en internaciaj
          rilatoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        komence Aureliano Segundo ne rimarkis la alarmajn proporciojn 
        de la <tld/>ado
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>,
        <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Vermehrung</trd>,
      <trd>Proliferation</trd>,
      <trd>Zellwachstum</trd>,
      <trd>Ausbreitung</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>bujen&iacute;</trd>,
    <trd>bun&ecaron;&ccaron;n&eacute; d&ecaron;len&iacute;</trd>,
    <trd>novotvo&rcaron;en&iacute; tk&aacute;n&ecaron;</trd>,
    <trd>proliferace</trd>
  </trdgrp>
  <trd lng="en">proliferation</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&f_k;&a_th;&f_y;&a_r;</trd>,
    <trd>&a_A;&a_f;&a_z;&a_A;&f_y;&a_sh;</trd>
  </trdgrp>
  <trd lng="fr">prolif&eacute;ration</trd>
  <trdgrp lng="it">
    <trd>proliferazione</trd>,
    <trd>prolificazione</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>prolifratie</trd>,
    <trd>woekeren<klr tip="ind"> (zn.)</klr></trd>
  </trdgrp>
  <trd lng="pl">proliferacja</trd>
  <trd lng="ro">proliferare</trd>
  <trdgrp lng="sk">
    <trd>bujnenie</trd>,
    <trd>novotvorienie tkaniva</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&#x43E;&c_z;&#x43C;&#x43D;&#x43E;&c_zh;&c_je;&#x43D;&#x43D;&#x44F;
  &#x43F;&#x43E;&c_d;&c_ib;&#x43B;&#x43E;&#x43C;</trd>
</drv>
</art>

