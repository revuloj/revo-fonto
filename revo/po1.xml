<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>po</rad>/acoj
</kap>
<drv mrk="po1.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de unukotiledonaj herboj 
      (<trd lng="la">Poaceae</trd>),
      kun pli ol 300 genroj:
      <refgrp tip="sub">
        <ref cel="aven.0">aveno</ref>
        <ref cel="horde.0o">hordeo</ref>, 
        <ref cel="maiz.0o">maizo</ref>, 
        <ref cel="sekal.0o">sekalo</ref>, 
        <ref cel="tritik.0o">tritiko</ref>
      </refgrp>.
    </dif>
  </snc>
  <trd lng="de">Gr&auml;ser</trd>
  </drv>
</art>

