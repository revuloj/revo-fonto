<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>p</rad> <fnt><bib>F</bib><lok>&FE; 1</lok></fnt>
</kap>
<subart>
<drv mrk="po1.0.konsonanto">
  <kap><tld/></kap> 
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>
   <dif>
   <ref tip="vid" cel="konson1.0o">Konsonanto</ref>, senvo&ccirc;a <ref
   tip="vid"
   cel="plosiv.0o">plosiva</ref> <ref tip="vid"
   cel="labial.0o">labialo</ref> [p].
   </dif>
   <trd lng="fr">p<klr> (consonne)</klr></trd>
  </snc>
  <snc>
    <dif>    
      La dudeka <ref tip="super" cel="liter.0o">litero</ref> de la
      Esperanta alfabeto, uzata i.a. kiel:
     </dif>
     <subsnc>
     <uzo tip="fak">KEM</uzo>
       <dif>
          (maj.) simbolo de fosforo:<ekz> <em>P</em></ekz>;
       </dif>
       <trd lng="fr">phosphore</trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">REL</uzo>
       <dif>
         (maj.) mallongigo de Patro:<ekz> <em>P</em></ekz>;
       </dif>
       <trd lng="fr">P&egrave;re</trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">FIZ</uzo>
       <dif>
         (maj.) simbolo de peta:<ekz> <em>P</em></ekz>;
       </dif>
       <trd lng="fr">p&eacute;ta</trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">TIP</uzo>
       <dif>
         (min.) mallongigo de pa&gcirc;o:<ekz> <em>p.</em></ekz>;
       </dif>
       <trd lng="fr">page</trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">FIZ</uzo>
       <dif>
         (min.) simbolo de piko:<ekz> <em>p</em></ekz>;
       </dif>
       <trd lng="fr">pico</trd>
     </subsnc>
     <trd lng="fr">p<klr> (lettre)</klr></trd>
  </snc>
  </drv>
</subart>

<subart>
<drv mrk="po1.0.nomo">
  <kap><tld/>o</kap>
  <snc>
  <dif>
  Nomo de tiu <ref tip="super" cel="liter.0o.propra">litero</ref>.
  </dif>
  <trd lng="fr">p</trd>
  </snc>
  <ref tip="hom" cel="po">po (prepozicio)</ref>
</drv> 
</subart>

</art>

