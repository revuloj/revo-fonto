<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: po1.xml,v 1.21 2023/09/03 12:45:09 revo Exp $">
<kap>
    <ofc>*</ofc><ofc>7</ofc><rad>po</rad>
</kap>
<drv mrk="po1.0">
  <kap><tld/></kap> 
  <uzo tip="fak">LIN</uzo>
  <uzo tip="fak">FON</uzo>
  <uzo tip="fak">GRA</uzo>
  <snc mrk="po1.0.konsonanto">
    <dif>
      Nomo de la dudeka litero de la Esperanta alfabeto:
      <ekz>
        nomoj de la literoj: <klr>[...]</klr> no, o, 
        po, ro <klr>[...]</klr>
        <fnt>
          <bib>F</bib>
          <lok>&FE; 1</lok>
        </fnt>;
      </ekz>
      <ekz>
         Go-<tld lit="P"/>-U! &ndash; aldonis jam tute la&ubreve;te la kasisto
         <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      uzata i.a. kiel:
    </dif>
    <subsnc>
       <uzo tip="fak">KEM</uzo>
       <dif>
          (maj.) simbolo de fosforo:<ekz> <em>P</em>;</ekz>
       </dif>
       <trd lng="fr">phosphore</trd>
       <trd lng="pl">fosfor P</trd>
       <trd lng="ro">fosfor P</trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">REL</uzo>
       <dif>
         (maj.) mallongigo de Patro:<ekz> <em>P</em>;</ekz>
       </dif>
       <trd lng="fr">P&egrave;re</trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">FIZ</uzo>
       <dif>
         (maj.) simbolo de peta:<ekz> <em>P</em>;</ekz>
       </dif>
       <trd lng="fr">p&eacute;ta</trd>
       <trd lng="pl">peta P</trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">TIP</uzo>
       <dif>
         (min.) mallongigo de pa&gcirc;o:<ekz> <em>p.</em>;</ekz>
       </dif>
       <trd lng="fr">page</trd>
     </subsnc>
     <subsnc>
       <uzo tip="fak">FIZ</uzo>
       <dif>
         (min.) simbolo de piko:<ekz> <em>p</em>.</ekz>
       </dif>
       <trd lng="fr">pico</trd>
       <trd lng="pl">piko p</trd>
     </subsnc>
     <trd lng="fr">p<klr> (lettre)</klr></trd>
  </snc>
  <ref tip="hom" cel="po">po (prepozicio)</ref>
  <trd lng="ja">&#x30DD;&#x30FC;</trd>
  <trd lng="uk">&#x43F;&#x43E;</trd>
</drv> 

</art>

