<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dinorn.xml,v 1.20 2025/11/28 20:34:37 revo Exp $">
<kap>
  <rad>dinornit</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="n">dinorn</rad>/o</kap></var>,
  <var><kap><rad var="s">dinornis</rad>/o <fnt><bib>PVEF</bib></fnt></kap></var>
</kap>
<drv mrk="dinorn.0o">
  <kap><tld/>o, <var><kap><tld var="n"/>o</kap></var></kap>
  <snc mrk="dinorn.0o.ZOO">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Grandega fosilia birdo, dufoje pli granda ol homo, simila al
      hodia&ubreve;aj stilzbirdoj kaj renkontata en la A&ubreve;stralia
      plejstoceno:
      <ekz>
        <tld/>o finpereis nur anta&ubreve; ducent jaroj;
      </ekz>
      <ekz>
        <nac>moa</nac> a&ubreve; <tld var="n"/>o estas nun tute elmortinta,
        sed &gcirc;i ekzistis multnombre ankora&ubreve; kiam venis la maorioj &ccirc;i tien,
        kaj ili &ccirc;asis &gcirc;in por forigi la &ccirc;iaman viandomankon
        <fnt>
          <aut>B. Clark</aut>
          <vrk>Pratempaj postvivantoj en Nov-Zelando</vrk>
          <lok>Malgranda Revuo, 1947/4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_n;&c_o;&c_r;&c_n;&c_ib;&c_s;</trd>
  <trd lng="cs">dinornis</trd>
  <trdgrp lng="de">
    <trd>Moa</trd>,
    <trd>Dinornis [fak]</trd>
  </trdgrp>
  <trd lng="fr">dinornis</trd>
  <trd lng="he">&#x5DE;&#x5D5;&#x5D0;&#x5D4; (&#x5E0;&#x5DB;&#x5D7;&#x5D3;&#x5D4;)</trd>
  <trdgrp lng="hu">
    <trd>Dinornis</trd>,
    <trd>Moa</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C7;&#x30A3;&#x30CE;&#x30EB;&#x30CB;&#x30B9;</trd>,
    <trd>&#x30E2;&#x30A2;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>moa</trd>,
    <trd>dinornis</trd>
    </trdgrp>
  <trdgrp lng="pl">
    <trd>donornis</trd>,
    <trd>moa</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>dinornit&iacute;deo</trd>,
    <trd>moa</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_i;&c_n;&c_o;&c_r;&c_n;&c_i;&c_s;</trd>
  <trd lng="sk">moa mamut&#xED;</trd>
  <trd lng="uk">&c_d;&c_i;&#x43D;&#x43E;&c_r;&#x43D;&c_ib;&c_s;</trd>
</drv>
</art>
