<drv mrk="invent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i; rezulto de tiu ago:
      <ekz>
        la <tld/>o de la presma&scirc;ino
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="patent.0o">patento</ref> por la <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        markas <klr>[&#8230;]</klr> la komenci&gcirc;on de la historio homa <klr>[&#8230;]</klr> la
        <tld/>o de la mano &ndash; se oni povas tion diri &ndash; kaj tiu de la lingvo
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la <tld/>o de la armena alfabeto la popolo dum jarmiloj uzis kojnoformajn
        skriba&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Lida Elbakjan</aut>
          <vrk><url ref="&Monato;/2003/007590.html">Armenio festis 1700 jarojn da
          kristanismo</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_n;&c_a;&c_h;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="cs">vyn&aacute;lez</trd>
