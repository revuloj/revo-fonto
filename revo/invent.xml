<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>invent</rad>/i
</kap>
<drv mrk="invent.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Krei, elpensi ion novan en la kampo de te&hcirc;niko
      per la forto de la spirito:
      <ekz>
        <tld/>i ma&scirc;inon, novan metodon, procedon, modon, kombinon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pens.el0i">elpensi</ref>,
      <ref cel="trov.el0i">eltrovi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_n;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_d;&c_u;&c_m;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">vynalézat</trd>
  <trd lng="de">erfinden</trd>
  <trd lng="en">invent</trd>
  <trd lng="es">inventar</trd>
  <trd lng="fr">inventer<klr tip="ind"> (proc&eacute;d&eacute;, dispositif)</klr></trd>
  <trd lng="hu">feltal&aacute;l</trd>
  <trd lng="nl">uitvinden</trd>
  <trd lng="pt">inventar</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_o;&c_b;&c_r;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_o;&c_b;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>objaviť</trd>,
    <trd>vynájsť</trd>
  </trdgrp>
</drv>

<drv mrk="invent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        <tld/>o de la presado;
      </ekz>
      <ekz>
        <ref cel="patent.0o">patento</ref> pri <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">vynález</trd>
  <trdgrp lng="de">
    <trd>Erfindung</trd>,
    <trd>Erfinden</trd>
  </trdgrp>
  <trd lng="en">invention</trd>
  <trd lng="es">invenci&oacute;n</trd>
  <trd lng="fr">invention<klr tip="ind"> (action)</klr></trd>
  <trd lng="hu">feltal&aacute;l&aacute;s</trd>
  <trd lng="nl">het <ind>uitvinden</ind></trd>
  <trd lng="pt">inven&ccedil;&atilde;o<klr tip="amb"> (a&ccedil;&atilde;o)</klr></trd>
  <trd lng="sk">vynález</trd>
</drv>

<drv mrk="invent.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Io <tld/>ita.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_n;&c_a;&c_h;&c_o;&c_d;&c_z;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_n;&c_a;&c_h;&c_o;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erfindung</trd>,
    <trd>Erfundenes</trd>
  </trdgrp>
  <trd lng="en">invention</trd>
  <trd lng="es">invento</trd>
  <trd lng="fr">invention<klr tip="ind"> (proc&eacute;d&eacute;, dispositif)</klr></trd>
  <trd lng="hu">tal&aacute;lm&aacute;ny</trd>
  <trd lng="nl">uitvinding</trd>
  <trd lng="pt">inven&ccedil;&atilde;o<klr tip="amb"> (coisa inventada)</klr></trd>
  <trd lng="ru">&c_i;&c_z;&c_o;&c_b;&c_r;&c_je;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="invent.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu sin okupas pri elpensado de novaj <tld/>a&jcirc;oj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_n;&c_a;&c_h;&c_o;&c_d;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_v;&c_y;&c_n;&c_a;&c_h;&c_o;&c_d;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">vynálezce</trd>
  <trdgrp lng="de">
    <trd>Erfinder</trd>,
    <trd>Erfinderin</trd>
  </trdgrp>
  <trd lng="en">inventor</trd>
  <trd lng="es">inventor</trd>
  <trd lng="fr">inventeur</trd>
  <trd lng="hu">feltal&aacute;l&oacute;</trd>
  <trd lng="nl">uitvinder</trd>
  <trd lng="pt">inventor</trd>
  <trd lng="ru">&c_i;&c_z;&c_o;&c_b;&c_r;&c_je;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>objaviteľ</trd>,
    <trd>vynálezca</trd>
  </trdgrp>
</drv>
</art>
