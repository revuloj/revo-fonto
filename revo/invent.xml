<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>invent</rad>/i
</kap>
<drv mrk="invent.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Krei, elpensi ion novan per la forto de la spirito:
      <ekz><tld/>i ma&scirc;inon, novan metodon, procedon, modon, kombinon.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="trov.el0i">eltrovi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">erfinden</trd>
  <trd lng="en">invent</trd>
  <trd lng="nl">uitvinden</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_z;&c_o;&c_b;&c_r;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_o;&c_b;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
<drv mrk="invent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i: <ekz><tld/>o de la presado.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Erfindung</trd>,
    <trd>Erfinden</trd>
  </trdgrp>
  <trd lng="nl">het <ind>uitvinden</ind></trd>
 </drv>
<drv mrk="invent.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <snc>
    <dif>
      Io <tld/>ita: <ekz>patento pri <tld/>o.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Erfindung</trd>,
    <trd>Erfundenes</trd>
  </trdgrp>
  <trd lng="nl">uitvinding</trd>
  <trd lng="ru">&c_i;&c_z;&c_o;&c_b;&c_r;&c_je;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
<drv mrk="invent.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu sin okupas pri elpensado de novaj <tld/>a&jcirc;oj.  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Erfinder</trd>,
    <trd>Erfinderin</trd>
  </trdgrp>
  <trd lng="nl">uitvinder</trd>
  <trd lng="ru">&c_i;&c_z;&c_o;&c_b;&c_r;&c_je;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>
</art>
