<drv mrk="invent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i; rezulto de tiu ago:
      <ekz>
        la <tld/>o de la presma&scirc;ino
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="patent.0o">patento</ref> por la <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        markas <klr>[&#x2026;]</klr> la komenci&gcirc;on de la historio homa <klr>[&#x2026;]</klr> la
        <tld/>o de la mano &ndash; se oni povas tion diri &ndash; kaj tiu de la lingvo
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la <tld/>o de la armena alfabeto la popolo dum jarmiloj uzis kojnoformajn
        skriba&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Lida Elbakjan</aut>
          <vrk><url ref="&Monato;/2003/007590.html">Armenio festis 1700 jarojn da kristanismo</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_n;&c_a;&c_h;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="cs">vyn&aacute;lez</trd>
  <trdgrp lng="de">
    <trd>Erfindung</trd>,
    <trd>Erfinden</trd>
  </trdgrp>
  <trd lng="en">invention</trd>
  <trd lng="es">invenci&oacute;n</trd>
  <trd lng="fr">invention<klr tip="ind"> (action)</klr></trd>
  <trd lng="hu">feltal&aacute;l&aacute;s</trd>
  <trd lng="ja">&#x767A;&#x660E;  <pr>&#x306F;&#x3064;&#x3081;&#x3044;</pr></trd>
  <trd lng="nl">het <ind>uitvinden</ind></trd>
  <trdgrp lng="pl">
    <trd>wynalazek</trd>,
    <trd>pomys&lstroke;</trd>
  </trdgrp>
  <trd lng="pt">inven&ccedil;&atilde;o<klr tip="amb"> (a&ccedil;&atilde;o)</klr></trd>
  <trdgrp lng="ro">
    <trd>inven&#x21B;ie</trd>,
    <trd>cugetare</trd>
  </trdgrp>
  <trd lng="sk">vyn&aacute;lez</trd>
  <trdgrp lng="zh">
    <trd>&#x53D1;&#x660E; <pr>f&#x101;m&#xED;ng</pr></trd>,
    <trd>&#x53D1;&#x660E;&#x6784;&#x601D; <pr>f&#x101;m&#xED;ngg&#xF2;us&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="invent.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Io <tld/>ita:
      <ekz>
        artisma prozrekreo de fantaziaj <tld/>a&jcirc;oj
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili a&ccirc;etas akciojn <klr>[&#x2026;]</klr> <klr>[&#x2026;]</klr> alia grandioza <tld/>a&jcirc;o de la
        homa cerbo
        <fnt>
          <bib>VaK</bib>
          <lok>Kvara &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu nova tendenco <klr>[&#x2026;]</klr> ne estis <tld/>a&jcirc;o de laboristoj
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        po&scirc;lanterno ekflamas, <klr>[&#x2026;]</klr> plia <tld/>a&jcirc;o de Satano, kiun oni vidas
        &ccirc;iufoje nur tiam, kiam &gcirc;i trovi&gcirc;as proksime anta&ubreve; la okuloj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        al la fifamaj <tld/>a&jcirc;oj de tiuj markuiristoj apartenas &leftquot;<nac>the potage</nac>&rightquot;, la
        kunkuirado de la man&gcirc;restoj el pluraj tagoj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 29</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fantaz.0ajxo">fantazia&jcirc;o</ref>,
      <ref cel="ide.0o">ideo</ref>,
      <ref cel="kre.0ajxo.KOMUNE">krea&jcirc;o<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_n;&c_a;&c_h;&c_o;&c_d;&c_z;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_n;&c_a;&c_h;&c_o;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erfindung</trd>,
    <trd>Erfundenes</trd>
  </trdgrp>
  <trd lng="en">invention</trd>
  <trd lng="es">invento</trd>
  <trd lng="fr">invention<klr tip="ind"> (proc&eacute;d&eacute;,
  dispositif)</klr></trd>
  <trd lng="hu">tal&aacute;lm&aacute;ny</trd>
  <trd lng="ja">&#x767A;&#x660E;&#x54C1;  <pr>&#x306F;&#x3064;&#x3081;&#x3044;&#x3072;&#x3093;</pr></trd>
  <trd lng="nl">uitvinding</trd>
  <trd lng="pl">wynalazek</trd>
  <trd lng="pt">inven&ccedil;&atilde;o<klr tip="amb"> (coisa inventada)</klr></trd>
  <trd lng="ro">inven&#x21B;ie</trd>
  <trd lng="ru">&c_i;&c_z;&c_o;&c_b;&c_r;&c_je;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="uk">&c_v;&c_i;&c_n;&c_a;&c_h;&c_ib;&c_d;</trd>
