<drv mrk="invent.0ema">
  <kap>
    <tld/>ema
  </kap>
  <snc>
    <dif>
      Ema kaj lerta trovi originalajn novajn aparatojn, metodojn, solvojn ktp:
      <ekz>
        la &ccirc;iam alie apatia kaj mallaborema Anadirano i&gcirc;as dum tiu tempo vigla, energia,
        sprita kaj <tld/>ema
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>Unua skizo:</lok>
        </fnt>;
      </ekz>
      <ekz>
        liberaluloj entuziasme akceptis <klr>[Esperanton]</klr> kiel unu admirindan
        rezulton de okcidenta <tld/>emo
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej multaj kantoj <klr>[kaj]</klr> la aran&gcirc;oj sonis nekutime originale,
        fre&scirc;e kaj <tld/>eme
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_n;&c_a;&c_h;&c_o;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_z;&c_n;&c_a;&c_h;&c_o;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_p;&c_n;&c_y;</trd>,
    <trd>&c_k;&c_r;&c_e;&c_a;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>erfinderisch</trd>,
    <trd>innovativ</trd>,
    <trd>einfallsreich</trd>,
    <trd>kreativ</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ingenious</trd>,
    <trd>inventive</trd>,
    <trd>innovative</trd>,
    <trd>creative</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>inventive</trd>,
    <trd>ing&eacute;nieux</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>innowacyjny</trd>,
    <trd>wynalazczy</trd>
  </trdgrp>
</drv>

<drv mrk="invent.0isto">
