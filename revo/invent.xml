<drv mrk="invent.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="kre.0i.fondi">Krei</ref>, <ref tip="super" cel="pens.el0i">elpensi</ref>
      ion
      novan, precipe en la kampo de te&hcirc;niko, per la forto de la spirito:
      <ekz>
        <tld/>i ma&scirc;inon, novan metodon, procedon, modon, kombinon;
      </ekz>
      <ekz>
        li <tld/>is iujn specialajn fajrosendan&gcirc;erajn vandojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is <klr>(false elpensis)</klr> la faktojn por pravigi
        siajn teoriajn asertojn
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>!
      </ekz>
      <ekz>
        e&ubreve;ropanoj <klr>[&#8230;]</klr> <tld/>is &ccirc;aspafilon, motorboaton, kuir- kaj
        kudroma&scirc;inojn, gramofonon
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>4. La fatala sa&gcirc;eco.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Mozart</nom> <tld/>is originalan manieron de modulado (intertonala transiro)
        <fnt>
          <bib>KFl</bib>
          <lok>III. Atomoj de evoluo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi povas <tld/>i <klr>[&#8230;]</klr> rimitan frazon
        <fnt>
          <bib>KrB</bib>
          <lok>Specimene</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="trov.el0i">eltrovi</ref>
    </refgrp>
