<drv mrk="invent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        <tld/>o de la presado;
      </ekz>
      <ekz>
        <ref cel="patent.0o">patento</ref> pri <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_n;&c_a;&c_h;&c_o;&c_d;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="cs">vyn&aacute;lez</trd>
  <trdgrp lng="de">
    <trd>Erfindung</trd>,
    <trd>Erfinden</trd>
  </trdgrp>
  <trd lng="en">invention</trd>
  <trd lng="es">invenci&oacute;n</trd>
