<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kret</rad>/o
</kap>
<drv mrk="kret.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Malfortika blanka karbonato de kalko, kiun oni trovas
      la&ubreve; tavoloj en tero.
    </dif>
    <trd lng="de">Kreide</trd>
    <trd lng="nl">krijt</trd>
  </snc>
  <snc>
    <dif>
      Bastoneto el tiu substanco uzata por skribi sur nigra supra&jcirc;o.
    </dif>
    <trd lng="de">Kreidest&uuml;ck</trd>
    <trdgrp lng="nl">
     <trd>krijtje</trd>,
     <trd>stukje <ind>krijt</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_e;&c_j;&c_d;&c_a;</trd>
  <trd lng="en">chalk</trd>
  <trd lng="fr">craie</trd>
  <trd lng="hu">kr&eacute;ta</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;</trd>
  <trd lng="sv">krita</trd>
</drv>
</art>
