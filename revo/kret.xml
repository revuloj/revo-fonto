<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kret</rad>/o
</kap>
<drv mrk="kret.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Malfortika blanka karbonato de kalko, kiun oni trovas
      la&ubreve; tavoloj en tero.
    </dif>
    <trd lng="de">Kreide</trd>
  </snc>
  <snc>
    <dif>
      Bastoneto el tiu substanco uzata por skribi sur nigra supra&jcirc;o.
    </dif>
    <trd lng="de">Kreidest&uuml;ck</trd>
  </snc>
  <trd lng="en">chalk</trd>
  <trd lng="fr">craie</trd>
  <trd lng="ru">&c_m;&c_je;&c_l;</trd>
  <trd lng="sv">krita</trd>
</drv>
</art>
