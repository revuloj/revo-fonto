  <trdgrp lng="ro">
    <trd>antebelic</trd>
    <trd>&icirc;nainte de r&abreve;zboi</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_v;&c_o;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_v;&c_o;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
