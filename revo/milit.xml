  <trd lng="be">&c_D;&c_r;&c_u;&c_g;&c_a;&c_ja; &c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_a;&c_ja; <ind>&c_v;&c_a;&c_j;&c_n;&c_a;</ind></trd>
