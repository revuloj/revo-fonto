<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>milit</rad>/i
</kap>

<drv mrk="milit.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="milit.0i.MIL">
    <dif>
      Batali per armiloj kontra&ubreve; fremdaj armeoj a&ubreve;
      kontra&ubreve; malamika partio:
      <ekz>
        via gento <klr>[...]</klr> &ccirc;iam militas kontra&ubreve; nia
        <fnt><bib>FK</bib>, <lok>2, fabeloj kaj legendoj, la naski&gcirc;o de
        la
        tabako, araba legendo, la&ubreve; la hispana rakonto de Cervera
        Bachiller, tradukis Jayme Heinlein Ferreira</lok></fnt>;
      </ekz>
      <ekz>
        du <tld/>as, tria profitas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_v;&c_o;&c_d;&c_ja; <ind>&c_v;&c_o;&c_j;&c_n;&c_a;</ind></trd>
  <trdgrp lng="br">
    <trd>brezelekaat</trd>,
    <trd>brezeli&ntilde;</trd>
  </trdgrp>
  <trd lng="de">Krieg f&uuml;hren</trd>
  <trdgrp lng="fr">
    <trd>guerroyer</trd>,
    <trd>faire la <ind>guerre</ind></trd>
  </trdgrp>
  <trd lng="es">guerrear</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;bor&uacute;zik</trd>,
    <trd>hadat visel</trd>
  </trdgrp>
  <trd lng="nl">oorlog voeren</trd>
  <trd lng="pt">guerrear</trd>
  <trd lng="ru">&c_v;&c_o;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">kriga</trd>
  <trd lng="vo">klig&ouml;n</trd>
</drv>

<drv mrk="milit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="milit.0o.MIL">
    <dif>
      Oficiala <ref tip="super" cel="pac.mal0o.KOMUNE">malpaco</ref>
      inter
      nacioj a&ubreve; partioj, kun <ref tip="prt"
      cel="batal.0o">bataloj</ref>:
      <ekz>
        ha, se tiel, tiam ne estos <tld/>o kun la turkoj
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 2a</lok></fnt>;
      </ekz>
      <ekz>
        la besto, kiu suprenvenas el la abismo, faros <tld/>on
kontra&ubreve; ili
        <fnt><bib>NT</bib>, <lok>&Apo; 11:7</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>on li perdis
        <fnt><bib>SatirRak</bib>, <lok>supersti&ccirc;o</lok></fnt>;
      </ekz>
      <ekz>
        proklami, deklari <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_j;&c_n;&c_a;</trd>
  <trd lng="bg">&c_v;&c_o;&c_j;&c_n;&c_a;</trd>
  <trd lng="br">brezel</trd>
  <trd lng="cs">v&aacute;lka</trd>
  <trd lng="de">Krieg</trd>
  <trd lng="en">war</trd>
  <trd lng="es">guerra</trd>
  <trd lng="fr">guerre</trd>
  <trdgrp lng="hu">
    <trd>h&aacute;bor&uacute;</trd>,
    <trd>hadvisel&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="ku">
    <trd>&scedil;er</trd>,
    <trd>ceng</trd>
  </trdgrp>
  <trd lng="nl">oorlog</trd>
  <trd lng="ru">&c_v;&c_o;&c_j;&c_n;&c_a;</trd>
  <trd lng="sv">krig</trd>
  <trd lng="tp">utala</trd>
  <trd lng="vo">klig</trd>
</drv>

<drv mrk="milit.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Koncernanta la <tld/>on:
      <ekz>
        la <tld/>a le&gcirc;o tre precizas pri tiu punkto
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro
        15a</lok></fnt>.
      </ekz>
      <ekz>
        <tld/>a kanto, rakonto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_je;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>-<ind>brezel</ind></trd>,
    <trd>brezelel</trd>
  </trdgrp>
  <trd lng="de">Kriegs-</trd>
  <trdgrp lng="fr">
    <trd>guerrier</trd>,
    <trd>martial</trd>,
    <trd>militaire<klr tip="amb"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="es">militar</trd>
  <trdgrp lng="hu">
    <trd>harci</trd>,
    <trd>h&aacute;bor&uacute;s</trd>,
    <trd>katonai</trd>
  </trdgrp>
  <trd lng="nl">oorlogs-</trd>
  <trd lng="ru">&c_v;&c_o;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="vo">kliga</trd>
</drv>

<drv mrk="milit.0estro">
  <kap><tld/>estro</kap>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="milit.0estro.MIL">
    <dif>
      Homo &ndash; pliofte oficiro &ndash; kiu decidas pri strategio, gvidas dum
      bataloj:
      <ekz>
        Moseo ekkoleris kontra&ubreve; la <tld/>estroj, la milestroj kaj
        centestroj, kiuj venis el la <tld/>o
        <fnt><bib>MT</bib>, <lok>&Nom; 31:14</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stab.0o.MIL">stabo <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="br">penn-brezel</trd>
  <trd lng="fr">chef de <ind>guerre</ind></trd>
  <trdgrp lng="nl">
    <trd>legeraanvoerder</trd>,
    <trd>bevelhebber</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o;&c_je;&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="milit.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="milit.0isto.MIL">
    <dif>
      Homo, kiu profesie a&ubreve; rekrute <tld/>as:
      <ekz>
        la fortika&jcirc;oj brulas per fajro, kaj la <tld/>istojn atakis
        teruro
        <fnt><bib>MT</bib>, <lok>&Jer; 51:32</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="soldat.0o.militisto">soldato <sncref/></ref>
    <refgrp tip="sub">
      <ref cel="oficir.0o.MIL">oficiro</ref>,
      <ref cel="oficir.sub0o">suboficiro</ref>
    </refgrp>
  </snc>
  <adm>
    * Kiel oni nomas la militistojn, kiuj estas nek oficiroj, nek
    suboficiroj
      (fr: militaires du rang, usone: enlisted personnel), kelkaj el ili
      tamen
      estas ranguloj (ekz-e kaporalo)?
  </adm>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_ja;&c_r;</trd>,
    <trd>&c_v;&c_a;&c_je;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>brezelour</trd>,
    <trd>kadour</trd>
  </trdgrp>
  <trd lng="de">Milit&auml;r</trd>
  <trd lng="es">militar</trd>
  <trdgrp lng="hu">
    <trd>harcos</trd>,
    <trd>katona</trd>
  </trdgrp>
  <trd lng="nl">beroeps<ind>militair</ind></trd>
  <trdgrp lng="fr">
    <trd>militaire<klr tip="ind"> (subst.)</klr></trd>,
    <trd>soldat<klr tip="ind"> (militaire)</klr></trd>
  </trdgrp>
  <trd lng="sv">milit&auml;r</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_i;&c_n;</trd>,
    <trd>&c_v;&c_o;&c_je;&c_n;&c_n;&c_y;&c_j;<klr tip="amb">
(&c_s;&c_u;&c_shch;.)</klr></trd>
  </trdgrp>
  <trd lng="vo">kligel</trd>
</drv>

<drv mrk="milit.0istaro">
  <kap><tld/>istaro</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">POL</uzo>
  <snc mrk="milit.0istaro.MIL">
    <dif>
      La <ref tip="prt" cel="milit.0isto"><tld/>istoj</ref> kiel
      grupo a&ubreve; klaso a&ubreve;
      <ref tip="super" cel="stat.0o.rango">stato</ref> de personoj:
      <ekz>
        la Bizancia ikonrompismo estis konflikto inter la
        mona&hcirc;aro kaj <tld/>istaro;
      </ekz>
      <ekz>
        en la tempo, kiam vi staris apude, kiam fremduloj kondukis
        lian <tld/>istaron en kaptitecon kaj aligentuloj eniris en
        liajn pordegojn kaj lotis pri Jerusalem, vi anka&ubreve; estis
        kiel unu el ili
        <fnt><bib>MT</bib>, <lok>&Oba; 1:11</lok></fnt>;
      </ekz>
      <ekz>
        kaj la Eternulo ektondras per sia vo&ccirc;o anta&ubreve;
        sia armeo,
        &ccirc;ar lia <tld/>istaro estas tre granda, kaj la plenumantoj
        de lia vorto estas potencaj
        <fnt><bib>MT</bib>, <lok>&Joe; 2:11</lok></fnt>;
      </ekz>
      <ekz>
        poste liaj filoj sin armos kaj kolektos grandan
        <tld/>istaron
        <fnt><bib>MT</bib>, <lok>&Dan; 11:10</lok></fnt>;
      </ekz>
      <ekz>
        jen staras virino kun menso de infano, kun entuziasmo kaj
        kredo, &ndash;
        la standardon &scirc;i portas anta&ubreve; la batalanta
        <tld/>istaro
        kaj alportas al sia patrujo venkojn kaj savon
        <fnt><bib>Fab3</bib>, <lok>dorna vojo de la honoro</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      <ref cel="arme.0o.sxtata">Armeo</ref> estas <tld/>istaro
      organizita kaj ekipita por batalado.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_je;&c_n;&c_n;&c_y;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_j;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_j;&c_s;&c_k;&c_o;&c_v;&c_a;&c_je;
&c_s;&c_a;&c_s;&c_l;&c_o;&c_w;&c_je;</trd>
  </trdgrp>
  <trd lng="br">arme</trd>
  <trd lng="de">Milit&auml;r</trd>
  <trd lng="es">ej&eacute;rcito</trd>
  <trd lng="fr">arm&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>hadsereg</trd>,
    <trd>hadn&eacute;p</trd>
  </trdgrp>
  <trd lng="nl">leger</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_v;&c_o;&c_j;&c_s;&c_k;&c_o;</trd>,
    <trd>&c_v;&c_o;&c_je;&c_n;&c_n;&c_o;&c_je;
&c_s;&c_o;&c_s;&c_l;&c_o;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="milit.al0i">
  <kap><ofc>*</ofc>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="milit.al0i.MIL">
    <ref tip="dif" cel="konker.0i">konkeri</ref>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      Sed kion mi faru por al<tld/>i simpation?
      <fnt>
      <aut>Julio BAGHY</aut>, <vrk>drameto sur la monto</vrk>,
      <lok>NLR 2:2</lok>
      </fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_v;&c_a;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">gounit</trd>
  <trdgrp lng="hu">
    <trd>kiv&iacute;v</trd>,
    <trd>megh&oacute;d&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">veroveren</trd>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_o;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="milit.antaux0a">
  <kap>anta&ubreve;<tld/>a</kap>
  <snc>
    <dif>
      Rilata al tempo anta&ubreve;anta la koncernan <tld/>on, okazinta
      anta&ubreve; &gcirc;i:
      <ekz>
        dum du horoj &scirc;i estis sensukcese ser&ccirc;anta inter
        duonputrinta forbalaa&jcirc;o iun anta&ubreve;<tld/>an libron
        pri la berlina metropoliteno
        <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 4a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_v;&c_a;&c_je;&c_n;&c_n;&c_y;</trd>
  <trd lng="fr">d'<ind>avant-guerre</ind></trd>
  <trd lng="hu">h&aacute;bor&uacute; el&odblac;tti</trd>
  <trd lng="nl">vooroorlogs</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_v;&c_o;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_d;&c_v;&c_o;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="milit.kruc0o">
  <kap>kruc<tld/>o, kruc<tld/>iro</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="milit.kruc0o.MIL">
    <dif>
      &Ccirc;iu el la militiroj (inter proksimume jaroj 1100 kaj 1300),
      kiujn faris aliancaj kristanaj e&ubreve;ropanoj kontra&ubreve;
precipe islamaj
      orient-mediteraneaj regnoj, oficiale por akiri Jerusalemon kiel
      sanktan urbon.
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>brezel ar Groaz</trd>,
    <trd>kroaziadeg</trd>
  </trdgrp>
  <trd lng="de">Kreuzzug</trd>
  <trd lng="fr">croisade</trd>
  <trd lng="es">cruzada</trd>
  <trd lng="nl">kruistocht</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_s;&c_t;&c_o;&c_v;&c_y;&c_j;
&c_p;&c_o;&c_h;&c_o;&c_d;</trd>
</drv>

<drv mrk="milit.per0i">
  <kap>per<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="milit.per0i.MIL">
      <ref tip="dif" cel="konker.0i.militakiri">konkeri <sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_v;&c_a;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kiharcol</trd>,
    <trd>megh&oacute;d&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">veroveren</trd>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_o;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="milit.post0a">
  <kap>post<tld/>a</kap>
  <snc>
    <dif>
      Rilata al tempo sekvanta la koncernan <tld/>on, okazanta
      post &gcirc;i:
      <ekz>
        en la unua post<tld/>a n-ro de <ctl>Esperantista
        Laboristo</ctl>,
        ni faris tre kategorian deklaron, kiun hodia&ubreve; ni konfirmas
        <fnt><bib>Lanti</bib>, <lok>nia pozicio</lok></fnt>;
      </ekz>
      <ekz>
        anta&ubreve;<tld/>e Usono &scirc;uldis dolare al
        E&ubreve;ropo 5 miliardojn, post<tld/>e tiu &scirc;uldo
        alii&gcirc;is en kreditora&jcirc;on da 16 miliardoj
        <fnt><bib>Lanti</bib>, <lok>oni petas volontulojn</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_v;&c_a;&c_je;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_mol;&c_l;&c_ja;&c_v;&c_a;&c_je;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">nachkriegs-</trd>
  <trd lng="fr">d'<ind>apr&egrave;s-guerre</ind></trd>
  <trd lng="nl">na<ind>oorlog</ind>s</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_l;&c_je;&c_v;&c_o;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="milit.0akiri">
  <kap><tld/>akiri</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="milit.0akiri.MIL">
    <dif>
      <ref tip="super" cel="akir.0i">Akiri</ref> per milita forto:
      <ekz>
        kaj Moseo sendis, por esplorrigardi Jazeron, kaj ili
        <tld/>akiris &gcirc;iajn vila&gcirc;ojn, kaj forpelis la Amoridojn,
        kiuj tie estis
        <fnt><bib>MT</bib>, <lok>&Nom; 21:32</lok></fnt>;
      </ekz>
      <ekz>
        se vi dum longa tempo sie&gcirc;os urbon, por <tld/>akiri &gcirc;in kaj
        preni &gcirc;in, ne ruinigu &gcirc;iajn arbojn, levante
        kontra&ubreve; ilin hakilon
        <fnt><bib>MT</bib>, <lok>&Rea; 20:19</lok></fnt>;
      </ekz>
      <ekz>
        Holger, la Dano, <tld/>akiris la grandan landon Hindujon
        &gcirc;is la orienta fino de la mondo
        <fnt><bib>Fab3</bib>, <lok>&scirc;tono de la sa&gcirc;uloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_v;&c_a;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_a;&c_p;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="br">aloubi&ntilde;</trd>
  <trd lng="fr">conqu&eacute;rir</trd>
  <trdgrp lng="hu">
    <trd>zs&aacute;km&aacute;nyol</trd>,
    <trd>zs&aacute;km&aacute;nyul ejt</trd>,
    <trd>megh&oacute;d&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">veroveren</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_o;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_v;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="milit.0akiro">
  <kap><tld/>akiro</kap>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="milit.0akiro.ajxo">
    <dif>
      <tld lit="M"/>akira&jcirc;o, poseda&jcirc;o de malamiko akirita per
      milita ago:
      <ekz>
        niaj edzinoj kaj infanoj fari&gcirc;os <tld/>akiro
        <fnt><bib>MT</bib>, <lok>&Nom; 14:32</lok></fnt>;
      </ekz>
      <ekz>
        ili ja trovis kaj dividas <tld/>akiron
        <fnt><bib>MT</bib>, <lok>&Jug; 5:30</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="br">preizh</trd>
    <trdgrp lng="hu">
      <trd>hadizs&aacute;km&aacute;ny</trd>,
      <trd>pr&eacute;da</trd>
    </trdgrp>
    <trd lng="nl">oorlogsbuit</trd>
    <trd lng="ru">&c_d;&c_o;&c_b;&c_y;&c_ch;&c_a;</trd>
  </snc>
  <snc mrk="milit.0akiro.ado">
    <dif>
      <tld lit="M"/>akirado.
    </dif>
    <trd lng="br">preizhadur</trd>
    <trdgrp lng="hu">
      <trd>zs&aacute;km&aacute;nyol&aacute;s</trd>,
      <trd>pr&eacute;d&aacute;l&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">het nemen van <ind>oorlogsbuit</ind></trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_d;&c_a;&c_b;&c_y;&c_ch;&c_a; <klr
tip="amb">(&c_v;&c_a;&c_je;&c_n;&c_n;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_z;&c_a;&c_v;&c_a;&c_jo;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>booty</trd>,
    <trd>loot</trd>,
    <trd>plunder</trd>
  </trdgrp>
  <trd lng="es">bot&iacute;n</trd>
  <trdgrp lng="fr">
    <trd>butin</trd>,
    <trd><ind>prise</ind> de guerre</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_o;&c_je;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="milit.malvarma0o">
  <kap>malvarma <tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">POL</uzo>
  <snc mrk="milit.malvarma0o.MIL">
    <dif>
      Malpaca stato inter malamikoj, sen rektaj bataloj &ndash;
      esprimo uzata tre ofte pri la epoko 1946-1989 dum kiu konkuris
      Usono kaj NATO unuflanke, Sovetio kaj la varsovia pakto aliflanke:
      <ekz>
        komenci&gcirc;is la &leftquot;malvarma <tld/>o&rightquot;
inter la oriento kaj okcidento
        kaj estis malpermesite al li okupi&gcirc;i pri instruado pro lia
        politika kredo
        <fnt><aut>Liu Ling</aut>, <vrk><url
        ref="www.chinareport.com.cn/05e/05e-03k/05e-3k-5.htm">Li tenis
        en
        sia koro la tutan homaron &ndash; Memore al s-ro Bruno
        Vogelmann</url></vrk>, <lok>El Popola &Ccirc;inio,
        2002-08-08</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">brezel yen</trd>
  <trd lng="de">Kalter Krieg</trd>
  <trd lng="en">Cold War</trd>
  <trd lng="fr"><ind>guerre</ind> froide</trd>
  <trd lng="hu">hidegh&aacute;bor&uacute;</trd>
  <trd lng="nl">koude <ind>oorlog</ind></trd>
  <trd lng="ru">&c_h;&c_o;&c_l;&c_o;&c_d;&c_n;&c_a;&c_ja;
<ind>&c_v;&c_o;&c_j;&c_n;&c_a;</ind></trd>
</drv>

<drv mrk="milit.mond0o">
  <kap>mond<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">POL</uzo>
  <snc mrk="milit.mond0o.MIL">
    <dif>
      <ref tip="super" cel="milit.0o.MIL"><tld lit="M"/>o</ref>, kiu
      implikas multajn &scirc;tatojn en diversaj lokoj de la mondo:
      <ekz>
        ni ne devas malatenti la teruran sperton de la mond<tld/>o
        <fnt><bib>Lanti</bib>, <lok>&leftquot;Liberiga Stelo&rightquot; al
        la Verdru&gcirc;uloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_a;&c_ja;
<ind>&c_v;&c_a;&c_j;&c_n;&c_a;</ind></trd>
  <trd lng="br">brezel-bed</trd>
  <trd lng="de">Weltkrieg</trd>
  <trd lng="es">Guerra Mundial</trd>
  <trd lng="fr"><ind>guerre</ind> mondiale</trd>
  <trd lng="hu">vil&aacute;gh&aacute;bor&uacute;</trd>
  <trd lng="nl">wereldoorlog</trd>
  <trd lng="ru">&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_ja;
<ind>&c_v;&c_o;&c_j;&c_n;&c_a;</ind></trd>
</drv>

<drv mrk="milit.unuamond0o">
  <kap>unua mond<tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc mrk="milit.unuamond0o.HIS">
    <dif>
      <tld lit="M"/>o da&ubreve;rinta depende je la landoj de 1914
      &gcirc;is 1918, ekinta en E&ubreve;ropo, vasti&gcirc;inta al Japanujo,
      Usono, &Ccirc;inujo, sudamerikaj &Scirc;tatoj, ka&ubreve;zinta pli
      ol 8 milionoj da hommortoj.
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_je;&c_r;&c_sh;&c_a;&c_ja;
&c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_a;&c_ja;
<ind>&c_v;&c_a;&c_j;&c_n;&c_a;</ind></trd>
  <trd lng="br">brezel-bed kenta&ntilde;</trd>
  <trd lng="de">erster <ind>Weltkrieg</ind></trd>
  <trd lng="fr">premi&egrave;re <ind>guerre</ind> mondiale</trd>
  <trd lng="hu">els&odblac; vil&aacute;gh&aacute;bor&uacute;</trd>
  <trd lng="nl">eerste <ind>wereldoorlog</ind></trd>
  <trd lng="ru">&c_P;&c_je;&c_r;&c_v;&c_a;&c_ja;
&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_ja;
<ind>&c_v;&c_o;&c_j;&c_n;&c_a;</ind></trd>
</drv>

<drv mrk="milit.duamond0o">
  <kap>dua mond<tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc mrk="milit.duamond0o.HIS">
    <dif>
      <tld lit="M"/>o da&ubreve;rinta depende je la landoj de 1939
      &gcirc;is 1945, ekinta en E&ubreve;ropo, vasti&gcirc;inta al diversaj
      e&ubreve;ropaj kolonioj, al orienta Azio, Ameriko, ka&ubreve;zinta
      inter 40 kaj 52 milionoj da hommortoj:
      <ekz>
        post la dua mond<tld/>o li dedi&ccirc;is sin plene al teatra laboro
        <fnt><bib>SkandalJozef</bib>, <lok>pri la a&ubreve;toro</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_D;&c_r;&c_u;&c_g;&c_a;&c_ja;
&c_s;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_a;&c_ja;
<ind>&c_v;&c_a;&c_j;&c_n;&c_a;</ind></trd>
  <trd lng="br">eil brezel-bed</trd>
  <trd lng="de">zweiter <ind>Weltkrieg</ind></trd>
  <trdgrp lng="fr">
    <trd>deuxi&egrave;me <ind>guerre</ind> mondiale</trd>,
    <trd>seconde <ind>guerre</ind> mondiale</trd>
  </trdgrp>
  <trd lng="hu">m&aacute;sodik vil&aacute;gh&aacute;bor&uacute;</trd>
  <trd lng="nl">tweede <ind>wereldoorlog</ind></trd>
  <trd lng="ru">&c_V;&c_t;&c_o;&c_r;&c_a;&c_ja;
&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_ja;
<ind>&c_v;&c_o;&c_j;&c_n;&c_a;</ind></trd>
</drv>
