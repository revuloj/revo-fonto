<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>milit</rad>/i
</kap>
<drv mrk="milit.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="milit.0i.MIL">
    <dif>
      Batali per armiloj kontra&ubreve; fremdlandaj armeoj a&ubreve;
      kontra&ubreve; samnacia malamika partio:
      <ekz>
        du <tld/>as, tria profitas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Krieg f&uuml;hren</trd>
  <trd lng="fr">faire la <ind>guerre</ind></trd>
  <trd lng="nl">oorlog voeren</trd>
  <trd lng="ru">&c_v;&c_o;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">kriga</trd>
  <trd lng="vo">klig&ouml;n</trd>
</drv>

<drv mrk="milit.0o">
  <kap><tld/>o</kap>
  <snc mrk="milit.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Batala agado inter <tld/>antoj:
      <ekz>
        proklami, deklari, fari, gajni, perdi <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">v&aacute;lka</trd>
  <trd lng="de">Krieg</trd>
  <trd lng="en">war</trd>
  <trd lng="fr">guerre</trd>
  <trd lng="nl">oorlog</trd>
  <trd lng="ru">&c_v;&c_o;&c_j;&c_n;&c_a;</trd>
  <trd lng="sv">krig</trd>
  <trd lng="vo">klig</trd>
</drv>

<drv mrk="milit.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Koncernanta la <tld/>on:
      <ekz>
        <tld/>a kanto, rakonto, preparado.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kriegs-</trd>
  <trdgrp lng="fr">
    <trd>guerrier</trd>,
    <trd>martial</trd>,
    <trd>militaire</trd>
  </trdgrp>
  <trd lng="nl">oorlogs-</trd>
  <trd lng="ru">&c_v;&c_o;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="vo">kliga</trd>
</drv>

<drv mrk="milit.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="milit.0isto.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Homo, kiu profesie a&ubreve; porokaze <tld/>as.
    </dif>
    <refgrp tip="sub">
      <ref cel="oficir.0o">oficiro</ref>,
      <ref cel="soldat.0o">soldato</ref>
    </refgrp>
  </snc>
  <trd lng="de">Milit&auml;r</trd>
  <trd lng="nl">militair</trd>
  <trd lng="fr">militaire <klr>(subst.)</klr></trd>
  <trd lng="sv">milit&auml;r</trd>
  <trd lng="ru">&c_v;&c_o;&c_i;&c_n;</trd>
  <trd lng="vo">kligel</trd>
</drv>

<drv mrk="milit.0istaro">
  <kap><tld/>istaro</kap>
  <snc mrk="milit.0istaro.MIL">
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      La <ref tip="prt" cel="milit.0isto"><tld/>stoj</ref> kiel
      grupo a&ubreve; klaso a&ubreve;
      <ref tip="super" cel="stat.0o.rango">stato</ref> de personoj:
      <ekz>
        la Bizancia ikonrompismo estis konflikto inter la
        mona&hcirc;aro kaj <tld/>istaro;
      </ekz>
      <ekz>
        en la tempo, kiam vi staris apude, kiam fremduloj kondukis
        lian <tld/>istaron en kaptitecon kaj aligentuloj eniris en
        liajn pordegojn kaj lotis pri Jerusalem, vi anka&ubreve; estis
        kiel unu el ili
        <fnt><bib>MT</bib>, <lok>&Oba; 1:11</lok></fnt>;
      </ekz>
      <ekz>
        kaj la Eternulo ektondras per sia vo&ccirc;o anta&ubreve;
        sia armeo,
        &ccirc;ar lia <tld/>istaro estas tre granda, kaj la plenumantoj
        de lia vorto estas potencaj
        <fnt><bib>MT</bib>, <lok>&Joe; 2:11</lok></fnt>;
      </ekz>
      <ekz>
        poste liaj filoj sin armos kaj kolektos grandan
        <tld/>istaron
        <fnt><bib>MT</bib>, <lok>&Dan; 11:10</lok></fnt>;
      </ekz>
      <ekz>
        jen staras virino kun menso de infano, kun entuziasmo kaj
        kredo, ---
        la standardon &scirc;i portas anta&ubreve; la batalanta
        <tld/>istaro
        kaj alportas al sia patrujo venkojn kaj savon
        <fnt><bib>Fab3</bib>, <lok>Dorna vojo de la honoro</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      <ref cel="arme.0o.sxtata">Armeo</ref> estas <tld/>istaro
      organizita kaj ekipita por batalado.
    </rim>
  </snc>
  <trd lng="de">Milit&auml;r</trd>
  <trd lng="fr">arm&eacute;e</trd>
  <trd lng="nl">leger</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_v;&c_o;&c_j;&c_s;&c_k;&c_o;</trd>,
    <trd>&c_v;&c_o;&c_je;&c_n;&c_n;&c_o;&c_je;
    &c_s;&c_o;&c_s;&c_l;&c_o;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="milit.antaux0a">
  <kap>anta&ubreve;<tld/>a</kap>
  <snc>
    <dif>
      Rilata al tempo anta&ubreve;anta la koncernan <tld/>on, okazinta
      anta&ubreve; &gcirc;i:
      <ekz>
        dum du horoj &scirc;i estis sensukcese ser&ccirc;anta inter
        duonputrinta forbalaa&jcirc;o iun anta&ubreve;<tld/>an libron
        pri la berlina metropoliteno
        <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 4a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">d'<ind>avant-guerre</ind></trd>
</drv>

<drv mrk="milit.0akiri">
  <kap><tld/>akiri</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="milit.0akiri.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="akir.0i">Akiri</ref> per milita forto:
      <ekz>
        kaj Moseo sendis, por esplorrigardi Jazeron, kaj ili
        <tld/>akiris &gcirc;iajn vila&gcirc;ojn, kaj forpelis la Amoridojn,
        kiuj tie estis
        <fnt><bib>MT</bib>, <lok>&Nom; 21:32</lok></fnt>;
      </ekz>
      <ekz>
        se vi dum longa tempo sie&gcirc;os urbon, por <tld/>akiri &gcirc;in kaj
        preni &gcirc;in, ne ruinigu &gcirc;iajn arbojn, levante
        kontra&ubreve; ilin
        hakilon
        <fnt><bib>MT</bib>, <lok>&Rea; 20:19</lok></fnt>;
      </ekz>
      <ekz>
        Holger, la Dano, <tld/>akiris la grandan landon Hindujon
        &gcirc;is la orienta fino de la mondo
        <fnt><bib>Fab3</bib>, <lok>&Scirc;tono de la sa&gcirc;uloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">veroveren</trd>
  <trd lng="fr">conqu&eacute;rir</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_v;&c_o;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_v;&c_a;&c_t;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="milit.0akiro">
  <kap><tld/>akiro</kap>
  <snc mrk="milit.0akiro.ajxo">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <tld lit="M"/>akira&jcirc;o, poseda&jcirc;o de malamiko akirita per
      milita ago:
      <ekz>
        niaj edzinoj kaj infanoj fari&gcirc;os <tld/>akiro
        <fnt><bib>MT</bib>, <lok>&Nom; 14:32</lok></fnt>;
      </ekz>
      <ekz>
        nur la brutojn ni prenis al ni, kiel <tld/>akira&jcirc;on,
        kaj la raba&jcirc;on el la urboj, kiujn ni <tld/>akiris
      </ekz>
      <ekz>
        ili ja trovis kaj dividas <tld/>akiron
        <fnt><bib>MT</bib>, <lok>&Jug; 5:30</lok></fnt>;
      </ekz>
    </dif>
    <trd lng="fr">butin</trd>
    <trd lng="ru">&c_d;&c_o;&c_b;&c_y;&c_ch;&c_a;</trd>
  </snc>
  <snc mrk="milit.0akiro.ado">
    <dif>
      <tld lit="M"/>akirado.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>booty</trd>,
    <trd>loot</trd>,
    <trd>plunder</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>prise de guerre</trd><trd>butin</trd>
  </trdgrp>
  <trd
  lng="ru">&c_z;&c_a;&c_v;&c_o;&c_je;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="milit.al0i">
  <kap><ofc>*</ofc>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="milit.al0i.MIL">
    <uzo tip="fak">MIL</uzo>
    <ref tip="dif" cel="konker.0i">konkeri</ref>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      Sed kion mi faru por al<tld/>i simpation?
      <fnt>
      <aut>Julio BAGHY</aut>, <vrk>Drameto sur la monto</vrk>,
      <lok>NLR 2:2</lok>
      </fnt>
    </ekz>
  </snc>
  <trd lng="nl">veroveren</trd>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_o;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="milit.duamond0o">
  <kap>dua mond<tld/>o</kap>
  <snc mrk="milit.duamond0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      <tld lit="M"/>o da&ubreve;rinta depende je la landoj de 1939
      &gcirc;is 1945, ekinta en E&ubreve;ropo, vasti&gcirc;inta al diversaj
      e&ubreve;ropaj kolonioj, al orienta Azio, Ameriko, ka&ubreve;zinta
      inter 40 kaj 52 milionoj da hommortoj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>deuxi&egrave;me guerre mondiale</trd>,
    <trd>seconde guerre mondiale</trd>
  </trdgrp>
</drv>

<drv mrk="milit.mond0o">
  <kap>mond<tld/>o</kap>
  <snc mrk="milit.mond0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="milit.0o.MIL"><tld lit="M"/>o</ref>, kiu
      implikas multajn &scirc;tatojn en diversaj lokoj de la mondo:
      <ekz>
        Ni ne devas malatenti la teruran sperton de la mond<tld/>o
        <fnt><bib>Lanti</bib>, <lok>&leftquot;Liberiga Stelo&rightquot; al
        la Verdru&gcirc;uloj</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">guerre mondiale</trd>
</drv>

<drv mrk="milit.per0i">
  <kap>per<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="milit.per0i.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="dif"
      cel="konker.0i.militakiri">konkeri <sncref/></ref>
    </dif>
  </snc>
  <trd lng="nl">veroveren</trd>
  <trd lng="ru">&c_z;&c_a;&c_v;&c_o;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="milit.unuamond0o">
  <kap>unua mond<tld/>o</kap>
  <snc mrk="milit.unuamond0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      <tld lit="M"/>o da&ubreve;rinta depende je la landoj de 1914
      &gcirc;is 1918, ekinta en E&ubreve;ropo, vasti&gcirc;inta al Japanujo,
      Usono, &Ccirc;inujo, sudamerikaj &Scirc;tatoj, ka&ubreve;zinta pli
      ol 8 milionoj da hommortoj.
    </dif>
  </snc>
  <trd lng="fr">premi&egrave;re guerre mondiale</trd>
</drv>
</art>
