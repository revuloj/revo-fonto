<kap>
  <rad>atoni</rad>/o
  <fnt>Z</fnt>
</kap>

<drv mrk="atoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Malvigleco de la organoj:
      <ekz>
        flegmulo malfacile kaj malrapide &scirc;an&gcirc;as unu specon de agado per alia kaj malbone
        sin sentas en novaj nekonataj medioj, se cirkonstancoj estos malfavoraj, li
        povas i&gcirc;i <tld/>a, malenergia
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=44551">Flegmulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_t;&c_a;&c_n;&c_ib;&c_ja;</trd>,
    <trd>&c_b;&c_ja;&c_s;&c_mol;&c_s;&c_ib;&c_l;&c_mol;&c_l;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">atonia</trd>
  <trdgrp lng="cs">
    <trd>chabost</trd>,
    <trd>ochablost</trd>,
    <trd>ztr&aacute;ta nap&ecaron;t&iacute;</trd>
  </trdgrp>
  <trd lng="de">Atonie</trd>
  <trd lng="en">atony</trd>
  <trd lng="es">aton&iacute;a</trd>
  <trd lng="fr">atonie</trd>
  <trd lng="he">&#x5D0;&#x5D9; &#x5D6;&#x5E8;&#x5D9;&#x5D6;&#x5D5;&#x5EA;
  &#x5D1;&#x5D2;&#x5D5;&#x5E3;</trd>
  <trdgrp lng="hu">
    <trd>at&oacute;nia</trd>,
    <trd>petyh&uuml;dts&eacute;g</trd>,
    <trd>renyhes&eacute;g</trd>
  </trdgrp>
  <trd lng="it">atonia</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30C8;&#x30CB;&#x30FC;</trd>,
    <trd>&#x5F1B;&#x7DE9;&#x75C7;  <pr>&#12375;&#x304B;&#12435;&#12375;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_t;&c_o;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_s;&c_l;&c_a;&c_b;&c_l;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_ja;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ochabnutos&tcaron;</trd>,
    <trd>strata nap&auml;tia</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>atoni</trd>,
    <trd>slapphet</trd>,
    <trd>bristande <ind>muskelsp&auml;nning</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_t;&#x43E;&#x43D;&c_ib;&#x44F;</trd>,
    <trd>&c_r;&#x43E;&c_z;&c_s;&#x43B;&c_a;&c_b;&#x43B;&c_je;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43C;&#x43B;&#x44F;&c_v;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>
