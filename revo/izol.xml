<drv mrk="izol.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="izol.0i.KOMUNE">
    <dif>
      <ref tip="vid" cel="apart.0igi">Apartigi</ref> de la aliaj,
      malhelpante &ccirc;iajn komunikojn a&ubreve; rilatojn:
      <ekz>
        <tld/>ita domo de leprulo
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 26:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        malri&ccirc;a vidvino lo&gcirc;is en <tld/>ita dometo
        <fnt><bib>ElektFab</bib>, <lok>Ne&gcirc;ulino kaj Rozulino</lok></fnt>;
      </ekz>
      <ekz>
        mi <tld/>is min en mia privata sanktejo kaj <klr>[&#8230;]</klr> fordonis min al la
        mistikaj influoj de superaj potencoj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>itaj familioj, kiuj renkontante unu alian en la bazaro de
          <nom>Pi-Bailos</nom> povis e&ccirc; ne scii, ke ili estas najbaroj en la dezerto
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        Himal<klr>[ajo]</klr> <tld/>as Nepalon de Tibeto kaj &Ccirc;inio
        <fnt>
          <bib>Kon11</bib>
          <lok>Kial Nepalaj viroj iras labori en arabaj landoj?</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam venas la suferoj, <klr>[&#8230;]</klr> ne <klr>[izolitaj]</klr> ili venas
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni respektis lian <tld/>econ
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sencon de la mesa&gcirc;o ni kaptas la&ubreve; la kunteksto, ne la&ubreve; <tld/>itaj vortoj a&ubreve;
        frazoj
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="izol.0i.ELE">
      <uzo tip="fak">ELE</uzo>
      <dif>
        &Ccirc;esigi &ccirc;ian kontakton kun korpoj kondukantaj la elektron:
        <ekz>
          oni devas zorge <tld/>i la kondukilojn;
        </ekz>
        <ekz>
          Dielektriko <klr>[povas esti]</klr> vitro, uzita por <tld/>i lineojn de altaj
          tensioj, <klr>[...]</klr> plasto, kiel polivinil-klorido, por <tld/>i elektrajn kablojn
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=320533">Dielektriko</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="hu">szigetel</trd>
    </subsnc>
    <subsnc mrk="izol.0i.KEM">
      <uzo tip="fak">KEM</uzo>
      <dif>
        Apartigi korpon for de la &hcirc;emiaj elementoj, kun kiuj
        &gcirc;i estas kombinita:
        <ekz>
          aluminio estas la plej abunda metalo en la tero, sed <nom>&Oslash;rsted</nom>
          povis <tld/>i la metalon nur en 1807
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=384">Aluminio</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="hu">izol&aacute;l</trd>
    </subsnc>
  <trd lng="de">absondern</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="bar.0o">baro</ref>,
    <ref cel="mur.0o">muro</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_ib;&c_z;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_a;&c_s;&c_o;&c_b;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_z;&c_o;&c_l;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="de">isolieren</trd>
  <trd lng="en">isolate</trd>
  <trd lng="es">aislar</trd>
  <trd lng="fr">isoler</trd>
  <trdgrp lng="hu">
    <trd>elszigetel</trd>,
    <trd>elk&uuml;l&ouml;n&iacute;t</trd>,
    <trd>izol&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#23396;&#31435;&#12373;&#12379;&#12427;  <pr>&#12371;&#12426;&#12388;&#12373;&#12379;&#12427;</pr></trd>,
    <trd>&#20999;&#12426;&#38626;&#12377;  <pr>&#12365;&#12426;&#12399;&#12394;&#12377;</pr></trd>,
    <trd>&#38548;&#38626;&#12377;&#12427;  <pr>&#12363;&#12367;&#12426;&#12377;&#12427;</pr></trd>,
    <trd>&#32118;&#32257;&#12377;&#12427;  <pr>&#12380;&#12388;&#12360;&#12435;&#12377;&#12427;</pr></trd>,
    <trd>&#21336;&#38626;&#12377;&#12427;  <pr>&#12383;&#12435;&#12426;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>isoleren</trd>,
    <trd>afzonderen</trd>
  </trdgrp>
  <trd lng="pt">isolar</trd>
  <trd lng="ru">&c_i;&c_z;&c_o;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">isolera</trd>
  <trdgrp lng="sk">
    <trd>izolova&tcaron;</trd>,
    <trd>oddeli&tcaron;</trd>,
    <trd>odl&uacute;&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>uvol&ncaron;ovat</trd>,
    <trd>vylu&ccaron;ovat</trd>,
    <trd>vyv&iacute;jet</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_z;&c_o;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_o;&c_k;&c_r;&c_je;&c_m;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38548;&#32085; <pr>g&eacute;ju&eacute;</pr></trd>,
    <trd>&#38548;&#31163; <pr>g&eacute;l&iacute;</pr></trd>,
    <trd>&#32085;&#32227; <pr>ju&eacute;yu&aacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="izol.0a">
  <kap><tld/>a</kap>
  <snc mrk="izol.0a.izolanta">
    <dif>
      Kiu <tld/>as, malproksimigas de &ccirc;io:
      <ekz>
        li estas <klr>[&#8230;]</klr> en <tld/>a &ccirc;asdomo de la re&gcirc;o
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 18a, maltrankvilo en la re&gcirc;a
        palaco</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_ib;&c_z;&c_a;&c_l;&c_ja;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
    <trd lng="ru">&c_i;&c_z;&c_o;&c_l;&c_ja;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="izol.0a.izolita">
    <dif>
      <tld lit="I"/>ita, trovi&gcirc;anta en <tld/>eco:
      <ekz>
        sian celon <klr>[ili]</klr> atingos pli rapide, ol per <tld/>a
        agado
        <fnt><bib>Paroloj</bib>, <lok>anta&ubreve; la malferma kunveno de UEA
        &ccirc;e la Kvina Kongreso Esperantista en Barcelona
        en la 7a de septembro 1909</lok></fnt>;
      </ekz>
      <ekz>
        vivi <tld/>e, sen amikoj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxton.0a">a&ubreve;tonoma</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_ib;&c_z;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_d;&c_a;&c_s;&c_o;&c_b;&c_l;&c_je;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="fr">isol&eacute;</trd>
    <trdgrp lng="hu">
      <trd>elszigetelt</trd>,
      <trd>k&uuml;l&ouml;n&aacute;ll&oacute;</trd>,
      <trd>mag&aacute;nyos</trd>
    </trdgrp>
    <trd lng="ru">&c_i;&c_z;&c_o;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
    <trdgrp lng="zh">
    <trd>&#23396;&#31435; <pr>g&umacron;l&igrave;</pr></trd>,
    <trd>&#38548;&#32085; <pr>g&eacute;ju&eacute;</pr></trd>,
    <trd>&#23396; <pr>g&umacron;</pr></trd>,
    <trd>&#23396;&#29420; <pr>g&umacron;d&uacute;</pr></trd>,
    <trd>&#23396;&#32085; <pr>g&umacron;ju&eacute;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>izola&ccaron;n&iacute;</trd>,
    <trd>ojedin&ecaron;l&yacute;</trd>,
    <trd>osam&ecaron;l&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>isoliert</trd>,
    <trd>abgeschieden</trd>
  </trdgrp>
  <trd lng="es">aislado</trd>
  <trd lng="nl">ge&iuml;soleerd</trd>
  <trd lng="pt">isolado</trd>
  <trdgrp lng="sk">
    <trd>izola&ccaron;n&yacute;</trd>,
    <trd>osamel&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_ib;&c_z;&c_o;&c_l;&c_ja;&c_c;&c_ib;&c_j;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="izol.0o">
  <kap><tld/>o</kap>
  <snc mrk="izol.0o.KOMUNE">
    <dif>
      Apartigo, senigo je kontaktoj a&ubreve; rilatoj:
      <ekz>
        <ctl>Izolo</ctl> <klr>(poemaro de Kalo&ccirc;ajo)</klr>
        <fnt><aut>K&aacute;lm&aacute;n Kalocsay</aut>, <vrk>Izolo</vrk>, <lok>Roterdamo:
        Universala Esperanto Asocio, 1977</lok></fnt>;
      </ekz>
      <ekz>
        korpo tie &ccirc;i kaj cerbo &gcirc;uas pacon de l' <tld/>o
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk>La Stranga Butiko</vrk>
          <lok>LIBERTEMPO</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_z;&c_a;&c_l;&c_ja;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Isolierung</trd>,
    <trd>Abgeschiedenheit</trd>
  </trdgrp>
  <trd lng="fr">isolement</trd>
  <trd lng="pt">isolamento<klr tip="amb"> (a&ccedil;&atilde;o)</klr></trd>
  <trd lng="ru">&c_i;&c_z;&c_o;&c_l;&c_ja;&c_c;&c_i;&c_ja;</trd>
</drv>

<drv mrk="izol.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      Loko, en kiu iu estas <tld/>ita:
      <ekz>
        &ccirc;iutage la maristo vizitas la belan voja&gcirc;antinon ka&scirc;itan en la holdo,
        <klr>[&#8230;]</klr> &scirc;i ne rezistas, &ccirc;ar <klr>[&#8230;]</klr> iom da amorado rompas la
        monotonecon de &scirc;ia <tld/>ejo
        <fnt>
          <bib>KrB</bib>
          <lok>&Scirc;ipestro rakontas</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fine nun estas starigita je lia oma&gcirc;o <klr>[&#8230;]</klr> monumento, <klr>[&#8230;]</klr>
        fine okazis simbola &leftquot;forkonduko&rightquot; de tiu granda humanisto el la <tld/>ejo
        &gcirc;isnuna
        <fnt>
          <bib>Monato</bib>
          <aut>Zofia Banet-Fornalowa</aut>
          <vrk><url ref="&Monato;/2007/009279.php"
              >Ni renkonti&gcirc;u &ccirc;e la monumento al Korczak</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Isolation <klr>(Ort)</klr></trd>,
    <trd>Ein&ouml;de</trd>
  </trdgrp>
  <trd lng="en">isolation <klr>(place)</klr></trd>
</drv>

<drv mrk="izol.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="izol.0ismo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Doktrino la&ubreve; kiu regno plejeble malmulte interagu kun aliaj regnoj, milite,
      pace, komerce a&ubreve; alimaniere:
      <ekz>
        la &scirc;oguna registaro konservadis 300-jaran <tld/>ismon ene de la longda&ubreve;ra fe&ubreve;disma
        socio
        <fnt>
          <aut>La Doktrina komitato de Oomoto</aut>
          <vrk><url ref="http://www.oomoto.or.jp/Esperanto/esBook/ppdres.html">
          El la Postparolo de "Diaj Revelacioj"</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_z;&c_a;&c_l;&c_ja;&c_c;&c_y;&c_ja;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trd lng="de">Isolationismus</trd>
  <trd lng="fr">isolationnisme</trd>
  <trd lng="ja">&#23396;&#31435;&#20027;&#32681;  <pr>&#12371;&#12426;&#12388;&#12375;&#12421;&#12366;</pr></trd>
  <trd lng="uk">&c_ib;&c_z;&c_o;&c_l;&c_ja;&c_c;&c_ib;&c_o;&c_n;&c_ib;&c_z;&c_m;</trd>
</drv>

<drv mrk="izol.0ita">
  <kap><tld/>ita</kap>
  <snc mrk="izol.0ita.KOMUNE">
    <dif>
      Kiun oni <tld/>is a&ubreve; izolis sin; ial trovi&gcirc;anta
      aparte de aliaj:
      <ekz>
        malpura li estas: li estu <tld/>ita, ekster la tendaro estu lia
        lo&gcirc;ejo
        <fnt><bib>MT</bib>, <lok>&Lev; 13:46</lok></fnt>;
      </ekz>
      <ekz>
        apena&ubreve; povas ekzisti homoj &dash; krom eble lo&gcirc;antoj en plej <tld/>itaj lokoj
        <fnt>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2000/006847.php">OPINIO: Sobraj pensoj de
          sobrulo</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 22</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>isoliert</trd>,
      <trd>abgeschieden</trd>
    </trdgrp>
    <trd lng="hu">elszigetelt</trd>
    <trd lng="ru">&c_i;&c_z;&c_o;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="izol.0ita.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="izol.0ita.punkto">
      <fnt><bib>MatTerm</bib>, <lok>p. 32</lok></fnt>
      <dif>
        <klr>(p.p. <ref cel="punkt.0o.spaco">punkto</ref>
        <frm><k>x</k></frm> de
        <ref cel="spac.topologia0o">topologia spaco</ref>, rilate al
        subaro <frm><g>A</g></frm>)</klr>
        Tia, ke &gcirc;i apartenas al <frm><g>A</g></frm>, sed ne estas
        akumuli&gcirc;a rilate al &gcirc;i (alidire: ekzistas
        <ref cel="cxirka.0ajxo.topologio">&ccirc;irka&ubreve;a&jcirc;o</ref>
        de <frm><k>x</k></frm>, kies nura komuna punkto kun
        <frm><g>A</g></frm> estas <frm><k>x</k></frm> mem):
        <ekz>
          en spaco kun diskreta topologio &ccirc;iu ajn punkto estas
          <tld/>ita rilate al ajna subaro, &gcirc;in enhavanta.
        </ekz>
      </dif>
      <ref tip="ant" cel="akumul.0igxa.punkto">akumuli&gcirc;a</ref>
      <ref tip="super" cel="adher.0a.punkto">adhera</ref>
      <trd lng="de">isoliert<klr tip="ind"> (-er Punkt)</klr></trd>
      <trd lng="en">isolated<klr tip="ind"> (point)</klr></trd>
      <trd lng="fr"><klr tip="ind">(point) </klr>isol&eacute;</trd>
      <trd lng="pl"><klr tip="ind">(punkt) </klr>izolowany</trd>
      <trd lng="ru">&c_i;&c_z;&c_o;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_a;&c_ja;<klr tip="amb"> (&c_t;&c_o;&c_ch;&c_k;&c_a;)</klr></trd>
    </subsnc>
    <subsnc mrk="izol.0ita.vertico">
      <dif>
        <klr>(p.p. vertico de <ref cel="grafe.0o">grafeo</ref>)</klr>
        Tia, ke neniu e&gcirc;o estas al &gcirc;i
        <ref cel="incid.0a.grafee">incida</ref>.
      </dif>
      <trd lng="de">isoliert<klr tip="ind"> (-er Knotenpunkt)</klr></trd>
      <trd lng="en">isolated<klr tip="ind"> (vertex)</klr></trd>
      <trd lng="fr"><klr tip="ind">(sommet) </klr>isol&eacute;</trd>
      <trd lng="pl"><klr tip="ind">(wierzcho&lstroke;ek) </klr>izolowany</trd>
      <trd lng="ru">&c_i;&c_z;&c_o;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_a;&c_ja;<klr tip="amb"> (&c_v;&c_je;&c_r;&c_sh;&c_i;&c_n;&c_a;)</klr></trd>
    </subsnc>
    <rim>
      Supozeble anka&ubreve; la pli malpeza sinonimo <ctl><tld/>a</ctl>,
      uzata ekster matematiko, tre bone ta&ubreve;gus &ccirc;i-kuntekste.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_z;&c_a;&c_l;&c_ja;&c_v;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_a;&c_d;&c_a;&c_s;&c_o;&c_b;&c_l;&c_je;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">izolovan&yacute; <klr>(vrchol grafu)</klr></trd>
  <trd lng="hu">izol&aacute;lt</trd>
  <trdgrp lng="ja">
    <trd>&#23396;&#29420;&#12394;  <pr>&#12371;&#12393;&#12367;&#12394;</pr></trd>,
    <trd>&#23396;&#31435;&#12375;&#12383;  <pr>&#12371;&#12426;&#12388;&#12375;&#12383;</pr></trd>,
    <trd>&#32118;&#32257;&#12375;&#12383;  <pr>&#12380;&#12388;&#12360;&#12435;&#12375;&#12383;</pr></trd>
  </trdgrp>
  <trd lng="pt">isolado</trd>
  <trd lng="sk">izolovan&yacute; <klr>(vrchol grafu)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_z;&c_o;&c_l;&c_mol;&c_o;&c_v;&c_a;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_o;&c_k;&c_r;&c_je;&c_m;&c_l;&c_je;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="izol.0iteco">
  <kap><tld/>iteco</kap>
  <snc mrk="izol.0iteco.KOMUNE">
    <dif>
      Stato de io a&ubreve; iu <tld/>a:
      <ekz>
        la profunda arbara <tld/>iteco
        <fnt><bib>Fab4</bib>, <lok>filino de la mar&ccirc;a re&gcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        ho, kiel mi memoras &ccirc;iun loketon kaj &gcirc;ian sanktan profundan <tld/>itecon
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="for.00ejo">forforejo</ref>,
