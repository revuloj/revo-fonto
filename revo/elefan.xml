<art mrk="$Id: elefan.xml,v 1.28 2023/08/31 11:49:28 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>elefant</rad>/o
</kap>

<drv mrk="elefan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genroj</ref> el familio <ref tip="malprt" cel="elefan2.0oj"><tld/>edoj</ref>, 
      nunaj plej grandaj surteraj mamuloj,
      vivantaj en <ref tip="malprt" cel="afrik.A0o">Afriko</ref> 
      (<trd lng="la">Loxodonta</trd>) kaj <ref tip="malprt" cel="azi.A0o">Azio</ref>
      (<trd lng="la">Elephas</trd>), havantaj longan fortan 
      <ref tip="prt" cel="rostr.0o">rostron</ref>:
      <ekz>fari el mu&scirc;o <tld/>on
        <klr>(troigi)</klr><fnt><bib>PrV</bib></fnt>.</ekz>  
    </dif>
    <ref tip="lst" cel="rostr.0ulo" lst="voko:rostruloj">rostrulo</ref>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_o;&c_n;</trd>
  <trd lng="bg">&c_s;&c_l;&c_o;&c_n;</trd>
  <trdgrp lng="bo">
    <trd>&t_ga;&t_lata;&t_nga;&t_tsheg;&t_cha;&t_drengbu;&t_na;&t_tsheg;</trd>,
    <trd>&t_ga;&t_lata;&t_nga;&t_tsheg;&t_pa;&t_naro;&t_tsheg;</trd>,
    <trd>&t_ga;&t_lata;&t_nga;&t_tsheg;&t_pa;&t_naro;&t_tsheg;&t_cha;&t_drengbu;&t_tsheg;</trd>
  </trdgrp>
  <trd lng="ca">elefant</trd>
  <trd lng="cs">slon</trd>
  <trd lng="de">Elefant</trd>
  <trd lng="en">elephant</trd>
  <trd lng="es">elefante</trd>
  <trd lng="fr">&eacute;l&eacute;phant</trd>
  <trd lng="hu">elef&aacute;nt</trd>
  <trd lng="id">gajah</trd>
  <trd lng="ja">&#x30BE;&#x30A6;  [<ind>&#x305E;&#x3046;</ind>]</trd>
  <trd lng="nl">olifant</trd>
  <trd lng="pl">s&lstroke;o&nacute;</trd>
  <trd lng="pt">elefante</trd>
  <trd lng="ru">&c_s;&c_l;&c_o;&c_n;</trd>
  <trd lng="sk">slon</trd>
  <trd lng="sv">elefant</trd>
  <trd lng="tr">fil</trd>
  <trd lng="uk">&c_s;&#x43B;&#x43E;&#x43D;</trd>
  <trd lng="zh">&#x8C61;<klr> [xi&agrave;ng]</klr></trd>
</drv>

</art>
