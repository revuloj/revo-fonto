<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>elefant</rad>/o
</kap>

<drv mrk="elefan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genroj</ref> el familio <ref
      tip="malprt"
      cel="elefan2.0oj"><tld/>edoj</ref>, 
      nunaj plej grandaj surteraj mamuloj,
      vivantaj en Afriko (<trd lng="la">Loxodonta</trd>) kaj Azio
      (<trd lng="la">Elephas</trd>), havantaj longan fortan rostron:
      <ekz>fari el mu&scirc;o <tld/>on
        <klr>(troigi)</klr><fnt><bib>PrV</bib></fnt>.</ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_l;&c_o;&c_n;</trd>
  <trd lng="bg">&c_s;&c_l;&c_o;&c_n;</trd>
  <trd lng="ca">elefant</trd>
  <trd lng="cs">slon</trd>
  <trd lng="de">Elefant</trd>
  <trd lng="tr">fil</trd>
  <trd lng="en">elephant</trd>
  <trd lng="es">elefante</trd>
  <trd lng="fr">&eacute;l&eacute;phant</trd>
  <trd lng="hu">elef&aacute;nt</trd>
  <trd lng="nl">olifant</trd>
  <trd lng="pl">s&lstroke;o&nacute;</trd>
  <trd lng="pt">elefante</trd>
  <trd lng="ru">&c_s;&c_l;&c_o;&c_n;</trd>
  <trd lng="sv">elefant</trd>
</drv>

</art>
