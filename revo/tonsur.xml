<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>tonsur</rad>/o
</kap>
<drv mrk="tonsur.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Razita ronda&jcirc;o sur la kapverto de katolikaj pastroj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_n;&c_z;&c_u;&c_r;&c_a;</trd>
  <trd lng="de">Tonsur</trd>
  <trd lng="fr">tonsure</trd>
  <trdgrp lng="hu">
    <trd>tonz&uacute;ra</trd>,
    <trd>pilis</trd>
  </trdgrp>
  <trd lng="nl">tonsuur</trd>
  <trdgrp lng="pt">
    <trd>tonsura</trd>,
    <trd>cercilho</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_o;&c_n;&c_z;&c_u;&c_r;&c_a;</trd>
</drv>

<drv mrk="tonsur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on al iu.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>tonsurieren</trd>,
    <trd>eine <ind>Tonsur</ind> scheren</trd>
  </trdgrp>
  <trd lng="hu">tonz&uacute;r&aacute;t v&aacute;g</trd>  
  <trd lng="nl">een <ind>tonsuur</ind> scheren</trd>
  <trdgrp lng="pt">
    <trd>tonsurar</trd>,
    <trd>cercilhar</trd>
  </trdgrp>
</drv>
</art>
