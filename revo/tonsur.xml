<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>tonsur</rad>/o
</kap>
<drv mrk="tonsur.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Razita ronda&jcirc;o sur la kapverto de katolikaj pastroj.
    </dif>
  </snc>
  <trd lng="de">Tonsur</trd>
  <trd lng="fr">tonsure</trd>
  <trd lng="nl">tonsuur</trd>
  <trd lng="ru">&c_t;&c_o;&c_n;&c_z;&c_u;&c_r;&c_a;</trd>
</drv>

<drv mrk="tonsur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on al iu.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>tonsurieren</trd>,
    <trd>eine <ind>Tonsur</ind> scheren</trd>
  </trdgrp>
  <trd lng="nl">een <ind>tonsuur</ind> scheren</trd>
</drv>
</art>
