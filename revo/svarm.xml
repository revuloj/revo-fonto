        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_v;&c_a;&c_r;&c_v;&c_a;&c_c;&c_c;&c_a; <klr>(&c_n;&c_a;&c_t;&c_o;&c_w;&c_p;&c_a;&c_m;)</klr></trd>
