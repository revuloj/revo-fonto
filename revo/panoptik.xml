<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: panoptik.xml,v 1.5 2023/05/28 12:38:59 revo Exp $">
<kap>
  <rad>panoptik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="panoptik.0o">
  <kap><tld/>o</kap>
  <snc mrk="panoptik.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="ekspoz1.0o">Ekspozicio</ref> de historiaj personoj, 
      medicinaj kurioza&jcirc;oj, ekzotikaj popoloj per pupoj a&ubreve; aktoroj:
      <ekz>
        mi estas <klr>...</klr> posedanto de la tutmonde konata migranta
        teatro, mirinda <tld/>o kun moveblaj pupoj
        <fnt><bib>RabistFab</bib></fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> komencis sian artistan karieron en Berlina <tld/>o, kie li dum
        tri jaroj &ccirc;iusemajne demonstris katalepsion, kiu da&ubreve;ris po 3 tagnoktoj
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 8-9 (58-59)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kabine.0o.ekspozicio">kabineto<sncref/></ref> de kurioza&jcirc;oj,
      <ref cel="muze.0o">muzeo</ref>,
      <ref cel="menagx.0o">mena&gcirc;erio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_o;&c_p;&c_t;&c_y;&c_k;&c_u;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>panoptikum</trd>,
    <trd>kabinet kuriozit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Panoptikum</trd>,
    <trd>Wachsfigurenkabinett</trd>,
    <trd>Kuriosit&auml;tenkabinett</trd>
  </trdgrp>
  <trd lng="fr">sc&egrave;ne faite &agrave; l'aide de figurines</trd>
  <trdgrp lng="pl">
    <trd>panoptikum</trd>,
    <trd>gabinet osobliwo&sacute;ci</trd>
  </trdgrp>
  <trd lng="ro">panoptikum</trd>
  <trd lng="uk">&c_p;&c_a;&c_n;&c_o;&c_p;&c_t;&c_i;&c_k;&c_u;&c_m;</trd>
</drv>
</art>

