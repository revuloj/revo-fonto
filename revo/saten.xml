<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: saten.xml,v 1.15 2022/03/26 20:43:11 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>saten</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="saten.0o">
  <kap><tld/>o</kap>
  <snc mrk="saten.0o.cxia">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      &Ccirc;ia <ref tip="super" cel="sxtof.0o.TEKS">&scirc;tofo</ref>
      teksita la&ubreve; <tld/>a <ref cel="teks.0arangxo">teksaran&gcirc;o</ref>,
      kun maldensa nodado de la vefto a&ubreve;
      varpo, tiel ke unu flanko estas glacee glata (je la kosto de
      malplia fortikeco):
      <ekz>
        <ref tip="sub" cel="atlas.0o.TEKS">atlaso</ref> estas silka
        <tld/>o;
      </ekz>
      <ekz>
        pure <ind>kotona <tld/>o</ind>.
        <trd lng="fr">satinette</trd>
        <trd lng="hu">szat&eacute;nkarton</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_c;&c_ib;&c_n;</trd>
    <trd lng="ca">set&iacute;</trd>
    <trdgrp lng="en">
      <trd>satin</trd>,
      <trd>sateen</trd>
    </trdgrp>
    <trd lng="es">sat&eacute;n</trd>
    <trd lng="fr">satin</trd>
    <trd lng="it">satin</trd>
    <trd lng="hu">szat&eacute;n</trd>
    <trd lng="ru">&c_s;&c_a;&c_t;&c_i;&c_n;</trd>
  </snc>
  <snc mrk="saten.0o.atlaso">
    <uzo tip="fak">TEKS</uzo>
    <ref tip="dif" cel="atlas.0o.TEKS">atlaso</ref>
  <trd lng="be">&c_a;&c_t;&c_l;&c_a;&c_s; <klr tip="amb">(&c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_a;)</klr></trd>
  </snc>
  <trd lng="cs">atlas</trd>
  <trd lng="nl">satijn</trd>
  <trd lng="pl">satyna</trd>
  <trd lng="sk">atlas</trd>
  <trd lng="uk">&#x430;&#x442;&#x43B;&#x430;&#x441;</trd>
</drv>
</art>
