<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tulip.xml,v 1.26 2024/05/10 10:43:47 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>tulip</rad>/o
</kap>
<drv mrk="tulip.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="liliac.0oj">liliacoj</ref>
       (<trd lng="la">Tulipa</trd>), ornama vegeta&jcirc;o kun granda
       belkoloraj klo&scirc;forma floro kaj du &gcirc;is ses, malofte pli,
       mangrandaj nedividitaj, glatrandaj folioj:
      <ekz>
        hiacintoj kaj <tld/>oj staris en du longaj vicoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj havis la plej belajn kolorojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        paperaj lanternoj radiis kiel grandaj diafanaj <tld/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/4/4d/Wild_tulip_in_the_steppe_of_Kazakhstan.jpg/450px-Wild_tulip_in_the_steppe_of_Kazakhstan.jpg" prm="CC-BY-SA-3.0">sova&gcirc;a <tld/>o en la kaza&hcirc;a stepo
        <fnt>
          <aut>Carole &ccirc;e Anlga Vikipedio</aut>
          <lok><url ref="&WCW;/File:Wild_tulip_in_the_steppe_of_Kazakhstan.jpg">&WCW;/File:Wild_tulip_in_the_steppe_of_Kazakhstan.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_c;&c_ju;&c_l;&c_mol;&c_p;&c_a;&c_n;</trd>
  <trd lng="bg">&c_l;&c_a;&c_l;&c_je;</trd>
  <trdgrp lng="ca">
     <trd>tulipa</trd>,
     <trd>g&egrave;nere Tulipa</trd>
   </trdgrp>
  <trd lng="cs">tulip&aacute;n</trd>
  <trd lng="de">Tulpe</trd>
  <trd lng="en">tulip</trd>
  <trd lng="es">tulip&aacute;n</trd>
  <trd lng="fa">&a_l;&a_A;&a_l;&a_h;</trd>
  <trd lng="fr">tulipe</trd>
  <trd lng="hu">tulip&aacute;n</trd>
  <trd lng="ja">&#x30C1;&#x30E5;&#x30FC;&#x30EA;&#x30C3;&#x30D7;</trd>
  <trd lng="nl">tulp</trd>
  <trd lng="pl">tulipan</trd>
  <trd lng="pt">tulipa</trd>
  <trd lng="ru">&c_t;&c_ju;&c_l;&c_mol;&c_p;&c_a;&c_n;</trd>
  <trd lng="sk">tulip&aacute;n</trd>
  <trd lng="sv">tulpan</trd>
  <trd lng="uk">&#x442;&#x44E;&#x43B;&#x44C;&#x43F;&#x430;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x90C1;&#x91D1;&#x9999; <pr>y&#xF9;j&#x12B;nxi&#x101;ng</pr></trd>,
    <trd>&#x9B31;&#x91D1;&#x9999; <pr>y&#xF9;j&#x12B;nxi&#x101;ng</pr></trd>
  </trdgrp>
</drv>
</art>
