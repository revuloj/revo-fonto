<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>meteologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="meteol.0o">
  <kap><tld/>o</kap>
  <snc mrk="meteol.0o.SCI">
    <uzo tip="fak">MET</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      <ref tip="lst" cel="scienc.0o.fako"
      lst="voko:sciencoj">Scienco</ref>,
      kiu studas la
      atmosferajn fenomenojn kaj provas konjekti la estontan veteron.
    </dif>
    <refgrp tip="sin">
      <ref cel="meteor2.0o.SCI">meteorologio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_e;&c_a;&c_r;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="ca">meteorologia</trd>
  <trd lng="de">Meteorologie</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;t&eacute;orologie<klr> (science)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>meteorol&oacute;gia</trd>,
    <trd>id&odblac;j&aacute;r&aacute;stan</trd>,
    <trd>l&eacute;gk&ouml;rtan</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>weerkunde</trd>,
    <trd>meteorologie</trd>
  </trdgrp>
  <trd lng="pl">meteorologia</trd>
  <trd lng="pt">meteorologia</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_je;&c_o;&c_r;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="meteol.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="meteol.0isto.SCI">
    <uzo tip="fak">MET</uzo>
    <dif>
      Fakulo pri <ref tip="vid" cel="meteol.0o.SCI"><tld/>o</ref>:
      <ekz>
        Eugeniu Mustea <klr>...</klr> sveda esperantisto
        de la klubo en Norrk&ouml;ping <klr>...</klr> servis
        kiel <tld/>isto en Rumanio kaj Svedio
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;Eugeniu Mustea&rightquot;,
        2008-08-16</lok></fnt>.
      </ekz>
    </dif> 
    <refgrp tip="sin">
      <ref cel="meteolog.0o.SCI">meteologo</ref>,
      <ref cel="meteor2.0isto.SCI">meteorologiisto</ref>,
      <ref cel="meteorolog.0o.SCI">meteorologo</ref>
    </refgrp>
  </snc>
  <trd lng="ca">meteor&ograve;leg</trd>
  <trd lng="fr">m&eacute;t&eacute;orologiste</trd>
</drv>

<drv mrk="meteol.0o.MOM">
  <kap>Monda Organiza&jcirc;o pri <tld lit="M"/>o</kap>
  <mlg>MOM</mlg>
  <snc>
    <uzo tip="fak">MET</uzo>
    <uzo tip="fak">SCI</uzo>
    <fnt><bib>ManPol</bib></fnt>
    <dif>
      Speciala <ref tip="lst" 
cel="organi2.0ajxo" lst="voko:organiza&jcirc;oj">organiza&jcirc;o</ref> de
      <ref tip="malprt" cel="naci.Unuigxintaj_0oj">Unui&gcirc;intaj 
Nacioj</ref>
      por
      mondvasta kunlaborado en instalado de <tld/>aj mezur-stacioj kaj
      inter&scirc;an&gcirc;o de informoj.
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_a;&c_ja; 
<ind>&c_m;&c_e;&c_t;&c_e;&c_a;&c_r;&c_a;&c_l;&c_ja;&c_g;&c_ib;&c_ch;&c_n;&c_a;&c_ja;</ind>
  &c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>Organitzaci&oacute; <ind>Meteorol&ograve;gic</ind>a Mundial</trd>,
    <trd>OMM</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>World <ind>Meteorological</ind> Organisation</trd>,
    <trd>WMO</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>World <ind>Meteorological</ind> Organisation</trd>,
    <trd>WMO</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>Organizaci&oacute;n <ind>Metereol&oacute;gica</ind> Mundial</trd>,
    <trd>OMM</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Organisation <ind>M&eacute;t&eacute;orologique</ind> 
Mondiale</trd>,
    <trd>OMM</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Meteorol&oacute;giai Vil&aacute;gszervezet</trd>,
    <trd>MVO</trd>
  </trdgrp> 
  <trdgrp lng="nl">
    <trd>Wereldorganisatie voor <ind>Meteorologie</ind></trd>,
    <trd>WOM</trd>
  </trdgrp>
  <trd lng="pl">&Sacute;wiatowa Organizacja <ind>Meteorologii</ind></trd>
  <trd lng="pt">Organiza&ccedil;&atilde;o Mundial de 
<ind>Meteorologia</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_V;&c_s;&c_je;&c_m;&c_i;&c_r;&c_n;&c_a;&c_ja; 
<ind>&c_m;&c_je;&c_t;&c_je;&c_o;&c_r;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;</ind>
    &c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_V;&c_M;&c_O;</trd>
  </trdgrp>
</drv>
</art>
