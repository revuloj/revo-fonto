<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>meteologi</rad>/o
</kap>
<drv mrk="meteol.0o">
<kap><tld/>o</kap>
<snc>
<uzo tip="fak">MET</uzo>
<uzo tip="fak">SCI</uzo>
<fnt><bib>PIV1</bib></fnt>
<dif>
<ref tip="super" cel="scienc.0o.fako">Scienco</ref>, kiu studas la atmosferajn fenomenojn kaj provas konjekti la estontan veteron.  
</dif>
</snc>
<trdgrp lng="nl">
    <trd>weerkunde</trd>,
    <trd>meteorologie</trd>
  </trdgrp>
<trd lng="pt">meteorologia</trd>
</drv>
<drv mrk="meteol.0o.MOM">
<kap>Monda Organiza&jcirc;o pri <tld lit="M"/>o</kap>
<mlg>MOM</mlg>
<snc>
<uzo tip="fak">MET</uzo>
<uzo tip="fak">SCI</uzo>
<fnt><bib>ManPol</bib></fnt>
<dif>
Speciala <ref tip="super" cel="organi2.0ajxo">organiza&jcirc;o</ref> de <ref tip="malprt" cel="naci.0o.regno.UN">Unui&gcirc;intaj Nacioj</ref> por mondvasta kunlaborado en instalado de <tld/>aj mezur-stacioj kaj inter&scirc;an&gcirc;o de informoj.
</dif>
</snc>
<trdgrp lng="en">
   <trd>World <ind>Meteorological</ind> Organisation</trd>,
<trd>WMO</trd>
</trdgrp>
<trdgrp lng="es">
   <trd>Organizaci&oacute;n <ind>Metereol&oacute;gica</ind> Mundial</trd>,
<trd>OMM</trd>
</trdgrp>
<trdgrp lng="fr">
   <trd>Organisation <ind>M&eacute;t&eacute;orologique</ind> Mondiale</trd>,
<trd>OMM</trd>
</trdgrp>
<trdgrp lng="nl">
   <trd>Wereldorganisatie voor <ind>Metereologie</ind></trd>,
<trd>WOM</trd>
</trdgrp>
<trd lng="pt">Organiza&ccedil;&atilde;o Mundial de <ind>Meteorologia</ind></trd>
</drv>
</art>

