<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glaciologi.xml,v 1.6 2025/08/20 16:36:42 revo Exp $">
<kap>
  <rad>glaciologi</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>

<drv mrk="glaciologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="glaciologi.0o.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Esplorado kaj scienco pri glacio, pri glaciaj naturaj medioj:
      <ekz>
        <tld/>o de la polusaj regionoj
        <fnt>
          <aut>E. Dudich</aut>
          <vrk>Tersciencoj &ndash; &ccirc;u krizo a&ubreve; novaj perspektivoj?</vrk>
          <lok>Scienca Revuo, 1997:48, p. 30a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_l;&c_ja;&c_c;&c_y;&c_ja;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
    <trd lng="fr">glaciologie</trd>
    <trd lng="pl">glacjologia</trd>
  </snc>
  <trd lng="uk">&c_g;&#x43B;&#x44F;&c_c;&c_ib;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="glaciologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="glaciologi.0isto.SCI">
    <dif>
      <ref tip="sin" cel="glaciolog.0o.SCI">Glaciologo</ref>,
      fakulo pri <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_c;&c_y;&c_jo;&c_l;&c_ja;&c_g;</trd>
  <trd lng="fr">glaciologiste</trd>
  <trd lng="pl">glacjolog</trd>
</drv>
</art>

