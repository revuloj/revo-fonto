<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>frez</rad>/i
</kap>
<drv mrk="frez.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Prilabori, entajli lignon, metalon per <tld/>ilo:
      <ekz><tld/>i foldojn.</ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_z;&c_je;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="frez.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Speciala ilo provizita per diversspecaj dentetoj simile al fajlo,
      muntita sur turni&gcirc;anta akso k per kiu oni mordas lignon, metalojn:
      <ekz>cilindra, dentrada, helico, kava <tld/>ilo.</ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_z;&c_a;</trd>
</drv>
<drv mrk="frez.0masxino">
  <kap><tld/>ma&scirc;ino</kap>
  <snc>
    <dif>
      Ma&scirc;ino por <tld/>i:
      <ekz>porligna, pordentrada <tld/>ma&scirc;ino.</ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_z;&c_je;&c_r;&c_n;&c_y;&c_j; &c_s;&c_t;&c_a;&c_n;&c_o;&c_k;</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>
