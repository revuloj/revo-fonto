<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>frez</rad>/i</kap>
<drv mrk="frez.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Prilabori, entajli lignon, metalon per <tld/>ilo:
      <ekz>
        <tld/>i foldojn.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_e;&c_z;&c_e;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">fr&auml;sen</trd>
  <trd lng="es">fresar</trd>
  <trd lng="fr">fraiser</trd>
  <trd lng="hu">mar <klr tip="ind">(szersz&aacute;mmal)</klr></trd>  
  <trdgrp lng="nl">
    <trd>frezen</trd>,
    <trd>uitfrezen</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_z;&c_je;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="frez.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Speciala ilo provizita per diversspecaj dentetoj simile al fajlo,
      muntita sur turni&gcirc;anta akso kaj per kiu oni mordas lignon, metalojn:
      <ekz>
        cilindra, dentrada, helico, kava <tld/>ilo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_e;&c_z;&c_a;</trd>
  <trd lng="de">Fr&auml;ser</trd>
  <trd lng="es">fresa<klr tip="ind"> (herramienta)</klr></trd>
  <trd lng="fr">fraise<klr tip="ind"> (outil)</klr></trd>
  <trd lng="hu">mar&oacute;<klr tip="ind">(szersz&aacute;m)</klr></trd>  
  <trd lng="nl">frees</trd>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_z;&c_a;</trd>
</drv>

<drv mrk="frez.0masxino">
  <kap><tld/>ma&scirc;ino</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ma&scirc;ino por <tld/>i:
      <ekz>
        porligna, pordentrada <tld/>ma&scirc;ino.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_r;&c_e;&c_z;&c_e;&c_r;&c_n;&c_y; &c_s;&c_t;&c_a;&c_n;&c_o;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Fr&auml;se</trd>,
    <trd>Fr&auml;smaschine</trd>
  </trdgrp>
  <trd lng="es">fresadora</trd>
  <trd lng="fr">fraiseuse</trd>
  <trd lng="hu">mar&oacute;g&eacute;p</trd>  
  <trdgrp lng="nl">
    <trd>frees</trd>,
    <trd>freesmachine</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_r;&c_je;&c_z;&c_je;&c_r;&c_n;&c_y;&c_j; &c_s;&c_t;&c_a;&c_n;&c_o;&c_k;</trd>
</drv>
</art>
