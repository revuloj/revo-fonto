<drv mrk="solen.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="solen.0a.sankta">
    <uzo tip="fak">REL</uzo>
    <dif>
      Okazanta la&ubreve; la sanktaj <ref cel="rit.0o.MOR">rito</ref>j:
      <ekz>
        Rakonti matene pri Via boneco // Kaj nokte pri Via fideleco // <klr>[&#x2026;]</klr>
        Per <tld/>aj sonoj de harpo
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 92:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj tagoj la farunofero
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 46:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi devis alporti dankan oferdonon, hodia&ubreve; mi plenumis mian <tld/>an promeson
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 7:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili venu al la <tld/>a malkovro de la statuo, kiun starigis la re&gcirc;o <nom>Nebukadnecar</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 3:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        sanktigu faston, proklamu <tld/>an kunvenon, kunvoku la maljunulojn kaj &ccirc;iujn
        lo&gcirc;antojn de la lando
        <fnt>
          <bib>MT</bib>
          <lok>Joel 1:14</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ceremo.0a">ceremonia</ref>,
      <ref cel="sankt.0a.REL">sankta<sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="solen.0a.MOR">
     <dif>
       Pompa kaj gravigata per grandaj ceremonioj:
        <ekz>
          ni en <tld/>a formo faros veton
          <fnt>
            <bib>Hamlet</bib>
          </fnt>;
        </ekz>
        <ekz>
          francoj en <tld/>a frako
          <fnt>
            <bib>VivZam</bib>
          </fnt>;
        </ekz>
        <ekz>
          en tiu &ccirc;i <tld/>a momento, mia koro estas plena de io nedifinebla kaj mistera,
          kaj mi sentas la deziron faciligi la koron per ia pre&gcirc;o
          <fnt>
            <bib>VivZam</bib>
          </fnt>;
        </ekz>
        <ekz>
          li skribis &ccirc;i tion per la varma sango de sia koro, <klr>[&#x2026;]</klr> sur la <tld/>a
          rando de la eterneco
          <fnt>
            <bib>Rabistoj</bib>
            <lok>Akto Dua</lok>
          </fnt>;
        </ekz>
        <ekz>
          kiam tiu mallevi&gcirc;o de la suno alproksimi&gcirc;as, <klr>[&#x2026;]</klr> la foriranta tago
          &scirc;ajnas fari&gcirc;i pli <tld/>a kaj pli trankvila
          <fnt>
            <bib>BdV</bib>
          </fnt>;
        </ekz>
        <ekz>
          kun tiu firma kaj <tld/>a decido en la koro ni kriu: vivu, kresku kaj floru nia
          afero
          <fnt>
            <bib>Paroloj</bib>
            <vrk>Parolado anta&ubreve; la Sepa Kongreso Esperantista en Antwerpen en la 21a de a&ubreve;gusto
            1911</vrk>
          </fnt>!
        </ekz>
     </dif>
     <refgrp tip="sin">
       <ref cel="bril">brila</ref>,
       <ref cel="grandi.0a">grandioza</ref>,
       <ref cel="impon.0a">impona</ref>,
       <ref cel="majest.0a">majesta</ref>,
       <ref cel="parad">parada</ref>
     </refgrp>
     <refgrp tip="vid">
       <ref cel="afekt">afekta</ref>,
       <ref cel="akcent">akcenta</ref>,
       <ref cel="bombas">bombasta</ref>,
       <ref cel="emfaz">emfaza</ref>
     </refgrp>
   </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_y;</trd>
  <trd lng="cs">slavnostn&iacute;</trd>
  <trd lng="de">feierlich</trd>
  <trd lng="en">solemn</trd>
  <trd lng="es">solemne</trd>
  <trd lng="fr">solennel</trd>
  <trd lng="hu">&uuml;nnep&eacute;lyes</trd>
  <trdgrp lng="ja">
    <trd>&#x8358;&#x53B3;&#x306A;  <pr>&#x305D;&#x3046;&#x3054;&#x3093;&#x306A;</pr></trd>,
    <trd>&#x76DB;&#x5927;&#x306A;  <pr>&#x305B;&#x3044;&#x3060;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x53B3;&#x7C9B;&#x306A;  <pr>&#x3052;&#x3093;&#x3057;&#x3085;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x304A;&#x3054;&#x305D;&#x304B;&#x306A;</trd>,
    <trd>&#x91CD;&#x3005;&#x3057;&#x3044;  <pr>&#x304A;&#x3082;&#x304A;&#x3082;&#x3057;&#x3044;</pr></trd>,
    <trd>&#x5F62;&#x5F0F;&#x306B;&#x306E;&#x3063;&#x3068;&#x3063;&#x305F; 
    <pr>&#x3051;&#x3044;&#x3057;&#x304D;&#x306B;&#x306E;&#x3063;&#x3068;&#x3063;&#x305F;</pr></trd>,
    <trd>&#x6B63;&#x5F0F;&#x306E;  <pr>&#x305B;&#x3044;&#x3057;&#x304D;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">plechtig</trd>
  <trd lng="pl">uroczysty</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">sl&aacute;vnostn&yacute;</trd>
  <trd lng="sv">h&ouml;gtidlig</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_r;&#x43E;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&#x43D;&c_je;&c_s;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_c;&c_je;&c_r;&c_je;&#x43C;&#x43E;&#x43D;&c_ib;&c_a;&#x43B;&#x44C;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_je;&#x43B;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_i;&c_sh;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6114; <pr>y&#x12B;n</pr></trd>,
    <trd>&#x4FE8;&#x7136; <pr>y&#x1CE;nr&#xE1;n</pr></trd>,
    <trd>&#x513C;&#x7136; <pr>y&#x1CE;nr&#xE1;n</pr></trd>,
    <trd>&#x7A46; <pr>m&#xF9;</pr></trd>,
    <trd>&#x9686;&#x91CD; <pr>l&#xF3;ngzh&#xF2;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="solen.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="solen.0i.MOR">
    <dif>
      <tld lit="S"/>e plenumi, <tld/>e festi, <tld/>e honori:
      <ekz>
        ili <tld/>is la feston de la&ubreve;boj, kiel estas skribite, kaj faris la &ccirc;iutagajn
        bruloferojn
        <fnt>
          <bib>MT</bib>
          <lok>Ezra 3:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is sian edzi&gcirc;on
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i kun vi la Paskan feston
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        fininte la militon <klr>[li]</klr> revenis Romon <klr>[por]</klr> <tld/>i la
        triumfon
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i la alvenon de &scirc;ia patro
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="celebr.0i.MOR">celebri</ref>,
       <ref cel="fest.pri0i.KOMUNE">prifesti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_a; &c_s;&c_mol;&c_v;&c_ja;&c_t;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>oslavovat</trd>,
    <trd>slavit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>feierlich <ind>begehen</ind></trd>,
    <trd><klr>(etwas)</klr> feiern</trd>
  </trdgrp>
  <trd lng="en">to solemnize</trd>
  <trd lng="es">solemnizar</trd>
  <trd lng="fr">c&eacute;l&eacute;brer</trd>
  <trd lng="hu">&uuml;nnepel</trd>
  <trdgrp lng="ja">
    <trd>&#x5100;&#x5F0F;&#x3092;&#x884C;&#x3046;  <pr>&#x304E;&#x3057;&#x304D;&#x3092;&#x304A;&#x3053;&#x306A;&#x3046;</pr></trd>,
    <trd>&#x76DB;&#x5178;&#x3092;&#x884C;&#x3046;  <pr>&#x3082;&#x3066;&#x3093;&#x3092;&#x304A;&#x3053;&#x306A;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>uroczy&sacute;cie obchodzi&cacute;</trd>,
    <trd>&sacute;wi&eogonek;ci&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o; &c_o;&c_t;&c_m;&c_je;&c_ch;&c_a;&c_t;&c_mol;</trd>,
     <trd>&c_p;&c_r;&c_a;&c_z;&c_d;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
  <trd lng="sk">oslavova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_r;&#x43E;&c_ch;&c_i;&c_s;&c_t;&#x43E; &c_v;&c_ib;&c_d;&c_z;&#x43D;&c_a;&c_ch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_v;&#x44F;&c_t;&#x43A;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5E86;&#x795D; <pr>q&#xEC;ngzh&#xF9;</pr></trd>,
    <trd>&#x6176;&#x795D; <pr>q&#xEC;ngzh&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="solen.0o">
  <kap><tld/>o, <var><kap><tld/>a&jcirc;o</kap></var></kap>
  <snc mrk="solen.0o.MOR">
    <dif>
      Grava <tld/>a
      <ref cel="ceremo.0o.solena">ceremonio<sncref/></ref>:
      <ekz>
        festa Kristnaska <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Lasta son&gcirc;o de maljuna kverko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[por]</klr> iom kontribui al la <tld/>a&jcirc;oj <klr>[&#x2026;]</klr> li a&ccirc;etis
        raketojn, krakpizojn kaj plej diversajn fajra&jcirc;ojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[je]</klr> Novjara nokto <klr>[&#x2026;]</klr> komenci&gcirc;is la <tld/>o per kantado
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
       <ekz>
         tra &ccirc;iuj <tld/>a&jcirc;oj li sin tenis tre modeste
         <fnt><bib>VivZam</bib></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="fest.0o">festo</ref>,
       <ref cel="liturg.0o">liturgio</ref>,
       <ref cel="rit.0o.MOR">rito</ref>
     </refgrp>
   </snc>
   <trd lng="be">&c_u;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
   <trdgrp lng="de">
    <trd>Fest</trd>,
    <trd>Feier</trd>
  </trdgrp>
   <trd lng="cs">slavnost</trd>
   <trd lng="en">solemnity</trd>
   <trd lng="es">solemnidad<klr> (evento)</klr></trd>
   <trd lng="fr">solennit&eacute;<klr> (&eacute;v&egrave;nement)</klr></trd>
   <trd lng="hu">&uuml;nnep</trd>
   <trd lng="ja">&#x5F0F;&#x6B21;&#x7B2C;  <pr>&#x3057;&#x304D;&#x3057;&#x3060;&#x3044;</pr></trd>
  <trd lng="nl">plechtigheid</trd>
   <trd lng="pl">uroczysto&sacute;&cacute;</trd>
   <trd lng="ru">&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&#x43E;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&#x43E;</trd>,
    <trd><klr>(&c_v;&c_je;&#x43B;&c_i;&#x43A;&c_je;)</klr> &c_s;&c_v;&#x44F;&c_t;&#x43E;</trd>,
    <trd>&c_c;&c_je;&c_r;&#x43A;. &c_shch;&c_je; &#x43F;&c_r;&c_a;&c_z;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5E86; <pr>q&#xEC;ng</pr></trd>,
    <trd>&#x6176; <pr>q&#xEC;ng</pr></trd>,
    <trd>&#x4EEA;&#x5F0F; <pr>y&#xED;sh&#xEC;</pr></trd>,
    <trd>&#x5100;&#x5F0F; <pr>y&#xED;sh&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="solen.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="solen.0eco.MOR">
    <dif>
      Eco de io <tld/>a:
      <ekz>
        en nigra frako kaj blankaj gantoj, kun tute speciala <tld/>eco <klr>[li]</klr>
        dismetis sur la tablo man&gcirc;ilaron por kvin personoj
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        la juna homo elrekti&gcirc;is kaj diris kun <tld/>eco: &ndash; Amen!
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eco kaj sincera mal&gcirc;ojo ja regis &scirc;in <klr>[&#x2026;]</klr> la tragedio en <nom>Babel</nom>
        nun &scirc;ajnis al &scirc;i jam afero de malproksima pasinteco
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        la aparta <tld/>eco, kiun ka&ubreve;zas la momento
        <fnt>
          <bib>Ret</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>slavnostn&iacute; r&aacute;z</trd>,
    <trd>vzne&scaron;enost</trd>
  </trdgrp>
  <trd lng="de">Feierlichkeit</trd>
  <trd lng="en">solemnity</trd>
  <trd lng="es">solemnidad<klr> (cualidad de solemne)</klr></trd>
  <trd lng="fr">solennit&eacute;</trd>
  <trd lng="hu">&uuml;nnep&eacute;lyess&eacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x8358;&#x53B3;  <pr>&#x3057;&#x3087;&#x3046;&#x3054;&#x3093;</pr></trd>,
    <trd>&#x76DB;&#x5927;&#x3055;  <pr>&#x305B;&#x3044;&#x3060;&#x3044;&#x3055;</pr></trd>,
    <trd>&#x53B3;&#x7C9B;  <pr>&#x3052;&#x3093;&#x3057;&#x3085;&#x304F;</pr></trd>,
    <trd>&#x304A;&#x3054;&#x305D;&#x304B;&#x3055;</trd>
  </trdgrp>
  <trd lng="pl">uroczysty nastr&oacute;j</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>sl&aacute;vnostn&yacute; r&aacute;z</trd>,
    <trd>vzne&scaron;enos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&c_r;&#x43E;&c_ch;&c_i;&c_s;&c_t;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43F;&c_i;&c_sh;&#x43D;&c_ib;&c_s;&c_t;&#x44C; (&c_v;&#x43B;&c_a;&c_s;&c_t;&c_i;&c_v;&c_ib;&c_s;&c_t;&#x44C;)</trd>
  </trdgrp>
</drv>

<drv mrk="solen.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc mrk="solen.0ejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="vid" cel="salon.0ego.ARKI">Salonego</ref>,
      <ref tip="vid" cel="hal.0o.cxambrego">halo<sncref/></ref>
      aran&gcirc;ita kaj ornamita por inde akcepti <tld/>a&jcirc;ojn:
      <ekz>
        la majestaj arboj kun siaj densaj, kvaza&ubreve; tegmentonformantaj bran&ccirc;oj, prezentis
        <tld/>ejon similan al tiaj grandaj temploj
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        permesante al mi partopreni &ccirc;i tie nur en la ofero de pinbran&ccirc;eto, mi tuj
        forlasos &ccirc;i <tld/>ejon post la saluto al vi
        <fnt>
          <aut>Degu&ccirc;i Kijoko, trad. Hu&gcirc;imoto Tacuo</aut>
          <vrk><url ref="http://www.oomoto.or.jp/Esperanto/esSalu/2000kajso.html">
          Saluto &ccirc;e la Granda Festo de Oomoto-Fondintino</url></vrk>
          <lok>2000</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxdit.0o">a&ubreve;ditorio</ref>,
      <ref cel="auxl.0o">a&ubreve;lo</ref>,
      <ref cel="pregx.0ejo">pre&gcirc;ejo</ref>,
      <ref cel="templ.0o.kultejo">templo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_l;&c_ja; &c_w;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_w;</trd>
  <trd lng="de">Festsaal</trd>
  <trd lng="fr">salon d'honneur</trd>
  <trdgrp lng="ja">
    <trd>&#x5F0F;&#x5834;  <pr>&#x3057;&#x304D;&#x3058;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x5927;&#x30DB;&#x30FC;&#x30EB;  <pr>&#x3060;&#x3044;&#x30DB;&#x30FC;&#x30EB;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>aula</trd>,
    <trd>pomieszczenie na  <ind>uroczysto&sacute;ci</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#x43B; &c_d;&#x43B;&#x44F; &c_u;&c_r;&#x43E;&c_ch;&c_i;&c_s;&c_t;&#x43E;&c_s;&c_t;&c_je;&c_j;</trd>,
    <trd>&c_z;&c_a;&#x43B; &c_u;&c_r;&#x43E;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_h; &#x43F;&#x43E;&c_d;&c_ib;&c_j;</trd>,
    <trd>&c_a;&#x43A;&c_t;&#x43E;&c_v;&c_i;&c_j; &c_z;&c_a;&#x43B;</trd>
  </trdgrp>
</drv>
</art>
