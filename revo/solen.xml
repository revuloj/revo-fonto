<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>solen</rad>/a
</kap>
<drv mrk="solen.0a">
<kap><ofc>*</ofc><tld/>a</kap>
<snc>
  <dif>
     Pompa k gravigata per grandaj ceremonioj:  
  <ekz><tld/>a procesio, a&ubreve;dienco;</ekz> 
  <ekz><tld/>a promeso<fnt>Z</fnt>;</ekz> 
  <ekz><tld/>e deklari;</ekz> 
  <ekz>animstato <tld/>a
     <klr>(komprenanta la gravecon de la okazo)</klr>.
  </ekz> 
  </dif>
  </snc>
  <trd lng="de">feierlich</trd>
  <trd lng="nl">plechtig</trd>
  <trd lng="sv">h&ouml;gtidlig</trd>
</drv>
<drv mrk="solen.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Grava <tld/>a ceremonio: 
      <ekz>edzi&gcirc;a <tld/>o.</ekz>
    </dif>
  </snc>
  <trd lng="de">Fest</trd>
  <trd lng="cs">slavnost</trd>
  <trd lng="nl">plechtigheid</trd>
</drv>

<drv mrk="solen.0eco">
<kap><tld/>eco</kap>
<snc>
<dif>
Eco de io <tld/>a. 
</dif>
<refgrp tip="sin">
<ref cel="impon.0a">impona</ref>,
<ref cel="grandi.0a">grandioza</ref>,
<ref cel="majest.0a">majesta</ref>,
<ref cel="bril">brila</ref>,
<ref cel="ceremo">ceremonia</ref>,
<ref cel="etiket">etiketa</ref>,
<ref cel="parad">parada</ref>,
<ref cel="akcent">akcenta</ref>,
<ref cel="emfaz">emfaza</ref>,
<ref cel="bombas">bombasta</ref>,
<ref cel="afekt">afekta</ref>.
</refgrp>
</snc>
<trd lng="de">Feierlichkeit</trd>
</drv>
<drv mrk="solen.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
<tld/>e plenumi, <tld/>e festi, <tld/>e honori: <ekz><tld/>i la venon de iu<fnt>Z</fnt>.
</ekz> 
</dif>
<refgrp tip="sin">
<ref cel="celebr.0i">celebri</ref>.
</refgrp>
</snc>
<trd lng="de">feierlich begehen</trd>
</drv>
</art>
