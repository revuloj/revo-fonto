  <trd lng="fr">son<klr tip="amb"> (audible)</klr></trd>
  <trd lng="hu">hang</trd>
  <trd lng="it">suono</trd>
  <trdgrp lng="ja">
    <trd>&#x97F3;  [<ind>&#x304A;&#x3068;</ind>]</trd>,
    <trd>&#x7269;&#x97F3;  [<ind>&#x3082;&#x306E;&#x304A;&#x3068;</ind>]</trd>,
    <trd>&#x97F3;&#x97FF;  [<ind>&#x304A;&#x3093;&#x304D;&#x3087;&#x3046;</ind>]</trd>,
    <trd>&#x97F3;&#x58F0;  [<ind>&#x304A;&#x3093;&#x305B;&#x3044;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>klank</trd>,
    <trd>geluid</trd>
  </trdgrp>
  <trd lng="ro">sunet</trd>
  <trd lng="ru">&c_z;&c_v;&c_u;&c_k;</trd>
  <trd lng="sk">hl&aacute;ska</trd>
  <trd lng="th">&#xE40;&#xE2A;&#xE35;&#xE22;&#xE07;</trd>
  <trd lng="tok">kalama</trd>
