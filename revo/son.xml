<drv mrk="son.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="son.0i.SON">
    <dif>
      Estigi a&ubreve;deblan aervibradon:
      <ekz>
        barelo malplena <tld/>as plej la&ubreve;te
        <klr>(tiu plej fanfaronas, kiu malplej meritas)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        &ccirc;e la ronda tablo du tondiloj <tld/>is akre
        <fnt><bib>Marta</bib></fnt>;
      </ekz><ekz>
        <tld/>is tamburoj kaj flutoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tio]</klr> <tld/>is kiel kvakado de ranoj,
        tio <tld/>is kiel plenda kriado de birdoj
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la horlo&gcirc;o <tld/>is tri kvaronojn
        <fnt><bib>Fab1</bib>, <lok>vojkamarado</lok></fnt>;
      </ekz><ekz>
        denove ek<tld/>is krakoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Alpa korno <tld/>is profunde melankolie
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ek<tld/>is voko
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz><ekz>
        ek<tld/>is rapidaj pa&scirc;oj
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 24a</lok></fnt>;
      </ekz><ekz>
        tiu &ccirc;i frazo estas nur bele <tld/>anta
        kolekto da vortoj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz><ekz>
        apla&ubreve;doj tondre rul<tld/>is tra la plenega salono.
      </ekz>
    </dif>
    <ref tip="vid" cel="bru.0i.soni">brui</ref>
  </snc>
  <snc mrk="son.0i.impresi">
    <dif>
      A&ubreve;deble iel esprimi&gcirc;i:
      <ekz>
        en la malaltaj tonoj de lia vo&ccirc;o <tld/>is
        kvaza&ubreve; malkontenteco
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        jen la horlo&gcirc;o <tld/>is tri kvaronojn de la dek-dua
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        denove <tld/>os en la urbo la lingvo de <nom>Zamenhof</nom>;
      </ekz><ekz>
        tiu &ccirc;i sama peco en stilo Esperanta <tld/>us
        tiamaniere <klr>[&#x2026;]</klr>
        <fnt>
          <bib>LR</bib>
          <lok>2. La verkaro de Zamenhof</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">klink</trd>
  <trd lng="am">&#x12F5;&#x121D;&#x133D;</trd>
  <trd lng="ar">&a_S;&a_w;&a_t;</trd>
