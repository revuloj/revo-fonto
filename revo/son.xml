<drv mrk="son.0o">
  <kap><tld/>o</kap>
  <snc mrk="son.0o.SON">
    <dif>
      A&ubreve;debla aervibrado:
      <ekz>
        harmoniaj <tld/>oj;
      </ekz>
      <ekz>
        poton taksu la&ubreve; <tld/>o, sinjoron la&ubreve; tono
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi a&ubreve;dos la <tld/>on de trumpeto
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 15:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam vi eka&ubreve;dos la <tld/>on de pa&scirc;oj <klr>[&#x2026;]</klr>, tiam ataku
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 5:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        de malproksime alflugis <tld/>oj de flutoj kaj kornoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        bronzaj <tld/>oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de rulfalanta &scirc;tono
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj de l' pluvo
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>Pluvo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        mia plej alta <tld/>o
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        la pordo sen<tld/>e glitis flanken
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2004/008357.html">La unua nokto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni devas elparoli &ccirc;iun <tld/>on severe
        aparte<fnt><bib>LR</bib>,
          <lok>Respondo 56, Oficiala Gazeto, IV, 1911, p. 222</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[lingvo estas]</klr> sistemo da <tld/>oj por la esprimado de pensoj
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        la fina le&gcirc;o por nia ortografio estos: &leftquot;por unu <tld/>o unu litero&rightquot;
        <fnt>
          <bib>LR</bib>
          <lok>1. Alfabeto</lok>
        </fnt>.
      </ekz>
    </dif>
    <lstref lst="voko:sonoj"><tld/>oj</lstref>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_k;</trd>
  <trd lng="bo">&t_sa;&t_gata;&t_rata;&t_tsheg;</trd>
  <trd lng="cs">hl&aacute;ska</trd>
  <trdgrp lng="de">
    <trd>Ton</trd>,
    <trd>Klang</trd>,
    <trd>Laut</trd>,
    <trd>Schall</trd>
  </trdgrp>
  <trd lng="en">sound</trd>
