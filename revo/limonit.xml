<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: limonit.xml,v 1.20 2023/04/27 10:31:12 revo Exp $">
<kap>
  <rad>limonit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="limonit.0o">
  <kap><tld/>o</kap>
  <snc mrk="limonit.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Varia&jcirc;o de <ref tip="vid" cel="hemati.0o">hematito</ref>, miksa&jcirc;o
      el
      feroksidoj:
      <ekz>
        la grundo de la monto enhavas krudan okron, krome &gcirc;i konsistas
        el argilaj sabloj
        miksitaj kun feroksidoj: ru&gcirc;a hematito kaj bruna <tld/>o
        <fnt><aut>Robert Roze</aut>,
        <vrk><url ref="&Monato;/2000/006945.php">Flagrantaj koloroj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/06, p. 17</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/a/ac/Brauneisenerz_-_Horhausen%2C_Westerwald.jpg/640px-Brauneisenerz_-_Horhausen%2C_Westerwald.jpg"
    prm="CC BY-SA 3.0">multkolora <tld/>o el Horhausen, Westerwald, Germanujo,
      ekpoziciata en la Mineralogia
        Muzeo en Bonn
        <fnt>
          <aut>Ra'ike, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Brauneisenerz_-_Horhausen,_Westerwald.jpg">&WCW;/File:Brauneisenerz_-_Horhausen,_Westerwald.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_m;&c_a;&c_n;&c_ib;&c_t;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_y; &c_zh;&c_a;&c_l;&c_ja;&c_z;&c_mol;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hn&ecaron;del</trd>,
    <trd>limonit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brauneisenstein</trd>,
    <trd>Limonit</trd>
  </trdgrp>
  <trd lng="en">limonite</trd>
  <trd lng="fa">&a_l;&f_y;&a_m;&a_w;&a_n;&f_y;&a_t;</trd>
  <trd lng="fr">limonite</trd>
  <trdgrp lng="hu">
    <trd>barnavas&eacute;rc</trd>,
    <trd>barnavask&odblac;</trd>,
    <trd>limonit</trd>
  </trdgrp>
  <trd lng="nl">bruinijzererts</trd>
  <trd lng="pl">limonit</trd>
  <trdgrp lng="pt">
    <trd>limonita</trd>,
    <trd>estilpnossiderita</trd>,
    <trd>limonita-parda</trd>    
  </trdgrp>
  <trd lng="ro">limonit</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_m;&c_o;&c_n;&c_i;&c_t;</trd>,
    <trd>&c_b;&c_u;&c_r;&c_y;&c_j; &c_zh;&c_je;&c_l;&c_je;&c_z;&c_n;&c_ja;&c_k;</trd>
  </trdgrp>
  <trd lng="sk">limonit</trd>
  <trd lng="uk">&#x43B;&c_ib;&#x43C;&#x43E;&#x43D;&c_ib;&c_t;</trd>
</drv>
</art>
