    <kap>
      <rad>Paramarib</rad>/o
    </kap>

<drv mrk="paramarib.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
  <snc>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="surinam.S0o">Surinamo</ref>:
      <ekz>
        <tld/>o, la &ccirc;efurbo de Surinamo, situas &ccirc;e rivero Surinamo kaj proksimume 15&#8239;km
        de la Atlantika Oceano
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Paramaribo"
              >Paramaribo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Wim M.A. de Smet</nom> <klr>[&#8230;]</klr> en la okdekaj kaj na&ubreve;dekaj jaroj <klr>[&#8230;]</klr>
        estis profesoro en <klr>[&#8230;]</klr> <tld/>o, Surinamo
        <fnt>
          <vrk><url
          ref="http://www.esperantoland.org/eo/kategorio.php?lingvo=eo&amp;rubriko=Esperanto&amp;ekde=30"
              >Forpasis Wim de Smet</url></vrk>
          <lok>Esperantoland, Esperanto-nova&jcirc;oj 2012-03-19</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_P;&c_a;&c_r;&c_a;&c_m;&c_a;&c_r;&c_y;&c_b;&c_a;</trd>
    <trd lng="de">Paramaribo</trd>
    <trd lng="en">Paramaribo</trd>
    <trd lng="fr">Paramaribo</trd>
    <trd lng="pl">Paramaribo</trd>
    <trd lng="ro">Paramaribo</trd>
  </snc>
</drv>

