<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: viktim.xml,v 1.25 2024/06/25 23:25:43 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>viktim</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="viktim.0o">
  <kap><tld/>o</kap>
  <snc mrk="viktim.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Viva esta&jcirc;o oferita al dio.
    </dif>
    <trd lng="el">&sigma;&phi;&alfa_ton;&gamma;&jota;&omikron;</trd>
    <trd lng="en">sacrifice</trd>
    <trd lng="hu">&eacute;l&odblac;&aacute;ldozat</trd>
    <trd lng="pl">ofiara</trd>
  </snc>
  <snc mrk="viktim.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tiu, kiu suferas pro io:
      <ekz>
        la proletaro estis la unua <tld/>o de tiu krizo.
      </ekz>
    </dif>
    <trd lng="el">&theta;&ypsilon_ton;&my;&alfa;</trd>
    <trd lng="en">victim</trd>
    <trd lng="hu">&aacute;ldozat</trd>
  </snc>
  <trd lng="be">&c_a;&c_h;&c_v;&c_ja;&c_r;&c_a;</trd>
  <trd lng="ca">v&iacute;ctima</trd>
  <trd lng="cs">ob&#x11B;&tcaron;</trd>
  <trd lng="de">Opfer</trd>
  <trd lng="es">v&iacute;ctima</trd>
  <trd lng="fr">victime</trd>
  <trdgrp lng="ja">
    <trd>&#12356;&#12369;&#x306B;&#12360;</trd>,
    <trd>&#x72A0;&#29298;  <pr>&#12356;&#12369;&#x306B;&#12360;</pr></trd>,
    <trd>&#x72A0;&#29298;&#32773;  <pr>&#x304E;&#x305B;&#12356;&#12375;&#12419;</pr></trd>,
    <trd>&#x88AB;&#x5BB3;&#32773;  <pr>&#12402;&#x304C;&#12356;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trd lng="nl">slachtoffer</trd>
  <trdgrp lng="pl">
    <trd>ofiara</trd>,
    <trd>pastwa</trd>
  </trdgrp>
  <trd lng="pt">v&iacute;tima</trd>
  <trd lng="ru">&c_zh;&c_je;&c_r;&c_t;&c_v;&c_a;</trd>
  <trd lng="sk">obe&tcaron;</trd>
  <trd lng="uk">&c_zh;&c_je;&c_r;&c_t;&c_v;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x4F24;&#x4EA1; <pr>sh&amacron;ngw&#xE1;ng</pr></trd>,
    <trd>&#x50B7;&#x4EA1; <pr>sh&amacron;ngw&#xE1;ng</pr></trd>,
    <trd>&#x4F24;&#32773; <pr>sh&amacron;ngzh&#x11B;</pr></trd>,
    <trd>&#x50B7;&#32773; <pr>sh&amacron;ngzh&#x11B;</pr></trd>,
    <trd>&#x53D7;&#x5BB3;&#x4EBA; <pr>sh&#xF2;uh&#xE0;ir&#xE9;n</pr></trd>,
    <trd>&#x6B7B;&#x96BE;&#32773; <pr>s&#x1D0;n&#xE1;nzh&#x11B;</pr></trd>,
    <trd>&#x6B7B;&#x96E3;&#32773; <pr>s&#x1D0;n&#xE1;nzh&#x11B;</pr></trd>,
    <trd>&#36935;&#x96BE;&#32773; <pr>y&#xF9;n&#xE0;nzh&#x11B;</pr></trd>,
    <trd>&#36935;&#x96E3;&#32773; <pr>y&#xF9;n&#xE0;nzh&#x11B;</pr></trd>,
    <trd>&#x53D7;&#x5BB3;&#32773; <pr>sh&#xF2;uh&#xE0;izh&#x11B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="viktim.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="viktim.0igi.KOMUNE">
    <dif>
      Igi <tld/>o:
      <ekz>
        mi sentas grandan indignon pro la hodia&ubreve;a stato de
        medicinaj sciencoj, kiu <tld/>igas homan vivon, kaj
        manipulas homan korpon same kiel simplan materion
        <fnt>
          <aut>K. Degu&ccirc;i, trad. A. Ni&scirc;inaga</aut>,
          <vrk><url ref="http://www.oomoto.or.jp/Esperanto/esBiet/njparol.html">
          Respektu homan vivon</url></vrk>,
          <lok>2000</lok>
        </fnt>
      </ekz>.
    </dif>
    <refgrp tip="vid">
      <ref cel="ofer.0i">oferi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_a;&c_h;&c_v;&c_ja;&c_r;&c_a;&c_ju;</ind></trd>,
    <trd>&c_v;&c_ib;&c_k;&c_t;&c_y;&c_m;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">ob&#x11B;tovat</trd>
  <trd lng="fr">victimiser</trd>
  <trdgrp lng="ja">
    <trd>&#12356;&#12369;&#x306B;&#12360;&#x306B;&#12377;&#x308B;</trd>,
    <trd>&#x72A0;&#29298;&#x306B;&#12377;&#x308B;  <pr>&#x304E;&#x305B;&#12356;&#x306B;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">uczyni&cacute;<ind> ofiar&aogonek;</ind></trd>
  <trd lng="sk">obetova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &c_zh;&c_je;&c_r;&c_t;&c_v;&#x43E;&#x44E;</trd>,
    <trd>&#x43C;&c_u;&c_ch;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_s;&#x43B;&c_ib;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x5BB3;&#x4EBA; <pr>h&#xE0;ir&#xE9;n</pr></trd>
</drv>
</art>
