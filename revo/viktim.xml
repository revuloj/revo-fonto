<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>viktim</rad>/o
</kap>
<drv mrk="viktim.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Viva esta&jcirc;o oferita al dio.
    </dif>
    <trd lng="el">&sigma;&phi;&alfa_ton;&gamma;&jota;&omikron;</trd>
    <trd lng="en">sacrifice</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tiu, kiu suferas pro io:
      <ekz>
        la proletaro estis la unua <tld/>o de tiu krizo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="postul.0i">postuli 2</ref>.
    </refgrp>
    <trd lng="el">&theta;&ypsilon_ton;&my;&alfa;</trd>
    <trd lng="en">victim</trd>
  </snc>
  <trd lng="be">&c_a;&c_h;&c_v;&c_ja;&c_r;&c_a;</trd>
  <trd lng="de">Opfer</trd>
  <trdgrp lng="fr">
    <trd>victime</trd>
  </trdgrp>
  <trd lng="nl">slachtoffer</trd>
  <trd lng="pt">v&iacute;tima</trd>
  <trd lng="ru">&c_zh;&c_je;&c_r;&c_t;&c_v;&c_a;</trd>
  <trd lng="sk">obe&tcaron;</trd>
</drv>
</art>
