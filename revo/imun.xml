<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>imun</rad>/a
</kap>
<drv mrk="imun.0a">
  <kap><tld/>a</kap>
  <snc mrk="imun.0a.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Tute ŝirmita kontraŭ infekta malsano.
    </dif>
    <trdgrp lng="be">
      <trd>імунны</trd>,
      <trd>неўспрымальны <klr tip="amb">(да інфэкцыі, хваробы)</klr></trd>
    </trdgrp>
    <trd lng="de">immun</trd>
    <trd lng="en">immune</trd>
    <trdgrp lng="hu">
      <trd>védett<klr tip="ind"> (betegséggel szemben)</klr></trd>,
      <trd>ellenállóképes<klr tip="ind"> (betegséggel szemben)</klr></trd>,
      <trd>immunis</trd>
    </trdgrp>
    <trd lng="nl">immuun</trd>
    <trdgrp lng="ru">
      <trd>невосприимчивый (к болезни)</trd>,
      <trd>имунный</trd>
    </trdgrp>
    <trd lng="sv">immun</trd>
  </snc>
  <snc mrk="imun.0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Ne respondeca antaŭ la leĝo, ŝirmita per leĝa privilegio.
    </dif>
    <trd lng="be">недатыкальны</trd>
    <trd lng="de">immun</trd>
    <trd lng="en">exempt</trd>
    <trdgrp lng="hu">
      <trd>mentes<klr tip="ind">(mentességet élvező)</klr></trd>,
      <trd>mentességet élvező</trd>
    </trdgrp>
    <trd lng="nl">onschendbaar</trd>
    <trd lng="ru">неприкосновенный</trd>
  </snc>
  <trd lng="es">inmune</trd>
  <trdgrp lng="fr">
    <trd>immun</trd>,
    <trd>immuniser</trd>,
    <trd>jouissant de l'<ind>immunité</ind></trd>
  </trdgrp>
  <trd lng="pt">imune</trd>
  <trd lng="ru">имеющий иммунитет</trd>
</drv>

<drv mrk="imun.0eco">
  <kap><tld/>eco</kap>
  <dif>
    Stato de iu <tld/>a:
  </dif>
  <snc mrk="imun.0eco.MED">
    <ekz>
      <uzo tip="fak">MED</uzo>
      <tld/>eco kontraŭ tifoida febro povus esti akirita per serumo.
    </ekz>
    <trdgrp lng="be">
      <trd>імунітэт</trd>,
      <trd>неўспрымальнасьць</trd>
    </trdgrp>
    <trd lng="de">Immunität</trd>
    <trdgrp lng="hu">
      <trd>védettség<klr tip="ind"> (betegséggel szemben)</klr></trd>,
      <trd>ellenállóképesség<klr tip="ind"> (betegséggel szemben)</klr></trd>,
      <trd>immunitás</trd>
    </trdgrp>
    <trd lng="nl">immuniteit</trd>
  </snc>
  <snc mrk="imun.0eco.JUR">
    <ekz>
      <uzo tip="fak">JUR</uzo>
      <tld/>eco de la parlamentanoj.
    </ekz>
    <trdgrp lng="be">
      <trd>імунітэт</trd>,
      <trd>недатыкальнасьць</trd>
    </trdgrp>
    <trd lng="de">Immunität</trd>
    <trd lng="hu">mentesség</trd>
    <trd lng="nl">onschendbaarheid</trd>
    <trd lng="ru">неприкосновенность</trd>
  </snc>
  <trd lng="es">inmunidad</trd>
  <trd lng="en">immunity</trd>
  <trd lng="fr">immunité</trd>
  <trd lng="pt">imunidade</trd>
  <trd lng="ru">иммунитет</trd>
</drv>

<drv mrk="imun.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="imun.0igi.MED">
    <dif>
      Fari iun <tld/>a:
      <ekz>
        <uzo tip="fak">MED</uzo>
        vakcino <tld/>igas kontraŭ variolo.
      </ekz>
    </dif>
    <trd lng="be">імунізаваць</trd>
    <trd lng="de">immunisieren</trd>
    <trd lng="en">immunize</trd>
    <trd lng="fr">immuniser</trd>
    <trdgrp lng="hu">
      <trd>védetté tesz</trd>,
      <trd>immunissá tesz</trd>,
      <trd>immunizál</trd>
    </trdgrp>
    <trd lng="nl">immuniseren</trd>
    <trd lng="ru">иммунизировать</trd>
  </snc>
  <trd lng="es">inmunizar</trd>
  <trd lng="pt">imunizar</trd>
</drv>
<adm>
  Enkonduki drv-ojn de la tipo rust~a kaj la
  sencovastigon de ~a, kiun tio implicas.
  <aut>MB</aut>
</adm>
</art>
