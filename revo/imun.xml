<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: imun.xml,v 1.26 2024/09/14 12:11:17 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>imun</rad>/a
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="imun.0a">
  <kap><tld/>a</kap>
  <snc mrk="imun.0a.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Tute &scirc;irmita kontra&ubreve; infekta <ref cel="san.mal0o">malsano</ref>:
      <ekz>
        la <tld/>a sistemo de la organismo
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2007/008991.php">La &ccirc;efaj murdistoj de
          HIV-infektitoj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsanuloj uzis specialajn medikamentojn, kiuj subpremas <tld/>an reagon
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2007/008991.php">La &ccirc;efaj murdistoj de
          HIV-infektitoj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[tie]</klr> unuafoje naski&gcirc;is la viruso, provokanta &ccirc;e homo la tiel nomatan
        sindromon de <tld/>operdo, tio estas aidoso
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2004/008160.html">Afriko kaj aidoso</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la viruso de aidoso utiligas anka&ubreve; tiujn receptorojn por penetri en <tld/>ajn
        &ccirc;elojn <klr>(vd <ref cel="cxel.imun0o"><tld/>&ccirc;elo</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2008/009427.php">&Ccirc;u estas homoj rezistaj al
          aidoso?</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sistem.imun0o">imunsistemo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_ib;&c_m;&c_u;&c_n;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_w;&c_s;&c_p;&c_r;&c_y;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_y; <klr
      tip="amb">(&c_d;&c_a;
      &c_ib;&c_n;&c_f;&c_e;&c_k;&c_c;&c_y;&c_ib;, &c_h;&c_v;&c_a;&c_r;&c_o;&c_b;&c_y;)</klr></trd>
    </trdgrp>
    <trd lng="ca">immune</trd>
    <trd lng="de">immun</trd>
    <trd lng="en">immune</trd>
    <trdgrp lng="hu">
      <trd>v&eacute;dett<klr tip="ind"> (betegs&eacute;ggel szemben)</klr></trd>,
      <trd>ellen&aacute;ll&oacute;k&eacute;pes<klr tip="ind"> (betegs&eacute;ggel
      szemben)</klr></trd>,
      <trd>immunis</trd>
    </trdgrp>
    <trd lng="nl">immuun</trd>
    <trd lng="pl">odporny</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_v;&c_o;&c_s;&c_p;&c_r;&c_i;&c_i;&c_m;&c_ch;&c_i;&c_v;&c_y;&c_j; (&c_k;
      &c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_i;)</trd>,
      <trd>&c_i;&c_m;&c_u;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="sv">immun</trd>
  </snc>
  <snc mrk="imun.0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Ne respondeca anta&ubreve; la le&gcirc;o, &scirc;irmita per le&gcirc;a <ref cel="privil.0a">privilegio</ref>.
    </dif>
    <trd lng="be">&c_n;&c_je;&c_d;&c_a;&c_t;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trdgrp lng="ca">
      <trd>immune</trd>,
      <trd>que gaudeix d'immunitat</trd>
    </trdgrp>
    <trd lng="de">immun</trd>
    <trd lng="en">exempt</trd>
    <trdgrp lng="hu">
      <trd>mentes<klr tip="ind">(mentess&eacute;get &eacute;lvez&odblac;)</klr></trd>,
      <trd>mentess&eacute;get &eacute;lvez&odblac;</trd>
    </trdgrp>
    <trd lng="nl">onschendbaar</trd>
    <trdgrp lng="pl">
      <trd>nietykalny</trd>,
      <trd>korzystaj&aogonek;cy z <ind>immunitetu</ind></trd>,
      <trd>korzystaj&aogonek;cy z <ind>listu &zdot;elaznego</ind></trd>
    </trdgrp>
    <trd
    lng="ru">&c_n;&c_je;&c_p;&c_r;&c_i;&c_k;&c_o;&c_s;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="imun.0a.rezista">
    <dif>
      <ref cel="rezist.0i">Rezista</ref> kontra&ubreve; difektoj, kontra&ubreve; atako:
      <ekz>
        korod-<tld/>a &scirc;talo
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="https://www.monato.be/2003/008042.html">Pinte!</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> bezonas ta&ubreve;gajn vesta&jcirc;ojn, precipe varmajn gantojn kaj
        akvo-<tld/>ajn botojn
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2006/009096.php">Glaciskulptado</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la sola homo, kiu &scirc;ajnis <tld/>a kontra&ubreve; &scirc;ia &ccirc;armo, estis &scirc;ia propra patrino
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ni]</klr> spertis ke malblinda estas amo kaj ne <tld/>a kontra&ubreve; malkompreno
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="https://www.monato.be/2012/010461.php">Soneto por M.</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_t;&c_r;&c_y;&c_v;&c_a;&c_l;&c_y;</trd>
    <trdgrp lng="ca">
      <trd>resistent</trd>,
      <trd>immune</trd>
    </trdgrp>
    <trd lng="de">gefeit</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>chr&aacute;n&ecaron;n&yacute;</trd>,
    <trd>imunn&iacute;</trd>
  </trdgrp>
  <trd lng="es">inmune</trd>
  <trdgrp lng="fr">
    <trd>immun</trd>,
    <trd>immuniser</trd>,
    <trd>jouissant de l'<ind>immunit&eacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#20813;&#30123;&#12398;  <pr>&#12417;&#12435;&#12360;&#12365;&#12398;</pr></trd>,
    <trd>&#20813;&#38500;&#12398;  <pr>&#12417;&#12435;&#12376;&#12423;&#12398;</pr></trd>,
    <trd>&#32784;&#24615;&#12398;&#12354;&#12427;  <pr>&#12383;&#12356;&#12379;&#12356;&#12398;&#12354;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="pt">imune</trd>
  <trd lng="ru">&c_i;&c_m;&c_je;&c_ju;&c_shch;&c_i;&c_j;
  &c_i;&c_m;&c_m;&c_u;&c_n;&c_i;&c_t;&c_je;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>chr&aacute;nen&yacute;</trd>,
    <trd>im&uacute;nny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_m;&c_u;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_p;&c_r;&c_i;&c_j;&c_n;&c_ja;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_t;&c_a;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_shch;&c_o; &c_m;&c_a;&c_jeu; &c_ib;&c_m;&c_u;&c_n;&c_ib;&c_t;&c_je;&c_t;</trd>
  </trdgrp>
  <trd lng="zh">&#20813;&#30123; <pr>mi&#462;ny&igrave;</pr></trd>
</drv>

<drv mrk="imun.0eco">
  <kap><tld/>eco</kap>
  <dif>
    Stato de iu <tld/>a:
  </dif>
  <snc mrk="imun.0eco.MED">
      <uzo tip="fak">MED</uzo>
      <ekz>
        foresto de la <tld/>eco al &ccirc;i tiu viruso <klr>[&#8230;]</klr> akcelis disvastigon de
        epidemio
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2003/008099.html">Birda gripo, &ccirc;u dan&gcirc;era por la
          homo?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fakuloj ekzamenis la <tld/>ecan reagon &ccirc;e la vundorando kaj el tio deduktas,
        ke la morto okazis 10 a&ubreve; 15 minutojn post la vundi&gcirc;o
        <fnt>
          <bib>CKv</bib>
          <lok>4</lok>
        </fnt>.
      </ekz>
    <trd lng="ca">immunitat</trd>
    <trdgrp lng="be">
      <trd>&c_ib;&c_m;&c_u;&c_n;&c_ib;&c_t;&c_e;&c_t;</trd>,
      <trd>&c_n;&c_je;&c_w;&c_s;&c_p;&c_r;&c_y;&c_m;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">Immunit&auml;t</trd>
    <trdgrp lng="hu">
      <trd>v&eacute;detts&eacute;g<klr tip="ind"> (betegs&eacute;ggel
      szemben)</klr></trd>,
      <trd>ellen&aacute;ll&oacute;k&eacute;pess&eacute;g<klr tip="ind"> (betegs&eacute;ggel
      szemben)</klr></trd>,
      <trd>immunit&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">immuniteit</trd>
    <trd lng="pl">odporno&sacute;&cacute;</trd>
  </snc>
  <snc mrk="imun.0eco.JUR">
      <uzo tip="fak">JUR</uzo>
      <ekz>
        <tld/>eco de la parlamentanoj;
      </ekz>
      <ekz>
        li e&ccirc; elposteni&gcirc;is, rezignante pri sia deputita <tld/>eco, por ke la
        prokuratorejo libere enketu
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2011/010311.php">&Ccirc;u an&gcirc;elo a&ubreve; diablo?</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        Usono strikte rifuzas agnoskon de la monda puntribunalo kaj elmar&ccirc;andis
        <tld/>econ por siaj soldatoj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2004/008349.html">Morala degenero</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    <trd lng="ca">immunitat</trd>
    <trdgrp lng="be">
      <trd>&c_ib;&c_m;&c_u;&c_n;&c_ib;&c_t;&c_e;&c_t;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_a;&c_t;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">Immunit&auml;t</trd>
    <trd lng="hu">mentess&eacute;g</trd>
    <trd lng="nl">onschendbaarheid</trd>
    <trdgrp lng="pl">
      <trd>nietykalno&sacute;&cacute;</trd>,
      <trd>immunitet</trd>
    </trdgrp>
    <trd
    lng="ru">&c_n;&c_je;&c_p;&c_r;&c_i;&c_k;&c_o;&c_s;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>imunita</trd>,
    <trd>imunnost</trd>,
    <trd>nedotknutelnost</trd>
  </trdgrp>
  <trd lng="en">immunity</trd>
  <trd lng="es">inmunidad</trd>
  <trd lng="fr">immunit&eacute;</trd>
  <trdgrp lng="ja">
    <trd>&#20813;&#30123;  <pr>&#12417;&#12435;&#12360;&#12365;</pr></trd>,
    <trd>&#20813;&#38500;  <pr>&#12417;&#12435;&#12376;&#12423;</pr></trd>,
    <trd>&#20813;&#36012;  <pr>&#12417;&#12435;&#12379;&#12365;</pr></trd>,
    <trd>&#20813;&#36012;&#29305;&#27177;  <pr>&#12417;&#12435;&#12379;&#12365;&#12392;&#12387;&#12369;&#12435;</pr></trd>,
    <trd>&#32784;&#24615;  <pr>&#12383;&#12356;&#12379;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="pt">imunidade</trd>
  <trd lng="ru">&c_i;&c_m;&c_m;&c_u;&c_n;&c_i;&c_t;&c_je;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>imunita</trd>,
    <trd>nedotknute&lcaron;nos&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_ib;&c_m;&c_u;&c_n;&c_ib;&c_t;&c_je;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#20813;&#30123;&#21147; <pr>mi&#462;ny&igrave;l&igrave;</pr></trd>,
    <trd>&#20813;&#30123; <pr>mi&#462;ny&igrave;</pr></trd>,
    <trd>&#22806;&#20132;&#29305;&#26435; <pr>w&agrave;iji&amacron;ot&egrave;qu&aacute;n</pr></trd>,
    <trd>&#25269;&#25239;&#21147; <pr>d&#464;k&agrave;ngl&igrave;</pr></trd>,
    <trd>&#35905;&#20813; <pr>hu&ograve;mi&#462;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="imun.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="imun.0igi.MED">
    <dif>
      Fari iun <tld/>a:
      <ekz>
        <uzo tip="fak">MED</uzo>
        vakcino <tld/>igas kontra&ubreve; variolo;
      </ekz>
      <ekz>
        dum la 60aj jaroj, pro <tld/>iga programo, tuberkulozo ne plu mortigis kaj oni
        forgesis ties dan&gcirc;erojn
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007148.html">Ne mortis mortiga
          malsano</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        vakcinoj je 70-80 % <tld/>igas la vakcinitojn kontra&ubreve; kancero de la utercerviko
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2010/010159.php">Vakcino preventas
          kancerojn</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="vakcin.0i">vakcini</ref>
    </refgrp>
    <trd lng="be">&c_ib;&c_m;&c_u;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trd lng="de">immunisieren</trd>
    <trd lng="en">immunize</trd>
    <trd lng="fr">immuniser</trd>
    <trdgrp lng="hu">
      <trd>v&eacute;dett&eacute; tesz</trd>,
      <trd>immuniss&aacute; tesz</trd>,
      <trd>immuniz&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">immuniseren</trd>
    <trd lng="pl">uodparnia&cacute;</trd>
    <trd lng="ru">&c_i;&c_m;&c_m;&c_u;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    <trd lng="zh">&#20813;&#30123;&#27861; <pr>mi&#462;ny&igrave;f&#462;</pr></trd>
</snc>
  <trd lng="ca">immunitzar</trd>
  <trd lng="es">inmunizar</trd>
  <trdgrp lng="ja">
    <trd>&#20813;&#30123;&#12395;&#12377;&#12427;  <pr>&#12417;&#12435;&#12360;&#12365;&#12395;&#12377;&#12427;</pr></trd>,
    <trd>&#20813;&#38500;&#12377;&#12427;  <pr>&#12417;&#12435;&#12376;&#12423;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="pt">imunizar</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_m;&c_u;&c_n;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_ib;&c_m;&c_u;&c_n;&c_ib;&c_t;&c_je;&c_t;</trd>
  </trdgrp>
</drv>
<adm>
  Enkonduki drv-ojn de la tipo rust~a kaj la
  sencovastigon de ~a, kiun tio implicas.
  <aut>MB</aut>
</adm>
</art>
