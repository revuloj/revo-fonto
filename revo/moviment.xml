<kap>
  <rad>moviment</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="moviment.0o">
  <kap><tld/>o</kap>
  <snc mrk="moviment.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Senpa&ubreve;za <ref tip="super" cel="part.0o">parto</ref> de longa <ref tip="malprt" cel="kompon.0ajxo">kompona&jcirc;o</ref>:
      <ekz>
        la unua <tld/>o de Ludila Simfonio de <nom>Romberg</nom>
        <fnt>
        <aut>I. Fantom</aut>,
        <vrk>28 infanoj &ccirc;e Brita Kongreso</vrk>,
        <lok>soc.culture.esperanto, 1996-04-15</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni apla&ubreve;dis la 1an <tld/>on de la violona kon&ccirc;erto de <nom>&Ccirc;ajkovskij</nom>
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a; <klr tip="amb">(&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_a;&c_g;&c_a; &c_t;&c_v;&c_o;&c_r;&c_a;)</klr></trd>
  <trd lng="cs">rozd&ecaron;len&iacute; hudebn&iacute;ho d&iacute;la na &ccaron;&aacute;sti</trd>
  <trd lng="de">Satz <klr>(Musik)</klr></trd>
  <trd lng="en">movement</trd>
  <trd lng="es">movimiento</trd>
  <trd lng="fa">&a_m;&a_w;&a_w;&a_m;&a_A;&a_n;</trd>
  <trd lng="fr">mouvement<klr tip="amb"> (musique)</klr></trd>
  <trd lng="ja">&#x697D;&#x7AE0;  <pr>&#x304C;&#x304F;&#x3057;&#x3087;&#x3046;</pr></trd>
  <trdgrp lng="pl">
    <trd>movimento</trd>,
    <trd>tempo <klr>(muz)</klr></trd>
  </trdgrp>
  <trd lng="pt">movimento <klr tip="amb">(m&uacute;s.)</klr></trd>
  <trd lng="ro">movimento</trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_t;&c_mol;<klr tip="amb"> (&c_m;&c_u;&c_z;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_g;&c_o;
  &c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  <trd lng="sk">rozdelenie hudobn&eacute;ho diela na &ccaron;asti</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_o;&c_z;&c_m;&c_ib;&c_r;</trd>,
    <trd>&c_t;&c_je;&c_m;&c_p;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E50;&#x7AE0; <pr>yu&#xE8;zh&#x101;ng</pr></trd>,
    <trd>&#x6A02;&#x7AE0; <pr>yu&#xE8;zh&#x101;ng</pr></trd>
  </trdgrp>
</drv>
