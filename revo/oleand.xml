  <rad>oleandr</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="oleand.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="apocin.0oj">apocinacoj</ref>
      (<trd lng="la">Nerium</trd>), precipe la specio
      <trd lng="la">Nerium oleander</trd>,
      arbeto kun mallar&gcirc;aj folioj kaj grandaj ru&gcirc;aj a&ubreve;
      blankaj floroj.
    </dif>
  </snc>
  <trd lng="de">Oleander</trd>
  <trd lng="nl">oleander</trd>
  <trd lng="ru">&c_o;&c_l;&c_je;&c_a;&c_n;&c_d;&c_r;</trd>
</drv>
</art>
