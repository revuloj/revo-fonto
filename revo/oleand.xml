<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: oleand.xml,v 1.22 2024/01/30 16:09:26 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>oleandr</rad>/o
</kap>
<drv mrk="oleand.0o">
  <kap><tld/>o</kap>
  <snc mrk="oleand.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iamverda <ref tip="lst" cel="arb.0eto" lst="voko:gentianoparencoj">arbeto</ref> kun mallar&gcirc;aj folioj kaj 
      grandaj ru&gcirc;aj a&ubreve;
      blankaj floroj, kreskanta apud okaze seki&gcirc;antaj riveretoj de
      Mediteraneo &gcirc;is &Ccirc;inio, kun brilaj folioj, ofte kultivata en&ccirc;ambre
      en malvarmaj regionoj;
      sola <ref tip="lst" cel="speci.0o.BIO" lst="voko:botanikaj_specioj">specio</ref> (<trd lng="la">Nerium
      oleander</trd>) de <ref tip="super" cel="neri.0o">nerio</ref>:
      <ekz>
        tie estis <klr>...</klr> du grandaj <tld/>aj arboj en florpotoj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_a;&c_n;&c_d;&c_a;&c_r;</trd>
  <trd lng="bg">&c_o;&c_l;&c_je;&c_a;&c_n;&c_d;&c_malmol;&c_r;</trd>
  <trd lng="cs">oleandr</trd>
  <trd lng="de">Oleander</trd>
  <trd lng="fr">laurier-rose</trd>
  <trdgrp lng="hu">
    <trd>leander</trd>,
    <trd>oleander</trd>
  </trdgrp>
  <trd lng="ja">&#x30BB;&#x30A4;&#x30E8;&#x30A6;&#x30AD;&#x30E7;&#x30A6;&#x30C1;&#x30AF;&#x30C8;&#x30A6;</trd>
  <trd lng="nl">oleander</trd>
  <trd lng="pl">oleander</trd>
  <trdgrp lng="pt">
    <trd>espirradeira</trd>,
    <trd>oleandro</trd>
  </trdgrp>
  <trd lng="ro">oleandru</trd>
  <trd lng="ru">&c_o;&c_l;&c_je;&c_a;&c_n;&c_d;&c_r;</trd>
  <trd lng="sk">oleander</trd>
  <trd lng="uk">&#x43E;&#x43B;&#x435;&#x430;&#x43D;&#x434;&#x440;</trd>
  <trdgrp lng="zh">
    <trd>&#x5939;&#x7AF9;&#x6843; <pr>ji&#x101;zh&#xFA;t&#xE1;o</pr></trd>,
    <trd>&#x593E;&#x7AF9;&#x6843; <pr>ji&#x101;zh&#xFA;t&#xE1;o</pr></trd>
  </trdgrp>
</drv>
</art>
