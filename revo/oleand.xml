<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>oleandr</rad>/o
</kap>
<drv mrk="oleand.0o">
  <kap><tld/>o</kap>
  <snc mrk="oleand.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      &Ccirc;iamverda arbeto kun mallar&gcirc;aj folioj kaj grandaj ru&gcirc;aj a&ubreve;
      blankaj floroj kreskanta apud okaze seki&gcirc;antaj riveretoj de
      Mediteraneo &gcirc;is &Ccirc;inio, kun brilaj folioj, ofte kultivata en&ccirc;ambre
      en malvarmaj regionoj,
      sola <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:botanikaj_specioj">specio</ref> (<trd lng="la">Nerium
      oleander</trd>) de <ref tip="super" cel="neri.0o">nerio</ref>:
      <ekz>
        tie estis <klr>...</klr> du grandaj <tld/>aj arboj en florpotoj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_a;&c_n;&c_d;&c_a;&c_r;</trd>
  <trd lng="de">Oleander</trd>
  <trd lng="fr">laurier-rose</trd>
  <trdgrp lng="hu">
    <trd>leander</trd>,
    <trd>oleander</trd>
  </trdgrp>
  <trd lng="nl">oleander</trd>
  <trdgrp lng="pt">
    <trd>espirradeira</trd>,
    <trd>oleandro</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_l;&c_je;&c_a;&c_n;&c_d;&c_r;</trd>
</drv>
</art>
