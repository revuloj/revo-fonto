<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>zelot</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="zelot.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Fervora kaj obstina adepto de ia religio a&ubreve; doktrino.
    </dif>
    <refgrp tip="sin">
      <ref cel="fanati.0a">fanatika</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Zelot</trd>,
    <trd>Fanatiker</trd>,
    <trd>Glaubensfanatiker</trd>,
    <trd>religi&ouml;ser Eiferer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fanatic</trd>,
    <trd>zealot</trd>
  </trdgrp>
  <trd lng="es">celota</trd>
  <trdgrp lng="hu">
    <trd>zel&oacute;ta</trd>,
    <trd>vakbuzg&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zeloot</trd>,
    <trd>ijveraar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_je;&c_l;&c_o;&c_t;</trd>,
    <trd>&c_f;&c_a;&c_n;&c_a;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
