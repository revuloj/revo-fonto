<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gargar</rad>/i
</kap>
<drv mrk="gargar.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Tralavi, finlavi (pp glasoj, teleroj, tukoj) per nura multa akvo,
      sen sapo:
      <ekz>oni devas <tld/>i la flanelon per tre klara akvo.</ekz>
    </dif>
    <trdgrp lng="de">
     <trd>sp&uuml;len</trd>,
     <trd>auswaschen</trd></trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Tralavi la bu&scirc;on a&ubreve; la gor&gcirc;on per iu flua&jcirc;o, kiun oni skuas en &gcirc;i. 
    </dif>
    <trdgrp lng="de">
     <trd>gurgeln</trd>,
     <trd>sp&uuml;len</trd></trdgrp>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Konfuze prononca&ccirc;i ion kvaza&ubreve; <tld/>ante la gor&gcirc;on:
      <ekz>li el<tld/>is kelkajn nekompreneblajn vortojn.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="balbut.0i">balbuti</ref>.
    </refgrp>
  <trd lng="de">gurgeln</trd>
  </snc>
  <trd lng="en">gargle</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_s;&c_k;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="gargar.0ajxo0ilo">
  <kap><tld/>a&jcirc;o, <tld/>ilo</kap>
  <snc>
    <dif>
      Medikamento por <tld/>i al si la gor&gcirc;on.  
    </dif>
  </snc>
</drv>
</art>
