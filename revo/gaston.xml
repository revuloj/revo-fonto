<vortaro>
<art mrk="$Id: gaston.xml,v 1.10 2023/08/31 14:29:51 revo Exp $">
<kap>
  <rad>Gaston</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="gaston.0o">
  <kap><tld/>o</kap>
  <snc mrk="gaston.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        <tld/>o skribis al sia patrino dum la vespero mem
        <fnt>
          <aut>Henri Vallienne</aut>,
          <vrk>Kastelo de Prelongo</vrk>,
          <lok>1907</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_a;&c_s;&c_t;&c_o;&c_n;</trd>
  <trd lng="ca">Gast&oacute;</trd>
  <trd lng="de">Gaston</trd>
  <trd lng="fr">Gaston</trd>
  <trd lng="it">Gastone</trd>
  <trd lng="ja">&#x30AC;&#x30B9;&#x30C8;&#x30F3;</trd>
  <trd lng="pt">Gast&atilde;o</trd>
  <trd lng="ru">&c_G;&c_a;&c_s;&c_t;&c_o;&c_n;</trd>
  <trd lng="uk">&c_Gu;&c_a;&c_s;&c_t;&#x43E;&#x43D;</trd>
</drv>
</art>

