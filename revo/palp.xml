<vortaro>
<art mrk="$Id: palp.xml,v 1.31 2020/11/19 18:33:30 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>palp</rad>/i
</kap>
<drv mrk="palp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="palp.0i.tusxesplori">
    <dif>
      Tu&scirc;esplori per la mano:
      <ekz>
        la blindulo <tld/>is la libron;
      </ekz>
      <ekz>
        <tld/>i per la bastono;
      </ekz>
      <ekz>
        alproksimi&gcirc;u, kaj mi vin <tld/>os, mia filo, &ccirc;u tio estas vi
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 27:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi tra<tld/>is &ccirc;iujn miajn apartena&jcirc;ojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:37</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi trovos la vojon, li diris, <tld/>is &ccirc;irka&ubreve;e, retrovis la &ccirc;ambron
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>;
      </ekz>
      <ekz>
        la unua movo, kiun faris lia mano, estis triangula movo de la dekstra flanko al
        la maldekstra kaj supren al la brusto, por <tld/>i, &ccirc;u li havas ankora&ubreve; siajn
        valora&jcirc;ojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i en mallumo
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 12:25</lok>
        </fnt>
        <klr>(hezitante ser&ccirc;i certecon, ser&ccirc;i sen difinita celo
        kaj metodo)</klr>;
      </ekz>
      <ekz>
        <tld/>senso
        <fnt>
          <bib>Monato</bib>
          <aut>Aldo Grassini</aut>
          <vrk><url ref="https://www.monato.be/2006/008997.php">Arto videbla anka&ubreve; por blinduloj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; iom pli ol 100 jaroj, mortis <nom>Hieronymus Lorm</nom>, inventinto de <tld/>alfabeto
        por surdblinduloj <klr>(kp <ref cel="brajl.0o">brajlo</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Gert Heintze</aut>
          <vrk><url ref="https://www.monato.be/2003/008104.html">Alfabeto por surdblinduloj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_c;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_a;&c_c;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hmatat</trd>,
    <trd>makat</trd>,
    <trd>vy&scaron;et&rcaron;ovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bef&uuml;hlen</trd>,
    <trd>betasten</trd>,
    <trd>abtasten</trd>,
    <trd>palpieren</trd>
  </trdgrp>
  <trd lng="en">feel</trd>
  <trd lng="es">palpar</trd>
  <trd lng="fr">palper</trd>
  <trdgrp lng="hu">
    <trd>tapogad</trd>,
    <trd>megtapogat</trd>,
    <trd>tapint</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tasten</trd>,
    <trd>betasten</trd>,
    <trd>aftasten</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>ta p&aring;</trd>,
    <trd>f&oslash;le p&aring;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>maca&cacute;</trd>,
    <trd>obmacywa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">apalpar</trd>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_shch;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_shch;&c_u;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">mera&tcaron; pulz</trd>
</drv>
 
<drv mrk="palp.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld lit="P"/>ante:
      <ekz>
        <klr>[li]</klr> <tld/>e esploris la tutan tendon
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> <tld/>e ser&ccirc;as sekajn patkukojn en la <klr>[&#8230;]</klr> sako
        <fnt>
          <bib>Monato</bib>
          <aut>Wei Yubin</aut>
          <vrk><url ref="https://www.monato.be/2012/010573.php">La biciklo de mia patro</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        li &ccirc;irka&ubreve; si <tld/>ser&ccirc;is la libron
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="https://www.monato.be/2010/009883.php">Trancporto</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_b;&c_m;&c_a;&c_c;&c_k;&c_a;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_b;&c_m;&c_a;&c_c;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>f&uuml;hlend</trd>,
    <trd>tastend</trd>
  </trdgrp>
  <trd lng="es">a <ind>tientas</ind></trd>
  <trdgrp lng="fr">
    <trd>par t&acirc;tonnement</trd>,
    <trd>&agrave; t&acirc;tons</trd>
  </trdgrp>
  <trd lng="hu">tapogat&oacute;zva</trd>
  <trd lng="nl">op de <ind>tast</ind></trd>
  <trd lng="pl">po <ind>omacku</ind></trd>
  <trd lng="ru">&c_n;&c_a;&c_o;&c_shch;&c_u;&c_p;&c_mol;</trd>
</drv>
 
<drv mrk="palp.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Povanta esti <tld/>ata:
      <ekz>
        la vento estas sentebla sed ne <tld/>ebla;
      </ekz>
      <ekz>
        <klr>(troige)</klr>
        <tld/>ebla <klr>(densega)</klr> mallumo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 10:21</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_n;&c_a; <ind>&c_d;&c_o;&c_t;&c_y;&c_k;</ind></trd>,
      <trd>&c_d;&c_a;&c_t;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>f&uuml;hlbar</trd>,
      <trd>tastbar</trd>
    </trdgrp>
    <trd lng="pl">namacalny</trd>
  </snc>
  <snc mrk="palp.0ebla.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute evidenta, facile rimarkebla:
      <ekz>
        <tld/>ebla pruvo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ebla profito
        <fnt>
          <bib>Fab4</bib>
          <lok>Malnova pre&gcirc;eja sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li subtenas la nunan registaron, &ccirc;ar &gcirc;i alportas <tld/>eblan progreson
        <fnt>
          <bib>Monato</bib>
          <aut>Fidilalao Henriel</aut>
          <vrk><url ref="https://www.monato.be/2006/008777.php">&Scirc;tatlaikeco minacata</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ib;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_y;</trd>,
      <trd>&c_z;&c_a;&c_w;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>namacalny</trd>,
      <trd>uchwytny</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>hmatateln&yacute;</trd>,
    <trd>hmatn&yacute;</trd>
  </trdgrp>
  <trd lng="de">f&uuml;hlbar</trd>
  <trd lng="es">palpable</trd>
  <trd lng="fr">palpable</trd>
  <trdgrp lng="hu">
    <trd>(ki)<ind>tapinthat&oacute;</ind></trd>,
    <trd>&eacute;rz&eacute;kelhet&odblac;</trd>
  </trdgrp>
  <trd lng="nl">tastbaar</trd>
  <trd lng="ru">&c_o;&c_s;&c_ja;&c_z;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>hmatate&lcaron;n&yacute;</trd>,
    <trd>rukolapn&yacute;</trd>
  </trdgrp>
</drv>
 
<drv mrk="palp.0ilo">
  <kap>
    <tld/>ilo
  </kap>
  <snc mrk="palp.0ilo.organo">
    <dif>
      <tld lit="P"/>organo de diversaj bestoj, ekz-e &ccirc;ekape de insektoj:
      <ekz>
        la majskarab<klr>[&#8230;]</klr>oj levis siajn <tld/>ilojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        heliko etendas siajn <tld/>ilojn
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Sinjorino Ajuolo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sub">
     <ref cel="tentakl.0o">tentaklo</ref>,
     <ref cel="anten.0o.ZOO">anteno<sncref/></ref>
  </refgrp>
  <trd lng="de">F&uuml;hler</trd>
</drv>

<drv mrk="palp.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="P"/>i &ccirc;iuflanke, ofte por trovi
      ion, kion ne eblas vidi:
      <ekz>
        ili ser&ccirc;u Dion, se eble ili povus &ccirc;irka&ubreve;<tld/>i
        kaj trovi Lin
        <fnt><bib>NT</bib><lok>&Ago; 17:27</lok></fnt>;
      </ekz>
      <ekz>
        li &ccirc;irka&ubreve;<tld/>is per la manoj kaj ekkonsciis, ke li &scirc;vebis
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Perelin', la Noktarbaro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_m;&c_a;&c_c;&c_a;&c_c;&c_mol;<klr> (&c_z; &c_u;&c_s;&c_ib;&c_h; &c_b;&c_a;&c_k;&c_o;&c_w;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>allseits <ind>bef&uuml;hlen</ind></trd>,
    <trd>&uuml;berall <ind>betasten</ind></trd>,
    <trd>g&auml;nzlich <ind>abtasten</ind></trd>
  </trdgrp>
  <trd lng="es">buscar a tientas</trd>
  <trd lng="fr">chercher &agrave; t&acirc;tons</trd>
  <trd lng="hu">k&ouml;rbetapogat</trd>
  <trd lng="pl"><klr>szuka&cacute;</klr> po <ind>omacku</ind></trd>
  <trdgrp lng="pt">
   <trd>tatear</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_shch;&c_u;&c_p;&c_a;&c_t;&c_mol;<klr> (&c_s;&c_o; &c_v;&c_s;&c_je;&c_h; &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;)</klr></trd>
</drv>
 
