<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>palp</rad>/i
</kap>
<drv mrk="palp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tu&scirc;esplori per la mano:
      <ekz>
        la blindulo <tld/>is la libron;
      </ekz>
      <ekz>
        <tld/>i per la bastono;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i en mallumo<fnt>Z</fnt>
        <klr>(hezitante ser&ccirc;i certecon, ser&ccirc;i sen difinita celo
kaj
        metodo)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_c;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_m;&c_a;&c_c;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bef&uuml;hlen</trd>,
    <trd>betasten</trd>,
    <trd>abtasten</trd>,
    <trd>palpieren</trd>
    <trd lng="pt">apalpar</trd>
  </trdgrp>
  <trd lng="es">palpar</trd>
  <trd lng="en">feel</trd>
  <trdgrp lng="hu">
    <trd>tapogad</trd>,
    <trd>megtapogat</trd>,
    <trd>tapint</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tasten</trd>,
    <trd>betasten</trd>,
    <trd>aftasten</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>ta p&aring;</trd>,
    <trd>f&oslash;le p&aring;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_shch;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_shch;&c_u;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
 
<drv mrk="palp.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld lit="P"/>ante:
      <ekz>
        en la mallumo <tld/>e ser&ccirc;adi siajn vestojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la krepusko de la vivo, kie la homo eraras
        <tld/>e<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_o;&c_b;&c_m;&c_a;&c_c;&c_k;&c_a;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_b;&c_m;&c_a;&c_c;&c_a;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>f&uuml;hlend</trd>,
    <trd>tastend</trd>
  </trdgrp>
  <trd lng="es">a <ind>tientas</ind></trd>
  <trd lng="hu">tapogat&oacute;zva</trd>
  <trd lng="nl">op de <ind>tast</ind></trd>
  <trd lng="ru">&c_n;&c_a;&c_o;&c_shch;&c_u;&c_p;&c_mol;</trd>
</drv>
 
<drv mrk="palp.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Povanta esti <tld/>ata:
      <ekz>
        la vento estas sentebla sed ne <tld/>ebla;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>ebla <klr>(densega)</klr> mallumo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_ch;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;
&c_n;&c_a; <ind>&c_d;&c_o;&c_t;&c_y;&c_k;</ind></trd>,
      <trd>&c_d;&c_a;&c_t;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>f&uuml;hlbar</trd>,
      <trd>tastbar</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute evidenta, facile rimarkebla:
      <ekz>
        <tld/>ebla pruvo;
      </ekz>
      <ekz>
        la kresko de la kursoj estas man<tld/>ebla.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_ib;&c_d;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_y;</trd>,
      <trd>&c_z;&c_a;&c_w;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>
    </trdgrp>
  </snc>
  <trd lng="es">palpable</trd>
  <trdgrp lng="hu">
    <trd>(ki)<ind>tapinthat&oacute;</ind></trd>,
    <trd>&eacute;rz&eacute;kelhet&odblac;</trd>
  </trdgrp>
  <trd lng="nl">tastbaar</trd>
  <trd lng="ru">&c_o;&c_s;&c_ja;&c_z;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>
</drv>
 
<drv mrk="palp.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <tld lit="P"/>i &ccirc;iuflanke, ofte por trovi
      ion, kion ne eblas vidi:
      <ekz>
        ili ser&ccirc;u Dion, se eble ili povus &ccirc;irka&ubreve;<tld/>i
        kaj trovi Lin
        <fnt><bib>NT</bib><lok>&Ago; 17:27</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_m;&c_a;&c_c;&c_a;&c_c;&c_mol;<klr> (&c_z;
&c_u;&c_s;&c_ib;&c_h; &c_b;&c_a;&c_k;&c_o;&c_w;)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>allseits bef&uuml;hlen</trd>,
    <trd>&uuml;berall betasten</trd>,
    <trd>g&auml;nzlich abtasten</trd>
  </trdgrp>
  <trd lng="es">buscar a tientas</trd>
  <trd lng="fr">chercher &agrave; t&acirc;tons</trd>
  <trd lng="hu">k&ouml;rbetapogat</trd>
  <trdgrp lng="pt">
   <trd>tatear</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_shch;&c_u;&c_p;&c_a;&c_t;&c_mol;<klr> (&c_s;&c_o;
&c_v;&c_s;&c_je;&c_h; &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;)</klr></trd>
</drv>
 
<drv mrk="palp.fusx0i">
  <kap>fu&scirc;<tld/>i</kap>
  <snc>
    <dif>
      Difekti palpante, maldelikate palpi.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_d;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_u;&c_sh;&c_ch;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>patouiller</trd>,
    <trd>tripoter<klr tip="ind"> (toucher maladroitement)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;sszetapogat</trd>,
    <trd>fogdos</trd>
  </trdgrp>
</drv>
</art>
