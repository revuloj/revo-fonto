<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc><rad>palp</rad>/i</kap>
<drv mrk="palp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tu&scirc;esplori per la mano:
      <ekz>
        la blindulo <tld/>is la libron;
      </ekz>
      <ekz>
        <tld/>i per la bastono;
      </ekz>
      <ekz>
        <tld/>i en mallumo<fnt>Z</fnt>
        <klr>(f: hezitante ser&ccirc;i certecon, ser&ccirc;i sen difinita celo kaj
        metodo)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>bef&uuml;hlen</trd>,
    <trd>betasten</trd>,
    <trd>abtasten</trd>,
    <trd>palpieren</trd>
  </trdgrp>
  <trd lng="en">feel</trd>
  <trdgrp lng="nl">
    <trd>tasten</trd>,
    <trd>betasten</trd>,
    <trd>aftasten</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_u;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_shch;&c_u;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="palp.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld lit="P"/>ante:
      <ekz>
        en la mallumo <tld/>e ser&ccirc;adi siajn vestojn<fnt>Z</fnt>;
      </ekz>
      (f)
      <ekz>
        en la krepusko de la vivo, kie la homo eraras <tld/>e<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">op de <ind>tast</ind></trd>
  <trd lng="ru">&c_n;&c_a;&c_o;&c_shch;&c_u;&c_p;&c_mol;</trd>
</drv>

<drv mrk="palp.0ebla">
  <kap><tld/>ebla</kap>
  <snc>
    <dif>
      Povanta esti <tld/>ata:
      <ekz>
        la vento estas sentebla sed ne <tld/>ebla
        <klr>(f)</klr>;
      </ekz>
      <ekz>
        <tld/>ebla (densega) mallumo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>f&uuml;hlbar</trd>,
      <trd>tastbar</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tute evidenta, facile rimarkebla:
      <ekz>
        <tld/>ebla pruvo;
      </ekz>
      <ekz>
        la kresko de la kursoj estas man<tld/>ebla.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">tastbaar</trd>
  <trd lng="ru">&c_o;&c_s;&c_ja;&c_z;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>
</drv>

<drv mrk="palp.fusx0i">
  <kap>fu&scirc;<tld/>i</kap>
  <snc>
    <dif>
      Difekti palpante, maldelikate palpi.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>patouiller</trd>,
    <trd>tripoter<klr tip="ind"> (toucher maladroitement)</klr></trd>
  </trdgrp>
</drv>
</art>
