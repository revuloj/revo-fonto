<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: malgra.xml,v 1.26 2020/01/14 15:10:18 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>malgra&ubreve;</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="malgra.0">
   <kap><ofc>*</ofc><tld/></kap>
   <gra><vspec>prepozicio</vspec></gra>
   <snc mrk="malgra.0.KOMUNE">
     <dif>
       Neatentante kontra&ubreve;starantan ies volon a&ubreve; ian
       fizikan baron;
       <ref tip="sin" cel="spit.0eal">spite al</ref>:
       <ekz>
         vi eliris <tld/> mia malpermeso;
       </ekz>
       <ekz>
         vi pacience eltenis en la da&ubreve;ro de dudek jaroj, <tld/> la multaj atakoj kaj
         malagrabla&jcirc;oj
         <fnt>
           <bib>Paroloj</bib>
           <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge</vrk>
           <lok>1907</lok>
         </fnt>;
       </ekz>
       <ekz>
         ni promenis <tld/> la pluvo.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_ja; &c_g;&c_l;&c_je;&c_d;&c_z;&c_ja;&c_ch;&c_y;
     &c_n;&c_a;</trd>,
     <trd>&c_n;&c_je; &c_z;&c_v;&c_a;&c_zh;&c_a;&c_ju;&c_ch;&c_y;
     &c_n;&c_a;</trd>
   </trdgrp>
   <trd lng="bg">&c_v;&c_malmol;&c_p;&c_r;&c_je;&c_k;&c_i;</trd>
   <trd lng="br">daoust da</trd>
   <trd lng="cs">p&rcaron;es</trd>
   <trd lng="de">trotz</trd>
   <trdgrp lng="en">
     <trd>notwithstanding</trd>,
     <trd>in <ind>spite</ind> of</trd>,
   </trdgrp>
   <trdgrp lng="he">
     <trd>&het;&resh;&fpe;</trd>,
     <trd>&lamed;&mem;&resh;&vav;&tav;</trd>,
     <trd>&alef;&fpe; &ayin;&lamed; &pe;&yod;</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>a pesar de</trd>,
     <trd>no obstante</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>malgr&eacute;</trd>,
     <trd>en <ind>d&eacute;pit</ind> de</trd>,
     <trd>nonobstant <klr>(rare ou droit)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>dac&aacute;ra</trd>,
     <trd>ellen&eacute;re</trd>
   </trdgrp>
   <trd lng="nl">ondanks</trd>
  <trdgrp lng="pt">
    <trd>apesar de</trd>,
    <trd>n&atilde;o <ind>obstante</ind></trd>,
    <trd>a <ind>despeito</ind> de</trd>,        
    <trd>malgrado</trd>,
    <trd>embora</trd>
  </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_n;&c_je;&c_s;&c_m;&c_o;&c_t;&c_r;&c_ja; &c_n;&c_a;</trd>,
     <trd>&c_v;&c_o;&c_p;&c_r;&c_je;&c_k;&c_i;</trd>
   </trdgrp>
   <trd lng="sk">napriek</trd>
   <trd lng="sv">trots</trd>
</drv>

<drv mrk="malgra.0ke">
   <kap><tld/> ke</kap>
   <gra><vspec>konjunkcio</vspec></gra>
   <snc mrk="malgra.0ke.KOMUNE">
     <dif>
       <tld lit="M"/> tio ke;
       <ref tip="vid" cel="kvanka.0.KOMUNE">kvankam</ref>:
       <ekz>
         estis ankora&ubreve; sufi&ccirc;e varmege, <tld/> ke la suno staris jam malalte
         <fnt>
           <aut>M. Solovjev</aut>
           <vrk>Solvita problemo</vrk>
           <lok>Esperantisto, 1892:6</lok>
        </fnt>.
       </ekz>   
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_ja; &c_g;&c_l;&c_je;&c_d;&c_z;&c_ja;&c_ch;&c_y;
     &c_n;&c_a; &c_t;&c_o;&c_je;, &c_sh;&c_t;&c_o;</trd>,
     <trd>&c_n;&c_je; &c_z;&c_v;&c_a;&c_zh;&c_a;&c_ju;&c_ch;&c_y;
     &c_n;&c_a; &c_t;&c_o;&c_je;, &c_sh;&c_t;&c_o;</trd>
     <trd>&c_h;&c_a;&c_c;&c_ja;</trd>
   </trdgrp>
   <trd lng="bg">&c_v;&c_malmol;&c_p;&c_r;&c_je;&c_k;&c_i; &c_ch;&c_je;</trd>
   <trdgrp lng="br">
     <trd>beza&ntilde; ma</trd>,
     <trd>daoust ma</trd>,
     <trd>evit da</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>trotz der Tatsache dass</trd>,
     <trd>obgleich</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>notwithstanding the fact that</trd>,
     <trd>in <ind>spite</ind> of the fact that</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>aunque</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>bien que</trd>,
     <trd>quoique</trd>
   </trdgrp>
   <trdgrp lng="he">
     <trd>&lamed;&mem;&resh;&vav;&tav; &shin;-</trd>,
     <trd>&alef;&fpe; &ayin;&lamed; &pe;&yod; &shin;-</trd>
   </trdgrp>
  <trdgrp lng="hu">
    <trd>annak <ind>ellen&eacute;re</ind>, hogy</trd>,
    <trd>j&oacute;llehet</trd>
  </trdgrp>
   <trd lng="nl">hoewel</trd>
  <trdgrp lng="pt">
    <trd>apesar de que</trd>,
    <trd>mesmo assim</trd>,
    <trd>apesar disso</trd>
  </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_n;&c_je;&c_s;&c_m;&c_o;&c_t;&c_r;&c_ja; &c_n;&c_a;
     &c_t;&c_o;, &c_ch;&c_t;&c_o;</trd>;
     <trd>&c_h;&c_o;&c_t;&c_ja;</trd>
   </trdgrp>
   <trd lng="sv">trots att</trd>
</drv>

<drv mrk="malgra.0e">
   <kap><tld/>e</kap>
   <snc mrk="malgra.0e.KOMUNE">
     <dif>
       <tld lit="M"/> tio; <ref tip="vid"
       cel="tamen.0.KOMUNE">tamen</ref>:
    <ekz>
      anka&ubreve; vi estas ja barbaro, kaj <tld/>e mi allasis vin en
      mian
      domon
      <fnt>
        <aut>V. Vina&rcaron;</aut>,
        <vrk>La skandalo pro Jozefo</vrk>
        <lok>2002</lok>
      </fnt>.
    </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_ja; &c_g;&c_l;&c_je;&c_d;&c_z;&c_ja;&c_ch;&c_y;
     &c_n;&c_a; &c_g;&c_e;&c_t;&c_a;</trd>,
     <trd>&c_n;&c_je; &c_z;&c_v;&c_a;&c_zh;&c_a;&c_ju;&c_ch;&c_y;
     &c_n;&c_a; &c_g;&c_e;&c_t;&c_a;</trd>
   </trdgrp>
   <trdgrp lng="bg">
     <trd>&c_v;&c_malmol;&c_p;&c_r;&c_je;&c_k;&c_i; &c_t;&c_o;&c_v;&c_a;</trd>,
     <trd>&c_v;&c_s;&c_je; &c_p;&c_a;&c_k;</trd>
   </trdgrp>
   <trdgrp lng="br">
     <trd>evelato</trd>,
     <trd>evelkent</trd>,
     <trd>neoazh</trd>
   </trdgrp>
   <trd lng="de">trotzdem</trd>
   <trd lng="en">nevertheless</trd>
   <trd lng="es">sin embargo</trd>
   <trdgrp lng="fr">
     <trd>n&eacute;anmoins</trd>,
     <trd>malgr&eacute; cela</trd>,
     <trd>malgr&eacute; tout</trd>
   </trdgrp>
   <trdgrp lng="he">
     <trd>&lamed;&mem;&resh;&vav;&tav; &zayin;&alef;&tav;</trd>,
     <trd>&alef;&fpe; &ayin;&lamed; &pe;&yod; &kaf;&fnun;</trd>,
     <trd>&dalet;&vav;&vav;&qof;&alef;</trd>
   </trdgrp>
  <trdgrp lng="hu">
    <trd>annak <ind>ellen&eacute;re</ind></trd>,
    <trd>ennek <ind>ellen&eacute;re</ind></trd>
  </trdgrp>
   <trd lng="nl">desondanks</trd>
  <trdgrp lng="pt">
    <trd>ainda assim</trd>,
    <trd>mesmo assim</trd>,
    <trd>apesar disso</trd>
  </trdgrp>
   <trd lng="ru">&c_n;&c_je;&c_s;&c_m;&c_o;&c_t;&c_r;&c_ja; &c_n;&c_a;
   &c_e;&c_t;&c_o;</trd>
   <trd lng="sk">napriek tomu</trd>
</drv>
</art>
