<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>malgra&ubreve;</rad>
</kap>
<drv mrk="malgra.0">
  <kap>
    <ofc>*</ofc><tld/>
  </kap>
  <snc>
    <dif>
      Prep. signifanta &leftquot;neatentante kontra&ubreve;starantan ies volon a&ubreve; ian
      fizikan baron&rightquot;:
      <ekz>
        vi eliris <tld/> mia malpermeso;
      </ekz>
      <ekz>
        ni pacience eltenis <tld/> la multaj atakoj kaj
        malagrabla&jcirc;oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni promenis <tld/> la pluvo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="spit.0e">spite</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">p&rcaron;es</trd>
  <trd lng="de">trotz</trd>
  <trdgrp lng="en">
    <trd>notwithstanding</trd>,
    <trd>in <ind>spite</ind> of</trd>,
  </trdgrp>
  <trd lng="nl">ondanks</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_s;&c_m;&c_o;&c_t;&c_r;&c_ja; &c_n;&c_a;</trd>,
    <trd>&c_v;&c_o;&c_p;&c_r;&c_je;&c_k;&c_i;</trd>
  </trdgrp>
  <trd lng="sv">trots</trd>
</drv>

<drv mrk="malgra.0ke">
  <kap>
    <tld/> ke
  </kap>
  <snc>
    <dif>
      <tld lit="M"/> tio ke; kvankam:
      <ekz>
        estis ankora&ubreve; sufi&ccirc;e varmega, <tld/> ke la suno staris jam
        malalte<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>trotz der Tatsache dass</trd>,
    <trd>obgleich</trd>
  </trdgrp>
  <trd lng="nl">hoewel</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_s;&c_m;&c_o;&c_t;&c_r;&c_ja; &c_n;&c_a; &c_t;&c_o;, &c_ch;&c_t;&c_o;</trd>;
    <trd>&c_h;&c_o;&c_t;&c_ja;</trd>
  </trdgrp>
  <trd lng="sv">trots att</trd>
</drv>

<drv mrk="malgra.0e">
  <kap>
    <tld/>e
  </kap>
  <snc>
    <dif>
      <tld lit="M"/> tio.
    </dif>
  </snc>
  <trd lng="de">trotzdem</trd>
  <trd lng="nl">desondanks</trd>
  <trd lng="ru">&c_n;&c_je;&c_s;&c_m;&c_o;&c_t;&c_r;&c_ja; &c_n;&c_a; &c_e;&c_t;&c_o;</trd>
</drv>
</art>
