<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: malgra.xml,v 1.30 2024/01/25 20:10:33 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>malgra&ubreve;</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="malgra.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra><vspec>prepozicio</vspec></gra>
  <snc mrk="malgra.0.KOMUNE">
    <dif>
      Neatentante kontra&ubreve;starantan ies volon a&ubreve; ian
      fizikan baron; <ref tip="sin" cel="spit.0eal">spite al</ref>:
      <ekz>
         vi eliris <tld/> mia malpermeso;
      </ekz>
      <ekz>
         vi pacience eltenis en la da&ubreve;ro de dudek jaroj, <tld/> la multaj atakoj kaj
         malagrabla&jcirc;oj
         <fnt>
           <bib>Paroloj</bib>
           <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge</vrk>
           <lok>1907</lok>
         </fnt>;
       </ekz>
       <ekz>
        se vi <tld/> &ccirc;i tio ne obeos <klr>[&#x2026;]</klr> mi sepoble pligrandigos la punon
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 26:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lando <nom>Gosen</nom>, <tld/> la ondolinia tero, &scirc;ajnis ebena&jcirc;o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> la tre kruta vojo, la princo galope kuris malsupren
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ektusis hm, hm, hm, tamen <tld/> tio neniu venis
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pluvo faladis torente, sed <tld/> <klr>[tio]</klr> li devis nepre eliri
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/> sia sintrotakseco apartenas al la simpla popolo
        <fnt>
          <bib>Fab2</bib>
          <lok>Feli&ccirc;a familio</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_ja; &c_g;&c_l;&c_je;&c_d;&c_z;&c_ja;&c_ch;&c_y;
     &c_n;&c_a;</trd>,
     <trd>&c_n;&c_je; &c_z;&c_v;&c_a;&c_zh;&c_a;&c_ju;&c_ch;&c_y;
     &c_n;&c_a;</trd>
   </trdgrp>
   <trd lng="bg">&c_v;&c_malmol;&c_p;&c_r;&c_je;&c_k;&c_i;</trd>
   <trd lng="br">daoust da</trd>
   <trd lng="cs">p&rcaron;es</trd>
   <trd lng="de">trotz</trd>
   <trdgrp lng="en">
     <trd>notwithstanding</trd>,
     <trd>in <ind>spite</ind> of</trd>,
   </trdgrp>
   <trdgrp lng="he">
     <trd>&het;&resh;&fpe;</trd>,
     <trd>&lamed;&mem;&resh;&vav;&tav;</trd>,
     <trd>&alef;&fpe; &ayin;&lamed; &pe;&yod;</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>a pesar de</trd>,
     <trd>no obstante</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>malgr&eacute;</trd>,
     <trd>en <ind>d&eacute;pit</ind> de</trd>,
     <trd>nonobstant <klr>(rare ou droit)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>dac&aacute;ra</trd>,
     <trd>ellen&eacute;re</trd>
   </trdgrp>
   <trd lng="nl">ondanks</trd>
   <trdgrp lng="pl">
     <trd>mimo</trd>,
     <trd>pomimo</trd>,
     <trd>wbrew</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>apesar de</trd>,
     <trd>n&atilde;o <ind>obstante</ind></trd>,
     <trd>a <ind>despeito</ind> de</trd>,        
     <trd>malgrado</trd>,
     <trd>embora</trd>
   </trdgrp>
   <trdgrp lng="ro">
     <trd>ciuda</trd>,
     <trd>cu toate c&abreve;</trd>,
     <trd>&icirc;mpotriva</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_n;&c_je;&c_s;&c_m;&c_o;&c_t;&c_r;&c_ja; &c_n;&c_a;</trd>,
     <trd>&c_v;&c_o;&c_p;&c_r;&c_je;&c_k;&c_i;</trd>
   </trdgrp>
   <trd lng="sk">napriek</trd>
   <trd lng="sv">trots</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_z;&c_v;&c_a;&c_zh;&c_a;&c_ju;&c_ch;&c_i; &c_n;&c_a;...</trd>,
    <trd>&c_u;&c_s;&c_u;&c_p;&c_je;&c_r;&c_je;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5118;&#x7BA1; <pr>j&#x1D0;ngu&#x1CE;n</pr></trd>,
    <trd>&#x5C3D;&#x7BA1; <pr>j&#x1D0;ngu&#x1CE;n</pr></trd>,
    <trd>&#x4E0D;&#x9867;&#x53CD;&#x5C0D; <pr>b&#xF9;g&#xF9;f&#x1CE;ndu&#xEC;</pr></trd>,
    <trd>&#x4E0D;&#x987E;&#x53CD;&#x5BF9; <pr>b&#xF9;g&#xF9;f&#x1CE;ndu&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="malgra.0ke">
  <kap><tld/> ke</kap>
  <gra><vspec>konjunkcio</vspec></gra>
  <snc mrk="malgra.0ke.KOMUNE">
    <dif>
      <tld lit="M"/> tio ke;
      <ref tip="vid" cel="kvanka.0.KOMUNE">kvankam</ref>:
      <ekz>
        &gcirc;i ne rompi&gcirc;is, <tld/> ke &gcirc;in transveturis &scirc;ar&gcirc;veturilo
        <fnt>
          <bib>Fab2</bib>
          <lok>Flikilo</lok>
        </fnt>;
      </ekz>
      <ekz>
         estis ankora&ubreve; sufi&ccirc;e varmege, <tld/> ke la suno staris jam malalte
         <fnt>
           <aut>M. Solovjev</aut>
           <vrk>Solvita problemo</vrk>
           <lok>Esperantisto, 1892:6</lok>
        </fnt>.
      </ekz>   
    </dif>
    <ref tip="vid" cel="spit.0eke">spite ke</ref>
  </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_ja; &c_g;&c_l;&c_je;&c_d;&c_z;&c_ja;&c_ch;&c_y; &c_n;&c_a; &c_t;&c_o;&c_je;, &c_sh;&c_t;&c_o;</trd>,
     <trd>&c_n;&c_je; &c_z;&c_v;&c_a;&c_zh;&c_a;&c_ju;&c_ch;&c_y; &c_n;&c_a; &c_t;&c_o;&c_je;, &c_sh;&c_t;&c_o;</trd>
     <trd>&c_h;&c_a;&c_c;&c_ja;</trd>
   </trdgrp>
   <trd lng="bg">&c_v;&c_malmol;&c_p;&c_r;&c_je;&c_k;&c_i; &c_ch;&c_je;</trd>
   <trdgrp lng="br">
     <trd>beza&ntilde; ma</trd>,
     <trd>daoust ma</trd>,
     <trd>evit da</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd><ind>trotz</ind> der Tatsache dass</trd>,
     <trd>obgleich</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>notwithstanding the fact that</trd>,
     <trd>in <ind>spite</ind> of the fact that</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>aunque</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>bien que</trd>,
     <trd>quoique</trd>
   </trdgrp>
   <trdgrp lng="he">
     <trd>&lamed;&mem;&resh;&vav;&tav; &shin;-</trd>,
     <trd>&alef;&fpe; &ayin;&lamed; &pe;&yod; &shin;-</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>annak <ind>ellen&eacute;re</ind>, hogy</trd>,
     <trd>j&oacute;llehet</trd>
   </trdgrp>
   <trd lng="nl">hoewel</trd>
   <trdgrp lng="pl">
     <trd>acz</trd>,
     <trd>aczkolwiek</trd>,
     <trd>cho&cacute;</trd>,
     <trd>chocia&zdot;</trd>,
     <trd>pomimo &zdot;e</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>apesar de que</trd>,
     <trd>mesmo assim</trd>,
     <trd>apesar disso</trd>
   </trdgrp>
   <trdgrp lng="ro">
     <trd>de&#x219;i</trd>,
     <trd>m&abreve;car c&abreve;</trd>,
     <trd>parca</trd>,
     <trd>cu toate c&abreve;</trd>
   </trdgrp> 
   <trdgrp lng="ru">
     <trd>&c_n;&c_je;&c_s;&c_m;&c_o;&c_t;&c_r;&c_ja; &c_n;&c_a; &c_t;&c_o;, &c_ch;&c_t;&c_o;</trd>;
     <trd>&c_h;&c_o;&c_t;&c_ja;</trd>
   </trdgrp>
   <trd lng="sv">trots att</trd>
  <trdgrp lng="zh">
    <trd>&#x867D;&#x8BF4; <pr>su&#x12B;shu&#x14D;</pr></trd>,
    <trd>&#x96D6;&#x8AAA; <pr>su&#x12B;shu&#x14D;</pr></trd>,
    <trd>&#x867D;&#x7136; <pr>su&#x12B;r&#xE1;n</pr></trd>,
    <trd>&#x96D6;&#x7136; <pr>su&#x12B;r&#xE1;n</pr></trd>,
    <trd>&#x5118;&#x7BA1; <pr>j&#x1D0;ngu&#x1CE;n</pr></trd>,
    <trd>&#x5C3D;&#x7BA1; <pr>j&#x1D0;ngu&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="malgra.0e">
  <kap><tld/>e</kap>
  <snc mrk="malgra.0e.KOMUNE">
    <dif>
    <tld lit="M"/> tio; <ref tip="vid" cel="tamen.0.KOMUNE">tamen</ref>:
      <ekz>
        anka&ubreve; vi estas ja barbaro, kaj <tld/>e mi allasis vin en
        mian domon
        <fnt>
          <aut>V. Vina&rcaron;</aut>,
          <vrk>La skandalo pro Jozefo</vrk>
          <lok>2002</lok>
        </fnt>;
      </ekz>
      <ekz>
        li tamen ne apartenas al ili, sed <tld/>e, post iom da tempo, li rekonas inter
        ili sin mem
        <fnt>
          <bib>VaK</bib>
          <lok>Oka &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="malgra.0e.spite_al">
    <dif>
      Spitante al; tolerante:
      <ekz>
        tamen, <tld/>e doloro via, ne povos vi helpi ilin
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kara patrino, elportu mal&gcirc;ojon <tld/>e sufero
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_ja; &c_g;&c_l;&c_je;&c_d;&c_z;&c_ja;&c_ch;&c_y;
     &c_n;&c_a; &c_g;&c_e;&c_t;&c_a;</trd>,
     <trd>&c_n;&c_je; &c_z;&c_v;&c_a;&c_zh;&c_a;&c_ju;&c_ch;&c_y;
     &c_n;&c_a; &c_g;&c_e;&c_t;&c_a;</trd>
   </trdgrp>
   <trdgrp lng="bg">
     <trd>&c_v;&c_malmol;&c_p;&c_r;&c_je;&c_k;&c_i; &c_t;&c_o;&c_v;&c_a;</trd>,
     <trd>&c_v;&c_s;&c_je; &c_p;&c_a;&c_k;</trd>
   </trdgrp>
   <trdgrp lng="br">
     <trd>evelato</trd>,
     <trd>evelkent</trd>,
     <trd>neoazh</trd>
   </trdgrp>
   <trd lng="de">trotzdem</trd>
   <trd lng="en">nevertheless</trd>
   <trd lng="es">sin embargo</trd>
   <trdgrp lng="fr">
     <trd>n&eacute;anmoins</trd>,
     <trd>malgr&eacute; cela</trd>,
     <trd>malgr&eacute; tout</trd>
   </trdgrp>
   <trdgrp lng="he">
     <trd>&lamed;&mem;&resh;&vav;&tav; &zayin;&alef;&tav;</trd>,
     <trd>&alef;&fpe; &ayin;&lamed; &pe;&yod; &kaf;&fnun;</trd>,
     <trd>&dalet;&vav;&vav;&qof;&alef;</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>annak <ind>ellen&eacute;re</ind></trd>,
     <trd>ennek <ind>ellen&eacute;re</ind></trd>
   </trdgrp>
   <trd lng="nl">desondanks</trd>
   <trd lng="pl">mimo to</trd>
   <trdgrp lng="pt">
     <trd>ainda assim</trd>,
     <trd>mesmo assim</trd>,
     <trd>apesar disso</trd>
   </trdgrp>
   <trdgrp lng="ro">
     <trd>cu toate acestea</trd>,
     <trd>tot</trd>
   </trdgrp>
   <trd lng="ru">&c_n;&c_je;&c_s;&c_m;&c_o;&c_t;&c_r;&c_ja; &c_n;&c_a; &c_e;&c_t;&c_o;</trd>
   <trd lng="sk">napriek tomu</trd>
  <trdgrp lng="zh">
    <trd>&#x53CD;&#x6B63; <pr>f&#x1CE;nzh&#xE8;ng</pr></trd>,
    <trd>&#x5374;&#x662F; <pr>qu&#xE8;sh&#xEC;</pr></trd>,
    <trd>&#x537B;&#x662F; <pr>qu&#xE8;sh&#xEC;</pr></trd>,
    <trd>&#x603B;&#x8981; <pr>z&#x1D2;ngy&#xE0;o</pr></trd>,
    <trd>&#x7E3D;&#x8981; <pr>z&#x1D2;ngy&#xE0;o</pr></trd>
  </trdgrp>
</drv>
</art>
