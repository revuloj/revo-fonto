<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: bat1.xml,v 1.81 2024/06/21 07:25:30 revo Exp $">
    <kap>
      <rad>bat</rad>/o
    </kap>

<drv mrk="bat1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Antikva <ref tip="lst" lst="voko:kvantoj" cel="unu.mezur0o">mezurunuo</ref>
      por likva&jcirc;oj, egala al la enhavo de (granda)
      sitelo, &ccirc; 22 &ndash; 40 litroj, dekono de <ref tip="vid" cel="kor1.0o">koro</ref>:
      <ekz>
        li faris dek kuprajn lavujojn, &ccirc;iu lavujo havis la
        amplekson de kvardek <tld/>'oj, kvar ulnojn havis &ccirc;iu lavujo
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:38</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>'o estas dekono de kor'o, dek <tld/>'oj faras &hcirc;omeron, &ccirc;ar &hcirc;omero entenas dek
        <tld/>'ojn
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 45:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la efo kaj la <tld/>'o devas havi egalan amplekson, tiel,
        ke la <tld/>'o entenu dekonon de &hcirc;omero kaj la efo anka&ubreve;
        dekonon de &hcirc;omero: la&ubreve; la &hcirc;omero oni fiksu ilian amplekson
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 45:11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    Vd <ref tip="hom" cel="bat.0o">bato</ref> <klr>(ago)</klr>.
  </rim>
  <trd lng="be">&c_b;&c_a;&c_t; <klr tip="amb">(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a; &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trdgrp lng="de">
    <trd>Bat</trd>,
    <trd>Eimer</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#25731;  <pr>&#12356;&#12385;&#12370;&#x304D;</pr></trd>,
    <trd>&#25171;&#25731;  <pr>&#12384;&#12370;&#x304D;</pr></trd>,
    <trd>&#x6BB4;&#25171;  <pr>&#x304A;&#12358;&#12384;</pr></trd>
  </trdgrp>
</drv>

  </art>

