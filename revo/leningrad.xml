<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>Leningrad</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="leningrad.0o">
  <kap><tld/>o</kap>
  <snc mrk="leningrad.0o.urbo">
    <uzo tip="fak">POL</uzo>
    <dif>
      Nomo de <ref tip="sin" cel="peterburg.0o">Peterburgo</ref>
      en la jaroj 1924 &gcirc;is 1991:
      <ekz>
        sie&gcirc;o de <tld/>o estis unu el la plej tragediaj epizodoj de la
        Dua 
        mondmilito
        <fnt><bib>Viki</bib>, <lok>Sie&gcirc;o de Leningrado</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">Leningrad</trd>
  <trd lng="de">Leningrad</trd>
  <trd lng="fr">L&eacute;ningrad</trd>
  <trd lng="pl">Leningrad</trd>     
  <trd lng="ro">Leningrad</trd>
  <trd lng="sk">Leningrad</trd>
</drv>
</art>

