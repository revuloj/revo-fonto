<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arund.xml,v 1.5 2023/10/28 08:33:09 revo Exp $">
<kap>
    <rad>arund</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="arund.0o">
  <kap><tld/>o</kap>
  <snc mrk="arund.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="kan.0o.BOT" lst="voko:pooparencoj">Kano</ref>,
      kreskanta en Mediteraneo kaj Tajvano;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="poac.0oj">poacoj</ref>
      (<trd lng="la">Arundo</trd>):
      <ekz>
        kano estas genro (<tld lit="A"/>o el poacoj) el plurjaraj herboj, iufoje pli ol
        4 metrojn alta, kun dikaj, fortikaj tigoj kaj kun floroj, similaj al tiuj de
        fragmito
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=23199">Kano</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/9/9d/Illustration_Arundo_donax0.jpg/272px-Illustration_Arundo_donax0.jpg" prm="PD">botanika tabulo de <tld/>o (Arundo donax)
        <fnt>
          <aut>Prof. Dr. Otto Wilhelm Thom&eacute;: &leftquot;Flora von Deutschland, &Ouml;sterreich und der Schweiz&rightquot; 1885, Gera, Germanujo</aut>
          <lok><url ref="&WCW;/File:Illustration_Arundo_donax0.jpg">&WCW;/File:Illustration_Arundo_donax0.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="zh">
    <trd>&#x7C50; <pr>t&#xE9;ng</pr></trd>,
    <trd>&#x85E4; <pr>t&#xE9;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="de">
    <trd>Pfahlrohr</trd>,
    <trd>Spanisches Rohr</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x440;&#x443;&#x43D;&#x434;&#x43E;</trd>
</drv>
</art>

