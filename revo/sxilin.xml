<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxilin.xml,v 1.25 2025/03/12 06:36:23 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>&scirc;iling</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="sxilin.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxilin.0o.MON">
    <dif>
      Nomo de iuj
      <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">monunuoj</ref>.
    </dif>
    <subsnc mrk="sxilin.0o.afrika">
      <dif>
        Monunuo de pluraj afrikaj &scirc;tatoj:
        Kenjo, Tanzanio, Ugando, Somalujo.
      </dif>
    </subsnc>
    <subsnc mrk="sxilin.0o.auxstra">
      <dif>
        Iama a&ubreve;struja monunuo:
        <ekz>
          en 1996 la kamparanoj ricevis preska&ubreve; 8 miliardojn da
          a&ubreve;striaj <tld/>oj (667 milionojn da usonaj dolaroj)
          apoge al siaj libervolaj naturmediaj laboroj
          <fnt>
          <aut>E. Georgiev</aut>,
          <vrk>A&ubreve;strio: lando de ekologia kultivado</vrk>,
          <bib>Monato</bib>,
          <lok>1997:9, p. 19a</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="sxilin.0o.brita">
      <dif>
        Iama brita <ref tip="lst" cel="unu.monasub0o" lst="voko:monunuoj">mona subunuo</ref>:
        <ekz>
        &leftquot;vi havas e&ccirc; ne <tld/>on&rightquot;
          <fnt>
            <aut>J. Francis</aut>,
            <vrk>La Granda Kaldrono</vrk>,
            <lok>1978</lok>
          </fnt>;
        </ekz>
        <ekz>
        &ccirc;ambro de la unua klaso kostas du <tld/>ojn kaj tri pencojn &ccirc;iunokte
         <fnt>
           <bib>KKl</bib>
           <lok>&Ccirc;apitro 4</lok>
         </fnt>;
       </ekz>
        <ekz>
          kalkulo kun la bierfabrikanto farita <klr>[&#8230;]</klr> deksep funtoj kvar <tld/>oj
          en la bankon pagitaj
          <fnt>
            <bib>BdV</bib>
            <lok>La Batalo de l' Vivo</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_l;&c_ib;&c_n;&c_g;</trd>
  <trd lng="ca">x&iacute;ling</trd>
  <trd lng="de">Schilling</trd>
  <trd lng="el">&sigma;&epsilon;&lambda;&jota_ton;&ny;&jota;</trd>
  <trd lng="en">shilling</trd>
  <trd lng="es">chel&iacute;n</trd>
  <trdgrp lng="fr">
    <trd>schilling</trd>,
    <trd>shilling</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>schilling</trd>,
    <trd>shilling</trd>
  </trdgrp>
  <trd lng="id">shilling</trd>
  <trd lng="it">scellino</trd>
  <trd lng="ja">&#12471;&#12522;&#12531;&#12464;</trd>
  <trd lng="nl">shilling</trd>
  <trd lng="pl">szyling</trd>
  <trd lng="pt">xelim</trd>
  <trd lng="ru">&c_sh;&c_i;&c_l;&c_l;&c_i;&c_n;&c_g;</trd>
  <trd lng="uk">&c_sh;&c_i;&c_l;&c_ib;&c_n;&c_gu; (&c_g;&c_r;&c_o;&c_sh;&c_o;&c_v;&c_a; &c_o;&c_d;&c_i;&c_n;&c_i;&c_c;&c_ja;)</trd>
  <trd lng="zh">&#20808;&#20196; <pr>xi&amacron;nl&igrave;ng</pr></trd>
</drv>
</art>
