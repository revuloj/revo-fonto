<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>&scirc;iling</rad>/o
</kap>
<drv mrk="sxilin.0o">
  <kap><tld/>o</kap>
  <snc>
    <subsnc>
      <dif>
        <ref tip="super" cel="unu.mon0o">Monunuo</ref> de pluraj afrikaj
        &scirc;tatoj.
      </dif>
    </subsnc>
    <subsnc>
      <dif>
        Iama a&ubreve;stra monunuo:
        <ekz>
          en 1996 la kamparanoj ricevis preska&ubreve; 8 miliardojn da a&ubreve;striaj
          <tld/>oj (667 milionojn da usonaj dolaroj) apoge al siaj
          libervolaj naturmediaj laboroj<fnt><aut>Evgeni Georgiev</aut>:
          <vrk>A&ubreve;strio: lando de ekologia kultivado</vrk>,
          <bib>Monato</bib><lok>jaro 1997a, numero 9a, p. 19a</lok></fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc>
      <dif>
        Iama brita <ref tip="super"
        cel="unu.monasub0o">mona subunuo</ref>.
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_l;&c_ib;&c_n;&c_g;</trd>
  <trd lng="ca">x&iacute;ling</trd>
  <trd lng="de">Schilling</trd>
  <trd lng="el">&sigma;&epsilon;&lambda;&jota_ton;&ny;&jota;</trd>
  <trd lng="en">shilling</trd>
  <trd lng="es">chel&iacute;n</trd>
  <trdgrp lng="fr">
    <trd>schilling</trd>,
    <trd>shilling</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>schilling</trd>,
    <trd>shilling</trd>
  </trdgrp>
  <trd lng="nl">shilling</trd>
  <trd lng="pt">xelim</trd>
  <trd lng="ru">&c_sh;&c_i;&c_l;&c_l;&c_i;&c_n;&c_g;</trd>
</drv>
</art>
