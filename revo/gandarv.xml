<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gandarv</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="gandarv.0o">
  <kap><tld/>o</kap>
  <snc mrk="gandarv.0o.HIN">
    <uzo tip="fak">HIN</uzo>
    <dif>
      Iu el duondioj servantaj en diaj festenoj,
      kantistoj kaj muzikistoj, paruloj de
      <ref tip="vid" cel="apsar.0o.HIN">apsaroj</ref>:
      <ekz>
        loko nekonata al dioj, <tld/>oj kaj homoj
        <fnt>
          <aut>trad. Seiho Nishi</aut>,
          <vrk><url 
          ref="http://www001.upp.so-net.ne.jp/jble/sutro/darmapado/teksto.html">Darmapado</url></vrk>, 
          <lok>Japana Budhano, 1974-75, numeroj 162a-169a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">gandharva</trd>
  <trd lng="fr">gandharva</trd>
</drv>
</art>

<!--
$Log$
