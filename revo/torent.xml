<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>torent</rad>/o
</kap>
<drv mrk="torent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Rapidega da&ubreve;ra akvofluo estigata de granda dekliveco de
      grundo.
    </dif>
    <trdgrp lng="de">
      <trd>rei&szlig;ender Fluss</trd>,
      <trd>Sturzbach</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>zuhatag</trd>,
      <trd>hegyi <ind>patak</ind></trd>
    </trdgrp>
    <trd lng="nl">bergstroom</trd>
  </snc>
  <snc>
    <dif>
      Rapidega neda&ubreve;ra akvofluo estigata de pluvego a&ubreve;
      ne&gcirc;fandi&gcirc;o.
    </dif>
    <trdgrp lng="de">
      <trd>Sturzbach</trd>,
      <trd>Flut</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&aacute;r</trd>,
      <trd>&aacute;radat</trd>,
      <trd>folyam</trd>
    </trdgrp>
    <trd lng="nl">stroom</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io, kio estas abunda kaj rapidega:
      <ekz>
        la <tld/>o de liaj deziroj povos pli libere fluegi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la lucernoj disver&scirc;as <tld/>ojn da lumo;
      </ekz>
      <ekz>
        <tld/>oj da larmoj, da harmonio;
      </ekz>
      <ekz>
        <tld/>o da vortoj<fnt>Z</fnt>,
      </ekz>
      <ekz>
        <ind><tld/>oj da insultoj</ind>.
        <trd lng="hu">szitok&ouml;z&ouml;n</trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="lavang.0o">lavango</ref>,
      <ref cel="vent.0ego">ventego</ref>,
      <ref cel="uragan.0o">uragano</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Flut</trd>,
      <trd>Strom</trd>
    </trdgrp>
    <trd lng="hu">&ouml;z&ouml;n</trd>
    <trd lng="nl">stroom</trd>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_p;&c_o;&c_r;&c_o;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_je;&c_m;&c_i;&c_t;&c_je;&c_l;&c_je;&c_n; &c_p;&c_o;&c_t;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">bystřinný proud</trd>
  <trdgrp lng="en">
    <trd>torrent</trd>,
    <trd>stream</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>potok</trd>,
    <trd>strumie&nacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>torrente</trd>,
    <trd>caudal</trd>,
    <trd>enxurrada</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_o;&c_d;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>príval</trd>,
    <trd>riava</trd>,
    <trd>strmá bystrina</trd>
  </trdgrp>
</drv>

<drv mrk="torent.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Simila al <tld/>o:
      <ekz>
        <tld/>a rivereto,
      </ekz>
      <ekz>
        <ind><tld/>a pluvo</ind>;
        <trdgrp lng="hu">
          <trd>zuhog&oacute; es&odblac;</trd>,
          <trd>felh&odblac;szakad&aacute;s</trd>
        </trdgrp>
        <trdgrp lng="pl">
          <trd>ulewny deszcz</trd>,
          <trd>ulewa</trd>,
          <trd>rz&eogonek;sisty deszcz</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_p;&c_r;&c_o;&c_l;&c_i;&c_v;&c_n;&c_o;&c_j;
          &c_d;&c_o;&c_zh;&c_d;&c_mol;</trd>,
          <trd>&c_l;&c_i;&c_v;&c_je;&c_n;&c_mol;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>aj larmoj</ind>;
        <trdgrp lng="pl">
          <trd>rz&eogonek;siste &lstroke;zy</trd>,
          <trd>krokodyle &lstroke;zy</trd>
        </trdgrp>
      </ekz>
      <ekz>
        pluvas <tld/>e.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_o;&c_r;&c_o;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>přívalový</trd>,
    <trd>vířivý</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wildbachartig</trd>,
    <trd>str&ouml;mend</trd>
  </trdgrp>
  <trd lng="en">torrential</trd>
  <trdgrp lng="hu">
    <trd>zuhog&oacute;</trd>,
    <trd>z&uacute;dul&oacute;</trd>,
    <trd>&aacute;rad&oacute;</trd>,
    <trd>&ouml;ml&odblac;</trd>
  </trdgrp>
  <trd lng="nl">wild <ind>stromend</ind></trd>
  <trdgrp lng="pl">
    <trd>potokowy</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>prívalový</trd>,
    <trd>vírivý</trd>
  </trdgrp>
</drv>

<drv mrk="torent.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Flui <tld/>e:
      <ekz>
        la pluvo <tld/>is<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sxpruc.0i">&scirc;pruci</ref>,
      <ref cel="flu">fluegi</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>zuhog</trd>,
      <trd>z&uacute;dul</trd>,
      <trd>&ouml;mlik</trd>,
      <trd>&aacute;rad</trd>
    </trdgrp>
    <trd lng="nl">stromen</trd>
    <trdgrp lng="pl">
      <trd>p&lstroke;yn&aogonek;&cacute; <ind>potokiem</ind></trd>,
      <trd>p&lstroke;yn&aogonek;&cacute; <ind>strumieniem</ind></trd>,
      <trd>la&cacute;<klr> (o deszczu)</klr></trd>,
      <trd>la&cacute; ciurkiem<klr> (o deszczu, o
      p&lstroke;ynie)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sin fortege &jcirc;eti:
      <ekz>
        ili <tld/>is sur lin.
      </ekz>
    </dif>
    <trd lng="hu">r&aacute;z&uacute;dul</trd>
    <trd lng="nl">zich <ind>storten</ind></trd>
  </snc>
  <trd lng="bg">&c_p;&c_o;&c_t;&c_o;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>téci bystřinným proudem</trd>,
    <trd>vířivě proudit</trd>
  </trdgrp>
  <trd lng="de">str&ouml;men</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_je;&c_ch;&c_mol;
    <ind>&c_p;&c_o;&c_t;&c_o;&c_k;&c_o;&c_m;</ind></trd>,
    <trd>&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;
    <ind>&c_p;&c_o;&c_t;&c_o;&c_k;&c_o;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="sk">vírivo prúdiť</trd>
</drv>
</art>
