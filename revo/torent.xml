<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>torent</rad>/o
</kap>
<drv mrk="torent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Rapidega da&ubreve;ra akvofluo estigata de granda dekliveco de grundo.
    </dif>
    <trdgrp lng="de">
      <trd>rei&szlig;ender Fluss</trd>,
      <trd>Sturzbach</trd>
    </trdgrp>
    <trd lng="nl">bergstroom</trd>
  </snc>
  <snc>
    <dif>
      Rapidega neda&ubreve;ra akvofluo estigata de pluvego a&ubreve; ne&gcirc;fandi&gcirc;o.
    </dif>
    <trdgrp lng="de">
      <trd>Sturzbach</trd>,
      <trd>Flut</trd>
    </trdgrp>
    <trd lng="nl">stroom</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Io, kio estas abunda kaj rapidega:
      <ekz>
        la <tld/>o de liaj deziroj povos pli libere fluegi<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la lucernoj disver&scirc;as <tld/>ojn da lumo;
      </ekz>
      <ekz>
        <tld/>oj da larmoj, da harmonio;
      </ekz>
      <ekz>
        <tld/>o da vortoj<fnt>Z</fnt>,
      </ekz>
      <ekz>
        da insultoj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="lavang.0o">lavango</ref>,
      <ref cel="vent.0ego">ventego</ref>,
      <ref cel="uragan.0o">uragano</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Flut</trd>,
      <trd>Strom</trd>
    </trdgrp>
    <trd lng="nl">stroom</trd>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_o;&c_d;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="torent.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Simila al <tld/>o:
      <ekz>
        <tld/>a rivereto,
      </ekz>
      <ekz>
        <ind><tld/>a pluvo</ind>;
        <trdgrp lng="ru">
          <trd>&c_p;&c_r;&c_o;&c_l;&c_i;&c_v;&c_n;&c_o;&c_j; &c_d;&c_o;&c_zh;&c_d;&c_mol;</trd>,
          <trd>&c_l;&c_i;&c_v;&c_je;&c_n;&c_mol;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <tld/>aj larmoj;
      </ekz>
      <ekz>
        pluvas <tld/>e.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>wildbachartig</trd>,
    <trd>str&ouml;mend</trd>
  </trdgrp>
  <trd lng="nl">wild <ind>stromend</ind></trd>
</drv>

<drv mrk="torent.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Flui <tld/>e:
      <ekz>
        la pluvo <tld/>is<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sxpruc.0i">&scirc;pruci</ref>,
      <ref cel="flu">fluegi</ref>.
    </refgrp>
    <trd lng="nl">stromen</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Sin fortege &jcirc;eti:
      <ekz>
        ili <tld/>is sur lin.
      </ekz>
    </dif>
    <trd lng="nl">zich <ind>storten</ind></trd>
  </snc>
  <trd lng="de">str&ouml;men</trd>
</drv>
</art>
