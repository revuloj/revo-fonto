<art mrk="$Id: rod.xml,v 1.21 2023/10/21 05:38:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>rod</rad>/o
</kap>
<drv mrk="rod.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">MAR</uzo>
    <dif>
      Eksterhavena parto de maro nature a&ubreve; arte &scirc;irmita kontra&ubreve; la
      ventoj, kie &scirc;ipoj povas sendan&gcirc;ere haltadi:
      <ekz>
        &scirc;irmita per duoninsulo <nom>Hel</nom>, e&ccirc; de tiuj ventoj, kiuj dan&gcirc;erigas
        la havenon en Gdansko <klr>[do]</klr> &gcirc;usta grundo por <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=42593">Gdynia</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de <nom>Brest</nom> estas granda golfo je 180 km&quadrat;
        situanta en la departemento <nom>Finist&egrave;re</nom> en Bretonio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=421281">Rodo de Brest</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_j;&c_d; <klr>(&c_m;&c_a;&c_r;&c_s;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>kotvi&scaron;t&ecaron;</trd>,
    <trd>rejda</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Reede</trd>,
    <trd>Bucht</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>road<klr> (nautical)</klr></trd>,
    <trd>roads<klr> (nautical)</klr></trd>,
    <trd>roadstead</trd>,
    <trd>anchorage</trd>
  </trdgrp>
  <trd lng="fr">rade</trd>
  <trdgrp lng="hu">
    <trd>&ouml;b&ouml;l</trd>,
    <trd>ny&iacute;lt <ind>r&eacute;v</ind></trd>,
    <trd>k&uuml;ls&odblac; <ind>horgonyz&oacute;hely</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x505C;&#x6CCA;&#x5730;  <pr>&#x3066;&#x3044;&#x306F;&#x304F;&#x3061;</pr></trd>,
    <trd>&#x6295;&#x9328;&#x5730;  <pr>&#x3068;&#x3046;&#x3044;&#x304B;&#x308A;&#x3061;</pr></trd>
  </trdgrp>
  <trd lng="nl">rede</trd>
  <trd lng="pl">reda</trd>
  <trd lng="pt">enseada</trd>
  <trd lng="ro">rad&abreve;</trd>
  <trd lng="ru">&c_r;&c_je;&c_j;&c_d; <klr tip="amb">(&c_m;&c_o;&c_r;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>kotvi&scaron;te</trd>,
    <trd>otvoren&eacute; pr&iacute;stavisko</trd>,
    <trd>rejda</trd>
  </trdgrp>
  <trd lng="uk">&#x440;&#x435;&#x439;&#x434;</trd>
</drv>
</art>
