<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nekromanci.xml,v 1.16 2024/01/29 15:21:35 revo Exp $">
<kap>
  <rad>nekromanci</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="nekromanci.0o">
  <kap><tld/>o</kap>
  <snc mrk="nekromanci.0o.DIV">
    <dif>
      Fantom<ref tip="super" cel="auxgur.0o.DIV">a&ubreve;gurado</ref>,
      aperigo kaj pridemandado de spiritoj por sortodiveno:
      <ekz>
        <tld/>o estis vaste praktikita de Babilonanoj, Egiptanoj, grekoj
        <fnt>
          <aut>E. Pauli</aut>,
          <vrk><url ref="http://www.cfh.ufsc.br/~simpozio/">
          Enciklopedio simpozio</url></vrk>,
          <lok>artikolo &leftquot;demono&rightquot;, 1999-03</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="okult.0ismo">okultismo</ref>,
      <ref cel="spirit.0ismo">spiritismo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_k;&c_r;&c_a;&c_m;&c_a;&c_n;&c_t;&c_y;&c_ja;</trd>
  <trd lng="ca">nigrom&agrave;ncia</trd>
  <trd lng="cs">p&rcaron;edpov&iacute;d&aacute;n&iacute; budoucnosti vyvol&aacute;v&aacute;n&iacute;m mrtv&yacute;ch</trd>
  <trdgrp lng="de">
    <trd>Nekromantie</trd>,
    <trd>Geisterbeschw&ouml;rung</trd>,
    <trd>S&eacute;ance</trd>
  </trdgrp>
  <trd lng="en">necromancy</trd>
  <trd lng="fr">n&eacute;cromancie</trd>
  <trd lng="hu">halottid&eacute;z&eacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#x964D;&#x970A;&#x5360;&#x3044;  <pr>&#x3053;&#x3046;&#x308C;&#x3044;&#x3046;&#x3089;&#x306A;&#x3044;</pr></trd>,
    <trd>&#x964D;&#x970A;&#x8853;  <pr>&#x3053;&#x3046;&#x308C;&#x3044;&#x3058;&#x3085;&#x3064;</pr></trd>,
    <trd>&#x53E3;&#x5BC4;&#x305B;  <pr>&#x304F;&#x3061;&#x3088;&#x305B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>necromantie</trd>,
    <trd>dodenbezwering</trd>
  </trdgrp>
  <trd lng="pl">nekromancja</trd>
  <trd lng="sk">nekromancia</trd>
  <trd lng="uk">&#x43D;&c_je;&#x43A;&c_r;&#x43E;&#x43C;&c_a;&#x43D;&c_t;&c_ib;&#x44F;</trd>
</drv>

<drv mrk="nekromanci.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="nekromanci.0isto.DIV">
    <dif>
      <ref tip="super" cel="auxgur.0isto.DIV">A&ubreve;guristo</ref> kiu
      aperigas kaj pridemandas spiritojn de mortintoj:
      <ekz>
        malbonintencaj kritikantoj priskribas la seancojn, kiel
        kunsidojn de <tld/>istoj a&ubreve; sor&ccirc;istoj
        <fnt>
          <aut>A. Kardec, trad. A. K. Afondo Costa, trad. B.
          Silva</aut>,
          <vrk><url ref="http://www.febnet.org.br/file/919.pdf">
          La &Ccirc;ielo kaj la Infero</url></vrk>,
          <lok>Brazila Spiritisma Federacio, 1979</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_k;&c_r;&c_a;&c_m;&c_a;&c_n;&c_t;</trd>
  <trdgrp lng="ca">
    <trd>nigrom&agrave;ntic</trd>,
    <trd>nigromant</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nekromant</trd>,
    <trd>Geisterbeschw&ouml;rer</trd>,
    <trd>Schwarzk&uuml;nstler</trd>
  </trdgrp>
  <trd lng="en">necromancer</trd>
  <trdgrp lng="fr">
    <trd>n&eacute;cromancien</trd>,
    <trd>n&eacute;cromant</trd>
  </trdgrp>
  <trdgrp lng="hu">
  <trd>halottid&eacute;z&odblac;</trd>,
  <trd>szellemid&eacute;z&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x964D;&#x970A;&#x5360;&#x3044;&#x5E2B;  <pr>&#x3053;&#x3046;&#x308C;&#x3044;&#x3046;&#x3089;&#x306A;&#x3044;&#x3057;</pr></trd>,
    <trd>&#x964D;&#x970A;&#x8853;&#x5E2B;  <pr>&#x3053;&#x3046;&#x308C;&#x3044;&#x3058;&#x3085;&#x3064;&#x3057;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dodenbezweerder</trd>,
    <trd>necromant</trd>
  </trdgrp>
  <trd lng="pl">nekromanta</trd>
  <trd lng="zh">&#x65B9;&#x58EB; <pr>f&#x101;ngsh&#xEC;</pr></trd>
</drv>
</art>

