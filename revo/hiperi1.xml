<kap>
  <rad>hiperikac</rad>/oj
</kap>
<drv mrk="hiperi1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref
      tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Hypericaceae</trd>):
      <ekz>
        <ref tip="prt" cel="hiperi.0o">hiperiko</ref>
        estas genro de la <tld/>oj, ofte uzataj kiel kuracplantoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21784">Hiperiko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_a;&c_ja;&c_n;&c_mol;&c_n;&c_ib;&c_k;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trd lng="cs">t&rcaron;ezalkovit&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Johanniskrautgew&auml;chse</trd>
  <trdgrp lng="en">
    <trd>Hypericaceae</trd>,
    <trd>St. John's <ind>wort</ind> family</trd>
  </trdgrp>
  <trd lng="fr">hypericac&eacute;es</trd>
  <trd lng="hu">orb&aacute;ncf&udblac;f&eacute;l&eacute;k</trd>
  <trd lng="it">ipericacee</trd>
  <trd lng="nl">hertshooifamilie</trd>
  <trd lng="pl">hienowate</trd>
  <trdgrp lng="pt">
    <trd>gut&iacute;feras</trd>,
    <trd>hiperic&aacute;ceas</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_v;&c_je;&c_r;&c_o;&c_b;&c_o;&c_j;&c_n;&c_y;&c_je;
<klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">&lcaron;ubovn&iacute;kovit&eacute;</trd>
  <trd lng="uk">&c_z;&c_v;&c_ib;&c_r;&c_o;&c_b;&c_ib;&c_j;&c_n;&c_ib;</trd>
  <trd lng="zh">&#37329;&#19997;&#26691; <pr>j&imacron;ns&imacron;t&aacute;o</pr></trd>
</drv>
