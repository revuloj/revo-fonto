<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>raben</rad>/o
</kap>
<drv mrk="raben.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">REL</uzo>
    <dif>
      Scienculo kaj instruanto pri la religia hebrea le&gcirc;o.
    </dif>
  </snc>
  <snc>
    <uzo tip="fak">REL</uzo>
    <dif>
      Hebrea pastro.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_v;&c_ib;&c_n;</trd>
  <trd lng="ca">rab&iacute;</trd>
  <trdgrp lng="de">
    <trd>Rabbiner</trd>,
    <trd>Rabbi</trd>
  </trdgrp>
  <trd lng="en">rabbi</trd>
  <trd lng="fa">&a_kh;&a_A;&a_kh;&a_A;&a_m;</trd>
  <trdgrp lng="fr">
    <trd>rabbin</trd>,
    <trd>rabbi</trd>
  </trdgrp>
  <trd lng="hu">rabbi</trd>
  <trd lng="nl">rabbijn</trd>
  <trdgrp lng="pt">
   <trd>rabino</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_v;&c_v;&c_i;&c_n;</trd>
</drv>
</art>
