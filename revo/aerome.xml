  <trd lng="ru">&c_a;&c_e;&c_r;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>aerometer</trd>,
    <trd>hustomer plynov</trd>
  </trdgrp>
</drv>
</art>
