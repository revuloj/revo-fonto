<kap>
  <rad>hologram</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hologram.0o">
  <kap><tld/>o</kap>
  <snc mrk="hologram.0o.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="dif" cel="holografi.0ajxo">Holografia&jcirc;o</ref>:
      <ekz>
        <nom>Jepsen</nom> anka&ubreve; kontribuis por pluraj nova&jcirc;oj <klr>[&#x2026;]</klr>, 
        inkluzive en la konstruado de la unua holografia videosistemo en 
        la tuta mondo kaj la plej granda <tld/>o en la mondo    
        <fnt>
          <aut>one laptop per child</aut>,
          <vrk><url ref="http://laptop.org/eo/vision/people/MaryLouJepsen/">
          Mary Lou Jepsen</url></vrk>,
          <lok>[vidita en 2007-10]</lok>
        </fnt>;
      </ekz>
      <ekz>
        la novaj monbiletoj de la japana banko havas <tld/>on: tiel oni esperas protekti
        la socion kontra&ubreve; la falsistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo/pg</aut>
          <vrk><url ref="&Monato;/2005/008598.php">Mono-malbono</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_a;&c_l;&c_ja;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>
    <trd lng="bg">&c_h;&c_o;&c_l;&c_o;&c_g;&c_r;&c_a;&c_m;&c_a;</trd>
    <trd lng="de">Hologramm</trd>
    <trd lng="fr">hologramme</trd>
    <trd lng="it">ologramma</trd>
    <trd lng="pl">hologram</trd>
    <trd lng="pt">holograma</trd>
    <trdgrp lng="zh">
    <trd>&#x5168;&#x606F;&#x7167;&#x76F8; <pr>qu&#xE1;nx&#x12B;zh&#xE0;oxi&#x101;ng</pr></trd>,
    <trd>&#x4E09;&#x7DAD;&#x5716; <pr>s&#x101;nw&#xE9;it&#xFA;</pr></trd>,
    <trd>&#x5168;&#x606F;&#x56FE; <pr>qu&#xE1;nx&#x12B;t&#xFA;</pr></trd>,
    <trd>&#x5168;&#x606F;&#x56FE;&#x8C61; <pr>qu&#xE1;nx&#x12B;t&#xFA;xi&#xE0;ng</pr></trd>,
    <trd>&#x5168;&#x606F;&#x7167;&#x7247; <pr>qu&#xE1;nx&#x12B;zh&#xE0;opi&#xE0;n</pr></trd>,
    <trd>&#x7ACB;&#x4F53;&#x7167;&#x76F8; <pr>l&#xEC;t&#x1D0;zh&#xE0;oxi&#xE0;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="cs">hologram</trd>
  <trd lng="ja">&#x30DB;&#x30ED;&#x30B0;&#x30E9;&#x30E0;</trd>
