<vortaro>
<art mrk="$Id: trauxmatiz.xml,v 1.1 2013/11/23 10:10:22 revo Exp $">
<kap>
  <rad>tra&ubreve;matiz</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="trauxmatiz.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="trauxmatiz.0i.MED">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="dif" cel="sxok.0i.KOMUNE">&Scirc;ok</ref>egi, ka&ubreve;zi
      <ref tip="vid" cel="trauxmat.0o">tra&ubreve;maton</ref>:
      <ekz>
        la menso de la <tld/>ita persono ne
        kapablas senkonsciigi la eventojn
        <fnt>
          <aut>D. Broadribb</aut>,
          <vrk><url
          ref="http://www.teleskopo.com/la_ekzamenado_de_songxoj.pdf">
          La Terapia Ekzamenado de Son&gcirc;oj</url></vrk>,
          <lok>Teleskopo, 2010 (2)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">traumatiser</trd>
</drv>
</art>

