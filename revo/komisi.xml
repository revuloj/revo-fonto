<drv mrk="komisi.0itaro">
  <kap><tld/>itaro</kap>
  <snc>
    <fnt><bib>VdE</bib><lok>komisio 2</lok></fnt>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="dif" cel="komisi1.0o">Komisiono</ref>:
      <ekz>
        li aliris Parizon <klr>(iris al Parizo)</klr>, por fari
        &ccirc;e la <tld/>itaro pro tio nomita la necesajn klopodojn
        <fnt>
          <aut>H. Vallienne</aut>
          <vrk>Kastelo de Prelongo</vrk><lok>&ccirc;ap. 2</lok>
        </fnt>;
      </ekz>
      <ekz>
        Max Weber <klr>[&#x2026;]</klr> estis unu el la germanaj reprezentantoj
        en 
        la Traktato de Versajlo kaj ano de la <tld/>itaro por
        skiza<klr>[d]</klr>o de la Konstitucio de Weimar
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=301092">Max
          Weber</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_m;&c_ib;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_ib;&c_t;&c_e;&c_t;</trd>
  </trdgrp>
  <trd lng="cs">&rcaron;e&scaron;itelsk&yacute; t&yacute;m</trd>
  <trd lng="de">Ausschuss</trd>
  <trd lng="es">comisi&oacute;n<klr tip="ind"> (grupo de personas
  en comisi&oacute;n)</klr></trd>
  <trd lng="fr">commission<klr tip="ind"> (groupe de personnes
  commises)</klr></trd>
  <trd lng="nl">commissie</trd>
  <trdgrp lng="pl">
    <trd>komisja</trd>,
    <trd>komitet</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>comitet</trd>,
    <trd>comisie</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_i;&c_t;&c_je;&c_t;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_i;&c_s;&c_s;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">zbor poveren&iacute;kov</trd>
</drv>

<drv mrk="komisi.0ulo">
