<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: bandoni.xml,v 1.12 2019/06/29 12:22:51 revo Exp $">
<kap>
  <rad>bandoni</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>

<drv mrk="bandoni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="harmon2.tir0o" lst="voko:blovinstrumentoj">Tirharmoniko</ref>, 
      pluevoluigita el la <ref cel="koncertin.0o">koncertino</ref>
      de <nom>Heinrich Band</nom>:
      <ekz>
        li scipovas ludi anka&ubreve; germanan kaj rusan tirharmonikojn, <tld/>on, koncertinon
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="https://www.monato.be/2003/008095.html">La segilo sonas!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>bandoneon</trd>,
    <trd>bandonika</trd>,
    <trd>bandonion</trd>,
    <trd>bandonium</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bandoneon</trd>,
    <trd>Bandonion</trd>
  </trdgrp>
  <trd lng="en">bandoneon</trd>
  <trd lng="es">bandone&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>bandon&eacute;on</trd>
  </trdgrp>
  <trd lng="hu">bandoneon</trd>
  <trdgrp lng="it">
    <trd>bandoneon</trd>,
    <trd>bandonion</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_a;&c_n;&c_d;&c_o;&c_n;&c_je;&c_o;&c_n;</trd>
  <trd lng="sk">bandonion</trd>
</drv>
</art>

