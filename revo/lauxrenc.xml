<vortaro>
  <art mrk="$Id: lauxrenc.xml,v 1.13 2024/04/03 06:28:34 revo Exp $">
    <kap>
      <rad>La&ubreve;renc</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>

    <drv mrk="lauxrenc.0o">
      <kap><tld/>o</kap>
      <snc mrk="lauxrenc.0o.NOM">
        <uzo tip="fak">NOM</uzo>
        <dif>
          Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref>
          latindevena:
          <ekz>
            la unua filipina beatulo, <tld/>o Ruiz
            <fnt>
              <aut>A. De Salvo</aut>,
              <vrk>La papo al Azio</vrk>,
              <lok><url ref="http://www.ikue.org/ek/ek1981_0304.htm">Espero
                  Katolika, 1981, numero 3-4a</url></lok>
            </fnt>;
          </ekz><ekz>
            voja&gcirc;fotoj de <tld/>o kaj Gudula
            <fnt>
              <aut>Amikeca Reto</aut>,
              <vrk><url ref="http://www.creaweb.fr/a.reto/gudfoto.html">Voja&gcirc;fotoj...</url></vrk>,
              <lok>2001-01-10</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_V;&c_a;&c_w;&c_r;&c_y;&c_n;&c_je;&c_c;</trd>,
        <trd>&c_L;&c_a;&c_w;&c_r;&c_e;&c_n;</trd>,
        <trd>&c_L;&c_a;&c_w;&c_r;&c_e;&c_n;&c_c;&c_ib;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Laurentius</trd>,
        <trd>Laurenz</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>Laurence</trd>,
        <trd>Lawrence</trd>
      </trdgrp>
      <trd lng="fr">Laurent</trd>
      <trd lng="it">Lorenzo</trd>
      <trd lng="lat">Laurentius</trd>
      <trd lng="nl">Laurens</trd>
      <trdgrp lng="pl">
        <trd>Wawrzyniec</trd>
        <trd>Laurencjusz</trd>,
        <trd>Laurencja<klr> (imi&eogonek; &zdot;e&nacute;skie)</klr></trd>
      </trdgrp>
      <trd lng="pt">Louren&ccedil;o</trd>
      <trdgrp lng="ro">
        <trd>Lauren&#x21B;iu</trd>,
        <trd>Lavrentie</trd>
      </trdgrp>
      <trd lng="ru">&c_L;&c_a;&c_v;&c_r;&c_je;&c_n;&c_t;&c_i;&c_j;</trd>
      <trd lng="uk">&#x41B;&c_a;&c_v;&c_r;&c_je;&#x43D;&c_t;&c_ib;&c_j;</trd>
</drv>
  </art>
