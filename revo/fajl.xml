<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fajl</rad>/i
</kap>

<drv mrk="fajl.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Malkrudigi a&ubreve; glatigi &ccirc;u metalon, &ccirc;u lignon, 
      skrapante &gcirc;in per speciala ilo.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;
      &c_n;&c_a;&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_ib;&c_k;&c_a;&c_m;</trd>,
      <trd>&c_sh;&c_l;&c_ib;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">feilen</trd>
    <trd lng="en">file</trd>
    <trd lng="hu">reszel</trd>
    <trdgrp lng="pl">
      <trd>pi&lstroke;owa&cacute;<klr> (pilnikiem)</klr></trd>,
      <trd>g&lstroke;adzi&cacute;<klr> (pilnikiem)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>limar</trd>,
      <trd>polir <klr tip="amb">(com lima)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_r;&c_a;&c_b;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol;
      &c_n;&c_a;&c_p;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_k;&c_o;&c_m;</trd>,
      <trd>&c_sh;&c_l;&c_i;&c_f;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Delikatigi, perfektigi, detale finpretigi iun verkon:
      <ekz>
        dum longaj jaroj Z obstine <tld/>is sian lingvon.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="polur.0i">poluri</ref>,
      <ref cel="rafin.0i">rafini</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_sh;&c_l;&c_ib;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_u;&c_d;&c_a;&c_s;&c_k;&c_a;&c_n;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">feilen</trd>
    <trd lng="hu">csiszol</trd>
    <trdgrp lng="pl">
      <trd>polerowa&cacute;</trd>,
      <trd>wyg&lstroke;adza&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_sh;&c_l;&c_i;&c_f;&c_o;&c_v;&c_a;&c_t;&c_mol;
    <klr>(&c_p;&c_je;&c_r;&c_je;&c_n;.)</klr></trd>
  </snc>
  <adm>
    Pri kolizio inter fakindiko kaj FIG:
    En cxi tiu artikolo la unua senco estas tehxnika kaj
    la dua estas metaforo de la tehxnika senco. Cxu oni
    metu la fakindikon al la tuta vorto (kiel mi faris) aux
    nur al la unua senco? <aut>MB</aut>
  </adm>
  <trd lng="bg">&c_p;&c_i;&c_l;&c_ja;</trd>
  <trd lng="de">feilen</trd>
  <trd lng="es">limar</trd>
  <trdgrp lng="fr">
    <trd>limer</trd>,
    <trd>peaufiner</trd>,
    <trd>fignoler</trd>
  </trdgrp>
  <trd lng="nl">vijlen</trd>
  <trd lng="sv">fila</trd>
</drv>

<drv mrk="fajl.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Metalaj deskrapa&jcirc;oj, devenantaj de la <tld/>ado.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_l;&c_a;&c_v;&c_ib;&c_n;&c_mol;&c_n;&c_je;
  <klr
  tip="amb">(&c_m;&c_e;&c_t;&c_a;&c_l;&c_ib;&c_ch;&c_n;&c_a;&c_je;)</klr></trd>
  <trdgrp lng="de">
    <trd>Feilsp&auml;ne</trd>,
    <trd>Feilstaub</trd>
  </trdgrp>
  <trd lng="en">filing</trd>
  <trd lng="fr">limaille</trd>
  <trd lng="hu">reszel&eacute;k</trd>
  <trd lng="nl">vijlsel</trd>
  <trdgrp lng="pl">
    <trd>opi&lstroke;ki<klr> {pl.}</klr></trd>,
    <trd>opi&lstroke;ek<klr> {sing.}</klr></trd>
  </trdgrp>
  <trd
  lng="ru">&c_m;&c_je;&c_t;&c_a;&c_l;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_je;
  <ind>&c_o;&c_p;&c_i;&c_l;&c_k;&c_i;</ind></trd>
  <trd lng="sv">filsp&aring;n</trd>
</drv>

<drv mrk="fajl.0ilo">
  <kap><tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Ilo, provizita je multo da strietoj, per kiu oni <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="bg">&c_p;&c_i;&c_l;&c_a;</trd>
  <trd lng="de">Feile</trd>
  <trd lng="en">file</trd>
  <trd lng="fr">lime</trd>
  <trd lng="hu">reszel&odblac;</trd>
  <trd lng="nl">vijl</trd>
  <trd lng="pl">pilnik</trd>
  <trd lng="ru">&c_n;&c_a;&c_p;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  <trd lng="sv">fil</trd>
</drv>
</art>
