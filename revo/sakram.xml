<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>sakrament</rad>/o
    </kap>
    <drv mrk="sakram.0o">
      <kap><tld/>o</kap>
      <snc  mrk="sakram.0o.rel">
        <uzo tip="fak">REL</uzo>
        <dif>
          Rita ago, destinita por komuniki al iu iom el ia supernatura
          dia forto.
        </dif>
        <trdgrp lng="be">
          <trd>&c_m;&c_ib;&c_s;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>,
          <trd>&c_r;&c_e;&c_l;&c_ib;&c_g;&c_ib;&c_j;&c_n;&c_a;&c_ja;
          &c_c;&c_y;&c_r;&c_y;&c_m;&c_o;&c_n;&c_ib;&c_ja;</trd>
        </trdgrp>
        <trdgrp lng="de">
           <trd>religi&ouml;se Zeremonie</trd>,
           <trd>Mysterien</trd>
        </trdgrp>
        <trd lng="pl">misterium</trd>
        <trd lng="ru">&c_m;&c_i;&c_s;&c_t;&c_je;&c_r;&c_i;&c_ja;</trd>
      </snc>
      <snc mrk="sakram.0o.kri">
        <uzo tip="fak">KRI</uzo>
        <dif>
          Rito iniciatita de Kristo por doni a&ubreve; pliigi la
          difavoron al iu a&ubreve; por havigi pardonon pri peko.
        </dif>
        <refgrp tip="vid">
          <ref cel="grac.0o">graco</ref>.
        </refgrp>
        <trd lng="be">&c_s;&c_a;&c_k;&c_r;&c_a;&c_m;&c_a;&c_n;&c_t;
        </trd>
        <trd lng="de">Sakrament</trd>
        <trd lng="en">sacrament</trd>
        <trd lng="nl">sacrament</trd>
        <trd lng="pl">sakrament</trd>
        <trdgrp lng="pt">
            <trd>sacramento</trd>
        </trdgrp>
        <trd lng="ru">&c_t;&c_a;&c_i;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>
      </snc>
    <trd lng="es">sacramento</trd>
    <trdgrp lng="hu">
      <trd>szents&eacute;g</trd>,
      <trd>szakramentum</trd>
    </trdgrp>
    <trd lng="it">sacramento</trd>
    </drv>
  </art>
