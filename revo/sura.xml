<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sura.xml,v 1.14 2023/11/28 09:26:59 revo Exp $">
<kap>
  <rad>sura</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="sura.0o">
  <kap><tld/>o</kap>
  <snc mrk="sura.0o.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      <ref tip="super" cel="cxapit.0o.TIP">&Ccirc;apitro</ref> de la <ref tip="malprt"
      cel="koran.0o.ISL">Korano</ref>:
      <ekz>
        oni la&ubreve;tlegis koranajn <tld/>ojn
        <fnt>
          <aut>N. Ruggiero</aut>,
          <vrk><url ref="http://www.nicolaruggiero.it/html/eo/">taglibro
          de nenifaranto</url></vrk>,
          <lok>2004-11-11</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Korano postulas en <klr>...</klr> <tld/>o 61 eksplicite
        pacpretecon : &leftquot;Kaj se ili estas pacemaj, tiam estu anka&ubreve; vi
        pacemaj kaj fidu Alahon.&rightquot;
        <fnt>
          <aut>J. Galtung, trad. V. L.</aut>,
          <vrk><url ref="http://mas-eo.org/article.php3?id_article=89">Davoso a&ubreve;
          Portalegro...</url></vrk>,
          <lok>Monda Asembleo Socia, 2006-03-31</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_r;&c_a; <klr>(&c_u; &c_K;&c_a;&c_r;&c_a;&c_n;&c_je;)</klr></trd>
  <trd lng="bg">&c_s;&c_u;&c_r;&c_a;</trd>
  <trd lng="cs">s&#xFA;ra</trd>
  <trd lng="de">Sure</trd>
  <trd lng="en">sura</trd>
  <trd lng="es">sura</trd>
  <trd lng="fr">sourate</trd>
  <trd lng="hu">sz&uacute;ra</trd>
  <trdgrp lng="id">
    <trd>Surah</trd>,
    <trd>Surat</trd>
  </trdgrp>
  <trd lng="it">sura</trd>
  <trd lng="ja">&#x30B9;&#x30FC;&#x30E9;</trd>
  <trd lng="nl">soera</trd>
  <trd lng="pl">sura</trd>
  <trd lng="sk">s&#xFA;ra</trd>
  <trd lng="uk">&c_s;&c_u;&c_r;&c_a; (&c_v;&c_ib;&c_r;&c_sh; &c_z; &#x41A;&#x43E;&c_r;&c_a;&#x43D;&c_u;)</trd>
</drv>
</art>

