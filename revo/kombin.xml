<drv mrk="kombin.lineara0ajxo">
  <kap>lineara <tld/>a&jcirc;o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV2</bib></fnt>
  <snc>
    <dif>
      <klr>(el finia nombro da <ref cel="vektor.0o.MAT">vektoroj</ref>
      <frm am="x_i"><k>x</k><sub><k>i</k></sub></frm>)</klr>
      Sumo de termoj, konsistantaj el produto de &ccirc;iu el la vektoroj
      <frm am="x_i"><k>x</k><sub><k>i</k></sub></frm> per
      <ref cel="skalar.0o.MAT">skalaro</ref>
      <frm am="alpha_i">&alfa;<sub><k>i</k></sub></frm>; la skalaroj estas
      nomataj
      <ref cel="koefic.0o.lineara">koeficientoj</ref>:
      <ekz>
        <frm am="sum alpha_i*x_i">&#8721; &alfa;<sub><k>i</k></sub>&#8729;<k>x</k><sub><k>i</k></sub></frm>;
      </ekz>
      <ekz>
        modulo <frm am="M"><k>M</k></frm> estas finie generita se tie
        ekzistas 
        finie multaj eroj <frm am="x_1,...,x_n"><k>x</k><sub>1</sub>, &#8230;, <k>x</k><sub>n</sub></frm> 
        en <frm am="M"><k>M</k></frm> tiaj ke &ccirc;iu ero de 
        <frm am="M"><k>M</k></frm>
        estas lineara <tld/>a&jcirc;o de tiuj eroj kun koeficientoj de la
        skalara
        ringo <frm am="R"><k>R</k></frm>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=87314">Modulo
          (matematiko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="pend.linearenede0a">lineare nedependa.</ref>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_n;&c_je;&c_j;&c_n;&c_a;&c_ja;
  <ind>&c_k;&c_a;&c_m;&c_b;&c_ib;&c_n;&c_a;&c_c;&c_y;&c_ja;</ind></trd>
  <trd lng="de">Linearkombination</trd>
  <trd lng="en">linear <ind>combination</ind></trd>
  <trd lng="fr">combinaison lin&eacute;aire</trd>
  <trd lng="pl">kombinacja liniowa</trd>
  <trd lng="ro">combina&#539;ie liniar&abreve;</trd>
  <trd lng="ru">&c_l;&c_i;&c_n;&c_je;&c_j;&c_n;&c_a;&c_ja;
  <ind>&c_k;&c_o;&c_m;&c_b;&c_i;&c_n;&c_a;&c_c;&c_i;&c_ja;</ind></trd>
  <trd lng="uk">&c_l;&c_ib;&c_n;&c_ib;&c_j;&c_n;&c_a; &c_k;&c_o;&c_m;&c_b;&c_ib;&c_n;&c_a;&c_c;&c_ib;&c_ja;</trd>
</drv>
