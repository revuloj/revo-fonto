<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>hirt</rad>/a
</kap>
<drv mrk="hirt.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rekte staranta (p.p. haroj a&ubreve; simila&jcirc;oj):
      <ekz>
        la <tld/>aj pikiloj de atakata histriko.
      </ekz>
    </dif>
    <trd lng="de">borstig</trd>
    <trd lng="nl">rechtopstaand</trd>
  </snc>
  <snc>
    <dif>
      Konfuze malordigita, malglata:
      <ekz>
        <tld/>a barbo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a stilo.
      </ekz>
    </dif>
    <trd lng="de">struppig</trd>
    <trdgrp lng="nl">
      <trd>ruig</trd>,
      <trd>verward</trd>
    </trdgrp>
  </snc>
  <trd lng="en">bristly</trd>
  <trdgrp lng="fr">
    <trd>&eacute;bouriff&eacute;</trd>,
    <trd>h&eacute;riss&eacute;</trd>,
    <trd>hirsute</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>hirto</trd>,
    <trd>eri&ccedil;ado</trd>,
    <trd>hirsuto</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_malmol;&c_je;&c_r;&c_o;&c_sh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_s;&c_k;&c_l;&c_o;&c_k;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_o;&c_p;&c_o;&c_r;&c_shch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="hirt.0igi">
  <kap><tld/>igi</kap>
  <dif>
    Igi ion <tld/>a:
  </dif>
  <snc>
    <ekz>
      la kato timante <tld/>igas siajn harojn;
    </ekz>
    <ekz>
      la kolero <tld/>igas la plumojn de la koko;
    </ekz>
    <trd lng="de">str&auml;uben</trd>
    <trd lng="nl">rechtop zetten</trd>
    <trd lng="pt">eri&ccedil;ar</trd>
  </snc>
  <snc>
    <ekz>
      la vento dis<tld/>igis la kapon de la infanoj.
    </ekz>
    <trd lng="nl">overhoop halen</trd>
  </snc>
  <trd lng="de">verstrubbeln</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;risser</trd>,
    <trd>&eacute;bouriffer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_malmol;&c_je;&c_r;&c_o;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_o;&c_p;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
