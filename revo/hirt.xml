<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hirt.xml,v 1.28 2025/10/17 20:37:07 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>hirt</rad>/a
<fnt><bib>JED</bib></fnt></kap>

<drv mrk="hirt.0a">
  <kap><tld/>a</kap>
  <snc mrk="hirt.0a.rekte">
    <dif>
      <ref tip="super" cel="rekt.0a.vertikala">Rekte</ref> staranta
      (p.p. haroj a&ubreve; simila&jcirc;oj):
      <ekz>
        la <tld/>aj pikiloj de atakata histriko;
      </ekz>
      <ekz>
        kolero denove komencis <tld/>igi la harojn sur lia kapo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vagpense rigardadis la kandelon dum duonminuto, kaj je subita konstato mia
        hararo <tld/>i&gcirc;is, la &scirc;vito guti&gcirc;is sur mia frunto
        <fnt>
          <bib>KrM</bib>
          <lok>7. Belaj Rakontoj El Trans La Montoj.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Wells</nom> sentis la harojn <tld/>i&gcirc;i sur lia nuko
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/007991.html">Espero</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hauxt.anser0o">anserha&ubreve;to</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>gestr&auml;ubt</trd>,
      <trd>borstig</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>erizado</trd>,
      <trd>hirsuto</trd>,
      <trd>h&iacute;spido</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>fel&aacute;ll&oacute;</trd>,
      <trd>borzas</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>irto</trd>,
      <trd>ritto</trd>
    </trdgrp>
    <trd lng="nl">rechtopstaand</trd>
  </snc>
  <snc mrk="hirt.0a.konfuze">
    <dif>
      <ref tip="super" cel="konfuz.0a.senorda">Konfuze</ref> malordigita,
      <ref cel="glat.mal0a">malglata</ref>:
      <ekz>
        maljuna rabista virino, kiu havis longan <tld/>an barbon kaj brovojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a pavianino
        <fnt>
          <bib>HsT</bib>
          <lok>Disaj vojoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mizera, <tld/>a hunda&ccirc;o, kiu pli similis eluzitan skraptapi&scirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2006/008788.php">Tenebro</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        hararo <tld/>a, ta&ubreve;za
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi razis min, mian <tld/>an hararon tondis la elektra &ccirc;apelo, miaj pedikoj estis
        ekstermitaj
        <fnt>
          <bib>VaK</bib>
          <lok>Deksepa &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a herbo, vipata de la vento, kovras abunde la tutan tombejon
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kie ili lasis la honton? <klr>[&#8230;]</klr> homoj ili ne plu estis, nur griza,
        malseka, <tld/>a amaso kun apena&ubreve;a me&ccirc;o de la
        konscio
        <fnt>
          <bib>LGA</bib>
          <lok>II.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vil.0a">vila</ref>
    </refgrp>
    <trd lng="de">struppig</trd>
    <trdgrp lng="it">
      <trd>ispido</trd>,
      <trd>arruffato</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>ruig</trd>,
      <trd>verward</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_k;&c_u;&c_d;&c_l;&c_a;&c_ch;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_s;&c_k;&c_a;&c_l;&c_m;&c_a;&c_ch;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_a;&c_p;&c_y;&c_r;&c_a;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>je&zcaron;at&yacute;</trd>,
    <trd>kostrbat&yacute;</trd>,
    <trd>zje&zcaron;en&yacute;</trd>,
    <trd>&ccaron;n&ecaron;j&iacute;c&iacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bristly</trd>,
    <trd>bushy</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;bouriff&eacute;</trd>,
    <trd>h&eacute;riss&eacute;</trd>,
    <trd>hirsute</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#36870;&#x7ACB;&#12387;&#x305F;  <pr>&#x304E;&#12419;&#x304F;&#x305F;&#12387;&#x305F;</pr></trd>,
    <trd>&#x4E71;&#x308C;&#x305F;  <pr>&#x307F;&#12384;&#x308C;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="pl">chropowaty</trd>
  <trdgrp lng="pt">
    <trd>hirto</trd>,
    <trd>eri&ccedil;ado</trd>,
    <trd>hirsuto</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_malmol;&c_je;&c_r;&c_o;&c_sh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_s;&c_k;&c_l;&c_o;&c_k;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_o;&c_p;&c_o;&c_r;&c_shch;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>naje&zcaron;en&yacute;</trd>,
    <trd>strapat&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&c_ji;&c_zh;&c_a;&c_ch;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_a;&c_ji;&c_zh;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_a;&c_s;&c_t;&#x43E;&c_v;&c_b;&c_u;&c_r;&c_ch;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43A;&c_u;&c_j;&#x43E;&c_v;&c_d;&c_zh;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43A;&#x43E;&c_sh;&#x43B;&c_a;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&c_a;&c_t;&#x43B;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_a;&c_t;&#x43B;&c_a;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="hirt.0igi">
  <kap><tld/>igi</kap>
  <dif>
    Igi ion <tld/>a:
  </dif>
  <snc ref="hirt.0a.rekte">
    <ekz>
      la birdo <klr>[&#8230;]</klr> elpin&ccirc;as el &gcirc;i plumon, hirtigas la aliajn plumojn &ccirc;irka&ubreve;
      la kolo
      <fnt>
        <bib>Fab2</bib>
        <lok>Bildo el kastelremparo</lok>
      </fnt>;
    </ekz>
    <ekz>
      teruro denove <tld/>igis la harojn sur lia kapo
      <fnt>
        <bib>QuV</bib>
        <lok>&Ccirc;apitro XXIV</lok>
      </fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="rekt.0igi.KOMUNE">rektigi<sncref/></ref>,
      <ref cel="star.0igi.vertikala">starigi<sncref/></ref>
    </refgrp>
    <trd lng="de">str&auml;uben</trd>
    <trd lng="it">rizzare</trd>
    <trd lng="nl">rechtop zetten</trd>
    <trd lng="pt">eri&ccedil;ar</trd>
  </snc>
  <snc ref="hirt.0a.konfuze">
    <ekz>
      la kampon <tld/>igis milda vento
      <fnt>
        <bib>VaK</bib>
        <lok>Tria &Ccirc;apitro</lok>
      </fnt>;
    </ekz>
    <ekz>
      la vento <tld/>igis liajn maldensajn harojn
      <fnt>
        <bib>HsT</bib>
        <lok>Feli&ccirc;a homo</lok>
      </fnt>;
    </ekz>
    <ekz>
      <uzo tip="stl">FIG</uzo>
      la libro <klr>[&#8230;]</klr> estas <tld/>igita je obskuraj ideoj a&ubreve; esprimoj, kaj
      strangaj parolturnoj
      <fnt>
        <bib>Monato</bib>
        <aut>Marcel Lemeltier</aut>
        <vrk><url ref="&Monato;/2012/010235.php">&Ccirc;u forgesita trezoro?</url></vrk>
        <lok>2012</lok>
      </fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="ord.mal0igi">malordigi</ref>,
      <ref cel="tauxz.0i.malordige">ta&ubreve;zi<sncref/></ref>
    </refgrp>
    <trd lng="it">arruffare</trd>
    <trd lng="nl">overhoop halen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_k;&c_u;&c_d;&c_l;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_k;&c_a;&c_l;&c_m;&c_a;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_t;&c_a;&c_p;&c_y;&c_r;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>naje&zcaron;it</trd>,
    <trd>zje&zcaron;it</trd>,
    <trd>&ccaron;ep&yacute;&rcaron;it</trd>
  </trdgrp>
  <trd lng="de">verstrubbeln</trd>
  <trdgrp lng="en">
    <trd>tousle</trd>,
    <trd>bristle</trd>
  </trdgrp>
  <trd lng="es">erizar</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;risser</trd>,
    <trd>&eacute;bouriffer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felborzol</trd>,
    <trd>&ouml;sszek&oacute;col</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#36870;&#x7ACB;&#12390;&#x308B;  <pr>&#12373;&#x304B;&#12384;&#12390;&#x308B;</pr></trd>,
    <trd>&#x4E71;&#12377;  <pr>&#x307F;&#12384;&#12377;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>naje&zdot;y&cacute;</trd>,
    <trd>wyprostowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_malmol;&c_je;&c_r;&c_o;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_s;&c_t;&c_o;&c_p;&c_o;&c_r;&c_shch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>naje&zcaron;i&tcaron;</trd>,
    <trd>zje&zcaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&c_ji;&c_zh;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&#x43D;&c_ib;&#x43C;&c_a;&c_t;&c_i; &c_s;&c_t;&#x43E;&c_r;&c_ch;</trd>,
    <trd>&c_s;&c_t;&#x43E;&c_v;&c_b;&c_u;&c_r;&c_ch;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_a;&c_s;&c_t;&#x43E;&c_v;&c_b;&c_u;&c_r;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43A;&c_u;&c_j;&#x43E;&c_v;&c_d;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_z;&#x43F;&c_a;&c_t;&#x43B;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>
