<art mrk="$Id: erudit.xml,v 1.17 2023/11/07 20:34:11 revo Exp $">
<kap>
  <rad>erudit</rad>/o
</kap>
<drv mrk="erudit.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="erudic.0ulo">Erudiciulo</ref>:
      <ekz>
        mi sciis, ke li estas <tld/>o, pro liaj intervenoj dum debatoj pri iu ajn temo
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2010/009958.php">Kosmologio de nia jarcento</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kunmiksa&jcirc;oj de elementoj el malsamaj idiomoj, kies fremdaj ingrediencoj estis
        pruntitaj de <tld/>oj kaj verkistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2012/010679.php">Lingvoj, &ccirc;u naturaj, &ccirc;u artefaritaj?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        lukto por paco kaj justeco estas <nac>&gcirc;ihad</nac>, sen <nac>&gcirc;ihad</nac>
        tiu lukto estus malsincera kaj duonkora, multe da islamaj <tld/>oj kaj
        iliaj studoj tion konfirmas
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2014/011402.php">Ne interveni signifas ne solvi</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_r;&c_u;&c_d;&c_y;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>erudovan&yacute; odborn&iacute;k</trd>,
    <trd>vzd&ecaron;lanec</trd>,
    <trd>zb&ecaron;hl&yacute; &ccaron;lov&ecaron;k</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Gelehrter</trd>,
     <trd>gebildeter Mann</trd></trdgrp>
  <trd lng="es">erudito</trd>
  <trd lng="fr">&eacute;rudit</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;pzetts&eacute;g</trd>,
    <trd>tanults&aacute;g</trd>
  </trdgrp>
  <trd lng="pl">erudyta</trd>
  <trd lng="pt">erudito</trd>
  <trd lng="ru">&c_e;&c_r;&c_u;&c_d;&c_i;&c_t;</trd>
  <trd lng="sk">erudovan&yacute; odborn&iacute;k</trd>
  <trd lng="uk">&#x435;&#x440;&#x443;&#x434;&#x438;&#x442;</trd>
  <trd lng="zh">&#x5148;&#x8CE2; <pr>xi&#x101;nxi&#xE1;n</pr></trd>
</drv>
</art>
