<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>alez</rad>/i
</kap>
<drv mrk="alez.0i">
<kap><tld/>i</kap>
<uzo tip="fak">TEK</uzo>
<snc>
<gra><vspec>tr</vspec></gra>
<dif>
Glatigi la internon de cilindro, tubo, ktp. per  speciala ilo.
</dif>
</snc>
<trdgrp lng="be">
  <trd>&c_r;&c_a;&c_s;&c_t;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>, 
  <trd>&c_r;&c_a;&c_s;&c_mol;&c_s;&c_mol;&c_v;&c_ib;&c_d;&c_r;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
</trdgrp>
<trdgrp lng="en">
  <trd>drill</trd>,
  <trd>bore</trd>,
  <trd>ream</trd>
</trdgrp>
<trd lng="fr">al&eacute;ser</trd>
<trd lng="hu">d&ouml;rzs&aacute;raz</trd>
<trd lng="nl">uitboren</trd>
<trd lng="pt"><ind>brocar</ind> <klr>um cilindro</klr></trd>
<trdgrp lng="ru">
  <trd>&c_s;&c_v;&c_je;&c_r;&c_l;&c_i;&c_t;&c_mol;</trd>,
  <trd>&c_r;&c_a;&c_s;&c_s;&c_v;&c_je;&c_r;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
  <trd>&c_r;&c_a;&c_s;&c_t;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
</trdgrp>
<trd lng="sk">vybr&uacute;si&tcaron; <klr>(valec zvn&uacute;tra)</klr></trd>
</drv>
<drv mrk="alez.0ilo">
<kap><tld/>ilo</kap>
<snc>
<dif>
Instrumento por alezi.
</dif>
</snc>
<trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_r;&c_d;&c_z;&c_je;&c_l;</trd>
<trd lng="en">drill</trd>
<trd lng="fr">al&eacute;soir</trd>
<trd lng="hu">d&ouml;rzs&aacute;r</trd>
<trd lng="pt">mandril</trd>
<trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_l;&c_o;</trd>
</drv>
</art>
