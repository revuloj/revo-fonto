<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fug</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="fug.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kompon.0ajxo">Kompona&jcirc;o</ref>
      kutime por <ref tip="vid" cel="orgen.0o">orgeno</ref>
      a&ubreve; alia klav-muzikilo, en kiu
      melodion komencas konduka vo&ccirc;o kaj ripetas post
      mallongaj prokrastoj imitaj vo&ccirc;oj
      konforme al <ref tip="vid" cel="kontra7.0o">kontrapunktaj</ref>
      reguloj.
    </dif>
  </snc>
  <trd lng="de">Fuge</trd>
  <trd lng="fr">fugue<klr> (musicale)</klr></trd>
  <trd lng="nl">fuga</trd>
  <trd lng="ru">&c_f;&c_u;&c_g;&c_a;</trd>
</drv>
</art>
