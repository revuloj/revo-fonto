<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fug</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="fug.0o">  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Muzika &gcirc;enro populara en e&ubreve;ropa klasiko,
      ofte plenumata per <ref tip="vid" cel="orgen.0o">orgeno</ref>
      a&ubreve; alia klav-muzikilo, en kiu
      melodion prezentas konduka vo&ccirc;o kaj ripetas post
      mallongaj prokrastoj imitaj vo&ccirc;oj
      konforme al <ref tip="vid" cel="kontra7.0o">kontrapunktaj</ref> reguloj.
    </dif>
  </snc>
  <trd lng="de">Fuge</trd>
  <trd lng="fr">fugue<klr> (musique)</klr></trd>
  <trd lng="nl">fuga</trd>
  <trd lng="ru">&c_f;&c_u;&c_g;&c_a;</trd>
</drv>
</art>
