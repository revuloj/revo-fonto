<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rund.xml,v 1.16 2025/04/05 10:02:33 revo Exp $">
<kap>
  <rad>rund</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="rund.0i">
  <kap><tld/>i</kap>
  <snc mrk="rund.0i.MIL">
    <uzo tip="stl">RAR</uzo>
    <dif>
      <ref tip="sin" cel="ir.rond0i.cxien">Rondiri</ref>
      tra loko por &gcirc;in <ref tip="super" cel="kontro.0i">kontroli</ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_a;&c_b;&c_h;&c_o;&c_d;</ind></trd>,
      <trd>&c_a;&c_b;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; <klr>(&c_v;&c_a;&c_r;&c_t;&c_a;&c_j;)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kontrollgang machen</trd>,
      <trd>Runde gehen<klr tip="amb"> (kontrollieren)</klr></trd>
    </trdgrp>
    <trd lng="fr">faire une <ind>ronde</ind><klr> (de
    surveillance)</klr></trd>
    <trdgrp lng="hu">
      <trd>j&aacute;r&odblac;r&ouml;z</trd>,
      <trd>cirk&aacute;l</trd>,
      <trd>ellen&odblac;rz&odblac; <ind>k&ouml;rutat</ind> tesz</trd>
    </trdgrp>
    <trd lng="it">fare la <ind>ronda</ind></trd>
    <trdgrp lng="pl">
      <trd>obch&oacute;d wykona&cacute;</trd>,
      <trd>rund&eogonek; zrobi&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>rondar</trd>,
      <trd>fazer a <ind>ronda</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_o;&c_b;&c_h;&c_o;&c_d;</ind></trd>,
      <trd>&c_p;&c_a;&c_t;&c_r;&c_u;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#24033;&#36923; <pr>x&#250;nlu&#243;</pr></trd>,
    <trd>&#24033;&#37007; <pr>x&#250;nlu&#243;</pr></trd>
  </trdgrp>
</snc>
</drv>

<drv mrk="rund.0o">
  <kap><tld/>o</kap> <fnt><bib>PIV1</bib></fnt>
  <snc mrk="rund.0o.MIL">
    <uzo tip="stl">RAR</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      Kontrola vizito, rondiro en iu loko por sekureco, ordo:
      <ekz>
        la junulo foriris, kaj la gubernatoro da&ubreve;rigis sian <tld/>on
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 49a, p.
        668a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rauxnd.0o">ra&ubreve;ndo</ref>,
      <ref cel="rond.0o">rondo</ref>
    </refgrp>
    <trd lng="be">&c_a;&c_b;&c_h;&c_o;&c_d;</trd>
    <trdgrp lng="cs">
      <trd>kolo sport. z&aacute;pasu</trd>,
      <trd>obch&uring;zka</trd>,
      <trd>vylu&ccaron;ovac&iacute; st&rcaron;etnut&iacute;</trd>,
      <trd>vylu&ccaron;ovac&iacute; z&aacute;pasy</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kontrollgang</trd>,
      <trd>Runde<klr tip="amb"> (Kontrollgang)</klr></trd>
    </trdgrp>
    <trd lng="fr">ronde<klr> (de surveillance)</klr></trd>
    <trdgrp lng="hu">
      <trd>j&aacute;r&odblac;r&ouml;z&eacute;s</trd>,
      <trd>ellen&odblac;rz&odblac; k&ouml;r&uacute;t</trd>
    </trdgrp>
    <trd lng="it">ronda</trd>
    <trdgrp lng="pl">
      <trd>obch&oacute;d</trd>,
      <trd>runda</trd>
    </trdgrp>
    <trd lng="pt">ronda</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_o;&c_z;&c_o;&c_r;</trd>,
      <trd>&c_o;&c_b;&c_h;&c_o;&c_d;</trd>,
      <trd>&c_p;&c_a;&c_t;&c_r;&c_u;&c_l;&c_mol;</trd>
    </trdgrp>
    <trd lng="sk">kolo &scaron;portov&eacute;ho z&aacute;pasu</trd>
    <trdgrp lng="zh">
      <trd>&#24033;&#35222;&#22280; <pr>x&#250;nsh&#236;qu&#257;n</pr></trd>,
      <trd>&#24033;&#35270;&#22280; <pr>x&#250;nsh&#236;qu&#257;n</pr></trd>,
      <trd>&#24033;&#36923;&#21306;&#22495; <pr>x&#250;nlu&#243;q&#363;y&#249;</pr></trd>,
      <trd>&#24033;&#37007;&#21312;&#22495; <pr>x&#250;nlu&#243;q&#363;y&#249;</pr></trd>,
      <trd>&#35222;&#23519;&#21312; <pr>sh&#236;ch&#225;q&#363;</pr></trd>,
      <trd>&#35270;&#23519;&#21306; <pr>sh&#236;ch&#225;q&#363;</pr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>patrolrondiro</ke>.
  </rim>

  <rim>
    <ref tip="hom" cel="rund1.0o">Rundo</ref> estas anka&ubreve; parto de riptetata procedo, ekz. vo&ccirc;donado
    a&ubreve; ludo.
  </rim>
  <trdgrp lng="uk">
    <trd>&#1086;&c_b;&c_h;&c_ib;&c_d; (&#1082;&#1086;&#1085;&c_t;&c_r;&#1086;&#1083;&#1100;&#1085;&c_i;&c_j;)</trd>,
    <trd>&c_r;&c_a;&c_u;&#1085;&c_d;</trd>
  </trdgrp>
</drv>
</art>

