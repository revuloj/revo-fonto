<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: polistiren.xml,v 1.12 2024/02/17 07:52:34 revo Exp $">
<kap>
  <rad>polistiren</rad>/o <fnt><bib>GDFE</bib></fnt>,
  <var><kap><rad var="ol">polistirol</rad>/o</kap></var>
</kap>
<drv mrk="polistiren.0o">
  <kap><tld/>o, <var><kap><tld var="ol"/>o</kap></var></kap>
  <snc mrk="polistiren.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="plast.0o.TEK">Plasto</ref> solida, 
      rompebla, kiun oni povas fari diafana a&ubreve; kolora, substanco de 
      multaj komunuzaj objektoj:
      <ekz>
        <tld/>o estas termoplasta rezino
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
          <lok>2006-09-12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hodia&ubreve;aj laboratoriaj iloj estas produktitaj plejofte el
         polipropileno kaj <tld var="ol"/>o
        <fnt><bib>Viki</bib>, <lok>In vitro</lok></fnt>;
      </ekz>
      <ekz>
         celofano, <tld var="ol"/>o kaj polikarbonato estas duoblorefraktaj
         materialoj
         <fnt>
           <bib>Viki</bib>,
           <lok>Duobla refrakto</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_l;&c_ib;&c_s;&c_t;&c_y;&c_r;&c_o;&c_l;</trd>
  <trd lng="bg">&c_p;&c_o;&c_l;&c_i;&c_s;&c_t;&c_i;&c_r;&c_o;&c_l;</trd>
  <trd lng="ca">poliestir&eacute;</trd>
  <trd lng="cs">polystyren</trd>
  <trd lng="de">Polystyrol</trd>
  <trd lng="en">polystyrene</trd>
  <trd lng="fr">polystyr&egrave;ne</trd>
  <trdgrp lng="it">
    <trd>polistirene</trd>,
    <trd>polistirolo</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>isomo</trd>,
    <trd>piepschuim</trd>,
    <trd>polystyreen</trd>
  </trdgrp>
  <trd lng="pl">polistyren</trd>
  <trd lng="ro">polistiren</trd>
  <trd lng="sk">polystyr&eacute;n</trd>
  <trdgrp lng="zh">
    <trd>&#x805A;&#x82EF;&#x4E59;&#x70EF; <pr>j&#xF9;b&#x11B;ny&#x1D0;x&#x12B;</pr></trd>,
    <trd>&#x4FDD;&#x529B;&#x9F8D; <pr>b&#x1CE;ol&#xEC;l&#xF3;ng</pr></trd>,
    <trd>&#x4FDD;&#x529B;&#x9F99; <pr>b&#x1CE;ol&#xEC;l&#xF3;ng</pr></trd>
  </trdgrp>
</drv>
</art>

