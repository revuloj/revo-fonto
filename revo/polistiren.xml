<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>polistiren</rad>/o <fnt><bib>GDFE</bib></fnt>
</kap>
<drv mrk="polistiren.0o">
  <kap><tld/>o</kap>
  <snc mrk="polistiren.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="plast.0o.TEK">Plasto</ref> solida, 
      rompebla, kiun oni povas fari diafana a&ubreve; kolora, substanco de 
      multaj komunuzaj objektoj:
      <ekz>
        <tld/>o estas termoplasta rezino
        <fnt>
          <aut>Vastalto</aut>,
          <vrk><url 
          ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
          <lok>2006-09-12</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <rim mrk="polistiren.0o.RIM_SIN">
    Alia vortoformo:
    <em>polistirolo</em>.
  </rim>
  <trd lng="fr">polystyr&egrave;ne</trd>
  <trdgrp lng="it">
    <trd>polistirene</trd>,
    <trd>polistirolo</trd>
  </trdgrp>
</drv>
</art>

