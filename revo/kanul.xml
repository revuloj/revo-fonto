<kap>
  <rad>kanul</rad>/o <fnt>Z</fnt>
</kap>

<drv mrk="kanul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="malprt" cel="tub.0o.TEK">Tubeto</ref>, 
      kiun oni aligas al <ref tip="malprt" cel="injekt.0ilo.MED">injektilo<sncref/></ref>
      a&ubreve; ofte uzas &ccirc;e kelkaj operacioj:
      <ekz>
        graso-desu&ccirc;ado estas beliga operacio, &ccirc;e kiu oni &ccirc;e celitaj loko
        desu&ccirc;as per
        <tld/>o (tubeto) gras&ccirc;elojn el sub la ha&ubreve;to
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=232088">Liposukcio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_ju;&c_l;&c_ja;</trd>
  <trd lng="bg">&c_k;&c_a;&c_n;&c_ju;&c_l;&c_a;</trd>
  <trd lng="ca">c&agrave;nula</trd>
  <trd lng="cs">kanyla</trd>
  <trd lng="de">Kan&uuml;le</trd>
  <trd lng="en">cannula</trd>
  <trd lng="es">c&aacute;nula</trd>
  <trd lng="fr">canule</trd>
  <trd lng="hu">kan&uuml;l</trd>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30CB;&#x30E5;&#x30FC;&#x30EC;</trd>,
    <trd>&#x633F;&#x7BA1;  <pr>&#x305D;&#x3046;&#x304B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">canule</trd>
  <trdgrp lng="pl">
    <trd>kaniula</trd>,
    <trd>wenflon</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>canul&abreve;</trd>,
    <trd>cateter</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_ju;&c_l;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>kanyla</trd>,
    <trd>r&uacute;rka</trd>,
    <trd>trubi&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_a;&#x43D;&#x44E;&#x43B;&#x44F;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_b;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5C0F;&#x7BA1; <pr>xi&#x1CE;ogu&#x1CE;n</pr></trd>,
    <trd>&#x7B69; <pr>t&#x1D2;ng</pr></trd>
  </trdgrp>
</drv>
</art>
