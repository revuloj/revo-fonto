<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kanul</rad>/o <fnt>Z</fnt>
</kap>

<drv mrk="kanul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="malprt" cel="tub.0o.TEK">Tubeto</ref>, 
      kiun oni aligas al <ref tip="malprt" cel="injekt.0ilo.MED">injektilo<sncref/></ref>
      a&ubreve; ofte uzas &ccirc;e kelkaj operacioj:
      <ekz>
        graso-desu&ccirc;ado estas beliga operacio, &ccirc;e kiu oni &ccirc;e celitaj loko desu&ccirc;as per
        <tld/>o (tubeto) gras&ccirc;elojn el sub la ha&ubreve;to
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=232088"
              >Liposukcio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_a;&c_n;&c_ju;&c_l;&c_a;</trd>
  <trd lng="ca">c&agrave;nula</trd>
  <trd lng="cs">kanyla</trd>
  <trd lng="de">Kan&uuml;le</trd>
  <trd lng="en">cannula</trd>
  <trd lng="es">c&aacute;nula</trd>
  <trd lng="fr">canule</trd>
  <trd lng="hu">kan&uuml;l</trd>
  <trd lng="nl">canule</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_ju;&c_l;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>kanyla</trd>,
    <trd>r&uacute;rka</trd>,
    <trd>trubi&ccaron;ka</trd>
  </trdgrp>
</drv>
</art>
