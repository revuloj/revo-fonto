<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lugr.xml,v 1.20 2023/09/02 05:33:11 revo Exp $">
<kap>
<rad>lugr</rad>/o
</kap>
<drv mrk="lugr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Unu-, du- a&ubreve; tri-masta eta
      <ref tip="super" cel="sxip.vel0o">vel&scirc;ipo</ref>,
      uzanta <tld/>ovelojn:
      <ekz>
        ni alligis la pruon de la bar&gcirc;o al la po&ubreve;po de la lugro anta&ubreve; ol ekvoja&gcirc;i
        <fnt>
          <aut>Kenneth G. Linton</aut>
          <vrk><url ref="https://books.google.de/books?id=zjaPYqGXm2UC&amp;lpg=PA178&amp;ots=7hyCzkMS6W&amp;pg=PA182#v=onepage&amp;q=lugro&amp;f=false">Kanako el Kananam</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/8/8f/Lugger_at_Yarmouth_Old_Gaffers_Festival_2009_2.jpg/640px-Lugger_at_Yarmouth_Old_Gaffers_Festival_2009_2.jpg" prm="CC BY-SA 2.0"><tld/>o &ccirc;e Yarmouth, Insulo Wight, 2009
        <fnt>
          <aut>Steve Parkes, Flickr: Lugger</aut>
          <lok><url ref="&WCW;/File:Lugger_at_Yarmouth_Old_Gaffers_Festival_2009_2.jpg">&WCW;/File:Lugger_at_Yarmouth_Old_Gaffers_Festival_2009_2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="cs">jednost&ecaron;&zcaron;n&iacute;k</trd>
  <trdgrp lng="de">
    <trd>Lugger</trd>,
    <trd>Logger</trd>
  </trdgrp>
  <trd lng="en">lugger</trd>
  <trdgrp lng="fr">
    <trd>lougre</trd>
  </trdgrp>
  <trd lng="he">&samekh;&pe;&yod;&nun;&he; <klr>(&bet;&ayin;&lamed;&tav; &mem;&pe;&resh;&shin;&yod;&fmem; &mem;&resh;&vav;&bet;&ayin;&yod;&fmem;)</klr></trd>
  <trd lng="hu">lugger</trd>
  <trd lng="ja">&#x30E9;&#x30AC;&#x30FC;</trd>
  <trd lng="nl">logger</trd>
  <trd lng="pl">lugier</trd>
  <trdgrp lng="pt">
    <trd>l&uacute;gar</trd>,
    <trd>lugre</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_ju;&c_g;&c_je;&c_r;</trd>,
    <trd>&c_l;&c_ju;&c_g;&c_g;&c_je;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">mal&yacute; jednos&tcaron;a&zcaron;n&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x43E;&#x491;&#x435;&#x440;</trd>,
    <trd>&#x43B;&#x44E;&#x491;&#x435;&#x440;</trd>
  </trdgrp>
</drv>

<drv mrk="lugr.0ovelo">
  <kap><tld/>ovelo</kap>
  <snc>
    <dif>
      Kvarangula <ref tip="super" cel="vel.0o.SHI">velo</ref>,
      fiksita sur oblikva <ref tip="vid" cel="jard.0o.MAR">jardo</ref>,
      kaj levata kaj mallevata per &gcirc;i.
    </dif>
  </snc>
  <trd lng="cs">plachta</trd>
  <trd lng="de">Luggersegel</trd>
  <trd lng="en">lugsail</trd>
  <trdgrp lng="fr">
    <trd>bourcet</trd>,
    <trd>voile au tiers</trd>
  </trdgrp>
  <trd lng="he">&mem;&pe;&resh;&shin; &mem;&resh;&vav;&bet;&ayin;</trd>
  <trd lng="hu">lugvitorla</trd>
  <trd lng="ja">&#x30E9;&#x30B0;&#x30B9;&#x30EB;</trd>
  <trd lng="nl">loggerzeil</trd>
  <trdgrp lng="pl">
    <trd>&zdot;agiel <ind>lugrowy</ind></trd>,
    <trd>&zdot;agiel <ind>rejkowy</ind></trd>
  </trdgrp>
  <trd lng="pt">vela de l&uacute;gar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_j;&c_k;&c_o;&c_v;&c_y;&c_j;
    &c_p;&c_a;&c_r;&c_u;&c_s;</trd>,
    <trd>&c_l;&c_ju;&c_g;&c_je;&c_r;&c_n;&c_y;&c_j;
    &c_p;&c_a;&c_r;&c_u;&c_s;</trd>
  </trdgrp>
  <trd lng="sk">plachta</trd>
</drv>
</art>
