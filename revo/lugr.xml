<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>lugr</rad>/o
</kap>
<drv mrk="lugr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Unu-, du- a&ubreve; tri-masta eta
      <ref tip="super" cel="sxip.vel0o">vel&scirc;ipo</ref>,
      uzanta <tld/>ovelojn.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Lugger</trd>,
     <trd>Logger</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lougre</trd>
  </trdgrp>
  <trd lng="hu">lugger</trd>
  <trd lng="nl">logger</trd>
  <trdgrp lng="pt">
    <trd>l&uacute;gar</trd>,
    <trd>lugre</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_ju;&c_g;&c_je;&c_r;</trd>,
    <trd>&c_l;&c_ju;&c_g;&c_g;&c_je;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="lugr.0ovelo">
  <kap><tld/>ovelo</kap>
  <snc>
    <dif>
      Kvarangula <ref tip="super" cel="vel.0o.MAR">velo</ref>,
      fiksita sur oblikva <ref tip="vid" cel="jard.0o.MAR">jardo</ref>,
      kaj levata kaj mallevata per &gcirc;i.
    </dif>
  </snc>
  <trd lng="de">Luggersegel</trd>
  <trd lng="hu">lugvitorla</trd>
  <trd lng="nl">loggerzeil</trd>
  <trd lng="pt">vela de l&uacute;gar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_j;&c_k;&c_o;&c_v;&c_y;&c_j;
    &c_p;&c_a;&c_r;&c_u;&c_s;</trd>,
    <trd>&c_l;&c_ju;&c_g;&c_je;&c_r;&c_n;&c_y;&c_j;
    &c_p;&c_a;&c_r;&c_u;&c_s;</trd>
  </trdgrp>
</drv>
</art>
