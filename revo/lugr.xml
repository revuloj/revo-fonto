<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lugr</rad>/o
</kap>
<drv mrk="lugr.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Malgranda &scirc;ipo, havanta unu, du a&ubreve; tri mastojn kaj
      uzanta kvarangulajn oblikvajn velojn.
    </dif>
  </snc>
  <trd lng="nl">logger</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_ju;&c_g;&c_je;&c_r;</trd>,
    <trd>&c_l;&c_ju;&c_g;&c_g;&c_je;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="lugr.0ovelo">
  <kap>
    <tld/>ovelo
  </kap>
  <snc>
    <dif>
      Velo uzata sur lugro.
    </dif>
  </snc>
  <trd lng="nl">loggerzeil</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_j;&c_k;&c_o;&c_v;&c_y;&c_j; &c_p;&c_a;&c_r;&c_u;&c_s;</trd>,
    <trd>&c_l;&c_ju;&c_g;&c_je;&c_r;&c_n;&c_y;&c_j; &c_p;&c_a;&c_r;&c_u;&c_s;</trd>
  </trdgrp>
</drv>
</art>
