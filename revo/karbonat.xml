<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: karbonat.xml,v 1.7 2023/12/03 08:00:23 afido Exp $">
    <kap>
      <rad>karbonat</rad>/o
    </kap>

<drv mrk="karbonat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="karbonat.0o.KEM">
    <dif>
      <ref tip="super" cel="sal.0o.KEM">Salo</ref> a&ubreve; <ref tip="super" cel="ester.0o">estero</ref>
      de karbonacido (<frm>H<sub>2</sub>CO<sub>3</sub></frm>):
      <ekz>
        kalcia karbonato estas konservata en la sedimentoj de la oceano
        <fnt>
          <bib>Monato</bib>
          <aut>Marco van Hulten</aut>
          <vrk><url ref="&Monato;/2015/011605.php">Interrilato de la oceano kaj
          klimat&scirc;an&gcirc;i&gcirc;o</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        natria <tld/>o
        <fnt>
          <vrk><url ref="&Viki;/Natrikarbonato">Natria karbonato</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Karbonat</trd>,
      <trd>Carbonat</trd>
    </trdgrp>
    <trd lng="en">Carbonat</trd>
    <trd lng="fr">Carbonat</trd>
    <trd lng="zh">&#x78B3;&#x9178;&#x76D0; <pr>t&#xE0;nsu&#x101;ny&#xE1;n</pr></trd>
</snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_b;&c_a;&c_n;&c_a;&c_t;</trd>
  <trd lng="ja">&#x70AD;&#x9178;&#x5869;  <pr>&#x305F;&#x3093;&#x3055;&#x3093;&#x3048;&#x3093;</pr></trd>
  <trd lng="uk">&#x43A;&c_a;&c_r;&c_b;&#x43E;&#x43D;&c_a;&c_t;</trd>
</drv>

<drv mrk="karbonat.0a_acido">
  <kap>
    <tld/>a acido
  </kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="karbonat.0a_acido.KEM">
    <dif>
      <ref tip="dif" cel="acid.karbon0o">Karbonacido</ref>
      (<frm>H<sub>2</sub>CO<sub>3</sub></frm>):
      <ekz>
        etilena <tld/>o estas organika kompona&jcirc;o klasita kiel <tld/>a estero de la
        etileno-glikolo kaj <tld/>a acido
        <fnt>
          <vrk><url ref="&Viki;/Etilena_karbonato">Etilena karbonato</url></vrk>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; <ind>&c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</ind></trd>
  <trd lng="de">Kohlens&auml;ure</trd>
  <trd lng="en">carbonic acid</trd>
  <trd lng="fr">acide carbonique</trd>
  <trd lng="ja">&#x70AD;&#x9178;  <pr>&#x305F;&#x3093;&#x3055;&#x3093;</pr></trd>
  <trd lng="zh">&#x78B3;&#x9178; <pr>t&#xE0;nsu&#x101;n</pr></trd>
</drv>




  </art>

