<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: karbonat.xml,v 1.3 2022/05/01 08:09:23 afido Exp $">
    <kap>
      <rad>karbonat</rad>/o
    </kap>

<drv mrk="karbonat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="karbonat.0o.KEM">
    <dif>
      <ref tip="super" cel="sal.0o.KEM">Salo</ref> a&ubreve; <ref tip="super" cel="ester.0o">estero</ref>
      de karbonacido (<frm>H<sub>2</sub>CO<sub>3</sub></frm>):
      <ekz>
        kalcia karbonato estas konservata en la sedimentoj de la oceano
        <fnt>
          <bib>Monato</bib>
          <aut>Marco van Hulten</aut>
          <vrk><url ref="&Monato;/2015/011605.php">Interrilato de la oceano kaj klimat&scirc;an&gcirc;i&gcirc;o</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        natria <tld/>o
        <fnt>
          <vrk><url ref="&Viki;/Natrikarbonato">Natria karbonato</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Karbonat</trd>,
      <trd>Carbonat</trd>
    </trdgrp>
    <trd lng="en">Carbonat</trd>
    <trd lng="fr">Carbonat</trd>
  </snc>
  <trd lng="uk">&#x43A;&#x430;&#x440;&#x431;&#x43E;&#x43D;&#x430;&#x442;</trd>
</drv>

<drv mrk="karbonat.0a_acido">
  <kap>
    <tld/>a acido
  </kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="karbonat.0a_acido.KEM">
    <dif>
      <ref tip="dif" cel="acid.karbon0o">Karbonacido</ref>
      (<frm>H<sub>2</sub>CO<sub>3</sub></frm>):
      <ekz>
        etilena <tld/>o estas organika kompona&jcirc;o klasita kiel <tld/>a estero de la
        etileno-glikolo kaj <tld/>a acido
        <fnt>
          <vrk><url ref="&Viki;/Etilena_karbonato">Etilena karbonato</url></vrk>
        </fnt>.
      </ekz>      
    </dif>
  </snc>
  <trd lng="de">Kohlens&auml;ure</trd>
  <trd lng="en">carbonic acid</trd>
  <trd lng="fr">acide carbonique</trd>
</drv>




  </art>

