<drv mrk="obstin.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="obstin.0a.KOMUNE">
    <dif>
      Firme restanta &ccirc;e sia propra opinio a&ubreve; volo,
      malgra&ubreve; &ccirc;iaj kontra&ubreve;aj admonoj:
      <ekz>
        mi ne amas <tld/>ajn homojn
        <fnt><bib>F</bib>, <lok>&FE; 10</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a infano;
      </ekz>
      <ekz>
        <ind><tld/>a kiel kapro</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">makacs, mint az &ouml;szv&eacute;r</trd>
      </ekz>
      <ekz>
        se homo ofte admonita restos obstina, li subite pereos sen ia helpo  
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 29:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> havas malmolan viza&gcirc;on kaj <tld/>an koron
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 2:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        arogantaj, <tld/>aj, ili ne timas insulti a&ubreve;toritatojn
        <fnt>
          <bib>NT</bib>
          <lok>II. Petro 2:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni devis plenumi &scirc;ian volon, &ccirc;ar &scirc;i estis tre eldorlotita kaj tre <tld/>a
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivega kiel leono kaj <tld/>a kiel bovo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        malfacila&jcirc;oj <klr>[&#x2026;]</klr> prezentante <tld/>an kaj nevenkeblan taskon
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia patro, la maljuna tedulo, estas ja <tld/>a kaj malsa&gcirc;a, kiel trabo
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        sa&gcirc;a kaj severa estis la patro, viro skeptika je revoj, sed <tld/>a je laboro
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a batalo, malamo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        dissaltos la <tld/>aj baroj inter la popoloj
        dividitaj
        <fnt>
          <aut>L. L. Zamenhof</aut>
          <vrk>La Espero</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e kaj sen ia fino plendi tute ne konvenas al la
        viro
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ced.mal0ema">malcedema</ref>,
      <ref cel="konvin">nekonvinkebla</ref>,
      <ref cel="spit.0ema">spitema</ref>
    </refgrp>
    <ref tip="vid" cel="mol.mal0a.morale">malmola<sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_a;&c_r;&c_t;&c_y;</trd>,
    <trd>&c_u;&c_p;&c_o;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_c;&c_ja;&c_t;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_w;&c_z;&c_ja;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&#xF58;&#xF42;&#xF7C;&#xF0B;&#xF58;&#xF41;&#xFB2;&#xF7A;&#xF42;&#xF66;&#xF0B;&#xF54;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>neoblomn&yacute;</trd>,
    <trd>obstin&aacute;tn&iacute;</trd>,
    <trd>pali&ccaron;at&yacute;</trd>,
    <trd>sv&eacute;hlav&yacute;</trd>,
    <trd>sv&eacute;voln&yacute;</trd>,
    <trd>tuh&yacute;</trd>,
    <trd>tvrdohlav&yacute;</trd>,
    <trd>tvrdo&scaron;&iacute;jn&yacute;</trd>,
    <trd>um&iacute;n&ecaron;n&yacute;</trd>,
    <trd>vzdorn&yacute;</trd>,
    <trd>vzdorovit&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>halsstarrig</trd>,
    <trd>starrsinnig</trd>,
    <trd>st&ouml;rrig</trd>,
    <trd>stur</trd>,
    <trd>bockbeinig</trd>,
    <trd>hartn&auml;ckig</trd>,
    <trd>dickk&ouml;pfig</trd>,
    <trd>eigensinnig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>obstinate</trd>,
    <trd>stubborn</trd>
  </trdgrp>
  <trd lng="es">obstinado</trd>
  <trdgrp lng="fr">
    <trd>obstin&eacute;</trd>,
    <trd>ent&ecirc;t&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>makacs</trd>,
    <trd>meg&aacute;talkodott</trd>,
    <trd>cs&ouml;k&ouml;ny&ouml;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>koppig</trd>,
    <trd>obstinaat</trd>,
    <trd>onbuigbaar</trd>,
    <trd>onbuigzaam</trd>,
    <trd>onverzettelijk</trd>,
    <trd>stijfhoofdig</trd>,
    <trd>stijfkoppig</trd>  
  </trdgrp>
  <trd lng="pt">obstinado</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_r;&c_ja;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_u;&c_p;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>neoblomn&yacute;</trd>,
    <trd>spurn&yacute;</trd>,
    <trd>svojhlav&yacute;</trd>,
    <trd>tvrdohlav&yacute;</trd>,
    <trd>tvrdo&scaron;ijn&yacute;</trd>
  </trdgrp>
  <trd lng="sv">envis</trd>
</drv>

<drv mrk="obstin.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="obstin.0i.KOMUNE">
    <dif>
      Sekvi nur iun opinion a&ubreve; decidon, ne lasante sin
      konvinki de aliaj:
      <ekz>
        <tld/>i en sia eraro;
      </ekz>
      <ekz>
        li <tld/>as en la silento;
      </ekz>
      <ekz>
        ne iru, &ccirc;ar <klr>[&#x2026;]</klr> vi falos de glavo <klr>[&#x2026;]</klr>,
        sed ili <tld/>is, kaj iris sur la supron de la monto
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 14:42</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> <tld/>as en siaj pekoj  
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 68:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la konsidero de la e&ubreve;ska konflikto kiel politika anstata&ubreve; simpla banditismo, kiel <tld/>as la hispana registaro
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos I Font</aut>
          <vrk><url ref="https://www.monato.be/2003/008082.html">Kiam arestoj pruvas terorismajn ligojn ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        se dekstrulaj regantoj slavmakedonaj ne tiom longe <tld/>us, Makedonio estus jam la 29a NATO-membro
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2009/009804.php">Naski&gcirc;as du pliaj NATO-membroj</url></vrk>
        </fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="insist.0i">insisti</ref>,
      <ref cel="persis.0i">persisti</ref>,
      <ref cel="ripet.0i">ripeti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_a;&c_r;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_u;&c_p;&c_ib;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_c;&c_ib;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">stav&ecaron;t se na zadn&iacute; nohy</trd>
  <trdgrp lng="de">
    <trd>halsstarrig sein</trd>,
    <trd>sich <ind>versteifen</ind></trd>,
    <trd>bei seiner <ind>Meinung</ind> bleiben</trd>,
    <trd>dickk&ouml;pfig sein</trd>
  </trdgrp>
  <trd lng="en">persist</trd>
  <trd lng="es">obstinar</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>obstiner</ind></trd>,
    <trd>s'<ind>ent&ecirc;ter</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>makacskodik</trd>,
    <trd>nem <ind>hajland&oacute;</ind></trd>
  </trdgrp>
  <trd lng="nl"><ind>koppig</ind> volhouden</trd>
  <trd lng="pt">obstinar</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_r;&c_ja;&c_m;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_p;&c_o;&c_r;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_o;&c_ja;&c_t;&c_mol; &c_n;&c_a;
    &c_s;&c_v;&c_o;&c_jo;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>by&tcaron; tvrdo&scaron;ijn&yacute;</trd>,
    <trd>tvrdohlav&yacute;</trd>,
    <trd>za&tcaron;at&yacute;; potrucova&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">envisas</trd>
</drv>

<drv mrk="obstin.0o">
  <kap><tld/>o</kap>
  <snc mrk="obstin.0o.KOMUNE">
    <dif>
      Karaktero, maniero, agado de homo tro firma, ne&scirc;an&gcirc;ema,
      necedema:
      <ekz>
        l' espero, l' <tld/>o kaj la pacienco, jen estas la signoj
        per kies potenco ni <klr>[&#x2026;]</klr> atingos la celon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#x2026;]</klr> ripro&ccirc;is ilian nekredemon kaj kor<tld/>on
        <fnt>
          <bib>NT</bib>
          <lok>Marko 16:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> havas <klr>[&#x2026;]</klr> celon, kiun li penos atingi kun nevenkebla <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patro <klr>[&#x2026;]</klr> trankvili&gcirc;is, kvankam li ne pardonis al
        la filino &scirc;ian <tld/>on
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, &ccirc;apitro
        3a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_a;&c_r;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_c;&c_ja;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_w;&c_z;&c_ja;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obstinace</trd>,
    <trd>tvrdohlavost</trd>,
    <trd>tvrdo&scaron;&iacute;jnost</trd>,
    <trd>um&iacute;n&ecaron;nost</trd>,
    <trd>zarputilost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Halsstarrigkeit</trd>,
    <trd>Starrsinn</trd>,,
    <trd>Sturheit</trd>,
    <trd>Eigensinn</trd>,
    <trd>Hartn&auml;ckigkeit</trd>,
    <trd>Dickk&ouml;pfigkeit</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>obstinacy</trd>,
    <trd>obstinance</trd>,
    <trd>stubbornness</trd>
  </trdgrp>
  <trd lng="es">obstinaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>obstination</trd>,
    <trd>ent&ecirc;tement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>makacss&aacute;g</trd>,
    <trd>makacskod&aacute;s</trd>,
    <trd>cs&ouml;k&ouml;ny&ouml;ss&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vastberadenheid</trd>,
    <trd>koppigheid</trd>
  </trdgrp>
  <trd lng="pt">obstina&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_r;&c_ja;&c_m;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_u;&c_p;&c_o;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;&c_ch;&c_i;&c_v;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>tvrdohlavos&tcaron;</trd>,
    <trd>tvrdo&scaron;ijnos&tcaron;</trd>,
    <trd>za&tcaron;atos&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">envishet</trd>
</drv>

