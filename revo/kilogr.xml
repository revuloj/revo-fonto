<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kilogr.xml,v 1.37 2024/08/23 06:35:18 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>1</ofc><ofc>8</ofc>
  <rad>kilogram</rad>/o
</kap>

<drv mrk="kilogr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <mlg kod="SI">kg</mlg>
  <snc>
    <dif>
      Baza <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">mezurunuo</ref> de
      <ref cel="mas.0o.FIZ">maso</ref> en la Internacia Sistemo de
      unuoj:
      <ekz>
        kilogram'o da viand'o
        <fnt><bib>F</bib>, <lok>Universala Vortaro, da</lok></fnt>;
      </ekz>
      <ekz>
        unu <tld/>o egalas al la maso de prototipa pesilo el
        iridia-platena
        alojo, konservata en <nom>S&egrave;vres</nom> apud Parizo (Francujo);
      </ekz>
      <ekz>
        unu arbeta&jcirc;o de tiu &ccirc;i kreska&jcirc;o ombras la spacon de tri
        kvadrataj metroj,
        kaj la tuta maso da folioj de unu arbeta&jcirc;o pezas 30 <tld/>ojn
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        1 mino = 1,5 <tld/>o <klr>[&#8230;]</klr> unu soldato povas porti sur
        la dorso 26 minojn da &scirc;ar&gcirc;o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        dol&ccirc;akvaj krokodiloj mezuras malpli ol tri metrojn kaj normale
        pezas nur 70 <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>&Scirc;andor Monostori</aut>
          <vrk><url ref="&Monato;/2015/011466.php">Bani kun
          krokodiloj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun kvar <tld/>oj da hidrogeno oni povas veturi &gcirc;is 400&#x202F;km
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2013/010915.php">Brula&jcirc;o
          en kilogramoj</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu kuba metro da ne&gcirc;o pezas 300&#x202F;-&#x202F;500 <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo/pg</aut>
          <vrk><url ref="&Monato;/2006/008872.php">Revenas
          la ne&gcirc;o</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_ib;&c_l;&c_a;&c_g;&c_r;&c_a;&c_m;</trd>
    <trd lng="ca">quilogram</trd>
    <trdgrp lng="de">
      <trd>Kilogramm</trd>,
      <trd>Kilo</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>kilogram</trd>,
      <trd>kilo</trd>
    </trdgrp>
    <trd lng="es">kilogramo</trd>
    <trdgrp lng="fr">
      <trd>kilogramme</trd>,
      <trd>kilo</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kilogramm</trd>,
      <trd>kil&oacute;</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>kilo</trd>,
      <trd>kilogram</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>kilogram</trd>,
      <trd>kilo</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_i;&c_l;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;</trd>
  </snc>
  <trd lng="cs">kilogram</trd>
  <trd lng="ja">&#x30AD;&#x30ED;&#x30B0;&#x30E9;&#x30E0;  <pr>&#x304D;&#x308D;&#12368;&#12425;&#12416;</pr></trd>
  <trd lng="pl">kilogram</trd>
  <trd lng="ro">kilogram</trd>
  <trd lng="sk">kilogram</trd>
  <trdgrp lng="zh">
    <trd>&#x516C;&#x65A4; <pr>g&#x14D;ngj&#x12B;n</pr></trd>,
    <trd>&#21315;&#x514B; <pr>qi&amacron;nk&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kilogr.0forto">
  <kap><tld/>forto</kap>
  <uzo tip="fak">FIZ</uzo>
  <mlg>kgf</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">Mezurunuo</ref> de
      <ref cel="fort.0o.FIZ">forto</ref>, egala al la gravitoforto
      estigata de
      Tero &ccirc;e korpo kun maso je unu <tld/>o, egala al 9,81
      <ref cel="neuxto.n0o">ne&ubreve;tonoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_a;&c_g;&c_r;&c_a;&c_m;-&c_s;&c_ib;&c_l;&c_a;</trd>
  <trd lng="cs">kilogram</trd>
  <trd lng="de">Kilopond</trd>
  <trdgrp lng="fr">
    <trd><ind>kilogramme</ind>-force</trd>,
    <trd><ind>kilogramme</ind>-poids</trd>
  </trdgrp>
  <trd lng="hu">kilogramm-er&odblac;</trd>
  <trd lng="id"><ind>kilogram</ind> gaya</trd>
  <trd lng="nl">kilogramkracht</trd>
  <trd lng="pl">kilogram-si&lstroke;a</trd>
  <trd lng="ro">kilogram-for&#x21B;&abreve;</trd>
  <trd lng="ru">&c_k;&c_i;&c_l;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;-&c_s;&c_i;&c_l;&c_a;</trd>
  <trd lng="sk">kilogram</trd>
</drv>

<drv mrk="kilogr.0metro">
  <kap><tld/>metro</kap>
  <uzo tip="fak">FIZ</uzo>
  <mlg>kgm</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">Mezurunuo</ref>, esprimanta la
      laboron necesan por levi unu <tld/>on je unu <ref cel="metr.0o.unuo">metro</ref>,
      egala al 9,81 <ref cel="jxul.jx0o">&jcirc;uloj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_a;&c_g;&c_r;&c_a;&c_m;&c_a;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Kilopondmeter</trd>
  <trd lng="fr">kilogramm&egrave;tre</trd>
  <trd lng="hu">kilogramm-m&eacute;ter</trd>
  <trd lng="id"><ind>kilogram</ind> meter</trd>
  <trd lng="ja">&#x30AD;&#x30ED;&#x30B0;&#x30E9;&#x30E0;&#x30E1;&#x30FC;&#x30C8;&#x30EB; 
  <pr>&#x304D;&#x308D;&#12368;&#12425;&#12416;&#12417;&#x30FC;&#12392;&#x308B;</pr></trd>
  <trd lng="nl">kilogrammeter</trd>
  <trd lng="pl">kilogramometr</trd>
  <trd lng="ro">kilogram-metru</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_l;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;-&c_s;&c_i;&c_l;&c_a;-&c_m;&c_je;&c_t;&c_r;</trd>,
    <trd>&c_k;&c_i;&c_l;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  </trdgrp>
</drv>
<drv mrk="kilogr.0o_en_kuba_metro">
  <kap><tld/>o en kuba metro</kap>
  <uzo tip="fak">FIZ</uzo>
  <mlg>kg/m&cubic;</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:mezurunuoj">Mezurunuo</ref> de
      <ref cel="mas.volumena0o">volumena maso</ref> en la Internacia
      Sistemo de
      unuoj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_l;&c_a;&c_g;&c_r;&c_a;&c_m; &c_n;&c_a; &c_k;&c_u;&c_b;&c_ib;&c_ch;&c_n;&c_y; &c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="fr">kilogramme par m&egrave;tre cube</trd>
  <trd lng="hu">kilogramm per k&ouml;bm&eacute;ter</trd>
  <trd lng="id"><ind>kilogram</ind> per meter kubik</trd>
  <trd lng="nl"><ind>kilogram</ind> per kubieke meter</trd>
  <trd lng="pl">kilogram na metr sze&sacute;cienny</trd>
  <trd lng="ro">kilogram pe metru cub</trd>
  <trd lng="ru">&c_k;&c_i;&c_l;&c_o;&c_g;&c_r;&c_a;&c_m;&c_m; &c_n;&c_a; &c_k;&c_u;&c_b;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
  &c_m;&c_je;&c_t;&c_r;</trd>
</drv>
</art>
