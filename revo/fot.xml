<drv mrk="fot.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="fot.0isto.FOT">
    <dif>
      <ref tip="dif" cel="fotogr.0isto">Fotografisto</ref>:
      <ekz>
        Johano estas meza&gcirc;a profesia <tld/>isto
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2009/009680.php">Proksimaj kaj malproksimaj
          najbaroj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la <tld/>o mi glui&gcirc;is al la panjo kaj estis iom timema pro la
        <tld/>isto ka&scirc;ita malanta&ubreve; la nigra tolo de la
        <tld/>ilo
        <fnt><aut>S. &Scaron;timec</aut>, <vrk>Morto en Melburna
        tramo</vrk>, <bib>Monato</bib>, <lok>1997:8, p.
        24a</lok></fnt>;
      </ekz>
      <ekz>
        anta&ubreve; duonjarcento <tld/>istoj komencis uzi fulmolumilojn
        <fnt>
          <bib>Monato</bib>
          <aut>J&aacute;nos S&aacute;rk&ouml;zi</aut>
          <vrk><url ref="&Monato;/2005/008401.php">La fenomeno de ru&gcirc;aj
          okuloj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
