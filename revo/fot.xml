<drv mrk="fot.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="fot.0ado.FOT">
    <dif>
      <ref tip="dif" cel="fotogr.0ado">Fotografado</ref>:
      <ekz>
        societo, okupi&gcirc;anta pri <tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="&Monato;/2003/007718.html">&Ccirc;u komerco helpas?</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        kreskanta intereso pri la naturo kaj ofte pri <tld/>ado
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2010/009971.php">Nestokestoj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        aera <tld/>ado kaj filmado
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2015/011535.php">Malsukcesa aviado</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
