  </trdgrp>
  <trd lng="ja">&#x5199;&#x771F;  <pr>&#x3057;&#x3083;&#x3057;&#x3093;</pr></trd>
  <trd lng="nl">foto</trd>
  <trdgrp lng="pl">
    <trd>fotografia</trd>,
    <trd>zdj&eogonek;cie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_t;&c_o;</trd>,
    <trd>&c_f;&c_o;&c_t;&c_o;&c_s;&c_n;&c_i;&c_m;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_v;&c_ib;&c_t;&#x43B;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_f;&#x43E;&c_t;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x76F8;&#x6846; <pr>xi&#xE0;ngku&#x101;ng</pr></trd>,
    <trd>&#x50CF;&#x6846; <pr>xi&#xE0;ngku&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="fot.0i">
  <kap><ofc>9</ofc><tld/>i <fnt><bib>PV</bib></fnt></kap>
  <snc mrk="fot.0i.FOT">
    <gra><vspec>tr</vspec></gra>
    <uzo tip="fak">FOT</uzo>
    <dif>
      <ref tip="dif" cel="fotogr.0i">Fotografi</ref>:
      <ekz>
        mi volis <tld/>i la tiean barilon, sed policisto malpermesis
        tion
        <fnt><aut>W. Klag</aut>, <vrk>Bariloj kontra&ubreve; bomboj</vrk>,
        <bib>Monato</bib>, <lok>1999:3, p. 7a</lok></fnt>;
      </ekz>
      <ekz>
        jam venis &jcirc;urnalisto, kiu ek<tld/>is
        <fnt>
          <bib>Monato</bib>
          <aut>Helga Rapley</aut>
          <vrk><url ref="&Monato;/2004/008356.html">Blanka dramo en Bureso</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la opoziciuloj, anta&ubreve; ol enurnigi siajn balotkartojn, ilin <tld/>is per
        po&scirc;telefono por pruvi siajn vo&ccirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo Juh&aacute;sz</aut>
          <vrk><url ref="&Monato;/2005/008731.php">Pri falko kaj talpoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la usona satelito F15 dum kvin minutoj <tld/>is Litovion el la kosmo
        <fnt>
          <bib>Monato</bib>
          <aut>Last</aut>
          <vrk><url ref="&Monato;/2004/008337.html">Luma lando</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
