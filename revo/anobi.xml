<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>anobi</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="anobi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="anobi.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Anobium</trd>), kies larvo boras truojn en ligno.  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pochk&auml;fer</trd>, 
    <trd>Klopfk&auml;fer</trd>,
    <trd lng="de">Holzwurm</trd>
  </trdgrp> 
</drv>
<drv mrk="anobi.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Anobiidae</trd>), kies larvoj boras en ligno kaj sekaj
      materialoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pochk&auml;fer</trd>, 
    <trd>Klopfk&auml;fer</trd>
  </trdgrp> 
</drv>
</art>
