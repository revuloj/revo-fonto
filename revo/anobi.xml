<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>anobi</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="anobi.0o">
  <kap><tld/>o</kap>
  <snc mrk="anobi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Skarabo, kies larvo boras truojn en ligno;
      genro el familio <ref tip="super"
      cel="anobie.0oj"><tld/>edoj</ref>
      (<trd lng="la">Anobium</trd>):
      <ekz>
        modesta ligna kruceto sur &ccirc;iu tombo.
        la plimulto staras oblikve kaj estas
        ron&gcirc;ata de <tld/>oj
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pochk&auml;fer</trd>,
    <trd>Klopfk&auml;fer</trd>,
    <trd>Holzwurm</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>anobie</trd>,
    <trd>vers de bois</trd>,
    <trd>vrillette</trd>
  </trdgrp>
  <trd lng="nl">houtworm</trd>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_i;&c_l;&c_mol;&c_shch;&c_i;&c_k; <klr tip="amb">(&c_zh;&c_u;&c_k;)</klr></trd>
</drv>
</art>
