<vortaro>
<art mrk="$Id$">
<kap>
  <rad>anobi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="anobi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="anobie.0oj"><tld/>edoj</ref>
      (<trd lng="la">Anobium</trd>), kies larvo boras truojn en ligno.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pochk&auml;fer</trd>,
    <trd>Klopfk&auml;fer</trd>,
    <trd lng="de">Holzwurm</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>anobie</trd>,
    <trd>vrillette</trd>
  </trdgrp>
  <trd lng="nl">houtworm</trd>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_i;&c_l;&c_mol;&c_shch;&c_i;&c_k; <klr tip="amb">(&c_zh;&c_u;&c_k;)</klr></trd>
</drv>
</art>
