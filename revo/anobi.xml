<vortaro>
<art mrk="$Id$">
<kap>
<rad>anobi</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="anobi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="anobie.0oj"><tld/>edoj</ref>
      (<trd lng="la">Anobium</trd>), kies larvo boras truojn en ligno.  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Pochk&auml;fer</trd>, 
    <trd>Klopfk&auml;fer</trd>,
    <trd lng="de">Holzwurm</trd>
  </trdgrp> 
</drv>

</art>
