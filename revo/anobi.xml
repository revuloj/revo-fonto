<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anobi.xml,v 1.22 2025/02/10 16:32:05 revo Exp $">
<kap>
  <rad>anobi</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="anobi.0o">
  <kap><tld/>o</kap>
  <snc mrk="anobi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="skarab.0o" lst="voko:skaraboj">Skarabo</ref>,
      kies larvo boras truojn en ligno;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>
      el familio <ref tip="malprt" cel="anobie.0oj"><tld/>edoj</ref>
      (<trd lng="la">Anobium</trd>):
      <ekz>
        modesta ligna kruceto sur &ccirc;iu tombo;
        la plimulto staras oblikve kaj estas
        ron&gcirc;ata de <tld/>oj
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/e/e0/Anobium_punctatum_above.jpg/320px-Anobium_punctatum_above.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Anobium punctatum)
        <fnt>
          <aut>Siga, propra verko</aut>
          <lok><url ref="&WCW;/File:Anobium_punctatum_above.jpg"
              >https://commons.wikimedia.org/wiki/File:Anobium_punctatum_above.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_ch;&c_y;&c_l;&c_mol;&c_sh;&c_ch;&c_y;&c_k; <klr>(&c_zh;&c_u;&c_k;)</klr></trd>
  <trd lng="ca">corc de la fusta</trd>
  <trd lng="cs">&ccaron;ervoto&ccaron;</trd>
  <trdgrp lng="de">
    <trd>Pochk&auml;fer</trd>,
    <trd>Klopfk&auml;fer</trd>,
    <trd>Holzwurm</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>anobie</trd>,
    <trd>vers de bois</trd>,
    <trd>vrillette</trd>
  </trdgrp>
  <trd lng="ja">&#12450;&#12494;&#12499;&#12454;&#12512;</trd>
  <trd lng="nl">houtworm</trd>
  <trd lng="pl">ko&lstroke;atek</trd>
  <trd lng="ru">&c_t;&c_o;&c_ch;&c_i;&c_l;&c_mol;&c_shch;&c_i;&c_k; <klr tip="amb">(&c_zh;&c_u;&c_k;)</klr></trd>
  <trd lng="sk">&ccaron;ervoto&ccaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_o;&c_ch;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_sh;&c_a;&c_sh;&c_ib;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
