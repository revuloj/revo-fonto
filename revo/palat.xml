<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: palat.xml,v 1.31 2024/03/07 06:26:57 revo Exp $">
<kap>
  <ofc>*</ofc><rad>palat</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="palat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="palat.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Supera volboforma vando de <ref tip="malprt" cel="busx.0o.ANA">bu&scirc;interno</ref>,
      tegita de mukomembrano:
      <ekz>
        estis tiel varmege, ke la lango <klr>[&#8230;]</klr> alglui&gcirc;is pro soifo al la <tld/>o
        <fnt>
          <bib>ElektFab</bib>
          <lok>Feli&ccirc;a Jo&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi balda&ubreve; kontentigos vian tikladon de la <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        frotaj konsonantoj a&ubreve; frikativoj, <klr>[&#8230;]</klr> formi&gcirc;as diversmaniere
        <klr>[&#8230;]</klr>: h en la laringo, &hcirc; en la gor&gcirc;o per alproksimigo de la
        langradiko al la mola <tld/>o
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;e stomako malsata ne kapricas <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="faring.0o">faringo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_n;&c_ja;&c_b;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="br">staon<klr> (korf.)</klr></trd>
  <trd lng="ca">paladar</trd>
  <trdgrp lng="cs">
    <trd>palatum</trd>,
    <trd>patro v &uacute;stech</trd>,
    <trd>poneb&iacute;</trd>
  </trdgrp>
  <trd lng="de">Gaumen</trd>
      <trd lng="el">&omikron;&ypsilon;&rho;&alfa;&ny;&jota_ton;&sigma;&kappa;&omikron;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>palate</trd>,
    <trd>hard palate</trd>
  </trdgrp>
  <trd lng="fr">palais<klr> (anatomie)</klr></trd>
  <trdgrp lng="hu">
    <trd>&iacute;ny</trd>,
    <trd>sz&aacute;jpadl&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>palato</trd>,
    <trd><ind>palato</ind> duro</trd>,
    <trd><ind>palato</ind> anteriore</trd>,
    <trd>volta palatina</trd>
  </trdgrp>
  <trd lng="ja">&#21475;&#33995;  <pr>&#12371;&#12358;&#12364;&#12356;</pr></trd>
  <trd lng="nl">verhemelte</trd>
  <trd lng="no">gane</trd>
  <trd lng="pl">podniebienie</trd>
  <trd lng="pt">palato</trd>
  <trd lng="ru">&c_n;&c_jo;&c_b;&c_o;</trd>
  <trd lng="sk">podnebie v &uacute;stach</trd>
  <trd lng="uk">&c_m;&#8217;&c_ja;&c_k;&c_je; &c_p;&c_ib;&c_d;&c_n;&c_je;&c_b;&c_ib;&c_n;&c_n;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#38990; <pr>&egrave;</pr></trd>,
    <trd>&#39066; <pr>&egrave;</pr></trd>,
    <trd>&#33133; <pr>&egrave;</pr></trd>,
    <trd>&#40822; <pr>&egrave;</pr></trd>,
    <trd>&#33220; <pr>ju&eacute;</pr></trd>,
    <trd>&#19978;&#29273;&#33179; <pr>sh&agrave;ngy&aacute;t&aacute;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="palat.0a">
  <kap><tld/>a</kap>
  <snc mrk="palat.0a.ANA">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <uzo tip="fak">FON</uzo>
        <ind><tld/>aj</ind> konsonantoj, vokaloj
        <klr>(kp <ref cel="mol.0a.FON">mola<sncref/></ref>, <ref cel="palata.0o">palatalo</ref>)</klr>.
        <trd lng="en">palatal</trd>
        <trdgrp lng="hu">
          <trd>palat&aacute;lis</trd>,
          <trd>&iacute;ny-</trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_n;&c_jo;&c_b;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_p;&c_a;&c_l;&c_a;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_n;&c_ja;&c_b;&c_je;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_ja;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>-<ind>staon</ind></trd>,
    <trd>staoniekaet</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>palatal</trd>,
    <trd>palat&iacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>palatal</trd>,
    <trd>palatane</trd>
  </trdgrp>
  <trd lng="fr">palatal</trd>
  <trdgrp lng="hu">
    <trd>&iacute;ny-</trd>,
    <trd>sz&aacute;jpadl&aacute;s-</trd>
    <trd>palat&aacute;lis</trd>
  </trdgrp>
  <trd lng="it">palatale</trd>
  <trd lng="ja">&#21475;&#33995;&#12398;  <pr>&#12371;&#12358;&#12405;&#12383;&#12398;</pr></trd>
  <trd lng="nl">palataal</trd>
  <trd lng="no">palatal</trd>
  <trd lng="pl">podniebienny</trd>
  <trd lng="ru">&c_n;&c_jo;&c_b;&c_n;&c_y;&c_j;</trd>
  <trd lng="uk">&c_p;&c_ib;&c_d;&c_n;&c_je;&c_b;&c_ib;&c_n;&c_n;&c_i;&c_j;</trd>
</drv>

<drv mrk="palat.post0o">
  <kap>post<tld/>o</kap>
  <snc mrk="palat.post0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Muskolmembrana vandeto, apartiganta la faringon de la bu&scirc;o, kaj kiu
      levi&gcirc;ante forfermas la nazan kava&jcirc;on a&ubreve; mallevi&gcirc;ante &gcirc;in
      malfermas;
      mola palato
    </dif>
    <refgrp tip="sin">
      <ref cel="vel.0o.ANA">velo</ref>
    </refgrp>
    <refgrp tip="prt">
      <ref cel="uvul.0o">uvulo</ref>
    </refgrp>
    <trd lng="be">&c_m;&c_ja;&c_k;&c_k;&c_a;&c_je; <ind>&c_p;&c_a;&c_d;&c_n;&c_ja;&c_b;&c_je;&c_n;&c_mol;&c_n;&c_je;</ind></trd>
    <trd lng="ca"><ind>paladar</ind> tou</trd>
    <trdgrp lng="de">
      <trd>Gaumensegel</trd>,
      <trd>Velum</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>soft palate</trd>,
      <trd>muscular palate</trd>,
      <trd>velum</trd>
    </trdgrp>
    <trd lng="es">velo</trd>
    <trdgrp lng="fr">
      <trd>palais mou</trd>,
      <trd>voile du palais</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>&iacute;nyvitorla</trd>,
      <trd>l&aacute;gy &iacute;ny</trd>
    </trdgrp>
    <trdgrp lng="it">
    <trd>velo</trd>,
    <trd><ind>palato</ind> posteriore</trd>,
    <trd><ind>palato</ind> molle</trd>,
    <trd><ind>velo</ind> palatino</trd>
  </trdgrp>
    <trdgrp lng="nl">
      <trd>velum</trd>,
      <trd>zacht <ind>verhemelte</ind></trd>
    </trdgrp>
    <trd lng="pl">j&eogonek;zyczek podniebienny</trd>
    <trd lng="ru">&c_m;&c_ja;&c_g;&c_k;&c_o;&c_je; <ind>&c_n;&c_jo;&c_b;&c_o;</ind></trd>
  </snc>
  <trdgrp lng="zh">
    <trd>&#36575;&#40822; <pr>ru&#462;n&egrave;</pr></trd>,
    <trd>&#36719;&#40822; <pr>ru&#462;n&egrave;</pr></trd>
  </trdgrp>
</drv>
</art>
