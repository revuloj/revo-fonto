<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: janikul.xml,v 1.3 2025/11/05 08:41:50 afido Exp $">
  <kap>
    <rad>Janikul</rad>/o <fnt><bib>PIV1</bib></fnt>
  </kap>
  <drv mrk="janikul.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">GEOG</uzo>
    <snc>
      <dif>
        Monteto en <ref tip="malprt" cel="rom.0o">Romo</ref>
        sur la dekstra bordo de <ref cel="tiber.0o">Tibero</ref>:
        <ekz>
          <klr>[la sundisko]</klr> jam ka&scirc;is sin duone post <tld/>o
          <fnt><bib>QuV</bib></fnt>;
        </ekz><ekz>
          la tombo de <nom>Torquato Tasso</nom> en Romo, en la
          pre&gcirc;ejeto de v Sankta <nom>Onofrio </nom>sur monteto <tld/>o
          <fnt>
            <aut>Antonio De Salvo</aut>,
            <vrk><url
 ref="https://www.bitoteko.it/esperanto-vivo/eo/2019/04/25/torquato-tasso-3/"
 >Torquato Tasso</url></vrk>.
            <lok>Esperanto-vivo, 25 Aprilo 2019.</lok>
          </fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="be">
      <trd>&c_Ja;&c_n;&c_ib;&c_k;&c_u;&c_l;</trd>,
      <trd>&c_D;&c_zh;&c_a;&c_n;&c_ib;&c_k;&c_a;&c_l;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Gianicolo</trd>,
      <trd>Ianiculum</trd>
    </trdgrp>
    <trd lng="en">Janiculum</trd>
    <trd lng="fr">Janicule</trd>
    <trd lng="it">Gianicolo</trd>
    <trdgrp lng="la">
      <trd>Janiculum</trd>,
      <trd>Mons <ind>Janiculus</ind></trd>
    </trdgrp>
    <trd lng="pl">Janikulum</trd>
    <trd lng="ru">&c_Ja;&c_n;&c_i;&c_k;&c_u;&c_l;</trd>
  </drv>
</art>

