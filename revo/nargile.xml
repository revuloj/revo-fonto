<art mrk="$Id: nargile.xml,v 1.15 2024/01/29 15:21:35 revo Exp $">
<kap>
  <rad>nargile</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="nargile.0o">
  <kap><tld/>o</kap>
  <snc mrk="nargile.0o.FUM">
    <dif>
      Akvo<ref tip="super" cel="pip.0o.FUM">pipo</ref>
      kun alta akvovazo kaj longa enspirtubo,
      populara de Orienta kaj Norda afriko &gcirc;is Orientazio:
      <ekz>
        vi plenigu la forneton de &scirc;ia <tld/>o per tabako
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 3a, p. 18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_ja;&c_n;</trd>
  <trd lng="ca">narguil</trd>
  <trdgrp lng="cs">
    <trd>kali&aacute;n</trd>,
    <trd>nargil&eacute;</trd>,
    <trd>vodn&iacute; d&yacute;mka</trd>,
    <trd>&scaron;&iacute;&scaron;a</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wasserpfeife</trd>,
    <trd>Shisha</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>water pipe</trd>,
    <trd>hooka</trd>,
    <trd>hookah</trd>,
    <trd>narghile</trd>,
  </trdgrp>
  <trd lng="fr">narguil&eacute;</trd>
  <trd lng="ja">&#x6C34;&#x7159;&#x7BA1;  <pr>&#x307F;&#x305A;&#x304E;&#x305B;&#x308B;</pr></trd>
  <trd lng="nl">nargileh</trd>
  <trd lng="pl">szisza <klr>(fajka wodna)</klr></trd>
  <trdgrp lng="pt">
    <trd>narguil&eacute;</trd>,
    <trd>narguilh&eacute;</trd>
  </trdgrp>
  <trd lng="ro">narghilea</trd>
  <trdgrp lng="sk">
    <trd>nargil&eacute;</trd>,
    <trd>vodn&aacute; fajka</trd>
  </trdgrp>
  <trd lng="uk">&#x43D;&#x430;&#x440;&#x433;&#x456;&#x43B;&#x435; (&#x43A;&#x443;&#x440;&#x438;&#x43B;&#x44C;&#x43D;&#x438;&#x439; &#x43F;&#x440;&#x438;&#x431;&#x43E;&#x440;)</trd>
  <trdgrp lng="zh">
    <trd>&#x6C34;&#x70DF;&#x888B; <pr>shu&#x1D0;y&#x101;nd&#xE0;i</pr></trd>,
    <trd>&#x6C34;&#x7159;&#x888B; <pr>shu&#x1D0;y&#x101;nd&#xE0;i</pr></trd>,
    <trd>&#x6C34;&#x70DF; <pr>shu&#x1D0;y&#x101;n</pr></trd>,
    <trd>&#x6C34;&#x7159; <pr>shu&#x1D0;y&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>

