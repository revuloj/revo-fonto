<drv mrk="gvid.0a">
  <kap><tld/>a</kap>
  <snc mrk="gvid.0a.KOMUNE">
    <dif>
      Kiu <tld/>as:
      <ekz>
        <tld/>a stelo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        starigu al vi <tld/>ajn &scirc;tonojn, aran&gcirc;u al vi vojsignojn
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 31:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Fundamento de Esperanto devas trovi&gcirc;i en la manoj de &ccirc;iu bona esperantisto
        kiel konstanta <tld/>a dokumento
        <fnt>
          <bib>F</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a ideo
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj principoj de la Asocio
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;tatestroj en <tld/>aj pozicioj
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        prudentulo akiros <tld/>ajn kapablojn
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 1:5</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_r;&c_u;&c_ju;&c_ch;&c_y;</trd>
