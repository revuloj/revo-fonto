  </snc>
  <trd lng="be">&c_k;&c_ib;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_k; &c_p;&c_r;&c_a;&c_d;&c_p;&c_r;&c_y;&c_je;&c_m;&c_s;&c_t;&c_v;&c_a;</trd>
