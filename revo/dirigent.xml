<drv mrk="dirigent.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="direkt.0anto.de_organizo">Direktanto</ref> de <ref tip="malprt"
      cel="grup.muzik0o">muzikgrupo</ref>
      (orkestro, &hcirc;oro kc):
      <ekz>
        entuziasmajn vortojn mi legis en la tag&jcirc;urnalo <klr>[de]</klr> iu gastigata
        <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Julia Sigmond</aut>
          <vrk><url ref="&Monato;/2011/010147.php">Georgo</url></vrk>
          <lok>2011/01, p. 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;skultantoj longe, sincere kaj varme apla&ubreve;dis <klr>[&#8230;]</klr> al la orkestro,
        solistoj kaj la <tld/>o
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_y;&c_r;&c_y;&c_zh;&c_o;&c_r;</trd>
    <trd lng="de">Dirigent</trd>
    <trd lng="en">conductor <klr>(music)</klr></trd>
    <trd lng="fr">chef d'orchestre</trd>
    <trd lng="nl">dirigent</trd>
    <trd lng="pl">dyrygent</trd>
    <trd lng="ru">&c_d;&c_i;&c_r;&c_i;&c_zh;&c_jo;&c_r;</trd>
  </snc>
  <trd lng="ca">director <klr tip="amb">(d'orquestra, de cor)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#20048;&#38431;&#25351;&#25381; <pr>yu&egrave;du&igrave;zh&#464;hu&#299;</pr></trd>,
    <trd>&#25351;&#25381;&#32773; <pr>zh&#464;hu&#299;zh&ecaron;</pr></trd>,
    <trd>&#25351;&#25381;&#23478; <pr>zh&#464;hu&#299;ji&amacron;</pr></trd>,
    <trd>&#25351;&#25381; <pr>zh&#464;hu&#299;</pr></trd>
  </trdgrp>
