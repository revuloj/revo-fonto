<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ignac.xml,v 1.5 2023/08/07 07:24:36 revo Exp $">
<kap>
  <rad>Ignac</rad>/o
  <fnt>
    <bib>PIV1</bib>
  </fnt>
</kap>

<drv mrk="ignac.0o">
  <kap><tld/>o</kap>
  <snc mrk="ignac.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        jam komence de la 2a j<klr>arcento</klr> s<klr>ank</klr>ta
        Ignaco de Antio&hcirc;io deklaris ke ne gravas la aritmetika
        plimulto, sed ke <ctl>kie estas Kristo, tie estas la katolika
        eklezio</ctl>
        <fnt>
          <aut>S. Pokrovskij</aut>,
          <vrk>Religiaj notoj</vrk>,
          <bib>LOdE</bib>, <lok>2001:4 (78)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">Ignace</trd>
    <trd lng="pl">Ignacy</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_Ib;&c_g;&c_n;&c_a;&c_t;</trd>,
    <trd>&c_Ib;&c_g;&c_n;&c_a;&c_s;&c_mol;</trd>,

  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_Ib;&c_g;&#x43D;&c_a;&c_t;&c_ib;&c_j;</trd>,
    <trd>&c_Ib;&c_g;&#x43D;&c_a;&c_t;</trd>,
    <trd>&c_G;&#x43D;&c_a;&c_t;</trd>
  </trdgrp>
</drv>
</art>

