<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kaka</rad>/o
</kap>
<drv mrk="kaka.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Grajno, entenata en la frukto de la
      <ref tip="malprt" cel="kaka.0ujo"><tld/>ujo</ref>,
      el kiu oni faras la &ccirc;okoladon.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_a;&c_v;&c_a;</trd>
  <trd lng="de">Kakao</trd>
  <trd lng="en">cocoa bean</trd>
  <trd lng="fr">f&egrave;ve de cacao</trd>
  <trd lng="hu">kaka&oacute;bab</trd>
  <trd lng="ru">&c_k;&c_a;&c_k;&c_a;&c_o;</trd>
  <trd lng="tr">kakao</trd>
  <trd lng="nl">cacao</trd>
</drv>
<drv mrk="kaka.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
      cel="sterku.0oj">sterkuliacoj</ref>
      (<trd lng="la">Theobroma</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_k;&c_a;&c_v;&c_a; <klr>(&c_d;&c_r;&c_e;&c_v;&c_a;)</klr></trd>
  <trd lng="de">Kakaobaum</trd>
  <trd lng="en">cocoa tree</trd>
  <trd lng="fr">cacaoyer</trd>
  <trd lng="hu">kaka&oacute;fa</trd>
  <trd lng="ru">&c_k;&c_a;&c_k;&c_a;&c_o;
<klr>(&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;)</klr></trd>
  <trd lng="tr">kakao</trd>
  <trd lng="nl">cacaoboom</trd>
</drv>
</art>
