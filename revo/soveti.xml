<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: soveti.xml,v 1.7 2022/05/05 14:43:48 revo Exp $">
<kap>
    <rad>Soveti</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="soveti.0o">
  <kap><tld/>o</kap>
  <snc mrk="soveti.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="dif" cel="sovet.UniodeS0ajSocialismajRespublikoj">Unio de
      Sovetaj Socialismaj Respublikoj</ref>:
      <ekz>
        tiel restis &gcirc;is la atako de Germanio kontra&ubreve; <tld/>o, la 22an
        de junio 1941<fnt><aut>Zofia Banet-Fornalowa</aut>, <vrk>Kadi&scirc;
        por
        Jedwabne</vrk>, <bib>Monato</bib>, <lok>jaro 2002a, numero 1a, p.
        18a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>o estas laborista &scirc;tato
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        la malapero de la Varsovia Pakto kaj de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato1/monato-9310-sarajevo.txt">La Fino de la Mondo Komenci&gcirc;as en Sarajevo</url></vrk>
          <lok>93/10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib;
  &c_S;&c_a;&c_ju;&c_z;</trd>
  <trd lng="cs">Sov&ecaron;tsk&yacute; svaz</trd>
  <trd lng="de">Sowjetunion</trd>
  <trd lng="fr">URSS</trd>
  <trdgrp lng="pl">
    <trd>Zwi&aogonek;zek Socjalistycznych Republik Radzieckich</trd>,
    <trd>Sowiety<klr> (pot., obra&zacute;l.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_S;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;
    &c_S;&c_o;&c_ju;&c_z;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_n;&c_a;
    &c_S;&c_o;&c_v;&c_je;&c_t;&c_o;&c_v;</trd>
  </trdgrp>
  <trd lng="sk">Sovietsky zv&auml;z</trd>
  <trd lng="uk">&#x420;&#x430;&#x434;&#x44F;&#x43D;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x421;&#x43E;&#x44E;&#x437;</trd>
</drv>

<drv mrk="soveti.s0a">
  <kap><tld lit="s"/>a</kap>
  <snc mrk="soveti.s0a.rilata">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        ili havas &ccirc;i tie pli, ol cent leterojn el USSR,
        po&scirc;tkartojn,
        amason da fotoj, <tld lit="s"/>ajn librojn kaj gazetojn
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        la disfalo de la <tld lit="s"/>a imperio
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Steloj;monato2/006815.html">Opinio: &Ccirc;u nova &scirc;anco por malfortaj lingvoj?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a Esperantista Junulara Movado
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Steloj;monato2/006807.html">Fascina gazeto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_v;&c_je;&c_c;&c_k;&c_ib;</trd>
  <trd lng="fr">sovi&eacute;tique<klr> (adj.)</klr></trd>
  <trdgrp lng="pl">
    <trd>radziecki</trd>,
    <trd>sowiecki<klr> (pot., obra&zacute;l.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="de">sowjetisch</trd>
</drv>

</art>

