<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: pilzen.xml,v 1.2 2025/03/03 12:53:25 afido Exp $">
    <kap>
      <rad>Pilzen</rad>/o, <var><kap><rad var="s">Pilsen</rad>/o</kap></var>
    </kap>

<drv mrk="pilzen.0o">
  <kap><tld/>o, <var><kap><tld var="s"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="malprt" cel="bohem.0ujo">Bohema</ref>
      <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>,
      &ccirc;. 80&#8239;km sudokcidente de Prago, fama pro bierspeco nomita la&ubreve; la urbo:
      <ekz>
        alloge &ccirc;emane ku&scirc;is en ladujoj <klr>[&#8230;]</klr> eloj, maltbieroj, <tld lit="p"/>aj
        bieroj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 47</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; 1968, la <tld var="s"/>biera konsumado en la belga merkato kulminis je 90
        procentoj
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van De Velde</aut>
          <vrk>Bierlando Belgio</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">Plze&ncaron;</trd>
    <trd lng="de">Pilsen</trd>
    <trd lng="en">Plze&ncaron;</trd>
    <trd lng="fr">Pilsen</trd>
    <trd lng="ru">&c_P;&c_l;&c_mol;&c_z;&c_je;&c_n;&c_mol;</trd>
    <trd lng="uk">&c_P;&c_l;&c_mol;&c_z;&c_je;&c_n;&c_mol;</trd>
  </snc>
<trd lng="be">&c_P;&c_l;&c_mol;&c_z;&c_e;&c_n;&c_mol;</trd>
</drv>

  </art>

