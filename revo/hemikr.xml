<art mrk="$Id: hemikr.xml,v 1.16 2023/11/15 16:45:18 revo Exp $">
<kap>
  <rad>hemikrani</rad>/o <fnt><bib>SPV</bib></fnt> 
</kap>
<drv mrk="hemikr.0o">
  <kap><tld/>o</kap>
  <snc mrk="hemikr.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="migren.0o.MED">Migreno</ref>:
      <ekz>
        kelkaj homoj trasuferas <tld/>on dum tagoj,
        ne horoj
        <fnt>
          <aut>C. McKee</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/cindio/344237">
          What a Migraine Feels Like</url></vrk>,
          <lok>2011-09-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_g;&c_r;&c_e;&c_n;&c_mol;</trd>,
    <trd>&c_g;&c_je;&c_m;&c_ib;&c_k;&c_r;&c_a;&c_n;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_i;&c_g;&c_r;&c_je;&c_n;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>migranya</trd>,
    <trd>hemicr&agrave;nia</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hemikranie</trd>,
    <trd>migr&eacute;na</trd>
  </trdgrp>
  <trd lng="de">Migr&auml;ne</trd>
  <trd lng="en">migraine</trd>
  <trdgrp lng="es">
    <trd>jaqueca</trd>,
    <trd>migra&ntilde;a</trd>
  </trdgrp>
  <trd lng="fr">migraine</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;loldali <ind>fejf&aacute;j&aacute;s</ind></trd>,
    <trd>hemicrania</trd>,
    <trd>migr&eacute;n</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>migrain</trd>,
    <trd><ind>sakit</ind> kepala sebelah</trd>
  </trdgrp>
  <trd lng="it">emicrania</trd>
  <trd lng="nl">migraine</trd>
  <trdgrp lng="pt">
   <trd>hemicrania</trd>, 
   <trd>enxaqueca</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_g;&c_r;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_g;&c_je;&c_m;&c_i;&c_k;&c_r;&c_a;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>boles&tcaron; hlavy <klr>(jednostrann&aacute;)</klr></trd>,
    <trd>migr&eacute;na</trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&#x456;&#x491;&#x440;&#x435;&#x43D;&#x44C;</trd>
  <trd lng="zh">&#x504F;&#x5934;&#x75DB; <pr>pi&#x101;nt&#xF3;ut&#xF2;ng</pr></trd>
</drv>
</art>

