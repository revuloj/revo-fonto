<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: lauxr1.xml,v 1.9 2024/12/13 08:36:44 revo Exp $">
    <kap>
      <rad>La&ubreve;r</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="lauxr1.0o">
  <kap><tld/>o</kap>
  <snc mrk="lauxr1.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Virina <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.antaux0o">anta&ubreve;nomo</ref>
      latindevena:
      <ekz>
        mi devas tie esti kaj vidi <tld/>on en la egipta danco!
        <fnt>
          <bib>Marta</bib>
        </fnt>
      </ekz>
      <ekz>
        &ndash; Mi volas vidi &scirc;in, &ndash; Kiun? &ndash; <tld/>on, &ndash; &Scirc;i foriris
        <fnt>
          <bib>Kon11</bib>
          <lok>Novelo</lok>
        </fnt>.
      </ekz>
      <!-- ne asimilita formo:
      <ekz>
        la pordo de la domo malfermi&gcirc;is, kaj <tld/>a elvenis portante
        sitelon da for&jcirc;eta&jcirc;oj el la kuirejo
        <fnt><aut>Jouko Lindstedt</aut>,
        <vrk><url ref="&Monato;/2000/006918.php">La helikoj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/07, p. 16</lok>
        </fnt>.
      </ekz>
      -->
    </dif>
    <trd lng="de">Laura</trd>
    <trd lng="fr">Laura</trd>
    <trd lng="pl">Laura</trd>
    <trd lng="ro">Laura</trd>
    <trd lng="uk">&c_L;&c_a;&c_u;&c_r;&c_a;</trd>
  </snc>
  <rim>
    Kp <ref tip="hom" cel="lauxr.0o">la&ubreve;ro</ref> (vegeta&jcirc;o).
  </rim>

  <trdgrp lng="be">
    <trd>&c_L;&c_a;&c_w;&c_r;&c_a;</trd>,
    <trd>&c_L;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x52B3;&#x62C9; <pr>l&#xE1;ol&amacron;</pr></trd>,
    <trd>&#x52DE;&#x62C9; <pr>l&#xE1;ol&amacron;</pr></trd>
  </trdgrp>
</drv>
  </art>

