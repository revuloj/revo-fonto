<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>tamtam</rad>/o
</kap>
<drv mrk="tamtam.0o">
<kap><tld/>o</kap>
<snc>
<uzo tip="fak">MUZ</uzo> 
<dif>
&Ccirc;ina <ref tip="super" cel="instru2.frap0o">frapinstrumento</ref> konsistanta el granda metala disko vertikale pendanta, kiun oni frapas. 
</dif>
</snc>
<trd lng="de">Tamtam</trd>
<trd lng="nl">gong</trd>
<trd lng="sv">gonggong</trd>
</drv>
<drv mrk="tamtam.0i">
<kap><tld/>i</kap>
<gra><vspec>ntr</vspec></gra>
<snc num="1">
<dif>
Sonigi <tld/>on. 
</dif>
</snc>
<snc num="2">
<uzo tip="stl">FIG</uzo><dif>
Brue reklami 
</dif>
<refgrp tip="sin">
<ref cel="trumpe.dis0i">distrumpeti</ref>.
</refgrp>
</snc>
</drv>
</art>
