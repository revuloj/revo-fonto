<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tamtam.xml,v 1.29 2024/01/06 21:24:08 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>tamtam</rad>/o
</kap>
<drv mrk="tamtam.0o">
  <kap><tld/>o</kap>
  <snc mrk="tamtam.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      &Ccirc;ina <ref tip="lst" cel="instru2.frap0o" lst="voko:frapinstrumentoj">frapinstrumento</ref>
      konsistanta el granda metala disko vertikale pendanta, kiun oni
      frapas:
      <ekz>
        de bran&ccirc;o de apuda arbo pendis kupra <tld/>o
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 15a, Omar fari&gcirc;as
        ventegocentro</lok></fnt>;
      </ekz>
      <ekz>
        li ofte iris ludi <tld/>on &ccirc;e semajnfinoj de afrika dancado
        <fnt>
          <bib>ChR</bib>
          <lok>Ne nur tamtamado</lok>
        </fnt>;
      </ekz>
      <ekz>
        de bran&ccirc;o de apuda arbo pendis kupra <tld/>o, en kiu, en tiu momento, pentrinde
        speguli&gcirc;is oraj sunradioj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cimbal.0o">cimbalo</ref>,
      <ref cel="gong.0o">gongo</ref>
    </refgrp>
  </snc>
  <snc mrk="tamtam.0o.sonigo">
    <dif>
      Sonigo de <tld/>o<sncref ref="tamtam.0o.MUZ"/>:
      <ekz>
        post plendtona funebra <tld/>o, la&ubreve; ordono de la orkestro-direktanto levi&gcirc;is
        supren malpaciencaj ar&ccirc;oj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam mi ree kaj ree estis prilaborinta la vandojn kaj anka&ubreve; per krioj akompanis
        miajn <tld/>ojn, oni malfermas lukon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 45</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_m;&c_t;&c_a;&c_m;</trd>,
    <trd>&c_g;&c_o;&c_n;&c_g;</trd>
  </trdgrp>
  <trd lng="bg">&c_t;&c_a;&c_m;-&c_t;&c_a;&c_m;</trd>
  <trd lng="ca">tam-tam</trd>
  <trd lng="cs">tamtam</trd>
  <trd lng="de">Tamtam</trd>
  <trd lng="en">tamtam</trd>
  <trdgrp lng="fr">
    <trd>tam-tam</trd>,
    <trd>tamtam</trd>
  </trdgrp>
  <trd lng="hu">tamtam</trd>
  <trdgrp lng="ja">
    <trd>&#x30BF;&#x30E0;&#x30BF;&#x30E0;</trd>,
    <trd>&#x30C8;&#x30E0;&#x30C8;&#x30E0;</trd>
  </trdgrp>
  <trd lng="nl">gong</trd>
  <trd lng="pl">tam-tam</trd>
  <trdgrp lng="pt">
   <trd>tant&atilde;</trd>,
   <trd>gongo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_m;&c_t;&c_a;&c_m;</trd>,
    <trd>&c_g;&c_o;&c_n;&c_g;</trd>
  </trdgrp>
  <trd lng="sk">tamtam</trd>
  <trd lng="sv">gonggong</trd>
  <trd lng="uk">&c_t;&c_a;&c_m;&c_t;&c_a;&c_m; (&c_m;&c_u;&c_z;. &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</trd>
</drv>

<drv mrk="tamtam.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tamtam.0i.MUZ">
    <dif>
      Sonigi <tld/>on:
      <ekz>
        li <tld/>is kun senkompara energio
        <fnt>
          <bib>ChR</bib>
          <lok>Ne nur tamtamado</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frap.0i.produktiSonon">frapi</ref>,
      <ref cel="perkut.0i.muziki">perkuti<sncref/></ref>,
      <ref cel="tambur.0i">tamburi</ref>
    </refgrp>
    <trd lng="ca">tocar el tam-tam</trd>
    <trd lng="nl">op de <ind>gong</ind> slaan</trd>
  </snc>
  <snc mrk="tamtam.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Brue <ref cel="reklam.0i">reklami</ref>:
      <ekz>
        ni nun komencis tamtami la grandan avanta&gcirc;on de la krampoj sur nia kuirilo
        <fnt>
          <bib>KrM</bib>
          <lok>38. Du Botoj Faras Paron.</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>as per listo de pli ol cent e&ubreve;roparlamentanoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2002, &#8470;6</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="trumpe.dis0i.FIG">distrumpeti</ref>,
      <ref cel="herold.0i">heroldi</ref>
    </refgrp>
    <trdgrp lng="ca">
      <trd>protestar iradament</trd>,
      <trd>baladrejar</trd>
    </trdgrp>
    <trd lng="nl">tamtam maken</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_c;&c_mol; &c_u; <ind>&c_t;&c_a;&c_m;&c_t;&c_a;&c_m;</ind></trd>,
    <trd>&c_b;&c_ib;&c_c;&c_mol; &c_u; <ind>&c_g;&c_o;&c_n;&c_g;</ind></trd>
  </trdgrp>
  <trd lng="de">Tamtam machen</trd>
  <trd lng="hu">nagy <ind>h&udblac;h&oacute;t csap</ind> k&ouml;r&uuml;l&ouml;tte</trd>
  <trdgrp lng="ja">
    <trd>&#x30BF;&#x30E0;&#x30BF;&#x30E0;&#12434;&#25171;&#12385;&#x9CF4;&#12425;&#12377; 
    <pr>&#x30BF;&#x30E0;&#x30BF;&#x30E0;&#12434;&#12358;&#12385;&#x306A;&#12425;&#12377;</pr></trd>,
    <trd>&#x9CF4;&#x308A;&#29289;&#20837;&#x308A;&#12391;&#x5BA3;&#x4F1D;&#12377;&#x308B; 
    <pr>&#x306A;&#x308A;&#12418;&#x306E;&#12356;&#x308A;&#12391;&#x305B;&#12435;&#12391;&#12435;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">gra&cacute; na  <ind>tam-tamach</ind></trd>
  <trd lng="ru">&c_b;&c_i;&c_t;&c_mol; &c_v; <ind>&c_t;&c_a;&c_m;&c_t;&c_a;&c_m;</ind></trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_i;&c_t;&c_i; &c_v; &c_t;&c_a;&c_m;&c_t;&c_a;&c_m;</trd>,
    <trd>&c_sh;&c_u;&c_m;&c_n;&c_o; &c_r;&c_o;&c_z;&c_r;&c_je;&c_k;&c_l;&c_a;&c_m;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_shch;&c_o;-&c_n;.)</trd>
  </trdgrp>
</drv>
</art>
