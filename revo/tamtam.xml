<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>tamtam</rad>/o
</kap>
<drv mrk="tamtam.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      &Ccirc;ina <ref tip="super" cel="instru2.frap0o">frapinstrumento</ref>
      konsistanta el granda metala disko vertikale pendanta, kiun oni
      frapas.
    </dif>
  </snc>
  <trd lng="de">Tamtam</trd>
  <trd lng="nl">gong</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_m;&c_t;&c_a;&c_m;</trd>,
    <trd>&c_g;&c_o;&c_n;&c_g;</trd>
  </trdgrp>
  <trd lng="sv">gonggong</trd>
</drv>

<drv mrk="tamtam.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Sonigi <tld/>on.
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Brue reklami
    </dif>
    <refgrp tip="sin">
      <ref cel="trumpe.dis0i">distrumpeti</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_b;&c_i;&c_t;&c_mol; &c_v; <ind>&c_t;&c_a;&c_m;&c_t;&c_a;&c_m;</ind></trd>
</drv>
</art>
