<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>tamtam</rad>/o
</kap>
<drv mrk="tamtam.0o">
  <kap><tld/>o</kap>
  <snc mrk="tamtam.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      &Ccirc;ina <ref tip="super" cel="instru2.frap0o">frapinstrumento</ref>
      konsistanta el granda metala disko vertikale pendanta, kiun oni
      frapas:
      <ekz>
        de bran&ccirc;o de apuda arbo pendis kupra <tld/>o
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 15a, Omar fari&gcirc;as
        ventegocentro</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gong.0o">gongo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_m;&c_t;&c_a;&c_m;</trd>,
    <trd>&c_g;&c_o;&c_n;&c_g;</trd>
  </trdgrp>
  <trd lng="de">Tamtam</trd>
  <trd lng="hu">tamtam</trd>
  <trd lng="nl">gong</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_m;&c_t;&c_a;&c_m;</trd>,
    <trd>&c_g;&c_o;&c_n;&c_g;</trd>
  </trdgrp>
  <trd lng="sv">gonggong</trd>
</drv>

<drv mrk="tamtam.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="tamtam.0i.MUZ">
    <dif>
      Sonigi <tld/>on.
    </dif>
  </snc>
  <snc mrk="tamtam.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Brue reklami.
    </dif>
    <refgrp tip="sin">
      <ref cel="trumpe.dis0i.FIG">distrumpeti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_c;&c_mol; &c_u; <ind>&c_t;&c_a;&c_m;&c_t;&c_a;&c_m;</ind></trd>,
    <trd>&c_b;&c_ib;&c_c;&c_mol; &c_u; <ind>&c_g;&c_o;&c_n;&c_g;</ind></trd>
  </trdgrp>
  <trd lng="de">Tamtam machen</trd>
  <trd lng="hu">nagy <ind>h&udblac;h&oacute;t csap</ind> k&ouml;r&uuml;l&ouml;tte</trd>
  <trd lng="ru">&c_b;&c_i;&c_t;&c_mol; &c_v; <ind>&c_t;&c_a;&c_m;&c_t;&c_a;&c_m;</ind></trd>
</drv>
</art>
