<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sturm.xml,v 1.16 2023/11/19 01:40:30 revo Exp $">
<kap>
  <rad>sturm</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sturm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Impetege <ref tip="super" cel="atak.0i.batali">ataki<sncref/></ref>:
      <ekz>
        centoj da manifestaciantoj provis <tld/>i la Kongreson
        <fnt>
          <bib>MD02</bib>
        </fnt>;
      </ekz>
      <ekz>
        la a&ubreve;toritatoj sie&gcirc;as la vila&gcirc;on dum 3 monatoj, hezitas <tld/>i &gcirc;in,
        <fnt>
          <bib>MD02</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        densa svarmo da homoj al<tld/>is min kaj kriis: eli&gcirc;u <klr>[&#8230;]</klr>
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        anka&ubreve; &ccirc;iuj malsanaj kaj mortantaj
        elektorajtaj irakanoj heroe <tld/>is al urnoj por elekti sian
        heroon
        <fnt><aut>Stefan Maul</aut>: <vrk>Du mil du</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 12a, p. 7a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konker.0i.militakiri">konkeri</ref>,
      <ref cel="akir.milit0i">militakiri</ref>,
      <ref cel="okup.0i.preni">okupi<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_u;&c_r;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>arremetre</trd>,
    <trd>assaltar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vz&iacute;t &uacute;tokem</trd>,
    <trd>&uacute;to&ccaron;it zte&ccaron;&iacute;</trd>
  </trdgrp>
  <trd lng="de">st&uuml;rmen <klr>(Mil.)</klr></trd>
  <trdgrp lng="en">
    <trd>storm</trd>,
    <trd>assault</trd>,
    <trd>charge</trd>,
    <trd>attack</trd>
  </trdgrp>
  <trd lng="fr">assaillir</trd>
  <trdgrp lng="hu">
    <trd>rohamoz</trd>,
    <trd>megrohamoz</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>assaltare</trd>,
    <trd>caricare<klr> (assalire)</klr></trd>,
    <trd>attaccare<klr> (assalire)</klr></trd>,
    <trd>investire<klr> (assalire)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7A81;&#25731;&#12377;&#x308B;  <pr>&#12392;&#12388;&#12370;&#x304D;&#12377;&#x308B;</pr></trd>,
    <trd>&#35186;&#25731;&#12377;&#x308B;  <pr>&#12375;&#12421;&#12358;&#12370;&#x304D;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">bestormen</trd>
  <trdgrp lng="pl">
    <trd>szturmowa&cacute;</trd>,
    <trd>rzuci&cacute; si&eogonek; na</trd>
  </trdgrp>
  <trd lng="pt">assaltar</trd>
  <trd lng="sk">vzia&tcaron; &uacute;tokom</trd>
  <trd lng="uk">&c_sh;&c_t;&c_u;&c_r;&c_m;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="sturm.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Impeta <ref tip="super" cel="atak.0o.batalo">atako</ref>:
      <ekz>
        <klr>[li]</klr> detale priskribas sie&gcirc;ojn kaj <tld/>ojn alfrontitajn de la Ru&gcirc;a
        Armeo
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2015/011403.php">Misiisto, mar&scirc;ado kaj detenanta mano</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lasta minuto anta&ubreve; la <tld/>o
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>;
      </ekz>
      <ekz>
        batu lin! &ndash; li kriis per la vo&ccirc;o, per kiu dum granda <tld/>o iu bravega
        le&ubreve;tenanto krias al sia plotono: &leftquot;kamaradoj, anta&ubreve;en!&rightquot;
        <fnt>
          <bib>LOdE</bib>
          <lok> 2001, &#8470;2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[la]</klr> &jcirc;urnaloj sukcese eltenis la <tld/>on de la retaj gazetoj kaj
        nova&jcirc;servoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 2 (100)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konker.0o">konkero</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_u;&c_r;&c_m;</trd>
  <trdgrp lng="ca">
    <trd>assalt</trd>,
    <trd>assalt</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prudk&yacute; &uacute;tok</trd>,
    <trd>zte&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erst&uuml;rmung</trd>,
    <trd>Ansturm</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>storm</trd>,
    <trd>assault</trd>,
    <trd>charge</trd>,
    <trd>attack</trd>
  </trdgrp>
  <trd lng="fr">assaut</trd>
  <trd lng="hu">roham</trd>
  <trdgrp lng="it">
    <trd>assalto</trd>,
    <trd>carica<klr> (assalto)</klr></trd>,
    <trd>attacco<klr> (assalto)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7A81;&#25731;  <pr>&#12392;&#12388;&#12370;&#x304D;</pr></trd>,
    <trd>&#35186;&#25731;  <pr>&#12375;&#12421;&#12358;&#12370;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">bestorming</trd>
  <trdgrp lng="pl">
    <trd>szturm</trd>,
    <trd>atak</trd>
  </trdgrp>
  <trd lng="pt">assalto</trd>
  <trd lng="sk">&uacute;tok</trd>
  <trd lng="uk">&c_sh;&c_t;&c_u;&c_r;&c_m;</trd>
</drv>
</art>

