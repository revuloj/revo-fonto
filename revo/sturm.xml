<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sturm</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sturm.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Impetege ataki:
      <ekz>
        <uzo tip="stl">FIG</uzo>anka&ubreve; &ccirc;iuj malsanaj kaj mortantaj
        elektorajtaj irakanoj heroe <tld/>is al urnoj por elekti sian
        heroon<fnt><aut>Stefan Maul</aut>: <vrk>Du mil du</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 12a, p. 7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_u;&c_r;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="fr">assaillir</trd>
  <trd lng="pt">assaltar</trd>
  <trdgrp lng="hu">
    <trd>rohamoz</trd>,
    <trd>megrohamoz</trd>
  </trdgrp>
  <trd lng="nl">bestormen</trd>
  <trd lng="uk">&c_sh;&c_t;&c_u;&c_r;&c_m;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="sturm.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ordinare amasa, impeta atako.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_u;&c_r;&c_m;</trd>
  <trd lng="fr">assaut</trd>
  <trd lng="pt">assalto</trd>
  <trd lng="hu">roham</trd>
  <trd lng="nl">bestorming</trd>
  <trd lng="uk">&c_sh;&c_t;&c_u;&c_r;&c_m;</trd>
</drv>
</art>

