<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pulm.xml,v 1.28 2024/02/17 12:12:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pulm</rad>/o
</kap>

<drv mrk="pulm.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="organ.0o.BIO">Organo</ref> de la aerspirado:
      <ekz>
        trejnitaj helpantoj povas atingi viktimojn inter tri kaj kvar
        minutoj: tiam ankora&ubreve; restas oksigeno en la
        <tld/>oj<fnt><aut>Jomo
        Ipfelkofer</aut>: <vrk>Esploroj kortu&scirc;aj</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 9a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_jo;&c_g;&c_k;&c_a;&c_je; <klr>(&c_v;&c_o;&c_r;&c_g;&c_a;&c_n;)</klr></trd>
  <trd lng="bg">&c_b;&c_ja;&c_l; &c_d;&c_r;&c_o;&c_b;</trd>
  <trd lng="bo">&#xF42;&#xFB3;&#xF7C;&#xF0B;&#xF56;&#xF0B;</trd>
  <trd lng="de">Lunge</trd>
  <trd lng="en">lung</trd>
  <trd lng="es">pulm&oacute;n</trd>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_damma;&a_sh;</trd>,
    <trd>&a_r;&f_y;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">poumon</trd>
  <trd lng="hu">t&uuml;d&odblac;</trd>
  <trd lng="id">paru-paru</trd>
  <trd lng="it">polmone</trd>
  <trd lng="ja">&#x80BA;  <pr>&#x306F;&#x3044;</pr></trd>
  <trd lng="nl">long</trd>
  <trd lng="pl">p&lstroke;uco</trd>
  <trd lng="pt">pulm&atilde;o</trd>
  <trd lng="ru">&c_l;&c_jo;&c_g;&c_k;&c_o;&c_je; <klr tip="amb">(&c_o;&c_r;&c_g;&c_a;&c_n;)</klr></trd>
  <trd lng="sk">p&#x13E;&#xFA;ca</trd>
  <trd lng="uk">&#x43B;&#x435;&#x433;&#x435;&#x43D;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x80BA; <pr>f&#xE8;i</pr></trd>,
    <trd>&#x80BA;&#x90E8; <pr>f&#xE8;ib&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pulm.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        inter la mortintoj pro
        fumado 45 % suferis pro kronika malsano &ccirc;e <tld/>o, 15 %
        malsani&gcirc;is
        je <tld/>a kancero<fnt><aut>Mu Binghua</aut>: <vrk>Mortintoj pro
        fumado</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 3a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_jo;&c_g;&c_a;&c_ch;&c_n;&c_y;</trd>
  <trd lng="en">pulmonary</trd>
  <trd lng="es">pulmonar</trd>
  <trd lng="fa">&a_r;&f_y;&a_w;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>pulmonaire</trd>
  </trdgrp>
  <trd lng="hu">t&uuml;d&odblac;-</trd>
  <trd lng="it">polmonare</trd>
  <trd lng="ja">&#x80BA;&#x306E;  <pr>&#x306F;&#x3044;&#x306E;</pr></trd>
  <trd lng="nl">long-</trd>
  <trd lng="pl">p&lstroke;ucny</trd>
  <trd lng="uk">&#x43B;&#x435;&#x433;&#x435;&#x43D;&#x435;&#x432;&#x438;&#x439;</trd>
</drv>
</art>
