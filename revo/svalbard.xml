<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: svalbard.xml,v 1.2 2025/03/21 08:35:22 afido Exp $">
    <kap>
      <rad>Svalbard</rad>/o
    </kap>

<drv mrk="svalbard.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Arkta <ref tip="super" cel="insul.0aro">insularo</ref> &ccirc;. 2000&#8239;km norde de Oslo:
      <ekz>
        en la <tld/>a traktato Norvegio akiris limigitan suverenecon pri la insuloj en
        1920
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Svalbardo"
              >Svalbardo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        juna hungara paro, kiu trajnis <klr>[&#8230;]</klr> al Bergeno, kaj franca veteranino,
        kiu partoprenas en seminario de radioamatoroj, kiuj poste ekskursos al <tld/>o
        <fnt>
          <vrk><url ref="https://esperanto.no/uploads/images/oslo/bulteno/kluba-bulteno-200903.pdf"
              >Someraj gastoj</url></vrk>
          <lok>Kluba Bulteno, Oslo, n-ro 3 2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Spitzbergen <klr>(Inselgruppe)</klr></trd>
    <trd lng="en">Svalbard</trd>
    <trd lng="fr">Svalbard</trd>
    <trd lng="ru">&c_Sh;&c_p;&c_i;&c_c;&c_b;&c_je;&c_r;&c_g;&c_je;&c_n;</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_S;&c_v;&c_a;&c_l;&c_mol;&c_b;&c_a;&c_r;&c_d;</trd>,
  <trd>&c_Sh;&c_p;&c_ib;&c_c;&c_b;&c_e;&c_r;&c_g;&c_je;&c_n;</trd>
</trdgrp>
</drv>

  </art>

