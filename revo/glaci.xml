<drv mrk="glaci.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Akvo malfluidi&gcirc;inta, solidi&gcirc;inta pro malvarmo:
      <ekz>
        en la printempo la <tld/>o kaj la ne&gcirc;o fluidi&gcirc;as
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> disbatis la <tld/>on per sia ligna &scirc;uo, savis la anasidon
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve;e na&gcirc;is grandaj montoj de <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o krakis, <klr>[&#8230;]</klr> la glitkurantoj svingi&gcirc;adis sur la brilantaj
        lagoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia koro estis kvaza&ubreve; bulo da <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la direktoro disrompis la <tld/>on de silento per facilanima rido
        <fnt>
          <bib>Kiso</bib>
          <lok>La Sakramento De La Komunio</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>konstrui kastelon sur <tld/>o</ind>
        <klr>(<ref tip="vid" cel="aer.0o">aero</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">leveg&odblac;be &eacute;p&iacute;t v&aacute;rat</trd>
      </ekz>
      <ekz>
        al <tld/>o printempa kaj al amiko tro nova ne fidu
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>mielo sur lango kaj <tld/>o <klr>(indiferenteco)</klr>
        en koro</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">szava m&eacute;z, sz&iacute;ve j&eacute;g</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="frost.0o">frosto</ref>
  </snc>
