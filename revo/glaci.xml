<drv mrk="glaci.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="glaci.0ejo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Surtera, precipe surmonta, amasego da <tld/>o el ne&gcirc;o dum
      jarcentoj transformi&gcirc;inta en <tld/>on:
      <ekz>
        la korpo de prahistoria homo, trovita anta&ubreve; dek jaroj en
        <tld/>ejo en la Alpoj, estos malfrostigita
        <fnt>
          <aut>E. Georgiev</aut>,
          <vrk>Pa&scirc;tisto de la pasinteco</vrk>,
          <bib>Monato</bib>
        </fnt>;
      </ekz>
      <ekz>
        la natura pejza&gcirc;o <klr>...</klr> estas varia rezulte de
        erozio de la antikvaj <tld/>ejoj, hodia&ubreve; ne plu
        ekzistantaj
        <fnt>
          <aut>J. de N. Marques</aut>,
          <vrk>Voja&gcirc;o al Ushuaia</vrk>,
          <bib>Monato</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi falis en grandan fendegon, kiu trairas la tutan
        <tld/>ejon tra &gcirc;ia tuta dikeco
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>&Ccirc;u li?</vrk>,
          <lok>&Ccirc;ap. 6a.</lok>
        </fnt>
      </ekz>
    </dif>
      <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_d;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_l;&c_je;&c_d;&c_a;&c_v;&c_ib;&c_k;</trd>
  </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_g;&c_l;&c_je;&c_t;&c_ch;&c_je;&c_r;</trd>,
      <trd>&c_l;&c_je;&c_d;&c_n;&c_i;&c_k;</trd>
    </trdgrp>
  <trd lng="cs">ledovec</trd>
    <trd lng="de">Gletscher</trd>
    <trd lng="en">glacier</trd>
    <trd lng="es">glaciar</trd>
    <trd lng="fa">&f_y;&a_kh;&f_ch;&a_A;&a_l; &a_T;&a_b;&f_y;&a_ayn;&f_y;</trd>
    <trd lng="fr">glacier<klr tip="ind"> (g&eacute;og.)</klr></trd>
    <trdgrp lng="hu">
      <trd>j&eacute;gtakar&oacute;</trd>,
      <trd>j&eacute;gmez&odblac;</trd>,
      <trd>gleccser</trd>
    </trdgrp>
    <trd lng="id">gletser</trd>
    <trd lng="nl">gletsjer</trd>
    <trd lng="pt">geleira</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_je;&c_d;&c_n;&c_i;&c_k;<klr tip="ind"> (&c_v; &c_g;&c_o;&c_r;&c_a;&c_h;)</klr></trd>,
      <trd>&c_g;&c_l;&c_je;&c_t;&c_ch;&c_je;&c_r;</trd>
    </trdgrp>
  </snc>
  <snc mrk="glaci.0ejo.ujo">
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="glaci.0ujo0sxranko0tenejo"><tld/>ujo</ref>
  </snc>
  <trd lng="sk">&lcaron;adovec</trd>
  <trd lng="uk">&#x43B;&#x44C;&#x43E;&c_d;&#x43E;&c_v;&c_i;&#x43A;</trd>
</drv>

<drv mrk="glaci.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malfluecigi, solidigi flua&jcirc;on per malvarmo:
      <ekz>
        la frosto <tld/>igis la lagon.
      </ekz>
    </dif>
    <trd lng="hu">fagyaszt</trd>
    <trd lng="tr">dondurmak</trd>
    <trd lng="pt">congelar</trd>
    <trd lng="ru">&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;
      &c_l;&c_mol;&c_d;&c_o;&c_m;</trd>
  </snc>
  <snc>
    <dif>
      Meti en malvarmegan staton:
      <ekz>
        la ekstera frosto <tld/>igis min, miajn fingrojn;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        tute <tld/>igita de teruro.
      </ekz>
    </dif>
    <trd lng="hu">dermeszt</trd>
    <trd lng="pt">congelar</trd>
    <trd lng="ru">&c_z;&c_a;&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>mrazit</trd>,
    <trd>nechat zmrznout</trd>,
    <trd>zmrazit</trd>
  </trdgrp>
  <trd lng="be">&c_z;&c_a;&c_m;&c_a;&c_r;&c_o;&c_z;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="de">vereisen</trd>
  <trd lng="en">freeze</trd>
  <trd lng="es">congelar</trd>
  <trd lng="fa">&a_m;&a_n;&a_j;&a_m;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
      <trd>glacer<klr tip="ind"> (transformer en glace)</klr></trd>,
      <trd>geler<klr tip="ind"> (tr.)</klr></trd>
    </trdgrp>
  <trd lng="id">mem<ind>beku</ind>kan</trd>
  <trd lng="nl">(doen) <ind>bevriezen</ind></trd>
  <trd lng="sk">mrazi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#x43C;&#x43E;&c_r;&#x43E;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&#x43A;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i; &#x43B;&#x44C;&#x43E;&c_d;&#x43E;&#x43C;/&#x43A;&c_r;&c_i;&c_g;&#x43E;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="glaci.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Malflueci&gcirc;i pro malvarmo:
      <ekz>
        la rivero <tld/>i&gcirc;as.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>eljegesedik</trd>,
      <trd>j&eacute;gg&eacute; fagy</trd>
    </trdgrp>
    <trd lng="tr">donmak</trd>
  </snc>
  <snc>
    <dif>
      Veni en malvarmegan staton:
      <ekz>
        la soldatoj <tld/>i&gcirc;is dum la nokta gardostaro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la rideto <tld/>i&gcirc;is <klr>(haltis, malaperis)</klr>
        sur &scirc;iaj lipoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i&gcirc;i de timo.
      </ekz>
    </dif>
    <trd lng="hu">megdermed</trd>
  </snc>
  <refgrp tip="vid">
    <ref cel="frost.0igxi">frosti&gcirc;i</ref>
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_l;&c_je;&c_d;&c_z;&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_jo;&c_r;&c_z;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">vereisen</trd>
  <trd lng="en">freeze</trd>
  <trd lng="es">congelarse</trd>
  <trdgrp lng="fa">
    <trd>&f_y;&a_kh; &a_b;&a_s;&a_t;&a_n;</trd>,
    <trd>&a_m;&a_n;&a_j;&a_m;&a_d; &a_sh;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>glacer</ind></trd>,
    <trd>geler<klr tip="ind"> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="id">mem<ind>beku</ind></trd>
  <trd lng="nl">bevriezen</trd>
  <trd lng="pt">congelar-se</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_l;&c_je;&c_d;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_y;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&#x43C;&c_je;&c_r;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43A;&c_r;&c_i;&c_zh;&c_a;&#x43D;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_i;&c_s;&#x44F; &#x43A;&c_r;&c_i;&c_g;&#x43E;&#x44E;/&#x43B;&#x44C;&#x43E;&c_d;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>

