<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>glaci</rad>/o
</kap>
<drv mrk="glaci.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Akvo malfluidigita, solidigita per malvarmo:
      <ekz>
        en la printempo la <tld/>o kaj la ne&gcirc;o fluidi&gcirc;as
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
      </ekz>
      <ekz>
        la rivero forrulas <tld/>on;
      </ekz>
      <ekz>
        artefarita <tld/>o;
      </ekz>
      <ekz>
        konstrui kastelon sur <tld/>o
        <klr>(<ref tip="vid"
        cel="aer.0o">aero</ref>)</klr><fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;ia alveno disrompis la <tld/>on
        <klr>(&ccirc;esigis la inter&gcirc;enatecon)</klr>
        de la &gcirc;isnuna rigida konversacio<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mielo sur lango kaj <tld/>o <klr>(indiferenteco)</klr> en
        koro<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="frost.0o">frosto</ref>
  </snc>
  <trd lng="de">Eis</trd>
  <trd lng="nl">ijs</trd>
  <trd lng="pl">l&oacute;d</trd>
  <trd lng="en">ice</trd>
  <trd lng="es">hielo</trd>
  <trd lng="fr">glace<klr tip="ind"> (eau gel&eacute;e)</klr></trd>
  <trd lng="pt">gelo</trd>
  <trd lng="ru">&c_l;&c_jo;&c_d;</trd>
  <trd lng="sv">is</trd>
  <trd lng="tr">buz</trd>
</drv>

<drv mrk="glaci.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        <tld/>a praepoko;
      </ekz>
      <ekz>
        <tld/>aj maroj.
      </ekz>
    </dif>
    <trd lng="de">Eis-</trd>
    <trd lng="nl">ijs-</trd>
    <trd lng="pt">glacial</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_je;&c_d;&c_ja;&c_n;&c_o;&c_j;</trd>,
      <trd>&c_l;&c_je;&c_d;&c_o;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Malvarmega:
      <ekz>
        <tld/>a vento;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a <klr>(malardega, malpasiega)</klr> viza&gcirc;o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a <klr>(malfavorega, malafablega)</klr> akcepto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>a tavolo da anta&ubreve;ju&gcirc;oj de la mondo estas rompita
        <fnt>(<aut>Zamenhof</aut>, <vrk>Parolo anta&ubreve; la Tria
        Kongreso Esperantista en Cambridge, 1907</vrk>)</fnt>.
      </ekz>
    </dif>
    <trd lng="de">eisig</trd>
    <trd lng="nl">ijskoud</trd>
    <trd lng="pt">g&eacute;lido</trd>
    <trd lng="ru">&c_l;&c_je;&c_d;&c_ja;&c_n;&c_o;&c_j;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>glac&eacute;<klr tip="ind"> (froid)</klr></trd>,
    <trd>glaciaire</trd>,
    <trd>glacial</trd>
  </trdgrp>
  <trd lng="nl">ijzig</trd>
</drv>

<drv mrk="glaci.0aro">
  <kap><tld/>aro, <tld/>ejo</kap>
  <snc mrk="glaci.0aro.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Konstanta amaso da <tld/>o en montaroj.
    </dif>
  </snc>
  <trd lng="de">Gletscher</trd>
  <trd lng="es">glaciar</trd>
  <trd lng="fr">glacier<klr tip="ind"> (g&eacute;og.)</klr></trd>
  <trd lng="nl">gletsjer</trd>
  <trd lng="pt">geleira</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_d;&c_n;&c_i;&c_k; <klr>(&c_v; &c_g;&c_o;&c_r;&c_a;&c_h;)</klr></trd>,
    <trd>&c_g;&c_l;&c_je;&c_t;&c_ch;&c_je;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="glaci.0ujo0sxranko0tenejo">
  <kap><tld/>ujo, <tld/>&scirc;ranko, <tld/>tenejo</kap>
  <snc>
    <dif>
      Kesto a&ubreve; ejo destinita por konservi <tld/>on.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="frid.0ujo">fridujo</ref>,
    <ref cel="frost.0ofako">frostofako</ref>,
    <ref cel="frost.0ujo">frostujo</ref>.
  </refgrp>
  <trd lng="de">Eisschrank</trd>
  <trd lng="fr">glaci&egrave;re</trd>
  <trd lng="es">hielera</trd>
  <trd lng="nl">ijskast</trd>
  <trd lng="pt">balde de gelo</trd>
  <trd lng="ru">&c_l;&c_je;&c_d;&c_n;&c_i;&c_k; <klr>(&c_ja;&c_shch;&c_i;&c_k; &c_s;&c_o; &c_l;&c_mol;&c_d;&c_o;&c_m;)</klr></trd>
  <trd lng="tr">buz dolab&inodot;</trd>
</drv>

<drv mrk="glaci.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <snc mrk="glaci.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Franda&jcirc;o farita el sukero kaj <tld/>igita kremo kun
      bongustaj aldona&jcirc;oj:
      <ekz>
        <tld/>a&jcirc;o estas dol&ccirc;a glaciigita franda&jcirc;o
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz>
      <ekz>
        vanila, &ccirc;okolada <tld/>a&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Eis</trd>,
    <trd>Speiseeis</trd>
  </trdgrp>
  <trd lng="fr">glace<klr tip="ind"> (aliment)</klr></trd>
  <trd lng="es">helado</trd>
  <trd lng="pl">lody</trd>
  <trd lng="nl">ijsroom</trd>
  <trd lng="pt">sorvete</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_zh;&c_je;&c_n;&c_o;&c_je;</trd>
  <trd lng="sv">glass</trd>
  <trd lng="tr">dondurma</trd>
</drv>

<drv mrk="glaci.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <snc>
    <dif>
      Malfluecigi, solidigi flua&jcirc;on per malvarmo:
      <ekz>
        la frosto <tld/>igis la lagon.
      </ekz>
    </dif>
    <trd lng="tr">dondurmak</trd>
    <trd lng="pt">congelar</trd>
    <trd lng="ru">&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol; &c_l;&c_mol;&c_d;&c_o;&c_m;</trd>
  </snc>
  <snc>
    <dif>
      Meti en malvarmegan staton:
      <ekz>
        la ekstera frosto <tld/>igis min, miajn fingrojn;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tute <tld/>igita de teruro.
      </ekz>
    </dif>
    <trdgrp lng="pt">
      <trd>congelar</trd>,
      <trd>gelar <klr>(de terror)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_a;&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="de">vereisen</trd>
  <trd lng="es">congelar</trd>
  <trdgrp lng="fr">
    <trd>glacer<klr tip="ind"> (transformer en glace)</klr></trd>,
    <trd>geler<klr tip="ind"> (tr.)</klr></trd>
  </trdgrp>
  <trd lng="nl">(doen) <ind>bevriezen</ind></trd>
</drv>

<drv mrk="glaci.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Malfluecigi pro malvarmo:
      <ekz>
        la rivero <tld/>i&gcirc;as.
      </ekz>
    </dif>
    <trd lng="tr">donmak</trd>
  </snc>
  <snc>
    <dif>
      Veni en malvarmegan staton:
      <ekz>
        la soldatoj <tld/>i&gcirc;is dum la nokta gardostaro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la rideto <tld/>i&gcirc;is <klr>(haltis, malaperis)</klr>
        sur &scirc;iaj lipoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i&gcirc;i de timo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">vereisen</trd>
  <trd lng="es">congelarse</trd>
  <trdgrp lng="fr">
    <trd>se <ind>glacer</ind></trd>,
    <trd>geler<klr tip="ind"> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="nl">bevriezen</trd>
  <trd lng="pt">congelar-se</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_l;&c_je;&c_d;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_y;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="glaci.0pluvo">
  <kap><tld/>pluvo</kap>
  <snc mrk="glaci.0pluvo.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      Pluvo, kiu glitige <tld/>i&gcirc;as tu&scirc;ante la teron.
    </dif>
    <refgrp tip="vid">
      <ref cel="hajl.0o">hajlo</ref>,
      <ref cel="negx.0o">ne&gcirc;o</ref>,
      <ref cel="veter">glitvetero</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Eisregen</trd>
  <trd lng="es">granizo</trd>
  <trd lng="fr">pluie vergla&ccedil;ante</trd>
  <trd lng="nl">sneeuwwater</trd>
  <trd lng="pt">chuva de gelo</trd>
</drv>

<drv mrk="glaci.0insulo">
  <kap><tld/>insulo, <tld/>monto</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="glaci.0insulo.GEOG">
    <dif>
      Pecego da <tld/>o libere flosanta en polusaj kaj apudpolusaj maroj.
    </dif>
    <refgrp tip="vid">
      <ref cel="bankiz.0o">bankizo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Eisberg</trd>
  <trd lng="es">t&eacute;mpano</trd>
  <trd lng="fr">iceberg</trd>
  <trd lng="nl">ijsberg</trd>
  <trd lng="pl">g&oacute;ra lodowa</trd>
  <trdgrp lng="pt">
    <trd>iceberg</trd>,<trd>aicebergue</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_j;&c_s;&c_b;&c_je;&c_r;&c_g;</trd>
</drv>

<drv mrk="glaci.0umi">
  <kap><tld/>umi</kap>
  <snc>
    <ref tip="dif" cel="glace.0igi">glaceigi</ref>
  </snc>
  <trd lng="fr">glacer<klr tip="ind"> (recouvrir d'un enduit
  brillant)</klr></trd>
  <trd lng="nl">glaceren</trd>
  <trd lng="ru">&c_g;&c_l;&c_ja;&c_n;&c_c;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="glaci.0rompilo">
  <kap><tld/>rompilo</kap>
  <snc mrk="glaci.0rompilo.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="sxip.0o">&Scirc;ipo</ref> speciale ekipita por rompi
      la <tld/>on.
    </dif>
    <trd lng="de">Eisbrecher</trd>
  </snc>
  <snc mrk="glaci.0rompilo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Ludo por interkonatigi homojn en nova grupo.
    </dif>
  </snc>
  <trd lng="de">Eisbrecher</trd>
  <trd lng="es">rompehielos</trd>
  <trd lng="fr">brise-glace</trd>
  <trd lng="nl">ijsbreker</trd>
  <trd lng="pl">lodo&lstroke;amacz</trd>
  <trd lng="pt">quebra-gelo</trd>
  <trd lng="ru">&c_l;&c_je;&c_d;&c_o;&c_k;&c_o;&c_l;</trd>
  <trd lng="sv">isbrytare</trd>
</drv>
</art>
