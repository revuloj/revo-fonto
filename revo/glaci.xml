<drv mrk="glaci.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        la <tld/>a kovro krakis
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a groto, kiun admiras &ccirc;iuj vizitantoj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj kristaloj poluritaj de frosto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la Hejdelbergulo vivis en la dua <tld/>a periodo, anta&ubreve; proksimume 450 mil jaroj
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Eis-</trd>
    <trdgrp lng="hu">
      <trd>j&eacute;g-</trd>,
      <trd>jeges</trd>
    </trdgrp>
    <trd lng="nl">ijs-</trd>
    <trd lng="pt">glacial</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_je;&c_d;&c_ja;&c_n;&c_o;&c_j;</trd>,
      <trd>&c_l;&c_je;&c_d;&c_o;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      <ref tip="super" cel="varm.mal0a">Malvarmega</ref>:
      <ekz>
        la norda vento <klr>[&#x2026;]</klr> eniris kun vera <tld/>a malvarmo
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne longe eltenas &ccirc;i tiun <tld/>an karceron
        <fnt>
          <bib>VaK</bib>
          <lok>Oka &Ccirc;apitro</lok>
        </fnt>!
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mieno tiel <tld/>a kaj tiel malhumila
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>a tavolo da anta&ubreve;ju&gcirc;oj de la mondo estas rompita
        <fnt>
          <bib>Paroloj</bib>,
          <vrk>Anta&ubreve; la Tria
          Kongreso Esperantista en Cambridge, 1907</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">eisig</trd>
    <trd lng="hu">j&eacute;ghideg</trd>
    <trd lng="nl">ijskoud</trd>
    <trd lng="pt">g&eacute;lido</trd>
    <trd lng="ru">&c_l;&c_je;&c_d;&c_ja;&c_n;&c_o;&c_j;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_d;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_l;&c_je;&c_d;&c_z;&c_ja;&c_n;&c_y;</trd>,
    <trd>&c_l;&c_je;&c_d;&c_z;&c_ja;&c_n;&c_ib;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&t_-a;&t_kha;&t_yata;&t_ga;&t_sa;&t_tsheg;&t_pa;&t_tsheg;&t_cha;&t_ga;&t_sa;&t_tsheg;</trd>
  <trd lng="en">icy</trd>
  <trdgrp lng="fa">
    <trd>&f_y;&a_kh;&f_y;</trd>,
    <trd>&f_y;&a_kh;&#x200C;&f_p;&a_w;&a_sh;&f_y;&a_d;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>glac&eacute;<klr tip="ind"> (froid)</klr></trd>,
    <trd>glaciaire</trd>,
    <trd>glacial</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6C37;&#x306E;  [<ind>&#x3053;&#x304A;&#x308A;&#x306E;</ind>]</trd>,
    <trd>&#x6C37;&#x306E;&#x3088;&#x3046;&#x306A;  [<ind>&#x3053;&#x304A;&#x308A;&#x306E;&#x3088;&#x3046;&#x306A;</ind>]</trd>,
    <trd>&#x51B7;&#x3084;&#x3084;&#x304B;&#x306A;  [<ind>&#x3072;&#x3084;&#x3084;&#x304B;&#x306A;</ind>]</trd>,
    <trd>&#x3088;&#x305D;&#x3088;&#x305D;&#x3057;&#x3044;</trd>
  </trdgrp>
  <trd lng="nl">ijzig</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_mol;&c_o;&c_d;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_mol;&c_o;&c_d;&c_ja;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_zh;&c_a;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_mol;&c_o;&c_d;&c_o;&c_v;&c_i;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_mol;&c_o;&c_d;&c_o;&c_v;&c_i;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_mol;&c_o;&c_d;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_zh;&c_a;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="glaci.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <snc mrk="glaci.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Franda&jcirc;o farita el sukero kaj <tld/>igita kremo kun
      bongustaj aldona&jcirc;oj:
      <ekz>
        <tld/>a&jcirc;o estas dol&ccirc;a glaciigita franda&jcirc;o
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i estis laca de la bala muziko kaj vanila <tld/>a&jcirc;o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[en]</klr> vendejo &leftquot;Veganista&rightquot; <klr>[&#x2026;]</klr> oni ricevas nur veganajn
        <tld/>a&jcirc;o-specojn
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2013/011068.php">Franda&jcirc;oj fratinaj</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_o;&c_z;&c_ib;&c_v;&c_a;</trd>
  <trd lng="bg">&c_s;&c_l;&c_a;&c_d;&c_o;&c_l;&c_je;&c_d;</trd>
