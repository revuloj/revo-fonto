<drv mrk="glaci.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        <tld/>a praepoko;
      </ekz><ekz>
        <tld/>aj maroj.
      </ekz>
    </dif>
    <trd lng="de">Eis-</trd>
    <trdgrp lng="hu">
      <trd>j&eacute;g-</trd>,
      <trd>jeges</trd>
    </trdgrp>
    <trd lng="nl">ijs-</trd>
    <trd lng="pt">glacial</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_je;&c_d;&c_ja;&c_n;&c_o;&c_j;</trd>,
      <trd>&c_l;&c_je;&c_d;&c_o;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Malvarmega:
      <ekz>
        <tld/>a vento;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a <klr>(malardega, malpasiega)</klr> viza&gcirc;o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a <klr>(malfavorega, malafablega)</klr> akcepto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>a tavolo da anta&ubreve;ju&gcirc;oj de la mondo estas rompita
        <fnt>
          <bib>Paroloj</bib>,
          <vrk>Anta&ubreve; la Tria
          Kongreso Esperantista en Cambridge, 1907</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">eisig</trd>
    <trd lng="hu">j&eacute;ghideg</trd>
    <trd lng="nl">ijskoud</trd>
    <trd lng="pt">g&eacute;lido</trd>
    <trd lng="ru">&c_l;&c_je;&c_d;&c_ja;&c_n;&c_o;&c_j;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_d;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_l;&c_je;&c_d;&c_z;&c_ja;&c_n;&c_y;</trd>,
    <trd>&c_l;&c_je;&c_d;&c_z;&c_ja;&c_n;&c_ib;&c_s;&c_t;&c_y;</trd>
  </trdgrp>
  <trd
  lng="bo">&#xF60;&#xF41;&#xFB1;&#xF42;&#xF66;&#xF0B;&#xF54;&#xF0B;&#xF46;&#xF42;&#xF66;&#xF0B;</trd>
  <trd lng="en">icy</trd>
  <trdgrp lng="fa">
    <trd>&f_y;&a_kh;&f_y;</trd>,
    <trd>&f_y;&a_kh;&#8204;&f_p;&a_w;&a_sh;&f_y;&a_d;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>glac&eacute;<klr tip="ind"> (froid)</klr></trd>,
    <trd>glaciaire</trd>,
    <trd>glacial</trd>
  </trdgrp>
  <trd lng="nl">ijzig</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x44C;&#x43E;&c_d;&#x43E;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43B;&#x44C;&#x43E;&c_d;&#x44F;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43A;&c_r;&c_i;&c_zh;&c_a;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43B;&#x44C;&#x43E;&c_d;&#x43E;&c_v;&c_i;&#x43A;&#x43E;&c_v;&c_i;&c_j;</trd>,
    <trd>&#x43B;&#x44C;&#x43E;&c_d;&#x43E;&c_v;&c_i;&c_t;&c_i;&c_j;</trd>,
    <trd>&#x43B;&#x44C;&#x43E;&c_d;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&#x43A;&c_r;&c_i;&c_zh;&c_a;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="glaci.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <snc mrk="glaci.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Franda&jcirc;o farita el sukero kaj <tld/>igita kremo kun
      bongustaj aldona&jcirc;oj:
      <ekz>
        <tld/>a&jcirc;o estas dol&ccirc;a glaciigita franda&jcirc;o
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>;
      </ekz><ekz>
        vanila, &ccirc;okolada <tld/>a&jcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_o;&c_z;&c_ib;&c_v;&c_a;</trd>
  <trd lng="bg">&c_s;&c_l;&c_a;&c_d;&c_o;&c_l;&c_je;&c_d;</trd>
  <trdgrp lng="de">
    <trd>Eiskrem</trd>,
    <trd>Speiseeis</trd>
  </trdgrp>
  <trd lng="en">ice cream</trd>
  <trd lng="es">helado</trd>
  <trd lng="fa">&a_b;&a_s;&a_t;&a_n;&f_y;</trd>
  <trd lng="fr">glace<klr tip="ind"> (aliment)</klr></trd>
  <trd lng="he">&gimel;&lamed;&yod;&dalet;&he;</trd>
  <trd lng="hu">fagylalt</trd>
  <trd lng="id"><ind>es</ind> krim</trd>
  <trd lng="pl">lody</trd>
  <trd lng="nl">ijsroom</trd>
  <trd lng="pt">sorvete</trd>
  <trd lng="ru">&c_m;&c_o;&c_r;&c_o;&c_zh;&c_je;&c_n;&c_o;&c_je;</trd>
  <trd lng="sv">glass</trd>
  <trd lng="tr">dondurma</trd>
  <trd lng="uk">&#x43C;&#x43E;&c_r;&#x43E;&c_z;&c_i;&c_v;&#x43E;</trd>
</drv>

