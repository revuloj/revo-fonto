  <trd lng="he">&gimel;&lamed;&yod;&dalet;&he;</trd>
  <trd lng="hu">fagylalt</trd>
  <trd lng="id"><ind>es</ind> krim</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30A4;&#x30B9;&#x30AF;&#x30EA;&#x30FC;&#x30E0;</trd>,
    <trd>&#x6C37;&#x83D3;  [<ind>&#x3072;&#x3087;&#x3046;&#x304B;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">lody</trd>
