<drv mrk="glaci.0igi">
  <kap><ofc>*</ofc><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Malfluecigi, solidigi flua&jcirc;on per malvarmo:
      <ekz>
        la frosto <tld/>igis la lagon.
      </ekz>
    </dif>
    <trd lng="hu">fagyaszt</trd>
    <trd lng="tr">dondurmak</trd>
    <trd lng="pt">congelar</trd>
    <trd lng="ru">&c_s;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;
      &c_l;&c_mol;&c_d;&c_o;&c_m;</trd>
  </snc>
  <snc>
    <dif>
      Meti en malvarmegan staton:
      <ekz>
        la ekstera frosto <tld/>igis min, miajn fingrojn;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        tute <tld/>igita de teruro.
      </ekz>
    </dif>
    <trd lng="hu">dermeszt</trd>
    <trd lng="pt">congelar</trd>
    <trd lng="ru">&c_z;&c_a;&c_m;&c_o;&c_r;&c_o;&c_z;&c_i;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>mrazit</trd>,
    <trd>nechat zmrznout</trd>,
    <trd>zmrazit</trd>
  </trdgrp>
  <trd lng="be">&c_z;&c_a;&c_m;&c_a;&c_r;&c_o;&c_z;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="de">vereisen</trd>
  <trd lng="en">freeze</trd>
  <trd lng="es">congelar</trd>
  <trd lng="fa">&a_m;&a_n;&a_j;&a_m;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
      <trd>glacer<klr tip="ind"> (transformer en glace)</klr></trd>,
      <trd>geler<klr tip="ind"> (tr.)</klr></trd>
    </trdgrp>
  <trd lng="id">mem<ind>beku</ind>kan</trd>
  <trdgrp lng="ja">
    <trd>&#x51CD;&#x3089;&#x305B;&#x308B;  [<ind>&#x3053;&#x304A;&#x3089;&#x305B;&#x308B;</ind>]</trd>,
    <trd>&#x6C37;&#x7D50;&#x3055;&#x305B;&#x308B;  [<ind>&#x3072;&#x3087;&#x3046;&#x3051;&#x3064;&#x3055;&#x305B;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">(doen) <ind>bevriezen</ind></trd>
  <trd lng="sk">mrazi&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_m;&c_o;&c_r;&c_o;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_k;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i; &c_l;&c_mol;&c_o;&c_d;&c_o;&c_m;/&c_k;&c_r;&c_i;&c_g;&c_o;&c_ju;</trd>
  </trdgrp>
</drv>

<drv mrk="glaci.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Malflueci&gcirc;i pro malvarmo:
      <ekz>
        fari&gcirc;is vintro, kaj la akvoj <tld/>i&gcirc;is
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        miaj &scirc;ipoj en<tld/>i&gcirc;is
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tie estas tiel malvarme, ke la nuboj <tld/>i&gcirc;as en bulojn kaj falas malsupren
        en formo de malgrandaj blankaj &ccirc;ifonetoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Cikonioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        alvenis la tempo, kiam la maro <tld/>i&gcirc;is kaj oni povis piediri sur la <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="https://www.monato.be/2013/011057.php">Nerezisteble altira poluso</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frost.0igxi">frosti&gcirc;i</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>eljegesedik</trd>,
      <trd>j&eacute;gg&eacute; fagy</trd>
    </trdgrp>
    <trd lng="tr">donmak</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Subite <ref cel="rigid.0igxi">rigidi&gcirc;i</ref>, senemocii&gcirc;i pro konsterno ks:
      <ekz>
        la rideto <tld/>i&gcirc;is sur la duone malfermitaj lipoj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam mi pensas pri la malfeli&ccirc;o, <klr>[&#x2026;]</klr> mia koro <tld/>i&gcirc;as
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Vasilij Ivanovi&ccirc;</nom> ektremis kaj <tld/>i&gcirc;is pro teruro
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXVII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konste1.0igxi">konsterni&gcirc;i</ref>,
      <ref cel="sxton.0igxi.FIG">&scirc;toni&gcirc;i<sncref/></ref>,
      <ref cel="stupor.0i">stupori</ref>
    </refgrp>
    <trd lng="hu">megdermed</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_l;&c_je;&c_d;&c_z;&c_ja;&c_n;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_jo;&c_r;&c_z;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">vereisen</trd>
  <trd lng="en">freeze</trd>
  <trd lng="es">congelarse</trd>
  <trdgrp lng="fa">
    <trd>&f_y;&a_kh; &a_b;&a_s;&a_t;&a_n;</trd>,
    <trd>&a_m;&a_n;&a_j;&a_m;&a_d; &a_sh;&a_d;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>se <ind>glacer</ind></trd>,
    <trd>geler<klr tip="ind"> (intr.)</klr></trd>
  </trdgrp>
  <trd lng="id">mem<ind>beku</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x51CD;&#x308B;  [<ind>&#x3053;&#x304A;&#x308B;</ind>]</trd>,
    <trd>&#x6C37;&#x7D50;&#x3059;&#x308B;  [<ind>&#x3072;&#x3087;&#x3046;&#x3051;&#x3064;&#x3059;&#x308B;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">bevriezen</trd>
  <trd lng="pt">congelar-se</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_l;&c_je;&c_d;&c_je;&c_n;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_y;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_m;&c_je;&c_r;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_zh;&c_a;&c_n;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_i;&c_s;&c_ja; &c_k;&c_r;&c_i;&c_g;&c_o;&c_ju;/&c_l;&c_mol;&c_o;&c_d;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

