<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>konspir</rad>/i
</kap>
<drv mrk="konspi.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Komploti en politika afero:
      <ekz>
        <tld/>i por renversi la registaron.
      </ekz>
    </dif>
    <trd lng="de">sich <ind>verschw&ouml;ren</ind></trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Celi al komuna malamika celo:
      <ekz>
        <tld/>is kontra&ubreve; li amikeco, amo kaj socio;
      </ekz>
      <ekz>kontra&ubreve; vesto malbona <tld/>as &ccirc;iu
&scirc;tono<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        la &ccirc;ielo <tld/>as kontra&ubreve; ni<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="de">konspirieren</trd>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol;
<ind>&c_z;&c_m;&c_o;&c_v;&c_u;</ind></trd>
  <trd lng="en">conspire</trd>
  <trd lng="nl">samenzweren</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_s;&c_t;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;
&c_z;&c_a;&c_g;&c_o;&c_v;&c_o;&c_r;</trd>,
    <trd>&c_z;&c_a;&c_m;&c_y;&c_sh;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">konspirera</trd>
</drv>

<drv mrk="konspi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Politika komploto:
      <ekz>
        fari <tld/>on;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        aran&gcirc;i kontra&ubreve; iu a&ubreve; io la <tld/>on de silento
        <klr>(ignori kaj intence tute prisilenti)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        disrompi la <tld/>on de silento.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_m;&c_o;&c_v;&c_a;</trd>
  <trdgrp lng="de">
     <trd>Verschw&ouml;rung</trd>,
     <trd>Konspiration</trd>
  </trdgrp>
  <trd lng="nl">samenzwering</trd>
  <trd lng="sv">konspiration</trd>
</drv>
</art>
