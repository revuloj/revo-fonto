<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: polu.xml,v 1.30 2024/12/06 20:29:49 revo Exp $">
<kap>
  <rad>polu</rad>/i
  <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="polu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="polu.0i.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Malpurigi vivesencan elementon de la naturo:
      <ekz>
        la urino kaj feka&jcirc;oj de intense breditaj bestoj <tld/>as
        riverojn
        <fnt>
        <aut>K. Ungar</aut>,
        <vrk><url ref="&Monato;/1995/004583.php">Vivu vigle kaj
        vegetare!</url></vrk>,
        <bib>Monato</bib>,
        <lok>1995/07</lok>;
        </fnt>
      </ekz>
      <ekz>
        nuklea elektra energio <klr>[&#8230;]</klr> ne uzas fosilian brula&jcirc;on, kaj tiel ne
        <tld/>as la medion per forcej-efikaj gasoj
        <fnt>
          <bib>Kon11</bib>
          <lok>Nuklea energio &ndash; La necesa risko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la angla lingvo ne nur dan&gcirc;ere, detrue <tld/>as &ccirc;iujn lingvojn de nia planedo,
        sed mem dialekti&gcirc;is en 38 anglajn apartajn nuancojn ene de 8 &ccirc;efaj lingvaj
        grupoj
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_r;&c_u;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">zne&ccaron;i&scaron;&tcaron;ovat &zcaron;ivot. prost&rcaron;.</trd>
  <trd lng="de">verschmutzen</trd>
  <trd lng="en">pollute</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_l;&a_w;&a_d;&a_n;</trd>,
    <trd>&a_A_madda;&a_l;&a_w;&a_d;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">polluer</trd>
  <trd lng="hu">szennyez <klr>(k&ouml;rnyezetet)</klr></trd>
  <trd lng="ja">&#27738;&#26579;&#12377;&#12427;  <pr>&#12362;&#12379;&#12435;&#12377;&#12427;</pr></trd>
  <trdgrp lng="nl">
    <trd>verontreinigen</trd>,
    <trd>vervuilen</trd>
  </trdgrp>
  <trd lng="pl">zanieczyszcza&cacute;</trd>
  <trd lng="pt">poluir</trd>
  <trd lng="ro">polua</trd>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_r;&c_ja;&c_z;&c_n;&c_ja;&c_t;&c_mol; <klr>(&c_o;&c_k;&c_r;&c_u;&c_zh;&c_a;&c_ju;&c_shch;&c_u;&c_ju; &c_s;&c_r;&c_je;&c_d;&c_u;)</klr></trd>
  <trd lng="sk">zne&ccaron;is&tcaron;ova&tcaron; &zcaron;ivotn&eacute; prostredie</trd>
  <trd lng="tok">jaki</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_b;&c_r;&c_u;&c_d;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_je;&c_ch;&c_i;&c_shch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_u;&c_ju;&c_v;&c_a;&c_t;&c_i; (&c_d;&c_o;&c_v;&c_k;&c_ib;&c_l;&c_l;&c_ja;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#27745;&#26579; <pr>w&umacron;r&#462;n</pr></trd>,
    <trd>&#24324;&#33039; <pr>n&ograve;ngz&amacron;ng</pr></trd>,
    <trd>&#24324;&#39634; <pr>n&ograve;ngz&amacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="polu.0o">
  <kap><tld/>o</kap>
  <snc mrk="polu.0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <tld lit="P"/>ado kaj la rezulto de tiu ago:
      <ekz>
        la vintra hejtado seriozigis la <tld/>on en Pekino
        <fnt>
          <aut>A. Liu</aut>
          <vrk><url ref="http://www.monato.be/2013/010988.php">Tra densa mallumo ...</url></vrk>
          <lok>Monato, 2013/05, p. 9a</lok>
        </fnt>;
      </ekz>
      <ekz>
        konstruado de elektrocentralo estas bazita sur pluraj studoj pri riskoj, kostoj,
        <tld/>o
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Verschmutzung</trd>
  <trd lng="fr">pollution</trd>
</drv>

<drv mrk="polu.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="polu.0ado.EKOL">
    <dif>
      La ago <tld/>i;
      <ref tip="sin" cel="poluci.0o.medi">polucio</ref>:
      <ekz>
        la poleno informas pri la atmosfera <tld/>ado
        <fnt>
        <aut>R. Roze</aut>,
        <vrk><url ref="&Monato;/1997/005775.html">Vivisekcio de
        polenero</url></vrk>,
        <bib>Monato</bib>,
        <lok>1997/11</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i rifuzas voja&gcirc;i per aviadilo pro la grandega <tld/>ado ka&ubreve;zata de &ccirc;i tiu
        transportilo
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_r;&c_u;&c_d;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Verschmutzung</trd>
  <trd lng="en">pollution</trd>
  <trd lng="fa">&a_A_madda;&a_l;&a_w;&a_d;&a_h;&zwnj;&a_s;&a_A;&a_z;&f_y;</trd>
  <trd lng="fr">pollution</trd>
  <trd lng="hu">k&ouml;rnyezetszennyez&eacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#27738;&#26579;  <pr>&#12362;&#12379;&#12435;</pr></trd>,
    <trd>&#29872;&#22659;&#27738;&#26579;  <pr>&#12363;&#12435;&#12365;&#12423;&#12358;&#12362;&#12379;&#12435;</pr></trd>,
    <trd>&#20844;&#23475;  <pr>&#12371;&#12358;&#12364;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vervuiling</trd>,
    <trd>pollutie</trd>,
    <trd>verontreiniging</trd>
  </trdgrp>
  <trd lng="pl">zanieczyszczenie &sacute;rodowiska</trd>
  <trd lng="ro">poluare</trd>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_r;&c_ja;&c_z;&c_n;&c_je;&c_n;&c_i;&c_je; &c_o;&c_k;&c_r;&c_u;&c_zh;&c_a;&c_ju;&c_shch;&c_je;&c_j; &c_s;&c_r;&c_je;&c_d;&c_y;</trd>
  <trd lng="zh">&#27745;&#26579; <pr>w&umacron;r&#462;n</pr></trd>
</drv>

<drv mrk="polu.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc mrk="polu.0ajxo.EKOL">
    <dif>
      <tld lit="P"/>a substanco:
      <ekz>
        aeraj <tld/>a&jcirc;oj
        <fnt>
          <aut>Isikawa T.</aut>
          <vrk><url ref="&Monato;/2013/011050.php?p">Monto Fu&jcirc;i</url></vrk>
          <lok>Monato, 2013/08-09, p. 17a</lok>
        </fnt>;
      </ekz>
      <ekz>
        submara tubego, 47 kilometrojn longa, portas la <tld/>a&jcirc;ojn en la foson
        <fnt>
          <bib>Monato</bib>
          <aut>Ren&eacute;e Triolle</aut>
          <vrk><url ref="&Monato;/2015/011584.php"
              >Ru&gcirc;aj &scirc;limoj en Mediteraneo</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_r;&c_u;&c_d;&c_zh;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_je; &c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;</trd>
  <trd lng="de">Schadstoff</trd>
  <trd lng="fr">polluant<klr> (subst.)</klr></trd>
  <trd lng="ja">&#29872;&#22659;&#27738;&#26579;&#29289;&#36074;
  <pr>&#12363;&#12435;&#12365;&#12423;&#12358;&#12362;&#12379;&#12435;&#12406;&#12387;&#12375;&#12388;</pr></trd>
  <trd lng="zh">&#27745;&#26579;&#29289; <pr>w&umacron;r&#462;nw&ugrave;</pr></trd>
</drv>

<drv mrk="polu.aer0o">
  <kap>aer<tld/>o</kap>
  <snc mrk="polu.aer0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="super" cel="polu.0o.EKOL"><tld lit="P"/>o</ref> de aero pro malsanigaj a&ubreve;
      malbonefikaj gasoj a&ubreve; partikloj:
      <ekz>
        la sanagentejo de UN raportis, ke la klimata krizo anka&ubreve; estas sankrizo,
        en kiu aer<tld/>o mortigis sep milionojn da homoj &ccirc;iujare
        <fnt>
          <aut>Lucia</aut>
          <vrk><url ref="https://esperanto.cri.cn/news/3037/20220408/748150.html">
          MOS vokis mildigi klimatan krizon por sanprotekto</url></vrk>
          <lok>&Ccirc;ina Radio Internacia, 2022-04-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Luftverschmutzung</trd>
  <trd lng="fr">pollution de l'air</trd>
</drv>

<drv mrk="polu.akvo0o">
  <kap>akvo<tld/>o</kap>
  <snc mrk="polu.akvo0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="super" cel="polu.0o.EKOL"><tld lit="P"/>o</ref>
      de natura akvejo pro enmikso de dan&gcirc;eraj &hcirc;emia&jcirc;oj, varmigo, properigo de bakterioj...:
      <ekz>
        multaj kritikis lian registaron pro fu&scirc;oj pri manko de lo&gcirc;ejoj, kresko de malri&ccirc;eco, plii&gcirc;o
        de akvo<tld/>o
        <fnt>
          <aut>D. E. Rogers</aut>
          <vrk><url ref="http://www.monato.be/2018/012165.php">
          Parlamentaj elektoj</url></vrk>
          <lok>Monato, 2018/01, p. 8a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Wasserverschmutzung</trd>
  <trd lng="fr">pollution de l'eau</trd>
</drv>

<drv mrk="polu.bru0o">
  <kap>bru<tld/>o</kap>
  <snc mrk="polu.bru0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="super" cel="polu.0o.EKOL"><tld lit="P"/>o</ref> pro tro multaj, tro konstantaj, tro
      la&ubreve;taj bruoj:
      <ekz>
        la nombro <klr>[de &gcirc;ibaj balenoj]</klr> rekreskis &gcirc;is proksimume 135 000,
        sed impliki&gcirc;oj en fi&scirc;retoj, kolizioj kun &scirc;ipoj, kaj bru<tld/>o da&ubreve;re ka&ubreve;zas vundojn kaj
        mortojn
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://eo.wikipedia.org/wiki/%C4%9Ciba_baleno">
          &Gcirc;iba baleno</url></vrk>
          <lok>Vikipedio, 2023</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">L&auml;rmbelastung</trd>
  <trd lng="fr">pollution sonore</trd>
</drv>

<drv mrk="polu.grund0o">
  <kap>grund<tld/>o</kap>
  <snc mrk="polu.grund0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="super" cel="polu.0o.EKOL"><tld lit="P"/>o</ref>
      de grundo pro enigo de dan&gcirc;eraj substancoj:
      <ekz>
        la sekvantaj aferoj estas neetikaj: aer-, akvo-, kaj grund<tld/>o
        <fnt>
          <aut>M. Wittenberg</aut>
          <vrk><url ref="https://esperanto-ondo.ru/88-lode.htm">Ne malutilu al la
          vivmedio!</url></vrk>
          <lok>La Ondo de Esperanto, 2002:2</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Bodenbelastung</trd>,
    <trd>Bodenverunreinigung</trd>
  </trdgrp>
  <trd lng="fr">pollution des sols</trd>
</drv>

<drv mrk="polu.lum0o">
  <kap>lum<tld/>o</kap>
  <snc mrk="polu.lum0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="super" cel="polu.0o.EKOL"><tld lit="P"/>o</ref>
      pro arta lumado dum normale malhela tempo:
      <ekz>
        elektu lokon for de la lum<tld/>o de la urboj; ju pli malhela estas la medio,
        des pli bona estas la videbleco de la steloj
        <fnt>
          <aut>-</aut>
          <vrk><url
          ref="https://www.meteorologiaenred.com/eo/plej-bonaj-lokoj-por-vidi-la-%C4%89ielon-en-Hispanio.html">
          Plej bonaj lokoj por vidi la &ccirc;ielon en Hispanio</url></vrk>
          <lok>[vidita en 2023]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Lichtverschmutzung</trd>
  <trd lng="fr">pollution lumineuse</trd>
</drv>

<drv mrk="polu.mar0o">
  <kap>mar<tld/>o</kap>
  <snc mrk="polu.mar0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="super" cel="polu.0o.EKOL"><tld lit="P"/>o</ref> de la mara medio:
      <ekz>
        je la 12a de decembro 1999, la petrol&scirc;ipo <nom>Erika</nom>, konstruita en 1975,
        rompi&gcirc;is en du partojn en la dan&gcirc;era Biskaja Golfo, &ccirc;. 70 km de la franca marbordo;
        tio ka&ubreve;zis mar<tld/>on je skalo neniam vidita en E&ubreve;ropo
        <fnt>
          <aut>R. Rotsaert</aut>
          <vrk>Piratoj diversaspektaj</vrk>
          <lok>Monato, 2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Meeresverschmutzung</trd>
  <trd lng="fr">pollution marine</trd>
</drv>
</art>
