<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: polu.xml,v 1.26 2024/03/19 09:23:20 revo Exp $">
<kap>
  <rad>polu</rad>/i
  <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="polu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="polu.0i.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Malpurigi vivesencan elementon de la naturo:
      <ekz>
        la urino kaj feka&jcirc;oj de intense breditaj bestoj <tld/>as
riverojn
        <fnt>
        <aut>K. Ungar</aut>,
        <vrk><url ref="&Monato;/1995/004583.php">Vivu vigle kaj
vegetare!</url></vrk>,
        <bib>Monato</bib>,
        <lok>1995/07</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_r;&c_u;&c_d;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">zne&ccaron;i&scaron;&tcaron;ovat &zcaron;ivot. prost&rcaron;.</trd>
  <trd lng="de">verschmutzen</trd>
  <trd lng="en">pollute</trd>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_l;&a_w;&a_d;&a_n;</trd>,
    <trd>&a_A_madda;&a_l;&a_w;&a_d;&a_h; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">polluer</trd>
  <trd lng="hu">szennyez <klr>(k&ouml;rnyezetet)</klr></trd>
  <trd lng="ja">&#x6C5A;&#x67D3;&#12377;&#x308B;  <pr>&#x304A;&#x305B;&#12435;&#12377;&#x308B;</pr></trd>
  <trdgrp lng="nl">
    <trd>verontreinigen</trd>,
    <trd>vervuilen</trd>
  </trdgrp>
  <trd lng="pl">zanieczyszcza&cacute;</trd>
  <trd lng="pt">poluir</trd>
  <trd lng="ro">polua</trd>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_r;&c_ja;&c_z;&c_n;&c_ja;&c_t;&c_mol; <klr>(&c_o;&c_k;&c_r;&c_u;&c_zh;&c_a;&c_ju;&c_shch;&c_u;&c_ju; &c_s;&c_r;&c_je;&c_d;&c_u;)</klr></trd>
  <trd lng="sk">zne&ccaron;is&tcaron;ova&tcaron; &zcaron;ivotn&eacute; prostredie</trd>
  <trd lng="tok">jaki</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_b;&c_r;&c_u;&c_d;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_je;&c_ch;&c_i;&c_shch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_t;&c_r;&c_u;&c_ju;&c_v;&c_a;&c_t;&c_i; (&c_d;&c_o;&c_v;&c_k;&c_ib;&c_l;&c_l;&c_ja;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C61;&#x67D3; <pr>w&#x16B;r&#x1CE;n</pr></trd>,
    <trd>&#x5F04;&#x810F; <pr>n&#xF2;ngz&amacron;ng</pr></trd>,
    <trd>&#x5F04;&#x9AD2; <pr>n&#xF2;ngz&amacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="polu.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="polu.0ado.EKOL">
    <dif>
      La ago <tld/>i;
      <ref tip="sin" cel="poluci.0o.medi">polucio</ref>:
      <ekz>
        la poleno informas pri la atmosfera <tld/>ado
        <fnt>
        <aut>R. Roze</aut>,
        <vrk><url ref="&Monato;/1997/005775.html">Vivisekcio de
polenero</url></vrk>,
        <bib>Monato</bib>,
        <lok>1997/11</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_r;&c_u;&c_d;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Verschmutzung</trd>
  <trd lng="en">pollution</trd>
  <trd lng="fa">&a_A_madda;&a_l;&a_w;&a_d;&a_h;&#8204;&a_s;&a_A;&a_z;&f_y;</trd>
  <trd lng="fr">pollution</trd>
  <trd lng="hu">k&ouml;rnyezetszennyez&eacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#x6C5A;&#x67D3;  <pr>&#x304A;&#x305B;&#12435;</pr></trd>,
    <trd>&#x74B0;&#22659;&#x6C5A;&#x67D3;  <pr>&#x304B;&#12435;&#x304D;&#12423;&#12358;&#x304A;&#x305B;&#12435;</pr></trd>,
    <trd>&#x516C;&#x5BB3;  <pr>&#12371;&#12358;&#x304C;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>vervuiling</trd>,
    <trd>pollutie</trd>,
    <trd>verontreiniging</trd>
  </trdgrp>
  <trd lng="pl">zanieczyszczenie &sacute;rodowiska</trd>
  <trd lng="ro">poluare</trd>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_r;&c_ja;&c_z;&c_n;&c_je;&c_n;&c_i;&c_je; &c_o;&c_k;&c_r;&c_u;&c_zh;&c_a;&c_ju;&c_shch;&c_je;&c_j; &c_s;&c_r;&c_je;&c_d;&c_y;</trd>
  <trd lng="zh">&#x6C61;&#x67D3; <pr>w&#x16B;r&#x1CE;n</pr></trd>
</drv>

<drv mrk="polu.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <snc mrk="polu.0ajxo.EKOL">
    <dif>
      <tld lit="P"/>a substanco:
      <ekz>
        aeraj <tld/>a&jcirc;oj
        <fnt>
          <aut>Isikawa T.</aut>
          <vrk><url ref="&Monato;/2013/011050.php?p">Monto Fu&jcirc;i</url></vrk>
          <lok>Monato, 2013-08 &gcirc;is 09, p. 17a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_b;&c_r;&c_u;&c_d;&c_zh;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_je; &c_r;&c_e;&c_ch;&c_y;&c_v;&c_a;</trd>
  <trd lng="fr">polluant<klr> (subst.)</klr></trd>
  <trd lng="ja">&#x74B0;&#22659;&#x6C5A;&#x67D3;&#29289;&#x8CEA; 
  <pr>&#x304B;&#12435;&#x304D;&#12423;&#12358;&#x304A;&#x305B;&#12435;&#12406;&#12387;&#12375;&#12388;</pr></trd>
  <trd lng="zh">&#x6C61;&#x67D3;&#29289; <pr>w&#x16B;r&#x1CE;nw&#xF9;</pr></trd>
</drv>
</art>
