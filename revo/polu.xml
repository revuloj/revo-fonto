<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>polu</rad>/i
  <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="polu.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKOL</uzo>
  <snc>
    <dif>
      Malpurigi vivesencan elementon de la naturo:
      <ekz>
        la urino kaj feka&jcirc;oj de intense breditaj bestoj <tld/>as riverojn
        <fnt><aut>Krys Ungar</aut>:
        <vrk><url ref="&Monato;/004583.html">Vivu vigle kaj vegetare!</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 07/1995</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verschmutzen</trd>
  <trd lng="en">pollute</trd>
  <trdgrp lng="nl">
    <trd>verontreinigen</trd>,
    <trd>vervuilen</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_r;&c_ja;&c_z;&c_n;&c_ja;&c_t;&c_mol; <klr>(&c_o;&c_k;&c_r;&c_u;&c_zh;&c_a;&c_ju;&c_shch;&c_u;&c_ju; &c_s;&c_r;&c_je;&c_d;&c_u;)</klr></trd>
</drv>

<drv mrk="polu.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      La ago <tld/>i:
      <ekz>
        la poleno informas pri la atmosfera <tld/>ado
        <fnt><aut>Robert Roze</aut>:
        <vrk><url ref="&Monato;/005775.html">Vivisekcio de polenero</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 11/1997</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Verschmutzung</trd>
  <trd lng="en">pollution</trd>
  <trd lng="fr">pollution</trd>
  <trdgrp lng="nl">
    <trd>vervuiling</trd>,
    <trd>pollutie</trd>,
    <trd>verontreiniging</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_r;&c_ja;&c_z;&c_n;&c_je;&c_n;&c_i;&c_je; &c_o;&c_k;&c_r;&c_u;&c_zh;&c_a;&c_ju;&c_shch;&c_je;&c_j; &c_s;&c_r;&c_je;&c_d;&c_y;</trd>
</drv>
</art>
