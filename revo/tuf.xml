<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tuf</rad>/o
</kap>
<drv mrk="tuf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Densa aro da maldikaj kunkreskantaj a&jcirc;oj:
      <ekz>
        <tld/>o da haroj, herboj, floroj, plumoj;
      </ekz>
      <ekz>
        de malbona &scirc;afo estas bona e&ccirc; <tld/>o.
        (<ref tip="vid" cel="sxuld.0o">&scirc;uldo</ref>)
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="buked.0o">bukedo</ref>,
      <ref cel="fask.0o">fasko</ref>,
      <ref cel="garb.0o">garbo</ref>,
      <ref cel="kvast.0o">kvasto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_m;&c_a;</trd>,
    <trd>&c_ch;&c_u;&c_b;</trd>,
    <trd>&c_ch;&c_u;&c_p;&c_r;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_h;&c_o;&c_r;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_m;&c_y;&c_k;</trd>,
    <trd>&c_zh;&c_m;&c_u;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>floc</trd>,
    <trd>borrall&oacute;</trd>,
    <trd>manyoc</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chomáč</trd>,
    <trd>chuchvalec</trd>
  </trdgrp>
  <trd lng="de">B&uuml;schel</trd>
  <trdgrp lng="en">
    <trd>tuft</trd>,
    <trd>wisp</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>plumet</trd>,
    <trd>touffe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csom&oacute;<klr> (nyal&aacute;b)</klr></trd>,
    <trd>nyal&aacute;b</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bosje</trd>,
    <trd>plok</trd>,
    <trd>dot</trd>,
    <trd>kuifje</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>k&eogonek;pka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_ja;&c_d;&c_mol;</trd>,
    <trd>&c_h;&c_o;&c_h;&c_o;&c_l;</trd>,
    <trd>&c_ch;&c_u;&c_b;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_o;&c_r;</trd>,
    <trd>&c_k;&c_l;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chuchvalec</trd>,
    <trd>chumáč</trd>
  </trdgrp>
</drv>

<drv mrk="tuf.har0o">
  <kap>har<tld/>o</kap>
  <snc>
    <dif>
      Kvasteto da elstaraj haroj:
      <ekz>
        havante du har<tld/>ojn sur la kapo
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 63a, p.
        372a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">cua de cavall</trd>
  <trdgrp lng="cs">
    <trd>chomáč vlasů</trd>,
    <trd>kštice</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>forelock</trd>,
    <trd>lock of hair</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>couette<klr> (de cheveux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;st&ouml;k</trd>,
    <trd>hajcsom&oacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kosmyk</trd>,
    <trd>czupryna</trd>,
    <trd>czub</trd>,
    <trd>kuku-ryku<klr> (na w&lstroke;osach)</klr></trd>,
    <trd>wicherek<klr> (na w&lstroke;osach)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chumáč vlasov</trd>,
    <trd>štica</trd>
  </trdgrp>
</drv>

<drv mrk="tuf.plum0o">
  <kap>plum<tld/>o</kap>
  <snc>
    <ref tip="dif" cel="egret.0o">egreto</ref>
  </snc>
  <trd lng="ca">plomall</trd>
  <trdgrp lng="cs">
    <trd>péřový chochol</trd>,
    <trd>péřový hřeben</trd>
  </trdgrp>
  <trd lng="de">Federkrone</trd>
  <trdgrp lng="en">
    <trd>crest</trd>
  </trdgrp>
  <trd lng="fr">aigrette<klr tip="ind"> (touffe de plumes)</klr></trd>
  <trd lng="hu">b&oacute;bita</trd>
  <trdgrp lng="pl">
    <trd>pi&oacute;ropusz</trd>,
    <trd>czub</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_h;&c_o;&c_l;&c_o;&c_k;<klr tip="ind"> (&c_i;&c_z;
    &c_p;&c_je;&c_r;&c_mol;&c_je;&c_v;)</klr></trd>,
    <trd>&c_s;&c_u;&c_l;&c_t;&c_a;&c_n; <klr tip="amb">(&c_i;&c_z;
    &c_p;&c_je;&c_r;&c_mol;&c_je;&c_v;)</klr></trd>
  </trdgrp>
  <trd lng="sk">chochol z peria</trd>
</drv>
</art>
