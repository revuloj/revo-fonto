<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tuf</rad>/o
</kap>
<drv mrk="tuf.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Densa aro da maldikaj kunkreskantaj a&jcirc;oj:
      <ekz>
        <tld/>o da haroj, herboj, floroj, plumoj;
      </ekz>
      <ekz>de malbona &scirc;afo estas bona e&ccirc; <tld/>o.
        (<ref tip="vid" cel="sxuld.0o">&scirc;uldo</ref>)
<fnt><bib>PrV</bib></fnt></ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="buked.0o">bukedo</ref>,
      <ref cel="fask.0o">fasko</ref>,
      <ref cel="garb.0o">garbo</ref>,
      <ref cel="grapol.0o">grapolo</ref>,
      <ref cel="penik.0o">peniko</ref>,
      <ref cel="spik.0o">spiko</ref>,
      <ref cel="kvast.0o">kvasto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">B&uuml;schel</trd>
  <trdgrp lng="fr">
    <trd>plumet</trd>,
    <trd>touffe</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bosje</trd>,
    <trd>plok</trd>,
    <trd>dot</trd>,
    <trd>kuifje</trd>
  </trdgrp>
</drv>

<drv mrk="tuf.plum0o">
  <kap>
    plum<tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="egret.0o">Egreto</ref>.
    </dif>
  </snc>
</drv>


</art>
