<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tuf.xml,v 1.29 2024/05/10 10:43:47 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tuf</rad>/o
</kap>
<drv mrk="tuf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Densa aro da maldikaj kunkreskantaj a&jcirc;oj:
      <ekz>
        <tld/>o da haroj, herboj, floroj, plumoj;
      </ekz>
      <ekz>
        la vento blovis tra liaj blondaj haroj, tiel ke ili levi&gcirc;adis en la formo de
        longaj <tld/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pinto de &ccirc;iu monteto estas kronita de malgranda <tld/>o de grizaj folioj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la malnova komodo <klr>[&#x2026;]</klr> staris granda rezeda <tld/>o
        <klr>(<ref cel="buked.0o">bukedo</ref>)</klr> en glaso
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        sub la <klr>[&#x2026;]</klr> brako &scirc;i havis <tld/>on de sekigitaj plantoj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La tri Magiaj Pordegoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[vi]</klr> vian amikon, kiu pli bone povosciis trinki ol na&gcirc;i, kaptis je la
        <tld/>o kaj tiris sur sekan lokon
        <fnt>
          <bib>RBa</bib>
        </fnt>;
      </ekz>
      <ekz>
        de malbona &scirc;afo estas bona e&ccirc; <tld/>o;
        <klr>(<ref tip="vid" cel="sxuld.0o">&scirc;uldo</ref>)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fask.0o">fasko</ref>,
      <ref cel="garb.0o">garbo</ref>,
      <ref cel="kvast.0o">kvasto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_m;&c_a;</trd>,
    <trd>&c_ch;&c_u;&c_b;</trd>,
    <trd>&c_ch;&c_u;&c_p;&c_r;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_h;&c_o;&c_r;</trd>,
    <trd>&c_k;&c_a;&c_s;&c_m;&c_y;&c_k;</trd>,
    <trd>&c_zh;&c_m;&c_u;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>floc</trd>,
    <trd>borrall&oacute;</trd>,
    <trd>manyoc</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>chom&aacute;&ccaron;</trd>,
    <trd>chuchvalec</trd>
  </trdgrp>
  <trd lng="de">B&uuml;schel</trd>
  <trdgrp lng="en">
    <trd>tuft</trd>,
    <trd>wisp</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>plumet</trd>,
    <trd>touffe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csom&oacute;<klr> (nyal&aacute;b)</klr></trd>,
    <trd>nyal&aacute;b</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x675F;  <pr>&#x305D;&#x304F;</pr></trd>,
    <trd>&#x623F;  <pr>&#x307C;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bosje</trd>,
    <trd>plok</trd>,
    <trd>dot</trd>,
    <trd>kuifje</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>k&eogonek;pka</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_ja;&c_d;&c_mol;</trd>,
    <trd>&c_h;&c_o;&c_h;&c_o;&c_l;</trd>,
    <trd>&c_ch;&c_u;&c_b;</trd>,
    <trd>&c_v;&c_i;&c_h;&c_o;&c_r;</trd>,
    <trd>&c_k;&c_l;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chuchvalec</trd>,
    <trd>chum&aacute;&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&c_s;&#x43C;&#x43E; (&c_v;&#x43E;&#x43B;&#x43E;&c_s;&c_s;&#x44F;)</trd>,
    <trd>&c_ch;&c_u;&c_b;</trd>,
    <trd>&c_ch;&c_u;&#x43F;&c_r;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_h;&#x43E;&c_r;</trd>,
    <trd>&#x43F;&#x43B;&#x44E;&#x43C;&c_a;&c_zh;</trd>,
    <trd>&#x43F;&c_u;&c_ch;&#x43E;&#x43A;</trd>,
    <trd>&c_v;&#x2019;&#x44F;&c_z;&#x43A;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6C02; <pr>m&#xE1;o</pr></trd>,
    <trd>&#x6735; <pr>du&#x1D2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tuf.0eto">
  <kap>
    <tld/>eto
  </kap>
  <snc>
    <dif>
      Malgranda, maldika <tld/>o:
      <ekz>
        <klr>[li]</klr> estis iom fremdaspekta, diketa kun lipharoj kaj tufeto da barbo
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        el &scirc;ia nekombita hararo pendas <tld/>etoj &ccirc;irka&ubreve; &scirc;ia ronda viza&gcirc;o
        <fnt>
        <aut>Laure Patas d'Illiers</aut>
          <vrk><url ref="&Monato;/2018/012290.php">Novelo &dash;
          Sentabaka mateno</url></vrk>
          <bib>Monato</bib>
          <lok>2018/08, p. 36</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="cxarpi.0o">&ccirc;arpio</ref>,
       <ref cel="flok.0o.tufeto">floko<sncref/></ref>,
       <ref cel="vat.0o">vato</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_m;&c_a;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_ch;&c_u;&c_b;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>B&uuml;schelchen</trd>,
    <trd>Fussel</trd>
  </trdgrp>
<trd lng="en">bit of lint</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_u;&c_b;&c_ch;&c_i;&#x43A;</trd>,
    <trd>&#x43F;&c_u;&c_ch;&c_je;&c_ch;&#x43E;&#x43A;</trd>
  </trdgrp>
  <trd lng="zh">&#x76AE;&#x68C9; <pr>p&#xED;mi&#xE1;n</pr></trd>
</drv>

<drv mrk="tuf.har0o">
  <kap>har<tld/>o</kap>
  <snc>
    <dif>
      Densa kuna&jcirc;o de kelkaj elstaraj haroj:
      <ekz>
        havante du har<tld/>ojn sur la kapo
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 63a, p.
        372a</lok></fnt>;
      </ekz>
      <ekz>
        juna viro kun bukla har<tld/>o
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        plurtage ne razita, har<tld/>oj kreskantaj dis kaj mis
        <fnt>
          <bib>ChV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="bukl.0o.hara">buklo<sncref/></ref>,
       <ref cel="har.0ligo">harligo</ref>,
       <ref cel="har.0plektajxo">harplekta&jcirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_u;&c_b;&c_o;&c_k;</trd>,
    <trd>&c_h;&c_o;&c_h;&c_l;&c_ib;&c_k;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_m;&c_a;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">cua de cavall</trd>
  <trdgrp lng="cs">
    <trd>chom&aacute;&ccaron; vlas&uring;</trd>,
    <trd>k&scaron;tice</trd>
  </trdgrp>
  <trd lng="de">Haarb&uuml;schel</trd>
  <trdgrp lng="en">
    <trd>forelock</trd>,
    <trd>lock of hair</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>couette<klr> (de cheveux)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;st&ouml;k</trd>,
    <trd>hajcsom&oacute;</trd>
  </trdgrp>
  <trd lng="ja">&#x6BDB;&#x30FB;&#x9AEA;&#x306E;&#x623F; 
  <pr>&#x3051;&#x30FB;&#x304B;&#x307F;&#x306E;&#x3075;&#x3055;</pr></trd>
  <trdgrp lng="pl">
    <trd>kosmyk</trd>,
    <trd>czupryna</trd>,
    <trd>czub</trd>,
    <trd>kuku-ryku<klr> (na w&lstroke;osach)</klr></trd>,
    <trd>wicherek<klr> (na w&lstroke;osach)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chum&aacute;&ccaron; vlasov</trd>,
    <trd>&scaron;tica</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_u;&c_b;</trd>,
    <trd>&c_ch;&c_u;&c_b;&c_ch;&c_i;&#x43A;</trd>,
    <trd>&c_ch;&c_u;&#x43F;&c_r;&c_i;&#x43D;&c_a;</trd>,
    <trd>&#x43E;&c_s;&c_je;&#x43B;&c_je;&c_d;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7DB9; <pr>li&#x1D4;</pr></trd>,
    <trd>&#x7EFA; <pr>li&#x1D4;</pr></trd>,
    <trd>&#x4E31; <pr>gu&#xE0;n</pr></trd>,
    <trd>&#x6C02; <pr>m&#xE1;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tuf.plum0o">
  <kap>plum<tld/>o</kap>
  <snc>
    <ref tip="dif" cel="egret.0o">egreto</ref>
  </snc>
     <trdgrp lng="be">
      <trd>&c_ch;&c_u;&c_b;&c_o;&c_k;</trd>,
      <trd>&c_s;&c_u;&c_l;&c_t;&c_a;&c_n; <klr tip="amb">(&c_z;&c_mol; &c_p;&c_je;&c_r;'&c_ja;)</klr></trd>
    </trdgrp>
  <trd lng="ca">plomall</trd>
  <trdgrp lng="cs">
    <trd>p&eacute;&rcaron;ov&yacute; chochol</trd>,
    <trd>p&eacute;&rcaron;ov&yacute; h&rcaron;eben</trd>
  </trdgrp>
  <trd lng="de">Federkrone</trd>
  <trdgrp lng="en">
    <trd>crest</trd>
  </trdgrp>
  <trd lng="fr">aigrette<klr tip="ind"> (touffe de plumes)</klr></trd>
  <trd lng="hu">b&oacute;bita</trd>
  <trdgrp lng="ja">
    <trd>&#x7FBD;&#x98FE;  <pr>&#x306F;&#x306D;&#x304B;&#x3056;</pr></trd>,
    <trd>&#x51A0;&#x7FBD;  <pr>&#x304B;&#x3093;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pi&oacute;ropusz</trd>,
    <trd>czub</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_h;&c_o;&c_l;&c_o;&c_k;<klr tip="ind"> (&c_i;&c_z;
    &c_p;&c_je;&c_r;&c_mol;&c_je;&c_v;)</klr></trd>,
    <trd>&c_s;&c_u;&c_l;&c_t;&c_a;&c_n; <klr tip="amb">(&c_i;&c_z;
    &c_p;&c_je;&c_r;&c_mol;&c_je;&c_v;)</klr></trd>
  </trdgrp>
  <trd lng="sk">chochol z peria</trd>
  <trd lng="uk">&c_ch;&c_u;&c_b;&c_ch;&c_i;&#x43A;</trd>
</drv>
</art>
