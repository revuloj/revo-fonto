<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>halogen</rad>/o
</kap>
<drv mrk="haloge.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Gcirc;enerala nomo donata al
      <refgrp tip="sub">
        <ref cel="klor.0o">kloro</ref>,
        <ref cel="brom.0o">bromo</ref>,
        <ref cel="jod.0o">jodo</ref> kaj
        <ref cel="fluor.0o">fluoro</ref>
      </refgrp>
      <klr>(etimologie: &leftquot;salformanto&rightquot;,
      &leftquot;salnaskanto&rightquot;)</klr>.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_ja;&c_g;&c_je;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Halogen</trd>,
    <trd>Salzbildner</trd>
  </trdgrp>
  <trd lng="es">hal&oacute;geno</trd>
  <trd lng="fr">halog&egrave;ne</trd>
  <trdgrp lng="hu">
    <trd>halog&eacute;n</trd>,
    <trd>s&oacute;k&eacute;pz&odblac;</trd>
  </trdgrp>
  <trd lng="nl">halogeen</trd>
  <trd lng="pt">halog&ecirc;nio</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_o;&c_g;&c_je;&c_n;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_o;&c_i;&c_d;</trd>
  </trdgrp>
</drv>
</art>
