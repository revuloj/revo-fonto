<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>halogen</rad>/o
</kap>
<drv mrk="haloge.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Gcirc;enerala nomo donata al kloro, bromo, jodo kaj fluoro
      (= salformanto, salnaskanto).  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Halogen</trd>,
     <trd>Salzbildner</trd></trdgrp>
  <trd lng="nl">halogeen</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_o;&c_g;&c_je;&c_n;</trd>,
    <trd>&c_g;&c_a;&c_l;&c_o;&c_i;&c_d;</trd>
  </trdgrp>
</drv>
</art>
