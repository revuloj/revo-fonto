<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>al</rad>/o
</kap>
<drv mrk="al1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Objekto forme (sed ne funkcie) iom simila al birda flugilo,
      precipe
    </dif>
    <subsnc>
      <dif>
        &Ccirc;iu ekstrema flanka parto de la nazo.
      </dif>
      <trd lng="bg">&c_n;&c_o;&c_z;&c_d;&c_r;&c_a;</trd>
      <trd lng="ca">ala del nas</trd>
      <trd lng="de">Nasenfl&uuml;gel</trd>
      <trd lng="en">nostril</trd>
      <trd lng="it">pinna<klr> (del naso)</klr></trd>
      <trd lng="nl">neusvleugel</trd>
      <trd lng="pt">aba</trd>
    </subsnc>
    <subsnc>
      <dif>
        &Ccirc;iu ekstrema parto de helico, &scirc;ra&ubreve;bo.
      </dif>
      <ref tip="sin" cel="padel.0o">padelo</ref>
      <trd lng="ca">pala, paleta</trd>
      <trd lng="de">Schaufel</trd>
      <trd lng="it">estremit&agrave;</trd>
      <trd lng="pt">p&aacute;</trd>
      <trd lng="ru">&c_l;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>
    </subsnc>
    <subsnc>
      <dif>
        &Ccirc;iu ekstrema flanko de konstrua&jcirc;o:
        <ekz>
          oni elprenis eksplod-tubegon [...], fiksis &gcirc;in al la kverka
          pordego kaj kun surdiga tondro detruis ties &scirc;talplatajn
          <tld/>ojn<fnt><aut>Jozefo Horvath</aut>: <vrk>Gy&odblac;r a&ubreve; kie
          ekkokerikis ventokoko</vrk>, <bib>Monato</bib><lok>jaro 2002a,
          numero 8a, p. 22a</lok></fnt>.
        </ekz>
      </dif>
      <trdgrp lng="ca">
        <trd>ala <klr>(constr.)</klr></trd>
      </trdgrp>
      <trd lng="de">Seitenfl&uuml;gel</trd>
      <trd lng="en">wing</trd>
      <trd lng="es">ala</trd>
      <trdgrp lng="it">
        <trd>ala</trd>, 
        <trd>braccio<klr> (arch.)</klr></trd>
      </trdgrp>
      <trd lng="pt">ala</trd>
    </subsnc>
    <subsnc>
      <dif>
        &Ccirc;iu ekstrema flanko de armeo a&ubreve; partio:
        <ekz>
          nun necesis igi la partion akceptinda [...] per forigo de la
          plej &leftquot;bruna&rightquot; <tld/>o<fnt><aut>Baard Hekland</aut>:
          <vrk>Triumfas
          tedulo</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 11a, p.
          6a</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="ca">flanc (mil.)</trd>
      <trd lng="de">Rand</trd>
      <trd lng="en">branch</trd>
      <trd lng="it">ala</trd>
      <trd lng="pt">ala</trd>
    </subsnc>
    <subsnc>
    <uzo tip="fak">SPO</uzo>
      <dif>
        Vertikala duono de la &scirc;aktabulo.
      </dif>
      <trd lng="de">Fl&uuml;gel</trd>
      <trd lng="en">wing</trd>
      <trdgrp lng="it">
        <trd>ala<klr> (scacchi)</klr></trd>, 
        <trd>fianco<klr> (scacchi)</klr></trd>,
        <trd>lato<klr> (scacchi)</klr></trd>
      </trdgrp>
    </subsnc>
  </snc>
  <ref tip="sin" cel="flug.0ilo.alo">flugilo</ref>
  <rim>
    PV markis la vorton kiel Zamenhofan, en PIV1 tiu atribuo
    malaperis.
  </rim>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_y;&c_l;&c_o;</trd>,
    <trd>&c_l;&c_o;&c_p;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_r;&c_i;&c_l;&c_o;</trd>
  <trd lng="ca">ala</trd>
  <trd lng="de">Fl&uuml;gel</trd>
  <trd lng="fr">aile</trd>
   <trd lng="it">ala</trd>
  <trd lng="nl">vleugel</trd>
  <trd lng="ru">&c_k;&c_r;&c_y;&c_l;&c_o;</trd>
</drv>
