<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>8</ofc>
    <rad>latv</rad>/o
</kap>

<drv mrk="latv.0o">
  <kap><tld/>o <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>,
  <var><kap><tld/>ino</kap></var></kap>
  <snc mrk="latv.0o.ETNO">
    <dif>
      Ano, anino de la <tld/>a etno.
    </dif>
  </snc>
  <snc mrk="latv.0o.ANO">
    <ref tip="dif" cel="latv.0ujano"><tld lit="L"/>ujano, <tld
    lit="L"/>ujanino</ref>
  </snc>
  <trd lng="bg">&c_l;&c_a;&c_t;&c_v;&c_i;&c_je;&c_c;</trd>
  <trdgrp lng="cs">
    <trd>Loty&scaron;</trd>,
    <trd>Loty&scaron;ec</trd>
  </trdgrp>
  <trd lng="de">Lette</trd>
  <trd lng="en">Latvian</trd>
  <trd lng="fr">Letton<klr> (subst.)</klr></trd>
  <trd lng="nl">Let</trd>
  <trd lng="pl">&Lstroke;otysz</trd>
  <trd lng="pt">let&atilde;o<klr tip="amb"> (subs.)</klr></trd>
  <trdgrp lng="sk">
    <trd>Loty&scaron;</trd>,
    <trd>Loty&scaron;,</trd>
  </trdgrp>
</drv>

<drv mrk="latv.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj, ilia etno a&ubreve; &scirc;tato.
    </dif>
   <trd lng="en">Latvian</trd>
   <trdgrp lng="fr"> 
     <trd>letton</trd>
     <trd>lettonne<klr> (adj.)</klr></trd>
   </trdgrp>
  </snc>
  <snc>
    <dif>
      "la latva" = la nacia <ref tip="super"
    cel="lingv.0o">lingvo</ref> de Latvujo
    <ekz><fnt><bib>Viki</bib>, <lok><url 
    ref="http://eo.wikipedia.org/wiki/Latva lingvo">Latva
    lingvo</url></lok></fnt></ekz>.
    </dif>
   <trd lng="en">Latvian<klr> (language)</klr></trd>
   <trd lng="fr">letton<klr> (langue)</klr></trd>
  </snc>  
  <trd lng="bg">&c_l;&c_a;&c_t;&c_v;&c_i;&c_j;&c_s;&c_k;&c_i;</trd>
  <trd lng="cs">loty&scaron;sk&yacute;</trd>
  <trd lng="de">lettisch</trd>
  <trd lng="nl">Lets</trd>
  <trd lng="pl">&lstroke;otewski</trd>
  <trd lng="pt">let&atilde;o<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="sk">loty&scaron;sk&yacute;</trd>
</drv>

<drv mrk="latv.L0ujo">
  <kap><tld lit="L"/>ujo
  (<var><kap><tld lit="L"/>/i/o</kap></var>)
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Latvio">Latvio</url></lok></fnt>
  </kap>
  <snc mrk="latv.L0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      &Scirc;tato situanta en nordoriento de E&ubreve;ropo kaj limanta
      al Balta Maro, Litovujo, Belorusujo, Rusujo kaj Estonujo.
    </dif>
  <ref tip="sin" cel="latvi.L0o">Latvio</ref>
  </snc>
  <rim mrk="latv.L0io.RIM">
  "Gentobazaj landnomoj. Oni ordinare formas la nomon de la lando per la
  <em>Fundamenta sufikso UJ</em> a&ubreve; per la <em>neoficiala sufikso
  I</em>. Pri la karaktero de tiu I sufikso (uzata post popolvortoj
  anstata&ubreve; UJ) ekzistas diversaj opinioj. Iuj rigardas &gcirc;in kiel
  <em>efektivan sufikson</em> (kvankam neoficialan), dum aliaj preferas
  rigardi &gcirc;in kiel <em>eron de aparta landnoma radiko</em> paralela kun
  la responda popolnoma radiko. 
  Tiuj diversaj analizoj &ccirc;iuokaze ne faras praktikan diferencon en la
  uzado de tiaj landnomoj kiel Francio, Rusio, Japanio
  kaj &Ccirc;inio. <em>En tiu &ccirc;i dokumento la Akademio ne alprenas starpunkton
  pri tiu teoria demando</em>" <fnt><aut>Akademio de Esperanto</aut>, 
  <vrk><url
  ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
  Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>.
  </rim>
  <trd lng="bg">&c_L;&c_a;&c_t;&c_v;&c_i;&c_ja;</trd>
  <trd lng="de">Lettland</trd>
  <trd lng="en">Latvia</trd>
  <trdgrp lng="fr">
    <trd>Lettonie</trd>,
    <trd>R&eacute;publique de Lettonie</trd>
  </trdgrp>
  <trd lng="id">Latvia</trd>
  <trd lng="nl">Letland</trd>
  <trd lng="pl">&Lstroke;otwa</trd>
  <trdgrp lng="pt">
    <trd>Let&ocirc;nia</trd>,
    <trd>Let&oacute;nia</trd>
  </trdgrp>
</drv>

<drv mrk="latv.0ujano">
  <kap><tld/>ujano,
  <var><kap><tld/>ujanino</kap></var></kap>
  <snc>
    <dif>
      &Scirc;tatano de <tld lit="L"/>ujo sendepende de etno.
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_a;&c_t;&c_v;&c_i;&c_je;&c_c;</trd>
  <trd lng="de">Lette</trd>
  <trd lng="en">Latvian</trd>
  <trd lng="nl"><ind>Lets</ind> staatsburger</trd>
  <trd lng="pl">&Lstroke;otysz</trd>
  <trd lng="pt">let&atilde;o<klr tip="amb"> (subs.)</klr></trd>
</drv>

<drv mrk="latv.0uja">
  <kap><tld/>uja</kap>
  <snc>
    <dif>
      Rilata al la &scirc;tato <tld lit="L"/>ujo.
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_a;&c_t;&c_v;&c_i;&c_j;&c_s;&c_k;&c_i;</trd>
  <trd lng="de">lettisch</trd>
  <trd lng="en">Latvian</trd>
  <trd lng="nl">Lets<klr tip="ind"> (betr.de staat)</klr></trd>
  <trd lng="pl">&lstroke;otewski</trd>
</drv>

</art>

