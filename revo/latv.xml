<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>latv</rad>/o
</kap>

<drv mrk="latv.0o">
  <kap><tld/>o</kap>
  <snc mrk="latv.0o.ETNO">
    <dif>
      Ano de la <tld/>a etno.
    </dif>
  </snc>
  <snc mrk="latv.0o.ANO">
    <ref tip="dif" cel="latv.0ujano"><tld lit="L"/>ujano</ref>
  </snc>
  <trd lng="pt">let&atilde;o<klr tip="amb"> (subs.)</klr></trd>
</drv>

<drv mrk="latv.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj, ilia etno a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="pt">let&atilde;o<klr tip="amb"> (adj.)</klr></trd>
</drv>

<drv mrk="latv.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="latv.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      &Scirc;tato situanta en nordoriento de E&ubreve;ropo kaj limanta
      al Balta Maro, Litovujo, Belorusujo, Rusujo kaj Estonujo.
    </dif>
  </snc>
  <ref tip="sin" cel="latvi.0o">Latvio</ref>
  <trdgrp lng="pt">
  <trd>Let&ocirc;nia</trd>,
  <trd>Let&oacute;nia</trd>
</trdgrp>
</drv>

<drv mrk="latv.0ujano">
  <kap><tld/>ujano</kap>
  <snc>
    <dif>
      &Scirc;tatano de <tld lit="L"/>ujo sendepende de etno.
    </dif>
  </snc>
  <trd lng="pt">let&atilde;o<klr tip="amb"> (subs.)</klr></trd>
</drv>

<drv mrk="latv.0uja">
  <kap><tld/>uja</kap>
  <snc>
    <dif>
      Rilata al <tld lit="L"/>ujo.
    </dif>
  </snc>
</drv>

</art>

<!--
$Log$
-->
</vortaro>