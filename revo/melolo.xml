<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>melolont</rad>/o
</kap>
<drv mrk="melolo.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="skarab1.0oj">skarabedoj</ref>
      (<trd lng="la">Melolontha</trd>),
      herboman&gcirc;anta koleoptero, tre malutila same kiel &gcirc;ia
      larvo t. n. blanka vermo.
    </dif>
    <ref tip="sin" cel="skarab.maj0o">majskarabo</ref>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_u;&c_sh;&c_ch;</trd>
  <trd lng="de">Maik&auml;fer</trd>
  <trd lng="fr">hanneton</trd>
  <trd lng="hu">cserebog&aacute;r</trd>
  <trd lng="nl">meikever</trd>
  <trdgrp lng="pt">
    <trd>melolonta</trd>,
    <trd>besouro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_j;&c_s;&c_k;&c_i;&c_j; &c_zh;&c_u;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_j;&c_s;&c_k;&c_i;&c_j; &c_h;&c_r;&c_u;&c_shch;</trd>
  </trdgrp>
  <trd lng="sk">chr&uacute;st</trd>
</drv>
</art>
