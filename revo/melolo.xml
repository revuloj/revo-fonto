<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: melolo.xml,v 1.18 2022/03/23 14:01:06 revo Exp $">
<kap>
  <rad>melolont</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="melolo.0o">
  <kap><tld/>o</kap>
  <snc mrk="melolo.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="skarab.maj0o.ZOO">Majskarabo</ref>:
      <ekz>
        li <klr>[&#x2026;]</klr> veturas tra ponto / al la melolonto / &ccirc;i maja skarabo /
        eklo&gcirc;is sur fabo <klr>[&#x2026;]</klr>
        <fnt>
          <aut>Zofia Mirska</aut>
          <vrk><url ref="http://dvd.ikso.net/edukado/lerniloteko/RegxokajSkarabo/PoemoRegxoKajSkarabo20040418.pdf">Re&gcirc;o kaj skarabo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_u;&c_sh;&c_ch;</trd>
  <trdgrp lng="ca">
    <trd>escarabat de maig</trd>,
    <trd>escarabat de Sant Joan</trd>
  </trdgrp>
  <trd lng="cs">chroust</trd>
  <trd lng="de">Maik&auml;fer</trd>
  <trd lng="en">may bug</trd>
  <trdgrp lng="fr">
    <trd>hanneton</trd>,
    <trd>ver blanc<klr tip="ind"> (insecte)</klr></trd>
  </trdgrp>
  <trd lng="hu">cserebog&aacute;r</trd>
  <trd lng="nl">meikever</trd>
  <trd lng="pl">chrab&aogonek;szcz majowy</trd>
  <trdgrp lng="pt">
    <trd>melolonta</trd>,
    <trd>besouro</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_j;&c_s;&c_k;&c_i;&c_j; &c_zh;&c_u;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_j;&c_s;&c_k;&c_i;&c_j; &c_h;&c_r;&c_u;&c_shch;</trd>
  </trdgrp>
  <trd lng="sk">chr&uacute;st</trd>
  <trdgrp lng="uk">
    <trd>&#x445;&#x440;&#x443;&#x449;</trd>,
    <trd>&#x442;&#x440;&#x430;&#x432;&#x43D;&#x435;&#x432;&#x438;&#x439; &#x445;&#x440;&#x443;&#x449;</trd>
  </trdgrp>
</drv>
</art>
