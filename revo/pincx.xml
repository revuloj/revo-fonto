<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pin&ccirc;</rad>/i
</kap>
<drv mrk="pincx.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Premi inter du fingroj a&ubreve; inter du pecoj:
      <ekz>
        <tld/>i la orelon de infano;
      </ekz>
      <ekz>
        la papago <tld/>is al mi la fingron per sia beko;
      </ekz>
      <ekz>
        <tld/>i la kordojn de liro;
      </ekz>
      <ekz>
        <tld/>i kelke da akordoj sur la harpo<fnt>B</fnt>;
      </ekz>
      <ekz>
        malgrasaj bovinoj avide <tld/>is (&scirc;iris <tld/>ante per la lipoj)
        herbon en la apudvojaj kavoj<fnt>B</fnt>;
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="tauxz.0i">ta&ubreve;zi</ref>.
  </refgrp>
  <trdgrp lng="de">
    <trd>kneifen</trd>,
    <trd>zwicken</trd>,
    <trd>klemmen</trd>
  </trdgrp>
  <trd lng="en">pinch</trd>
  <trdgrp lng="fr">
    <trd>pincer</trd>
  </trdgrp>
  <trd lng="nl">knijpen</trd>
</drv>

<drv mrk="pincx.0preno">
  <kap>
    <tld/>preno
  </kap>
  <snc>
    <dif>
      Eta kvanto, tiom, kiom oni povas preni pin&ccirc;e.
      <ekz>
        <tld/>preno da salo, da tabako.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>pinc&eacute;e</trd>
  </trdgrp>
  <trd lng="nl">snuifje</trd>
</drv>

<drv mrk="pincx.0ilo">
  <kap>
    <tld/>ilo
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="pincx.0ilo.ZOO">
    <dif>
      Pin&ccirc;kapabla ekstremo de membroj de iuj <ref tip="vid" cel="krust.0uloj">krustuloj</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>pince<klr> (anatomie)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="pincx.naz0ilo">
  <kap>
    naz<tld/>ilo
  </kap>
  <uzo tip="fak">FOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="naz.0umo">Nazumo</ref>.
    </dif>
  </snc>
</drv>
</art>
