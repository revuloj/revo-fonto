<drv mrk="pincx.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pincx.0i.KOMUNE">
    <dif>
      Premi inter du fingroj a&ubreve; inter du pecoj:
      <ekz>
        <tld/>i la orelon de infano;
      </ekz>
      <ekz>
        la papago <tld/>is al mi la fingron per sia beko;
      </ekz>
      <ekz>
        <tld/>i la kordojn de liro;
      </ekz>
      <ekz>
        <tld/>i kelke da akordoj sur la harpo<fnt>B</fnt>;
      </ekz>
      <ekz>
        malgrasaj bovinoj avide <tld/>is
        <klr>(&scirc;iris <tld/>ante per la
        lipoj)</klr>
        herbon en la apudvojaj kavoj<fnt>B</fnt>;
      </ekz>
    </dif>
    <trdgrp lng="cs">
      <trd>&scaron;t&iacute;pnout</trd>,
      <trd>&scaron;t&iacute;pat</trd>,
      <trd>sk&rcaron;&iacute;pnout</trd>,
      <trd>sk&rcaron;&iacute;pat</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="tauxz.0i">ta&ubreve;zi</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_sh;&c_ch;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_ch;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_u;&c_b;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kneifen</trd>,
    <trd>zwicken</trd>,
    <trd>klemmen</trd>
  </trdgrp>
  <trd lng="en">pinch</trd>
  <trd lng="ca">pin&ccedil;ar</trd>
  <trdgrp lng="es">
    <trd>pellizcar</trd>,
    <trd>pinzar</trd>
  </trdgrp>
  <trd lng="fr">pincer</trd>
  <trdgrp lng="hu">
    <trd>cs&iacute;p</trd>,
    <trd>megcs&iacute;p</trd>,
    <trd>cs&iacute;ptet</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3064;&#x307E;&#x3080;</trd>,
    <trd>&#x306F;&#x3055;&#x3080;</trd>,
    <trd>&#x3064;&#x306D;&#x308B;</trd>,
    <trd>&#x306F;&#x3080;</trd>,
    <trd>&#x6458;&#x3080;  <pr>&#x3064;&#x307E;&#x3080;</pr></trd>,
    <trd>&#x3064;&#x307E;&#x5F3E;&#x304F;  <pr>&#x3064;&#x307E;&#x3073;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">knijpen</trd>
  <trdgrp lng="pl">
    <trd>szczypa&cacute;</trd>,
    <trd>uszczypn&aogonek;&cacute;</trd>,
    <trd>skuba&cacute;</trd>
  </trdgrp>
  <trd lng="pt">beliscar</trd>
  <trdgrp lng="ro">
    <trd>ciupi</trd>,
    <trd>pi&#x219;ca</trd>,
    <trd>peni</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_i;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_zh;&c_i;&c_m;&c_a;&c_t;&c_mol;
    &c_m;&c_je;&c_zh;&c_d;&c_u;
    &c_p;&c_a;&c_l;&c_mol;&c_c;&c_a;&c_m;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_i;&#x43F;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_h;&#x43E;&#x43F;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_s;&#x43A;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_ib;&#x43D;&c_c;&c_i;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_shch;&c_i;&#x43F;&c_u;&c_v;&c_a;&c_t;&c_i; (&#x43A;&c_ib;&#x43D;&c_c;&c_ib; &c_g;&c_ib;&#x43B;&#x43E;&#x43A;)</trd>,
    <trd>&c_v;&c_i;&#x43A;&#x43E;&#x43D;&c_u;&c_v;&c_a;&c_t;&c_i; &#x43F;&c_ib;&c_c;&c_i;&#x43A;&c_a;&c_t;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9257; <pr>qi&#xE1;n</pr></trd>,
    <trd>&#x94B3; <pr>qi&#xE1;n</pr></trd>,
    <trd>&#x62E7; <pr>n&#xED;ng</pr></trd>,
    <trd>&#x64F0; <pr>n&#xED;ng</pr></trd>,
    <trd>&#x634F; <pr>ni&#x113;</pr></trd>,
    <trd>&#x6390; <pr>qi&#x101;</pr></trd>,
    <trd>&#x6423; <pr>mi&#xE8;</pr></trd>,
    <trd>&#x9477; <pr>ni&#xE8;</pr></trd>,
    <trd>&#x954A; <pr>ni&#xE8;</pr></trd>,
    <trd>&#x5939; <pr>ji&#x101;</pr></trd>,
    <trd>&#x593E; <pr>ji&#x101;</pr></trd>,
    <trd>&#x62D1; <pr>qi&#xE1;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pincx.0ajxo">
  <kap><tld/>a&jcirc;o, <var><kap><tld/>preno</kap></var></kap>
  <snc mrk="pincx.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Eta kvanto, tiom, kiom oni povas preni <tld/>e.
      <ekz>
        <tld/>preno da salo, da tabako;
      </ekz>
      <ekz>
        matene ili metas en tetason <tld/>a&jcirc;on da rostita
        faruno, malgrandan kvanton da rekremento de lakto
        kaj malgrandan pecon da butero kaj enver&scirc;as varmegan
        akvon
        <fnt>
          <aut>&Ccirc;ina Interreta Informa Centro</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/world/shi-living/moroj/48.htm"/>
          Tetrinkado de Tibet-nacianoj</vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">&scaron;petka</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_ch;&c_o;&c_p;&c_c;&c_ib;</trd>,
    <trd>&c_d;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_ib;&c_v;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_s;&c_a;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">pinc&eacute;e</trd>
  <trd lng="de">Prise</trd>
  <trd lng="es">Pellizco <klr>(referido a una cantidad muy peque&ntilde;a de
  harina, azucar, etc...)</klr></trd>
  <trd lng="hu">csipetnyi</trd>
  <trd lng="ja">&#x4E00;&#x3064;&#x307E;&#x307F;  <pr>&#x3072;&#x3068;&#x3064;&#x307E;&#x307F;</pr></trd>
  <trd lng="nl">snuifje</trd>
  <trd lng="pl">szczypta</trd>
  <trdgrp lng="ro">
    <trd>ciupit</trd>,
    <trd>bucat&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_shch;&c_je;&c_p;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="zh">&#x634F; <pr>ni&#x113;</pr></trd>
</drv>

<drv mrk="pincx.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="pincx.0ilo.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Pin&ccirc;kapabla ekstremo de membroj de iuj
      <ref tip="vid" cel="krust.0uloj">krustuloj</ref>:
      <ekz>
        <ctl>La Krabo kun Oraj <tld lit="P"/>iloj</ctl>
        <klr>(titolo de bildstrio)</klr>
      </ekz>.
    </dif>
    <trd lng="cs">klepeto</trd>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ju;&c_sh;&c_n;&c_ja;</trd>
  <trd lng="bg">&c_k;&c_l;&c_je;&c_shch;&c_i;</trd>
  <trd lng="cs">kle&scaron;t&ecaron;</trd>
  <trd lng="de">Zange</trd>
  <trd lng="en">pliers</trd>
  <trd lng="fr">pince<klr> (anatomie)</klr></trd>
  <trd lng="hu">oll&oacute;<klr tip="ind"> (r&aacute;k&eacute;)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x306F;&#x3055;&#x307F;</trd>,
    <trd>&#x30DA;&#x30F3;&#x30C1;</trd>,
    <trd>&#x9257;&#x5B50;  <pr>&#x304B;&#x3093;&#x3057;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schaar<klr tip="ind"> (v.kreeft bv.)</klr></trd>,
    <trd>tang</trd>,
    <trd>knijptang</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pinceta</trd>,
    <trd>kleszcze</trd>,
    <trd>szczypce</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>penset&abreve;</trd>,
    <trd>cle&#x219;te</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_sh;&c_n;&c_ja;</trd>
  <trd lng="ca">pin&ccedil;a</trd>
  <trd lng="es">pinza</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_ib;&#x43D;&c_c;&c_je;&c_t;</trd>,
    <trd>&c_shch;&c_i;&#x43F;&c_c;&c_ib;</trd>,
    <trd>&#x43A;&#x43B;&c_ib;&c_shch;&c_ib;</trd>,
    <trd>&#x43F;&#x43B;&#x43E;&c_s;&#x43A;&#x43E;&c_g;&c_u;&c_b;&c_c;&c_ib;</trd>,
    <trd>&#x43A;&#x43B;&c_ib;&c_sh;&#x43D;&#x44F;</trd>,
    <trd>&c_r;&c_ib;&c_d;&c_sh;&c_je; &#x43A;&#x43B;&c_je;&c_sh;&#x43D;&#x44F;</trd>,
    <trd>&c_shch;&c_u;&#x43F;&c_a;&#x43B;&#x44C;&c_c;&c_je; (&#x43A;&#x43E;&#x43C;&c_a;&c_h;&c_i;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9257; <pr>qi&#xE1;n</pr></trd>,
    <trd>&#x94B3; <pr>qi&#xE1;n</pr></trd>,
    <trd>&#x9257;&#x5B50; <pr>qi&#xE1;nzi</pr></trd>,
    <trd>&#x94B3;&#x5B50; <pr>qi&#xE1;nzi</pr></trd>,
    <trd>&#x87AF; <pr>&#xE1;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pincx.naz0ilo">
  <kap>naz<tld/>ilo</kap>
  <snc>
    <ref tip="dif" cel="naz.0umo">nazumo</ref>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_n;&c_s;&c_n;&c_e;</trd>
  <trdgrp lng="cs">
    <trd>sk&rcaron;ipec</trd>,
    <trd>cvikr</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>orrcsiptet&odblac;</trd>,
    <trd>cvikker</trd>
  </trdgrp>
  <trd lng="fr">pince-nez</trd>
  <trdgrp lng="nl">
