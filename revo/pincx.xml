<kap>
  <ofc>*</ofc>
  <rad>pin&ccirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="pincx.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pincx.0i.KOMUNE">
    <dif>
      Premi inter du fingroj a&ubreve; inter du pecoj:
      <ekz>
        <tld/>i la orelon de infano;
      </ekz>
      <ekz>
        la papago <tld/>is al mi la fingron per sia beko;
      </ekz>
      <ekz>
        <tld/>i la kordojn de liro;
      </ekz>
      <ekz>
        <tld/>i kelke da akordoj sur la harpo<fnt>B</fnt>;
      </ekz>
      <ekz>
        malgrasaj bovinoj avide <tld/>is
        <klr>(&scirc;iris <tld/>ante per la
        lipoj)</klr>
        herbon en la apudvojaj kavoj<fnt>B</fnt>;
      </ekz>
    </dif>
    <trdgrp lng="cs">
      <trd>&scaron;t&iacute;pnout</trd>,
      <trd>&scaron;t&iacute;pat</trd>,
      <trd>sk&rcaron;&iacute;pnout</trd>,
      <trd>sk&rcaron;&iacute;pat</trd>
    </trdgrp>
  </snc>
  <refgrp tip="vid">
    <ref cel="tauxz.0i">ta&ubreve;zi</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_sh;&c_ch;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_sh;&c_ch;&c_y;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_u;&c_b;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kneifen</trd>,
    <trd>zwicken</trd>,
    <trd>klemmen</trd>
  </trdgrp>
  <trd lng="en">pinch</trd>
  <trd lng="ca">pin&ccedil;ar</trd>
  <trdgrp lng="es">
    <trd>pellizcar</trd>,
    <trd>pinzar</trd>
  </trdgrp>
  <trd lng="fr">pincer</trd>
  <trdgrp lng="hu">
    <trd>cs&iacute;p</trd>,
    <trd>megcs&iacute;p</trd>,
    <trd>cs&iacute;ptet</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12388;&#12414;&#12416;</trd>,
    <trd>&#12399;&#12373;&#12416;</trd>,
    <trd>&#12388;&#12397;&#12427;</trd>,
    <trd>&#12399;&#12416;</trd>,
    <trd>&#25688;&#12416;  <pr>&#12388;&#12414;&#12416;</pr></trd>,
    <trd>&#12388;&#12414;&#24382;&#12367;  <pr>&#12388;&#12414;&#12403;&#12367;</pr></trd>
  </trdgrp>
  <trd lng="nl">knijpen</trd>
  <trdgrp lng="pl">
    <trd>szczypa&cacute;</trd>,
    <trd>uszczypn&aogonek;&cacute;</trd>,
    <trd>skuba&cacute;</trd>
  </trdgrp>
  <trd lng="pt">beliscar</trd>
  <trdgrp lng="ro">
    <trd>ciupi</trd>,
    <trd>pi&scomma;ca</trd>,
    <trd>peni</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_shch;&c_i;&c_p;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_zh;&c_i;&c_m;&c_a;&c_t;&c_mol;
    &c_m;&c_je;&c_zh;&c_d;&c_u;
    &c_p;&c_a;&c_l;&c_mol;&c_c;&c_a;&c_m;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_shch;&c_i;&c_p;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_h;&c_o;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_s;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_n;&c_c;&c_i;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_shch;&c_i;&c_p;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_k;&c_ib;&c_n;&c_c;&c_ib; &c_g;&c_ib;&c_l;&c_o;&c_k;)</trd>,
    <trd>&c_v;&c_i;&c_k;&c_o;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i; &c_p;&c_ib;&c_c;&c_i;&c_k;&c_a;&c_t;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#37463; <pr>qi&aacute;n</pr></trd>,
    <trd>&#38067; <pr>qi&aacute;n</pr></trd>,
    <trd>&#25319; <pr>n&iacute;ng</pr></trd>,
    <trd>&#25840; <pr>n&iacute;ng</pr></trd>,
    <trd>&#25423; <pr>ni&emacron;</pr></trd>,
    <trd>&#25488; <pr>qi&amacron;</pr></trd>,
    <trd>&#25635; <pr>mi&egrave;</pr></trd>,
    <trd>&#38007; <pr>ni&egrave;</pr></trd>,
    <trd>&#38218; <pr>ni&egrave;</pr></trd>,
    <trd>&#22841; <pr>ji&amacron;</pr></trd>,
    <trd>&#22846; <pr>ji&amacron;</pr></trd>,
    <trd>&#25297; <pr>qi&aacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pincx.0ajxo">
  <kap><tld/>a&jcirc;o, <var><kap><tld/>preno</kap></var></kap>
  <snc mrk="pincx.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Eta kvanto, tiom, kiom oni povas preni <tld/>e.
      <ekz>
        <tld/>preno da salo, da tabako;
      </ekz>
      <ekz>
        matene ili metas en tetason <tld/>a&jcirc;on da rostita
        faruno, malgrandan kvanton da rekremento de lakto
        kaj malgrandan pecon da butero kaj enver&scirc;as varmegan
        akvon
        <fnt>
          <aut>&Ccirc;ina Interreta Informa Centro</aut>,
          <vrk><url ref="http://esperanto.china.org.cn/world/shi-living/moroj/48.htm"/>
          Tetrinkado de Tibet-nacianoj</vrk>,
          <lok>2001</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">&scaron;petka</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_ch;&c_o;&c_p;&c_c;&c_ib;</trd>,
    <trd>&c_d;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_ib;&c_v;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_s;&c_a;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">pinc&eacute;e</trd>
  <trd lng="de">Prise</trd>
  <trd lng="en">pinch <klr>(quantity)</klr></trd>
  <trd lng="es">Pellizco <klr>(referido a una cantidad muy peque&ntilde;a de
  harina, azucar, etc...)</klr></trd>
  <trd lng="hu">csipetnyi</trd>
  <trd lng="ja">&#19968;&#12388;&#12414;&#12415;  <pr>&#12402;&#12392;&#12388;&#12414;&#12415;</pr></trd>
  <trd lng="nl">snuifje</trd>
  <trd lng="pl">szczypta</trd>
  <trdgrp lng="ro">
    <trd>ciupit</trd>,
    <trd>bucat&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_shch;&c_je;&c_p;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="zh">&#25423; <pr>ni&emacron;</pr></trd>
</drv>

<drv mrk="pincx.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="pincx.0ilo.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Pin&ccirc;kapabla ekstremo de membroj de iuj
      <ref tip="vid" cel="krust.0uloj">krustuloj</ref>:
      <ekz>
        <ctl>La Krabo kun Oraj <tld lit="P"/>iloj</ctl>
        <klr>(titolo de bildstrio)</klr>
      </ekz>.
    </dif>
    <trd lng="cs">klepeto</trd>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ju;&c_sh;&c_n;&c_ja;</trd>
  <trd lng="bg">&c_k;&c_l;&c_je;&c_shch;&c_i;</trd>
  <trd lng="cs">kle&scaron;t&ecaron;</trd>
  <trd lng="de">Zange</trd>
  <trd lng="en">pliers</trd>
  <trd lng="fr">pince<klr> (anatomie)</klr></trd>
  <trd lng="hu">oll&oacute;<klr tip="ind"> (r&aacute;k&eacute;)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#12399;&#12373;&#12415;</trd>,
    <trd>&#12506;&#12531;&#12481;</trd>,
    <trd>&#37463;&#23376;  <pr>&#12363;&#12435;&#12375;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schaar<klr tip="ind"> (v.kreeft bv.)</klr></trd>,
    <trd>tang</trd>,
    <trd>knijptang</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pinceta</trd>,
    <trd>kleszcze</trd>,
    <trd>szczypce</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>penset&abreve;</trd>,
    <trd>cle&scomma;te</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_sh;&c_n;&c_ja;</trd>
  <trd lng="ca">pin&ccedil;a</trd>
  <trdgrp lng="en">
    <trd>pliers</trd>,
    <trd>pincer</trd>
  </trdgrp>
  <trd lng="es">pinza</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_n;&c_c;&c_je;&c_t;</trd>,
    <trd>&c_shch;&c_i;&c_p;&c_c;&c_ib;</trd>,
    <trd>&c_k;&c_l;&c_ib;&c_shch;&c_ib;</trd>,
    <trd>&c_p;&c_l;&c_o;&c_s;&c_k;&c_o;&c_g;&c_u;&c_b;&c_c;&c_ib;</trd>,
    <trd>&c_k;&c_l;&c_ib;&c_sh;&c_n;&c_ja;</trd>,
    <trd>&c_r;&c_ib;&c_d;&c_sh;&c_je; &c_k;&c_l;&c_je;&c_sh;&c_n;&c_ja;</trd>,
    <trd>&c_shch;&c_u;&c_p;&c_a;&c_l;&c_mol;&c_c;&c_je; (&c_k;&c_o;&c_m;&c_a;&c_h;&c_i;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#37463; <pr>qi&aacute;n</pr></trd>,
    <trd>&#38067; <pr>qi&aacute;n</pr></trd>,
    <trd>&#37463;&#23376; <pr>qi&aacute;nzi</pr></trd>,
    <trd>&#38067;&#23376; <pr>qi&aacute;nzi</pr></trd>,
    <trd>&#34735; <pr>&aacute;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pincx.naz0ilo">
  <kap>naz<tld/>ilo</kap>
  <snc>
    <ref tip="dif" cel="naz.0umo">nazumo</ref>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_n;&c_s;&c_n;&c_e;</trd>
  <trdgrp lng="cs">
    <trd>sk&rcaron;ipec</trd>,
    <trd>cvikr</trd>
  </trdgrp>
  <trd lng="en">pince-nez</trd>
  <trdgrp lng="hu">
    <trd>orrcsiptet&odblac;</trd>,
    <trd>cvikker</trd>
  </trdgrp>
  <trd lng="fr">pince-nez</trd>
  <trdgrp lng="nl">
    <trd>neusknijper</trd>,
    <trd>knijpbril</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>binokle</trd>,
    <trd>pince-nez</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ochelari</trd>,
    <trd>pince-nez</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_je;&c_n;&c_s;&c_n;&c_je;</trd>
</drv>

<drv mrk="pincx.tuk0ilo">
  <kap>tuk<tld/>ilo</kap>
  <snc mrk="pincx.tuk0ilo.HEJ">
    <dif>
      <tld lit="P"/>ilo per kiu oni pendigas seki&gcirc;ontan
      tukon a&ubreve; veston je &scirc;nuro:
      <ekz>
        ili postlasis al Usono plurajn <klr>...</klr> inventa&jcirc;ojn,
        ekzemple vaste uzatan specon de tuk<tld/>ilo
        <fnt>
          <aut>R. Haruo</aut>,
          <vrk><url ref="http://www.yoko-oshin.info/cigneto/thctxt/j/jendonaco1.html">
          Jen donaco : Simpleco</url></vrk>,
          <lok>[vidita en 2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_sh;&c_ch;&c_e;&c_p;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_p;&c_a;&c_d;&c_v;&c_je;&c_sh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="cs">kol&iacute;&ccaron;ek na pr&aacute;dlo</trd>
  <trd lng="en"><ind>towel</ind> clip</trd>
  <trd lng="fr">pince &agrave; linge</trd>
  <trd lng="ja">&#27927;&#28655;&#12400;&#12373;&#12415;  <pr>&#12379;&#12435;&#12383;&#12367;&#12496;&#12469;&#12511;</pr></trd>
  <trd lng="pl">klamerka</trd>
  <trd lng="ro">agraf&abreve;</trd>
  <trd lng="uk">&c_p;&c_r;&c_i;&c_shch;&c_ib;&c_p;&c_k;&c_a; (&c_d;&c_l;&c_ja; &c_b;&c_ib;&c_l;&c_i;&c_z;&c_n;&c_i;)</trd>
  <trdgrp lng="zh">
    <trd>&#22841;&#23376; <pr>ji&amacron;zi</pr></trd>,
    <trd>&#22846;&#23376; <pr>ji&amacron;zi</pr></trd>
  </trdgrp>
</drv>
