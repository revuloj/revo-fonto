</drv>

<drv mrk="pincx.tuk0ilo">
  <kap>tuk<tld/>ilo</kap>
  <snc mrk="pincx.tuk0ilo.HEJ">
    <dif>
      <tld lit="P"/>ilo per kiu oni pendigas seki&gcirc;ontan
      tukon a&ubreve; veston je &scirc;nuro:
      <ekz>
        ili postlasis al Usono plurajn <klr>...</klr> inventa&jcirc;ojn, 
        ekzemple vaste uzatan specon de tuk<tld/>ilo
        <fnt>
          <aut>R. Haruo</aut>,
          <vrk><url ref="http://www.yoko-oshin.info/cigneto/thctxt/j/jendonaco1.html">
          Jen donaco : Simpleco</url></vrk>,
          <lok>[vidita en 2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_sh;&c_ch;&c_e;&c_p;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_p;&c_a;&c_d;&c_v;&c_je;&c_sh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="cs">kol&iacute;&ccaron;ek na pr&aacute;dlo</trd>
  <trd lng="fr">pince &agrave; linge</trd>
  <trd lng="ja">&#x6D17;&#x6FEF;&#x3070;&#x3055;&#x307F;  <pr>&#x305B;&#x3093;&#x305F;&#x304F;&#x30D0;&#x30B5;&#x30DF;</pr></trd>
  <trd lng="pl">klamerka</trd>
  <trd lng="ro">agraf&abreve;</trd>
  <trd lng="uk">&#x43F;&c_r;&c_i;&c_shch;&c_ib;&#x43F;&#x43A;&c_a; (&c_d;&#x43B;&#x44F; &c_b;&c_ib;&#x43B;&c_i;&c_z;&#x43D;&c_i;)</trd>
  <trdgrp lng="zh">
    <trd>&#x5939;&#x5B50; <pr>ji&#x101;zi</pr></trd>,
    <trd>&#x593E;&#x5B50; <pr>ji&#x101;zi</pr></trd>
  </trdgrp>
</drv>
