<art mrk="$Id: leonard.xml,v 1.9 2025/04/28 18:38:15 revo Exp $">
<kap>
  <rad>Leonard</rad>/o
  <fnt>
    <bib>PIV1</bib>
  </fnt>
</kap>

<drv mrk="leonard.0o">
  <kap><tld/>o</kap>
  <snc mrk="leonard.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        kvankam &ccirc;iu el la sinjoroj vidis la movon, neniu faris
        mienon
        kvaza&ubreve; li estis kaptonta la brakon de sinjoro <tld/>o
        <fnt>
          <aut>B. Traven, trad. Cezar</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/cezar/251611">
          La &Ccirc;aro</url></vrk>,
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &Gcirc;okonda estas pentra&jcirc;o de <tld/>o da Vinci
        <fnt>
          <bib>Viki</bib>, <lok><url ref="&Viki;/La_Gioconda">
          la Gioconda</url>, 2010-09-06</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_L;&c_je;&c_a;&c_n;&c_a;&c_r;&c_d;&c_a;</trd>
    <trd lng="fr">L&eacute;onard</trd>
    <trd lng="pl">Leonard</trd>
    <trd lng="zh">&#38647;&#22885;&#32435;&#22810; <pr>l&eacute;i&agrave;on&agrave;du&omacron;</pr></trd>
</snc>
  <trd lng="en">Leonard</trd>
  <trd lng="ja">&#12524;&#12458;&#12490;&#12523;&#12489;</trd>
  <trd lng="uk">&c_L;&c_je;&c_o;&c_n;&c_a;&c_r;&c_d;</trd>
</drv>
</art>

