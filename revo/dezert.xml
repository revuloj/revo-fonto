  <trd lng="bn">&#x9AE;&#x9B0;&#x9C1;&#x9AD;&#x9C2;&#x9AE;&#x9BF;</trd>
  <trd lng="bo">&#xF56;&#xFB1;&#xF7A;&#xF0B;&#xF50;&#xF44;&#xF0B;</trd>
  <trd lng="bs">pustinja</trd>
  <trd lng="co">u disertu</trd>
  <trdgrp lng="cs">
    <trd>pou&scaron;&tcaron;</trd>,
    <trd>pustina</trd>
  </trdgrp>
  <trd lng="da">&#xF8;rken</trd>
  <trd lng="de">W&uuml;ste</trd>
  <trd lng="en">desert</trd>
  <trd lng="es">desierto</trd>
  <trd lng="et">k&#xF5;rb</trd>
  <trd lng="eu">basamortuko</trd>
  <trd lng="fr">d&eacute;sert<klr> (subst.)</klr></trd>
  <trd lng="fy">woastyn</trd>
  <trd lng="gl">deserto</trd>
  <trd lng="gu">&#xAB0;&#xAA3;</trd>
  <trd lng="ha">hamada</trd>
  <trd lng="he">&#x5DE;&#x5B4;&#x5D3;&#x5D1;&#x5BC;&#x5B8;&#x5E8;</trd>
  <trd lng="hi">&#x930;&#x947;&#x917;&#x93F;&#x938;&#x94D;&#x924;&#x93E;&#x928;</trd>
  <trd lng="hr">pustinja</trd>
  <trd lng="hu">sivatag</trd>
  <trd lng="hy">&#x561;&#x576;&#x561;&#x57A;&#x561;&#x57F;</trd>
  <trd lng="id">gurun</trd>
  <trd lng="is">ey&#xF0;im&#xF6;rk</trd>
