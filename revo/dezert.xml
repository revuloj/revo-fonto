<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>dezert</rad>/o
</kap>
<drv mrk="dezert.0o">
<kap><ofc>*</ofc><tld/>o</kap>
<uzo tip="fak">GEOG</uzo>
<snc>
<dif>
Regiono en kiu tute mankas vegeta&jcirc;oj k lo&gcirc;antoj:
<ekz>voki, prediki en <tld/>o
<klr>(senutile peni por konvinki)</klr>.
</ekz> 
</dif>
</snc>
<trd lng="fr">d&eacute;sert</trd>
<trd lng="ru">&c_p;&c_u;&c_s;&c_t;&c_y;&c_n;&c_ja;</trd>
</drv>
<drv mrk="dezert.0a">
<kap><tld/>a</kap>
<snc>
<dif>
Simila  al <tld/>o, senhoma. 
</dif>
</snc>
<trd lng="fr">d&eacute;sertique</trd>
<trdgrp lng="ru">
  <trd>&c_p;&c_u;&c_s;&c_t;&c_y;&c_n;&c_n;&c_y;&c_j;</trd>,
  <trd>&c_n;&c_je;&c_o;&c_b;&c_i;&c_t;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>
</trdgrp>
</drv>
<drv mrk="dezert.0ulo">
<kap><tld/>ulo</kap>
<snc>
<dif>
Ermito.  
</dif>
</snc>
<trd lng="fr">ermite</trd>
<trdgrp lng="ru">
  <trd>&c_p;&c_u;&c_s;&c_t;&c_y;&c_n;&c_n;&c_i;&c_k;</trd>,
  <trd>&c_o;&c_t;&c_sh;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
</trdgrp>
</drv>
</art>
