<drv mrk="dezert.0a">
  <kap><tld/>a</kap>
  <snc mrk="dezert.0a.GEOG">
    <dif>
      Rilata al, karakterizata de <tld/>o:
      <ekz>
        lo&gcirc;i en lando <tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 21:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        kaj matene <klr>[&#8230;]</klr> li levi&gcirc;is, kaj foriris en
        <tld/>an
        lokon, kaj tie pre&gcirc;adis
        <fnt><bib>NT</bib> <lok>&Mar; 1:35</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a plata&jcirc;o de Arizono, Nevado kaj Utaho,
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        duon<tld/>a regiono
        <fnt>
          <bib>Ret</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_n;&c_a;&c_s;&c_je;&c_l;&c_je;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_ja;&c_z;&c_mol;&c_l;&c_ju;&c_d;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="bg">&c_p;&c_u;&c_s;&c_t;&c_i;&c_n;&c_je;&c_n;</trd>
    <trdgrp lng="cs">
      <trd>pou&scaron;tn&iacute;</trd>,
      <trd>pust&yacute;</trd>,
      <trd>sir&yacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>w&uuml;st</trd>,
      <trd>W&uuml;sten-</trd>
    </trdgrp>
    <trd lng="en">desolate</trd>
    <trd lng="es">des&eacute;rtico</trd>
    <trdgrp lng="fr">
      <trd>d&eacute;sert<klr> (adj.)</klr></trd>,
      <trd>d&eacute;sertique</trd>
    </trdgrp>
    <trd lng="he">&mem;&#x5B4;&dalet;&bet;&#x5BC;&#x5B8;&resh;&yod;</trd>
    <trdgrp lng="hu">
      <trd>puszta</trd>,
      <trd>kietlen</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pustynny</trd>,
      <trd>bezludny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_u;&c_s;&c_t;&c_y;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_o;&c_b;&c_i;&c_t;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>opusten&yacute;</trd>,
      <trd>pust&yacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dezert.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forlasita, <ref tip="super" cel="plen.mal0a">malplena</ref>, 
      <ref cel="frukt.sen0a.FIG">senfrukta</ref>:
      <ekz>
        ili mar&scirc;is en la a&ubreve;tuna, malvarma tago sur la <tld/>a &scirc;oseo
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        vaka kaj <tld/>a animo
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_p;&c_u;&c_s;&c_t;&c_o;&c_sh;&c_a;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_k;&c_ib;&c_n;&c_u;&c_t;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>trostlos</trd>,
      <trd>&ouml;de</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="dezert.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dezert.0igi.KOMUNE">
    <dif>
      &Scirc;an&gcirc;i en <tld/>an loko, <ref tip="super" cel="detru.0i">detrui</ref>, senvivigi, 
      malfekundigi:
      <ekz>
        ili venadis <klr>[&#8230;]</klr> simile al akridoj <klr>[&#8230;]</klr> en la landon, por
        <tld/>igi &gcirc;in
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 6:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel flamo brul<tld/>igas montojn
        <fnt><bib>MT</bib> <lok>&Psa; 83:14</lok></fnt>;
      </ekz>
      <ekz>
        lando <klr>[&#8230;]</klr> <tld/>igita per fajro kaj glavo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la Dua Mondmilito <tld/>igis vastajn surfacojn de la terglobo kaj rekte a&ubreve;
        malrekte ka&ubreve;zis mizeron al la tuta homaro
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>wyja&lstroke;awia&cacute;</trd>,
      <trd>wyludnia&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_u;&c_s;&c_t;&c_o;&c_sh;&c_y;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_o;&c_p;&c_u;&c_s;&c_t;&c_o;&c_sh;&c_a;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>vyplenit</trd>,
    <trd>zpusto&scaron;it</trd>
  </trdgrp>
  <trd lng="de">verw&uuml;sten</trd>
  <trd lng="en">devastate</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;nuder</trd>
    <trd>d&eacute;vaster</trd>
  </trdgrp>
  <trd lng="he">&lamed;&nun;&tet;&vav;&shin;</trd>
  <trd lng="sk">pusto&scaron;i&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43F;&c_u;&c_s;&c_t;&#x43E;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_t;&c_v;&#x43E;&c_r;&#x44E;&c_v;&c_a;&c_t;&c_i; &c_v; &#x43F;&c_u;&c_s;&c_t;&c_je;&#x43B;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="dezert.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="dezert.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>o, senhomi&gcirc;i, senvegeta&jcirc;i&gcirc;i, senvivi&gcirc;i:
      <ekz>
        la tero <tld/>i&gcirc;is pro tio, ke ne estis pluvo
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 14:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        neniu bienulo povis tie lo&gcirc;i en paco kaj la bieno <tld/>i&gcirc;is
        <fnt>
          <aut>H. Laxness, trad. B. Ragnarsson</aut>
          <vrk>Sendependaj homoj</vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        la stepo <tld/>i&gcirc;as, la tajgo &scirc;rumpas norden
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 2 (100)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_u;&c_s;&c_t;&c_o;&c_sh;&c_y;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_v;&c_ja;&c_r;&c_n;&c_u;&c_c;&c_c;&c_a; &c_w; &c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_ju;</trd>
  </trdgrp>
  <trd lng="de">ver&ouml;den</trd>
  <trd lng="fr">se <ind>d&eacute;sertifier</ind></trd>
  <trd lng="he">&lamed;&ayin;&zayin;&vav;&bet;</trd>
</drv>

<drv mrk="dezert.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="dezert.0ulo.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      <ref tip="dif" cel="ermit.0o.REL">Ermito</ref>:
      <ekz>
        steloj &ccirc;e vortoj de mi, <tld/>ulo, pli &gcirc;oje radias, pli hele lumadas
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kune kun tri aliaj mona&hcirc;oj, li retiri&gcirc;is kiel <tld/>ulo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=308340">Sankta Samsono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      La signifo &leftquot;ermito&rightquot; ne &scirc;ajnas tute logika, &ccirc;ar unuflanke ermitoj lo&gcirc;as ne nur en
      <tld/>aj lokoj kaj aliflanke en <tld/>o lo&gcirc;as anka&ubreve; aliaj homoj, ekz-e
      nomadoj, kiujn oni same povus nomi <tld/>uloj.
    </rim>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>poustevn&iacute;k</trd>,
    <trd>samot&aacute;&rcaron;</trd>
  </trdgrp>
  <trd lng="de">Einsiedler</trd>
  <trd lng="fr">ermite</trd>
  <trd lng="he">&ayin;&resh;&yod;&qof;</trd>
  <trd lng="hu">remete</trd>
  <trd lng="pl">pustelnik</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_s;&c_t;&c_y;&c_n;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_o;&c_t;&c_sh;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pustovn&iacute;k</trd>,
    <trd>samot&aacute;r</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_u;&c_s;&c_t;&c_je;&#x43B;&#x44C;&#x43D;&c_i;&#x43A;</trd>
</drv>
