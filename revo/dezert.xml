  <trd lng="ne">&#x92E;&#x930;&#x941;&#x92D;&#x942;&#x92E;&#x93F;</trd>
  <trd lng="ny">chipululu</trd>
  <trd lng="pa">&#xA2E;&#xA3E;&#xA30;&#xA42;&#xA25;&#xA32;</trd>
  <trd lng="pl">pustynia</trd>
  <trd lng="ps">&#x62F;&#x69A;&#x62A;&#x647;</trd>
  <trdgrp lng="pt">
