<drv mrk="dezert.0a">
  <kap><tld/>a</kap>
  <snc mrk="dezert.0a.GEOG">
    <dif>
      Rilata al, karakterizata de <tld/>o:
      <ekz>
        lo&gcirc;i en lando <tld/>a
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 21:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        kaj matene <klr>[&#8230;]</klr> li levi&gcirc;is, kaj foriris en
        <tld/>an
        lokon, kaj tie pre&gcirc;adis
        <fnt><bib>NT</bib> <lok>&Mar; 1:35</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a plata&jcirc;o de Arizono, Nevado kaj Utaho,
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        duon<tld/>a regiono
        <fnt>
          <bib>Ret</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_n;&c_a;&c_s;&c_je;&c_l;&c_je;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_ja;&c_z;&c_mol;&c_l;&c_ju;&c_d;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="bg">&c_p;&c_u;&c_s;&c_t;&c_i;&c_n;&c_je;&c_n;</trd>
    <trdgrp lng="cs">
      <trd>pou&scaron;tn&iacute;</trd>,
      <trd>pust&yacute;</trd>,
      <trd>sir&yacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>w&uuml;st</trd>,
      <trd>W&uuml;sten-</trd>
    </trdgrp>
    <trd lng="en">desolate</trd>
    <trd lng="es">des&eacute;rtico</trd>
    <trdgrp lng="fr">
      <trd>d&eacute;sert<klr> (adj.)</klr></trd>,
      <trd>d&eacute;sertique</trd>
    </trdgrp>
    <trd lng="he">&mem;&#1460;&dalet;&bet;&#1468;&#1464;&resh;&yod;</trd>
    <trdgrp lng="hu">
      <trd>puszta</trd>,
      <trd>kietlen</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>kering kerontang</trd>,
      <trd>gersang</trd>,
      <trd>tandus</trd>,
      <trd><baz>gurun</baz>bergurun</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>pustynny</trd>,
      <trd>bezludny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_u;&c_s;&c_t;&c_y;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_o;&c_b;&c_i;&c_t;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>opusten&yacute;</trd>,
      <trd>pust&yacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dezert.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Forlasita, <ref tip="super" cel="plen.mal0a">malplena</ref>,
      <ref cel="frukt.sen0a.FIG">senfrukta</ref>:
      <ekz>
        ili mar&scirc;is en la a&ubreve;tuna, malvarma tago sur la <tld/>a &scirc;oseo
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        vaka kaj <tld/>a animo
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_p;&c_u;&c_s;&c_t;&c_o;&c_sh;&c_a;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_k;&c_ib;&c_n;&c_u;&c_t;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>trostlos</trd>,
      <trd>&ouml;de</trd>
    </trdgrp>
    <trd lng="en">deserted</trd>
    <trd lng="id"><baz>pencil</baz>terpencil</trd>
    <trdgrp lng="zh">
    <trd>&#21388;&#20518; <pr>y&agrave;nju&agrave;n</pr></trd>,
    <trd>&#33831;&#32034; <pr>xi&amacron;osu&#466;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="ja">
    <trd>&#30722;&#28448;&#12398;  <pr>&#12373;&#12400;&#12367;&#12398;</pr></trd>,
    <trd>&#33618;&#12428;&#12399;&#12390;&#12383;  <pr>&#12354;&#12428;&#12399;&#12390;&#12383;</pr></trd>,
    <trd>&#20154;&#12369;&#12398;&#12394;&#12356;  <pr>&#12376;&#12435;&#12369;&#12398;&#12394;&#12356;</pr></trd>,
    <trd>&#28961;&#20154;&#12398;  <pr>&#12416;&#12376;&#12435;&#12398;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="dezert.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dezert.0igi.KOMUNE">
    <dif>
      &Scirc;an&gcirc;i en <tld/>an loko, <ref tip="super" cel="detru.0i">detrui</ref>, senvivigi,
      malfekundigi:
      <ekz>
        ili venadis <klr>[&#8230;]</klr> simile al akridoj <klr>[&#8230;]</klr> en la landon, por
        <tld/>igi &gcirc;in
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 6:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel flamo brul<tld/>igas montojn
        <fnt><bib>MT</bib> <lok>&Psa; 83:14</lok></fnt>;
      </ekz>
      <ekz>
        lando <klr>[&#8230;]</klr> <tld/>igita per fajro kaj glavo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la Dua Mondmilito <tld/>igis vastajn surfacojn de la terglobo kaj rekte a&ubreve;
        malrekte ka&ubreve;zis mizeron al la tuta homaro
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>wyja&lstroke;awia&cacute;</trd>,
      <trd>wyludnia&cacute;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_u;&c_s;&c_t;&c_o;&c_sh;&c_y;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_o;&c_p;&c_u;&c_s;&c_t;&c_o;&c_sh;&c_a;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>vyplenit</trd>,
    <trd>zpusto&scaron;it</trd>
  </trdgrp>
  <trd lng="de">verw&uuml;sten</trd>
  <trd lng="en">devastate</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;nuder</trd>
    <trd>d&eacute;vaster</trd>
  </trdgrp>
  <trd lng="he">&lamed;&nun;&tet;&vav;&shin;</trd>
  <trdgrp lng="id">
    <trd><baz>binasa</baz>membinasakan</trd>,
    <trd><baz>gersang</baz>menggersangkan</trd>,
    <trd><baz>hancur</baz>menghancurkan</trd>,
    <trd><baz>musnah</baz>memusnahkan</trd>,
    <trd><baz>rusak</baz>merusak</trd>,
    <trd><baz>rusak</baz>merusakkan</trd>,
    <trd><baz>tandus</baz>menanduskan</trd>,
    <trd><baz>gurun</baz>menggurunkan</trd>
  </trdgrp>
  <trd lng="ja">&#33618;&#12428;&#12399;&#12390;&#12373;&#12379;&#12427;  <pr>&#12354;&#12428;&#12399;&#12390;&#12373;&#12379;&#12427;</pr></trd>
  <trd lng="sk">pusto&scaron;i&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_p;&c_u;&c_s;&c_t;&c_o;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_t;&c_v;&c_o;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i; &c_v; &c_p;&c_u;&c_s;&c_t;&c_je;&c_l;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="zh">
