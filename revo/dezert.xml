    <dif>
      Regiono en kiu tute mankas vegeta&jcirc;oj kaj lo&gcirc;antoj:
      <ekz>
        voki, prediki en <tld/>o
        <klr>(senutile peni por konvinki)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">woestyn</trd>
  <trd lng="am">&#x121D;&#x12F5;&#x1228; &#x1260;&#x12F3;</trd>
  <trd lng="ar">&#x635;&#x62D;&#x631;&#x627;&#x621;</trd>
  <trd lng="az">s&#x259;hra</trd>
  <trd lng="be">&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_ja;</trd>
