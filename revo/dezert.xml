<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dezert</rad>/o
</kap>
<drv mrk="dezert.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="dezert.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Regiono en kiu tute mankas vegeta&jcirc;oj kaj lo&gcirc;antoj:
      <ekz>
        voki, prediki en <tld/>o
        <klr>(senutile peni por konvinki)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="de">W&uuml;ste</trd>
  <trd lng="es">desierto</trd>
  <trd lng="fr">d&eacute;sert</trd>
  <trd lng="hu">sivatag</trd>
  <trd lng="ru">&c_p;&c_u;&c_s;&c_t;&c_y;&c_n;&c_ja;</trd>
  <trd lng="sk">p&uacute;&scaron;&tcaron;</trd>
</drv>

<drv mrk="dezert.0a">
  <kap><tld/>a</kap>
  <snc mrk="dezert.0a.GEOG">
    <dif>
      Simila al <tld/>o, senhoma:
      <ekz>
        kaj matene <klr>...</klr> li levi&gcirc;is, kaj foriris en <tld/>an
        lokon, kaj tie pre&gcirc;adis
        <fnt><bib>NT</bib>, <lok>&Mar; 1:35</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_n;&c_a;&c_s;&c_je;&c_l;&c_je;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_mol;&c_l;&c_ju;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">w&uuml;st</trd>
  <trd lng="es">des&eacute;rtico</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;sert<klr> (adj.)</klr></trd>,
    <trd>d&eacute;sertique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>puszta</trd>,
    <trd>kietlen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_s;&c_t;&c_y;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_b;&c_i;&c_t;&c_a;&c_je;&c_m;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="dezert.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="dezert.0igi.KOMUNE">
    <dif>
      Senigi je lo&gcirc;antoj, je vivebleco:
      <ekz>
        kiel flamo brul<tld/>igas montojn
        <fnt><bib>MT</bib>, <lok>&Psa; 83:14</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">ravager</trd>
</drv>

<drv mrk="dezert.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="dezert.0ulo.REL">
    <uzo tip="fak">REL</uzo>
    <ref tip="dif" cel="ermit.0o.REL">ermito</ref>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_s;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="de">Einsiedler</trd>
  <trd lng="fr">ermite</trd>
  <trd lng="hu">remete</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_s;&c_t;&c_y;&c_n;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_o;&c_t;&c_sh;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
