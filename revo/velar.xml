<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>velar</rad>/o
</kap>
<drv mrk="velar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      Konsonanto a&ubreve; kvaza&ubreve;vokalo, kies brueron naskas la fermo a&ubreve; la
      malvastigo de la bu&scirc;kanalo per la alproksimigo de la langa dorso al
      la velo (k, g, r, &hcirc;).
    </dif>
    <refgrp tip="sin">
      <ref cel="labial.0o">labialo</ref>,
      <ref cel="dental.0o">dentalo</ref>,
      <ref cel="palata.0o">palatalo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_e;&c_l;&c_ja;&c_r;&c_n;&c_y; &c_g;&c_u;&c_k;</trd>
  <trdgrp lng="de">
    <trd>Velar</trd>,
    <trd>Gaumensegellaut</trd>
  </trdgrp>
  <trd lng="es">velar</trd>
  <trd lng="fr">v&eacute;laire<klr> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>vel&aacute;ris m&aacute;ssalhangz&oacute;</trd>,
    <trd>h&aacute;ts&oacute; <ind>sz&aacute;jpadl&aacute;shang</ind></trd>
  </trdgrp>
  <trd lng="nl">velaar</trd>
  <trd lng="pt">velar</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_je;&c_l;&c_ja;&c_r;&c_n;&c_y;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_n;&c_je;&c_n;&c_jo;&c_b;&c_n;&c_y;&c_j; &c_z;&c_v;&c_u;&c_k;</trd>
  </trdgrp>
</drv>
</art>
