    <trd>&#38283;&#22987;&#28857;  [<ind>&#12363;&#12356;&#12375;&#12390;&#12435;</ind>]</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_s;&c_h;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_t;&c_o;&c_ch;&c_k;&c_a;</trd>
