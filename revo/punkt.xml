    <trd>&#36578;&#27231;  [<ind>&#12390;&#12435;&#12365;</ind>]</trd>,
    <trd>&#22793;&#12431;&#12426;&#30446;  [<ind>&#12363;&#12431;&#12426;&#12417;</ind>]</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_v;&c_o;&c_r;&c_o;&c_t;&c_n;&c_y;&c_j; &c_p;&c_u;&c_n;&c_k;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>bod obratu</trd>,
    <trd>oto&ccaron;n&yacute; bod</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_v;&c_o;&c_r;&c_o;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_v;&c_o;&c_r;&c_o;&c_t;&c_n;&c_a; &c_t;&c_o;&c_ch;&c_k;&c_a;</trd>,
