<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: podajr.xml,v 1.9 2023/10/17 09:24:39 revo Exp $">
<kap>
  <rad>podajr</rad>/o
</kap>
<drv mrk="podajr.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
  <snc>
    <dif>
      <klr>(de ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>
      <frm am="K">K</frm>, rilate al punkto <frm am="O">O</frm> en la
      sama
      ebeno)</klr>
      Ebena kurbo, konsistanta el &ccirc;iuj
      ortaj <ref cel="projek.0ajxo.MAT">projekcia&jcirc;oj</ref>
      de <frm am="O">O</frm> al la tan&gcirc;antoj de <frm am="K">K</frm>:
      <ekz>
        <ref cel="kardio.0o">kardioido</ref> estas
        <tld/>o de cirklo rilate al punkto de &gcirc;i.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_a;&c_sh;&c_v;&c_o;&c_v;&c_a;&c_ja; &c_k;&c_r;&c_y;&c_v;&c_a;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Fu&szlig;punkt[s]kurve</trd>,
    <trd>Fu&szlig;punktenkurve</trd>
  </trdgrp>
  <trd lng="en">pedal</trd>
  <trd lng="fr">podaire</trd>
  <trd lng="hu">talpponti <ind>g&ouml;rbe</ind></trd>
  <trdgrp lng="nl">
    <trd>voetpuntskromme</trd>,
    <trd>podaire</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>spodkowa</trd>,
    <trd>podera</trd></trdgrp>
  <trd lng="pt">hipocicl&oacute;ide</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_sh;&c_v;&c_je;&c_n;&c_n;&c_a;&c_ja; <ind>&c_k;&c_r;&c_i;&c_v;&c_a;&c_ja;</ind></trd>
</drv>
</art>

