<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: antipo.xml,v 1.17 2021/02/28 16:37:33 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>antipod</rad>/o
</kap>
<drv mrk="antipo.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Loko de la tero, ku&scirc;anta sub niaj piedoj sur la alia duonglobo,
      kaj diametre kontra&ubreve;a al la loko, sur kiu ni staras:
      <ekz>
        kiam estas tagmezo en iu loko, estas noktomezo en &gcirc;ia <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_t;&c_y;&c_p;&c_o;&c_d;</trd>,
    <trd>&c_d;&c_y;&c_ja;&c_m;&c_e;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a; &c_p;&c_r;&c_o;&c_c;&c_ib;&c_l;&c_je;&c_g;&c_l;&c_y; &c_p;&c_u;&c_n;&c_k;&c_t;</trd>
  </trdgrp>
  <trd lng="bg">&c_a;&c_n;&c_t;&c_i;&c_p;&c_o;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>antipod</trd>,
    <trd>protino&#x17E;ec</trd>,
    <trd>p&#x159;en. odp&#x16F;rce</trd>
  </trdgrp>
  <trdgrp lng="de">
       <trd>Gegenort</trd>,
       <trd>Antipode</trd>
  </trdgrp>
  <trd lng="en">antipode</trd>
  <trd lng="es">ant&iacute;poda</trd>
  <trd lng="fa">&f_p;&a_A;&a_d;&f_p;&a_A;</trd>
  <trd lng="fr">antipode</trd>
  <trdgrp lng="he">
    <trd>&#x5D0;&#x5E0;&#x5D8;&#x5D9;&#x5E4;&#x5D5;&#x5D3;</trd>,
    <trd>&#x5D4;&#x5E6;&#x5D3; &#x5D4;&#x5E9;&#x5E0;&#x5D9; &#x5E9;&#x5DC; &#x5DB;&#x5D3;&#x5D5;&#x5E8; &#x5D4;&#x5D0;&#x5E8;&#x5E5;</trd>
  </trdgrp>
  <trd lng="nl">antipode</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_p;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>antip&#xF3;d</trd>,
    <trd>protichodec</trd>,
    <trd>protino&#x17E;ec</trd>,
    <trd>protivn&#xED;k</trd>
  </trdgrp>
</drv>

<drv mrk="antipo.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">FIG</uzo>
  <snc>
    <dif>
      Tute kontra&ubreve;a:
      <ekz>
        <tld/>aj opinioj;
      </ekz>
      <ekz>
        nacio tute fremda kaj e&ccirc; <tld/>a por ni.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_t;&c_y;&c_p;&c_o;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_c;&c_ib;&c_l;&c_je;&c_g;&c_l;&c_y;</trd>,
    <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;&c_l;&c_je;&c_g;&c_l;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>antipodick&#xFD;</trd>,
    <trd>antipodn&#xED;</trd>,
    <trd>opa&#x10D;n&#xFD;</trd>,
    <trd>protich&#x16F;dn&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="de">
       <trd>gegens&auml;tzlich</trd>,
       <trd>gegen&uuml;berstehend</trd>,
       <trd>antipodisch</trd>,
       <trd>kontr&auml;r</trd>
  </trdgrp>
  <trd lng="en">antipodal</trd>
  <trd lng="fa">&a_m;&a_t;&a_D;&a_A;&a_d;</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_t;&c_i;&c_p;&c_o;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>antipodick&#xFD;</trd>,
    <trd>antipodi&#xE1;lny</trd>
  </trdgrp>
</drv>
</art>
