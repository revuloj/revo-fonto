<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: magi.xml,v 1.33 2022/03/23 14:01:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>magi</rad>/o <fnt><bib>FK</bib></fnt>
</kap>

<drv mrk="magi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="magi.0o.MAG">
    <dif>
      Arto, kiu pretendas fari mirinda&jcirc;ojn per misteraj, supernaturaj
      rimedoj:
      <ekz>
        <nom>Simon</nom> <klr>[&#x2026;]</klr> faradis <tld/>on en la urbo kaj
        sor&ccirc;adis la popolon de
        Samario
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 8:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi tuj vendis al li talismanon por defendi lin kontra&ubreve; mia
        <tld/>o
        <fnt><bib>SatirRak</bib>, <lok>la fluidumo de la
        &ccirc;iovido</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_ib;&c_ja;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_v;&c_ja;&c_d;&c_z;&c_mol;&c_m;&c_a;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>hudouriezh</trd>,
    <trd>strobinellerezh</trd>
  </trdgrp>
  <trd lng="ca">m&agrave;gia</trd>
  <trdgrp lng="cs">
    <trd>kouzelnictv&iacute;</trd>,
    <trd>magie</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zauberei</trd>,
    <trd>Magie</trd>
  </trdgrp>
  <trdgrp lng="el">
    <trd>&my;&alfa;&gamma;&epsilon;&jota_ton;&alfa;</trd>
  </trdgrp>
  <trd lng="es">magia</trd>
  <trdgrp lng="fa">
    <trd>&a_j;&a_A;&a_d;&a_w;</trd>,
    <trd>&a_s;&a_H;&a_r;</trd>,
    <trd>&a_A;&a_f;&a_s;&a_w;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">magie</trd>
  <trdgrp lng="hu">
    <trd>m&aacute;gia</trd>,
    <trd>var&aacute;zslat</trd>
  </trdgrp>
  <trd lng="id">sihir</trd>
  <trd lng="nl">magie</trd>
  <trdgrp lng="pt">
    <trd>magia</trd>,
    <trd>m&aacute;gica</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>magia</trd>,
    <trd>czarnoksi&eogonek;stwo</trd>,
    <trd>czary</trd>
  </trdgrp> 
  <trd lng="ro">magie</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_g;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_sh;&c_je;&c_b;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_d;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>k&uacute;zeln&iacute;ctvo</trd>,
    <trd>m&aacute;gia</trd>
  </trdgrp>
  <trd lng="sv">magi</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x433;&#x456;&#x44F;</trd>,
    <trd>&#x447;&#x430;&#x440;&#x456;&#x432;&#x43D;&#x438;&#x446;&#x442;&#x432;&#x43E;</trd>,
    <trd>&#x447;&#x430;&#x43A;&#x43B;&#x443;&#x43D;&#x441;&#x442;&#x432;&#x43E;</trd>
  </trdgrp>
</drv>

<drv mrk="magi.0a">
  <kap><tld/>a</kap>
  <snc mrk="magi.0a.MAG">
    <dif>
      Bazita sur <tld/>o, sor&ccirc;a:
      <ekz>
        ne malmultaj <klr>[&#x2026;]</klr> praktikis <tld/>ajn artojn
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 19:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi aran&gcirc;is &ccirc;irka&ubreve; vi <tld/>an rondon de malbenoj, kiun li ne
        povos transsalti
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        persono, inspiranta preska&ubreve; solenan timon pro siaj supozitaj
        <tld/>aj povoj
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 16a, heroa
        sinofero</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="magi.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Havanta malordinaran efikon:
      <ekz>
        tiuj paroloj faris <tld/>an impreson;
      </ekz>
      <ekz>
        magia lanterno, kiu per lumbildoj sur la malluma fono
        <klr>[&#x2026;]</klr> montras
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Fab3</bib>
          <lok>Dorna vojo de la honoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        en &scirc;ia koro kantis la <tld/>a vorto: li amas
        <fnt>
          <bib>IK</bib>
        </fnt>!
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> havis neniom el la <tld/>a potenco de Signe
        super mia koro
        <fnt><bib>Kiso</bib>, <lok>Signe</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_g;&c_ib;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_o;&c_w;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>hud<klr tip="ind"> (ag.)</klr></trd>,
    <trd>strobinellus</trd>
  </trdgrp>
  <trd lng="ca">m&agrave;gic</trd>
  <trdgrp lng="cs">
    <trd>kouzeln&yacute;</trd>,
    <trd>magick&yacute;</trd>,
    <trd>&ccaron;arovn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>zauberhaft</trd>,
    <trd>wunderbar</trd>,
    <trd>magisch</trd>
  </trdgrp>
  <trd lng="es">m&aacute;gico</trd>
  <trdgrp lng="fa">
    <trd>&a_j;&a_A;&a_d;&a_w;&f_y;&f_y;</trd>,
    <trd>&a_s;&a_H;&a_r;&a_A_madda;&a_m;&f_y;&a_z;</trd>
  </trdgrp>
  <trd lng="fr">magique</trd>
  <trdgrp lng="hu">
    <trd>m&aacute;gikus</trd>,
    <trd>var&aacute;zs-</trd>
  </trdgrp>
  <trd lng="id">magis</trd>
  <trd lng="nl">magisch</trd>
  <trdgrp lng="pl">
    <trd>magiczny</trd>,
    <trd>czarodziejski</trd>
  </trdgrp>
  <trd lng="pt">m&aacute;gico <klr>(adj.)</klr></trd>
  <trdgrp lng="ro">
    <trd>magic</trd>,
    <trd>magie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_sh;&c_je;&c_b;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>k&uacute;zeln&yacute;</trd>,
    <trd>magick&yacute;</trd>,
    <trd>&ccaron;arovn&yacute;</trd>
  </trdgrp>
  <trd lng="sv">magisk</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x433;&#x456;&#x447;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x447;&#x430;&#x440;&#x456;&#x432;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="magi.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="magi.0ajxo.MAG">
    <dif>
      <tld lit="M"/>a procedo, faro, ilo a&ubreve; efiko:
      <ekz>
        restu do &ccirc;e viaj <tld/>a&jcirc;oj, kaj &ccirc;e viaj multaj sor&ccirc;oj,
        super kiuj
        vi laboris de via juneco; eble vi povos profiti
        <fnt><bib>MT</bib>, <lok>&Jes; 47:12</lok></fnt>;
      </ekz>
      <ekz>
        tiu <klr>[&#x2026;]</klr> duonkantis misterajn <tld/>a&jcirc;ojn
        <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>blindaj &ccirc;uk&ccirc;oj, (du skizoj),
        unua
        skizo:, Filip Onkudimov, 2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>strobinell</trd>,
    <trd>tro-hud</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>encanteri</trd>,
    <trd>fetilleria</trd>,
    <trd>truc</trd>
  </trdgrp>
  <trd lng="fr">tour de magie</trd>
  <trd lng="nl">iets <ind>magisch</ind></trd>
  <trdgrp lng="pl">
    <trd>sztuczka <ind>magiczna</ind></trd>,
    <trd>zakl&eogonek;cie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>truc magic</trd>,
    <trd>desc&acirc;ntec</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_l;&c_i;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_je; &c_p;&c_a;&c_s;&c_s;&c_y;</trd>
  </trdgrp>
</drv>

<drv mrk="magi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="magi.0isto.MAG">
    <dif>
      Persono havanta <tld/>ajn kapablojn, spertulo en la <tld/>o:
      <ekz>
        ne a&ubreve;skultu do viajn profetojn, nek viajn <tld/>istojn, nek
        viajn son&gcirc;oklarigistojn, nek viajn a&ubreve;guristojn, nek viajn
        sor&ccirc;istojn
        <fnt><bib>MT</bib>, <lok>&Jer; 27:9</lok></fnt>;
      </ekz>
      <ekz>
        ne turnu vin al <tld/>istoj nek al sor&ccirc;istoj, ne provu malpuri&gcirc;i
        per ili
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 19:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bramano venis kaj faris &ccirc;ion, kion faras la aliaj
        <tld/>istoj, sed la diablo
        ne eliris
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_g;</trd>,
      <trd>&c_ch;&c_a;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_v;&c_ja;&c_d;&c_z;&c_mol;&c_m;&c_a;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="br">
      <trd>hudour</trd>,
      <trd>strobineller</trd>
    </trdgrp>
    <trd lng="ca">mag</trd>
    <trdgrp lng="de">
      <trd>Zauberer</trd>,
      <trd>Magier</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_j;&a_A;&a_d;&a_w;&f_g;&a_r;</trd>,
      <trd>&a_A;&a_f;&a_s;&a_w;&a_n;&f_g;&a_r;</trd>,
      <trd>&a_s;&a_A;&a_H;&a_r;</trd>
    </trdgrp>
    <trd lng="fr">magicien</trd>
    <trdgrp lng="hu">
      <trd>m&aacute;gus</trd>,
      <trd>var&aacute;zsl&oacute;</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>dukun</trd>,
      <trd>pe<ind>sulap</ind></trd>
    </trdgrp>
    <trd lng="id"><baz>sihir</baz>penyihir</trd>
    <trd lng="nl">tovenaar</trd>
    <trdgrp lng="pl">
      <trd>magik</trd>,
      <trd>czarodziej</trd>,
      <trd>czarnoksi&eogonek;&zdot;nik</trd>
    </trdgrp>   
    <trdgrp lng="ro">
      <trd>magician</trd>,
      <trd>vr&abreve;jitor</trd>,
      <trd>mag</trd>
    </trdgrp> 
    <trdgrp lng="ru">
      <trd>&c_m;&c_a;&c_g;</trd>,
      <trd>&c_v;&c_o;&c_l;&c_sh;&c_je;&c_b;&c_n;&c_i;&c_k;</trd>,
      <trd>&c_ch;&c_a;&c_r;&c_o;&c_d;&c_je;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="magi.0isto.TEA">
    <dif>
      <ref tip="dif" cel="iluzi.0isto.TEA">Iluziisto</ref>:
      <ekz>
        <nom>Elena</nom> estis <tld/>isto kiu povis elpreni
        kion ajn el sia &ccirc;apelo
          <fnt>
            <aut>T. Jansson, trad. A. Nordstr&ouml;m</aut>,
            <vrk><url ref="http://hem.bredband.net/andnor/lsg/lagranda.htm">
            La granda voja&gcirc;o</url></vrk>,
            <lok>[2010?]</lok>
          </fnt>. 
      </ekz>
    </dif>
    <trd lng="ca">il&middot;lusionista</trd>
    <trd lng="pl">magik</trd>     
    <trd lng="ro">mag</trd>
  </snc>
  <trd lng="ca">mag</trd>
  <trdgrp lng="cs">
    <trd>kouzeln&iacute;k</trd>,
    <trd>&ccaron;arod&ecaron;j</trd>
  </trdgrp>
  <trd lng="sk">&ccaron;arodejn&iacute;k</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x433;</trd>,
    <trd>&#x447;&#x430;&#x440;&#x456;&#x432;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x447;&#x430;&#x43A;&#x43B;&#x443;&#x43D;</trd>
  </trdgrp>
</drv>

<drv mrk="magi.blanka0o">
  <kap>blanka <tld/>o</kap>
  <snc mrk="magi.blanka0o.MAG">
    <dif>
      <tld lit="M"/>o uzata por bono, feli&ccirc;igo, bon&scirc;anco:
      <ekz>
        sacerdotino, kiu praktikas parte nur blankan <tld/>on, parte nur
        nigran <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=532291">Mambo (sacerdotino)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_je;&c_l;&c_a;&c_ja; <ind>&c_m;&c_a;&c_g;&c_ib;&c_ja;</ind></trd>
  <trd lng="ca">m&agrave;gia blanca</trd>
  <trdgrp lng="cs">
    <trd>b&iacute;l&aacute; magie</trd>,
    <trd>trikov&eacute; kouzlen&iacute;</trd>
  </trdgrp>
  <trd lng="de">Wei&szlig;magie</trd>
  <trd lng="fa">&a_j;&a_A;&a_d;&a_w;&f_y; &a_s;&a_f;&f_y;&a_d;</trd>
  <trd lng="fr">magie blanche</trd>
  <trd lng="id"><ind>ilmu</ind> putih</trd>
  <trd lng="nl">witte <ind>magie</ind></trd>
  <trd lng="pl">bia&lstroke;a <ind>magia</ind></trd>     
  <trd lng="ro">magie alb&abreve;</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_a;&c_ja; &c_m;&c_a;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">biela m&aacute;gia</trd>
  <trd lng="uk">&#x431;&#x456;&#x43B;&#x430; &#x43C;&#x430;&#x433;&#x456;&#x44F;</trd>
</drv>

<drv mrk="magi.nigra0o">
  <kap>nigra <tld/>o</kap>
  <snc mrk="magi.nigra0o.MAG">
    <dif>
      <tld lit="M"/>o uzata por malbono, malsaniga, malprosperiga,
      mortiga:
      <ekz>
        &ccirc;io &ccirc;i tio <klr>[&#x2026;]</klr> apartenas al la tiel nomata nigra
        <tld/>o
        <fnt><bib>Fab2</bib>, <lok>La najtingalo</lok></fnt>!
      </ekz>
      <ekz>
        &scirc;ia nigra <tld/>a&jcirc;o jam malka&scirc;i&gcirc;is
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 81a, volumo 3a, p.
        7a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_o;&c_r;&c_n;&c_a;&c_ja; <ind>&c_m;&c_a;&c_g;&c_ib;&c_ja;</ind></trd>
  <trd lng="ca">m&agrave;gia negra</trd>
  <trdgrp lng="de">
    <trd>Schwarzmagie</trd>,
    <trd>schwarze Magie</trd>
  </trdgrp>
  <trd lng="fa">&a_j;&a_A;&a_d;&a_w;&f_y; &a_s;&f_y;&a_A;&a_h;</trd>
  <trd lng="fr">magie noire</trd>
  <trd lng="id"><ind>ilmu</ind> hitam</trd>
  <trd lng="nl">zwarte <ind>magie</ind></trd>
  <trd lng="pl">czarna <ind>magia</ind></trd>     
  <trd lng="ro">magie neagr&abreve;</trd>
  <trd lng="ru">&c_ch;&c_jo;&c_r;&c_n;&c_a;&c_ja; &c_m;&c_a;&c_g;&c_i;&c_ja;</trd>
  <trd lng="uk">&#x447;&#x43E;&#x440;&#x43D;&#x430; &#x43C;&#x430;&#x433;&#x456;&#x44F;</trd>
</drv>
</art>
