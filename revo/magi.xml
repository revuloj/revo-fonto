<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>magi</rad>/o
</kap>
<drv mrk="magi.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Arto, kiu pretendas fari mirinda&jcirc;ojn per supernaturaj rimedoj:
      <ekz>
        <tld/>isto<fnt>Z</fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Zauberei</trd>,
     <trd>Magie</trd>
  </trdgrp>
  <trd lng="es">magia</trd>
  <trd lng="fr">magie</trd>
  <trdgrp lng="hu">
     <trd>m&aacute;gia</trd>,
     <trd>var&aacute;zslat</trd>
  </trdgrp>
  <trd lng="nl">magie</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_g;&c_i;&c_ja;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_sh;&c_je;&c_b;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_d;&c_o;&c_v;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
  <trd lng="sv">magi</trd>
</drv>

<drv mrk="magi.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Persono havanta magiajn kapablojn, spertulo en la magio.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Zauberer</trd>,
     <trd>Magier</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>m&aacute;gus</trd>,
     <trd>var&aacute;zsl&oacute;</trd>
  </trdgrp>
</drv>

<drv mrk="magi.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Bazita sur <tld/>o, sor&ccirc;a:
      <ekz>
        <tld/>a povo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Havanta eksterordinaran efikon:
      <ekz>
        tiuj paroloj faris <tld/>an impreson
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sorcx.0a">sor&ccirc;a</ref>,
      <ref cel="fe">fea</ref>,
      <ref cel="rav">rava</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="lanter.0o">lanterno</ref>.
    </refgrp>
  </snc>
  <trd lng="de">magisch</trd>
  <trdgrp lng="de">
     <trd>zauberhaft</trd>,
     <trd>wunderbar</trd>,
     <trd>magisch</trd>
  </trdgrp>
  <trd lng="es">m&aacute;gico</trd>
  <trd lng="fr">magique</trd>
  <trdgrp lng="hu">
     <trd>m&aacute;gikus</trd>,
     <trd>var&aacute;zs-</trd>
  </trdgrp>
  <trd lng="nl">magisch</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_a;&c_g;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_o;&c_l;&c_sh;&c_je;&c_b;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">magisk</trd>
</drv>
</art>
