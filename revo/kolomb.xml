<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolomb.xml,v 1.49 2022/10/01 22:35:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kolomb</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kolomb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kolomb.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="kolomb1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Columba</trd>), 
      <ref tip="lst" cel="bird.0o" lst="voko:birdoj">birdo</ref> kun
      gor&gcirc;opo&scirc;o el kiu estas komence nutrataj la idoj:
      <ekz>
        <tld/>o estas birdo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Libianoj estis teruritaj, kiel la <tld/>oj, super kiuj
        rondflugas akcipitro
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi donos al vi <klr>[&#8230;]</klr> ka&gcirc;on kun <tld/>oj
        <klr>[&#8230;]</klr> &ccirc;iufoje kiam vi rememoros vian servistinon, 
        malfermu la ka&gcirc;on kaj ellasu unu birdon... &gcirc;i alportos al mi
        nova&jcirc;ojn de vi
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kolomboj, &gcirc;i diris, estas e&ccirc; grandaj, blankaj kaj briletantaj
        kiel perlamoto <klr>[&#8230;]</klr> sed ili estas dikaj kaj malsa&gcirc;aj
        <fnt>
          <bib>Fab4</bib>
          <lok>Korta koko kaj ventkoko</lok>
        </fnt>;
      </ekz>
      <ekz>
        kropo de <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arbaraj <tld/>oj diris: kurre, kurre!
        <fnt>
          <bib>Fab2</bib>
          <lok>Fabelo en sep rakontoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arbaraj <tld/>oj kveris
        <fnt>
          <bib>Fab2</bib>
          <lok>Fabelo en sep rakontoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        pac<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki LINS</aut>
          <vrk><url ref="&Monato;/2004/008364.html">Plej
          malforta futbalnacio</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        la blanka <tld/>o de senkulpeco flugetadis super la &ccirc;efaltaro.
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o, &ccirc;apitro 4a</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_l;&c_u;&c_b;</trd>
  <trd lng="bg">&c_g;&c_malmol;&c_l;&c_malmol;&c_b;</trd>
  <trdgrp lng="cs">
    <trd>holub</trd>,
    <trd>holub <klr>k.a.</klr></trd>
  </trdgrp>
  <trd lng="de">Taube</trd>
  <trd lng="en">dove</trd>
  <trd lng="fa">&f_k;&a_b;&a_w;&a_t;&a_r;</trd>
  <trdgrp lng="fr">
    <trd>colombe</trd>,
    <trd>pigeon</trd>
  </trdgrp>
  <trd lng="hu">galamb</trd>
  <trdgrp lng="id">
    <trd>dara</trd>,
    <trd>merpati</trd>
  </trdgrp>
  <trd lng="nl">duif</trd>
  <trd lng="pl">go&lstroke;&aogonek;b</trd>
  <trdgrp lng="pt">
    <trd>pombo</trd>,
    <trd>pomba</trd>
  </trdgrp>
  <trd lng="ro">porumbel</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_u;&c_b;&c_mol; <klr>k.a.</klr></trd>
  <trd lng="sk">holub</trd>
</drv>

<drv mrk="kolomb.K0o">
  <kap><tld lit="K"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Col</mlg>
  <snc mrk="kolomb.K0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera 
      <ref tip="lst" cel="stel.0figuro" lst="voko:stelfiguroj">stelfiguro</ref> 
      (<trd lng="la">Columba</trd>):
      <ekz>
        NGC 1800 estas galaksio en konstelacio Kolombo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=111637">NGC
          1800</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_o;&c_l;&c_u;&c_b; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#8217;&c_je;)</klr></trd>
  <trd lng="cs">holub</trd>
  <trd lng="de">Taube</trd>
  <trd lng="en">Columba</trd>
  <trd lng="es">Paloma</trd>
  <trd lng="fa">&f_k;&a_b;&a_w;&a_t;&a_r;<klr tip="ind"> (&a_S;&a_w;&a_r;&a_t;
  &a_f;&a_l;&f_k;&f_y;)</klr></trd>
  <trd lng="fr">Colombe<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="nl">Duif<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trd lng="pl">Go&lstroke;&aogonek;b  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Porumbelul  <klr>(constela&#539;ie)</klr></trd>
  <trd lng="sk">holub</trd>
  <trd lng="uk">&c_g;&c_o;&c_l;&c_u;&c_b;</trd>
</drv>

<drv mrk="kolomb.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kolomb.0umi.ZOO">
    <dif>
      A&ubreve;digi la karesan murmuron, karakterizan
      de la <tld/>oj kaj turtoj.
    </dif>
    <refgrp tip="super">
      <ref cel="blek.0i.beste">bleki</ref>
    </refgrp>
    <trd lng="fa">&a_b;&a_gh;&#8204;&a_b;&a_gh;&a_w; &f_k;&a_r;&a_d;&a_n;</trd>
    <trd lng="nl">koeren</trd>
  </snc>
  <snc mrk="kolomb.0umi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="am.0indumi">Amindume</ref> interparoli:
      <ekz>
        la fajra spirito disvolvi&gcirc;is, <klr>[&#8230;]</klr> vidu tiun &ccirc;i molecon, kiel
        dorloteme &gcirc;i <tld/>umas por koketulinoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas vero, ke Francisko ne fleksas sin anta&ubreve; vi kiel <tld/>umanta Celadono
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fa"><klr tip="amb">(&a_ayn;&a_A;&a_sh;&a_q;&a_A;&a_n;&a_h;) </klr>&a_r;&a_A;&a_z;&a_w;&a_n;&f_y;&a_A;&a_z;
    &f_k;&a_r;&a_d;&a_n;</trd>
    <trd lng="nl">kirren</trd>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_r;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">vrkat a l&iacute;sat se k sob&ecaron; jako holoubci</trd>
  <trd lng="de">turteln</trd>
  <trd lng="en">coo</trd>
  <trd lng="fr">roucouler</trd>
  <trd lng="hu">turb&eacute;kol</trd>
  <trd lng="id">men<ind>dekut</ind></trd>
  <trd lng="pl">grucha&cacute;</trd>
  <trd lng="ro">g&acirc;nguri</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">hrk&uacute;ta&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_o;&c_r;&c_k;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_t;&c_zh; &c_p;&c_je;&c_r;&c_je;&c_n;.)</trd>,
    <trd>&c_v;&c_o;&c_r;&c_k;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_u;&c_r;&c_k;&c_o;&c_t;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="kolomb.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="kolomb.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Columbae</trd>):
      <ekz>
        la ordo Kolomboformaj <klr>[&#8230;]</klr> enhavas la didon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=218653">Rafedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">m&ecaron;kkozob&iacute;</trd>
  <trd lng="de">Taubenv&ouml;gel</trd>
  <trd lng="fa">&f_k;&a_b;&a_w;&a_t;&a_r;&a_s;&a_A;&a_n;&a_A;&a_n;</trd>
  <trd lng="fr">columbiformes</trd>
  <trd lng="hu">galambalak&uacute;ak</trd>
  <trd lng="nl">duiven<klr tip="ind"> (orde)</klr></trd>
  <trd lng="pl">go&lstroke;&eogonek;biowe</trd>
  <trd lng="ro">columbiformes </trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_u;&c_b;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
  <trd lng="sk">holuby</trd>
  <trd lng="uk">&c_g;&c_o;&c_l;&c_u;&c_b;&c_o;&c_p;&c_o;&c_d;&c_ib;&c_b;&c_n;&c_ib;</trd>
</drv>

<drv mrk="kolomb.dom0o">
  <kap>
    dom<tld/>o,
    <var><kap>hejm<tld/>o</kap></var>
  </kap>
  <snc mrk="kolomb.dom0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <ref tip="super" cel="bird.korto0o">Kortbirdo</ref> 
      devenanta de la
      <ref tip="vid" cel="kolomb.rok0o">rok<tld/>o</ref>,
      uzata interalie kiel leter<tld/>o (<trd lng="la">Columba livia
      domestica</trd>), multe renkontata en urboj.
    </dif>
  </snc>
  <bld lok="../bld/kolomb.jpg">
    dom<tld/>o
  </bld>
  <trd lng="be">&c_g;&c_o;&c_l;&c_u;&c_b; &c_s;&c_v;&c_o;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">Haustaube</trd>
  <trd lng="en">pigeon</trd>
  <trd lng="fa">&f_k;&a_b;&a_w;&a_t;&a_r; &a_kh;&a_A;&a_n;&f_g;&f_y;</trd>
  <trd lng="fr">pigeon domestique</trd>
  <trd lng="hu">h&aacute;zi <ind>galamb</ind></trd>
  <trd lng="nl">huisduif</trd>
  <trd lng="pl">go&lstroke;&aogonek;b domowy</trd>
  <trd lng="ro">porumbelul de cas&abreve;</trd>
  <trd lng="pt">pombo</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j; <ind>&c_g;&c_o;&c_l;&c_u;&c_b;&c_mol;</ind></trd>
  <trd lng="uk">&c_g;&c_o;&c_l;&c_u;&c_b; &c_s;&c_v;&c_ib;&c_j;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="kolomb.leter0o">
  <kap>leter<tld/>o,
    <var><kap>mesa&gcirc;<tld/>o</kap></var>,
    <var><kap>kurier<tld/>o</kap></var>
  </kap>
  <snc mrk="kolomb.leter0o.POSX">
    <uzo tip="fak">POSX</uzo>
    <dif>
      Dom<tld/>o uzata por transporti <ref cel="leter.0o">leterojn</ref>
      reflugante hejmen:
      <ekz>
        &scirc;ajnis ke legio da mesa&gcirc;<tld/>oj preterpasas super
        niaj tegmentoj 
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>;
      </ekz>
      <ekz>
        la internacia federacio de bredistoj de kurier<tld/>oj, kun
        membroj en 56 landoj, okazigas internacian olimpiadon &ccirc;iun jaron
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2013/010979.php">Aera
          kuriero</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>mi sentis min kiel rehejmi&gcirc;anta leter<tld/>o
        <fnt>
          <aut>Charlotte Bront&euml;, trad. H. J. Bulthuis</aut>
          <vrk>Jane Eyre</vrk>
          <lok>Dua parto, &ccirc;apitro 36a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_sh;&c_t;&c_o;&c_v;&c_y; <ind>&c_g;&c_o;&c_l;&c_u;&c_b;</ind></trd>
  <trd lng="cs">po&scaron;tovn&iacute; holub</trd>
  <trd lng="de">Brieftaube</trd>
  <trdgrp lng="en">
    <trd>messenger <ind>pigeon</ind></trd>,
    <trd>carrier <ind>pigeon</ind></trd>,
  </trdgrp>
  <trd lng="fa">&f_k;&a_b;&a_w;&a_t;&a_r; &a_n;&a_A;&a_m;&a_h;&#8204;&a_b;&a_r;</trd>
  <trd lng="fr">pigeon voyageur</trd>
  <trd lng="hu">postagalamb</trd>
  <trd lng="id"><ind>merpati</ind> pos</trd>
  <trd lng="nl">postduif</trd>
  <trd lng="pl">go&lstroke;&aogonek;b pocztowy</trd>
  <trd lng="ro">porumbel voiajor</trd>
  <trd lng="ru">&c_p;&c_o;&c_ch;&c_t;&c_o;&c_v;&c_y;&c_j;
  <ind>&c_g;&c_o;&c_l;&c_u;&c_b;&c_mol;</ind></trd>
  <trd lng="sk">po&scaron;tov&yacute; holub</trd>
  <trd lng="uk">&c_p;&c_o;&c_sh;&c_t;&c_o;&c_v;&c_i;&c_j; &c_g;&c_o;&c_l;&c_u;&c_b;</trd>
</drv>

<drv mrk="kolomb.ringo0o">
  <kap>ringo<tld/>o</kap>
  <snc mrk="kolomb.ringo0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">Specio</ref>
       de <ref tip="lst" cel="kolomb.0o" lst="voko:birdoj">kolombo</ref>
      (<trd lng="la">Columba palumbus</trd>), kun blankaj kolmakuloj kaj
      blankaj flugilrandoj:
      <ekz>
        la ring<tld/>o a&ubreve; palumbo <klr>[&#8230;]</klr> havas rozkoloran
        bruston kaj 
        verdajn nuancoj en la strioj super a kolmakuloj, kiuj vere ne
        formas 
        kompletan ringon spite la nomon de la specio, la ringokulo estas
        flava 
        (kiel la bekopinto), dum tiu de la rok<tld/>o oran&gcirc;a kaj tiu de
        la trukolombo hela kaj mallar&gcirc;ega
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17708">Palumbo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="palumb.0o">palumbo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_h;&c_ib;&c_r;</trd>,
    <trd>&c_g;&c_o;&c_l;&c_u;&c_b; &c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>h&rcaron;ivn&aacute;&ccaron; obecn&yacute;</trd>,
    <trd>h&rcaron;ivn&aacute;&ccaron;</trd>
  </trdgrp>
  <trd lng="bg">&c_g;&c_r;&c_i;&c_v;&c_ja;&c_k;</trd>
  <trd lng="de">Ringeltaube</trd>
  <trd lng="en">wood-pigeon</trd>
  <trd lng="fa">&f_k;&a_b;&a_w;&a_t;&a_r; &a_j;&a_n;&f_g;&a_l;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>palombe</trd>,
    <trd>pigeon ramier</trd>,
    <trd>ramier</trd>
  </trdgrp>
  <trd lng="hu">&ouml;rv&ouml;s <ind>galamb</ind></trd>
  <trd lng="nl">houtduif</trd>
  <trd lng="pl">grzywacz</trd>
  <trd lng="ro">porumbel gulerat</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_ja;&c_h;&c_i;&c_r;&c_mol;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_ju;&c_t;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hrivn&aacute;k</trd>,
    <trd>hrivn&aacute;&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_o;&c_l;&c_u;&c_b; &c_l;&c_ib;&c_s;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_p;&c_u;&c_t;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kolomb.rok0o">
  <kap>rok<tld/>o</kap>
  <snc mrk="kolomb.rok0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">Specio</ref>
       de <ref tip="lst" cel="kolomb.0o" lst="voko:birdoj">kolombo</ref>
      (<trd lng="la">Columba livia</trd>), kun blanka dorso:
      <ekz>
        la leter<tld/>o estas <tld/>o kiu utilas por transporti leterojn
        a&ubreve; mesa&gcirc;ojn 
        profitante la kapablon de <tld/>oj reveni al siaj <tld/>ejoj
        <klr>[ili]</klr> kutime estas hejm<tld/>oj devenantaj el la
        origina specio Rok<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=55653">Leterkolombo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="livi.0o">livio</ref>
  </snc>
  
  <trdgrp lng="cs">
    <trd>holub skalni</trd>,
    <trd>holub skaln&iacute;</trd>
  </trdgrp>
  <trd lng="de">Felsentaube</trd>
  <trd lng="en">rock-dove</trd>
  <trd lng="fa">&f_k;&a_b;&a_w;&a_t;&a_r; &f_ch;&a_A;&a_h;&f_y;</trd>
  <trd lng="fr">pigeon biset</trd>
  <trd lng="hu">szirti <ind>galamb</ind></trd>
  <trd lng="nl">rotsduif</trd>
  <trd lng="pl">go&lstroke;&aogonek;b skalny</trd>
  <trd lng="ro">porumbel</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_z;&c_y;&c_j;
