<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kolomb</rad>/o
</kap>
<drv mrk="kolomb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="kolomb.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Columba</trd>), kun gor&gcirc;opo&scirc;o el kiu estas
      komence nutrataj la idoj.
    </dif>
  </snc>
  <trd lng="cs">holub <klr>k.a.</klr></trd>
  <trd lng="de">Taube</trd>
  <trd lng="en">dove</trd>
  <trd lng="hu">galamb</trd>
  <trd lng="pl">go&lstroke;&aogonek;b <klr>k.a.</klr></trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_u;&c_b;&c_mol; <klr>k.a.</klr></trd>
</drv>
<drv mrk="kolomb.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      A&ubreve;digi la karesan murmuron, karakterizan de la <tld/>oj; kveri.
    </dif>
  </snc>
  <snc>
    <uzo tip="klr">(f)</uzo>
    <dif>
      Amindume interparoli.
    </dif>
    <refgrp tip="sin">
      <ref cel="rukul.0i">rukuli</ref>.
    </refgrp>
  </snc>
  <trd lng="de">turteln</trd>
</drv>
<drv mrk="kolomb.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="kolomb.0oformaj"><tld/>oformaj birdoj</ref>
      (<trd lng="la">Columbidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Tauben <klr tip="ind">(Familie)</klr></trd>
</drv>
<drv mrk="kolomb.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Columbae</trd>).
    </dif>
  </snc>
  <trd lng="de">Taubenv&ouml;gel</trd>
</drv>
<drv mrk="kolomb.dom0o">
  <kap>dom<tld/>o</kap>
  <snc>
    <dif>
      Kortbirdo devenanta de la
      <ref tip="vid" cel="kolomb.rok0o">rok<tld/>o</ref>,
      uzata interalie kiel leter<tld/>o.
    </dif>
  </snc>
  <trd lng="de">Haustaube</trd>
</drv>
<drv mrk="kolomb.leter0o">
  <kap>leter<tld/>o</kap>
  <snc>
    <dif>
      Dom<tld/>o uzata por transporti leterojn reflugante hejmen.
    </dif>
  </snc>
  <trd lng="de">Brieftaube</trd>
</drv>
<drv mrk="kolomb.rok0o">
  <kap>rok<tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="kolomb.0o">kolombo</ref>
      (<trd lng="la">Columba livia</trd>), kun blanka dorso. 
    </dif>
    <ref tip="sin" cel="livi.0o">livio</ref>
  </snc>
  <trd lng="cs">holub skalni</trd>
  <trd lng="de">Felsentaube</trd>
  <trd lng="en">rock-dove</trd>
  <trd lng="hu">szirti galamb</trd>
  <trd lng="pl">go&lstroke;&aogonek;b skalny</trd>
  <trd lng="ru">&c_s;&c_i;&c_z;&c_y;&c_j;
    &c_g;&c_o;&c_l;&c_u;&c_b;&c_mol;</trd>
</drv>
</art>
