<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kolomb</rad>/o
</kap>
<drv mrk="kolomb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kolomb.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="kolomb1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Columba</trd>), kun gor&gcirc;opo&scirc;o el kiu estas
      komence nutrataj la idoj.
    </dif>
  </snc>
  <trd lng="cs">holub <klr>k.a.</klr></trd>
  <trd lng="de">Taube</trd>
  <trd lng="en">dove</trd>
  <trdgrp lng="fr">
    <trd>colombe</trd>,
    <trd>pigeon</trd>
  </trdgrp>
  <trd lng="hu">galamb</trd>
  <trd lng="nl">duif</trd>
  <trd lng="pl">go&lstroke;&aogonek;b</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_u;&c_b;&c_mol; <klr>k.a.</klr></trd>
</drv>

<drv mrk="kolomb.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kolomb.0umi.ZOO">
    <dif>
      A&ubreve;digi la karesan murmuron, karakterizan de la <tld/>oj kaj turtoj.
    </dif>
  </snc>
  <snc mrk="kolomb.0umi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Amindume interparoli.
    </dif>
  </snc>
  <trd lng="de">turteln</trd>
  <trd lng="en">coo</trd>
  <trd lng="fr">roucouler</trd>
  <trd lng="hu">turb&eacute;kol</trd>
  <trd lng="nl">koeren</trd>
  <trd lng="pl">grucha&cacute;</trd>
  <trd lng="ru">&c_v;&c_o;&c_r;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="kolomb.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="kolomb.0oformaj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Columbae</trd>).
    </dif>
  </snc>
  <trd lng="de">Taubenv&ouml;gel</trd>
  <trd lng="fr">columbiformes</trd>
  <trd lng="hu">galambalak&uacute;ak</trd>
  <trd lng="nl">duiven<klr tip="ind"> (orde)</klr></trd>
  <trd
  lng="ru">&c_g;&c_o;&c_l;&c_u;&c_b;&c_je;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;</trd>
</drv>

<drv mrk="kolomb.dom0o">
  <kap>dom<tld/>o</kap>
  <snc mrk="kolomb.dom0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Kortbirdo devenanta de la
      <ref tip="vid" cel="kolomb.rok0o">rok<tld/>o</ref>,
      uzata interalie kiel leter<tld/>o (<trd lng="la">Columba livia
      domestica</trd>), multe renkontata en urboj.
    </dif>
  </snc>
  <bld lok="../bld/kolomb.jpg">
    dom<tld/>o
  </bld>
  <trd lng="de">Haustaube</trd>
  <trd lng="en">pigeon</trd>
  <trd lng="fr">pigeon domestique</trd>
  <trd lng="hu">h&aacute;zi <ind>galamb</ind></trd>
  <trd lng="nl">huisduif</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_i;&c_j;
  <ind>&c_g;&c_o;&c_l;&c_u;&c_b;&c_mol;</ind></trd>
</drv>

<drv mrk="kolomb.leter0o">
  <kap>leter<tld/>o</kap>
  <snc mrk="kolomb.leter0o.POSX">
    <uzo tip="fak">POSX</uzo>
    <dif>
      Dom<tld/>o uzata por transporti leterojn reflugante hejmen.
    </dif>
  </snc>
  <trd lng="de">Brieftaube</trd>
  <trdgrp lng="en">
    <trd>messenger <ind>pigeon</ind></trd>,
    <trd>carrier <ind>pigeon</ind></trd>,
  </trdgrp>
  <trd lng="fr">pigeon voyageur</trd>
  <trd lng="hu">postagalamb</trd>
  <trd lng="nl">postduif</trd>
  <trd lng="pl">go&lstroke;&aogonek;b pocztowy</trd>
  <trd lng="ru">&c_p;&c_o;&c_ch;&c_t;&c_o;&c_v;&c_y;&c_j;
  <ind>&c_g;&c_o;&c_l;&c_u;&c_b;&c_mol;</ind></trd>
</drv>

<drv mrk="kolomb.ringo0o">
  <kap>ringo<tld/>o</kap>
  <snc mrk="kolomb.ringo0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="kolomb.0o">kolombo</ref>
      (<trd lng="la">Columba palumbus</trd>), kun blankaj kolmakuloj kaj
      blankaj flugilrandoj.
    </dif>
    <ref tip="sin" cel="palumb.0o">palumbo</ref>
  </snc>
  <trd lng="cs">h&rcaron;ivn&aacute;&ccaron; obecn&yacute;</trd>
  <trd lng="de">Ringeltaube</trd>
  <trd lng="en">wood-pigeon</trd>
  <trdgrp lng="fr">
    <trd>palombe</trd>,
    <trd>pigeon ramier</trd>,
    <trd>ramier</trd>
  </trdgrp>
  <trd lng="hu">&ouml;rv&ouml;s <ind>galamb</ind></trd>
  <trd lng="nl">houtduif</trd>
  <trd lng="pl">grzywacz</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_ja;&c_h;&c_i;&c_r;&c_mol;</trd>,
    <trd>&c_v;&c_i;&c_t;&c_ju;&c_t;&c_je;&c_n;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kolomb.rok0o">
  <kap>rok<tld/>o</kap>
  <snc mrk="kolomb.rok0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Specio de <ref tip="super" cel="kolomb.0o">kolombo</ref>
      (<trd lng="la">Columba livia</trd>), kun blanka dorso.
    </dif>
    <ref tip="sin" cel="livi.0o">livio</ref>
  </snc>
  <trd lng="cs">holub skalni</trd>
  <trd lng="de">Felsentaube</trd>
  <trd lng="en">rock-dove</trd>
  <trd lng="fr">pigeon biset</trd>
  <trd lng="hu">szirti <ind>galamb</ind></trd>
  <trd lng="nl">rotsduif</trd>
  <trd lng="pl">go&lstroke;&aogonek;b skalny</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_z;&c_y;&c_j;
    <ind>&c_g;&c_o;&c_l;&c_u;&c_b;&c_mol;</ind></trd>,
    <trd><ind>&c_g;&c_o;&c_l;&c_u;&c_b;&c_mol;</ind>
    &c_o;&c_b;&c_y;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd><ind>&c_g;&c_o;&c_l;&c_u;&c_b;&c_mol;</ind>
    &c_p;&c_o;&c_l;&c_je;&c_v;&c_o;&c_j;</trd>
  </trdgrp>
</drv>
</art>
