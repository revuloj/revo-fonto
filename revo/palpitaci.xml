<art mrk="$Id: palpitaci.xml,v 1.9 2022/04/19 14:40:26 revo Exp $">
<kap>
  <rad>palpitaci</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="palpitaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="palpitaci.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      &Gcirc;ena momenta sentado de propraj korbatoj,
      plej ofte sendan&gcirc;era:
      <ekz>
        &scirc;i sciis, ke tio donis al &scirc;i la <tld/>ojn
        <fnt>
          <aut>J. Joyce, trad. J. Wilson</aut>,
          <vrk><url ref="http://www.geocities.com/galimatio/eveline1.html">
          Eveline</url></vrk>,
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <klr>...</klr> ne plu suferis angor-atakojn
        nek premantajn <tld/>ojn
        <fnt><aut>H. Hesse, trad. D. Karthaus</aut>, <vrk>Demian</vrk>, 
        <lok>2007</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">palpitaci&oacute;</trd>
  <trd lng="cs">palpitace</trd>
  <trd lng="de">Palpitation</trd>
  <trd lng="en">palpitation</trd>
  <trd lng="fr">palpitation<klr> (cardiaque)</klr></trd>
  <trdgrp lng="pl">
    <trd>palpitacja</trd>,
    <trd>ko&lstroke;atanie serca</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>palpita&#539;ie</trd>,
    <trd>b&abreve;taie de inim&abreve;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>b&uacute;chanie srdca</trd>,
    <trd>palpit&aacute;cia</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_je;&c_r;&c_c;&c_je;&c_b;&c_i;&c_t;&c_t;&c_ja;</trd>,
    <trd>&c_p;&c_u;&c_l;&c_mol;&c_s;&c_a;&c_c;&c_ib;&c_ja;</trd>
  </trdgrp>
</drv>
</art>

