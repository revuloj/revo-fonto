<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>mort</rad>/i
    </kap>

    <drv mrk="mort.0i">
      <kap><ofc>*</ofc><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
	<dif>
	  &Ccirc;esi vivi, perdi la vivon:
	  <ekz>
	    ni &ccirc;iuj <tld/>as;
	  </ekz><ekz>
	    <tld/>i pro akcidento;
	  </ekz><ekz>
	    <tld/>intaj folioj.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="pere.0i">perei</ref>,
	  <ref cel="velk.0i">velki</ref>.
	</refgrp>
	<trdgrp lng="be">
	  <trd>&c_p;&c_a;&c_m;&c_je;&c_r;&c_c;&c_ib;</trd>,
	  <trd>&c_z;&c_a;&c_g;&c_ib;&c_n;&c_u;&c_c;&c_mol;</trd>
	</trdgrp>
      </snc>
      <snc>
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Ne plu esti, malesti&gcirc;i, ekstermi&gcirc;i, forpasi, fini&gcirc;i,
	  ne plu funkcii:
	  <ekz>
	    multaj antikvaj civilizoj <tld/>is;
	  </ekz><ekz>
	    jam <tld/>is la printempo, mia juneco;
	  </ekz><ekz>
	    tiu ideo ne povas <tld/>i;
	  </ekz><ekz>
	    rigardi la balda&ubreve; <tld/>ontan tagon;
	  </ekz><ekz>
	    sed tiu brueto balda&ubreve; for<tld/>is.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="detru.0igxi">detrui&gcirc;i</ref>,
	  <ref cel="dron.0i">droni</ref>,
	  <ref cel="visx">elvi&scirc;i&gcirc;i</ref>,
	  <ref cel="esting">estingi&gcirc;i</ref>,
	  <ref cel="pere.0i">perei</ref>,
	  <ref cel="ruin">ruini&gcirc;i</ref>
	</refgrp>
	<trdgrp lng="be">
	  <trd>&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>,
	  <trd>&c_s;&c_k;&c_o;&c_n;&c_ch;&c_y;&c_c;&c_c;&c_a;</trd>
	</trdgrp>
      </snc>
      <trd lng="cs">zem&rcaron;&iacute;t</trd>
      <trd lng="en">die</trd>
      <trd lng="fr">mourir</trd>
      <trd lng="nl">sterven</trd>
      <trd lng="ru">&c_u;&c_m;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>
      <trd lng="sv">d&ouml;</trd>
    </drv>

    <drv mrk="mort.0o">
      <kap><tld/>o</kap>
      <snc>
	<dif>
	  Perdo de la vivo:
	  <ekz>
	    trofrua <tld/>o;
	  </ekz><ekz>
	    kondamni al <tld/>o;
	  </ekz><ekz>
	    &gcirc;is<tld/>e batali;
	  </ekz><ekz>
	    <ind><tld/>omerita</ind> peko<fnt>Z</fnt>.
	    <trd lng="fr">mortel<klr tip="ind"> (qui m&eacute;rite la mort)</klr>
	    </trd>
	    <trd lng="nl">dood-</trd>
	  </ekz>
	</dif>
      </snc>
      <snc>
	<dif>
	  Simbola esta&jcirc;o, personigo de la <tld/>o:
	  <ekz>
	    la <tld/>o ne distingas, &ccirc;iujn egale atingas
	    <fnt><bib>PrV</bib></fnt>.
	  </ekz>
	</dif>
      </snc>
      <snc>
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Ekstermi&gcirc;o, detrui&gcirc;o, ruini&gcirc;o, malapero:
	  <ekz>
	    <tld/>o de Kartago;
	  </ekz><ekz>
	    <tld/>o de l' arto.
	  </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_s;&c_mol;&c_m;&c_je;&c_r;&c_c;&c_mol;</trd>
      <trd lng="fr">mort</trd>
      <trd lng="nl">dood<klr tip="ind"> (z.nw.)</klr></trd>
      <trd lng="ru">&c_s;&c_m;&c_je;&c_r;&c_t;&c_mol;</trd>
      <trd lng="sv">d&ouml;d</trd>
    </drv>

    <drv mrk="mort.0a">
      <kap><tld/>a</kap>
      <snc mrk="mort.0a.hora">
	<dif>
	  Rilata, simila, konforma, konvena al <tld/>o:
	  <ekz>
	    <tld/>a kanto, silento;
	  </ekz><ekz>
	    <tld/>e pala.
	  </ekz>
	</dif>
	<trd lng="be">&c_s;&c_mol;&c_m;&c_ja;&c_r;&c_o;&c_t;&c_n;&c_y;</trd>
	<trdgrp lng="ru">
	  <trd>&c_s;&c_m;&c_je;&c_r;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
	  <trd>&c_s;&c_m;&c_je;&c_r;&c_t;&c_n;&c_y;&c_j;</trd>
	</trdgrp>
      </snc>
      <snc mrk="mort.0a.dangxera">
	<dif>
	  <tld lit="M"/>iga:
	  <ekz>
	    <tld/>a angoro, malsano<fnt>Z</fnt>;
	  </ekz><ekz>
	    <tld/>a vundo;
	  </ekz><ekz>
	    <tld/>e vundita;
	  </ekz><ekz>
	    <tld/>e enui;
	  </ekz><ekz>
	    <tld/>a <klr>(&gcirc;is<tld/>e senpardona)</klr>
	    malamiko<fnt>Z</fnt>, ofendo.
	  </ekz>
	</dif>
	<trdgrp lng="be">
	  <trd>&c_s;&c_mol;&c_m;&c_ja;&c_r;&c_o;&c_t;&c_n;&c_y;</trd>,
	  <trd>&c_p;&c_a;&c_g;&c_ib;&c_b;&c_je;&c_l;&c_mol;&c_n;&c_y;</trd>,
	  <trd>&c_s;&c_mol;&c_m;&c_je;&c_r;&c_c;&c_ja;&c_n;&c_o;&c_s;&c_n;&c_y;</trd>
	</trdgrp>
	<trd lng="nl">dodeliijk</trd>
	<trdgrp lng="ru">
	  <trd>&c_s;&c_m;&c_je;&c_r;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
	  <trd>&c_s;&c_m;&c_je;&c_r;&c_t;&c_n;&c_y;&c_j;</trd>
	</trdgrp>
      </snc>
      <snc mrk="mort.0a.ema">
	<ref tip="dif" cel="mort.0ema"><tld/>ema</ref>
	<ekz>
	  nia korpo estas <tld/>a<fnt>Z</fnt>;
	</ekz><ekz>
	  la <tld/>uloj.
	</ekz>
	<trd lng="nl">dodelijk</trd>
	<trd lng="be">&c_s;&c_mol;&c_m;&c_ja;&c_r;&c_o;&c_t;&c_n;&c_y;</trd>
	<trd lng="ru">&c_s;&c_m;&c_je;&c_r;&c_t;&c_n;&c_y;&c_j;</trd>
      </snc>
      <snc mrk="mort.0a.malviva">
	<uzo tip="stl">EVI</uzo>
	<dif>
	  <ref tip="dif" cel="viv.mal0a">Malviva</ref>:
	  <ekz>
	    en&ccirc;erkigi la <tld/>an korpon.
	  </ekz>
	</dif>
	<rim>
	  Tiu uzo estas tute nelogika kaj &scirc;uldata al hazarda ekstera koincido
	  kun la similsona adjektivo en la latinidaj lingvoj; do, al
	  fenomeno fremdlingva, kaj atestas pri nesufi&ccirc;a kompreno de
	  la esperanta derivado. Zamenhof instruis la diferencon inter
	  <tld/>a kaj malviva jam en la Fundamenta Ekzercaro (&para;31);
	  kaj en siaj gravaj verkoj li preferis paroli pri
	  <ctl><tld/>intaj lingvoj</ctl>:
	  <ekz>
	    elekti ian el la lingvoj <tld/>intaj (ekzemple
	    latinan, grekan, hebrean)
	    <fnt><bib>EE</bib>, <lok>&ccirc;ap. 5.</lok></fnt>.
	  </ekz>
	  La uzo de <tld/>a por la senco <ctl>malviva</ctl> kreas
	  embarasajn konfuzojn: &ccirc;u &leftquot;<tld/>a malamiko&rightquot; estas jam
	  <tld/>inta (kp ekz. &ccirc;e <tld/>a,<sncref ref="mort.0a.dangxera"/>)?
	  &Ccirc;u &leftquot;<tld/>aj lingvoj, kutimoj&rightquot; estas fatalaj kiel la
	  &leftquot;<tld/>a peko&rightquot; &dash; a&ubreve;, tute male, jam tute senefikaj,
	  <tld/>intaj?
	  <aut>Sergio Pokrovskij</aut>
	</rim>
	<trd lng="sv">d&ouml;d</trd>
	<trd lng="nl">dood</trd>
	<trdgrp lng="be">
	  <trd>&c_p;&c_a;&c_m;&c_je;&c_r;&c_l;&c_y;</trd>,
	  <trd>&c_z;&c_a;&c_g;&c_ib;&c_n;&c_u;&c_w;&c_sh;&c_y;</trd>,
	  <trd>&c_m;&c_jo;&c_r;&c_t;&c_v;&c_y;</trd>
	</trdgrp>
      </snc>
      <trdgrp lng="fr">
	<trd>mort<klr> (adj.)</klr></trd>,
	<trd>mortel<klr> (adj.)</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="mort.0ado">
      <kap><tld/>ado<fnt>Z</fnt></kap>
      <snc mrk="mort.0ado.ago">
	<dif>
	  La ago <tld/>i:
	  <ekz>
	    &ccirc;i leter' estis plej granda doloro kaj plej granda &gcirc;em',
	    Sed en la <tld/>ado &ccirc;iu devas esti sola, solega &gcirc;is ekstrem'
	    <fnt>
	      <aut>J. Wolker, tr. J. Ko&rcaron;inek</aut>:
	      <vrk>La mortanto</vrk>.
	      <lok>Nica literatura revuo, 1:1</lok>.
	    </fnt>
	  </ekz>
	</dif>
	<trd lng="ru">&c_u;&c_m;&c_i;&c_r;&c_a;&c_n;&c_i;&c_je;</trd>
      </snc>
      <snc mrk="mort.0ado.kvanto">
	<dif>
	  <tld lit="M"/>okvanto:
	  <ekz>
	    granda <tld/>ado de su&ccirc;infanoj.
	  </ekz>
	</dif>
	<trd lng="ru">&c_s;&c_m;&c_je;&c_r;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
      </snc>
      <trd lng="be">&c_s;&c_mol;&c_m;&c_ja;&c_r;&c_o;&c_t;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
      <trd lng="fr">mortalit&eacute;</trd>
      <trd lng="nl">sterfte</trd>
    </drv>

    <drv mrk="mort.0intajxo">
      <kap><tld/>inta&jcirc;o</kap>
      <snc>
	<dif>
	  Io <tld/>inta; kadavro kc.
	</dif>
      </snc>
      <trd lng="be">&c_t;&c_r;&c_u;&c_p;</trd>
      <trd lng="fr">charogne</trd>
      <trd lng="nl">lijk</trd>
      <trd lng="ru">&c_p;&c_a;&c_d;&c_a;&c_l;&c_mol;</trd>
    </drv>

    <drv mrk="mort.0ema">
      <kap><tld/>ema</kap>
      <snc>
	<dif>
	  Pli-malpli frue <tld/>onta pro sia propra naturo:
	  <ekz>
	    &ccirc;iu homo estas <tld/>ema.
	  </ekz>
	</dif>
	<ref tip="sin" cel="mort.0a.ema">morta <sncref/></ref>
      </snc>
      <trd lng="be">&c_s;&c_mol;&c_m;&c_ja;&c_r;&c_o;&c_t;&c_n;&c_y;</trd>
      <trd lng="fr">mortel<klr tip="ind"> (pas immortel)</klr></trd>
      <trd lng="nl">sterfelijk</trd>
      <trd lng="ru">&c_s;&c_m;&c_je;&c_r;&c_t;&c_n;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="mort.0igi">
      <kap><tld/>igi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="mort.0igi.malvivigi">
	<dif>
	  Senigi iun je vivo, ka&ubreve;zi <tld/>on de:
	  <ekz>
	    <tld/>igi bovon;
	  </ekz><ekz>
	    <ind>
	      <tld/>iga</ind> veneno;
	    <trd lng="fr">mortel<klr tip="ind"> (qui tue)</klr>
	    </trd>
	  </ekz><ekz>
	    mem<tld/>igo;
	  </ekz>
	</dif>
	<refgrp tip="sub">
	  <ref cel="bucx.0i">bu&ccirc;i</ref>,
	  <ref cel="murd.0i">murdi</ref>
	</refgrp>
	<ref tip="vid" cel="falcx.0i">fal&ccirc;i</ref>
	<trd lng="be">&c_z;&c_a;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>
      </snc>
      <snc mrk="mort.0igi.FIG">
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Ekstermi, forigi:
	  <ekz>
	    la esperantistoj ne volas <tld/>igi la naciajn
	    lingvojn<fnt>Z</fnt>;
	  </ekz><ekz>
	    ili volis tiun ideon tute <tld/>igi<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="detru.0i">detrui</ref>,
	  <ref cel="pere.0igi">pereigi</ref>,
	  <ref cel="ruin.0igi">ruinigi</ref>.
	</refgrp>
	<trd lng="be">&c_v;&c_y;&c_n;&c_ib;&c_sh;&c_ch;&c_a;&c_c;&c_mol;</trd>
      </snc>
      <trd lng="fr">tuer</trd>
      <trd lng="nl">doden</trd>
      <trd lng="sv">d&ouml;da</trd>
      <trd lng="ru">&c_u;&c_b;&c_i;&c_t;&c_mol;</trd>
    </drv>

    <drv mrk="mort.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
	<dif>
	  I&gcirc;i <tld/>anta, agonii.
	</dif>
	<trd lng="fr">se <ind>mourir</ind></trd>
      </snc>
      <snc>
	<dif>
	  I&gcirc;i <tld/>inta:
	  <ekz>
	    li ne <tld/>is de natura <tld/>o kaj anka&ubreve; estis <tld/>igita de
	    neniu, sed li falis sub la radojn de vagonaro kaj
	    <tld/>i&gcirc;is<fnt>Z</fnt>.
	  </ekz>
	</dif>
	<trd lng="fr">p&eacute;rir</trd>
	<trd lng="ru">&c_u;&c_b;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
      </snc>
      <trd lng="nl">sterven</trd>
    </drv>

    <drv mrk="mort.for0i">
      <kap>for<tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
	<dif>
	  Malaperi sekve de mortado (precipe pri iu grupo, familio,
	  specio, metio ktp) a&ubreve; ekstermado:
	  <ekz>
	    la dinosa&ubreve;roj for<tld/>is;
	  </ekz><ekz>
	    kaj la tempo, dum kiu ni iris de Kade-Barnea
	    <klr>...</klr> estis tridek ok jaroj, &gcirc;is
	    for<tld/>is el la tendaro la tuta generacio de
	    militistoj, kiel &jcirc;uris al ili la Eternulo
	    <fnt><bib>MT</bib>, <lok>&Rea; 2:14</lok></fnt>;
	  </ekz><ekz>
	    mi &gcirc;ojas, ke nia kredo ankora&ubreve; ne for<tld/>is
	    <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;ap. 12a</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="en">
	<trd>die out</trd>,
	<trd>die off</trd>,
	<trd>become <ind>extinct</ind></trd>
      </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_v;&c_y;&c_m;&c_je;&c_r;&c_je;&c_t;&c_mol;</trd>,
	<trd>&c_v;&c_y;&c_m;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>

    </drv>
    <drv mrk="mort.sen0ane0ema">
      <kap>sen<tld/>a, ne<tld/>ema<fnt>Z</fnt></kap>
      <snc>
	<dif>
	  Neniam <tld/>onta:
	  <ekz>
	    sen<tld/>eco de la animo
	  </ekz><ekz>
	    <uzo tip="stl">FIG</uzo>
	    la sen<tld/>aj <klr>(neniam forgesotaj)</klr> verkoj de
	    Homero.
	  </ekz>
	</dif>
      </snc>
      <trd lng="fr">immortel</trd>
      <trd lng="nl">onsterfbaar</trd>
      <trdgrp lng="be">
	<trd>&c_n;&c_je;&c_w;&c_m;&c_ib;&c_r;&c_u;&c_ch;&c_y;</trd>,
	<trd>&c_b;&c_je;&c_s;&c_mol;&c_s;&c_mol;&c_m;&c_ja;&c_r;&c_o;&c_t;&c_n;&c_y;</trd>
      </trdgrp>
      <trd lng="ru">&c_b;&c_je;&c_s;&c_s;&c_m;&c_je;&c_r;&c_t;&c_n;&c_y;&c_j;</trd>
    </drv>

    <drv mrk="mort.mem0igi">
      <kap>mem<tld/>igi</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
	<dif>
	  <ref tip="super" cel="mort.0igi.malvivigi"><tld lit="M"/>igi</ref>
	  la propran eston, ka&ubreve;zi la propran <tld/>on.
	</dif>
      </snc>
      <trd lng="fr">se <ind>tuer</ind></trd>
      <trd lng="ru">&c_u;&c_b;&c_i;&c_t;&c_mol; &c_s;&c_je;&c_b;&c_ja;</trd>
    </drv>

    <drv mrk="mort.mem0igo">
      <kap>mem<tld/>igo</kap>
      <snc>
	<dif>
	  <tld lit="M"/>o pro propra konduto.
	</dif>
      </snc>
      <trd lng="fr">suicide<klr> (volontaire ou non)</klr></trd>
    </drv>

    <drv mrk="mort.paf0igi">
      <kap>paf<tld/>igi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
	<dif>
	  <ref tip="super" cel="mort.0igi.malvivigi"><tld lit="M"/>igi</ref>
	  per pafo.
	</dif>
	<refgrp tip="sin">
	  <ref cel="paf.mort0i">mortpafi</ref>.
	</refgrp>
      </snc>
      <trd lng="be">&c_z;&c_a;&c_s;&c_t;&c_r;&c_e;&c_l;&c_ib;&c_c;&c_mol;</trd>
      <trdgrp lng="fr">
	<trd>abattre<klr tip="ind"> (tuer qn)</klr></trd>,
	<trd>fusiller</trd>,
	<trd>flinguer</trd>
      </trdgrp>
      <trdgrp lng="nl">
	<trd>neerschieten</trd>,
	<trd>doodschieten</trd>,
	<trd>fusilleren</trd>
      </trdgrp>
      <trdgrp lng="ru">
	<trd>&c_z;&c_a;&c_s;&c_t;&c_r;&c_je;&c_l;&c_i;&c_t;&c_mol;</trd>,
	<trd>&c_r;&c_a;&c_s;&c_s;&c_t;&c_r;&c_je;&c_l;&c_ja;&c_t;&c_mol; <klr>(&c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="mort.prem0igi">
      <kap>prem<tld/>igi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
	<dif>
	  <ref tip="super" cel="mort.0igi.malvivigi"><tld lit="M"/>igi</ref>
	  per forta premo, kiu haltigas spiradon:
	  <ekz>
	    <klr>[&scirc;i]</klr> kisis la kornikon tiel fortege, ke &scirc;i
	    preska&ubreve; prem<tld/>igis &gcirc;in
	    <fnt>
	      <bib>Fab2</bib>, <lok>Kvara Rakonto</lok>
	    </fnt>.
	  </ekz>
	</dif>
	<refgrp tip="vid">
	  <ref cel="sufok.0i">sufoki</ref>
	</refgrp>
      </snc>
      <trdgrp lng="be">
	<trd>&c_z;&c_a;&c_d;&c_a;&c_v;&c_ib;&c_c;&c_mol;</trd>,
	<trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="fr">&eacute;touffer<klr tip="ind"> (tr)</klr></trd>
      <trd lng="nl">verstikken</trd>
      <trd lng="ru">&c_z;&c_a;&c_d;&c_a;&c_v;&c_i;&c_t;&c_mol; (&c_n;&c_a;&c_s;&c_m;&c_je;&c_r;&c_t;&c_mol;)</trd>
    </drv>

    <drv mrk="mort.sin0igi">
      <kap>sin<tld/>igi</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
	<dif>
	  Vole, konscie <ref tip="super"
	    cel="mort.0igi.malvivigi"><tld/>igi</ref> sin mem:
	  <ekz>
	    se mi estus tiom fiera, anta&ubreve; longe mi jam sin<tld/>igus per
	    kapfrapi&gcirc;o kontra&ubreve; la muro
	    <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 74a, p. 563a</lok></fnt>.
	  </ekz>
	</dif>
	<ref tip="sin" cel="suicid.0i">suicidi</ref>
      </snc>
      <trd lng="fr">se <ind>suicider</ind></trd>
      <trdgrp lng="ru">
	<trd>&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_i;&c_t;&c_mol; &c_s;&c_a;&c_m;&c_o;&c_u;&c_b;&c_i;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>,
	<trd>&c_n;&c_a;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol; &c_n;&c_a; &c_s;&c_je;&c_b;&c_ja; &c_r;&c_u;&c_k;&c_i;</trd>,
	<trd>&c_p;&c_o;&c_k;&c_o;&c_n;&c_ch;&c_i;&c_t;&c_mol; &c_s; &c_s;&c_o;&c_b;&c_o;&c_j;</trd>
      </trdgrp>
    </drv>

    <drv mrk="mort.sin0igo">
      <kap>sin<tld/>igo</kap>
      <snc>
	<dif>
	  Intenca ago por sin <tld/>igi.
	</dif>
      </snc>
      <ref tip="sin" cel="suicid.0o">suicido</ref>
      <trd lng="fr">suicide</trd>
      <trd lng="ru">&c_s;&c_a;&c_m;&c_o;&c_u;&c_b;&c_i;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
    </drv>

    <drv mrk="mort.sxton0igi">
      <kap>&scirc;ton<tld/>igi</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
	<dif>
	  <ref tip="super" cel="mort.0igi.malvivigi"><tld lit="M"/>igi</ref>
	  per &scirc;ton&jcirc;etado:
	  <ekz>
	    ili faris konspiron kontra&ubreve; li, kaj &scirc;ton<tld/>igis lin la&ubreve;
	    ordono de la re&gcirc;o sur la korto de la domo de la Eternulo
	    <fnt><bib>MT</bib>, <lok>&Kr2; 24:21</lok>.</fnt>.
	  </ekz>
	</dif>
	<ref tip="vid" cel="sxton.0umi">&scirc;tonumi</ref>
      </snc>
      <trd lng="be">&c_p;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_k;&c_a;&c_m;&c_ja;&c_n;&c_ja;&c_m;&c_ib;
	<klr>(&c_p;&c_a;&c_k;&c_a;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>
      <trd lng="fr">lapider</trd>
      <trd lng="nl">stenigen</trd>
      <trd lng="sv">stena till <ind>d&ouml;ds</ind></trd>
      <trd lng="ru">&c_p;&c_o;&c_b;&c_i;&c_t;&c_mol; &c_k;&c_a;&c_m;&c_n;&c_ja;&c_m;&c_i;
	<klr>(&c_k;&c_a;&c_z;&c_n;&c_i;&c_t;&c_mol;)</klr></trd>
    </drv>
