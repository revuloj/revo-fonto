<kap>
  <ofc>*</ofc>
  <rad>servic</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="servic.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="servic.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Tuto da man&gcirc;ilaro, vazaro, tukoj kaj aliaj, kiu harmonie garnas 
      man&gcirc;otablon:
      <ekz>
        ili sentis sin ankora&ubreve; pli liberaj <klr>...</klr> post 
        surtabligo de l' kafo<tld/>o
        <fnt><bib>KielAkvo</bib>, <lok>1:2</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ensemb.0o.KOMUNE">ensemblo</ref>,
      <ref cel="garnit.0o.KOMUNE">garnituro</ref>,
      <ref cel="komple2.0o.KOMUNE">kompleto</ref>,
      <ref cel="sortim.0o.KOMUNE">sortimento</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>teleraro</ke>, <ke>man&#285;ilaro</ke>.
  </rim>

  <trd lng="be">&c_s;&c_e;&c_r;&c_v;&c_ib;&c_z;</trd>
  <trdgrp lng="cs">
    <trd>servis</trd>,
    <trd>souprava stoln&iacute;ho n&aacute;dob&iacute;</trd>
  </trdgrp>
  <trd lng="de">Service</trd>
  <trdgrp lng="en">
    <trd>crockery set</trd>,
    <trd>service</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>vajilla</trd>,
    <trd>servicio</trd>
  </trdgrp>
  <trd lng="fa">&a_s;&a_r;&a_w;&f_y;&a_s;<klr tip="amb"> (&a_gh;&a_dh;&a_A;&a_kh;&a_w;&a_r;&f_y;)</klr></trd>
  <trd lng="fr">service de table</trd>
  <trdgrp lng="hu">
    <trd>asztali k&eacute;szlet</trd>,
    <trd>szerviz<klr tip="ind"> (asztali k&eacute;szlet)</klr></trd>
  </trdgrp>
  <trd lng="it">servizio<klr> (stoviglie)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#39135;&#22120;&#12475;&#12483;&#12488;  <pr>&#12375;&#12423;&#12387;&#12365;&#12379;&#12387;&#12392;</pr></trd>,
    <trd>&#39135;&#22120;&#12402;&#12392;&#12381;&#12429;&#12356;  <pr>&#12375;&#12423;&#12387;&#12365;&#12402;&#12392;&#12381;&#12429;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">servies</trd>
  <trdgrp lng="pl">
    <trd>zastawa sto&lstroke;owa</trd>,
    <trd>serwis</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_v;&c_i;&c_z;</trd>
  <trd lng="sk">s&uacute;prava <klr>(jed&aacute;lna)</klr></trd>
  <trd lng="uk">&#1089;&#1077;&#1088;&#1074;&#1110;&#1079;</trd>
  <trd lng="zh">&#33590;&#20855; <pr>ch&#225;j&#249;</pr></trd>
</drv>
