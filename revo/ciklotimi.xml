<art mrk="$Id: ciklotimi.xml,v 1.3 2025/10/31 08:39:32 afido Exp $">
<kap>
    <rad>ciklotimi</rad>/o <fnt><aut>M. Briquet</aut> <vrk><url
    ref="https://www.eventoj.hu/steb/vortaroj/eo-teknika-medicina-vortaro-Briquet.pdf">Esperanta
    teknika medicina vortaro</url></vrk> <lok>1932</lok></fnt>
</kap>
<drv mrk="ciklotimi.0o">
  <kap><tld/>o</kap>
  <snc mrk="ciklotimi.0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Alterno de viglaj kaj malviglaj humoroj, simila, malpli severa,
      al <ref cel="san.dupolusamal0o.PSI">dupolusa malsano</ref>:
      <ekz>
        hiera&ubreve;, mi a&ubreve;dis je la unua fojo pri <tld/>o, milda formo de dupolusa perturbo
        <fnt>
          <aut>J. Genberg</aut>
          <vrk><url ref="https://www.miavivo.net/grupoj/mensa-sano">Mensa Sano</url></vrk>
          <lok>Mia Vivo, 2025-03-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_k;&c_l;&c_ja;&c_t;&c_y;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="de">Zyklothymia </trd>
  <trd lng="en">cyclothymia</trd>
  <trd lng="fr">cyclothymie</trd>
  <trd lng="ru">&c_c;&c_i;&c_k;&c_l;&c_o;&c_t;&c_i;&c_m;&c_i;&c_ja;</trd>
</drv>
</art>

