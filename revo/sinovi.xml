<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: sinovi.xml,v 1.1 2025/01/23 18:33:35 afido Exp $">
    <kap>
      <rad>sinovi</rad>/o
      <fnt><bib>JED</bib></fnt>
    </kap>

<drv mrk="sinovi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="sinovi.0o.ANA">
    <dif>
      <ref tip="super" cel="likv.0o">Likvo</ref> viskoza, lubrikanta la membranojn de <ref cel="artik.0o.ANA">artiko</ref>j
      kaj <ref cel="tenden.0o">tenden</ref>ujoj:
      <ekz>
        kiomaj blindige belaj sentoj kaj agoj pendas sur hareto, en plena sklaveco de
        maldikega ha&ubreve;teto, de mizeraj kotaj buletoj sur vandoj de angioj, de iu
        <tld/>o, de ka&scirc;ita fonto de hormonoj
        <fnt>
          <aut>Vja&ccirc;eslavo Rybakov, trad. Jurij Finkel</aut>
          <vrk><url ref="https://www.esperanto.mv.ru/Gravitavio/03.html"
              >Gravitavio &leftquot;Carido&rightquot;</url></vrk>
          <lok>1. Tjuratamo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Synovia</trd>,
      <trd>Gelenkschmiere</trd>
    </trdgrp>
    <trd lng="en">synovial fluid</trd>
    <trd lng="fr">liquide synovial</trd>
  </snc>
</drv>

  </art>

