<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxos.xml,v 1.18 2024/03/25 07:28:39 revo Exp $">
<kap>
  <rad>&scirc;os</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxos.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxos.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Elkreskanta <ref tip="super" cel="tig.0o">tigo</ref>
      sen flankaj tigoj kaj kun <ref tip="prt" cel="burgxo.0o">bur&gcirc;ono</ref>
      a&ubreve; folioj:
      <ekz>
        junaj <tld/>oj belbrilas per dol&ccirc;a verdo en la arbidejoj
        <fnt>
       <aut>Maryvonne kaj Bruno Robineau</aut>
          <vrk><url ref="&Monato;/2000/006831.php">La&ubreve; ritmo de la rizejoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/02, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la terpomoj ne &ccirc;esas elpu&scirc;i <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2015/011613.php">Terpomoj gravedaj</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
<ref tip="sin" cel="plant.0ido">plantido</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_s;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_a;&c_d;&c_r;&c_o;&c_s;&c_t;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_f;&c_i;&c_l;&c_i;&c_z;</trd>
  <trdgrp lng="de">
    <trd>Spross</trd>,
    <trd>Trieb</trd>,
    <trd>Sch&ouml;ssling</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sprout</trd>,
    <trd>shoot</trd>
  </trdgrp>
  <trd lng="fr">pousse<klr> (plante)</klr></trd>
  <trdgrp lng="hu">
    <trd>hajt&aacute;s</trd>,
    <trd>sarj</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>butto</trd>,
    <trd>germoglio</trd>,
    <trd>germma<klr> (bot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x82E5;&#x679D;  <pr>&#x308F;&#x304B;&#x3048;&#x3060;</pr></trd>,
    <trd>&#x3072;&#x3053;&#x3070;&#x3048;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loot</trd>,
    <trd>scheut</trd>
  </trdgrp>
  <trd lng="pt">broto</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_r;&c_o;&c_s;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_je;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&c_r;&#x43E;&c_s;&c_t;&#x43E;&#x43A;</trd>,
    <trd>&#x43F;&c_a;&c_r;&#x43E;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43F;&c_a;&c_g;&c_ib;&#x43D;</trd>,
    <trd>&c_zh;&c_i;&c_v;&c_je;&c_c;&#x44C; &c_d;&#x43B;&#x44F; &c_shch;&c_je;&#x43F;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x82D7; <pr>mi&#xE1;o</pr></trd>,
    <trd>&#x79E7; <pr>y&#x101;ng</pr></trd>,
    <trd>&#x82BD; <pr>y&#xE1;</pr></trd>,
    <trd>&#x6881;&#x72B6;&#x7ED3;&#x6784; <pr>li&#xE1;ngzhu&#xE0;ngji&#xE9;g&#xF2;u</pr></trd>,
    <trd>&#x6881;&#x72C0;&#x7D50;&#x69CB; <pr>li&#xE1;ngzhu&#xE0;ngji&#xE9;g&#xF2;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxos.0i">
  <kap><tld/>i</kap>
  <snc mrk="sxos.0i.BOT">
    <dif>
      Elmeti <tld/>ojn,
      ek<ref tip="super" cel="kresk.0i.vegetajxo">kreski</ref>:
      <ekz>
        la semo falas kaj <tld/>as
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        herboj <tld/>as, floroj bur&gcirc;onas, arboj verdi&gcirc;as
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;1/2005/04/12/1@23333.htm">
          Esperanto-bosko</url></vrk>,
          <lok>2005-04-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol; <ind>&c_p;&c_a;&c_r;&c_a;&c_s;&c_t;&c_k;&c_ib;</ind></trd>
  <trdgrp lng="bg">
    <trd>&c_i;&c_z;&c_r;&c_a;&c_s;&c_t;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_malmol;&c_l;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_malmol;&c_p;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="de">sprie&szlig;en</trd>
  <trd lng="en">sprout</trd>
  <trdgrp lng="fr">
    <trd>germer</trd>,
    <trd>lever<klr> (plante)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x82E5;&#x679D;&#x3092;&#x4F38;&#x3070;&#x3059;  <pr>&#x308F;&#x304B;&#x3048;&#x3060;&#x3092;&#x306E;&#x3070;&#x3059;</pr></trd>,
    <trd>&#x3072;&#x3053;&#x3070;&#x3048;&#x3092;&#x751F;&#x3058;&#x308B;  <pr>&#x3072;&#x3053;&#x3070;&#x3048;&#x3092;&#x3057;&#x3087;&#x3046;&#x3058;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">ontspruiten</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_u;&c_s;&#x43A;&c_a;&c_t;&c_i; &#x43F;&c_a;&c_r;&#x43E;&c_s;&c_t;&#x43A;&c_i;</trd>,
    <trd>&#x43F;&c_a;&c_g;&#x43E;&#x43D;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x840C;&#x53D1; <pr>m&#xE9;ngf&#x101;</pr></trd>,
    <trd>&#x840C;&#x767C; <pr>m&#xE9;ngf&#x101;</pr></trd>,
    <trd>&#x840C;&#x52A8; <pr>m&#xE9;ngd&#xF2;ng</pr></trd>,
    <trd>&#x840C;&#x52D5; <pr>m&#xE9;ngd&#xF2;ng</pr></trd>,
    <trd>&#x62BD; <pr>ch&#x14D;u</pr></trd>
  </trdgrp>
</drv>
</art>

