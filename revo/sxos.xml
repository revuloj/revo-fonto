<vortaro>
<art mrk="$Id$">
<kap>
<rad>&scirc;os</rad>/o
</kap>
<drv mrk="sxos.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Elkreskanta <ref tip="super" cel="tig.0o">tigo</ref>
      sen flankaj tigoj kaj kun 
      <ref tip="prt" cel="burgxo.0o">bur&gcirc;ono</ref>
      a&ubreve; folioj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_s;&c_t;&c_a;&c_k;</trd>, 
    <trd>&c_a;&c_d;&c_r;&c_o;&c_s;&c_t;&c_a;&c_k;</trd>
  </trdgrp>  
  <trdgrp lng="de">
    <trd>Spross</trd>,
    <trd>Trieb</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sprout</trd>,
    <trd>shoot</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>hajt&aacute;s</trd>,
    <trd>sarj</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loot</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_r;&c_o;&c_s;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_je;&c_g;</trd>
  </trdgrp>
</drv>
</art>

