<art mrk="$Id: sxos.xml,v 1.19 2025/04/06 06:47:46 revo Exp $">
<kap>
  <rad>&scirc;os</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxos.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxos.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Elkreskanta <ref tip="super" cel="tig.0o">tigo</ref>
      sen flankaj tigoj kaj kun <ref tip="prt" cel="burgxo.0o">bur&gcirc;ono</ref>
      a&ubreve; folioj:
      <ekz>
        junaj <tld/>oj belbrilas per dol&ccirc;a verdo en la arbidejoj
        <fnt>
       <aut>Maryvonne kaj Bruno Robineau</aut>
          <vrk><url ref="&Monato;/2000/006831.php">La&ubreve; ritmo de la rizejoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/02, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        la terpomoj ne &ccirc;esas elpu&scirc;i <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2015/011613.php">Terpomoj gravedaj</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
<ref tip="sin" cel="plant.0ido">plantido</ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>bran&#265;ido</ke>, <ke>&#285;ermo</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_s;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_a;&c_d;&c_r;&c_o;&c_s;&c_t;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_f;&c_i;&c_l;&c_i;&c_z;</trd>
  <trdgrp lng="de">
    <trd>Spross</trd>,
    <trd>Trieb</trd>,
    <trd>Sch&ouml;ssling</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sprout</trd>,
    <trd>shoot</trd>
  </trdgrp>
  <trd lng="fr">pousse<klr> (plante)</klr></trd>
  <trdgrp lng="hu">
    <trd>hajt&aacute;s</trd>,
    <trd>sarj</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>butto</trd>,
    <trd>germoglio</trd>,
    <trd>germma<klr> (bot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#33509;&#26525;  <pr>&#12431;&#12363;&#12360;&#12384;</pr></trd>,
    <trd>&#12402;&#12371;&#12400;&#12360;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loot</trd>,
    <trd>scheut</trd>
  </trdgrp>
  <trd lng="pt">broto</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_r;&c_o;&c_s;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_je;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1087;&c_a;&c_r;&#1086;&c_s;&c_t;&#1086;&#1082;</trd>,
    <trd>&#1087;&c_a;&c_r;&#1086;&c_s;&c_t;&#1100;</trd>,
    <trd>&#1087;&c_a;&c_g;&c_ib;&#1085;</trd>,
    <trd>&c_zh;&c_i;&c_v;&c_je;&c_c;&#1100; &c_d;&#1083;&#1103; &c_shch;&c_je;&#1087;&#1083;&c_je;&#1085;&#1085;&#1103;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33495; <pr>mi&#225;o</pr></trd>,
    <trd>&#31207; <pr>y&#257;ng</pr></trd>,
    <trd>&#33469; <pr>y&#225;</pr></trd>,
    <trd>&#26753;&#29366;&#32467;&#26500; <pr>li&#225;ngzhu&#224;ngji&#233;g&#242;u</pr></trd>,
    <trd>&#26753;&#29376;&#32080;&#27083; <pr>li&#225;ngzhu&#224;ngji&#233;g&#242;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxos.0i">
  <kap><tld/>i</kap>
  <snc mrk="sxos.0i.BOT">
    <dif>
      Elmeti <tld/>ojn,
      ek<ref tip="super" cel="kresk.0i.vegetajxo">kreski</ref>:
      <ekz>
        la semo falas kaj <tld/>as
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        herboj <tld/>as, floroj bur&gcirc;onas, arboj verdi&gcirc;as
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;1/2005/04/12/1@23333.htm">
          Esperanto-bosko</url></vrk>,
          <lok>2005-04-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol; <ind>&c_p;&c_a;&c_r;&c_a;&c_s;&c_t;&c_k;&c_ib;</ind></trd>
  <trdgrp lng="bg">
    <trd>&c_i;&c_z;&c_r;&c_a;&c_s;&c_t;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_malmol;&c_l;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_malmol;&c_p;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="de">sprie&szlig;en</trd>
  <trd lng="en">sprout</trd>
  <trdgrp lng="fr">
    <trd>germer</trd>,
    <trd>lever<klr> (plante)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#33509;&#26525;&#12434;&#20280;&#12400;&#12377;  <pr>&#12431;&#12363;&#12360;&#12384;&#12434;&#12398;&#12400;&#12377;</pr></trd>,
    <trd>&#12402;&#12371;&#12400;&#12360;&#12434;&#29983;&#12376;&#12427;  <pr>&#12402;&#12371;&#12400;&#12360;&#12434;&#12375;&#12423;&#12358;&#12376;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">ontspruiten</trd>
  <trdgrp lng="uk">
    <trd>&#1087;&c_u;&c_s;&#1082;&c_a;&c_t;&c_i; &#1087;&c_a;&c_r;&#1086;&c_s;&c_t;&#1082;&c_i;</trd>,
    <trd>&#1087;&c_a;&c_g;&#1086;&#1085;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#33804;&#21457; <pr>m&#233;ngf&#257;</pr></trd>,
    <trd>&#33804;&#30332; <pr>m&#233;ngf&#257;</pr></trd>,
    <trd>&#33804;&#21160; <pr>m&#233;ngd&#242;ng</pr></trd>,
    <trd>&#33804;&#21205; <pr>m&#233;ngd&#242;ng</pr></trd>,
    <trd>&#25277; <pr>ch&#333;u</pr></trd>
  </trdgrp>
</drv>
</art>

