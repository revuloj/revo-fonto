<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxos.xml,v 1.13 2022/05/05 14:43:49 revo Exp $">
<kap>
  <rad>&scirc;os</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxos.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxos.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Elkreskanta <ref tip="super" cel="tig.0o">tigo</ref>
      sen flankaj tigoj kaj kun <ref tip="prt" cel="burgxo.0o">bur&gcirc;ono</ref>
      a&ubreve; folioj:
      <ekz>
        junaj <tld/>oj belbrilas per dol&ccirc;a verdo en la arbidejoj
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk><url ref="&Steloj;monato2/006831.html">La&ubreve; ritmo de la rizejoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la terpomoj ne &ccirc;esas elpu&scirc;i <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2015/011613.php">Terpomoj gravedaj</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
<ref tip="sin" cel="plant.0ido">plantido</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_a;&c_s;&c_t;&c_a;&c_k;</trd>, 
    <trd>&c_a;&c_d;&c_r;&c_o;&c_s;&c_t;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_f;&c_i;&c_l;&c_i;&c_z;</trd>
  <trdgrp lng="de">
    <trd>Spross</trd>,
    <trd>Trieb</trd>,
    <trd>Sch&ouml;ssling</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sprout</trd>,
    <trd>shoot</trd>
  </trdgrp>
  <trd lng="fr">pousse<klr> (plante)</klr></trd>
  <trdgrp lng="hu">
    <trd>hajt&aacute;s</trd>,
    <trd>sarj</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>butto</trd>,
    <trd>germoglio</trd>, 
    <trd>germma<klr> (bot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>loot</trd>,
    <trd>scheut</trd>
  </trdgrp>
  <trd lng="pt">broto</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_r;&c_o;&c_s;&c_t;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_b;&c_je;&c_g;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x430;&#x440;&#x43E;&#x441;&#x442;&#x43E;&#x43A;</trd>,
    <trd>&#x43F;&#x430;&#x440;&#x43E;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x43F;&#x430;&#x433;&#x456;&#x43D;</trd>,
    <trd>&#x436;&#x438;&#x432;&#x435;&#x446;&#x44C; &#x434;&#x43B;&#x44F; &#x449;&#x435;&#x43F;&#x43B;&#x435;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="sxos.0i">
  <kap><tld/>i</kap>
  <snc mrk="sxos.0i.BOT">
    <dif>
      Elmeti <tld/>ojn, 
      ek<ref tip="super" cel="kresk.0i.vegetajxo">kreski</ref>:
      <ekz>
        la semo falas kaj <tld/>as
        <fnt>
          <bib>Kon11</bib>
        </fnt>;
      </ekz>
      <ekz>
        herboj <tld/>as, floroj bur&gcirc;onas, arboj verdi&gcirc;as
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url ref="&CRI;1/2005/04/12/1@23333.htm">
          Esperanto-bosko</url></vrk>,
          <lok>2005-04-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_i;&c_z;&c_r;&c_a;&c_s;&c_t;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_malmol;&c_l;&c_v;&c_a;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_p;&c_malmol;&c_p;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="de">sprie&szlig;en</trd>
  <trd lng="en">sprout</trd>
  <trdgrp lng="fr">
    <trd>germer</trd>,
    <trd>lever<klr> (plante)</klr></trd>
  </trdgrp>
  <trd lng="nl">ontspruiten</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x443;&#x441;&#x43A;&#x430;&#x442;&#x438; &#x43F;&#x430;&#x440;&#x43E;&#x441;&#x442;&#x43A;&#x438;</trd>,
    <trd>&#x43F;&#x430;&#x433;&#x43E;&#x43D;&#x438;</trd>
  </trdgrp>
</drv>
</art>

