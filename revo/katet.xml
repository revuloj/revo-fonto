<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: katet.xml,v 1.20 2023/12/03 08:00:25 revo Exp $">
<kap><rad>katet</rad>/o</kap>
<drv mrk="katet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="angul.ort0a">ortangula</ref> <ref cel="angul.tri0o">triangulo</ref>)</klr>
      &Ccirc;iu el la <ref tip="super" cel="later.0o">lateroj</ref>
      de &gcirc;ia orta angulo; la longo de tia streko:
      <ekz>
        pere de &leftquot;&gcirc;enerala Pitagora teoremo&rightquot; matematikistoj jam en antikva tempo trovis,
        ke la sumo de tiuj du krescentoj super la <tld/>oj estas egala al la areo de
        la orta triangulo.
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=6957">Pi (nombro)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="hipote1.0o">hipotenuzo</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_e;&c_t;</trd>
  <trd lng="cs">ko&ccaron;i&ccaron;ka</trd>
  <trd lng="de">Kathete</trd>
  <trd lng="en">other <ind>side</ind><klr tip="ind"> (of the right
  angle)</klr></trd>
  <trd lng="es">cateto</trd>
  <trd lng="fa">&a_D;&a_l;&a_ayn; &a_q;&a_A;&a_y_hamza;&a_m;</trd>
  <trdgrp lng="fr">
    <trd>cath&egrave;te</trd>,
    <trd>c&ocirc;t&eacute; de l'angle droit</trd>
  </trdgrp>
  <trd lng="hu">befog&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x5C0F;&#x732B;  <pr>&#x3053;&#x306D;&#x3053;</pr></trd>,
    <trd>&#x306B;&#x3083;&#x3093;&#x3053;</trd>
  </trdgrp>
  <trd lng="nl">kathete</trd>
  <trd lng="pl">przyprostok&aogonek;tna</trd>
  <trd lng="pt">cateto</trd>
  <trd lng="ru">&c_k;&c_a;&c_t;&c_je;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>ma&ccaron;a</trd>,
    <trd>ma&ccaron;i&ccaron;ka</trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&#x430;&#x442;&#x435;&#x442;</trd>
  <trd lng="zh">&#x76F4;&#x89D2;&#x8FB9; <pr>zh&#xED;ji&#x1CE;obi&#x101;n</pr></trd>
</drv>
</art>
