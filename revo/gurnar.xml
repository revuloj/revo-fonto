<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gurnar.xml,v 1.16 2023/08/31 14:29:55 revo Exp $">
<kap>
  <rad>gurnard</rad>/o
</kap>
<drv mrk="gurnar.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">Specio</ref>
      de <ref tip="super" cel="trigl.0o">triglo</ref>
      (<trd lng="la">Trigla gurnardus</trd>), kun grizaj
      makuloj:
      <ekz>
        la <tld/>o kapablas eligi grumblajn a&ubreve; gruntajn sonojn per muskolo vibriganta la
        du&ccirc;ambran na&gcirc;vezikon
        <fnt>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/02/Illustration_from_The_Natural_History_of_British_Fishes_(1802)_by_Edward_Donovan%2C_digitally_enhanced_by_rawpixel-com_27.jpg/640px-Illustration_from_The_Natural_History_of_British_Fishes_(1802)_by_Edward_Donovan%2C_digitally_enhanced_by_rawpixel-com_27.jpg" prm="CC BY-SA 4.0">Griza <tld/>o, ilustra&jcirc;o en &leftquot;The Natural History of British Fishes&rightquot;
        (1802) de Edward Donovan (1768-1837)
        <fnt>
          <aut>Rawpixel, prilaborinta</aut>
          <lok><url ref="&WCW;/File:Illustration_from_The_Natural_History_of_British_Fishes_(1802)_by_Edward_Donovan,_digitally_enhanced_by_rawpixel-com_27.jpg">&WCW;/File:Illustration_from_The_Natural_History_of_British_Fishes_(1802)_by_Edward_Donovan,_digitally_enhanced_by_rawpixel-com_27.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="ca">
    <trd>cap d'ase</trd>,
    <trd>lluerna verda</trd>
  </trdgrp>
  <trd lng="de">Grauer Knurrhahn</trd>
  <trd lng="en">grey gurnard</trd>
  <trd lng="fr">grondin</trd>
  <trd lng="hu">sz&uuml;rke <ind>morg&oacute;hal</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x30DB;&#x30A6;&#x30DC;&#x30A6;</trd>,
    <trd>&#x30AB;&#x30CA;&#x30AC;&#x30B7;&#x30E9;</trd>
  </trdgrp>
  <trd lng="nl">grauwe <ind>poon</ind></trd>
  <trd lng="pl">kurek szary</trd>
  <trd lng="pt">roncador <klr tip="amb">(zool.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_i;&c_g;&c_l;&c_a;</trd>,
    <trd>&c_m;&c_o;&c_r;&c_s;&c_k;&c_o;&c_j; &c_p;&c_je;&c_t;&c_u;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x440;&#x438;&#x433;&#x43B;&#x430;</trd>,
    <trd>&#x43C;&#x43E;&#x440;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x43F;&#x456;&#x432;&#x435;&#x43D;&#x44C;</trd>
  </trdgrp>
</drv>
</art>

