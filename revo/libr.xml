<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>libr</rad>/o
    </kap>

    <drv mrk="libr.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="libr.0o.presajxo">
        <uzo tip="fak">TIP</uzo>
        <dif>
          Bindita a&ubreve; bro&scirc;urita paperfoliaro kun presita teksto:
          <ekz>
            tiu &ccirc;i <tld/>o havas sesdek pa&gcirc;ojn
            <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
          </ekz><ekz>
            nur <tld/>a <klr>(&ccirc;erpita el <tld/>oj, ne el sperto;
              kabineta)</klr> scienco;
          </ekz><ekz>
            koni la lingvon <tld/>e, sed ne libere.
          </ekz>
        </dif>
      </snc>
      <snc mrk="libr.0o.notilo">
        <dif>
          Samforma paperfoliaro destinita por ke oni skribu en &gcirc;i:
          <ekz>
            komerca <tld/>o;
          </ekz><ekz>
            <ind>not<tld/>o</ind>
            <trdgrp lng="ru">
              <trd>&c_b;&c_l;&c_o;&c_k;&c_n;&c_o;&c_t;</trd>,
              <trd>&c_z;&c_a;&c_p;&c_i;&c_s;&c_n;&c_a;&c_ja;
&c_k;&c_n;&c_i;&c_zh;&c_k;&c_a;</trd>
            </trdgrp>
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="kajer.0o">kajero</ref>
        </refgrp>
      </snc>
      <snc mrk="libr.0o.divido">
        <dif>
          <ref tip="super" cel="part.0o">Parto</ref> de literatura verko,
          ordinare grupo de &ccirc;apitroj.
        </dif>
      </snc>
      <trd lng="be">&c_k;&c_n;&c_ib;&c_g;&c_a;</trd>
      <trd lng="cs">kniha</trd>
      <trd lng="de">Buch</trd>
      <trd lng="el">&beta;&jota;&beta;&lambda;&jota_ton;&omikron;</trd>
      <trd lng="en">book</trd>
      <trd lng="es">libro</trd>
      <trd lng="fr">livre</trd>
      <trd lng="hu">k&ouml;nyv</trd>
      <trd lng="it">libro</trd>
      <trd lng="lat">liber, bri</trd>
      <trd lng="nl">boek</trd>
      <trd lng="pt">livro</trd>
      <trd lng="pl">ksi&aogonek;&zdot;ka</trd>
      <trd lng="ru">&c_k;&c_n;&c_i;&c_g;&c_a;</trd>
      <trd lng="sv">bok</trd>
      <trd lng="tr">kitap</trd>
      <trd lng="vo">buk</trd>
    </drv>

    <drv mrk="libr.0ejo">
      <kap><tld/>ejo</kap>
      <snc mrk="libr.0ejo.vendejo">
        <ref tip="dif" cel="libr.0ovendejo"><tld/>ovendejo:</ref>
        <ekz>
          <uzo tip="fak">ESP</uzo>
          <ind>Esperantista Centra <tld lit="L"/>ejo</ind>;
          <trd lng="fr">Librairie Centrale Esp&eacute;rantiste</trd>
        </ekz><ekz>
          &scirc;i timis, ke &scirc;i trovos en la <tld/>ejo
a&ccirc;etantojn,
          en kies &ccirc;eesto &scirc;i ne povus eldiri tion, pro kio
&scirc;i venis
          <fnt><bib>Marta</bib></fnt>.
        </ekz>
        <trd lng="be">&c_k;&c_n;&c_ib;&c_g;&c_a;&c_r;&c_n;&c_ja;</trd>
        <trd lng="de">Buchhandlung</trd>
        <trd
lng="el">&beta;&jota;&beta;&lambda;&jota;&omikron;&pi;&omega;&lambda;&epsilon;&jota_ton;&omikron;</trd>
        <trd lng="es">librer&iacute;a</trd>
        <trd lng="fr">librairie</trd>
        <trd lng="hu">k&ouml;nyvesbolt</trd>
        <trd lng="nl">boekhandel</trd>
        <trd lng="pt">livraria</trd>
        <trdgrp lng="ru">
          <trd>&c_k;&c_n;&c_i;&c_zh;&c_n;&c_a;&c_ja;
&c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
          <trd>&c_k;&c_n;&c_i;&c_zh;&c_n;&c_y;&c_j;
&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;</trd>
        </trdgrp>
        <trd lng="tr">kitabevi</trd>
      </snc>
      <snc mrk="libr.0ejo.biblioteko">
        <uzo tip="stl">ARK</uzo>
        <ref tip="dif" cel="biblio1.0o">biblioteko</ref>
        <ekz>
          &Ccirc;ernjaev estas fakestro <klr>[en la trusto
Mos&hcirc;ozupr]</klr>,
          li bezonas en sia <tld/>ejo helpantinon
          <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 2</lok></fnt>.
        </ekz>
      </snc>
    </drv>

    <drv mrk="libr.0eto">
      <kap><tld/>eto</kap>
      <snc mrk="libr.0eto.TIP">
        <dif>
          Malgranda <tld/>o.
        </dif>
        <trdgrp lng="be">
          <trd>&c_k;&c_n;&c_ib;&c_zh;&c_k;&c_a;</trd>,
          <trd>&c_k;&c_n;&c_ib;&c_zh;&c_a;&c_ch;&c_k;&c_a;</trd>
        </trdgrp>
        <trdgrp lng="de">
          <trd>B&uuml;chlein</trd>,
          <trd>Heft</trd>
        </trdgrp>
        <trd lng="hu">k&ouml;nyvecske</trd>
        <trd lng="lat">libellus, i</trd>
        <trd lng="nl">boekje</trd>
        <trdgrp lng="ru">
          <trd>&c_k;&c_n;&c_i;&c_zh;&c_k;&c_a;</trd>,
          <trd>&c_k;&c_n;&c_i;&c_zh;&c_je;&c_ch;&c_k;&c_a;</trd>
        </trdgrp>
      </snc>
      <snc mrk="libr.0eto.MUZ.kajero">
        <uzo tip="fak">MUZ</uzo>
        <dif>
          <tld lit="L"/>o enhavanta la parolojn de operverko.
        </dif>
        <trd lng="be">&c_l;&c_ib;&c_b;&c_r;&c_e;&c_t;&c_a;</trd>
        <trd lng="de">Libretto</trd>
        <trdgrp lng="hu">
          <trd>sz&ouml;vegk&ouml;nyv</trd>,
          <trd>librett&oacute;</trd>
        </trdgrp>
        <trd lng="nl">libretto</trd>
        <trd lng="ru">&c_l;&c_i;&c_b;&c_r;&c_je;&c_t;&c_t;&c_o;</trd>
      </snc>
      <snc mrk="libr.0eto.MUZ.teksto">
        <uzo tip="fak">MUZ</uzo>
        <dif>
          Tiuj paroloj mem.
        </dif>
        <trd lng="be">&c_l;&c_ib;&c_b;&c_r;&c_e;&c_t;&c_a;</trd>
        <trd lng="de">Libretto</trd>
        <trdgrp lng="hu">
          <trd>sz&ouml;vegk&ouml;nyv</trd>,
          <trd>librett&oacute;</trd>
        </trdgrp>
        <trd lng="nl">libretto</trd>
        <trd lng="ru">&c_l;&c_i;&c_b;&c_r;&c_je;&c_t;&c_t;&c_o;</trd>
      </snc>
      <trdgrp lng="fr">
        <trd>libretto</trd>,
        <trd>livret</trd>
      </trdgrp>
    </drv>

    <drv mrk="libr.aboco0o">
      <kap>aboco<tld/>o</kap>
      <snc>
        <ref tip="dif" cel="alfabe.0umo">alfabetumo</ref>
        <ekz>
          estis iam homo, kiu verkis iom da novaj versoj por la
aboco<tld/>o, 
          po du vicoj al &ccirc;iu litero
          <fnt><bib>Fab4</bib><lok>Abocolibro</lok></fnt>.
        </ekz>
      </snc>
      <trdgrp lng="fr">
        <trd>ab&eacute;c&eacute;daire</trd>,
        <trd>ABC<klr tip="ind"> (ab&eacute;c&eacute;daire)</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="libr.0otenado">
      <kap><tld/>otenado</kap>
      <snc mrk="libr.0otenado.EKON">
        <uzo tip="fak">EKON</uzo>
        <dif>
          Enskribado de en- kaj el-spezoj en komercajn <tld/>ojn.
        </dif>
      </snc>
      <trdgrp lng="be">
       
<trd>&c_b;&c_u;&c_g;&c_a;&c_l;&c_mol;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>,
        <trd>&c_b;&c_u;&c_g;&c_a;&c_l;&c_mol;&c_t;&c_a;&c_r;&c_s;&c_k;&c_ib;
&c_w;&c_l;&c_ib;&c_k;</trd>
      </trdgrp>
      <trd lng="de">Buchhaltung</trd>
      <trd lng="fr">comptabilit&eacute;</trd>
      <trd lng="hu">k&ouml;nyvel&eacute;s</trd>
      <trd lng="nl">boekhouding</trd>
      <trd
lng="ru">&c_b;&c_u;&c_h;&c_g;&c_a;&c_l;&c_t;&c_je;&c_r;&c_i;&c_ja;<klr
tip="amb">
        (&c_u;&c_ch;&c_jo;&c_t;)</klr></trd>
    </drv>

    <drv mrk="libr.0otenejo">
      <kap><tld/>otenejo</kap>
      <snc mrk="libr.0otenejo.EKON">
        <uzo tip="fak">EKON</uzo>
        <dif>
          Ejo, fako por <tld/>otenado (<tld/>oten-ejo):
          <ekz>
            se la CO ne <tld/>otenas siajn spezojn pro manko
            de laborforto, la CO-<tld/>otenejo ricevu pli da
            laborforto
            <fnt>
              <aut>Komitatano Z</aut>:
              <vrk>TEJO eliru el sia ka&scirc;ejo</vrk>.
              <bib>LOdE</bib><lok> 2003. N-ro 5 (103)</lok>
            </fnt>.
          </ekz>
        </dif>
        <trd
lng="ru">&c_b;&c_u;&c_h;&c_g;&c_a;&c_l;&c_t;&c_je;&c_r;&c_i;&c_ja;<klr
tip="amb">
           
(&c_p;&c_o;&c_d;&c_r;&c_a;&c_z;&c_d;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
      </snc>
      <snc mrk="libr.0otenejo.konservejo">
        <uzo tip="fak">LIB</uzo>
        <dif>
          Tenejo &ccirc;e biblioteko a&ubreve; <tld/>ovendejo en kiu oni
          konservas <tld/>ojn (<tld/>o-tenejo).
        </dif>
        <rim>
          <ref cel="biblio1.0o.ejo">Biblioteko<sncref/></ref> kutime
          implicas ke oni povas facile uzi la librojn por legado,
          privata a&ubreve; publika; <tld/>otenejo povas esti ekzemple loko
          kie oni konservas vendotajn librojn, kiujn oni do ne povas
          facile konsulti kiel librojn, sed kie ili estas konservataj
          kiel varo a&ubreve; kiel muzea&jcirc;o.  Cetere, biblioteko povas
havi
          sian <tld/>otenejon por libroj aparte valoraj a&ubreve; tre
malofte
          bezonataj, kiujn oni venigas per speciala mendo.
          <aut>Sergio Pokrovskij</aut>
        </rim>
        <trd lng="be">&c_b;&c_ib;&c_b;&c_l;&c_ib;&c_ja;&c_t;&c_e;&c_k;&c_a;
<klr>(&c_p;&c_a;&c_k;&c_o;&c_j;)</klr></trd>
        <trd lng="de">Bibliothek</trd>
        <trd
lng="el">&beta;&jota;&beta;&lambda;&jota;&omikron;&theta;&eta_ton;&kappa;&eta;</trd>
        <trd lng="es">biblioteca</trd>
        <trdgrp lng="fr">
          <trd>biblioth&egrave;que<klr tip="ind"> (r&eacute;serve de
livres)</klr></trd>,
          <trd>magasin<klr tip="ind"> (de livres &agrave;
vendre)</klr></trd>,
          <trd>r&eacute;serve<klr tip="ind"> (dans une
biblioth&egrave;que)</klr></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>k&ouml;nyvt&aacute;r</trd>,
          <trd>k&ouml;nyvrakt&aacute;r</trd>
        </trdgrp>
        <trd lng="lat">bibliotheca, ae</trd>
        <trd lng="nl">bibliotheek</trd>
        <trd lng="pt">biblioteca</trd>
        <trd
lng="ru">&c_k;&c_n;&c_i;&c_g;&c_o;&c_h;&c_r;&c_a;&c_n;&c_i;&c_l;&c_i;&c_shch;&c_je;</trd>
        <trd lng="tr">kitapl&inodot;k</trd>
      </snc>
    </drv>

    <drv mrk="libr.0otenisto">
      <kap><tld/>otenisto</kap>
      <snc mrk="libr.0otenisto.EKON">
        <uzo tip="fak">EKON</uzo>
        <dif>
          Komerca <ref tip="super" cel="kont.0isto">kontisto</ref>.
        </dif>
      </snc>
      <trd lng="be">&c_b;&c_u;&c_g;&c_a;&c_l;&c_mol;&c_t;&c_a;&c_r;</trd>
      <trd lng="de">Buchhalter</trd>
      <trd lng="fr">comptable</trd>
      <trd lng="hu">k&ouml;nyvel&odblac;</trd>
      <trd lng="nl">boekhouder</trd>
      <trd lng="ru">&c_b;&c_u;&c_h;&c_g;&c_a;&c_l;&c_t;&c_je;&c_r;</trd>
    </drv>

    <drv mrk="libr.0ovendejo">
      <kap><tld/>ovendejo</kap>
      <snc>
        <dif>
          Butiko, en kiu oni vendas <tld/>ojn.
        </dif>
        <ref tip="sin" cel="libr.0ejo.vendejo"><tld/>ejo <sncref/></ref>
      </snc>
      <trd lng="be">&c_k;&c_n;&c_ib;&c_g;&c_a;&c_r;&c_n;&c_ja;</trd>
      <trd lng="de">Buchhandlung</trd>
      <trd
lng="el">&beta;&jota;&beta;&lambda;&jota;&omikron;&pi;&omega;&lambda;&epsilon;&jota_ton;&omikron;</trd>
      <trd lng="es">librer&iacute;a</trd>
      <trd lng="fr">librairie</trd>
      <trd lng="hu">k&ouml;nyvesbolt</trd>
      <trd lng="nl">boekhandel</trd>
      <trd lng="pt">livraria</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_n;&c_i;&c_zh;&c_n;&c_a;&c_ja;
&c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
        <trd>&c_k;&c_n;&c_i;&c_zh;&c_n;&c_y;&c_j;
&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;</trd>
      </trdgrp>
      <trd lng="tr">kitabevi</trd>
    </drv>

    <drv mrk="libr.fest0o">
      <kap>fest<tld/>o</kap>
      <snc>
        <uzo tip="fak">TIP</uzo>
        <dif>
          Kolekto da artikoloj, eseoj a&ubreve; alispecaj tekstoj speciale
          verkitaj kaj eldonitaj por honori personon:
          <ekz>
            tiu &ccirc;i artikolo estis verkita por fest<tld/>o honore al
Ivo
            Lapenna okaze de ties 75-jara naski&gcirc;taga festo
            <fnt>
              <aut>Paul Neergaard</aut>,
              <vrk>La sencovasteco de kelkaj vortoj en Plena
                Ilustrita Vortaro de Esperanto</vrk>,
              <lok>Esperantologio (1999).</lok>
            </fnt>.
          </ekz>
        </dif>
        <ref tip="super" cel="artiko.0aro">artikolaro</ref>
      </snc>
      <trd lng="en">festschrift</trd>
      <trdgrp lng="en">
        <trd>festschrift</trd>,
        <trd>commemorative publication</trd>
      </trdgrp>
      <trd lng="es">homenaje</trd>
      <trd lng="fr">m&eacute;langes<klr> (recueil d'essais)</klr></trd>
      <trd lng="hu">eml&eacute;kk&ouml;tet</trd>
      <trd lng="nl">feestbundel</trd>
      <trd lng="ru">&c_ju;&c_b;&c_i;&c_l;&c_je;&c_j;&c_n;&c_y;&c_j;
&c_s;&c_b;&c_o;&c_r;&c_n;&c_i;&c_k; &c_s;&c_t;&c_a;&c_t;&c_je;&c_j;</trd>
    </drv>

    <drv mrk="libr.gast0o">
      <kap>gast<tld/>o</kap>
      <snc>
        <dif>
          <tld lit="L"/>o <sncref ref="libr.0o.notilo"/>, en kiu
          la gastoj a&ubreve; vizitantoj povas lasi sian subskribon
          a&ubreve; komenton.
        </dif>
      </snc>
      <trd lng="be">&c_k;&c_n;&c_ib;&c_g;&c_a;
&c_v;&c_o;&c_d;&c_g;&c_u;&c_k;&c_a;&c_w;</trd>
      <trd lng="de">G&auml;stebuch</trd>
      <trd lng="el">&beta;&jota;&beta;&lambda;&jota_ton;&omikron;
&epsilon;&pi;&jota;&sigma;&kappa;&epsilon;&pi;&tau;&omega_ton;&ny;</trd>
      <trd lng="fr">livre d'or</trd>
      <trd lng="hu">vend&eacute;gk&ouml;nyv</trd>
      <trd lng="pt">livro de visitantes</trd>
      <trd lng="ru">&c_k;&c_n;&c_i;&c_g;&c_a;
&c_o;&c_t;&c_z;&c_y;&c_v;&c_o;&c_v;</trd>
      <trd lng="sv">g&auml;stbok</trd>
    </drv>

    <drv mrk="libr.jar0o">
      <kap>jar<tld/>o</kap>
      <snc mrk="libr.jar0o.TIP">
        <dif>
          <tld lit="L"/>o &ccirc;iujare aperanta kaj prezentanta
          la &ccirc;efajn okazonta&jcirc;ojn de la jaro
          koncernantajn ian societon a&ubreve; fakon.
        </dif>
      </snc>
      <trd lng="be">&c_sh;&c_t;&c_o;&c_g;&c_o;&c_d;&c_n;&c_ib;&c_k;</trd>
      <trd lng="de">Jahrbuch</trd>
      <trd lng="en">yearbook</trd>
      <trd lng="es">anuario</trd>
      <trd lng="fr">annuaire</trd>
      <trd lng="hu">&eacute;vk&ouml;nyv</trd>
      <trd lng="nl">jaarboek</trd>
      <trd lng="ru">&c_je;&c_zh;&c_je;&c_g;&c_o;&c_d;&c_n;&c_i;&c_k;</trd>
      <trd lng="sk">ro&ccaron;enka</trd>
      <trd lng="sv">&aring;rsbok</trd>
    </drv>

    <drv mrk="libr.memor0o">
      <kap>memor<tld/>o</kap>
      <snc mrk="libr.memor0o.BELE">
        <uzo tip="fak">BELE</uzo>
        <dif>
          Rakonta verko pri propraj travivitaj okaza&jcirc;oj:
          <ekz>
            la generalo konfesis en sia memor<tld/>o, ke li sciis pri la
krimoj
            <fnt>
              <aut>Zlatko Ti&scirc;ljar</aut>,
              <vrk>Militkrima bu&gcirc;eto</vrk>,
              <bib>Monato</bib>,
              <lok>jaro 2002a, numero 12a, p. 8a</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_m;&c_e;&c_m;&c_u;&c_a;&c_r;&c_y;</trd>,
        <trd>&c_u;&c_s;&c_p;&c_a;&c_m;&c_ib;&c_n;&c_y;</trd>
      </trdgrp>
      <trd lng="fr">m&eacute;moires</trd>
      <trdgrp lng="hu">
        <trd>eml&eacute;kirat</trd>,
        <trd>eml&eacute;kk&ouml;nyv</trd>
      </trdgrp>
      <trd lng="nl">memoires</trd>
      <trd lng="de">Tagebuch</trd>
      <trdgrp lng="ru">
        <trd>&c_m;&c_je;&c_m;&c_u;&c_a;&c_r;&c_y;</trd>,
       
<trd>&c_v;&c_o;&c_s;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_n;&c_i;&c_ja;</trd>
      </trdgrp>
    </drv>
  </art>
