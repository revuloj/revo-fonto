<drv mrk="libr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="libr.0o.presajxo">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Bindita a&ubreve; bro&scirc;urita paperfoliaro kun presita teksto:
      <ekz>
        tiu &ccirc;i <tld/>o havas sesdek pa&gcirc;ojn
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz><ekz>
        nur <tld/>a <klr>(&ccirc;erpita el <tld/>oj, ne el sperto;
          kabineta)</klr> scienco;
      </ekz><ekz>
        koni la lingvon <tld/>e, sed ne libere;
      </ekz>
      <ekz>
        kun la <tld/>oj en la mano la infano iris en la lernejon
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
         en tiu &ccirc;i lingvo estos eldonataj <tld/>oj kaj gazetoj
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        li lasis gliti sian rigardon super la multajn <tld/>ojn, kiuj
        &gcirc;is la plafono staris &ccirc;e la muroj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Akvo de l' Vivo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="libr.0o.notilo">
    <dif>
      Samforma paperfoliaro destinita por ke oni skribu en &gcirc;i:
      <ekz>
        komerca <tld/>o;
      </ekz>
      <ekz>
        abako estas klare videbla restanta apud kont<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22971">Abako
          (me&hcirc;anika kalkulilo)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kajer.0o">kajero</ref>
    </refgrp>
  </snc>
  <snc mrk="libr.0o.divido">
    <dif>
      <ref tip="super" cel="part.0o">Parto</ref> de literatura
      verko, ordinare grupo de &ccirc;apitroj:
      <ekz>
        instruitulo revenis en sian landon, skribis <tld/>ojn pri la
        vera&jcirc;o en la mondo, 
        pri la bona&jcirc;o kaj bela&jcirc;o, kaj tiel li pasigis tagojn kaj jarojn
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o instruanta estas tre utila
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi decidis eldoni la <tld/>on <klr>[&#8230;]</klr> per apartaj
        kajeroj, 
        kiuj estos ellasataj en la da&ubreve;ro de la tuta jaro 1888 periode,
        kun intertempoj
        <fnt>
          <bib>DL</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="lat">liber, bri</trd>
  <trd lng="be">&c_k;&c_n;&c_ib;&c_g;&c_a;</trd>
  <trd lng="bg">&c_k;&c_n;&c_i;&c_g;&c_a;</trd>
  <trd lng="ca">llibre</trd>
  <trd lng="cs">kniha</trd>
  <trd lng="de">Buch</trd>
  <trd lng="el">&beta;&jota;&beta;&lambda;&jota_ton;&omikron;</trd>
  <trd lng="en">book</trd>
  <trd lng="es">libro</trd>
  <trd lng="fr">livre</trd>
  <trd lng="hu">k&ouml;nyv</trd>
  <trd lng="id">buku</trd>
  <trd lng="it">libro</trd>
  <trd lng="ja">&#26412; [<ind>&#12411;&#12435;</ind>]</trd>
  <trd lng="nl">boek</trd>
  <trd lng="pl">ksi&aogonek;&zdot;ka</trd>
  <trd lng="pt">livro</trd>
  <trd lng="ro">carte</trd>
  <trd lng="ru">&c_k;&c_n;&c_i;&c_g;&c_a;</trd>
  <trd lng="sk">kniha</trd>
  <trd lng="sv">bok</trd>
  <trd lng="tr">kitap</trd>
  <trd lng="vo">buk</trd>
  <trd lng="zh">&#20070; [<ind>sh&#363;</ind>]</trd>
</drv>

