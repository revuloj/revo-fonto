<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>libr</rad>/o
    </kap>
    <drv mrk="libr.0o">
      <kap>
        <ofc>*</ofc><tld/>o
      </kap>
      <snc mrk="libr.0o.presajxo">
        <dif>
          Bindita a&ubreve; bro&scirc;urita paperfoliaro kun presita teksto:
          <ekz>
            scienca, literatura <tld/>o;
          </ekz><ekz>
            nur <tld/>a (&ccirc;erpita el <tld/>oj, ne el sperto; kabineta)
            scienco;
          </ekz><ekz>
            koni la lingvon <tld/>e, sed ne libere.
          </ekz>
        </dif>
      </snc>
      <snc mrk="libr.0o.notilo">
        <dif>
          Samforma paperfoliaro destinita por ke oni skribu en &gcirc;i:
          <ekz>
            komerca <tld/>o;
          </ekz><ekz>
            memor<tld/>o;
          </ekz><ekz>
            <ind>not<tld/>o</ind>
            <trdgrp lng="ru">
              <trd>&c_b;&c_l;&c_o;&c_k;&c_n;&c_o;&c_t;</trd>,
              <trd>&c_z;&c_a;&c_p;&c_i;&c_s;&c_n;&c_a;&c_ja; &c_k;&c_n;&c_i;&c_zh;&c_k;&c_a;</trd>
            </trdgrp>
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="kajer.0o">kajero</ref>,
          <ref cel="regist.0o">registro</ref>.
        </refgrp>
      </snc>
      <snc mrk="libr.0o.divido">
        <dif>
          Nomo de grandaj dividoj de verko.
        </dif>
        <refgrp tip="sin">
          <ref cel="cxapit.0o">&ccirc;apitro</ref>,
          <ref cel="volum.0o">volumo</ref>.
        </refgrp>
      </snc>
      <trd lng="cs">kniha</trd>
      <trd lng="de">Buch</trd>
      <trd lng="en">book</trd>
      <trd lng="es">libro</trd>
      <trd lng="fr">livre</trd>
      <trd lng="it">libro</trd>
      <trd lng="lat">liber, bri</trd>
      <trd lng="nl">boek</trd>
      <trd lng="pl">ksi&aogonek;&zdot;ka</trd>
      <trd lng="ru">&c_k;&c_n;&c_i;&c_g;&c_a;</trd>
      <trd lng="sv">bok</trd>
      <trd lng="tr">kitap</trd>
    </drv>

    <drv mrk="libr.0ejo">
      <kap>
        <tld/>(oten)ejo
      </kap>
      <snc>
        <dif>
          &Ccirc;ambro, en kiu oni konservas multajn <tld/>ojn.
        </dif>
        <refgrp tip="vid">
          <ref cel="biblio.0o">biblioteko</ref>.
        </refgrp>
      </snc>
      <trd lng="es">biblioteca</trd>
      <trd lng="fr">biblioth&egrave;que</trd>
      <trd lng="lat">bibliotheca, ae</trd>
      <trd lng="nl">bibliotheek</trd>
      <trd lng="ru">&c_b;&c_i;&c_b;&c_l;&c_i;&c_o;&c_t;&c_je;&c_k;&c_a; (&c_k;&c_o;&c_m;&c_n;&c_a;&c_t;&c_a;)</trd>
      <trd lng="tr">kitapl&inodot;k</trd>
    </drv>

    <drv mrk="libr.0vendejo">
      <kap>
        <tld/>(ovend)ejo
      </kap>
      <snc>
        <dif>
          Butiko, en kiu oni vendas <tld/>ojn.
        </dif>
      </snc>
      <trd lng="de">Buchhandlung</trd>
      <trd lng="es">librer&iacute;a</trd>
      <trd lng="fr">librairie</trd>
      <trd lng="nl">boekhandel</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_n;&c_i;&c_zh;&c_n;&c_a;&c_ja; &c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
        <trd>&c_k;&c_n;&c_i;&c_zh;&c_n;&c_y;&c_j; &c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;</trd>
      </trdgrp>
      <trd lng="tr">kitabevi</trd>
    </drv>

    <drv mrk="libr.0eto">
      <kap>
        <tld/>eto
      </kap>
      <snc>
        <dif>
          Malgranda <tld/>o.
        </dif>
        <trd lng="lat">libellus, i</trd>
        <trd lng="nl">boekje</trd>
        <trdgrp lng="ru">
          <trd>&c_k;&c_n;&c_i;&c_zh;&c_k;&c_a;</trd>,
          <trd>&c_k;&c_n;&c_i;&c_zh;&c_je;&c_ch;&c_k;&c_a;</trd>
        </trdgrp>
      </snc>
      <snc>
        <dif>
          <tld lit="L"/>o enhavanta la parolojn de operverko.
        </dif>
        <trd lng="nl">libretto</trd>
        <trd lng="ru">&c_l;&c_i;&c_b;&c_r;&c_je;&c_t;&c_t;&c_o;</trd>
      </snc>
      <snc>
        <dif>
          Tiuj paroloj mem.
        </dif>
        <trd lng="fr">livret</trd>
        <trd lng="nl">libretto</trd>
        <trd lng="ru">&c_l;&c_i;&c_b;&c_r;&c_je;&c_t;&c_t;&c_o;</trd>
      </snc>
    </drv>

    <drv mrk="libr.0otenado">
      <kap>
        <tld/>otenado
      </kap>
      <snc>
        <dif>
          Enskribado de en- kaj el-spezoj en komercajn <tld/>ojn.
        </dif>
      </snc>
      <trd lng="de">Buchhaltung</trd>
      <trd lng="fr">comptabilit&eacute;</trd>
      <trd lng="nl">boekhouding</trd>
      <trd lng="ru">&c_b;&c_u;&c_h;&c_g;&c_a;&c_l;&c_t;&c_je;&c_r;&c_i;&c_ja;</trd>
    </drv>

    <drv mrk="libr.0tenisto">
      <kap>
        <tld/>tenisto
      </kap>
      <snc>
        <dif>
          Komerca kontisto.
        </dif>
      </snc>
      <trd lng="fr">comptable</trd>
      <trd lng="nl">boekhouder</trd>
      <trd lng="ru">&c_b;&c_u;&c_h;&c_g;&c_a;&c_l;&c_t;&c_je;&c_r;</trd>
    </drv>

    <drv mrk="libr.fest0o">
      <kap>
        fest<tld/>o
      </kap>
      <uzo tip="fak">TIP</uzo>
      <snc>
        <dif>
          Kolekto da artikoloj, eseoj a&ubreve; alispecaj tekstoj speciale
          verkitaj kaj eldonitaj por honori personon:
          <ekz>
            tiu &ccirc;i artikolo estis verkita por fest<tld/>o honore al Ivo
            Lapenna okaze de ties 75-jara naski&gcirc;taga festo
            <fnt>
              <aut>Paul Neergaard</aut>:
              <vrk>La sencovasteco de kelkaj vortoj en Plena
                Ilustrita Vortaro de Esperanto</vrk>,
              <lok>Esperantologio (1999).</lok>
            </fnt>.
          </ekz>
        </dif>
        <ref tip="super" cel="artiko.0aro">artikolaro</ref>
      </snc>
      <trd lng="de">Festschrift</trd>
      <trd lng="en">festschrift</trd>
      <trd lng="es">homenaje</trd>
      <trd lng="fr">m&eacute;langes</trd>
      <trd lng="nl">feestbundel</trd>
      <trd lng="ru">&c_ju;&c_b;&c_i;&c_l;&c_je;&c_j;&c_n;&c_y;&c_j; &c_s;&c_b;&c_o;&c_r;&c_n;&c_i;&c_k; &c_s;&c_t;&c_a;&c_t;&c_je;&c_j;</trd>
    </drv>

    <drv mrk="libr.gast0o">
      <kap>
        gast<tld/>o
      </kap>
      <snc>
        <dif>
          <tld lit="L"/>o_<sncref ref="libr.0o.notilo"/>, en kiu
          la gastoj a&ubreve; vizitantoj povas lasi sian subskribon
          a&ubreve; komenton.
        </dif>
      </snc>
      <trd lng="fr">livre d'or</trd>
      <trd lng="sv">g&auml;stbok</trd>
      <trd lng="ru">&c_k;&c_n;&c_i;&c_g;&c_a; &c_o;&c_t;&c_z;&c_y;&c_v;&c_o;&c_v;</trd>
    </drv>

    <drv mrk="libr.jar0o">
      <kap>
        jar<tld/>o
      </kap>
      <snc>
        <dif>
          <tld lit="L"/>o &ccirc;iujare aperanta kaj prezentanta
          la &ccirc;efajn okazonta&jcirc;ojn de la jaro
          koncernantajn ian societon a&ubreve; fakon.
        </dif>
      </snc>
      <trd lng="de">Jahrbuch</trd>
      <trd lng="en">yearbook</trd>
      <trd lng="es">anuario</trd>
      <trd lng="fr">annuaire</trd>
      <trd lng="nl">jaarboek</trd>
      <trd lng="ru">&c_je;&c_zh;&c_je;&c_g;&c_o;&c_d;&c_n;&c_i;&c_k;</trd>
      <trd lng="sv">&aring;rsbok</trd>
    </drv>
  </art>
