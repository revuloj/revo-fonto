<drv mrk="libr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="libr.0o.presajxo">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Bindita a&ubreve; bro&scirc;urita paperfoliaro kun presita teksto:
      <ekz>
        tiu &ccirc;i <tld/>o havas sesdek pa&gcirc;ojn
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        nur <tld/>a <klr>(&ccirc;erpita el <tld/>oj, ne el sperto;
        kabineta)</klr> scienco;
      </ekz>
      <ekz>
        koni la lingvon <tld/>e, sed ne libere.
      </ekz>
    </dif>
  </snc>
  <snc mrk="libr.0o.notilo">
    <dif>
      Samforma paperfoliaro destinita por ke oni skribu en &gcirc;i:
      <ekz>
        komerca <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kajer.0o">kajero</ref>
    </refgrp>
  </snc>
  <snc mrk="libr.0o.divido">
    <dif>
      <ref tip="super" cel="part.0o">Parto</ref> de literatura verko,
      ordinare grupo de &ccirc;apitroj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_n;&c_ib;&c_g;&c_a;</trd>
  <trd lng="bg">&c_k;&c_n;&c_i;&c_g;&c_a;</trd>
  <trd lng="cs">kniha</trd>
  <trd lng="de">Buch</trd>
  <trd lng="el">&beta;&jota;&beta;&lambda;&jota_ton;&omikron;</trd>
  <trd lng="en">book</trd>
  <trd lng="es">libro</trd>
  <trd lng="fr">livre</trd>
  <trd lng="hu">k&ouml;nyv</trd>
  <trd lng="it">libro</trd>
  <trd lng="ja">&#26412; [<ind>&#12411;&#12435;</ind>]</trd>
  <trd lng="lat">liber, bri</trd>
  <trd lng="nl">boek</trd>
  <trd lng="pt">livro</trd>
  <trd lng="pl">ksi&aogonek;&zdot;ka</trd>
  <trd lng="ro">carte</trd>
  <trd lng="ru">&c_k;&c_n;&c_i;&c_g;&c_a;</trd>
  <trd lng="sv">bok</trd>
  <trd lng="tr">kitap</trd>
  <trd lng="vo">buk</trd>
</drv>

<drv mrk="libr.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="libr.0ejo.vendejo">
    <ref tip="dif" cel="libr.0ovendejo"><tld/>ovendejo</ref>
    <ekz>
      <uzo tip="fak">ESP</uzo>
      <ind>Esperantista Centra <tld lit="L"/>ejo</ind>;
      <trd lng="fr">Librairie Centrale Esp&eacute;rantiste</trd>
      <trd lng="hu">K&ouml;zponti Eszperantista
      <ind>K&ouml;nyvkeresked&eacute;s</ind></trd>
    </ekz>
    <ekz>
      &scirc;i timis, ke &scirc;i trovos en la <tld/>ejo
      a&ccirc;etantojn, en kies &ccirc;eesto &scirc;i
      ne povus eldiri tion, pro kio &scirc;i venis
      <fnt><bib>Marta</bib></fnt>.
    </ekz>
    <trd lng="be">&c_k;&c_n;&c_ib;&c_g;&c_a;&c_r;&c_n;&c_ja;</trd>
    <trd lng="de">Buchhandlung</trd>
    <trd
    lng="el">&beta;&jota;&beta;&lambda;&jota;&omikron;&pi;&omega;&lambda;&epsilon;&jota_ton;&omikron;</trd>
    <trd lng="es">librer&iacute;a</trd>
    <trd lng="fr">librairie</trd>
    <trdgrp lng="hu">
      <trd>k&ouml;nyvesbold</trd>,
      <trd>k&ouml;nyvkeresked&eacute;s</trd>
    </trdgrp>
    <trd lng="nl">boekhandel</trd>
    <trd lng="pl">ksi&eogonek;garnia</trd>
    <trd lng="pt">livraria</trd>
    <trd lng="ro">libr&abreve;rie</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_n;&c_i;&c_zh;&c_n;&c_a;&c_ja;
&c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_k;&c_n;&c_i;&c_zh;&c_n;&c_y;&c_j;
&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;</trd>
    </trdgrp>
    <trd lng="tr">kitabevi</trd>
  </snc>
  <snc mrk="libr.0ejo.biblioteko">
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="biblio1.0o">biblioteko</ref>
    <ekz>
      &Ccirc;ernjaev estas fakestro <klr>[en la trusto
      Mos&hcirc;ozupr]</klr>,
      li bezonas en sia <tld/>ejo helpantinon
      <fnt><bib>Metrop</bib>, <lok>&ccirc;apitro 2</lok></fnt>.
    </ekz>
    <trd lng="hu">k&ouml;nyvt&aacute;r</trd>
  </snc>
</drv>

