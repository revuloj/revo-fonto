  <trd lng="sv">&aring;rsbok</trd>
</drv>

<drv mrk="libr.memor0o">
  <kap>memor<tld/>o</kap>
  <snc mrk="libr.memor0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Memoriga verko pri okaza&jcirc;oj, precipe kiujn la a&ubreve;toro
      spertis a&ubreve; vidis:
      <ekz>
        memor<tld/>o estas skribata <klr>...</klr> pri tiuj, kiuj timas la
        Eternulon
        <fnt><bib>MT</bib>, <lok>&Mal; 3:16</lok></fnt>;
      </ekz>
      <ekz>
        la generalo konfesis en sia memor<tld/>o, ke li sciis pri la
        krimoj
        <fnt>
        <aut>Zlatko Ti&scirc;ljar</aut>,
        <vrk>Militkrima bu&gcirc;eto</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 12a, p. 8a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_m;&c_u;&c_a;&c_r;&c_y;</trd>,
    <trd>&c_u;&c_s;&c_p;&c_a;&c_m;&c_ib;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;moires</trd>
  <trdgrp lng="hu">
    <trd>eml&eacute;kirat</trd>,
    <trd>memo&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">memoires</trd>
  <trd lng="de">Tagebuch</trd>
  <trd lng="pl">wspomnienia <klr>(ksi&aogonek;&zdot;ka)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_m;&c_u;&c_a;&c_r;&c_y;</trd>,
    <trd>&c_v;&c_o;&c_s;&c_p;&c_o;&c_m;&c_i;&c_n;&c_a;&c_n;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="libr.not0o">
  <kap>not<tld/>o</kap>
  <snc mrk="libr.not0o.KOMUNE">
    <dif>
      Blankfolia libro, eventuale kun anta&ubreve;presitaj datoj, en kiu oni
      skribas farenda&jcirc;ojn a&ubreve; privatajn notojn:
      <ekz>
        Brita <klr>...</klr> tiam kaj tiam skribis en malgranda not<tld/>o
        <fnt><bib>InfanTorent2</bib>, <lok>dua parto, 2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Notizbuch</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>agenda</trd>,
    <trd>calepin</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>jegyzetf&uuml;zet</trd>,
    <trd>hat&aacute;rid&odacute;napl&oacute;</trd>
  </trdgrp>
  <trd lng="pl">notes</trd>
  <trdgrp lng="ro">
    <trd>agend&abreve;</trd>,
    <trd>carnet</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_o;&c_k;&c_n;&c_o;&c_t;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_i;&c_s;&c_n;&c_a;&c_ja;
&c_k;&c_n;&c_i;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="libr.retotag0o">
  <kap>retotag<tld/>o</kap>
  <snc mrk="libr.retotag0o.blogo">
    <ref tip="dif" cel="blog.0o">blogo</ref>
