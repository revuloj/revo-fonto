<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ung</rad>/o
</kap>
<drv mrk="ung.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Korneca elkreska&jcirc;o &ccirc;e la ekstremo de homaj a&ubreve; bestaj
      fingroj:
      <ekz>
        tran&ccirc;i al si la <tld/>ojn;
      </ekz>
      <ekz>
        egaligi la <tld/>ojn de &ccirc;evalo, de bovo;
      </ekz>
      <ekz>
        la kato <tld/>ogratis, <tld/>ovundis min.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_o;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_z;&c_n;&c_o;&c_g;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_ib;&c_p;&c_c;&c_ju;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>ivin<klr> [homoj]</klr></trd>,
    <trd>karn</trd>,
    <trd>skilf</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>nehet</trd>,
    <trd>dr&aacute;p</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nagel</trd>,
    <trd>Kralle [bestoj]</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fingernail <klr>(p.p la fingro)</klr></trd>,
    <trd>toenail <klr>(p.p. la piedfingroj)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ongle</trd>,
    <trd>griffe</trd>,
    <trd>sabot</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&tsadi;&yod;&pe;&vav;&resh;&fnun;</trd>,
    <trd>&tet;&vav;&pe;&resh;</trd>
  </trdgrp>
  <trd lng="hu">k&ouml;r&ouml;m</trd>
  <trd lng="nl">nagel</trd>
  <trdgrp lng="pl">                                        
    <trd>paznokie&cacute;</trd>,                       
    <trd>pazur</trd>,                                   
  </trdgrp>
  <trd lng="pt">unha</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_o;&c_g;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_g;&c_o;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>nagel</trd>,
    <trd>klo</trd>
  </trdgrp>
</drv>

<drv mrk="ung.0ego">
  <kap>
    <ofc>*</ofc><tld/>ego
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Grandaj akraj ungoj de rabobestoj:
      <ekz>
        <tld/>egoj de leono, aglo
        <klr>(f)</klr>;
      </ekz>
      <ekz>
        tiri, liberigi iun el la <tld/>egoj (arbitra superpotenco) de la
        premantoj, ekspluatantoj.
      </ekz>
    </dif>
  <trdgrp lng="en">
    <trd>claw</trd>,
    <trd>talon <klr>(precipe p.p. birdoj)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>garra</trd>,
    <trd>presa</trd>
  </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_p;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_ib;&c_p;&c_c;&c_ju;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>kraban</trd>,
    <trd>skilf</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kralle</trd>,
    <trd>Klaue</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>griffe</trd>,
    <trd>serre</trd>
  </trdgrp>
  <trd lng="hu">karom</trd>
  <trd lng="nl">klauw</trd>
  <trd lng="pl">szpon</trd>
  <trd lng="ru">&c_k;&c_o;&c_g;&c_o;&c_t;&c_mol;</trd>
  <trd lng="sv">klo</trd>
</drv>
</art>
