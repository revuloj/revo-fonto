<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ung.xml,v 1.32 2023/09/05 09:43:52 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>ung</rad>/o
</kap>
<drv mrk="ung.0o">
   <kap>
     <ofc>*</ofc><tld/>o
   </kap>
   <uzo tip="fak">ANA</uzo>
   <snc>
     <dif>
       Korneca elkreska&jcirc;o &ccirc;e la ekstremo de homaj a&ubreve;
bestaj
       fingroj:
       <ekz>
         tran&ccirc;i al si la <tld/>ojn;
       </ekz>
       <ekz>
         egaligi la <tld/>ojn de &ccirc;evalo, de bovo;
       </ekz>
       <ekz>
         la kato <tld/>ogratis, <tld/>ovundis min.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_o;&c_g;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_a;&c_z;&c_n;&c_o;&c_g;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_k;&c_ib;&c_p;&c_c;&c_ju;&c_r;</trd>
   </trdgrp>
   <trd lng="bo">&#xF66;&#xF7A;&#xF53;&#xF0B;&#xF58;&#xF7C;&#xF0B;</trd>
  <trdgrp lng="br">
     <trd>ivin<klr> [homoj]</klr></trd>,
     <trd>karn</trd>,
     <trd>skilf</trd>
   </trdgrp>
   <trd lng="ca">ungla</trd>   
   <trdgrp lng="cs">
     <trd>nehet</trd>,
     <trd>dr&aacute;p</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Nagel</trd>,
     <trd>Kralle [bestoj]</trd>
   </trdgrp>
   <trdgrp lng="en">
     <trd>fingernail <klr>(p.p la fingro)</klr></trd>,
     <trd>toenail <klr>(p.p. la piedfingroj)</klr></trd>
   </trdgrp>
   <trd lng="es">u&ntilde;a</trd>
   <trd lng="fa">&a_n;&a_A;&a_kh;&a_n;</trd>
   <trd lng="fi">kynsi</trd>
   <trdgrp lng="fr">
     <trd>ongle</trd>,
     <trd>griffe</trd>,
     <trd>sabot</trd>
   </trdgrp>
   <trdgrp lng="he">
     <trd>&tsadi;&yod;&pe;&vav;&resh;&fnun;</trd>,
     <trd>&tet;&vav;&pe;&resh;</trd>
   </trdgrp>
   <trd lng="hu">k&ouml;r&ouml;m</trd>
   <trd lng="id">kuku</trd>
   <trd lng="it">unghia</trd>
   <trd lng="ja">&#x3064;&#x3081;</trd>
  <trd lng="nl">nagel</trd>
   <trdgrp lng="pl">
     <trd>paznokie&cacute;</trd>,
     <trd>pazur</trd>,
   </trdgrp>
   <trd lng="pt">unha</trd>
   <trdgrp lng="ru">
     <trd>&c_n;&c_o;&c_g;&c_o;&c_t;&c_mol;</trd>,
     <trd>&c_k;&c_o;&c_g;&c_o;&c_t;&c_mol;</trd>
   </trdgrp>
   <trdgrp lng="sv">
     <trd>nagel</trd>,
     <trd>klo</trd>
   </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x456;&#x433;&#x43E;&#x442;&#x44C;</trd>,
    <trd>&#x43A;&#x456;&#x433;&#x43E;&#x442;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="ung.0ego">
   <kap>
     <ofc>*</ofc><tld/>ego
   </kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Grandaj akraj ungoj de rabobestoj:
       <ekz>
         <tld/>egoj de leono, aglo
         <klr>(f)</klr>;
       </ekz>
       <ekz>
         tiri, liberigi iun el la <tld/>egoj (arbitra superpotenco) de
         la
         premantoj, ekspluatantoj.
       </ekz>
     </dif>
   <trdgrp lng="en">
     <trd>claw</trd>,
     <trd>talon <klr>(precipe p.p. birdoj)</klr></trd>
   </trdgrp>
   <trd lng="es">garra</trd>
   <trdgrp lng="pt">
     <trd>garra</trd>,
     <trd>presa</trd>
   </trdgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_k;&c_ib;&c_p;&c_je;&c_c;&c_mol;</trd>,
     <trd>&c_k;&c_ib;&c_p;&c_c;&c_ju;&c_r;</trd>
   </trdgrp>
   <trdgrp lng="br">
     <trd>kraban</trd>,
     <trd>skilf</trd>
   </trdgrp>
   <trdgrp lng="ca">
     <trd>grapa</trd>,
     <trd>urpa</trd>,
     <trd>arpa</trd>
   </trdgrp>
   <trdgrp lng="de">
     <trd>Kralle</trd>,
     <trd>Klaue</trd>
   </trdgrp>
   <trdgrp lng="fa">
     <trd>&f_ch;&a_n;&f_g;<klr tip="ind"> (&a_j;&a_A;&a_n;&a_w;&a_r;&a_A;&a_n;)</klr></trd>,
     <trd>&f_ch;&a_n;&f_g;&a_A;&a_l;<klr tip="ind"> (&a_j;&a_A;&a_n;&a_w;&a_r;&a_A;&a_n;)</klr></trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>griffe</trd>,
     <trd>serre</trd>
   </trdgrp>
   <trd lng="hu">karom</trd>
   <trd lng="id">cakar</trd>
   <trd lng="it">artiglio</trd>
   <trd lng="ja">&#x3064;&#x3081;</trd>
  <trd lng="nl">klauw</trd>
   <trd lng="pl">szpon</trd>
   <trd lng="ru">&c_k;&c_o;&c_g;&c_o;&c_t;&c_mol;</trd>
   <trd lng="sv">klo</trd>
  <trd lng="uk">&#x43F;&#x430;&#x437;&#x443;&#x440; (&#x43B;&#x435;&#x432;&#x430; &#x456; &#x442;.&#x43F;.)</trd>
</drv>
</art>
