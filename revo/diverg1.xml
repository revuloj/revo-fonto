<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: diverg1.xml,v 1.21 2023/11/21 15:28:31 revo Exp $">
  <kap>
    <rad>diver&gcirc;enc</rad>/o
  </kap>
<drv mrk="diverg1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <klr>(de <ref tip="vid" cel="vektor.0a">vektora</ref>
      <ref cel="kamp.0o.MAT">kampo</ref> inter samdimensiaj
      spacoj)</klr>
      <ref tip="super" cel="skalar.0a">Skalara</ref> kampo,
      kiu &ccirc;e &ccirc;iu punkto egalas al la sumo de la
      <ref tip="vid" cel="deriv.parta0ajxo">partaj deriva&jcirc;oj</ref>
      de la komponantoj de la vektora kampo la&ubreve; &ccirc;iu la&ubreve;vica
      <ref tip="vid" cel="koordi.0o.MAT">koordinato</ref>:
      <ekz>
        la <tld/>on de <frm am="E">E</frm> oni kutime signas per
        <frm am="nabla*E">&#8711;&middot;E</frm> a&ubreve;
        <frm am="&quot;div&quot; E=sum del_iE_i">div E =
        &#8721;&#8706;<sub><k>i</k></sub>E<sub><k>i</k></sub></frm>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_v;&c_e;&c_r;&c_g;&c_je;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="de">Divergenz</trd>
  <trd lng="en">divergence<klr tip="ind"> (of a field)</klr></trd>
  <trd lng="es">divergencia</trd>
  <trd lng="fa">&a_d;&f_y;&a_w;&a_r;&f_zh;&a_A;&a_n;&a_s;</trd>
  <trd lng="fr">divergence<klr tip="ind"> (d'un champ)</klr></trd>
  <trd lng="ja">&#x767A;&#25955;  <pr>&#x306F;&#12387;&#12373;&#12435;</pr></trd>
  <trd lng="pl">dywergencja</trd>
  <trd lng="pt">diverg&ecirc;ncia</trd>
  <trd lng="ru">&c_d;&c_i;&c_v;&c_je;&c_r;&c_g;&c_je;&c_n;&c_c;&c_i;&c_ja;</trd>
  <trd lng="uk">&c_d;&c_i;&c_v;&c_je;&c_r;&c_g;&c_je;&#x43D;&c_c;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x504F;&#x79BB; <pr>pi&amacron;nl&#xED;</pr></trd>,
    <trd>&#x53D1;&#25955;&#x91CF; <pr>f&amacron;s&#xE0;nli&#xE0;ng</pr></trd>,
    <trd>&#25955;&#x5EA6; <pr>s&#xE0;nd&#xF9;</pr></trd>,
    <trd>&#x53D1;&#25955;&#24615; <pr>f&amacron;s&#xE0;nx&#xEC;ng</pr></trd>,
    <trd>&#x53D1;&#25955; <pr>f&amacron;s&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="diverg1.sen0a">
  <kap>sen<tld/>a</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(p.p. vektora <ref tip="super" cel="kamp.0o.MAT">kampo</ref>)</klr>
      Kies <ref cel="diverg1.0o"><tld/>o</ref> egalas al nulo.
    </dif>
    <ref tip="vid" cel="potenc1.vektora0o">vektora potencialo.</ref>
  </snc>
  <adm>
  Mi ne trovis auxtoritatan fonton, sed la termino estas tute
  logika. <aut>MB</aut>
  </adm>
  <trd lng="be">&c_z; &c_n;&c_u;&c_l;&c_ja;&c_v;&c_o;&c_j; <ind>&c_d;&c_y;&c_v;&c_e;&c_r;&c_g;&c_je;&c_n;&c_c;&c_y;&c_ja;&c_j;</ind></trd>
  <trd lng="de">divergenzfrei<klr tip="ind"> (-es
  Vektorfeld)</klr></trd>
  <trd lng="en">zero-divergence<klr tip="amb"> (field)</klr></trd>
  <trdgrp lng="fr">
    <trd><klr tip="ind">(champ [&agrave; flux]) </klr>conservatif</trd>,
    <trd><klr tip="ind">(champ) </klr>indivergentiel</trd></trdgrp>
  <trd lng="he">&#x5DE;&#x5B4;&#x5E1;&#x5EA;&#x5B7;&#x5E2;&#x5B5;&#x5E3;</trd>
  <trd lng="pl"><klr tip="amb">(pole) </klr>bez&zacute;r&oacute;d&lstroke;owe</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_je;&c_n;&c_o;&c_i;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_je;<klr tip="amb"> (&c_p;&c_o;&c_l;&c_je;)</klr></trd>
</drv>
</art>

