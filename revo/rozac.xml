<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id$">
<kap><rad>rozac</rad>/oj</kap>
<drv mrk="rozac.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Rosaceae</trd>), al kiu apartenas
      ekzemple
      <refgrp tip="sub">
        <ref cel="roz.0o">rozo</ref>,
        <ref cel="pom.0ujo0arbo">pomujo</ref>,
        <ref cel="pir.0ujo0arbo">pirujo</ref>,
        <ref cel="prun.0ujo0arbo">prunujo</ref>,
        <ref cel="frag.0ujo">fragujo</ref>
      </refgrp>
      k.a.
    </dif>
  </snc>
  <trd lng="de">Rosengew&auml;chse</trd>
  <trd lng="nl">rozenfamilie</trd>
  <trd lng="ru">&c_r;&c_o;&c_z;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">rosv&auml;xter</trd>
</drv>
</art>

