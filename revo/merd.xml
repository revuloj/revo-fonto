<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: merd.xml,v 1.10 2020/08/25 20:10:34 revo Exp $">
<kap>
  <rad>merd</rad>/o
</kap>
<drv mrk="merd.0o">
  <kap><tld/>o</kap> 
  <uzo tip="stl">VULG</uzo>
  <snc mrk="merd.0o.fekajxo">
    <dif>
      <ref tip="dif" cel="fek.0ajxo">Feka&jcirc;o</ref>:
      <ekz>
        oni devas laborigi min kiel lernanton de <tld/>o&ccirc;erpisto en &hcirc;olerbarakaro
        <fnt>
          <bib>Monato</bib>
          <aut>Nina Kor&jcirc;enevskaja-Gouriou</aut>
          <vrk><url ref="https://www.monato.be/2006/008819.php">Kial lundo ekas sabate?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu libro estas <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi devus perei jam infana&gcirc;e: la mondo perdus unu <tld/>ulinon
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2014/011218.php"
              >Sen besto-testoj mi estus mortinta ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="acx.0">a&ccirc;-</ref>
  </snc>
  <trd lng="ca">merda</trd>
  <trdgrp lng="cs">
    <trd>fek&aacute;lie</trd>,
    <trd>hovno</trd>,
    <trd>lejno</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schei&szlig;e</trd>,
    <trd>Kacke</trd>
  </trdgrp>
  <trd lng="en">shit</trd>
  <trd lng="fr">merde</trd>
  <trdgrp lng="nl">
    <trd>kak<klr tip="ind"> (ontr.)</klr></trd>,
    <trd>poep<klr tip="ind"> (ontr.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>g&oacute;wno</trd>,
    <trd>kupa</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>c&abreve;cat</trd>,
    <trd>caca</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fek&aacute;lie</trd>,
    <trd>hovno</trd>,
    <trd>lajno</trd>
  </trdgrp>
</drv>
</art>

