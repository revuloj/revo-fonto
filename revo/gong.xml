<art mrk="$Id$">
<kap>
  <rad>gong</rad>/o
</kap>
<drv mrk="gong.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Orientdevena, la&ubreve;ta, da&ubreve;rsona <ref tip="super"
      cel="instru2.frap0o">frapinstrumento</ref> el metala disko,
      kiun oni frapas per tuke vindita martelo:
      <ekz>
        &ccirc;efe&ubreve;nuko de la Palaco Daming <klr>...</klr> venis en palankeno,
        akompanate per ombreloj, <tld/>oj kaj tamtamoj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 13a, p. 175a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gong</trd>
  <trd lng="es">gong</trd>
  <trd lng="fr">gong</trd>
  <trd lng="nl">gong</trd>
  <trd lng="ru">&c_g;&c_o;&c_n;&c_g;</trd>
</drv>
</art>
