  <drv mrk="kontraux.0eco">
    <kap><tld/>eco</kap>
    <snc mrk="kontraux.0eco.KOMUNE">
      <dif>
        Kontrasto, granda diferenco, malakordo:
        <ekz>
          akra kaj profunda <tld/>eco;
        </ekz>
        <ekz>
          pozicio <klr>[&#x2026;]</klr> en plena <tld/>eco kun la homa
          anatomio kaj kun la le&gcirc;oj de la egalpezo
          <fnt>
            <bib>BdV</bib>
          </fnt>;
        </ekz>
        <ekz>
          venki <tld/>econ por trovi harmonion reciprokan
          <fnt>
            <aut>Michael Ende, trad. Wolfram Diestel</aut>
            <vrk>La Sen&ccirc;esa Rakonto</vrk>
            <lok>La Urbo de Imperiestroj</lok>
          </fnt>;
        </ekz>
        <ekz>
          konfesia <tld/>eco al la universalaj supereco kaj senerareco,
          kiun la papo atribuas al si
          <fnt>
          <aut>Duilio Magnani</aut>
            <vrk><url ref="&Monato;/2001/007299.php">Fondi eklezion?</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 4</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="abism.0o">abismo</ref>,
        <ref cel="antipo.0o">antipodo</ref>
      </refgrp>
    </snc>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_o;&c_c;&c_ib;&c_l;&c_je;&c_g;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;&c_l;&c_je;&c_g;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_ja;&c_r;&c_e;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="de">Gegens&auml;tzlichkeit</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_r;&c_je;&c_ch;&c_i;&c_je;</trd>
    </trdgrp>
    <trd lng="nl">tegenstelling</trd>
    <trd lng="pl">przeciwie&nacute;stwo</trd>
    <trdgrp lng="ro">
