        <trd>ellenz&odblac;</trd>
      </trdgrp>
      <trd lng="nl">tegengesteld</trd>
      <trdgrp lng="pl">
        <trd>sprzeczny</trd>,
        <trd>przeciwleg&lstroke;y</trd>,
        <trd>przeciwny</trd>
      </trdgrp>   
      <trdgrp lng="ro">
        <trd>contrar</trd>,
        <trd>opus</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_s;&c_t;&c_o;&c_ja;&c_shch;&c_i;&c_j;</trd>
      </trdgrp>
      <trdgrp lng="zh">
    <trd>&#x76F8;&#x53CD; <pr>xi&#x101;ngf&#x1CE;n</pr></trd>,
    <trd>&#x76F8;&#x80CC; <pr>xi&#x101;ngb&#xE8;i</pr></trd>,
