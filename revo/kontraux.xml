        <trd>para</trd>,
        <trd>em <ind>troca</ind> de</trd>,
        <trd>em <ind>rela&ccedil;&atilde;o</ind> a</trd>,
        <trd>por</trd>                            
      </trdgrp>
      <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;</trd>
    </snc>
