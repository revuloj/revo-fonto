  <drv mrk="kontraux.0ajxo">
    <kap><tld/>a&jcirc;o</kap>
    <snc mrk="kontraux.0ajxo.KOMUNE">
      <dif>
        <tld lit="K"/>a cirkonstanco, kiu kontrastas kun alia
        a&ubreve; kiu malhelpas ian planon:
        <ekz>
          estas multaj <tld/>a&jcirc;oj en lia konduto;
        </ekz>
        <ekz>
          kiom da <tld/>a&jcirc;oj kaj &ccirc;agrenoj povus esti
          evitataj, se en la
          turismo estus enkondukata internacia lingvo;
        </ekz>
        <ekz>
          la prefikso &leftquot;mal&rightquot; signifas rektan <tld/>a&jcirc;on de la ideo
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          <nom>Stetten</nom> estis rekte la <tld/>a&jcirc;o de sia amiko
          <fnt>
            <bib>FK</bib>
          </fnt>;
        </ekz>
        <ekz>
          li travoja&gcirc;is la tutan mondon, por trovi <klr>[veran
          re&gcirc;idinon]</klr>, sed &ccirc;ie trovi&gcirc;is ia <tld/>a&jcirc;o
          <fnt>
            <bib>Fab1</bib>
            <lok>Re&gcirc;idino sur pizo</lok>
          </fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_o;&c_c;&c_ib;&c_l;&c_je;&c_g;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;&c_l;&c_je;&c_g;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_c;&c_ib;&c_l;&c_je;&c_g;&c_l;&c_a;&c_je; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>,
      <trd>&c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_mol;&c_l;&c_je;&c_g;&c_l;&c_a;&c_je; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>,
      <trd>&c_p;&c_je;&c_r;&c_a;&c_sh;&c_k;&c_o;&c_d;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Gegensatz</trd>,
      <trd>Hindernis</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>adversidad</trd>,
      <trd>problema</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>adversit&eacute;</trd>,
      <trd>contradiction</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bariera</trd>,
      <trd>przeszkoda</trd>,
      <trd>rygiel</trd>,
      <trd>zapora</trd>
      <trd>zasuwka</trd>
    </trdgrp>   
    <trdgrp lng="ro">
      <trd>barier&abreve;</trd>,
      <trd>obstacol</trd>,
      <trd>bol&#x21B;</trd>
      <trd>baraj</trd>,
      <trd>&#x219;urub</trd>
    </trdgrp> 
    <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_i;&c_v;&c_o;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;</trd>
    <trdgrp lng="zh">
    <trd>&#x969C;&#x788D; <pr>zh&#xE0;ng&#xE0;i</pr></trd>,
    <trd>&#x77DB;&#x76FE; <pr>m&#xE1;od&#xF9;n</pr></trd>,
    <trd>&#x53CD;&#x9AD8;&#x6F6E; <pr>f&#x1CE;ng&#x101;och&#xE1;o</pr></trd>,
