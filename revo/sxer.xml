<vortaro>
  <art mrk="$Id: sxer.xml,v 1.6 2023/11/30 18:32:20 afido Exp $">
    <kap>
      <rad>&scirc;er</rad>/o
    </kap>

<drv mrk="sxer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Roka <ref tip="super" cel="insul.0eto">insuleto</ref>
      formita dum la <ref cel="epok.glaci0o">glaciepoko</ref>,
      ofte aperantaj grupe &ccirc;e skandinavaj kaj kanadaj bordoj:
      <ekz>
        la malnova drinkrondo de Novponto <klr>[&#8230;]</klr> povos trankvile resti apud la
        forno, e&ccirc; se fari&gcirc;os iom varme inter la <tld/>oj kaj en la marbordaj provincoj
        <fnt>
          <aut>Stellan Engholm</aut>
          <vrk><url ref="&Tekstaro;?nomo=homoj-sur-la-tero">Homoj sur la tero</url></vrk>
          <lok>Feli&ccirc;a homo, &ccirc;ap. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi sendube povus oferi kelkajn nudajn <tld/>ojn <klr>[&#8230;]</klr>, kie ni povus
        resti en paco
        <fnt>
          <bib>LOdE</bib>
          <aut>Lagerl&ouml;f, Selma, trad. Sten Johansson</aut>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto">La mirinda voja&gcirc;o de Nils
          Holgersson</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/2/2d/Lempisaari%2C_Naantali%2C_Finland..jpg/640px-Lempisaari%2C_Naantali%2C_Finland..jpg"
    prm="CC BY 2.5"><tld/>oj &ccirc;e Naantali, Finnlando
        <fnt>
          <aut>Plenz, 15 October 2006, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Lempisaari,_Naantali,_Finland..jpg">&WCW;/File:Lempisaari,_Naantali,_Finland..jpg</url></lok>
        </fnt>
    </bld>
  </snc>
    <trd lng="be">&c_sh;&c_h;&c_e;&c_r;&c_a;</trd>
    <trd lng="de">Sch&auml;re</trd>
    <trd lng="en">skerry</trd>
    <trd lng="fr">&eacute;cueil</trd>
    <trd lng="is">sker</trd>
    <trd lng="no">skj&aelig;r</trd>
    <trd lng="pl">szkier</trd>
    <trd lng="sv">sk&auml;r</trd>
    <trd lng="uk">&c_sh;&c_h;&c_je;&c_r;</trd>
</drv>
