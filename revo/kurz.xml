<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kurz.xml,v 1.32 2023/12/03 09:59:04 revo Exp $">
<kap>
  <ofc>9</ofc><rad>kurz</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kurz.0o">
  <kap>
    <ofc>9</ofc><tld/>o
  </kap>
  <snc mrk="kurz.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Borsa a&ubreve; eksterborsa <ref tip="super" cel="prez.0o">prezo</ref> a&ubreve; valoro de akcioj, 
      obligacioj, kambioj, devizoj, mono kaj varoj:
      <ekz>
        alta, meza, malalta <tld/>o;
      </ekz>
      <ekz>
        radio-aparato <tld/>ojn krias de l' sterlingo
        <fnt>
          <bib>LSB</bib>
          <lok>Libertempo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la e&ubreve;ro kontra&ubreve; la dolaro malkreskis
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006821.html">E&ubreve;ropa Unio: E&ubreve;ro unujara</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        akciaj <tld/>oj alti&gcirc;is kaj la <tld/>o de la eno malalti&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="https://www.monato.be/2014/011179.php">Abenomiko, a&ubreve;daca defio</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kurzo unuafoje e&ccirc; dum entute iom pli ol horo falis &gcirc;is 0,999, kvankam je la
        fino de la tago &gcirc;i denove estis super 1
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006821.html">E&ubreve;ropa Unio: E&ubreve;ro unujara</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj de valutoj estas kiel tiuj de akcioj: kiel akcio dependas de la
        kompanio kiun &gcirc;i reprezentas, valuto reprezentas la valutan regionon
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006821.html">E&ubreve;ropa Unio: E&ubreve;ro unujara</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        eksterlandaj entreprenistoj interesi&gcirc;as pri kreditaj populareclistoj, akciaj
        <tld/>oj ktp
        <fnt>
          <bib>Kon11</bib>
          <lok>Scienco</lok>
        </fnt>;
      </ekz>
      <ekz>
        en ekonomika praktiko ekzistas avera&gcirc;a <tld/>o kalkulita el unuopaj kurzoj (po
        tago, monato, ktp)
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Hin&scaron;t</aut>
          <vrk><url ref="https://www.monato.be/2005/008517.php">Suplemento al ekonomiaj terminaroj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        fiksa inter&scirc;an&gcirc;a <tld/>o de la peso kontra&ubreve; la dolaro
        <fnt>
          <bib>Monato</bib>
          <aut>Andr&eacute; Staes</aut>
          <vrk><url ref="https://www.monato.be/2005/008765.php">Pinta sekureco &ccirc;e pinta kunsido</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la japanaj &scirc;tatobligacioj estas stabila
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="https://www.monato.be/2012/010776.php">Konkurado al &scirc;tata paneo</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_r;&c_s; <klr tip="amb">(&c_b;&c_ib;&c_r;&c_zh;&c_a;&c_v;&c_y;,
  &c_v;&c_a;&c_l;&c_ju;&c_t;&c_n;&c_y;)</klr></trd>
  <trd lng="de">Kurs</trd>
  <trdgrp lng="en">
    <trd>price</trd>,
    <trd>rate</trd>,
    <trd>quotation</trd>
  </trdgrp>
  <trd lng="es">valor burs&aacute;til</trd>
  <trd lng="fr">cours<klr> (de la bourse)</klr></trd>
  <trd lng="hu">&aacute;rfolyam</trd>
  <trdgrp lng="ja">
    <trd>&#x76F8;&#x5834;  <pr>&#x305D;&#x3046;&#x3070;</pr></trd>,
    <trd>&#x6642;&#x4FA1;  <pr>&#x3058;&#x304B;</pr></trd>
  </trdgrp>
  <trd lng="nl">koers<klr tip="ind"> (v.munt bijv.)</klr></trd>
  <trdgrp lng="pl">
    <trd>kurs <klr>(gie&lstroke;da)</klr></trd>,
    <trd>notowania</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>c&acirc;mbio</trd>,
    <trd>cota&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ro">curs</trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_s; <klr>(&c_b;&c_i;&c_r;&c_zh;&c_je;&c_v;&c_o;&c_j;,
  &c_v;&c_a;&c_l;&c_ju;&c_t;&c_n;&c_y;&c_j;)</klr></trd>
  <trd lng="uk">&c_k;&c_u;&c_r;&c_s; (&c_b;&c_ib;&c_r;&c_zh;&c_o;&c_v;&c_i;&c_j;)</trd>
  <trdgrp lng="zh">
    <trd>&#x4EF7;&#x94B1; <pr>ji&#xE0;qian</pr></trd>,
    <trd>&#x50F9;&#x9322; <pr>ji&#xE0;qian</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kurz.0a">
  <kap><tld/>a, <var><kap><tld/>ohava</kap></var></kap>
  <snc mrk="kurz.0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Negocata en la borso.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>jsouc&iacute; v kurzu</trd>,
    <trd>maj&iacute;c&iacute; dobr&yacute; kurz</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>b&ouml;rslich <ind>notiert</ind></trd>
  </trdgrp>
  <trd lng="hu">t&odblac;zsd&eacute;n <ind>jegyzett</ind></trd>
  <trd lng="ja">&#x4E0A;&#x5834;&#x3055;&#x308C;&#x305F;  <pr>&#x3058;&#x3087;&#x3046;&#x3058;&#x3087;&#x3046;&#x3055;&#x308C;&#x305F;</pr></trd>
  <trd lng="nl">genoteerd (<ind>koers</ind>)</trd>
  <trd lng="pl">notowany <klr>(gie&lstroke;da)</klr></trd>
  <trd lng="ro">cotate</trd>
  <trd lng="sk">maj&uacute;ci dobr&yacute; kurz</trd>
  <trdgrp lng="zh">
    <trd>&#x6458;&#x8B6F; <pr>zh&#x101;iy&#xEC;</pr></trd>,
    <trd>&#x6458;&#x8BD1; <pr>zh&#x101;iy&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kurz.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="kurz.sen0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Nehavanta kurzon, ne negocata en la borso.
    </dif>
  </snc>
  <trd lng="cs">neobchodovateln&yacute;</trd>
  <trdgrp lng="de">
     <trd>nicht notiert</trd>,
     <trd>gestrichen</trd>
  </trdgrp>
  <trd lng="hu">t&odblac;zsd&eacute;n nem <ind>jegyzett</ind></trd>
  <trd lng="ja">&#x53D6;&#x5F15;&#x76F8;&#x5834;&#x306E;&#x306A;&#x3044;  <pr>&#x3068;&#x308A;&#x3072;&#x304D;&#x305D;&#x3046;&#x3070;&#x306E;&#x306A;&#x3044;</pr></trd>
  <trd lng="nl">niet genoteerd (<ind>koers</ind>)</trd>
  <trd lng="pl">nienotowany <klr>(gie&lstroke;da)</klr></trd>
  <trd lng="ro">neadmis&abreve; la tranzac&#x21B;ionare</trd>
  <trd lng="sk">neozna&ccaron;en&yacute; <klr>(papier na burze)</klr> (pe&ncaron;a&zcaron;.)</trd>
  <trd lng="uk">&c_ja;&c_k;&c_i;&c_j; &c_n;&c_je; &c_p;&c_r;&c_o;&c_d;&c_a;&c_ju;&c_t;&c_mol; &c_n;&c_a; &c_b;&c_ib;&c_r;&c_zh;&c_ib;</trd>
</drv>

<drv mrk="kurz.eksterborsa0o">
  <kap>eksterborsa <tld/>o, <var><kap>neoficiala
  <tld/>o</kap></var></kap>
  <snc mrk="kurz.eksterborsa0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="K"/>o notata kaj uzata ekster la borso.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_a;&c_b;&c_ib;&c_r;&c_zh;&c_a;&c_v;&c_y; &c_k;&c_u;&c_r;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>mimoburzovn&iacute; kurz</trd>,
    <trd>neofici&aacute;ln&iacute; kurz</trd>
  </trdgrp>
  <trd lng="de">au&szlig;erb&ouml;rslicher Kurs</trd>
  <trdgrp lng="hu">
     <trd>t&odblac;zsd&eacute;n k&iacute;v&uuml;li <ind>&aacute;rfolyam</ind></trd>,
     <trd>nem hivatalos <ind>&aacute;rfolyam</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x5834;&#x5916;&#x76F8;&#x5834;  <pr>&#x3058;&#x3087;&#x3046;&#x304C;&#x3044;&#x305D;&#x3046;&#x3070;</pr></trd>
  <trdgrp lng="pl">
    <trd>kurs nieoficjalny</trd>,
    <trd>notowania nieoficjalne</trd>
  </trdgrp>
  <trd lng="ro">rata neoficial&abreve;</trd>
  <trd lng="sk">ne&uacute;radn&yacute; kurz</trd>
</drv>

<drv mrk="kurz.oficiala0o">
  <kap>oficiala <tld/>o</kap>
  <snc mrk="kurz.oficiala0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      &Ccirc;iutage en la borso notata <tld/>o:
      <ekz>
        estis konfirmita la oficiala <tld/>o de e&ubreve;ro kaj nacia valuto
        lido<klr>[&#x2026;]</klr>: 1 e&ubreve;ro da&ubreve;re valoros 3,4528 lidojn
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="https://www.monato.be/2014/011369.php">Lido foriras, e&ubreve;ro alvenas</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        konvertu la dolaran abonprezon al la ruslandaj rubloj la&ubreve; la oficiala <tld/>o,
        aldoninte 2% por kovri la diferencon inter la oficiala kaj banka vendokurzoj
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 11 (61)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_r;&c_zh;&c_a;&c_v;&c_y; &c_k;&c_u;&c_r;&c_s;</trd>,
    <trd>&c_a;&c_f;&c_ib;&c_c;&c_y;&c_j;&c_n;&c_y; &c_k;&c_u;&c_r;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>B&ouml;rsenkurs</trd>,
     <trd>b&ouml;rslicher Kurs</trd>,
     <trd>offizieller Kurs</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>offici&euml;le <ind>koers</ind></trd>,
    <trd>beurskoers</trd>
  </trdgrp>
  <trdgrp lng="hu">
   <trd>t&odblac;zsde&aacute;rfolyam</trd>,
   <trd>hivatalos <ind>&aacute;rfolyam</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kurs oficjalny</trd>,
    <trd>notowania oficjalne</trd>
  </trdgrp>
  <trd lng="ro">cursul oficial de schimb</trd>
  <trd lng="ru">&c_o;&c_f;&c_i;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_k;&c_u;&c_r;&c_s;</trd>
</drv>

<drv mrk="kurz.0altigxo">
  <kap>
    <tld/>alti&gcirc;o
  </kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Pliprezi&gcirc;o de <tld/>a valoro:
      <ekz>
        ricevinte la informon pri <tld/>alti&gcirc;o, <nom>Murakami</nom> senprokraste vendis tiujn
        akciojn kaj gajnis multege da mono
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="https://www.monato.be/2006/009053.php">Prezidanto de la japana centra banko profitis</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_o;&c_s;&c_t; &c_k;&c_u;&c_r;&c_s;&c_u;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_y;&c_sh;&c_e;&c_n;&c_mol;&c_n;&c_je; &c_k;&c_u;&c_r;&c_s;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kursanstieg</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;rfolyam-n&ouml;veked&eacute;s</trd>,
    <trd>&aacute;rfolyamnemelked&eacute;s</trd>
  </trdgrp>  
  <trdgrp lng="ja">
    <trd>&#x4E0A;&#x3052;&#x76F8;&#x5834;  <pr>&#x3042;&#x3052;&#x305D;&#x3046;&#x3070;</pr></trd>,
    <trd>&#x76F8;&#x5834;&#x306E;&#x4E0A;&#x6607;  <pr>&#x305D;&#x3046;&#x3070;&#x306E;&#x3058;&#x3087;&#x3046;&#x3057;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">koersstijging</trd>
  <trdgrp lng="pl">
    <trd>zwy&zdot;ka <ind>kursu</ind></trd>,
    <trd>zwy&zdot;ka <ind>notowa&nacute;</ind></trd>,
    <trd>hossa</trd>
  </trdgrp>
  <trd lng="ro">cre&#x219;tere</trd>
  <trd lng="ru">&c_p;&c_o;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_i;&c_je; &c_k;&c_u;&c_r;&c_s;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_v;&c_i;&c_shch;&c_je;&c_n;&c_n;&c_ja; &c_k;&c_u;&c_r;&c_s;&c_u;</trd>,
    <trd>&c_z;&c_r;&c_o;&c_s;&c_t;&c_a;&c_n;&c_n;&c_ja; &c_k;&c_u;&c_r;&c_s;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x98C4;&#x7D05; <pr>pi&#x101;oh&#xF3;ng</pr></trd>,
    <trd>&#x98D8;&#x7EA2; <pr>pi&#x101;oh&#xF3;ng</pr></trd>
  </trdgrp>
</drv>




