<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kurz</rad>/o
</kap>
<drv mrk="kurz.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Borsa a&ubreve; eksterborsa prezo a&ubreve; valoro de akcioj, obligacioj, kambioj,
      devizoj, mono kaj varoj:
      <ekz>
        alfa, meza, malalta <tld/>o;
      </ekz>
      <ekz>
        <tld/>o falas, alti&gcirc;as;
      </ekz>
      <ekz>
        konstanta, nekonstanta, varia,  &scirc;ancela <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_r;&c_s; <klr tip="amb">(&c_b;&c_ib;&c_r;&c_zh;&c_a;&c_v;&c_y;, &c_v;&c_a;&c_l;&c_ju;&c_t;&c_n;&c_y;)</klr></trd>
  <trd lng="de">Kurs</trd>
  <trdgrp lng="en">
    <trd>price</trd>,
    <trd>rate</trd>,
    <trd>quotation</trd>
  </trdgrp>
  <trd lng="hu">&aacute;rfolyam</trd>
  <trd lng="nl">koers<klr tip="ind"> (v.munt bijv.)</klr></trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_s; <klr>(&c_b;&c_i;&c_r;&c_zh;&c_je;&c_v;&c_o;&c_j;, &c_v;&c_a;&c_l;&c_ju;&c_t;&c_n;&c_y;&c_j;)</klr></trd>
</drv>

<drv mrk="kurz.eksterborsa0o">
  <kap>
    eksterborsa <tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <tld lit="K"/>o notata kaj uzata ekster la borso
      (= neoficiala <tld/>o).
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_a;&c_b;&c_ib;&c_r;&c_zh;&c_a;&c_v;&c_y; &c_k;&c_u;&c_r;&c_s;</trd>
  <trd lng="de">au&szlig;erb&ouml;rslicher Kurs</trd>
  <trdgrp lng="hu">
     <trd>t&odblac;zsd&eacute;n k&iacute;v&uuml;li <ind>&aacute;rfolyam</ind></trd>,
     <trd>nem hivatalos <ind>&aacute;rfolyam</ind></trd>
  </trdgrp>
</drv>

<drv mrk="kurz.oficiala0o">
  <kap>(oficiala) <tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      &Ccirc;iutage en la borso notata <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ib;&c_r;&c_zh;&c_a;&c_v;&c_y; &c_k;&c_u;&c_r;&c_s;</trd>,
    <trd>&c_a;&c_f;&c_ib;&c_c;&c_y;&c_j;&c_n;&c_y; &c_k;&c_u;&c_r;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>B&ouml;rsenkurs</trd>,
     <trd>b&ouml;rslicher Kurs</trd>,
     <trd>offizieller Kurs</trd>
  </trdgrp>
  <trdgrp lng="hu">
   <trd>t&odblac;zsde&aacute;rfolyam</trd>,
   <trd>hivatalos <ind>&aacute;rfolyam</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_f;&c_i;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_k;&c_u;&c_r;&c_s;</trd>
</drv>

<drv mrk="kurz.0hava">
  <kap>
    <tld/>(ohav)a
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Negocata en la borso.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>b&ouml;rslich <ind>notiert</ind></trd>
  </trdgrp>
  <trd lng="hu">t&odblac;zsd&eacute;n <ind>jegyzett</ind></trd>
</drv>

<drv mrk="kurz.0altigxo">
  <kap>
    <tld/>alti&gcirc;o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Pliprezi&gcirc;o de <tld/>a valoro.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_o;&c_s;&c_t; &c_k;&c_u;&c_r;&c_s;&c_u;</trd>,
    <trd>&c_p;&c_a;&c_v;&c_y;&c_sh;&c_e;&c_n;&c_mol;&c_n;&c_je; &c_k;&c_u;&c_r;&c_s;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kursanstieg</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;rfolyam-n&ouml;veked&eacute;s</trd>,
    <trd>&aacute;rfolyamnemelked&eacute;s</trd>
  </trdgrp>  
  <trd lng="nl">koersstijging</trd>
  <trd lng="ru">&c_p;&c_o;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_i;&c_je; &c_k;&c_u;&c_r;&c_s;&c_a;</trd>
</drv>

<drv mrk="kurz.0ofalo">
  <kap>
    <tld/>ofalo
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Malpliprezi&gcirc;o de <tld/>a valoro.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_n;&c_ib;&c_zh;&c_e;&c_n;&c_mol;&c_n;&c_je; &c_k;&c_u;&c_r;&c_s;&c_u;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_z;&c_je;&c_n;&c_mol;&c_n;&c_je; &c_k;&c_u;&c_r;&c_s;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kursr&uuml;ckgang</trd>,
     <trd>Kurseinbruch</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&aacute;rfolyames&eacute;s</trd>,
    <trd>&aacute;rfolyamcs&ouml;kken&eacute;s</trd>
  </trdgrp>  
  <trd lng="nl">koersverlaging</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_n;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_je; &c_k;&c_u;&c_r;&c_s;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_je;&c_n;&c_i;&c_je; &c_k;&c_u;&c_r;&c_s;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kurz.0noto">
  <kap>
    <tld/>noto
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      &Ccirc;iutaga fikso de valoro de <tld/>o en la borso.
    </dif>
    <refgrp tip="sin">
      <ref cel="kvot.0igo">kvotigo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_t;&c_y;&c_r;&c_o;&c_w;&c_k;&c_a; <klr>(&c_b;&c_ib;&c_r;&c_zh;&c_a;&c_v;&c_a;&c_ja;)</klr></trd>
  <trdgrp lng="de">
     <trd>B&ouml;rsenkursnotierung</trd>,
     <trd>Kursnotierung</trd>,
     <trd>Kursstellung</trd>
  </trdgrp>
  <trd lng="hu">&aacute;rfolyamjegyz&eacute;s</trd>  
  <trd lng="nl">beursnotering</trd>
</drv>

<drv mrk="kurz.sen0a">
  <kap>
    sen<tld/>a
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Nehavanta kurzon, ne negocata en la borso.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>nicht notiert</trd>,
     <trd>gestrichen</trd>
  </trdgrp>
  <trd lng="hu">t&odblac;zsd&eacute;n nem <ind>jegyzett</ind></trd>  
</drv>
</art>
