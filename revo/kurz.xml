<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kurz</rad>/o
</kap>
<drv mrk="kurz.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Borsa a&ubreve; eksterborsa prezo a&ubreve; valoro de akcioj, obligacioj, kambioj,
      devizoj, mono kaj varoj:
      <ekz>
        alfa, meza, malalta <tld/>o;
      </ekz>
      <ekz>
        <tld/>o falas, alti&gcirc;as;
      </ekz>
      <ekz>
        konstanta, nekonstanta, varia,  &scirc;ancela <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kurs</trd>
  <trdgrp lng="en">
    <trd>price</trd>,
    <trd>rate</trd>,
    <trd>quotation</trd>
  </trdgrp>
  <trd lng="nl">koers<klr tip="ind"> (v.munt bijv.)</klr></trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_s; <klr>(&c_b;&c_i;&c_r;&c_zh;&c_je;&c_v;&c_o;&c_j;, &c_v;&c_a;&c_l;&c_ju;&c_t;&c_n;&c_y;&c_j;)</klr></trd>
</drv>

<drv mrk="kurz.eksterborsa0o">
  <kap>
    eksterborsa <tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>o notata kaj uzata ekster la borso
      (= neoficiala <tld/>o).
    </dif>
  </snc>
</drv>

<drv mrk="kurz.oficiala0o">
  <kap>(oficiala) <tld/>o</kap>
  <snc>
    <dif>
      &Ccirc;iutage en la borso notata <tld/>o.
    </dif>
  </snc>
  <trd lng="ru">&c_o;&c_f;&c_i;&c_c;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_k;&c_u;&c_r;&c_s;</trd>
</drv>

<drv mrk="kurz.0hava">
  <kap>
    <tld/>(ohav)a
  </kap>
  <snc>
    <dif>
      Negocata en la borso.
    </dif>
  </snc>
</drv>

<drv mrk="kurz.0altigxo">
  <kap>
    <tld/>alti&gcirc;o
  </kap>
  <snc>
    <dif>
      Pliprezi&gcirc;o de <tld/>a valoro.
    </dif>
  </snc>
  <trd lng="nl">koersstijging</trd>
  <trd lng="ru">&c_p;&c_o;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_i;&c_je; &c_k;&c_u;&c_r;&c_s;&c_a;</trd>
</drv>

<drv mrk="kurz.0ofalo">
  <kap>
    <tld/>ofalo
  </kap>
  <snc>
    <dif>
      Malpliprezi&gcirc;o de <tld/>a valoro.
    </dif>
  </snc>
  <trd lng="nl">koersverlaging</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_n;&c_i;&c_zh;&c_je;&c_n;&c_i;&c_je; &c_k;&c_u;&c_r;&c_s;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_je;&c_n;&c_i;&c_je; &c_k;&c_u;&c_r;&c_s;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kurz.0noto">
  <kap>
    <tld/>noto
  </kap>
  <snc>
    <dif>
      &Ccirc;iutaga fikso de valoro de <tld/>o en la borso.
    </dif>
    <refgrp tip="sin">
      <ref cel="kvot.0igo">kvotigo</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">beursnotering</trd>
</drv>

<drv mrk="kurz.sen0a">
  <kap>
    sen<tld/>a
  </kap>
  <snc>
    <dif>
      Nehavanta kurzon, ne negocata en la borso.
    </dif>
  </snc>
</drv>
</art>
