<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>lir</rad>/o
</kap>
<drv mrk="lir1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>PV</bib><lok>samloke kiel la homonima muzikilo</lok></fnt>
  <snc>
    <dif>
      Iama <ref tip="super" cel="unu.mon0o">monunuo</ref> de
      <ref cel="ital.I0ujo">Italujo</ref>, nun
      anstata&ubreve;ita de <ref cel="euxr.0o">e&ubreve;ro</ref>:
      <ekz>
        la patra letero anoncis dungi&gcirc;on kaj &gcirc;uste 
        po&scirc;tmandaton je kvin <tld/>oj
        <fnt><aut>Armando Zecchin</aut>: <vrk>La kokino de nia
        najbarino</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 3a, p.
        27a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="hom">
    Ne konfuzu kun
    <ref cel="lir.0o">liro</ref>.
  </refgrp>
  <trd lng="be">&c_l;&c_ib;&c_r;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Lire</trd>,
    <trd>Lira</trd>
  </trdgrp>
  <trd lng="el">&lambda;&jota;&rho;&epsilon_ton;&tau;&tau;&alfa;</trd>
  <trd lng="en">lira</trd>
  <trd lng="es">lira</trd>
  <trd lng="fr">lire</trd>
  <trd lng="hu">l&iacute;ra</trd>  
  <trd lng="it">lira</trd>
  <trd lng="nl">lira</trd>
  <trd lng="pl">lir</trd>
  <trd lng="pt">lira</trd>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_a;</trd>
</drv>
</art>

