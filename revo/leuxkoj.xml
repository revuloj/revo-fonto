<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>le&ubreve;koj</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="leuxkoj.0o">
  <kap><tld/>o</kap>
  <snc mrk="leuxkoj.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Plurjara bulboherbo kreskanta en E&ubreve;ropo,
      Nordafriko kaj Okcidentazio, kun mallar&gcirc;aj folioj kaj pendaj,
      klo&scirc;aj, blankaj floroj, genro el familio <ref tip="super"
      cel="amaril1.0oj">amarilidacoj</ref> (<trd lng="la">Leucojum</trd>):
      <ekz>
        mi konsultis anka&ubreve; ilustritan gvidlibron pri &gcirc;ardenplantoj, 
kaj
        fine trovis &gcirc;in. &Gcirc;ia nomo estas <tld/>o [Leucojum aestivum] 
a&ubreve;
        ne&gcirc;ofloko
        <fnt>
        <aut>Vastalto</aut>,
        <vrk><url ref="http://www.vastalto.com/blog">Akvosfero</url></vrk>,
        <lok>2006-03-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="leuxkoj.0o.RIM_PARONIMO">
    Ne konfuzu kun <ref tip="vid" cel="levkoj.0o">levkojo</ref>.
  </rim>
  <trd lng="fr">niv&eacute;ole</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_l;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_a;&c_m;&c_a;&c_r;&c_i;&c_l;&c_l;&c_i;&c_s; 
&c_a;&c_l;&c_mol;&c_p;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

