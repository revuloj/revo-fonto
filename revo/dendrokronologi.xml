<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dendrokronologi.xml,v 1.6 2024/07/29 09:29:28 revo Exp $">
<kap>
  <rad>dendrokronologi</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="dendrokronologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="dendrokronologi.0o.KAL">
    <uzo tip="fak">ARKE</uzo>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Studo de la jaraj kreskoringoj en ligno,
      kiu provizas informojn por klimatohistorio
      kaj <ref tip="vid" cel="kronol.0o.SCI">kronologio</ref>:
      <ekz>
        <nom>Illig</nom> opinias, ke <tld/>o kaj la metodo C14
        estas &leftquot;malkorekte la&ubreve;normigitaj&rightquot;
        <fnt>
          <aut>M. Ebner, trad. I. Bengsch</aut>,
          <vrk><url ref="http://www.fantomzeit.de/?p=1527">
          Karolo la Falsigita</url></vrk>,
          <lok>2009-09-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_n;&c_d;&c_r;&c_a;&c_h;&c_r;&c_a;&c_n;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>chronologie letokruh&uring;</trd>,
    <trd>dendrochronologie</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Dendrochronologie</trd>
    <trd>Baumdatierung</trd>
  </trdgrp>
  <trd lng="fr">dendrochronologie</trd>
  <trd lng="sk">dendrochronol&oacute;gia</trd>
  <trd lng="uk">&c_d;&c_je;&#x43D;&c_d;&c_r;&#x43E;&c_h;&c_r;&#x43E;&#x43D;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&#x44F;</trd>
</drv>
</art>

