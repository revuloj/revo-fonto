<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pedik.xml,v 1.23 2022/03/25 12:53:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pedik</rad>/o
</kap>

<drv mrk="pedik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el la klaso <ref tip="super" cel="insekt.0oj">insektoj</ref>
      (<trd lng="la">Pediculus</trd>), senflugila parazita 
      <ref tip="lst" lst="voko:insektoj" cel="insekt.0o">insekto</ref>
      vivanta sur mamuloj, nutranta sin de sango:
      <ekz>
        venis fiinsektoj, <tld/>oj en &ccirc;iuj iliaj regionoj
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 105:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patrino kvietigis lin kaj ek&ccirc;asis la <tld/>ojn, &gcirc;is li ekdormis
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tri oraj haroj de la diablo</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi pereos, kiel <tld/>oj sub la ungo de pia Egiptano
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        por vi, pastro, nur tiu indas la kompaton, kiun man&gcirc;as la <tld/>oj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vesto<tld/>o <klr>(Pediculus humanus humanus)</klr>
        vivas precipe en vestoj kaj la kapo<tld/>o
        <klr>(Pediculus humanus capitis)</klr> en la haroj
        de homoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_sh;</trd>
  <trd lng="bg">&c_v;&c_malmol;&c_sh;&c_k;&c_a;</trd>
  <trd lng="bo">&#xF64;&#xF72;&#xF42;&#xF0B;</trd>
  <trd lng="br">laouenn</trd>
  <trd lng="ca">poll</trd>
  <trdgrp lng="cs">
    <trd>ve&scaron;</trd>,
    <trd>ve&scaron; d&ecaron;tsk&aacute;</trd>
  </trdgrp>
  <trd lng="de">Laus</trd>
  <trd lng="en">louse</trd>
  <trd lng="fa">&a_sh;&f_p;&a_sh;</trd>
  <trd lng="fr">pou</trd>
  <trd lng="hu">tet&udblac;</trd>
  <trd lng="nl">luis</trd>
  <trdgrp lng="pl">
    <trd>wesz</trd>,
    <trd>menda<klr> (wesz &lstroke;onowa)</klr></trd>
  </trdgrp>
  <trd lng="pt">piolho</trd>
  <trd lng="ru">&c_v;&c_o;&c_sh;&c_mol;</trd>
  <trd lng="sk">vo&scaron;</trd>
  
  <adm>
    Pediko apartenas al la ordo Phthirapteroj (~ senflugiluloj).
    Mi ne trovis jam uzatan Esperantan vorton por tiu ordo.
    <aut>Wolfram Diestel</aut>
  </adm>
  <trd lng="uk">&#x432;&#x43E;&#x448;&#x430;</trd>
</drv>

<drv mrk="pedik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Havanta <tld/>ojn:
      <ekz>
        ho, mia kara Dio, kiel &scirc;i estis <tld/>a! <tld/>oj gutis de
        sur &scirc;i
        <fnt>
          <aut>Koviljka Tisma Jankovi&cacute;</aut>,
          <vrk>La vojo tra la tombejo</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1999a, numero 4a, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif> 
    <trd lng="bg">&c_v;&c_malmol;&c_sh;&c_l;&c_ja;&c_s;&c_a;&c_l;</trd>
    <trd lng="br">laouek</trd>
    <trd lng="de">verlaust</trd>
    <trd lng="en">lousy</trd> 
    <trd lng="fa">&a_sh;&f_p;&a_sh;&a_w;</trd>
    <trd lng="fr">pouilleux</trd>
    <trd lng="pl">zawszony</trd>
  </snc>       
  <snc>
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        <tld/>aj ovoj;
      </ekz>                  
      <ekz>
        <ind><tld/>a malsano</ind>.
        <trd lng="fa">&f_p;&a_d;&f_y;&f_k;&a_w;&a_l;&a_w;&a_z;&f_y;&a_s;</trd>
        <trd lng="fr">phtiriase</trd>
      </ekz>
    </dif>    
    <trd lng="br">-al <ind>laou</ind></trd>    
    <trd lng="fr">p&eacute;diculaire<klr tip="amb"> (adj.)</klr></trd>
    <trd lng="pl">wszawy</trd>
  </snc> 
  <trd lng="be">&c_v;&c_a;&c_sh;&c_y;&c_v;&c_y;</trd>        
  <trd lng="de">verlaust</trd>
</drv>
                 
<drv mrk="pedik.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc>
    <dif>
      Senigi de <tld/>oj:
      <ekz>
        li metis sian kapon sur la genuojn de la patrino kaj petis, ke &scirc;i sen<tld/>igu lin
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tri oraj haroj de la diablo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i provis demeti la turbanon de Bastjano por sen<tld/>igi lin
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Urbo de Imperiestroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni ne povis sen<tld/>igi &scirc;in alie, sed ni kombis &scirc;in
        super la fajro
        <fnt>
          <aut>Koviljka Tisma Jankovi&cacute;</aut>,
          <vrk>La vojo tra la tombejo</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1999a, numero 4a, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_sh;&c_ch;&c_y;&c_c;&c_mol; <ind>&c_v;&c_o;&c_sh;&c_a;&c_j;</ind></trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_o;&c_shch;&c_ja;</trd>,
    <trd>&c_o;&c_p;&c_o;&c_shch;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="br">dilaoua&ntilde;</trd>
  <trdgrp lng="de">          
    <trd>entlausen</trd>,
    <trd>lausen</trd>
  </trdgrp>
  <trd lng="en">delouse</trd> 
  <trd lng="fa">&a_sh;&f_p;&a_sh;&zwnj;&a_z;&a_d;&a_A;&f_y;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">&eacute;pouiller</trd>
  <trd lng="pl">odwszawia&cacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x432;&#x43E;&#x434;&#x438;&#x442;&#x438; &#x432;&#x43E;&#x448;&#x456;</trd>,
    <trd>&#x437;&#x430;&#x441;&#x442;. &#x441;&#x44C;&#x43A;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="pedik.0ovo">
  <kap><tld/>ovo</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ovo de <tld/>o:
      <ekz>
        keroseno plene efikas anka&ubreve; kontra&ubreve; <tld/>oj kaj
        <tld/>ovoj
        <fnt><vrk>Skabio, pedikoj</vrk>,
          <lok>
          <url ref="http://www.naturkuracado.info/51088195f60b7be04/51088195f60ce9a30/51088195f70ba4595.html">naturkuracado.info</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_n;&c_ib;&c_d;&c_a;</trd>
  <trd lng="bg">&c_g;&c_n;&c_i;&c_d;&c_a;</trd>
  <trd lng="br">nezenn</trd>
  <trd lng="cs">hnida</trd>
  <trd lng="de">Nisse</trd>
  <trd lng="en">nit <klr tip="amb"> (of louse)</klr></trd>
  <trd lng="fa">&a_t;&a_kh;&a_m; <ind>&a_sh;&f_p;&a_sh;</ind></trd>
  <trd lng="fr">lente</trd>
  <trd lng="pl">gnida</trd>
  <trd lng="sk">hnida</trd>
</drv>

</art>
