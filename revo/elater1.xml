<art mrk="$Id: elater1.xml,v 1.14 2023/08/31 11:49:28 revo Exp $">
<kap>
<rad>elateri</rad>/o
</kap>
<drv mrk="elater1.0o">
<kap><tld/>o</kap>
  <snc mrk="elater1.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Unujara herbo, <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o.BIO">specio</ref>
      de ekbalio (<trd lng="la">Ecballium elaterium</trd>).  
    </dif>
    <bld lok="&WCU;/thumb/7/7e/Ecballium_elaterium.jpg/320px-Ecballium_elaterium.jpg" prm="CC-BY-SA-3.0"><tld/>o (Ecballium elaterium), Planto&gcirc;ardeno de Parizo
        <fnt>
          <aut>Bouba, foto el franca Vikipedio</aut>
          <lok><url ref="&WCW;/File:Ecballium_elaterium.jpg">&WCW;/File:Ecballium_elaterium.jpg</url></lok>
        </fnt>
    </bld>
    <adm>
      Ekbalio (Ecballium) mankas en REVO. <aut>MB</aut>
    </adm>
    <trd lng="de">Elaterium</trd>
    <trdgrp lng="hu">
      <trd>ubork&aacute;s magrug&oacute;</trd>,
      <trd>l&ouml;v&odblac;uborka</trd>
    </trdgrp>
    <trd lng="la">Ecballium elaterium</trd>
    <trd lng="pt">elat&eacute;rio</trd>
  </snc>
  <snc mrk="elater1.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Sekigita fruktosuko de <tld/>o<sncref ref="elater1.0o.BOT"/>, uzita kiel laksiga medikamento; koroda likva&jcirc;o de ekbalioj:
      <ekz>
        veneni&gcirc;o per <tld/>o
        <fnt>
          <aut>Mihail Bulgakov</aut>
          <vrk><url ref="https://www.litmir.me/br/?b=87746&amp;p=8">Fatalaj ovoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <adm>
      Ekbalio (Ecballium elaterium) mankas en Revo. <aut>MB</aut>
    </adm>
    <trd lng="de">Elaterium-Abf&uuml;hrmittel</trd>
    <trd lng="hu">elat&eacute;rium</trd>  
    <trd lng="pt">elat&eacute;rio</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>tykvice</trd>,
    <trd>tykvice st&rcaron;&iacute;kav&aacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C6;&#x30C3;&#x30DD;&#x30A6;&#x30A6;&#x30EA;</trd>,
    <trd>&#x30A8;&#x30E9;&#x30C6;&#x30EA;&#x30A6;&#x30E0;</trd>
  </trdgrp>
  <trd lng="sk">su&scaron;en&aacute; &scaron;&tcaron;ava tekvice</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x433;&#x456;&#x440;&#x43E;&#x43A;-&#x43F;&#x438;&#x440;&#x441;&#x43A;&#x430;&#x447; &#x43F;&#x440;&#x443;&#x436;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x43A;&#x430;&#x436;&#x435;&#x43D;&#x438;&#x439; &#x43E;&#x433;&#x456;&#x440;&#x43E;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
