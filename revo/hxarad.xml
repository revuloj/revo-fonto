<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&hcirc;aradri</rad>/o
</kap>
<drv mrk="hxarad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super"
      cel="hxarad1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Charadrius</trd>).
    </dif>
  </snc>
  <trd lng="cs">kulik</trd>
  <trd lng="de">Regenpfeiffer</trd>
  <trd lng="en">plover</trd>
  <trd lng="hu">lile</trd>
  <trd lng="nl">plevier</trd>
  <trd lng="pl">siewka</trd>
  <trd lng="ru">&c_z;&c_u;&c_je;&c_k;</trd>
</drv>

<drv mrk="hxarad.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Charadriiformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Watv&ouml;gel</trd>
  <trd lng="fr">charadriiformes</trd>
  <trd lng="nl">pleviervogels</trd>
  <trd lng="ru">&c_r;&c_zh;&c_a;&c_n;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
</drv>
</art>
