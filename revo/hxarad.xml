<vortaro>
<art mrk="$Id$">
<kap>
   <rad>&hcirc;aradri</rad>/o
</kap>
<drv mrk="hxarad.0o">
   <kap><tld/>o</kap>
   <snc mrk="hxarad.0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
       Genro el la familio 
       <ref tip="super" cel="hxarad1.0oj"><tld/>edoj</ref>
       (<trd lng="la">Charadrius</trd>).
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_s;&c_ib;&c_v;&c_je;&c_c;</trd>,
     <trd>&c_s;&c_ja;&c_v;&c_je;&c_c;</trd>,
     <trd>&c_g;&c_a;&c_l;&c_mol;&c_sh;&c_t;&c_u;&c_ch;&c_n;&c_ib;&c_k; 
<klr tip="amb">(Charadrius hiaticula L.)</klr></trd>
   </trdgrp>
   <trd lng="ca">corriol</trd>
   <trd lng="cs">kulik</trd>
   <trd lng="de">Regenpfeiffer</trd>
   <trd lng="en">plover</trd>
   <trd lng="fi">tylli</trd>
   <trdgrp lng="fr">
     <trd>gravelot</trd>,
     <trd>pluvier</trd>
   </trdgrp>
   <trd lng="hu">lile</trd>
   <trd lng="nl">plevier</trd>
   <trd lng="pl">siewka</trd>
   <trd lng="pt">car&#225;drio</trd>
   <trdgrp lng="ru">
     <trd>&c_z;&c_u;&c_jo;&c_k;</trd>,
     <trd>&c_r;&c_zh;&c_a;&c_n;&c_k;&c_a;</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>charadrius<klr> (genere)</klr></trd>,
     <trd>corriere<klr> (tipo di volatile)</klr></trd>,
     <trd>piviere</trd>
   </trdgrp>
</drv>

<drv mrk="hxarad.0oformaj">
   <kap><tld/>oformaj</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
       (<trd lng="la">Charadriiformes</trd>).
     </dif>
   </snc>
   <trd lng="be">
&c_s;&c_je;&c_w;&c_c;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; 
<klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
   <trd lng="ca">caradriformes</trd>
   <trd lng="de">Watv&ouml;gel</trd>
   <trd lng="fi">rantalinnut</trd>
   <trd lng="fr">charadriiformes</trd>
   <trd lng="hu">lileszer&udblac; madarak</trd>
   <trd lng="nl">pleviervogels</trd>
   <trd lng="ru">
&c_r;&c_zh;&c_a;&c_n;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je; 
<klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
    <trd lng="it">caradriiformi</trd>
</drv>
</art>
