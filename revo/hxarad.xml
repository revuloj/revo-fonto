<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hxarad.xml,v 1.21 2023/11/15 17:08:02 revo Exp $">
<kap>
   <rad>&hcirc;aradri</rad>/o <fnt><bib>PIV1</bib></fnt>,
   <var><kap><rad var="k">karadri</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="hxarad.0o">
   <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
   <snc mrk="hxarad.0o.ZOO">
     <uzo tip="fak">ZOO</uzo>
     <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el la familio
      <ref tip="malprt" cel="hxarad1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Charadrius</trd>):
      <ekz>
        <tld/>oj-&ccirc;ase sur selo pafilon ka&scirc;as
        <fnt>
          <bib>SrT</bib>
          <lok>Libro III.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj <klr>[&#x2026;]</klr> havas unu a&ubreve; pliajn malhelajn a&ubreve; nigrajn striojn &ccirc;e la
        brusto, <klr>[&#x2026;]</klr> anka&ubreve; rimarkindajn blankajn kaj nigrajn markojn &ccirc;e la
        viza&gcirc;o, ili lo&gcirc;as en diversaj medioj &ccirc;u internaj lagoj, &ccirc;u koralaj a&ubreve; sablaj
        strandoj, rokaj areoj, kotaj ebena&jcirc;oj, tajdaj regionoj, mar&ccirc;oj kaj aliaj
        humidejoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=137668">&Hcirc;aradrio</url></lok>
        </fnt>.
      </ekz>
     </dif>
    <bld lok="&WCU;/thumb/9/92/Killdeer_(Charadrius_vociferus_ternominatus)_chest.JPG/640px-Killdeer_(Charadrius_vociferus_ternominatus)_chest.JPG" prm="CC BY-SA 4.0"><tld/>o (Charadrius vociferus ternominatus), Kubo
        <fnt>
          <aut>Charles J Sharp, propra verko</aut>
          <lok><url ref="&WCW;/File:Killdeer_(Charadrius_vociferus_ternominatus)_chest.JPG">&WCW;/File:Killdeer_(Charadrius_vociferus_ternominatus)_chest.JPG</url></lok>
        </fnt>
    </bld>
   </snc>
   <trdgrp lng="be">
     <trd>&c_s;&c_ib;&c_v;&c_je;&c_c; <klr tip="amb">(&c_p;&c_t;&c_u;&c_sh;&c_k;&c_a;)</klr></trd>,
     <trd>&c_s;&c_ja;&c_v;&c_je;&c_c;</trd>,
     <trd>&c_g;&c_a;&c_l;&c_mol;&c_sh;&c_t;&c_u;&c_ch;&c_n;&c_ib;&c_k;
     <klr tip="amb">(Charadrius hiaticula L.)</klr></trd>
   </trdgrp>
   <trd lng="ca">corriol</trd>
   <trd lng="cs">kulik</trd>
   <trd lng="de">Regenpfeiffer</trd>
   <trd lng="en">plover</trd>
   <trd lng="fi">tylli</trd>
   <trdgrp lng="fr">
     <trd>gravelot</trd>,
     <trd>pluvier</trd>
   </trdgrp>
   <trd lng="hu">lile</trd>
   <trd lng="nl">plevier</trd>
   <trd lng="pl">siewka</trd>
   <trd lng="pt">car&aacute;drio</trd>
   <trdgrp lng="ru">
     <trd>&c_z;&c_u;&c_jo;&c_k;</trd>,
     <trd>&c_r;&c_zh;&c_a;&c_n;&c_k;&c_a;</trd>
   </trdgrp>
   <trdgrp lng="it">
     <trd>charadrius<klr> (genere)</klr></trd>,
     <trd>corriere<klr> (tipo di volatile)</klr></trd>,
     <trd>piviere</trd>
   </trdgrp>
  <trd lng="zh">&#x9D34; <pr>h&#xE9;ng</pr></trd>
</drv>

<drv mrk="hxarad.0oformaj">
   <kap><tld/>oformaj, <var><kap><tld var="k"/>formaj</kap></var></kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref tip="malprt" cel="bird.0oj">birdoj</ref>
       (<trd lng="la">Charadriiformes</trd>).
     </dif>
   </snc>
   <trd lng="be">
&c_s;&c_je;&c_w;&c_c;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja;
<klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
   <trd lng="ca">caradriformes</trd>
   <trd lng="de">Watv&ouml;gel</trd>
   <trd lng="fi">rantalinnut</trd>
   <trd lng="fr">charadriiformes</trd>
   <trd lng="hu">lileszer&udblac; madarak</trd>
   <trd lng="nl">pleviervogels</trd>
   <trd lng="ru">
&c_r;&c_zh;&c_a;&c_n;&c_k;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_n;&c_y;&c_je;
<klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
    <trd lng="it">caradriiformi</trd>
</drv>

</art>
