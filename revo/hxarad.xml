<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>&hcirc;aradri</rad>/o
</kap>
<drv mrk="hxarad.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="hxarad.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Charadrius</trd>).
    </dif>
  </snc>
  <trd lng="cs">kulik</trd>
  <trd lng="de">Regenpfeiffer</trd>
  <trd lng="en">plover</trd>
  <trd lng="hu">lile</trd>
  <trd lng="pl">siewka</trd>
  <trd lng="ru">&c_z;&c_u;&c_je;&c_k;</trd>
</drv>
<drv mrk="hxarad.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="hxarad.0oformaj"><tld/>oformaj
      birdoj</ref> (<trd lng="la">Charadriidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Regenpfeiffer <klr tip="ind">(Familie)</klr></trd>
  <trd lng="en">pluviers</trd>
</drv>
<drv mrk="hxarad.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Charadriiformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Watv&ouml;gel</trd>
</drv>
</art>

