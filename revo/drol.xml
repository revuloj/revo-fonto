<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: drol.xml,v 1.7 2023/04/30 06:30:43 afido Exp $">
    <kap>
      <rad>drol</rad>/a
    </kap>

<drv mrk="drol.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="drol.0a.strangeta">
    <dif>
      Strange <ref tip="sin" cel="komik.0a">komika</ref>;
      amuze <ref tip="sin" cel="kurioz.0a">kurioza</ref>:
      <ekz>
        por meznivela leganto restas nur gvate gapi la <tld/>an miksa&jcirc;on de raraj vortoj
        kaj lontana senco
        <fnt>
          <aut>Grigori Arosev</aut>
            <vrk><url ref="&Monato;/2000/007198.php">Poezio sed ne poemoj</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 28</lok>
          </fnt>;
      </ekz>
      <ekz>
        post granda nombro da komikaj noveletoj, <nom>&Ccirc;e&hcirc;ov</nom> komencis verki pli longajn kaj
        pli mal<tld/>ajn rakontojn, en kiu la komika elemento estis &gcirc;enerale nur
        aludata, anstata&ubreve; esti evidenta
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2006/008771.php">Inda kontribuo el la rusa literaturo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        vin atendas <tld/>aj partioj, kiujn povus partopreni anka&ubreve; viaj gebubetoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011351.php">Trufludo</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_b;&c_a;&c_w;&c_n;&c_y;</trd>,
      <trd>&c_s;&c_mol;&c_m;&c_je;&c_sh;&c_n;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_c;&c_je;&c_sh;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>drollig</trd>,
      <trd>possierlich</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>funny</trd>,
      <trd>droll</trd>
    </trdgrp>
    <trd lng="fr">dr&ocirc;le</trd>
  </snc>
  <trd lng="he">&#x5DE;&#x5E9;&#x5D5;&#x5E0;&#x5D4; &#x5D5;&#x5DE;&#x5E9;&#x5E2;&#x5E9;&#x5E2;</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_a;&#x43B;&#x43E;&c_v;&c_zh;&c_i;&c_v;. &c_z;&c_a;&c_b;&c_a;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&#x43C;&c_ib;&c_sh;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

  </art>

