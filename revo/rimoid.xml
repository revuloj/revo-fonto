<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rimoid</rad>/o
</kap>
<drv mrk="rimoid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <fnt><bib>LSF</bib>, <lok></lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="rim.nepura0o">Nepura rimo</ref>, en kiu
      la tuj postakcentaj konsonantoj iom malsimilas:
      <ekz>
        &leftquot;ruzo-ruso&rightquot;, &leftquot;altrangaj-blankaj&rightquot; <klr>(kun parencaj
        konsonantoj)</klr>,
        &leftquot;neniam-sub li jam&rightquot; <klr>(kun aldona konsonanto)</klr>
        konsistigas <tld/>ojn.
      </ekz>
    </dif>
    <ref tip="vid" cel="asonan.0o.rimo">asonanco <sncref/></ref>
  </snc>
  <trd lng="hu">mag&aacute;nhangz&oacute;s <ind>asszon&aacute;nc</ind></trd>  
  <trd lng="nl">halfrijm</trd>
  <trd lng="pt">rima toante</trd>
</drv>
</art>
