<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rimoid</rad>/o
</kap>
<drv mrk="rimoid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <fnt><aut>&K;</aut><vrk>Lingvo, Stilo, Formo; Budapest, 1931</vrk><lok>p.
  93</lok></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="rim.nepura0o">Nepura rimo</ref>, en kiu
      la postakcentaj konsonantoj iom malsimilas:
      <ekz>
        &leftquot;ruzo-ruso&rightquot;, &leftquot;altrangaj-blankaj&rightquot; <klr>(kun parencaj
        konsonantoj)</klr>,
        &leftquot;neniam-sub li jam&rightquot; <klr>(kun aldona konsonanto)</klr>
        konsistigas <tld/>ojn.
      </ekz>
      <ref tip="sin" cel="asonan.0o.rimo">asonanco 2</ref>
    </dif>
  </snc>
  <trd lng="nl">halfrijm</trd>
  <trd lng="ru">&c_a;&c_s;&c_s;&c_o;&c_n;&c_a;&c_n;&c_s; <klr tip="amb">(&c_v;&c_i;&c_d; &c_n;&c_je;&c_t;&c_o;&c_ch;&c_n;&c_o;&c_j; &c_r;&c_i;&c_f;&c_m;&c_y;)</klr></trd>
</drv>
</art>
