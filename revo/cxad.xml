<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&Ccirc;ad</rad>/o
</kap>

<drv mrk="cxad.0o">
  <kap>
    <tld lit="&Ccirc;"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="cxad.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la centra <ref tip="malprt" cel="afrik.A0o">Afriko</ref> 
      kun &ccirc;efurbo N&gcirc;ameno.
      Teritorio de Respubliko &Ccirc;ado.
      ISO-3166 kodo: TD.
      &Gcirc;i limas kun
      <ref tip="vid" cel="afrik.centr0o.GEOG">Centr-Afriko</ref>,
      <ref tip="vid" cel="kamerun.0o.GEOG">Kameruno</ref>,
      <ref tip="vid" cel="libi.0o.GEOG">Libio</ref>,
      <ref tip="vid" cel="nigxer.0o.GEOG">Ni&gcirc;ero</ref>,
      <ref tip="vid" cel="nigxeri.0o.GEOG">Ni&gcirc;erio</ref>
      kaj <ref tip="vid" cel="sudan.0o.GEOG">Sudano</ref>.
    </dif>
    <trd lng="bg">&c_Ch;&c_a;&c_d;</trd>
    <trd lng="de">Tschad</trd>
    <trd lng="en">Chad</trd>
    <trd lng="hu">Cs&aacute;d</trd>
    <trd lng="it">Ciad</trd>
    <trd lng="nl">Tsjaad</trd>
    <trdgrp lng="pl">
      <trd>Czad</trd>,
      <trd>Republika <ind>Czadu</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

