<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gastropod</rad>/oj
</kap>
<drv mrk="gastro2.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="molusk.0oj">moluskoj</ref>
      (<trd lng="la">Gastropoda</trd>).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schnecken</trd>,
    <trd>Bauchf&uuml;&szlig;er</trd>
  </trdgrp>
  <trd lng="es">gaster&oacute;podos</trd>
  <trd lng="fr">gast&eacute;ropodes</trd>
  <trdgrp lng="nl">
    <trd>slakken</trd>,
    <trd>buikpotigen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_s;&c_t;&c_r;&c_o;&c_p;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_h;&c_o;&c_n;&c_o;&c_g;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
