<art mrk="$Id: gastro2.xml,v 1.22 2023/10/15 06:07:00 revo Exp $">
<kap>
  <rad>gastropod</rad>/oj
</kap>
<drv mrk="gastro2.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="klas.0o.BIO" lst="voko:zoologiaj_klasoj">Klaso</ref> de <ref tip="malprt" cel="molusk.0oj">moluskoj</ref> (<trd lng="la">Gastropoda</trd>):
      <ekz>
        estas 611 familioj de <tld/>oj, el kiuj 202 estas formortintaj, kaj nur
        troveblaj en fosilioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=234381">Gastropodoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_s;&c_t;&c_r;&c_a;&c_p;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_b;&c_r;&c_u;&c_h;&c_a;&c_n;&c_o;&c_g;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">gastr&ograve;podes</trd>
  <trdgrp lng="cs">
    <trd>b&rcaron;ichono&zcaron;ci</trd>,
    <trd>pl&zcaron;i</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schnecken</trd>,
    <trd>Bauchf&uuml;&szlig;er</trd>
  </trdgrp>
  <trd lng="el">&gamma;&alfa;&sigma;&tau;&epsilon;&rho;&omikron_ton;&pi;&omikron;&delta;&alfa;</trd>
  <trd lng="es">gaster&oacute;podos</trd>
  <trd lng="fr">gast&eacute;ropodes</trd>
  <trdgrp lng="hu">
    <trd>csig&aacute;k<klr tip="ind"> (oszt&aacute;ly)</klr></trd>,
    <trd>hasl&aacute;b&uacute;ak</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8179;&#x8DB3;&#x7DB1;  <pr>&#x3075;&#x304F;&#x305D;&#x304F;&#x3053;&#x3046;</pr></trd>,
    <trd>&#x8179;&#x8DB3;&#x985E;  <pr>&#x3075;&#x304F;&#x305D;&#x304F;&#x308B;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slakken</trd>,
    <trd>buikpotigen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;limaki</trd>,
    <trd>brzuchonogi</trd>
  </trdgrp>
  <trd lng="pt">gastr&oacute;podes</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_s;&c_t;&c_r;&c_o;&c_p;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_h;&c_o;&c_n;&c_o;&c_g;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">m&auml;kk&yacute;&scaron;e <klr>(slim&aacute;kovit&eacute;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x430;&#x441;&#x442;&#x440;&#x43E;&#x43F;&#x43E;&#x434;&#x438;</trd>,
    <trd>&#x447;&#x435;&#x440;&#x435;&#x432;&#x43E;&#x43D;&#x43E;&#x433;&#x456; &#x43C;&#x43E;&#x43B;&#x44E;&#x441;&#x43A;&#x438;</trd>
  </trdgrp>
</drv>
</art>
