    <trd>hasl&aacute;b&uacute;ak</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slakken</trd>,
    <trd>buikpotigen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&sacute;limaki</trd>,
    <trd>brzuchonogi</trd>
  </trdgrp>
  <trd lng="pt">gastr&oacute;podes</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_s;&c_t;&c_r;&c_o;&c_p;&c_o;&c_d;&c_y;</trd>,
    <trd>&c_b;&c_r;&c_ju;&c_h;&c_o;&c_n;&c_o;&c_g;&c_i;&c_je;</trd>
