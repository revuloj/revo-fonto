<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>klerik</rad>/o
      <fnt>Z</fnt>
    </kap>
    <drv mrk="klerik.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
        <dif>
          Eklezia funkciulo, ekleziulo.  
        </dif>
        <ref tip="vid" cel="laik.0o">laiko</ref>
        <trdgrp lng="en">
          <trd>clergyman</trd>,
          <trd>ecclesiastic</trd>,
          <trd>divine</trd>
        </trdgrp>
        <trd lng="grc">&kappa;&lambda;&eta;&rho;&jota;&kappa;&omikron_acute;&sigma_fina;</trd>
        <trd lng="ru">&c_k;&c_l;&c_i;&c_r;&c_i;&c_k;</trd>
      </snc>
    </drv>
    <drv mrk="klerik.0aro">
      <kap><tld/>aro</kap>
      <uzo tip="fak">REL</uzo>
      <snc>
        <dif>
          La korporacio de <tld/>oj ene de eklezio.
        </dif>
        <trd lng="grc">&kappa;&lambda;&eta;&rho;&jota;&kappa;&omikron_acute;&sigma_fina;</trd>
        <trd lng="ru">&c_k;&c_l;&c_i;&c_r;</trd>
      </snc>
    </drv>
  </art>
  <!--
  $Log$
  Revision 1.4  2001/02/15 08:14:32  revo
  Karlo Freeman: +trdgrp=en

  -->
</vortaro>
