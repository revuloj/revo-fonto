<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: silikon.xml,v 1.7 2024/03/24 20:21:57 revo Exp $">
<kap>
    <rad>silikon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="silikon.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Gcirc;enerala nomo por diversaj organikaj <ref tip="dif" cel="silici.0o">silici</ref>-kombina&jcirc;oj:
      <ekz>
      <tld/>oj estas kemie tre rezistaj kaj tial trovas multajn tre
      diversajn aplikojn, i.a. kiel tega&jcirc;on por &scirc;irmi kontra&ubreve; korodo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_l;&c_ib;&c_k;&c_o;&c_n;</trd>
  <trd lng="bg">&c_s;&c_i;&c_l;&c_i;&c_k;&c_o;&c_n;</trd>
  <trd lng="da">silikon</trd>
  <trd lng="de">Silikon </trd>
  <trd lng="en">silicone</trd>
  <trd lng="es">silicona</trd>
  <trd lng="fr">silicone</trd>
  <trd lng="nl">silicoon</trd>
  <trd lng="pl">silikon</trd>
  <trd lng="ru">&c_s;&c_i;&c_l;&c_i;&c_k;&c_o;&c_n;&c_y;</trd>
  <trd lng="tr">silikon</trd>
  <trd lng="uk">&c_s;&c_i;&#x43B;&c_ib;&#x43A;&#x43E;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x7845;&#x80F6; <pr>gu&#x12B;ji&#x101;o</pr></trd>,
    <trd>&#x7845;&#x81A0; <pr>gu&#x12B;ji&#x101;o</pr></trd>,
    <trd>&#x7845;&#x916E; <pr>gu&#x12B;t&#xF3;ng</pr></trd>
  </trdgrp>
</drv>
</art>

