  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x445;&#x43E;&#x434;&#x43E;&#x437;&#x43D;&#x430;&#x432;&#x435;&#x446;&#x44C;</trd>,
    <trd>&#x43E;&#x440;&#x456;&#x454;&#x43D;&#x442;&#x430;&#x43B;&#x456;&#x441;&#x442;</trd>
