<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pare.xml,v 1.11 2023/09/03 12:45:04 revo Exp $">
<kap>
  <rad>pare</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pare.0o">
  <kap><tld/>o</kap>
  <snc mrk="pare.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Pacifikdevena <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="vest.0o.TEKS">vesto</ref>,
      vasta tuko &gcirc;enerale kolore ornamita, kiun oni 
      volvas diversmaniere &ccirc;irka&ubreve; la talio a&ubreve; la
      torso:
      <ekz>
        en Tahitio <klr>...</klr> nuntempe kaj virinoj
        kaj viroj portas <tld/>ojn
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Pareo">
          pareo</url>, 2012-07-01</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_e;&c_a;</trd>
  <trd lng="bg">&c_p;&c_a;&c_r;&c_je;&c_o;</trd>
  <trdgrp lng="cs">
    <trd>kopie</trd>,
    <trd>opis</trd>,
    <trd>pare</trd>,
    <trd>pr&uring;pis</trd>,
    <trd>stejnopis</trd>
  </trdgrp>
  <trd lng="de">Pareo</trd>
  <trd lng="en">pareo</trd>
  <trd lng="fr">par&eacute;o</trd>
  <trd lng="ja">&#x30D1;&#x30EC;&#x30AA;</trd>
  <trd lng="nl">pareo</trd>
  <trd lng="pl">pareo</trd>
  <trd lng="ro">perechea</trd>
  <trdgrp lng="sk">
    <trd>k&oacute;pia</trd>,
    <trd>opis</trd>,
    <trd>priepis</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_a;&c_r;&c_je; (&c_t;&c_a;&c_ji;&c_t;&#x44F;&#x43D;&c_s;&#x44C;&#x43A;&c_i;&c_j; &#x43E;&c_d;&#x44F;&c_g;)</trd>
</drv>
</art>

