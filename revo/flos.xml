<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>flos</rad>/o
</kap>

<drv mrk="flos.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Kuna&jcirc;o, aro da arbaj trunkoj &jcirc;etitaj en riveron,
      por ke la fluo ilin transportu.
    </dif>
  </snc>
  <snc>
    <dif>
      Similspeca trabaro, konstruita por transporti homojn
      a&ubreve; diversajn materiojn.
    </dif>
  <trd lng="pt">jangada</trd>
  </snc>
  <trd lng="de">Flo&szlig;</trd>
  <trdgrp lng="en">
    <trd>raft</trd>,
    <trd>(log) boom</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>balsa</trd>,
    <trd>armad&iacute;a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>radeau</trd>,
    <trd>train de bois</trd>
  </trdgrp>
  <trd lng="nl">vlot<klr tip="ind"> (z.nw.)</klr></trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_t;</trd>
</drv>

<drv mrk="flos.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Surakvi&gcirc;i, resti sur akvo, ku&scirc;i sur flua&jcirc;o ne
      enprofundi&gcirc;ante, na&gcirc;i pro malpezeco en maniero de <tld/>o,
      maldroni:
      <ekz>
        la barelo <tld/>as;
      </ekz>
      <ekz>
        <tld/>anta bastono, &scirc;ipruina&jcirc;o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        super la ebena&jcirc;oj <tld/>is <klr>(&scirc;vebis)</klr> opalkoloraj
        nuboj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="nagx.0i">na&gcirc;i</ref>,
      <ref cel="sxveb.0i">&scirc;vebi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">schwimmen</trd>
  <trd lng="en">float<klr tip="ind"> (on water)</klr></trd>
  <trd lng="es">flotar</trd>
  <trd lng="fr">flotter<klr tip="ind"> (intr., sur l'eau)</klr></trd>
  <trdgrp lng="nl">
    <trd>vlotten</trd>,
    <trd>bovendrijven</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_r;&c_zh;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_n;&c_a; &c_p;&c_l;&c_a;&c_v;&c_u;</trd>
</drv>

<drv mrk="flos.0igi">
  <kap><tld/>igi<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Fari ke io <tld/>u, meti en <tld/>an staton.
    </dif>
  </snc>
  <snc>
    <dif>
      <tld lit="F"/>e transporti:
      <ekz>
        <tld/>igi lignon;
      </ekz>
      <ekz>
        <tld/>igitaj arboj.
      </ekz>
    </dif>
    <rim>
      Z uzis kelkfoje la formon <em>flosi</em> kiel transitivan anstata&ubreve;
      <em>flosigi</em>.
    </rim>
    <trd lng="de">fl&ouml;&szlig;en</trd>
    <trd lng="ru">&c_s;&c_p;&c_l;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol; <klr>(&c_p;&c_o; &c_t;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_ju;)</klr></trd>
  </snc>
  <trd lng="en">float<klr tip="ind"> (tr.)</klr></trd>
  <trd lng="fr">flotter<klr tip="ind"> (tr.)</klr></trd>
  <trd lng="nl">doen <ind>vlotten</ind></trd>
</drv>

<drv mrk="flos.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Per<tld/>a transportado.
    </dif>
    <trd lng="de">Fl&ouml;&szlig;en</trd>
    <trd lng="fr">flottage</trd>
    <trd lng="ru">&c_s;&c_p;&c_l;&c_a;&c_v;<klr tip="amb"> (&c_n;&c_a;&c_p;&c_r;. &c_l;&c_je;&c_s;&c_a; &c_p;&c_o; &c_v;&c_o;&c_d;&c_je;)</klr></trd>
  </snc>
  <snc>
    <dif>
      Stato de tio, kio <tld/>as.
    </dif>
    <trd lng="de">Treiben</trd>
    <trd lng="en">floating</trd>
    <trd lng="fr">flottement<klr tip="ind"> (fait de
    flotter)</klr></trd>
  </snc>
  <trd lng="nl">vlotten<klr tip="ind"> (z.nw.)</klr></trd>
</drv>

<drv mrk="flos.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu kondukas <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Fl&ouml;&szlig;er</trd>
  <trd lng="fr">flotteur<klr tip="ind"> (ouvrier qui flotte le
  bois)</klr></trd>
  <trdgrp lng="nl">
    <trd>vlotter</trd>,
    <trd>houtvlotter</trd>,
    <trd>vlotvoerder</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_l;&c_a;&c_v;&c_shch;&c_i;&c_k;</trd>
</drv>

<drv mrk="flos.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Objekto a&ubreve; ilo, kiu <tld/>as, servanta por montri nivelon
      a&ubreve; rapidecon de fluo, a&ubreve; por subteni ion en akvo:
      <ekz>
        kork<tld/>ilo de hokfadeno por fi&scirc;kapti.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Schwimmer</trd>
  <trdgrp lng="en">
    <trd>float</trd>,
    <trd>bob</trd>,
  </trdgrp>
  <trd lng="fr">flotteur<klr tip="ind"> (objet)</klr></trd>
  <trdgrp lng="nl">
    <trd>dobber</trd>,
    <trd>vlotter</trd>,
    <trd>drijver</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_p;&c_l;&c_a;&c_v;&c_o;&c_k;</trd>
</drv>

<drv mrk="flos.0linio">
  <kap><tld/>linio</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Nivelo, &gcirc;is kiu enakvi&gcirc;as &scirc;ipo.
    </dif>
  </snc>
  <trd lng="de">Wasserlinie<klr tip="amb"> (Schiff)</klr></trd>
  <trd lng="en">waterline<klr tip="amb"> (ship)</klr></trd>
  <trd lng="fr">ligne de flottaison</trd>
  <trd lng="ru">&c_v;&c_a;&c_t;&c_je;&c_r;&c_l;&c_i;&c_n;&c_i;&c_ja;</trd>
</drv>
</art>
