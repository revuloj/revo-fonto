<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: poligo1.xml,v 1.13 2023/09/01 13:28:44 revo Exp $">
<kap>
  <rad>poligonac</rad>/oj
</kap>
<drv mrk="poligo1.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="poligo1.0oj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt"
      cel="kotile.du0aj">dukotiledonaj</ref>
      vegeta&jcirc;oj (<trd lng="la">Polygonaceae</trd>), al kiu apartenas ekz.
      <refgrp tip="prt">
        <ref cel="fagopi.0o">fagopiro</ref>,
        <ref cel="rumeks.0o">rumekso</ref>,
        <ref cel="rabarb.0o">rabarbo</ref>,
        <ref cel="poligo.0o.genro">poligono<sncref/></ref>
      </refgrp>:
      <ekz>
        la rabarbo a&ubreve; reumo estas genro el familio de <tld/>oj, kun tre ampleksaj folioj,
        kies trunkoj estas uzataj por fari kompoton a&ubreve; konfita&jcirc;on kaj kies
        pulvorigitaj radikoj servas kiel laksigilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30210">Rabarbo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kokolobo estas genro de tropikamerikaj arboj el la planta familio <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30211">Kokolobo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_s;&c_jo;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
  <trdgrp lng="en">
    <trd>Polygonaceae</trd>,
    <trd>buckwheat family</trd>, <trd>smartweed family</trd>,
    <trd>knotweed family</trd>
  </trdgrp>
  <trd lng="ca">poligon&agrave;cies</trd>
  <trd lng="de">Kn&ouml;terichgew&auml;chse</trd>
  <trd lng="fa">&a_h;&a_f;&a_t;&#8204;&a_b;&a_n;&a_d;&f_y;&a_A;&a_n;</trd>
  <trd lng="fr">polygonac&eacute;es</trd>
  <trd lng="hu">keser&udblac;f&udblac;f&eacute;l&eacute;k</trd>
  <trd lng="nl">veelknopigen</trd>
  <trd lng="pl">rdestowate</trd>
  <trd lng="pt">poligon&aacute;ceas</trd>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_ch;&c_i;&c_sh;&c_n;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>
