<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: adiabat.xml,v 1.6 2023/10/27 13:42:09 afido Exp $">
    <kap>
      <rad>adiabat</rad>/a
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="adiabat.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc mrk="adiabat.0a.termodinamika">
    <dif>
      Okazanta sen inter&scirc;an&gcirc;o de varmo kun la &ccirc;irka&ubreve;a medio:
      <ekz>
        procezo estas <tld/>a se &gcirc;i ne transportas varmon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30532">Termodinamiko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la izoterman kaj <tld/>an procezojn oni ne povas praktike realigi, ili
        reprezentas idealajn procezojn
        <fnt>
          <aut>Miroslav T&uring;ma</aut>
          <vrk><url ref="https://eo.wikibooks.org/wiki/Fiziko/Termodinamiko2">Termodinamiko</url></vrk>
          <lok>5) La politropa procezo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">adiabatisch</trd>
    <trd lng="en">adiabatic</trd>
    <trd lng="fr">adiabatique</trd>
    <trdgrp lng="zh">
    <trd>&#x4E0D;&#x4F20;&#x70ED; <pr>b&#xF9;zhu&#xE0;nr&#xE8;</pr></trd>,
    <trd>&#x4E0D;&#x50B3;&#x71B1; <pr>b&#xF9;zhu&#xE0;nr&#xE8;</pr></trd>,
    <trd>&#x7D55;&#x71B1; <pr>ju&#xE9;r&#xE8;</pr></trd>,
    <trd>&#x7EDD;&#x70ED; <pr>ju&#xE9;r&#xE8;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="adiabat.0a.kvantuma">
    <dif>Ne ka&ubreve;zanta kvantumstatan transiron.</dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_y;&c_ja;&c_b;&c_a;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="ja">
    <trd>&#x65AD;&#x71B1;&#x306E;  <pr>&#x3060;&#x3093;&#x306D;&#x3064;&#x306E;</pr></trd>,
    <trd>&#x65AD;&#x71B1;&#x7684;&#x306A;  <pr>&#x3060;&#x3093;&#x306D;&#x3064;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
</drv>

  </art>

