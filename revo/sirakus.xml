<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: sirakus.xml,v 1.10 2024/03/24 20:21:57 afido Exp $">
    <kap>
      <rad>Sirakus</rad>/o <fnt><bib>NT</bib></fnt>,
      <var><kap><rad var="z">Sirakuz</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
    </kap>

<drv mrk="sirakus.0o">
  <kap><tld/>o, <var><kap><tld var="z"/>o</kap></var></kap>
  <uzo tip="fak">GEOG</uzo>
  <snc mrk="sirakus.0o.sicilia_urbo">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Haven<ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      &ccirc;e la orienta bordo de <ref tip="malprt" cel="sicili.0o">Sicilio</ref>,
      en la antikvo tre grava politika kaj kultura centro:
      <ekz>
        albordi&gcirc;inte &ccirc;e <tld/>o, ni restis tie tri tagojn
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 28:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="malprt" cel="arhxim.0o">Arkimedo</ref> el <tld var="z"/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011496.php">3-14-15: la jarcenta pi-tago estis
          celebrita</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kartagaj interesoj sur Sicilio balda&ubreve; kruci&gcirc;is kun interesoj de tieaj grekaj
        kolonioj, precipe kun la potenca <tld/>o, kio sekve kondukis al sen&ccirc;esaj
        militoj inter amba&ubreve; urboj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=122584">Antikva epoko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_S;&c_y;&c_r;&c_a;&c_k;&c_u;&c_z;&c_y;</trd>
    <trd lng="cs">Syrakusy</trd>
    <trd lng="de">Syrakus</trd>
    <trd lng="fr">Syracuse</trd>
    <trd lng="grc">&Sigma;&ypsilon;&rho;&alfa_ton;&kappa;&omikron;&ypsilon;&sigma;&alfa;&jota;</trd>
    <trd lng="it">Siracusa</trd>
    <trd lng="lat">Syrac&#x163B;sae</trd>
    <trd lng="ru">&c_S;&c_i;&c_r;&c_a;&c_k;&c_u;&c_z;&c_y;</trd>
    <trd lng="sk">Syrak&uacute;zy</trd>
  </snc>
  <snc mrk="sirakus.0o.novjorko">
    <dif>
      <ref tip="lst" cel="urb.0o" lst="voko:urboj">Urbo</ref> en la
      usona sub&scirc;tato <ref tip="malprt" cel="jork.nov0o.GEOG_sxtato">Novjorko</ref>,
      situanta sude de Ontaria lago, nomita la&ubreve; la sicilia urbo:
      <ekz>
        mi mem rimarkis la saman similecon en lifto en
        <tld var="z"/>o, Nov-Jorko
        <fnt>
          <aut>K. Miner</aut>,
          <vrk><url ref="http://www.sunflower.com/~miner/TAGLIBRO_package/Novembro_2006.html">
          taglibro</url></vrk>,
          <lok>2006-11-30</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_S;&c_y;&c_r;&c_a;&c_k;&#x2019;&c_ju;&c_s;</trd>
    <trd lng="ru">&c_S;&c_i;&c_r;&c_a;&c_k;&c_mol;&c_ju;&c_s;</trd>
  </snc>
  <trd lng="en">Syracuse</trd>
  <trd lng="ja">&#x30B7;&#x30E9;&#x30AF;&#x30B6;</trd>
  <trd lng="pl">Syrakuzy</trd>
  <trd lng="uk">&c_S;&c_i;&c_r;&c_a;&#x43A;&c_u;&c_z;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x53D9;&#x62C9;&#x53E4; <pr>X&#xF9;l&#x101;g&#x1D4;</pr></trd>,
    <trd>&#x6558;&#x62C9;&#x53E4; <pr>X&#xF9;l&#x101;g&#x1D4;</pr></trd>,
    <trd>&#x932B;&#x62C9;&#x5EAB;&#x85A9; <pr>X&#x12B;l&#x101;k&#xF9;s&#xE0;</pr></trd>,
    <trd>&#x9521;&#x62C9;&#x5E93;&#x8428; <pr>X&#x12B;l&#x101;k&#xF9;s&#xE0;</pr></trd>,
    <trd>&#x96EA;&#x57CE; <pr>Xu&#x11B;ch&#xE9;ng</pr></trd>
  </trdgrp>
</drv>
  </art>

