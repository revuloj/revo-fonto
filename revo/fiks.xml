<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>fiks</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fiks.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fiks.0i.flagon">
    <dif>
      Firme senmovigi almetante al io:
      <ekz>
        <tld/>i flagon &ccirc;e
        fenestro, ankron &ccirc;e la fundo, tabulon per najloj, najlon
        en muro, rompitan membron per banda&gcirc;o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="forgx.al0i">alfor&gcirc;i</ref>,
      <ref cel="glu.al0i">alglui</ref>,
      <ref cel="krocx.al0i">alkro&ccirc;i</ref>,
      <ref cel="lig.al0i">alligi</ref>,
      <ref cel="ten.al0i">alteni</ref>,
      <ref cel="firm">firmigi</ref>,
      <ref cel="najl.0i">najli</ref>.
    </refgrp>
    <trd lng="de">befestigen</trd>
    <trdgrp lng="hu">
      <trd>r&ouml;gz&iacute;t</trd>,
      <trd>fix&aacute;l</trd>,
      <trd>r&aacute;er&odblac;s&iacute;t</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>vastmaken</trd>,
      <trd>vastzetten</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>mocowa&cacute;</trd>,
      <trd>umocowa&cacute;</trd>,
      <trd>przymocowa&cacute;</trd>,
      <trd>przytwierdza&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fiks.0i.rigardon">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Igi senmova, sen&scirc;an&gcirc;a:
      <ekz>
        <ind><tld/>i la okulojn</ind><fnt>Z</fnt>
        <trd lng="pl"><ind>utkwi&cacute;</ind> wzrok</trd>,
      </ekz>
      <ekz>
        la atenton sur io;
      </ekz>
      <ekz>
        <tld/>ita rigardo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i <klr>(precizigi)</klr> regulon, sencon de vorto,
        netu&scirc;eblan fundamenton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i sian konvinkon;
      </ekz>
      <ekz>
        <tld/>i prezon, rendevuon;
      </ekz>
      <ekz>
        &ccirc;e la <tld/>ita horo, dato.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="defini.0igi">definitivigi</ref>,
      <ref cel="difin.0i">difini</ref>.
    </refgrp>
    <trd lng="de">fixieren</trd>
    <trdgrp lng="hu">
      <trd>r&aacute;szegez<klr tip="ind"> (&aacute;tv.)</klr></trd>,
      <trd>megszab</trd>,
      <trd>meg&aacute;llap&iacute;t</trd>,
      <trd>lesz&ouml;gez<klr tip="ind"> (&aacute;tv.)</klr></trd>
    </trdgrp>
    <trd lng="nl">fixeren</trd>
    <trdgrp lng="pl">
      <trd>ustala&cacute;</trd>,
      <trd>zafiksowa&cacute; <klr> (pot.)</klr></trd>,
      <trd>upewnia&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fiks.0i.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Igi fotografa&jcirc;on nedifektebla de la lumo.
    </dif>
    <trd lng="de">fixieren</trd>
    <trd lng="nl">fixeren</trd>
    <trd lng="pl">utrwala&cacute;</trd>
  </snc>
  <snc mrk="fiks.0i.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Kapti el gasa a&ubreve; likva medio kaj enpreni en
      stabilan kombina&jcirc;on:
      <ekz>
        bakterioj <tld/>as nitrogenon el la aero
        <fnt>
          <aut>W. F. Rolt</aut>,
          <vrk>El la Polvo de la Tero</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>stanovit</trd>,
    <trd>ujednat</trd>,
    <trd>upevnit</trd>,
    <trd>upírat <klr>(zrak)</klr></trd>,
    <trd>určit</trd>,
    <trd>zakotvit</trd>
  </trdgrp>
  <trd lng="en">fix</trd>
  <trd lng="es">fijar</trd>
  <trdgrp lng="fr">
    <trd>fixer<klr tip="ind"> (caler, d&eacute;terminer, phot.)</klr>
    </trd>,
    <trd>arrimer</trd>,
    <trd>arr&ecirc;ter<klr tip="ind"> (fixer)</klr></trd>,
    <trd>caler</trd>,
    <trd>d&eacute;terminer</trd>,
    <trd>figer<klr tip="ind"> (fixer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fix&aacute;l</trd>,
    <trd>r&ouml;gz&iacute;t</trd>
  </trdgrp>
  <trd lng="pt">fixar</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_f;&c_i;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pripevniť</trd>,
    <trd>stanoviť</trd>,
    <trd>upevniť</trd>,
    <trd>určiť</trd>
  </trdgrp>
</drv>

<drv mrk="fiks.0a">
  <kap><tld/>a</kap>
  <snc mrk="fiks.0a.fiksita">
    <dif>
      Nemovebla, sen&scirc;an&gcirc;a:
      <ekz>
        <tld/>a aparato;
      </ekz>
      <ekz>
        <tld/>aj rigardoj;
      </ekz>
      <ekz>
        man&gcirc;o la&ubreve; <tld/>a <klr>(anta&ubreve;<tld/>ita)
        </klr> prezo <klr>(<ref tip="vid" cel="kart.0o">karto</ref>)
        </klr>;
      </ekz>
      <ekz>
        <tld/>a koloro de &scirc;tofo;
      </ekz>
      <ekz>
        festo okazanta je <tld/>a dato;
      </ekz>
      <ekz>
        <ind><tld/>e rigardi al iu en la okulojn</ind><fnt>Z</fnt>
        <trd lng="pl">patrze&cacute; <klr>komu&sacute;</klr> prosto w
        oczy</trd>,
      </ekz>
      <ekz>
        <ind><tld/>e rigardi al iu en la viza&gcirc;on</ind><fnt>Z</fnt>
        <trd lng="pl">patrze&cacute; <klr>komu&sacute;</klr> prosto w
        twarz</trd>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>fixní <klr>(termínovaný)</klr></trd>,
    <trd>s pevně určenou lhůtou</trd>,
    <trd>s pevně určeným  termínem</trd>,
    <trd>s pevně určeným dnem</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>fest</trd>,
    <trd>starr</trd>,
    <trd>fixiert</trd>,
    <trd>unbeweglich</trd>
  </trdgrp>
  <trd lng="es">fijo</trd>
  <trdgrp lng="fr">
    <trd>fixe</trd>,
    <trd>fix&eacute;</trd>,
    <trd>fig&eacute;<klr tip="ind"> (fixe)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>fix</trd>,
    <trd>szil&aacute;rd</trd>,
    <trd>meghat&aacute;rozott</trd>,
    <trd>r&ouml;gz&iacute;tett</trd>
  </trdgrp>
  <trd lng="nl">vast</trd>
  <trdgrp lng="pl">
    <trd>sta&lstroke;y</trd>,
    <trd>ustalony</trd>,
    <trd>niezmienny</trd>,
    <trd>trwa&lstroke;y</trd>,
    <trd>zafiksowany<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_r;&c_je;&c_p;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_f;&c_i;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; <klr>(&c_o; &c_v;&c_z;&c_g;&c_l;&c_ja;&c_d;&c_je;)</klr>
    </trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fixný</trd>,
    <trd>nepohyblivý</trd>,
    <trd>pevný</trd>
  </trdgrp>
</drv>

<drv mrk="fiks.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="fiks.0eco.fiksiteco">
    <dif>
      Eco de tio, kio estas <tld/>a.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>pevnost</trd>,
    <trd>stálost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Starrheit</trd>,
    <trd>Unbeweglichkeit</trd>
  </trdgrp>
  <trd lng="es">fijeza</trd>
  <trd lng="fr">fixit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>szil&aacute;rds&aacute;g</trd>,
    <trd>r&ouml;gz&iacute;tetts&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>trwa&lstroke;o&sacute;&cacute;</trd>,
    <trd>sta&lstroke;o&sacute;&cacute;</trd>,
    <trd>niezmienno&sacute;&cacute;</trd>,
    <trd>zafiksowanie <klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_r;&c_je;&c_p;&c_l;&c_jo;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_f;&c_i;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">pevnosť</trd>
</drv>

<drv mrk="fiks.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="fiks.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>ita:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu sceno <tld/>i&gcirc;is en mia memoro.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd><klr>sich </klr>festsetzen</trd>,
    <trd><klr>sich </klr>befestigen</trd>
  </trdgrp>
  <trd lng="es">fijarse</trd>
  <trd lng="fr">se <ind>fixer</ind></trd>
  <trdgrp lng="hu">
    <trd>r&ouml;gz&uuml;l</trd>,
    <trd>megszil&aacute;rdul</trd>
  </trdgrp>
  <trd lng="nl">zich <ind>vastzetten</ind></trd>
  <trdgrp lng="pl">
    <trd>ustala&cacute; si&eogonek;</trd>,
    <trd>utrwala&cacute; si&eogonek;</trd>,
    <trd>zafiksowa&cacute; si&eogonek;<klr> (pot.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>
    &c_z;&c_a;&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_f;&c_i;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="fiks.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="fiks.0ilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Me&hcirc;anika aran&gcirc;a&jcirc;o por <tld/>i ion en dezirata
      pozicio:
      <refgrp tip="sub">
        <ref cel="agraf.0o">agrafo</ref>,
        <ref cel="kramp.0o.U">krampo</ref>,
        <ref cel="najl.0o">najlo</ref>
      </refgrp>
      ktp.
    </dif>
    <ref tip="super" cel="junt.0o.ilo">junto</ref>
    <trdgrp lng="de">
      <trd>Befestigung</trd>,
      <trd>Befestigungsstift</trd>,
      <trd>Fixierung</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>fijador</trd>,
      <trd>cierre</trd>
    </trdgrp>
    <trd lng="fr">fixation<klr> (m&eacute;ca.)</klr></trd>
    <trdgrp lng="hu">
      <trd>r&ouml;gz&iacute;t&odblac;</trd>,
      <trd>r&ouml;gz&iacute;t&odblac;elem</trd>
    </trdgrp>
    <trd lng="pl">mocowanie</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_r;&c_je;&c_p;&c_a;</trd>,
      <trd>&c_f;&c_i;&c_k;&c_s;&c_a;&c_t;&c_o;&c_r;</trd>,
      <trd>&c_k;&c_r;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fiks.0ilo.FOT">
    <uzo tip="fak">FOT</uzo>
    <dif>
      &Hcirc;emia&jcirc;o por <tld/>i,<sncref ref="fiks.0i.FOT"/>
      fotografa&jcirc;ojn.
    </dif>
    <trdgrp lng="de">
      <trd>Fixierer</trd>,
      <trd>Fixiermittel</trd>
    </trdgrp>
    <trd lng="es">fijador</trd>
    <trd lng="fr">fixatif<klr> (subst.)</klr></trd>
    <trdgrp lng="hu">
      <trd>r&ouml;gz&iacute;t&odblac;szer</trd>,
      <trd>fix&aacute;l&oacute;szer</trd>,
      <trd>fix&iacute;rs&oacute;</trd>
    </trdgrp>
    <trd lng="nl">fixeer</trd>
    <trd lng="pl">utrwalacz</trd>
    <trd lng="ru">&c_f;&c_i;&c_k;&c_s;&c_a;&c_zh;</trd>
  </snc>
  <trd lng="cs">vázání <klr>(na lyžích)</klr></trd>
  <trd lng="en">fixer</trd>
  <trd lng="sk">aretačné zariadenie</trd>
</drv>

<drv mrk="fiks.antaux0i">
  <kap>anta&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fiks.antaux0i.KOMUNE">
    <dif>
      Anticipe <tld/>i:
      <ekz>
        <klr>[la edzo]</klr> ne revenis je la anta&ubreve;<tld/>ita tempo
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 19</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">prefijar</trd>
  <trdgrp lng="fr">
    <trd>fixer d'avance</trd>,
    <trd>convenir &agrave; l'avance</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ustala&cacute; <klr>zawczasu</klr></trd>,
    <trd>okre&sacute;la&cacute; <klr>zawczasu</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_r;&c_a;&c_n;&c_je;&c_je; &c_o;&c_p;&c_r;&c_je;&c_d;&c_je;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_a;&c_n;&c_je;&c_je; &c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="fiks.cxirkaux0igxi">
  <kap>&ccirc;irka&ubreve;<tld/>i&gcirc;i</kap>
  <snc mrk="fiks.cxirkaux0igxi.KOMUNE">
    <dif>
      <tld lit="F"/>i&gcirc;i sur la &ccirc;irka&ubreve;o:
      <ekz>
        la plumaj kovroj firme &ccirc;irka&ubreve;<tld/>i&gcirc;is
        sur ili, tiel ke ili balda&ubreve; levi&gcirc;is de la tero
        en la formo de du blankaj cignoj
        <fnt><bib>Fab4</bib><lok>Filino de la mar&ccirc;a re&gcirc;o
        </lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es"><ind>establecer</ind>se<klr tip="ind"> (sobre el
  per&iacute;metro)
  </klr></trd>
  <trd lng="fr">se <ind>fixer</ind><klr tip="ind"> (sur le pourtour)
  </klr></trd>
</drv>

<drv mrk="fiks.0punkta.koma">
  <kap><tld/>punkta, <var><kap><tld/>koma</kap></var></kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Rilata al la
      <ref tip="malprt" cel="frakci.pozicia0o">poziciaj frakcioj</ref>
      (praktike, dekumaj a&ubreve; duumaj frakcioj) kiuj
      havas sian <ref tip="prt" cel="on.0punkto.komo">onpunkton</ref>
      (resp. onkomon) en iu anta&ubreve;difinita, <tld/>ita pozicio:
      <ekz>
        la monsumoj $5.97 ktp estas <tld/>punktaj (la punkto
        apartigas la cendojn, <ctl>centonojn</ctl>);
      </ekz>
      <ekz>
        <tld/>punkta nombroprezento, aritmetiko.
      </ekz>
    </dif>
    <ref tip="malprt" cel="reel.0o.KOMP">reeloj;</ref>
    <ref tip="vid" cel="glit.0punkta.koma">glitpunkta, glitkoma.</ref>
  </snc>
  <trd lng="cs">s pevnou desetinou čárkou</trd>
  <trd lng="de">Festkomma</trd>
  <trd lng="en">fixed-point</trd>
  <trd lng="es">punto decimal</trd>
  <trd lng="fr">en <ind>virgule</ind> fixe</trd>
  <trd lng="hu">fixpontos</trd>
  <trd lng="pl">sta&lstroke;oprzecinkowy</trd>
  <trd lng="ru">&c_s; <ind>&c_f;&c_i;&c_k;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_o;&c_j;</ind> &c_t;&c_o;&c_ch;&c_k;&c_o;&c_j;
  <klr>(&c_i;&c_l;&c_i; &c_z;&c_a;&c_p;&c_ja;&c_t;&c_o;&c_j;)</klr>
  </trd>
  <trd lng="sk">s pevnou desatinnou čiarkou</trd>
</drv>
</art>
