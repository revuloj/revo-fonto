<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>fiks</rad>/i
    </kap>
    <drv mrk="fiks.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc num="1">
        <dif>
          Firme senmovigi almetante al io: <ekz><tld/>i flagon &ccirc;e
          fenestro, ankron &ccirc;e la fundo, tabulon per najloj, najlon en
          muro, rompitan membron per banda&gcirc;o.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="al">aligi</ref>,
          <ref cel="krocx.al0i">alkro&ccirc;i</ref>,
          <ref cel="najl.0i">najli</ref>,
          <ref cel="forgx.al0i">alfor&gcirc;i</ref>,
          <ref cel="glu.al0i">alglui</ref>,
          <ref cel="firm">firmigi</ref>.
        </refgrp>
      </snc>
      <snc num="2">
        <uzo tip="stl">FIG</uzo><dif>
          Igi senmova, ne&scirc;an&gcirc;anta:  <ekz><tld/>i la okulojn<fnt>Z</fnt>,
          </ekz> <ekz>la atenton sur io;
          </ekz> <ekz><tld/>ita rigardo<fnt>Z</fnt>;
          </ekz> <ekz><tld/>i (precizigi) regulon, sencon de vorto,
            netu&scirc;eblan fundamenton<fnt>Z</fnt>;
          </ekz> <ekz><tld/>i sian konvinkon;
          </ekz> <ekz><tld/>i prezon, rendevuon;
          </ekz> <ekz>&ccirc;e la <tld/>ita horo, dato.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="difin.0i">difini</ref>.
        </refgrp>
      </snc>
      <snc num="3">
        <uzo tip="fak">FOT</uzo>
        <dif>
          Igi fotografa&jcirc;on nedifektebla de la lumo. 
        </dif>
      </snc>
      <trd lng="en">fix</trd>
    </drv>
    <drv mrk="fiks.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Nemovebla, ne&scirc;an&gcirc;anta: <ekz><tld/>a aparato;
          </ekz> <ekz><tld/>aj rigardoj;
          </ekz> <ekz>man&gcirc;o la&ubreve; <tld/>a (anta&ubreve;<tld/>ita) prezo
            (<ref tip="sin" cel="kart.0o">karto</ref>);
          </ekz> <ekz><tld/>a koloro de &scirc;tofo; 
          </ekz> <ekz>festo okazanta je <tld/>a dato; 
          </ekz> <ekz><tld/>e rigardi al iu en la okulojn<fnt>Z</fnt>, 
            en la viza&gcirc;on<fnt>Z</fnt>. 
          </ekz>
        </dif>
      </snc>
    </drv>
    <drv mrk="fiks.0eco">
      <kap><tld/>eco</kap>
      <snc>
        <dif>
          Eco de tio, kio estas <tld/>a. 
        </dif>
      </snc>
    </drv>
    <drv mrk="fiks.0igxi">
      <kap><tld/>i&gcirc;i</kap>
      <snc>
        <dif>
          I&gcirc;i <tld/>ita: (f) <ekz>tiu sceno <tld/>i&gcirc;is en mia memoro.
          </ekz>
        </dif>
      </snc>
    </drv>
    <drv mrk="fiks.0punkta.koma">
      <kap><tld/>punkta, <tld/>koma</kap>
      <snc>
        <uzo tip="fak">KOMP</uzo>
        <dif>
          Rilata al la
          <ref tip="malprt" cel="frakci.0o.pozicia">poziciaj frakcioj</ref>
          (praktike, <ref cel="dek.0uma.frakcio">dekumaj frakcioj</ref>
          a&ubreve; <ref cel="du.0uma.frakcio">duumaj frakcioj</ref>) kiuj
          havas sian <ref tip="prt" cel="on.0punkto.komo">onpunkton</ref>
          (resp. onkomon) en iu anta&ubreve;difinita, <tld/>ita pozicio:
          <ekz>la monsumoj $5.97 ktp estas <tld/>punktaj (la punkto
            apartigas la cendojn, <ctl>centonojn</ctl>);</ekz>
          <ekz><tld/>punkta nombroprezento, aritmetiko</ekz>
        </dif>
        <ref tip="malprt" cel="reel.0o.komp">reeloj</ref>
        <ref tip="vid" cel="glit.0punkta.koma">glitpunkta, glitkoma</ref>
      </snc>
      <trd lng="en">fixed-point</trd>
      <trd lng="ru">&#x441; <ind>&#x444;&#x438;&#x43a;&#x441;&#x438;&#x440;&#x43e;&#x432;&#x430;&#x43d;&#x43d;&#x43e;&#x439;</ind> &#x442;&#x43e;&#x447;&#x43a;&#x43e;&#x439;
        <klr>(&#x438;&#x43b;&#x438; &#x437;&#x430;&#x43f;&#x44f;&#x442;&#x43e;&#x439;)</klr></trd>
    </drv>
  </art>
