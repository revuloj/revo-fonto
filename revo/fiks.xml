<drv mrk="fiks.0koma">
  <kap><tld/>punkta, <var><kap><tld/>koma</kap></var></kap>
  <uzo tip="fak">KOMP</uzo>
  <snc>
    <dif>
      Rilata al la
      <ref tip="malprt" cel="frakci.pozicia0o">poziciaj frakcioj</ref>
      (praktike, dekumaj a&ubreve; duumaj frakcioj) kiuj
      havas sian <ref tip="prt" cel="on.0komo">onpunkton</ref>
      (resp. onkomon) en iu anta&ubreve;difinita, <tld/>ita pozicio:
      <ekz>
        la monsumoj $5.97 ktp estas <tld/>punktaj (la punkto
        apartigas la cendojn, <ctl>centonojn</ctl>);
      </ekz><ekz>
        <tld/>punkta nombroprezento, aritmetiko.
      </ekz>
    </dif>
    <ref tip="malprt" cel="reel.0o.KOMP">reeloj</ref>
    <ref tip="vid" cel="glit.0punkta">glitpunkta, glitkoma</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol; <ind>&c_f;&c_ib;&c_k;&c_s;&c_a;&c_v;&c_a;&c_n;&c_a;&c_j;</ind> &c_k;&c_o;&c_s;&c_k;&c_a;&c_j;</trd>,
    <trd>&c_z;&c_mol; <ind>&c_f;&c_ib;&c_k;&c_s;&c_a;&c_v;&c_a;&c_n;&c_y;&c_m;</ind> &c_p;&c_u;&c_n;&c_k;&c_t;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="cs">s pevnou desetinou &ccaron;&aacute;rkou</trd>
  <trd lng="de">Festkomma</trd>
