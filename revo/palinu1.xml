  <trd lng="pt">palinur&iacute;deos</trd>
  <trd lng="ru">&c_l;&c_a;&c_n;&c_g;&c_u;&c_s;&c_t;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>  
</drv>
