<art mrk="$Id$">
<kap><rad>palinured</rad>/oj</kap>
<drv mrk="palinu1.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo
      <ref tip="super" cel="krust.dekpiedaj0uloj">dekpiedaj
      krustuloj</ref> (<trd lng="la">Palinuridae</trd>),
