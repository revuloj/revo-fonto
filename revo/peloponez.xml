    <kap>
      <rad>Peloponez</rad>/o
    </kap>

<drv mrk="peloponez.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="insul.duon0o">Duoninsulo</ref>,
      kiu formas la sudan parton de kontinenta <ref tip="malprt" cel="grek.G0ujo">Grekujo</ref>:
      <ekz>
        enkuris kun sova&gcirc;a <klr>[&#x2026;]</klr> bojado
        tutaj aroj da hundoj: flavharaj
        grandegaj molosoj el <tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        iom pli bona probable pro la turismo, estas la
        situacio en Atiko, en Kreto, en
        <tld/>o
        <fnt>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010708.php">Mil senlaboruloj tage</url></vrk>
              <bib>Monato</bib>
          <lok>2012/06, p. 12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Peloponnes</trd>
    <trd lng="el">&Pi;&epsilon;&lambda;&omikron;&pi;&omikron_ton;&ny;&ny;&eta;&sigma;&omikron;&sigma_fina;</trd>
    <trd lng="en">Peloponnese</trd>
    <trd lng="fr">P&eacute;loponn&egrave;se</trd>
  <trd lng="grc">&Pi;&epsilon;&lambda;&omikron;&pi;&omikron_ton;&ny;&ny;&eta;&sigma;&omikron;&sigma_fina;</trd>
  <trd lng="nl">Peloponnesos</trd>
  </snc>
  <trd lng="ja">&#x30DA;&#x30ED;&#x30DD;&#x30CD;&#x30BD;&#x30B9;&#x534A;&#x5CF6;  [<ind>&#x30DA;&#x30ED;&#x30DD;&#x30CD;&#x30BD;&#x30B9;&#x306F;&#x3093;&#x3068;&#x3046;</ind>]</trd>
</drv>

