<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: endemi.xml,v 1.20 2022/04/27 14:38:54 revo Exp $">
<kap>
  <rad>endemi</rad>/o <fnt>Z</fnt> <fnt> <bib>PV</bib></fnt>
</kap>
<drv mrk="endemi.0o">
  <kap><tld/>o</kap>
  <snc mrk="endemi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Malsano reganta konstante en iu lando a&ubreve; regiono. 
    </dif>
    <refgrp tip="vid">
      <ref cel="epidem.0o">epidemio</ref>.
    </refgrp>
    <trd lng="be">&c_e;&c_n;&c_d;&c_e;&c_m;&c_ib;&c_ja;</trd>
    <trdgrp lng="cs">
      <trd>endemie</trd>,
      <trd>m&iacute;stn&iacute; nemoc</trd>
    </trdgrp>
    <trdgrp lng="de">
     <trd>Endemie</trd>,
     <trd>endemische <ind>Krankheit</ind></trd>
    </trdgrp>
    <trd lng="en">endemic</trd>
    <trd lng="fr">end&eacute;mie</trd>
    <trd lng="hu">end&eacute;mia</trd>
    <trd lng="nl">endemie</trd>
    <trd lng="pl">endemia</trd>
    <trd lng="pt">endemia</trd>
    <trd lng="ru">&c_e;&c_n;&c_d;&c_je;&c_m;&c_i;&c_ja;</trd>
    <trd lng="sk">end&eacute;mia</trd>
  </snc>
  <snc mrk="endemi.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Specio aperanta nur en geografie limigita regiono:
      <ekz>
        Flavgor&gcirc;a kanario <klr>[...]</klr> trovi&gcirc;as kiel endemio nur en centrorienta Etiopio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=337088">Flavgor&gcirc;a kanario</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">endemische Art</trd>
  </snc>
  <trd lng="uk">&#x435;&#x43D;&#x434;&#x435;&#x43C;&#x456;&#x44F;</trd>
</drv>

<drv mrk="endemi.0a">
  <kap><tld/>a</kap>
  <snc mrk="endemi.0a.BIO">
    <dif>
      Havanta karakteron de <tld/>o; enlanda, loka:
      <ekz>
        7%  de la insulaj birdoj estas <tld/>aj
        <fnt>
         <aut>M. Gutierres Gonz&aacute;lez</aut>,
         <vrk><url ref="http://www.radiohc.cu/eo/noticias/ambiente/22943-okaze-de-la-internacia-tago-de-birdoj">
         Okaze de la Internacia Tago de Birdoj</url></vrk>,
         <lok>Radio Havano Kubo, 2014-12-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_d;&c_e;&c_m;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="de">endemisch</trd>
  <trd lng="en">endemic</trd>
  <trd lng="es">end&eacute;mico</trd>
  <trd lng="fr">end&eacute;mique</trd>
  <trd lng="hu">endemikus</trd>
  <trd lng="nl">endemisch</trd>
  <trd lng="pl">endemiczny</trd>
  <trd lng="pt">end&ecirc;mico</trd>
  <trd lng="ru">&c_e;&c_n;&c_d;&c_je;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="uk">&#x435;&#x43D;&#x434;&#x435;&#x43C;&#x456;&#x447;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="endemi.0eco">
  <kap><tld/>eco,
  <var><kap><tld/>ismo</kap></var></kap>
  <snc mrk="endemi.0eco.BIO">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">MED</uzo>
    <dif>
      Specifa ekzisto de malsano, de vivula specio
      en unu a&ubreve; kelkaj malgrandaj medioj a&ubreve; regionoj:
      <ekz>
        montaraj <tld/>ismoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Boliviaj_jungaoj">
          </url>, 2014-08-06</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">endemismus</trd>
  <trd lng="fr">end&eacute;misme</trd>
  <trd lng="sk">endemizmus</trd>
  <trd lng="uk">&#x435;&#x43D;&#x434;&#x435;&#x43C;&#x456;&#x447;&#x43D;&#x456;&#x441;&#x442;&#x44C;</trd>
</drv>
</art>
