<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konjunktiv</rad>/o
</kap>
<drv mrk="konjun4.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Mukmembrano, kiu surkovras la malanta&ubreve;an surfacon de la palpebroj kaj
      la surfacon de la okulglobo.
    </dif>
  </snc>
  <refgrp tip="hom">
    Ne konfuzu kun:
    <ref cel="konjun1.0o">konjunktivo</ref> (modalo)
  </refgrp>
  <trd lng="be">&c_k;&c_a;&c_n;&c_mol;&c_ju;&c_n;&c_k;&c_t;&c_y;&c_v;&c_a; <klr tip="amb">(&c_v;&c_o;&c_k;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>Konjunktiva</trd>,
    <trd>Bindehaut</trd>
  </trdgrp>
  <trd lng="en">conjunctiva</trd>
  <trd lng="fr">conjonctive</trd>
  <trd lng="nl">bindvlies v.h.oog</trd>
  <trd lng="pl">spoj&oacute;wka</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a; &c_g;&c_l;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_malmol;&c_ju;&c_n;&c_k;&c_t;&c_i;&c_v;&c_a;</trd>
  </trdgrp>
</drv>
</art>

