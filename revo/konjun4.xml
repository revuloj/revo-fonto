<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konjun4.xml,v 1.13 2023/12/03 09:58:56 revo Exp $">
<kap>
  <rad>konjunktiv</rad>/o
</kap>
<drv mrk="konjun4.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Mukmembrano, kiu surkovras la malanta&ubreve;an surfacon de la palpebroj
      kaj la surfacon de la okulglobo:
      <ekz>
        kronika kataro de la <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="konjun2.0o">konjunktivito</ref>
  </snc>
  <rim>
    Ne konfuzu kun <ref tip="hom" cel="konjun1.0o">konjunktivo</ref> (modalo).
  </rim>
  <trd lng="be">&c_k;&c_a;&c_n;&c_mol;&c_ju;&c_n;&c_k;&c_t;&c_y;&c_v;&c_a; <klr tip="amb">(&c_v;&c_o;&c_k;&c_a;)</klr></trd>
  <trd lng="bg">&c_k;&c_o;&c_n;&c_ju;&c_n;&c_k;&c_t;&c_i;&c_v;&c_a;</trd>
  <trd lng="ca">conjuntiva</trd>
  <trdgrp lng="de">
    <trd>Konjunktiva</trd>,
    <trd>Bindehaut</trd>
  </trdgrp>
  <trd lng="en">conjunctiva</trd>
  <trd lng="es">conjuntiva</trd>
  <trd lng="fa">&a_m;&a_l;&a_t;&a_H;&a_m;&a_h;</trd>
  <trd lng="fr">conjonctive</trd>
  <trd lng="it">congiuntiva</trd>
  <trd lng="ja">&#x7D50;&#x819C;  <pr>&#x3051;&#x3064;&#x307E;&#x304F;</pr></trd>
  <trd lng="nl">bindvlies v.h.oog</trd>
  <trd lng="pl">spoj&oacute;wka</trd>
  <trd lng="pt">conjuntiva</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_je;&c_d;&c_i;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_o;&c_b;&c_o;&c_l;&c_o;&c_ch;&c_k;&c_a; &c_g;&c_l;&c_a;&c_z;&c_a;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_malmol;&c_ju;&c_n;&c_k;&c_t;&c_i;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">konjunkt&iacute;v</trd>
  <trdgrp lng="cs">
    <trd>konjunktiva</trd>,
    <trd>o&ccaron;n&iacute; spojivka</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7D50;&#x819C; <pr>ji&#xE9;m&#xF2;</pr></trd>,
    <trd>&#x7D50;&#x819C; <pr>ji&#xE9;m&#xF3;</pr></trd>
  </trdgrp>
</drv>
</art>

