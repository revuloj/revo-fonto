<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>foj</rad>/o
</kap>
<drv mrk="foj.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      &Ccirc;iu el la okazoj, kiam io fari&gcirc;as:
      <ekz>
        li sonis tri <tld/>ojn;
      </ekz>
      <ekz>
        konstanta regulo estas mil
        <tld/>ojn pli bona<fnt>Z</fnt>;
      </ekz>
      <ekz>
        je la unua <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        por la lasta <tld/>o;
      </ekz>
      <ekz>
        unu <tld/>on por &ccirc;iam
        <fnt>(<aut>Zamenhof</aut>: <vrk>Fundamento de Esperanto</vrk>,
        <lok>Anta&ubreve;parolo</lok>)</fnt>;
      </ekz>
      <ekz>
        solvi per unu <tld/>o &ccirc;iujn
        malfacila&jcirc;ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;iu<tld/>e, kiam vi venos, oni akceptos vin plezure;
      </ekz>
      <ekz>
        vi &ccirc;iun <tld/>on faras la saman eraron;
      </ekz>
      <ekz>
        oni devus mult<tld/>e legi tekston por &gcirc;in bone memori.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Mal <klr tip="ind">(Wiederholung)</klr></trd>
  <trd lng="en">occasion</trd>
  <trdgrp lng="fr">
    <trd>fois</trd>,
    <trd>reprise<klr tip="ind"> (fois)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_z;</trd>
  <trd lng="sv">g&aring;ng <klr>(ett tillf&auml;lle)</klr></trd>
</drv>

<drv mrk="foj.0e">
  <kap>
    <tld/>e<fnt>Z</fnt>
  </kap>
  <snc>
    <dif>
      En nedifinita tempo:
      <ekz>
        <tld/>e estis re&gcirc;o...
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="iam.0">iam</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>jednou</trd>,
    <trd>kdysi</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>mal</trd>,
    <trd>einmal</trd>
  </trdgrp>
  <trd lng="fr">une <ind>fois</ind></trd>
  <trd lng="ru">&c_o;&c_d;&c_n;&c_a;&c_zh;&c_d;&c_y;</trd>
</drv>

<drv mrk="foj.0e0e">
  <kap>
    <tld/>e... <tld/>e...
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="jen.00">jen... jen...</ref>:
      <ekz>
        <tld/>e &scirc;i ridis, <tld/>e &scirc;i ploris.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;... &c_t;&c_o;...</trd>
  </trdgrp>
</drv>

<drv mrk="foj.re0e">
  <kap>
    re<tld/>e
  </kap>
  <snc>
    <dif>
      Denove.
    </dif>
  </snc>
  <trd lng="de">nochmal</trd>
  <trdgrp lng="sv">
    <trd>igen</trd>,
    <trd>en <ind>g&aring;ng</ind> till</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>une <ind>fois</ind> de plus</trd>,
    <trd>de <ind>nouveau</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_je;&c_shch;&c_jo; &c_r;&c_a;&c_z;</trd>
</drv>
</art>
