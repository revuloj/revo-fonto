<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&jcirc;onk</rad>/o <fnt>SPV</fnt>
</kap>
<drv mrk="jxonk.0o">
  <kap><tld/>o</kap>
  <snc mrk="jxonk.0o.SHI">
    <uzo tip="fak">SHI</uzo>
    <ref tip="dif" cel="gxonk.0o">&gcirc;onko</ref>
    <ekz>
      kiaj estas iliaj havenoj kun malmulto da
      <tld/>oj kaj barkoj, kompare al niaj
      multenombroj &scirc;iparoj?
      <fnt>
        <aut>D. Defo, trad. A. Krafft</aut>,
        <vrk><url
        ref="http://www.gutenberg.org/files/11511/11511-0.txt">
        Robinsono Kruso</url></vrk>,
        <lok>1908</lok>
      </fnt>.      
    </ekz>
  </snc>
  <trd lng="pt">junco<klr tip="amb">(embarca&ccedil;&atilde;o)</klr></trd>
  <trd lng="it">giunca</trd>
</drv>
</art>
