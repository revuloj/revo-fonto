<vortaro>

  <art mrk="$Id: garon.xml,v 1.5 2023/11/11 10:00:39 afido Exp $">
    <kap>
      <rad>Garon</rad>/o
    </kap>

<drv mrk="garon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:riveroj" cel="river.0o">Rivero</ref>
      fontanta en Pireneoj kaj trafluinte Tuluzon kaj Bordozon enfluanta Atlantikon:
      <ekz>
        ili la&ubreve;dis la vinon portitan de la &ccirc;irk<klr>[a&ubreve;]</klr>a&jcirc;oj de
        Garono, Loaro,
        Rodano
        <fnt>
          <aut>Frans Sammut</aut>
          <vrk><url ref="https://books.google.de/books?id=nEASBgAAQBAJ&amp;lpg=PA209&amp;ots=XXmbu_ln4A&amp;pg=PA209&amp;output=embed">La Malta Revo </url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la riverego <tld/>o kiu trairas Tuluzon grave alti&gcirc;is kaj kreis la krizan
        situacion
        <fnt>
          <aut>Robert Nielsen</aut>
          <vrk><url ref="https://www.liberafolio.org/2022/01/17/la-studio-de-vinilkosmo-trafita-de-inundo/">La studio de Vinilkosmo trafita de inundo</url></vrk>
          <lok>Libera Folio, 2022-01-17 </lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_G;&c_a;&c_r;&c_o;&c_n;&c_a;</trd>
    <trd lng="de">Garonne</trd>
    <trd lng="en">Garonne</trd>
    <trd lng="fr">Garonne</trd>
  </snc>
  <trd lng="ja">&#x30AC;&#x30ED;&#x30F3;&#x30CC;&#x5DDD;  <pr>&#x30AC;&#x30ED;&#x30F3;&#x30CC;&#x304B;&#x308F;</pr></trd>
  <trd lng="zh">&#x52A0;&#x9F8D;&#x6CB3; <pr>ji&#x101;l&#xF3;ngh&#xE9;</pr></trd>
</drv>

  </art>
