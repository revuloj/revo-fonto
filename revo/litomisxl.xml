<vortaro>
  <art mrk="$Id: litomisxl.xml,v 1.2 2020/01/25 00:10:41 revo Exp $">
    <kap>
      <rad>Litomi&scirc;l</rad>/o
    </kap>

<drv mrk="litomisxl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo><uzo
  tip="fak">BELE</uzo>
  <snc>
    <dif>
      Historia <ref tip="lst" lst="voko:urboj"
      cel="urb.0o.ARKI">urbo</ref>
      en <ref tip="malprt" cel="cxehx.CX0ujo">&Ccirc;e&hcirc;ujo</ref>, situanta en
      orienta Bohemujo:
      <ekz>
        &ccirc;iu esperantisto, kiu konas la literaturon, rekonas la nomon de
        <nom>Karolo Pi&ccirc;</nom> kaj
        la titolon de lia romano &leftquot;La <tld/>a Tombejo&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="https://www.monato.net/2009/009638.php"
              >La tulamora tombejo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[en]</klr> 1344 papo Klemento la 6-a establis en <tld/>o
        episkopejon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2666"
              >Litomy&scaron;l</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">Litomy&scaron;l</trd>
    <trd lng="de">Leitomischl</trd>
    <trd lng="pl">Litomy&sacute;l</trd>     
    <trd lng="ro">Litomy&scaron;l</trd>
  </snc>
</drv>

