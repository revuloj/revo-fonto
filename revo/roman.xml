<vortaro>
  <art mrk="$Id: roman.xml,v 1.35 2024/04/14 05:47:06 revo Exp $">
    <kap>
      <ofc>1</ofc>
      <rad>roman</rad>/o
    </kap>

    <drv mrk="roman.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">BELE</uzo>
      <snc mrk="roman.0o.proza">
        <dif>
          Grandforma beletra verko en rakonta prozo:
          <ekz>
            bonege esperantigita fare de la tradukmajstro William Auld,
            la
            <tld/>o Spartako konservas fidele la etoson kreitan de la
            skota
            verkisto
            <fnt><aut>Nicolae Macovei</aut>:
              <vrk><url ref="&Monato;/1995/monato199508.pdf#page=23">Arkitekto de la platona
              respubliko (J. L. Mitchell,
              Spartako)</url></vrk>,
              <bib>Monato</bib>
              <lok>1995/08, p. 23</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <ref tip="hom" cel="rom.0ano">rom&#x22C5;an&#x22C5;o</ref>
      <trd lng="be">&c_r;&c_a;&c_m;&c_a;&c_n;</trd>
      <trd lng="bg">&c_r;&c_o;&c_m;&c_a;&c_n;</trd>
      <trd lng="de">Roman</trd>
      <trd lng="en">novel</trd>
      <trd lng="es">novela</trd>
      <trd lng="fr">roman</trd>
      <trd lng="hu">reg&eacute;ny</trd>
      <trdgrp lng="ja">
    <trd>&#x30ED;&#x30FC;&#x30DE;&#x4EBA;  <pr>&#x30ED;&#x30FC;&#x30DE;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x30ED;&#x30FC;&#x30DE;&#x5E02;&#x6C11; 
    <pr>&#x308D;&#x30FC;&#x307E;&#x3057;&#x307F;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">roman</trd>
      <trd lng="pl">powie&sacute;&cacute;</trd>
      <trd lng="pt">romance</trd>
      <trd lng="sv">roman</trd>
      <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;</trd>
      <trd lng="sk">Riman</trd>
  <trdgrp lng="cs">
    <trd>obyvatel m&#x11B;sta &Rcaron;&#xED;ma</trd>,
    <trd>&Rcaron;&#xED;man</trd>
  </trdgrp>
  <trd lng="uk">&c_R;&#x43E;&#x43C;&c_a;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x5C0F;&#x8AAA; <pr>xi&#x1CE;oshu&#x14D;</pr></trd>,
    <trd>&#x5C0F;&#x8BF4; <pr>xi&#x1CE;oshu&#x14D;</pr></trd>,
    <trd>&#x9577;&#x7BC7;&#x5C0F;&#x8AAA; <pr>ch&#xE1;ngpi&#x101;nxi&#x1CE;oshu&#x14D;</pr></trd>,
    <trd>&#x957F;&#x7BC7;&#x5C0F;&#x8BF4; <pr>ch&#xE1;ngpi&#x101;nxi&#x1CE;oshu&#x14D;</pr></trd>
  </trdgrp>
</drv>

    <drv mrk="roman.vers0o">
      <kap>vers<tld/>o</kap>
      <uzo tip="fak">BELE</uzo>
      <snc>
        <dif>
          Versforma beletra verko rakonta, kiu &gcirc;enre similas la
          tradician <tld/>on prozan (la&ubreve; la elekto kaj prezento de la
          okaza&jcirc;oj):
          <ekz>
            tiu nenatura akcentado ofte malhelpas la sveltan kaj
            facilan fluon en la lego, tre necesan en tia vers<tld/>o
            <fnt>
              <aut>Enrico DONDI</aut>:
              <vrk>Recenzo: Sinjoro Tadeo a&ubreve; la lasta armita
                posedopreno en Litvo</vrk>.
              <lok>La nica literatura revuo, 2:2</lok>
            </fnt>;
          </ekz><ekz>
            Nikolai Lozga&ccirc;ev post longa hezitado malakceptis, kvankam
            li plu revis iam fronti la plej faman rusan vers<tld/>on
            <fnt>
              <aut>A. Kor&jcirc;enkov</aut>:
              <vrk>Enciklopedio de la rusa vivo</vrk>.
              <bib>LOdE</bib>. <lok>2003:6 (104)</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <rim>
        La diferenco inter vers<tld/>o kaj
        <ref tip="hom" cel="poem.0o.POE">poemo<sncref/></ref> estas
        precipe en
        etoso kaj a&ubreve;tora intonacio: versromano estas verko pli
        realisma, poemo estas pli fantazia.
      </rim>
      <trd lng="be">&c_r;&c_a;&c_m;&c_a;&c_n; &c_u; &c_v;&c_je;&c_r;&c_sh;&c_a;&c_h;</trd>
      <trd lng="pl">powie&sacute;&cacute; poetycka</trd>
      <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n; &c_v; &c_s;&c_t;&c_i;&c_h;&c_a;&c_h;</trd>
    </drv>

    <drv mrk="roman.0eca">
      <kap><tld/>eca</kap>
      <snc>
        <dif>
          Reveme kaj &hcirc;imere sentema kiel la herooj de la malnovaj
          <tld/>oj
        </dif>
        <refgrp tip="sin">
          <ref cel="sentim1.0a">sentimentala</ref>.
        </refgrp>
      </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_a;&c_n;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>neskute&#x10D;n&#xFD;</trd>,
    <trd>romantick&#xFD;</trd>,
    <trd>rom&#xE1;nov&#xFD;</trd>
  </trdgrp>
  <trd lng="de">romanhaft</trd>
  <trdgrp lng="fr">
        <trd>romanesque</trd>,
        <trd>romantique<klr> (romanesque)</klr></trd>
      </trdgrp>
  <trd lng="hu">reg&eacute;nyes</trd>
  <trdgrp lng="ja">
    <trd>&#x5C0F;&#x8AAC;&#x3058;&#x307F;&#x305F;  <pr>&#x3057;&#x3087;&#x3046;&#x305B;&#x3064;&#x3058;&#x307F;&#x305F;</pr></trd>,
    <trd>&#x7A7A;&#x60F3;&#x7684;&#x306A;  <pr>&#x304F;&#x3046;&#x305D;&#x3046;&#x3066;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">romantisch</trd>
  <trd lng="pl">romantyczny</trd>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;(&c_t;)&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>romantick&#xFD;</trd>,
    <trd>rom&#xE1;nov&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&#x43C;&c_a;&#x43D;&c_ib;&c_ch;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_r;&#x43E;&#x43C;&c_a;&#x43D;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

    <drv mrk="roman.0isto">
      <kap><tld/>isto</kap>
      <uzo tip="fak">BELE</uzo>
      <snc>
        <dif>
          Verkinto de romanoj:
          <ekz>
            aperis nova talenta krim<tld/>isto, kies nova libro
            pliri&ccirc;igos
            nian literaturon
            <fnt><aut>NikSt</aut>:
              <vrk><url ref="&Monato;/1994/monato199410.pdf#page=22">Bona distrilo kaj
              memlernilo</url></vrk>,
              <bib>Monato</bib>
              <lok>1994/10, p. 22</lok></fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_r;&c_a;&c_m;&c_a;&c_n;&c_ib;&c_s;&c_t;</trd>
  <trd lng="cs">romanopisec</trd>
  <trdgrp lng="de">
        <trd>Romanschriftsteller</trd>,
        <trd>Romanschreiber</trd>,
        <trd>Romanautor</trd>,
        <trd>Romancier</trd>
      </trdgrp>
  <trd lng="en">novelist</trd>
  <trd lng="es">novelista</trd>
  <trdgrp lng="fr">
        <trd>romancier</trd>
      </trdgrp>
  <trd lng="hu">reg&eacute;ny&iacute;r&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x5C0F;&#x8AAC;&#x5BB6;  <pr>&#x3057;&#x3087;&#x3046;&#x305B;&#x3064;&#x304B;</pr></trd>,
    <trd>&#x30ED;&#x30DE;&#x30F3;&#x4F5C;&#x5BB6;  <pr>&#x308D;&#x307E;&#x3093;&#x3055;&#x3063;&#x304B;</pr></trd>
  </trdgrp>
  <trd lng="nl">romanschrijver</trd>
  <trdgrp lng="pl">
    <trd>beletrysta</trd>,
    <trd>powie&sacute;ciopisarz</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">rom&#xE1;nopisec</trd>
  <trd lng="uk">&c_r;&#x43E;&#x43C;&c_a;&#x43D;&c_ib;&c_s;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x5C0F;&#x8AAA;&#x5BB6; <pr>xi&#x1CE;oshu&#x14D;ji&#x101;</pr></trd>,
    <trd>&#x5C0F;&#x8BF4;&#x5BB6; <pr>xi&#x1CE;oshu&#x14D;ji&#x101;</pr></trd>
  </trdgrp>
</drv>

    <drv mrk="roman.krim0o">
      <kap>krim<tld/>o</kap>
      <uzo tip="fak">BELE</uzo>
      <snc>
        <dif>
          Romano ordinare kun stre&ccirc;a a&ubreve; angora etoso
          rakontanta pri krimoj
          kaj persekutado de krimintoj:
          <ekz>
            moderna krim<tld/>eto kun realisme aktualaj ingrediencoj,
            kiel
            droga&ccirc;oj, pedofilio, fra&ubreve;do
            <fnt>
          <aut>Paul Veenman</aut>,
          <vrk><url ref="&Monato;/1997/ffoj.html">Krimoj ... por &ccirc;iuj temperamentoj</url></vrk>,
          <bib>Monato</bib>,
          <lok>1997/10, p. 19</lok>
        </fnt>.
          </ekz>
        </dif>
      </snc>
  <trd lng="be">&c_d;&c_e;&c_t;&c_e;&c_k;&c_t;&c_y;&c_w;
        <klr>(&c_t;&c_v;&c_o;&c_r;)</klr></trd>
  <trdgrp lng="cs">
    <trd>detektivka</trd>,
    <trd>krimin&#xE1;ln&#xED; rom&#xE1;n</trd>
  </trdgrp>
  <trdgrp lng="de">
        <trd>Kriminalroman</trd>,
        <trd>Krimi</trd>,
        <trd>Polizeiroman<klr> [ark]</klr></trd>
      </trdgrp>
  <trdgrp lng="en">
        <trd>crime novel</trd>,
        <trd>detective novel</trd>,
        <trd>murder mystery</trd>
      </trdgrp>
  <trdgrp lng="fr">
        <trd>policier<klr> (roman)</klr></trd>,
        <trd>roman noir</trd>,
        <trd>roman policier</trd>
      </trdgrp>
  <trdgrp lng="hu">
        <trd>b&udblac;n&uuml;gyi <ind>reg&eacute;ny</ind></trd>,
        <trd>krimi</trd>
      </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x72AF;&#x7F6A;&#x5C0F;&#x8AAC;  <pr>&#x306F;&#x3093;&#x3056;&#x3044;&#x3057;&#x3087;&#x3046;&#x305B;&#x3064;</pr></trd>,
    <trd>&#x63A8;&#x7406;&#x5C0F;&#x8AAC;  <pr>&#x3059;&#x3044;&#x308A;&#x3057;&#x3087;&#x3046;&#x305B;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">detectiveroman</trd>
  <trd lng="pl">krymina&lstroke;</trd>
  <trd lng="pt">romance policial</trd>
  <trdgrp lng="ru">
        <trd>&c_d;&c_je;&c_t;&c_je;&c_k;&c_t;&c_i;&c_v;<klr tip="amb"> (&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
        <trd>&c_k;&c_r;&c_i;&c_m;&c_i;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j; <ind>&c_r;&c_o;&c_m;&c_a;&c_n;</ind></trd>
      </trdgrp>
  <trd lng="sk">krimin&#xE1;lny rom&#xE1;n</trd>
  <trd lng="uk">&#x43A;&c_r;&c_i;&#x43C;&c_ib;&#x43D;&c_a;&#x43B;&#x44C;&#x43D;&c_i;&c_j; &c_r;&#x43E;&#x43C;&c_a;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x4FA6;&#x63A2;&#x5C0F;&#x8BF4; <pr>zh&#x113;nt&#xE0;nxi&#x1CE;oshu&#x14D;</pr></trd>,
    <trd>&#x5075;&#x63A2;&#x5C0F;&#x8AAA; <pr>zh&#x113;nt&#xE0;nxi&#x1CE;oshu&#x14D;</pr></trd>,
    <trd>&#x63A8;&#x7406;&#x5C0F;&#x8AAA; <pr>tu&#x12B;l&#x1D0;xi&#x1CE;oshu&#x14D;</pr></trd>,
    <trd>&#x63A8;&#x7406;&#x5C0F;&#x8BF4; <pr>tu&#x12B;l&#x1D0;xi&#x1CE;oshu&#x14D;</pr></trd>
  </trdgrp>
</drv>

