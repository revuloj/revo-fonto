<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>roman</rad>/o
</kap>
<drv mrk="roman.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BELE</uzo>
  <snc>
    <dif>
      Literatura eposa proza verko, konsistanta el longa rakonto pri image
      elpensitaj a&ubreve; aran&gcirc;itaj faktoj.
    </dif>
  </snc>
  <trd lng="de">Roman</trd>
  <trd lng="en">novel</trd>
  <trd lng="fr">roman</trd>
  <trd lng="nl">roman</trd>
  <trd lng="sv">roman</trd>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;</trd>
</drv>

<drv mrk="roman.0eca">
  <kap><tld/>eca</kap>
  <snc>
    <dif>
      Reveme kaj &hcirc;imere sentema kiel la herooj de la malnovaj <tld/>oj
    </dif>
    <refgrp tip="sin">
      <ref cel="sentim1.0a">sentimentala</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>romanesque</trd>,
    <trd>romantique</trd>
  </trdgrp>
  <trd lng="nl">romantisch</trd>
  <trd lng="ru">&c_r;&c_o;&c_m;&c_a;&c_n;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
