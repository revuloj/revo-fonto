<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tavern</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tavern.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Popola, ne tre kosta <ref tip="super" cel="drink.0ejo">drinkejo
      </ref> a&ubreve; <ref tip="super" cel="restor.0o">restoracio</ref>
      :
      <ekz>
        ne pagi en gastejoj kaj <tld/>oj
        <fnt><bib>DonKihxoto</bib>,
        <lok>&ccirc;apitro 17a, p. 120a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_v;&c_e;&c_r;&c_n;&c_a;</trd>
  <trd lng="ca">taverna</trd>
  <trdgrp lng="de">
    <trd>Taverne</trd>,
    <trd>Kneipe</trd>,
    <trd>Weinstube</trd>
  </trdgrp>
  <trd lng="es">taberna</trd>
  <trd lng="fr">taverne</trd>
  <trdgrp lng="hu">
    <trd>kocsma</trd>,
    <trd>vend&eacute;gl&odblac;</trd>,
    <trd>kiskocsma</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tawerna</trd>,
    <trd>karczma</trd>,
    <trd>ober&zdot;a</trd>,
    <trd>szynk</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>taberna</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_a;&c_v;&c_je;&c_r;&c_n;&c_a;</trd>
</drv>
</art>

