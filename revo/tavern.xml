<kap>
  <rad>tavern</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="tavern.0o">
  <kap><tld/>o</kap>
  <snc mrk="tavern.0o.TUR">
    <dif>
      Popola, ne tre kosta <ref tip="super" cel="drink.0ejo">drinkejo
      </ref> a&ubreve; <ref tip="super" cel="restor.0o">restoracio</ref>:
      <ekz>
        ili iris al <tld/>o apud la stacidomo, kaj trinkis tie &gcirc;is la 
        ferma horo
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>, 
          <lok>1978</lok>
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_a;&c_v;&c_e;&c_r;&c_n;&c_a;</trd>
  <trd lng="bo">&#xF46;&#xF44;&#xF0B;&#xF41;&#xF44;&#xF0B;</trd>
  <trd lng="ca">taverna</trd>
  <trdgrp lng="cs">
    <trd>hostinec</trd>,
    <trd>kr&ccaron;ma</trd>,
    <trd>lok&aacute;l</trd>,
    <trd>taverna</trd>,
    <trd>vin&aacute;rna</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Taverne</trd>,
    <trd>Kneipe</trd>,
    <trd>Weinstube</trd>
  </trdgrp>
  <trd lng="es">taberna</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&f_y;&f_k;&a_d;&a_h;</trd>,
    <trd>&a_m;&f_y;&a_kh;&a_A;&a_n;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gargotte</trd>,
    <trd>taverne</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kocsma</trd>,
    <trd>vend&eacute;gl&odblac;</trd>,
    <trd>kiskocsma</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>tawerna</trd>,
    <trd>karczma</trd>,
    <trd>ober&zdot;a</trd>,
    <trd>szynk</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>taberna</trd>
  </trdgrp>
  <trd lng="ro">han</trd>
  <trd lng="ru">&c_t;&c_a;&c_v;&c_je;&c_r;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>kr&ccaron;ma</trd>,
    <trd>vin&aacute;re&ncaron;</trd>
  </trdgrp>
</drv>
