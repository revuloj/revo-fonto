<art mrk="$Id: stret.xml,v 1.7 2025/04/06 06:47:41 revo Exp $">
<kap>
    <rad>stret</rad>/a <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="stret.0a">
  <kap><tld/>a</kap>
  <snc mrk="stret.0a.mallargxa">
    <uzo tip="stl">NEO</uzo>
    <dif>
      <ref tip="sin" cel="largx.mal0a">Mallar&gcirc;a</ref>:
      <ekz>
        por la trafiko estas du koridoroj kaj <tld/>a pado por
        biciklantoj
        <fnt><bib>Viki</bib>, <lok>Zelandoponto</lok></fnt>;
      </ekz>
       <ekz>
        la pado de la virto <tld/>as
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 6a, p.
        427a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>mallar&#285;a</ke>.
  </rim>

  <trd lng="be">&c_v;&c_u;&c_z;&c_k;&c_ib;</trd>
  <trd lng="cs">&#250;zk&#253;</trd>
  <trd lng="pl">w&aogonek;ski</trd>
  <trd lng="sk">&#250;zky</trd>
  <trd lng="uk">&c_v;&c_u;&c_z;&#1100;&#1082;&c_i;&c_j;</trd>
</drv>
</art>

