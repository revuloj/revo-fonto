<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: muze.xml,v 1.31 2023/09/02 06:04:15 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>muze</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="muze.0o">
  <kap><tld/>o</kap>
  <snc mrk="muze.0o.ejo">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Ejo por konstanta elmontrado de objektoj kaj dokumentoj pri sciencoj,
      arto a&ubreve; aliaj temoj:
      <ekz>
        mi utiligis mian libertempon, vizitante <klr>[&#x2026;]</klr> la <tld/>ojn kaj la
        diversajn monumentojn de Parizo
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pizon oni metis en la <tld/>on, kie oni ankora&ubreve; nun povas &gcirc;in vidi
        <fnt>
          <bib>Fab1</bib>
          <lok>Re&gcirc;idino sur pizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        arkeologia kaj antropologia <tld/>o de la universitato de
        Pensilvanio
        <fnt>
          <aut>OH</aut>,
          <vrk>Molekula arkeologio por frandemuloj</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2001a, numero 3a, p. 22a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> iras en <tld/>on, speciale malfermitan por personoj, kiuj kiel
        mi, amas &leftquot;la artojn&rightquot;
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kuratoro &ccirc;e la Zoologia <tld lit="M"/>o en Oslo
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekspoz1.0ejo">ekspoziciejo</ref>,
      <ref cel="galeri.0o.ekspoziciejo">galerio<sncref/></ref>,
      <ref cel="kabine.0o.ekspozicio">kabineto<sncref/></ref>,
      <ref cel="panoptik.0o">panoptiko</ref>
    </refgrp>
  </snc>
  <snc mrk="muze.0o.kolekto">
    <dif>
      Tia kolekto mem:
      <ekz>
        <nom>Paszkiewicz</nom> aran&gcirc;is <tld/>on de litvaj antikva&jcirc;oj
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IV.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la Internacia Esperanto-<tld lit="M"/>o en <nom>Wien</nom>
        <fnt>
          <bib>EeP</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ekspoz1.0o">ekspozicio</ref>,
      <ref cel="galeri.0o.kolekto">galerio<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_z;&c_e;&c_j;</trd>
  <trd lng="bo">&t_-a;&t_ga;&t_rata;&t_drengbu;&t_ma;&t_tsheg;&t_sa;&t_tata;&t_naro;&t_na;&t_tsheg;&t_kha;&t_nga;&t_tsheg;</trd>
  <trd lng="ca">museu</trd>
  <trd lng="cs">muzeum</trd>
  <trd lng="de">Museum</trd>
  <trd lng="en">museum</trd>
  <trd lng="es">museo</trd>
  <trd lng="fa">&a_m;&a_w;&a_z;&a_h;</trd>
  <trd lng="fr">mus&eacute;e</trd>
  <trd lng="hu">m&uacute;zeum</trd>
  <trd lng="id">museum</trd>
  <trdgrp lng="ja">
    <trd>&#x535A;&#x7269;&#x9928;  [<ind>&#x306F;&#x304F;&#x3076;&#x3064;&#x304B;&#x3093;</ind>]</trd>,
    <trd>&#x7F8E;&#x8853;&#x9928;  [<ind>&#x3073;&#x3058;&#x3085;&#x3064;&#x304B;&#x3093;</ind>]</trd>,
    <trd>&#x53CE;&#x8535;&#x54C1;  [<ind>&#x3057;&#x3085;&#x3046;&#x305E;&#x3046;&#x3072;&#x3093;</ind>]</trd>,
    <trd>&#x53CE;&#x96C6;&#x54C1;  [<ind>&#x3057;&#x3085;&#x3046;&#x3057;&#x3085;&#x3046;&#x3072;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">museum</trd>
  <trd lng="pl">muzeum</trd>
  <trd lng="pt">museu</trd>
  <trd lng="ro">muzeu</trd>
  <trd lng="ru">&c_m;&c_u;&c_z;&c_je;&c_j;</trd>
  <trd lng="sk">m&uacute;zeum</trd>
  <trd lng="tr">m&uuml;ze</trd>
  <trd lng="uk">&c_m;&c_u;&c_z;&c_je;&c_j;</trd>
  <trd lng="vo">musof&ouml;p</trd>
  <trd lng="zh">&#x535A;&#x7269;&#x9986; [<ind>b&oacute;w&ugrave;gu&#x1CE;n</ind>]</trd>
</drv>

<drv mrk="muze.0scienco">
  <kap><tld/>scienco, <var><kap><tld/>oscienco</kap></var></kap>
  <snc mrk="muze.0scienco.MUZE">
    <dif>
      Scienco pri aran&gcirc;o de muzeoj, pri konservo kaj konigo de iliaj
      hava&jcirc;oj:
      <ekz>
        &scirc;i studis historion kaj <tld/>osciencon en la Silezia
        Universitato
        <fnt>
        <aut>V. Ko&ccaron;vara</aut>,
        <vrk>Nia Esperanto-muzeo...</vrk>,
        <lok><url ref="http://members.chello.cz/malovec/start405.htm">
        Starto 2006/4 (217)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_z;&c_e;&c_ja;&c_z;&c_n;&c_a;&c_w;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_m;&c_u;&c_z;&c_e;&c_ja;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="de">Museologe</trd>
  <trd lng="fa">&a_m;&a_w;&a_z;&a_h;&#x200C;&a_sh;&a_n;&a_A;&a_s;&f_y;</trd>
  <trd lng="fr">mus&eacute;ologie</trd>
  <trd lng="id">museologi</trd>
  <trd lng="it">museologia</trd>
  <trdgrp lng="nl">
    <trd>museumkunde</trd>,
    <trd>museologie</trd>
  </trdgrp>
  <trd lng="pl">muzeologia</trd>
</drv>

<drv mrk="muze.0sciencisto">
  <kap><tld/>sciencisto, <var><kap><tld/>osciencisto</kap></var></kap>
  <snc mrk="muze.0sciencisto.MUZE">
    <dif>
      Fakulo pri <tld/>scienco.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_z;&c_e;&c_ja;&c_z;&c_n;&c_a;&c_v;&c_je;&c_c;</trd>,
    <trd>&c_m;&c_u;&c_z;&c_e;&c_ja;&c_z;&c_n;&c_a;&c_w;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Museologie</trd>
  <trd lng="fa">&a_m;&a_w;&a_z;&a_h;&#x200C;&a_sh;&a_n;&a_A;&a_s;</trd>
  <trd lng="fr">mus&eacute;ologue</trd>
  <trd lng="id">museolog</trd>
  <trdgrp lng="nl">
    <trd>museumkundige</trd>,
    <trd>museoloog</trd>
  </trdgrp>
  <trd lng="pl">muzeolog</trd>
</drv>
</art>
