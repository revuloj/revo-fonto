<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sofrologi.xml,v 1.8 2025/12/28 22:33:16 revo Exp $">
<kap>
  <rad>sofrologi</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>
<drv mrk="sofrologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="sofrologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Neoficiala metodo <ref tip="malprt" cel="medici.0o.SCI">medicina</ref> el la 1960aj jaroj, kiu
      rekomendas memkonscion kaj malstre&ccirc;i&gcirc;on kiel rimedojn de korpa
      kaj anima sani&gcirc;o:
      <ekz>
        <ctl><tld lit="S"/>o kaj la kuracanta vorto</ctl>
        <klr>(verko de A. Albault, 1987)</klr>.
        <fnt>
          <aut>A. Albault</aut>,
          <vrk>Sofrologio kaj la kuracanta vorto</vrk>,
          <lok>Sarbruko: Iltis, 1987</lok>
        </fnt>;
      </ekz>
      <ekz>
        plej grava en <tld/>o estas la trejnado <klr>[&#x2026;]</klr> de la
        personeco
        <fnt>
          <aut>J.-M. Jacques</aut>,
          <vrk><url ref="http://home.scarlet.be/~tor-4517/_sgg/m1m4s4_1.htm">Sofrologio</url></vrk>,
          <lok>[vidita en 2006-08]</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">sophrology</trd>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_f;&c_r;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="cs">sofrologie</trd>
  <trd lng="de">Sophrologie</trd>
  <trd lng="fr">sophrologie</trd>
  <trd lng="pl">sofrologia</trd>
  <trd lng="sk">sofrol&oacute;gia</trd>
  <trd lng="uk">&c_s;&c_o;&c_f;&c_r;&c_o;&c_l;&c_o;&c_g;&c_ib;&c_ja;</trd>
</drv>

<drv mrk="sofrologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="sofrologi.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Fakulo pri <ref tip="vid" cel="sofrologi.0o.MED"><tld/>o</ref>.
    </dif>
    <ref tip="sin" cel="sofrolog.0o">sofrologo</ref>
  <trd lng="en">sophrologist</trd>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_f;&c_r;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="de">Sophrologe</trd>
  <trd lng="fr">sophrologue</trd>
  <trd lng="pl">sofrolog</trd>
</drv>
</art>

