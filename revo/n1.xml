<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>n</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<subart>
<drv mrk="n1.0.konsonanto">
  <kap><tld/></kap>
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">TIP</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      <ref tip="dif" cel="konson1.0o">Konsonanto</ref>, vo&ccirc;a <ref
      tip="dif" cel="nazal.0o">nazala</ref> <ref tip="dif"
      cel="dental.0o">dentalo</ref> [n].
    </dif>
  <trd lng="fr">n<klr> (consonne)</klr></trd>  
  </snc>
  <snc>
     <dif>
      La dekoka <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta alfabeto, uzata i.a. kiel:
     </dif>
  <trd lng="fr">n<klr> (lettre)</klr></trd>
  <subsnc>
  <uzo tip="fak">KEM</uzo>
  <dif>
  (maj.) simbolo de nitrogeno: <ekz><tld lit="N"/></ekz>;
  </dif>
  <trd lng="fr">azote</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (maj.) simbolo de ne&ubreve;tono: <ekz><tld lit="N"/></ekz>;
  </dif>
  <trd lng="fr">Newton</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">MAT</uzo>
  <dif>
  (maj. kaj grase) simbolo por la aro de &ccirc;iuj naturaj nombroj 1, 2, 3,
  ...:
  <ekz><em><tld lit="N"/></em></ekz>;
  </dif>
  <trd lng="fr">N<klr> (ensemble des nombres naturels)</klr></trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">GEOG</uzo>
  <dif>
  (maj.) mallongigo de nordo: <ekz><tld lit="N"/></ekz>;
  </dif>
  <trd lng="fr">nord</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">TIP</uzo>
  <dif>
  (min.) mallongigo de naskita: <ekz><tld/>.</ekz>;
  </dif>
  <trd lng="fr">n&eacute;(e)</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (min.) simbolo de nano: <ekz><tld/></ekz>;
  </dif>
  <trd lng="fr">nano</trd>
  </subsnc>
  
  </snc>
  <snc>
  <ref tip="vid" cel="n.0">-n (fina&jcirc;o)</ref>
  <trd lng="fr">-n<klr> (fina&jcirc;o)</klr></trd>
  </snc>
  </drv>
</subart>

<subart>
<drv mrk="n1.0.nomo">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>    
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif>
  <trd lng="fr">n</trd>
  </snc>
</drv>
</subart>
</art>

