<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>no</rad>
</kap>

<drv mrk="n1.0.konsonanto">
  <kap><tld/></kap>
  <uzo tip="fak">LIN</uzo>
  <uzo tip="fak">GRA</uzo>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      Nomo de la dekoka <ref cel="liter.0o.propra">litero</ref>
      de la Esperanta alfabeto
      <ekz>... ko, lo, mo, no, o, po, ro, ...
        <fnt><bib>F</bib>, <lok>&FE; 1</lok></fnt>,
      </ekz>
      uzata i.a. kiel:
    </dif>
    <subsnc>
      <uzo tip="fak">KEM</uzo>
      <dif>
        (maj.) simbolo de nitrogeno: <ekz>N</ekz>.
      </dif>
      <trd lng="fr">azote</trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">FIZ</uzo>
      <dif>
        (maj.) simbolo de ne&ubreve;tono: <ekz>N</ekz>.
      </dif>
      <trd lng="fr">Newton</trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">MAT</uzo>
      <dif>
        (maj. kaj grase) simbolo por la aro de &ccirc;iuj naturaj nombroj 1, 2, 3,
        ...:
      <ekz><em>N</em></ekz>.
      </dif>
      <trd lng="fr">N<klr> (ensemble des nombres naturels)</klr></trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">GEOG</uzo>
      <dif>
        (maj.) mallongigo de nordo: <ekz>N</ekz>.
      </dif>
      <trd lng="fr">nord</trd>
    </subsnc>
  </snc>
</drv>

</art>

