<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: horus.xml,v 1.6 2018/12/25 09:10:13 revo Exp $">
<kap>
  <rad>Horus</rad>/o
  <fnt>B</fnt>
</kap>
<drv mrk="horus.0o">
  <kap><tld/>o</kap>
  <snc mrk="horus.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Antikva egipta,
      sun<ref tip="super" cel="di.0o.politeisma">dio</ref>,
      filo de <ref tip="vid" cel="izis.0o.MIT">Iziso</ref>:
      <ekz>
        <tld/>o estas <klr>[&#8230;]</klr> reprezentita kun kapo de falko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=12420">Horuso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        trio de la senmortaj dioj: Oziriso, Izido kaj <tld/>o, savu Egipton de l' pereo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>!
      </ekz>
    </dif>
    <trd lng="de">Horus</trd>
    <trd lng="pl">Horus</trd>
    <trdgrp lng="it">
     <trd>Horo</trd>, 
     <trd>Horus</trd>
    </trdgrp>
  </snc>
  <trd lng="ca">Horus</trd>
  <trd lng="cs">Horus</trd>
  <trd lng="sk">Horus</trd>
</drv>
</art>

