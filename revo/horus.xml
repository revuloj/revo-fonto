<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: horus.xml,v 1.9 2023/08/31 15:46:19 revo Exp $">
<kap>
  <rad>Horus</rad>/o
  <fnt>B</fnt>
</kap>
<drv mrk="horus.0o">
  <kap><tld/>o</kap>
  <snc mrk="horus.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Antikva egipta,
      sun<ref tip="super" cel="di.0o.politeisma">dio</ref>,
      filo de <ref tip="vid" cel="izis.0o.MIT">Iziso</ref>:
      <ekz>
        <tld/>o estas <klr>[&#x2026;]</klr> reprezentita kun kapo de falko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=12420">Horuso</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        trio de la senmortaj dioj: Oziriso, Izido kaj <tld/>o, savu Egipton de l' pereo
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>!
      </ekz>
    </dif>
    <trd lng="de">Horus</trd>
    <trd lng="pl">Horus</trd>
    <trdgrp lng="it">
     <trd>Horo</trd>, 
     <trd>Horus</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_G;&c_o;&c_r; <klr>(&c_s;&c_t;&c_a;&c_r;&c_a;&c_zh;&c_y;&c_t;&c_n;&c_a;&c_e;&c_g;&c_ib;&c_p;&c_je;&c_c;&c_k;&c_ib; &c_b;&c_o;&c_g;)</klr></trd>
  <trd lng="ca">Horus</trd>
  <trd lng="cs">Horus</trd>
  <trd lng="ja">&#x30DB;&#x30EB;&#x30B9;</trd>
  <trd lng="sk">Horus</trd>
  <trdgrp lng="uk">
    <trd>&#x413;&#x43E;&#x440;</trd>,
    <trd>&#x425;&#x43E;&#x440;</trd>
  </trdgrp>
</drv>
</art>

