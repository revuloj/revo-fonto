<drv mrk="sxajn.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ekstera <ref tip="super" cel="aspekt.0o">aspekto</ref>, kiu povas
      erarigi:
      <ekz>
        ne ju&gcirc;u la&ubreve; <tld/>o, sed ju&gcirc;u justan ju&gcirc;on
        <fnt>
          <bib>NT</bib>
          <lok>Johano 7:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        el sub la <tld/>o de &gcirc;entileco &ccirc;iumomente elglitis ilia fiero kaj mal&scirc;ato
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgra&ubreve; la malserioza <tld/>o, li havis noblan kaj energian karakteron
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[lia]</klr> viza&gcirc;o, dank&#8217; al ekpeno de lia volado, estis repreninta sian
        kutiman <tld/>on
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o tre perfekte hipokrita
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu &ccirc;ambro havis <tld/>on de saloneto, sed en efektiveco 
        &gcirc;i estis komercejo
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
      <ekz>
        <klr>[la]</klr> ludejo <klr>[&#8230;]</klr> malgra&ubreve; luksaj <tld/>oj estis nur vera
        &scirc;telejo
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o trompas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aper.0o.prezento">apero<sncref/></ref>,
      <ref cel="aspekt.0o">aspekto</ref>,
      <ref cel="ekster.0o0ajxo">ekstero</ref>,
      <ref cel="fizion.0o">fizionomio</ref>,
      <ref cel="mask.0o">masko</ref>,
      <ref cel="vest.0o">vesto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_g;&c_l;&c_ja;&c_d; <klr>(&c_p;&c_a;&c_d;&c_m;&c_a;&c_n;&c_l;&c_ib;&c_v;&c_y;)</klr></trd>,
    <trd>&c_b;&c_a;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
