<kap>
   <ofc>7</ofc> <rad>mitolog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="mitolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="mitolog.0o.SNC">
    <uzo tip="fak">MIT</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Fakulo pri
      <ref tip="vid" cel="mit.0o.MIT">mitoj<sncref/></ref>,
      pri <ref tip="vid" cel="mitolo.0o.SCI">mitologio</ref>;
      mitologiisto:
      <ekz>
        la zorgeme preciza <tld/>o Pindaro (522-443 a.K.) faras neniun mencion al Ze&ubreve;so kiel patro
        de Ilitio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=420745">Ilitio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la sveda poeto kaj frua <tld/>o <nom>Viktor Rydberg</nom> (1828&ndash;1895)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=255153">Burgundoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Mythologe</trd>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="fr">
    <trd>mythologiste</trd>
    <trd>mythologue</trd>
  </trdgrp>
  <trd lng="ja">&#x795E;&#x8A71;&#x5B66;&#x8005;  <pr>&#x3057;&#x3093;&#x308F;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  <trd lng="pl">mitolog</trd>
  <trd lng="ro">mitolog</trd>
  <trd lng="uk">&#x43C;&c_ib;&c_f;&#x43E;&#x43B;&#x43E;&c_g;</trd>
</drv>
</art>

