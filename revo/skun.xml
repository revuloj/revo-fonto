<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: skun.xml,v 1.21 2018/12/01 20:10:19 revo Exp $">
<kap>
  <rad>skun</rad>/o
  <fnt>Z</fnt>
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skun.0o">
  <kap><tld/>o</kap>
  <snc mrk="skun.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="sxip.vel0o">Vel&scirc;ipo</ref>
      kun du a&ubreve; pli da mastoj (la unua masto ne pli alta ol la
      ceteraj) kaj <ref tip="vid" cel="vel.oblikva0o">oblikvaj veloj</ref>:
      <ekz>
        <tld/>o a&ubreve; goeleto estas <klr>[&#8230;]</klr> vel&scirc;ipo kun origine du, sed pli poste anka&ubreve; pli da
        mastoj, la kareno estas longa kaj mallar&gcirc;a (4:1-proporcio) kun profunde
        elstara kilo kaj la varangoj havas, trans la tuta longo, S-formon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=596729"
              >Skuno</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="goelet.0o">goeleto</ref>.
    </refgrp>
    <bld
      lok="https://upload.wikimedia.org/wikipedia/commons/6/69/Schooner_Mary_Day_by_Shannon_Gallagher.jpg"
      prm="CC BY-SA 3.0"
      >la <tld/>o Mary Day kun plene malfalditaj veloj &ccirc;e Maine
        <fnt>
          <aut>Colbypup2010, propra verko</aut>
          <lok><url ref="&WCW;/File:Schooner_Mary_Day_by_Shannon_Gallagher.jpg"
              >https://commons.wikimedia.org/wiki/File:Schooner_Mary_Day_by_Shannon_Gallagher.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_sh;&c_h;&c_u;&c_n;&c_a;</trd>
  <trd lng="bg">&c_sh;&c_h;&c_u;&c_n;&c_a;</trd>
  <trd lng="ca">goleta</trd>
  <trd lng="cs">&scaron;kuner</trd>
  <trd lng="de">Schoner</trd>
  <trd lng="en">schooner</trd>
  <trdgrp lng="fr">
    <trd>go&eacute;lette</trd>,
    <trd>schooner</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szk&uacute;ner</trd>,
    <trd>k&eacute;t&aacute;rbocos <ind>haj&oacute;</ind></trd>
  </trdgrp>
  <trd lng="nl">schoener</trd>
  <trd lng="pl">szkuner</trd>
  <trd lng="ru">&c_sh;&c_h;&c_u;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>&lcaron;ahk&yacute; dvojs&tcaron;a&zcaron;n&iacute;k</trd>,
    <trd>&scaron;kuner</trd>
  </trdgrp>
</drv>
</art>
