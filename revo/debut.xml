<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: debut.xml,v 1.21 2022/08/03 22:33:04 revo Exp $">
<kap>
  <rad>debut</rad>/i <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="debut.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="debut.0i.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Sin prezenti unuafoje anta&ubreve; la publiko:
      <ekz>
        tiu aktoro <tld/>is kun granda sukceso.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>deb&uuml;tieren</trd>,
      <trd>zum ersten Mal <ind>auftreten</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="debut.0i.FIG">
    <dif>
      Unuafoje elpa&scirc;i, agi; komenci:
      <ekz>
        li <tld/>is en tute malalta posteno;
      </ekz>
      <ekz>
        nia <ind><tld/>o</ind>, la unua lernolibro de nia lingvo,
        eliris en lingvo rusa<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_e;&c_b;&c_ju;&c_t;</trd>
    <trdgrp lng="de">
      <trd>deb&uuml;tieren</trd>,
      <trd>zum ersten Mal in <ind>Erscheinung</ind> treten</trd>
    </trdgrp>
    <trd lng="pl">debiutowa&cacute;</trd>
    <trd lng="ro">debuta</trd>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_b;&c_ju;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>debutovat</trd>,
    <trd>poprv&#xE9; ve&rcaron;ejn&#x11B; vystupovat</trd>
  </trdgrp>
  <trd lng="en">debut</trd>
  <trd lng="es">debutar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;buter<klr tip="ind"> (faire ses
d&eacute;buts)</klr></trd>,
    <trd>faire ses d&eacute;buts</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5D4;&#x5D5;&#x5E4;&#x5D9;&#x5E2;
  &#x5D1;&#x5D4;&#x5D5;&#x5E4;&#x5E2;&#x5EA; &#x5D1;&#x5DB;&#x5D5;&#x5E8;&#x5D4;</trd>
  <trdgrp lng="hu">
    <trd>deb&uuml;t&aacute;l</trd>,
    <trd>bemutatkozik<klr tip="ind"> (deb&uuml;t&aacute;l)</klr></trd>
  </trdgrp>
  <trd lng="pl">debiutowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>debutar</trd>,
    <trd>estrear</trd>,
    <trd>fazer a <ind>estr&eacute;ia</ind></trd>
  </trdgrp>
  <trd lng="ro">debuta</trd>
  <trd lng="ru">&c_d;&c_je;&c_b;&c_ju;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>debutova&tcaron;</trd>,
    <trd>prv&#xFD; raz vyst&#xFA;pi&tcaron; pred obecenstvom</trd>
  </trdgrp>
  <trd lng="sv">debutera</trd>
  <trd lng="uk">&c_d;&c_je;&c_b;&#x44E;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>
</art>
