  <trd lng="be">&c_d;&c_a;&c_w;&c_g;&c_a;&c_v;&c_a;&c_ja; <ind>&c_r;&c_o;&c_s;&c_mol;&c_p;&c_ib;&c_s;&c_k;&c_a;</ind></trd>
