<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>atest</rad>/i
</kap>
<drv mrk="atest.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="atest.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Certigi fakton, kiel vidinto, a&ubreve;dinto a&ubreve; eksciinto:
      <ekz>
        <tld/>i la senkulpecon de la akuzito;
      </ekz>
      <ekz>
        mi vokas Dion kiel <tld/>anton
        <fnt><bib>NT</bib>, <lok>&Ko2;, 1, 23</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="atest.0i.KOMUNE">
    <dif>
      Pruvi, montri:
      <ekz>
        via hezito <tld/>as, ke vi mensogas;
      </ekz>
      <ekz>
        per tia ago vi <tld/>is vian perfidon;
      </ekz>
      <ekz>
        griza barbo sa&gcirc;on ne <tld/>as<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">sv&ecaron;d&ccaron;it</trd>
  <trd lng="de">bezeugen</trd>
  <trdgrp lng="en">
    <trd>attest</trd>,
    <trd>testify</trd>,
    <trd>affirm</trd>,
    <trd>bear <ind>witness</ind></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>atestiguar</trd>,
    <trd>testificar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>attester</trd>,
    <trd>certifier</trd>,
    <trd>t&eacute;moigner</trd>
  </trdgrp>
  <trd lng="he">&lamed;<ind>&he;&ayin;&yod;&dalet;</ind></trd>
  <trdgrp lng="hu">
    <trd>tan&uacute;s&iacute;t</trd>,
    <trd>igazol</trd>
  </trdgrp>
  <trdgrp lng="ru">
   
<trd>&c_s;&c_v;&c_i;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">sved&ccaron;i&tcaron;</trd>
</drv>

<drv mrk="atest.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago, vortoj, dokumento, per kio oni <tld/>as ion:
      <ekz>
        alvoki la <tld/>on de &ccirc;iuj, kiuj partoprenis<fnt>Z</fnt>;
      </ekz>
      <ekz>
        fari &ccirc;e la ju&gcirc;ejo sensacian <tld/>on;
      </ekz>
      <ekz>
        <tld/>o pri kapableco, pri bona konduto;
      </ekz>
      <ekz>
        ricev<tld/>o;
      </ekz>
      <ekz>
        unua <tld/>o estas la vesto<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la statistikoj liveras pri tio elokventan <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd
lng="be">&c_p;&c_a;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="de">
    <trd>Zeugnis</trd>, <trd>Urkunde</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>attestation</trd>,
    <trd>testimony</trd>
  </trdgrp>
  <trd lng="es">testimonio</trd>
  <trdgrp lng="fr">
    <trd>attestation</trd>,
    <trd>certificat</trd>,
    <trd>t&eacute;moignage</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&ayin;&dalet;&vav;&tav;</trd>,
    <trd>&he;&ayin;&dalet;&he;</trd>,
    <trd>&he;&tsadi;&he;&resh;&he;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>tan&uacute;s&iacute;tv&aacute;ny</trd>,
    <trd>igazol&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
   
<trd>&c_s;&c_v;&c_i;&c_d;&c_je;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_a;&c_t;&c_t;&c_je;&c_s;&c_t;&c_a;&c_t;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>svedectvo</trd>,
    <trd>osved&ccaron;enie</trd>
  </trdgrp>
</drv>

<drv mrk="atest.sxargx0o">
  <kap>&scirc;ar&gcirc;<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="konosament.0o">konosamento</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Frachtbrief</trd>,
    <trd>Konnossement</trd>,
    <trd>Ladeschein</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bill of <ind>lading</ind></trd>,
    <trd>consignment <ind>note</ind></trd>,
    <trd>waybill</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_u;&c_t;&c_je;&c_v;&c_o;&c_j; &c_l;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_n;&c_a;&c_k;&c_l;&c_a;&c_d;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_z;&c_o;&c_v;&c_o;&c_j;
&c_d;&c_o;&c_k;&c_u;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_o;&c_s;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
</drv>

</art>
