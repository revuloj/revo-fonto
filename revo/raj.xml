<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>raj</rad>/o
</kap>
<drv mrk="raj.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="raj.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Raja</trd>), bongusta plata romboforma marfi&scirc;o
      kun mallar&gcirc;a vosto.
    </dif>
  </snc>
  <trd lng="de">Rochen</trd>
  <trd lng="en">ray</trd>
  <trd lng="es">raya</trd>
  <trd lng="fr">raie</trd>
  <trd lng="it">razza</trd>
  <trdgrp lng="nl">
    <trd>rog</trd>,
    <trd>vleet</trd>
  </trdgrp>
  <trd lng="pl">raia</trd>
</drv>

</art>

