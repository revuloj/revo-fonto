<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: raj.xml,v 1.24 2024/04/13 20:15:51 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>raj</rad>/o
</kap>
<drv mrk="raj.0o">
  <kap><tld/>o</kap>
  <snc mrk="raj.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="raj.0oformaj"><tld/>oformaj</ref>
      fi&scirc;oj kaj parencaj ordoj; plata romboforma kartilaga <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>,
      kun bu&scirc;o kaj nazotruoj sur la malsupra, plej ofte hela flanko 
      kaj kun mallar&gcirc;a vosto:
      <ekz>
        en 1962 &ccirc;ina anatomo pri fi&scirc;o konstatis, ke dol&ccirc;akva kaj mara ru&gcirc;brunaj <tld/>oj
        apartenas al la sama specio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49880">Ru&gcirc;bruna rajo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="raj.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="rajed.0oj"><tld/>edoj</ref>
      (<trd lng="la">Raja</trd>).
    </dif>
    <bld lok="&WCU;/thumb/3/36/Raja_clavata_(juv).jpg/640px-Raja_clavata_(juv).jpg" prm="CC BY-SA     4.0">Dorna <tld/>o (Raja clavata)
        <fnt>
          <aut>&#xA9; Hans Hillewaert, propra verko</aut>
          <lok><url ref="&WCW;/File:Raja_clavata_(juv).jpg">&WCW;/File:Raja_clavata_(juv).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_t;</trd>
  <trd lng="cs">rejnok</trd>
  <trd lng="de">Rochen</trd>
  <trd lng="en">ray</trd>
  <trd lng="es">raya</trd>
  <trd lng="fr">raie</trd>
  <trd lng="hu">r&aacute;ja</trd>
  <trd lng="id">pari</trd>
  <trd lng="it">razza</trd>
  <trdgrp lng="ja">
    <trd>&#x30AC;&#x30F3;&#x30AE;&#x30A8;&#x30A4;</trd>,
    <trd>&#x30A8;&#x30A4;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>rog</trd>,
    <trd>vleet</trd>
  </trdgrp>
  <trd lng="pl">raja</trd>
  <trdgrp lng="pt">
    <trd>raia</trd>,
    <trd>arraia</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_t;</trd>
  <trd lng="sk">raja</trd>
  <trd lng="uk">&c_s;&#x43A;&c_a;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x9C29; <pr>y&#xE1;o</pr></trd>,
    <trd>&#x9CD0; <pr>y&#xE1;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="raj.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="raj.0oformaj.ordo">
    <uzo tip="fak">ZOO</uzo>
    <dif>
       <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> 
       de <ref tip="super" cel="fisx.kartilago0oj">kartilagofi&scirc;oj</ref>
       (<trd lng="la">Rajiformes</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_m;&c_b;&c_a;&c_s;&c_k;&c_a;&c_t;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="uk">&c_s;&#x43A;&c_a;&c_t;&#x43E;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_ib;</trd>
</drv>

</art>
