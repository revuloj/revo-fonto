<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: buk.xml,v 1.32 2025/11/22 08:35:44 revo Exp $">
<kap><ofc>*</ofc>
<rad>buk</rad>/o <fnt><bib>UV</bib></fnt></kap>
<drv mrk="buk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Diversforma kadreto, ordinare metala, kun movebla pinglo por fiksi
      <ref tip="malprt" cel="zon.0o.TEKS">zonon</ref>, rubandon kc:
      <ekz>
        <klr>[ili]</klr> alportis <tld/>ojn kaj orelringojn
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 35:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno <klr>[&#x2026;]</klr> origis la brilpurajn <tld/>ojn de la Biblio
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;uoj kun bantoj <klr>[&#x2026;]</klr> kaj &scirc;talaj <tld/>oj
        <fnt>
          <bib>MkM</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> prenis el la komodo ledan zonon kun &scirc;tala <tld/>o
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        brokant<klr>[i]</klr> per malnovaj pasamentoj kaj <tld/>oj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agraf.0o">agrafo</ref>,
      <ref cel="brocx.0o">bro&ccirc;o</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_a;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_sh;&c_p;&c_ib;&c_l;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_cha;&t_ba;&t_tsheg;&t_tsa;&t_drengbu;&t_tsheg;</trd>
  <trd lng="fr">boucle<klr tip="ind"> (attache)</klr></trd>
  <trd lng="de">Schnalle</trd>
  <trd lng="en">buckle</trd>
  <trd lng="es">hebilla</trd>
  <trd lng="he">&alef;&bet;&zayin;&fmem;</trd>
  <trd lng="hu">csat</trd>
  <trdgrp lng="ja">
    <trd>&#x30D0;&#x30C3;&#x30AF;&#x30EB;</trd>,
    <trd>&#x6B62;&#x3081;&#x91D1;  <pr>&#x3068;&#x3081;&#x304C;&#x306D;</pr></trd>
  </trdgrp>
  <trd lng="nl">gesp</trd>
  <trdgrp lng="pl">
    <trd>sprz&aogonek;czka</trd>,
    <trd>klamra</trd>,
    <trd>zapi&eogonek;cie</trd>
  </trdgrp>
  <trd lng="pt">fivela</trd>
  <trd lng="ro">&icirc;ncheietoare</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_ja;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_jo;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">pracka</trd>
  <trd lng="cs">p&rcaron;ezka <klr>(opasku, obuvi)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_ja;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_ib;&c_b;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="buk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="fiks.0i.flagon">Fiksi</ref> per <tld/>o:
      <ekz>
        <tld/>i rimenon, sakon;
      </ekz>
      <ekz>
        brosu la &scirc;uojn kaj <tld/>u
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        li helpis &scirc;in <tld/>i la glitumilojn
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        specon de granda litotuko <klr>[&#x2026;]</klr> li <tld/>is sub la kolo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_sh;&c_p;&c_ib;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&t_cha;&t_ba;&t_tsheg;&t_tsa;&t_drengbu;&t_tsheg;&t_ra;&t_gata;&t_yata;&t_ga;&t_tsheg;</trd>
  <trd lng="cs">zapnout na p&rcaron;ezku</trd>
  <trdgrp lng="de">
    <trd>schnallen</trd>,
    <trd>anschnallen</trd>
  </trdgrp>
  <trd lng="en">buckle</trd>
  <trd lng="es">hebillar</trd>
  <trd lng="fr">boucler</trd>
  <trd lng="he">&lamed;&pe;&resh;&vav;&fpe;</trd>
  <trd lng="hu">&ouml;sszecsatol</trd>
  <trd lng="ja">&#x30D0;&#x30C3;&#x30AF;&#x30EB;&#x3067;&#x3068;&#x3081;&#x308B;</trd>
  <trd lng="nl">gespen</trd>
  <trdgrp lng="pl">
    <trd>zapina&cacute;<klr> (na sprz&aogonek;czk&eogonek;)</klr></trd>,
    <trd>spina&cacute;<klr> (na sprz&aogonek;czk&eogonek;)</klr></trd>,
    <trd>dopina&cacute;<klr> (na sprz&aogonek;czk&eogonek;)</klr></trd>
  </trdgrp>
  <trd lng="ro">&icirc;nchide</trd>
  <trd lng="ru">&c_z;&c_a;&c_s;&c_t;&c_jo;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_r;&c_ja;&c_zh;&c_k;&c_u;)</klr></trd>
  <trd lng="sk">zapn&uacute;&tcaron; pracku</trd>
  <trd lng="uk">&c_z;&c_a;&c_s;&c_t;&c_ib;&c_b;&c_a;&c_t;&c_i; (&c_a;&c_b;&c_o;&nbsp;&c_z;&c_a;&c_shch;&c_ib;&c_b;&c_a;&c_t;&c_i;) &c_p;&c_r;&c_ja;&c_zh;&c_k;&c_u;</trd>
</drv>

