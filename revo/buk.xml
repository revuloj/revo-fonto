<drv mrk="buk.kun0i">
  <kap>kun<tld/>i</kap>
  <snc>
    <dif>
      <ref tip="super" cel="kun.0igi">Kunigi</ref> per <tld/>o:
      <ekz>
        la Sor&ccirc;isto kun<tld/>is kelkajn rimenojn kaj enpendigis unu finon en la truon
        <fnt>
          <aut>Lyman Frank Baum, trad. Donald Broadribb</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0082/0022.php"
              >La Perdita Princino de Oz</url></vrk>
          <lok>&ccirc;apitro 20: Pli da Surprizoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la monon enspezintan dum la hela tago, ili portis en alia ledorimena zono, kiun
        ili kun<tld/>is sur la nuda korpo
        <fnt>
          <aut>B. Traven, trad. Dorothea kaj Hans-Georg Kaiser</aut>
          <vrk><url ref="https://files.libcom.org/files/btraven_marsxo.pdf"
              >Mar&scirc;o en la Regnon de l' Mahagono</url></vrk>
          <lok>p. 57</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">zapnout na p&rcaron;ezku</trd>
    <trd lng="en">buckle<klr> (together)</klr></trd>
    <trd lng="fr">boucler</trd>
    <trdgrp lng="pl">
      <trd>spina&cacute; sprz&aogonek;czk&aogonek;</trd>,
      <trd>zapina&cacute; na sprz&aogonek;czk&eogonek;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_sh;&c_p;&c_ib;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol; </trd>,
    <trd>&c_s;&c_a;&c_sh;&c_p;&c_ib;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>mit einer <ind>Schnalle</ind> verbinden</trd>,
    <trd>zusammenschnallen</trd>
  </trdgrp>
</drv>

<drv mrk="buk.mal0i">
  <kap>
    mal<tld/>i, <var><kap>dis<tld/>i</kap></var>
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="ferm.mal0i.eniro">Malfermi</ref> <tld/>on; disigi ion ligita per <tld/>o:
      <ekz>
        <klr>[li]</klr> mal<tld/>is la sekurzonon kaj etendis siajn longajn krurojn
        <fnt>
          <bib>CKv</bib>
          <lok>26</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni farigos al ni konsciencon la&ubreve; la plej nova fasono, por ke ni povu &gcirc;in bone
        dis<tld/>i pli vaste, kiam ni pli enpakas
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i falis sur mian bruston, tuta malseka, kun malseka viza&gcirc;o kaj
        mal<tld/>li&gcirc;intaj haroj, tremanta
        <fnt>
          <bib>MkM</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> mal<tld/>is kaj de&jcirc;etis la mantelon, forpreninte de la tuniko la
        zonon
        <fnt>
          <bib>MkM</bib>
          <lok>26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>aufschnallen</trd>,
    <trd>die Schnalle <ind>&ouml;ffenen</ind></trd>
  </trdgrp>
</drv>

<drv mrk="buk.0dorno">
  <kap><tld/>dorno, <var><kap><tld/>pinglo</kap></var>
  </kap>
  <snc>
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Pinta movebla pinglo, adaptita al <tld/>o, kaj kiu servas por
      fermi &gcirc;in, trapikante rimenon a&ubreve; rubandon.
    </dif>
    <refgrp tip="vid">
      <ref cel="agraf.0o">agrafo</ref>,
      <ref cel="brocx.0o">bro&ccirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_p;&c_je;&c_n;&c_mol; <klr>(&c_s;&c_p;&c_r;&c_a;&c_zh;&c_k;&c_ib;)</klr></trd>
  <trdgrp lng="cs">
    <trd>jaz&yacute;&ccaron;ek p&rcaron;ezky</trd>,
    <trd>trn p&rcaron;ezky</trd>
  </trdgrp>
  <trd lng="de">Dorn (einer Schnalle)</trd>
  <trd lng="en">buckle <ind>pin</ind></trd>
  <trd lng="fr">ardillon</trd>
  <trd lng="hu">t&udblac;<klr tip="amb"> (csaton)</klr></trd>
  <trd lng="ja">&#x30D0;&#x30C3;&#x30AF;&#x30EB;&#x306E;&#x91DD;
  <pr>&#x30D0;&#x30C3;&#x30AF;&#x30EB;&#x306E;&#x306F;&#x308A;</pr></trd>
  <trd lng="nl">tong<klr tip="ind"> (v.gesp)</klr></trd>
  <trdgrp lng="pl">
    <trd>trzpie&nacute;<klr> u sprz&aogonek;czki</klr></trd>,
    <trd>sztyfcik<klr> u sprz&aogonek;czki</klr></trd>
  </trdgrp>
  <trd lng="ru">
  &c_sh;&c_p;&c_je;&c_n;&c_jo;&c_k;<klr tip="amb">
(&c_p;&c_r;&c_ja;&c_zh;&c_k;&c_i;)</klr></trd>
  <trd lng="sk">jaz&yacute;&ccaron;ek pracky</trd>
  <trd lng="uk">&c_sh;&c_p;&c_je;&c_n;&c_i;&c_k;</trd>
</drv>

