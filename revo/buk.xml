<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: buk.xml,v 1.30 2025/03/24 20:29:13 revo Exp $">
<kap><ofc>*</ofc>
<rad>buk</rad>/o <fnt><bib>UV</bib></fnt></kap>
<drv mrk="buk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Diversforma kadreto, ordinare metala, kun movebla pinglo por fiksi
      zonon, rubandon kc.
    </dif>
  </snc>
    <refgrp tip="vid">
      <ref cel="agraf.0o">agrafo</ref>,
      <ref cel="brocx.0o">bro&ccirc;o</ref>.
    </refgrp>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_a;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_sh;&c_p;&c_ib;&c_l;&c_mol;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF46;&#xF56;&#xF0B;&#xF59;&#xF7A;&#xF0B;</trd>
  <trd lng="fr">boucle<klr tip="ind"> (attache)</klr></trd>
  <trd lng="de">Schnalle</trd>
  <trd lng="en">buckle</trd>
  <trd lng="es">hebilla</trd>
  <trd lng="he">&alef;&bet;&zayin;&fmem;</trd>
  <trd lng="hu">csat</trd>
  <trdgrp lng="ja">
    <trd>&#x30D0;&#x30C3;&#x30AF;&#x30EB;</trd>,
    <trd>&#x6B62;&#12417;&#x91D1;  <pr>&#12392;&#12417;&#x304C;&#x306D;</pr></trd>
  </trdgrp>
  <trd lng="nl">gesp</trd>
  <trdgrp lng="pl">
    <trd>sprz&aogonek;czka</trd>,
    <trd>klamra</trd>,
    <trd>zapi&eogonek;cie</trd>
  </trdgrp>
  <trd lng="pt">fivela</trd>
  <trd lng="ro">&icirc;ncheietoare</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_ja;&c_zh;&c_k;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_jo;&c_zh;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">pracka</trd>
  <trd lng="cs">p&rcaron;ezka <klr>(opasku, obuvi)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x44F;&c_zh;&#x43A;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_ib;&c_b;&#x43A;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="buk.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fiksi per <tld/>o:
      <ekz>
        <tld/>i rimenon, sakon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_sh;&c_p;&c_ib;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&#xF46;&#xF56;&#xF0B;&#xF59;&#xF7A;&#xF0B;&#xF62;&#xF92;&#xFB1;&#xF42;&#xF0B;</trd>
  <trd lng="cs">zapnout na p&rcaron;ezku</trd>
  <trd lng="en">buckle</trd>
  <trd lng="es">hebillar</trd>
  <trd lng="fr">boucler</trd>
  <trd lng="he">&lamed;&pe;&resh;&vav;&fpe;</trd>
  <trd lng="hu">&ouml;sszecsatol</trd>
  <trd lng="ja">&#x30D0;&#x30C3;&#x30AF;&#x30EB;&#12391;&#12392;&#12417;&#x308B;</trd>
  <trd lng="nl">gespen</trd>
  <trdgrp lng="pl">
    <trd>zapina&cacute;<klr> (na sprz&aogonek;czk&eogonek;)</klr></trd>,
    <trd>spina&cacute;<klr> (na sprz&aogonek;czk&eogonek;)</klr></trd>,
    <trd>dopina&cacute;<klr> (na sprz&aogonek;czk&eogonek;)</klr></trd>
  </trdgrp>
  <trd lng="ro">&icirc;nchide</trd>
  <trd lng="ru">&c_z;&c_a;&c_s;&c_t;&c_jo;&c_g;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_p;&c_r;&c_ja;&c_zh;&c_k;&c_u;)</klr></trd>
  <trd lng="sk">zapn&uacute;&tcaron; pracku</trd>
  <trd lng="uk">&c_z;&c_a;&c_s;&c_t;&c_ib;&c_b;&c_a;&c_t;&c_i; (&c_a;&c_b;&#x43E;&#xA0;&c_z;&c_a;&c_shch;&c_ib;&c_b;&c_a;&c_t;&c_i;) &#x43F;&c_r;&#x44F;&c_zh;&#x43A;&c_u;</trd>
</drv>

<drv mrk="buk.0fermi">
  <kap><tld/>fermi</kap>
  <snc>
  <ref tip="dif" cel="buk.0i">buki</ref>
  </snc>
  <trd lng="cs">zapnout na p&rcaron;ezku</trd>
  <trd lng="en">buckle<klr> (closed)</klr></trd>
  <trd lng="fr">boucler</trd>
  <trdgrp lng="pl">
    <trd>zapi&aogonek;&cacute;</trd>,
    <trd>zapina&cacute; na sprz&aogonek;czk&eogonek;</trd>
  </trdgrp>
  <trd lng="sk">zapn&uacute;&tcaron; pracku</trd>
</drv>

<drv mrk="buk.kun0i">
  <kap>kun<tld/>i</kap>
  <snc>
  <ref tip="dif" cel="buk.0i">buki</ref>
  <trd lng="cs">zapnout na p&rcaron;ezku</trd>
  <trd lng="en">buckle<klr> (together)</klr></trd>
  <trd lng="fr">boucler</trd>
  <trdgrp lng="pl">
    <trd>spina&cacute; sprz&aogonek;czk&aogonek;</trd>,
    <trd>zapina&cacute; na sprz&aogonek;czk&eogonek;</trd>
  </trdgrp>
  </snc>
</drv>

<drv mrk="buk.0dorno">
  <kap><tld/>dorno</kap>
  <snc>
  <uzo tip="fak">TEKS</uzo>
    <dif>
      Pinta movebla pinglo, adaptita al <tld/>o, kaj kiu servas por
      fermi &gcirc;in, trapikante rimenon a&ubreve; rubandon.
    </dif>
    <refgrp tip="vid">
      <ref cel="agraf.0o">agrafo</ref>,
      <ref cel="brocx.0o">bro&ccirc;o</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>jaz&yacute;&ccaron;ek p&rcaron;ezky</trd>,
    <trd>trn p&rcaron;ezky</trd>
  </trdgrp>
  <trd lng="de">Dorn (einer Schnalle)</trd>
  <trd lng="en">buckle <ind>pin</ind></trd>
  <trd lng="fr">ardillon</trd>
  <trd lng="hu">t&udblac;<klr tip="amb"> (csaton)</klr></trd>
  <trd lng="ja">&#x30D0;&#x30C3;&#x30AF;&#x30EB;&#x306E;&#x91DD; 
  <pr>&#x30D0;&#x30C3;&#x30AF;&#x30EB;&#x306E;&#x306F;&#x308A;</pr></trd>
  <trd lng="nl">tong<klr tip="ind"> (v.gesp)</klr></trd>
  <trdgrp lng="pl">
    <trd>trzpie&nacute;<klr> u sprz&aogonek;czki</klr></trd>,
    <trd>sztyfcik<klr> u sprz&aogonek;czki</klr></trd>
  </trdgrp>
  <trd lng="ru">
  &c_sh;&c_p;&c_je;&c_n;&c_jo;&c_k;<klr tip="amb">
(&c_p;&c_r;&c_ja;&c_zh;&c_k;&c_i;)</klr></trd>
  <trd lng="sk">jaz&yacute;&ccaron;ek pracky</trd>
  <trd lng="uk">&c_sh;&#x43F;&c_je;&#x43D;&c_i;&#x43A;</trd>
</drv>

<drv mrk="buk.0pinglo">
  <kap><tld/>pinglo</kap>
  <snc>
  <uzo tip="fak">TEKS</uzo>
      <ref tip="dif" cel="buk.0dorno">bukodorno</ref>
  <trd lng="fr">ardillon</trd>
  <trd lng="en">buckle pin</trd>
  <trd lng="pl">sztyfcik u sprz&aogonek;czki</trd>
  </snc>
 </drv>
</art>
