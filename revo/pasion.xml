<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Pasion</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="pasion.0o">
  <kap><tld/>o</kap>
  <snc mrk="pasion.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <ref tip="malprt" cel="evange.0o.BIB">Evangelia</ref>
      rakonto pri la lasta man&gcirc;o, aresto, kondamno kaj morto 
      de <ref cel="jesu.0o.homo">Jesuo</ref>.
    </dif>
    <trdgrp lng="fr">
      <trd>Passion<klr tip="ind"> (r&eacute;cit religieux)</klr></trd>
    </trdgrp>
  <trdgrp lng="hu">
    <trd>Krisztus szenved&eacute;se</trd>,
    <trd>szenved&eacute;st&ouml;rt&eacute;net</trd>
  </trdgrp>
  <trd lng="nl">Passie</trd>
  </snc>
  <snc mrk="pasion.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="kompon.0ajxo">Kompona&jcirc;o</ref>
      la&ubreve; teksto de evangelia <tld/>o kaj eventuale aliaj tekstoj,
      por kanta kaj muzikila prezento:
      <ekz>
        <ctl><tld/>o la&ubreve; Sankta Mateo</ctl>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Passion<klr tip="ind"> (Musik)</klr></trd>
    </trdgrp>
    <trd lng="es">Pasi&oacute;n</trd>
    <trdgrp lng="fr">
      <trd>Passion<klr tip="ind"> (pi&egrave;ce de musique)</klr></trd>
    </trdgrp>
    <trd lng="hu">passi&oacute;</trd> 
    <trd lng="nl">passie<klr tip="ind"> (muziekstuk)</klr></trd>   
  </snc>
  <trd lng="en">Passion<klr> of Christ</klr></trd>
  <trd lng="pt">Paix&atilde;o <klr>de Cristo</klr></trd>
</drv>
</art>
