<art mrk="$Id: aspirin.xml,v 1.23 2024/09/04 01:49:45 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>aspirin</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="aspirin.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="medika1.0o">Medikamento</ref>, surbaze de
      acetilsalicil-acido (<frm>CH<sub>3</sub>-CO-O-C<sub>6</sub>H<sub>4</sub>-COOH</frm>),
      uzata precipe kontra&ubreve; doloro, febro kaj inflamo:
      <!-- C<sub>6</sub>H<sub>4</sub>(OCOCH<sub>3</sub>).
      <ref tip="super" cel="medika1.0o">Medikamento</ref>, konsistanta
      el
      salicil-acetata acido kaj uzata kontra&ubreve; febro kaj doloro. -->
      <ekz>
        &scirc;i infuzis teon, donis al li kuracilon, <tld/>on, la solan kuracilon, kiu estis en la lo&gcirc;ejo
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2012/010807.php">La memmortigo de Tanja</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Karl Ludwig Reimer</nom>, kemiisto, esploristo
        <klr>[&#8230;]</klr> malkovris la sintezon de la <tld/>o elde la fenolo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2663">Leipzig</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_mol;&c_p;&c_ib;&c_r;&c_y;&c_n;</trd>
  <trd lng="bg">&c_a;&c_s;&c_p;&c_i;&c_r;&c_i;&c_n;</trd>
  <trd
  lng="bo">&#xF58;&#xF42;&#xF7C;&#xF0B;&#xF66;&#xFA8;&#xF53;&#xF0B;&#xF68;&#xF7B;&#xF0B;&#xF66;&#xF7A;&#xF0B;&#xF54;&#xF7A;&#xF0B;&#xF62;&#xF7A;&#xF53;&#xF0B;</trd>
  <trd lng="de">Aspirin</trd>
  <trd lng="en">aspirin</trd>
  <trd lng="es">aspirina</trd>
  <trd lng="fa">&a_A_madda;&a_s;&f_p;&f_y;&a_r;&f_y;&a_n;</trd>
  <trd lng="fr">aspirine</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E1;&#x5E4;&#x5BC;&#x5B4;&#x5D9;&#x5E8;&#x5B4;&#x5D9;&#x5DF;</trd>
  <trd lng="hu">aszpirin</trd>
  <trd lng="it">aspirina</trd>
  <trd lng="ja">&#x30A2;&#x30B9;&#x30D4;&#x30EA;&#x30F3;</trd>
  <trd lng="nl">aspirine</trd>
  <trd lng="pl">aspiryna</trd>
  <trd lng="ro">aspirin&abreve;</trd>
  <trd lng="ru">&c_a;&c_s;&c_p;&c_i;&c_r;&c_i;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>acylpyr&iacute;n</trd>,
    <trd>aspir&iacute;n</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&c_s;&#x43F;&c_ib;&c_r;&c_i;&#x43D;</trd>
</drv>
</art>
