<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>aspirin</rad>/o
</kap>
<drv mrk="aspirin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      C<sub>6</sub>H<sub>4</sub>(OCOCH<sub>3</sub>).
      <ref tip="super" cel="medika1.0o">Medikamento</ref>, konsistanta el
      salicil-acetata acido kaj uzata
      kontra&ubreve; febro kaj doloro.
    </dif>
  </snc>
  <trd lng="de">Aspirin</trd>
  <trd lng="en">aspirin</trd>
  <trd lng="fr">aspirine</trd>
  <trd lng="nl">aspirine</trd>
  <trd lng="ru">&c_a;&c_s;&c_p;&c_i;&c_r;&c_i;&c_n;</trd>
</drv>
</art>
