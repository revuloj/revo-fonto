<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc>
  <rad>aspirin</rad>/o
</kap>
<drv mrk="aspirin.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="medika1.0o">Medikamento</ref>, uzata precipe
      kontra&ubreve; doloro, febro kaj inflamo, konsistanta el
      substanco kun la &hcirc;emia formulo
      <frm>CH<sub>3</sub>-CO-O-C<sub>6</sub>H<sub>4</sub>-COOH</frm>.
      <!-- C<sub>6</sub>H<sub>4</sub>(OCOCH<sub>3</sub>).
      <ref tip="super" cel="medika1.0o">Medikamento</ref>, konsistanta
      el
      salicil-acetata acido kaj uzata kontra&ubreve; febro kaj doloro. -->
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_mol;&c_p;&c_ib;&c_r;&c_y;&c_n;</trd>
  <trd lng="bg">&c_a;&c_s;&c_p;&c_i;&c_r;&c_i;&c_n;</trd>
  <trd lng="de">Aspirin</trd>
  <trd lng="en">aspirin</trd>
  <trd lng="es">aspirina</trd>
  <trd lng="fa">&a_A_madda;&a_s;&f_p;&f_y;&a_r;&f_y;&a_n;</trd>
  <trd lng="fr">aspirine</trd>
  <trd lng="hu">aszpirin</trd>
  <trd lng="it">aspirina</trd>
  <trd lng="nl">aspirine</trd>
  <trd lng="ru">&c_a;&c_s;&c_p;&c_i;&c_r;&c_i;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>acylpyr&iacute;n</trd>,
    <trd>aspir&iacute;n</trd>
  </trdgrp>
</drv>
</art>
