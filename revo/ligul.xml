<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ligul</rad>/o
</kap>
<drv mrk="ligul.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Ha&ubreve;ta alpenda&jcirc;o inter foliingo kaj 
      foliplato de la <ref tip="malprt" cel="poac.0oj">poacoj</ref>
      kaj <ref tip="malprt" cel="ciper.0acoj">ciperacoj</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Blatth&auml;tchen</trd>,
    <trd>Ligula</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ligula</trd>,
    <trd>ligule</trd>
  </trdgrp>

  
</drv>
</art>

