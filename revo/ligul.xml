<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ligul.xml,v 1.16 2022/11/03 09:31:58 revo Exp $">
<kap>
  <rad>ligul</rad>/o
</kap>
<drv mrk="ligul.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="ligul.0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      Ha&ubreve;ta alpenda&jcirc;o inter foliingo kaj
      foliplato de la <ref tip="malprt" cel="poac.0oj">poacoj</ref>
      kaj <ref tip="malprt" cel="ciper.0oj">ciperacoj</ref>:
      <ekz>
        &ccirc;e Kutima aveno <klr>[&#8230;]</klr> la <tld/>o estas blanka kaj
        longas de 2 &gcirc;is 5&#x202F;mm
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=269083">Kutima aveno</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        sekalo dum herba stadio rekoneblas per havi nek <tld/>on, nek
        oreletojn flanke
        de la foliobazo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22325">Sekalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_z;&c_y;&c_ch;&c_o;&c_k; <klr tip="amb">(&c_a;&c_d;&c_r;&c_o;&c_s;&c_t;&c_a;&c_k; &c_a;&c_d; &c_l;&c_ib;&c_s;&c_t;&c_a;&c_v;&c_o;&c_j; &c_p;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_k;&c_ib;)</klr></trd>
  <trd lng="ca">l&iacute;gula</trd>
  <trdgrp lng="de">
    <trd>Blatth&auml;utchen</trd>,
    <trd>Ligula</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ligula</trd>,
    <trd>ligule</trd>
  </trdgrp>
  <trd lng="fr">ligule</trd>
  <trdgrp lng="hu">
    <trd>lev&eacute;lh&aacute;rtya</trd>,
    <trd>lev&eacute;lnyelvekcse</trd>,
    <trd>ligula</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ligula</trd>,
    <trd>tongetje</trd>
  </trdgrp>
  <trd lng="pl">j&eogonek;zyczek li&sacute;ciowy</trd>
  <trdgrp lng="pt">
    <trd>l&iacute;gula <klr>(bot.)</klr></trd>,
    <trd>b&aacute;rbula <klr>(bot.)</klr></trd>,
    <trd>hemifl&oacute;sculo <klr>(bot.)</klr></trd>,
    <trd>semifl&oacute;sculo<klr>(bot.)</klr></trd>
  </trdgrp>
  <trd lng="ro">ligule</trd>
  <trd lng="ru">&c_ja;&c_z;&c_y;&c_ch;&c_o;&c_k; <klr tip="amb">(&c_u;
&c_z;&c_l;&c_a;&c_k;&c_o;&c_v;)</klr></trd>
  <trd lng="uk">&#x44F;&c_z;&c_i;&c_ch;&#x43E;&#x43A; &c_u; &c_z;&#x43B;&c_a;&#x43A;&c_ib;&c_v;</trd>
</drv>
</art>
