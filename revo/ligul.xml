<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ligul</rad>/o
</kap>
<drv mrk="ligul.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Ha&ubreve;ta alpenda&jcirc;o inter foliingo kaj
      foliplato de la <ref tip="malprt" cel="poac.0oj">poacoj</ref>
      kaj <ref tip="malprt" cel="ciper.0oj">ciperacoj</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Blatth&auml;tchen</trd>,
    <trd>Ligula</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ligula</trd>,
    <trd>ligule</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>ligula</trd>,
    <trd>tongetje</trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_z;&c_y;&c_ch;&c_o;&c_k; <klr tip="amb">(&c_u; &c_z;&c_l;&c_a;&c_k;&c_o;&c_v;)</klr></trd>
</drv>
</art>
