<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gupi</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="gupi.0o">
  <kap><tld/>o</kap>
  <snc mrk="gupi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mar&ccirc;a kaj rivera fi&scirc;eto kun brilkoloraj vosto kaj na&gcirc;iloj,
      el la norda parto de Sudameriko, facile bredata en akvarioj
      (<trd lng="la">Poecilia reticulata</trd>):
      <ekz>
        trovi&gcirc;is e&ccirc; akvujo, en kiu trankvile na&gcirc;is <klr>...</klr> <tld/>oj
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>man&gcirc;o interne de la
        &ccirc;irka&ubreve;barilo, p. 64a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>guppie</trd>,
    <trd>guppy</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gupi</trd>,
    <trd>guppy</trd>
  </trdgrp>
</drv>
</art>

