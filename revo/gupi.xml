<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gupi.xml,v 1.7 2023/11/11 11:36:37 revo Exp $">
<kap>
  <rad>gupi</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="gupi.0o">
  <kap><tld/>o</kap>
  <snc mrk="gupi.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mar&ccirc;a kaj rivera fi&scirc;eto kun brilkoloraj vosto kaj na&gcirc;iloj,
      el la norda parto de Sudameriko, facile bredata en akvarioj
      (<trd lng="la">Poecilia reticulata</trd>):
      <ekz>
        trovi&gcirc;is e&ccirc; akvujo, en kiu trankvile na&gcirc;is <klr>...</klr>
        <tld/>oj
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>man&gcirc;o interne de la
        &ccirc;irka&ubreve;barilo, p. 64a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_p;&c_ib;</trd>
  <trd lng="ca">guppy</trd>
  <trd lng="de">Guppy</trd>
  <trdgrp lng="en">
    <trd>guppie</trd>,
    <trd>guppy</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gupi</trd>,
    <trd>guppy</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pawie oczko</trd>,
    <trd>gupik</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>guppy</trd>,
    <trd>lebiste</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x53E4;&#x6BD4;&#x9B5A; <pr>g&#x1D4;b&#x1D0;y&#xFA;</pr></trd>,
    <trd>&#x5B54;&#x96C0;&#x9B5A; <pr>k&#x1D2;ngqu&#xE8;y&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>

