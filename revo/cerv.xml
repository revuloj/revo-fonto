<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cerv</rad>/o
</kap>
<drv mrk="cerv.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="cerved.0oj"><tld/>edoj</ref>
      (<trd lng="la">Cervus</trd>), kies viro portas grandajn
      disbran&ccirc;i&gcirc;antajn kornojn.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_je;&c_n;&c_mol;</trd>
  <trd lng="de">Edelhirsch</trd>
  <trdgrp lng="el">
    <trd>&epsilon_ton;&lambda;&alfa;&phi;&omikron;&sigma_fina;</trd>,
    <trd>&epsilon_ton;&lambda;&alfa;&phi;&jota;</trd>
  </trdgrp>
  <trd lng="fr">cerf</trd>
  <trd lng="he">&tsadi;&bet;&yod;</trd>
  <trd lng="hu">szarvas</trd>
  <trd lng="nl">edelhert</trd>
  <trd lng="pt">cervo</trd>
  <trd lng="ru">&c_o;&c_l;&c_je;&c_n;&c_mol;</trd>
</drv>

<drv mrk="cerv.norda0o">
  <kap>norda <tld/>o</kap>
  <snc>
    <ref tip="dif" cel="boac.0o">boaco</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_n;&c_o;&c_ch;&c_n;&c_y; <ind>&c_a;&c_l;&c_je;&c_n;&c_mol;</ind></trd>
  <trd lng="de">Ren</trd>
  <trdgrp lng="hu">
    <trd>tar&aacute;ndszarvas</trd>,
    <trd>r&eacute;nszarvas</trd>
  </trdgrp>
  <trd lng="nl">rendier</trd>
  <trd lng="ru">&c_s;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j;
  &c_o;&c_l;&c_je;&c_n;&c_mol;</trd>
</drv>

</art>
