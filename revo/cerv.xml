<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>cerv</rad>/o
</kap>
<drv mrk="cerv.0o">
 <kap><ofc>*</ofc><tld/>o</kap>
 <uzo tip="fak">ZOO</uzo>
 <snc>
  <dif>
   Genro el familio <ref tip="super" cel="cerv.0edoj"><tld/>edoj</ref>
   (<trd lng="la">Cervus</trd>), kies viro portas grandajn
   disbran&ccirc;i&gcirc;antajn kornojn.
  </dif>
 </snc>
 <bld lok="http://131.211.121.124/REVO/NovajBildoj/cervo2.jpg">
  Cervo (<trd lng="la">Cervus Elaphus</trd>)
 </bld>
 <trd lng="de">Edelhirsch</trd>
 <trd lng="nl">edelhert</trd>
 <trd lng="ru">&c_o;&c_l;&c_je;&c_n;&c_mol;</trd>
</drv>
<drv mrk="cerv.norda0o">
 <kap>norda <tld/>o</kap>
 <snc>
  <ref tip="dif" cel="boac.0o">boaco</ref>
 </snc>
 <trd lng="de">Ren</trd>
 <trd lng="nl">rendier</trd>
 <trd lng="ru">&c_s;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j; &c_o;&c_l;&c_je;&c_n;&c_mol;</trd>
</drv>
<drv mrk="cerv.0edoj">
 <kap><tld/>edoj</kap>
 <uzo tip="fak">ZOO</uzo>
 <snc>
  <dif>
   Familio el la ordo <ref tip="super" cel="huf.par0uloj">parhufuloj</ref>
   (<trd lng="la">Cervidae</trd>).
  </dif>
 </snc>
 <trd lng="de">Hirsche</trd>     
</drv>
</art>
