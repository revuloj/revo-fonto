<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mar&scirc;</rad>/i
</kap>
<drv mrk="marsx.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Iri per egalaj militistaj pa&scirc;oj.
    </dif>
    <trd lng="en">march</trd>
    <trdgrp lng="sv">
      <trd>marschera</trd>,
      <trd>t&aring;ga</trd>
    </trdgrp>
    <trd lng="nl">marcheren</trd>
    <trd lng="ru">&c_m;&c_a;&c_r;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <dif>
      Pa&scirc;i.
    </dif>
    <trd lng="en">walk</trd>
    <trd lng="nl">lopen</trd>
    <trdgrp lng="ru">
      <trd>&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_sh;&c_a;&c_g;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">stega</trd>
  </snc>
  <trd lng="cs">pochodovat</trd>
</drv>

<drv mrk="marsx.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        militista, parada, procesia, peniga <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Ario akompananta kaj reguliganta la pa&scirc;adon; <tld/>ario:
      <ekz>
        funebra <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="nl">mars</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_sh;</trd>
  <trd lng="sv">marsch</trd>
</drv>
</art>
