<vortaro>
  <art mrk="$Id: kvadrilion.xml,v 1.1 2025/02/23 20:25:49 afido Exp $">
    <kap>
      <rad>kvadrilion</rad>/o <fnt><bib>JED</bib></fnt>,
      <var><kap><rad var="4">kvarilion</rad>/o</kap></var>
    </kap>

<drv mrk="kvadrilion.0o">
  <kap><tld/>o, <var><kap><tld var="4"/>o</kap></var></kap>
  <snc>
    <dif>
      Miliono da trilionoj, 10<sup>24</sup>:
      <ekz>
        la komputilego <nom>&Ccirc;ervonenkis</nom> okupis la 19-an lokon tutmonde <klr>[kun]</klr> 21,53
        petaflopoj, do 21,53 <tld var="4"/>oj da operacioj kun glitkomo sekunde
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=586868"
              >Yandex</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o da tesloj
        <fnt>
          <bib>PMEG</bib>
          <lok><url ref="https://dvd.ikso.net/edukado/pmeg/pmeg_12.1_x/vortfarado/neoficialaj_afiksoj/internaciaj.html"
              >Prefiksoj de mezurunuoj </url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Quadrillion</trd>
    <trd lng="en">quadrillion</trd>
    <trd lng="fr">quadrillion</trd>
    <trd lng="ru">&c_k;&c_v;&c_a;&c_d;&c_r;&c_i;&c_l;&c_l;&c_i;&c_o;&c_n;</trd>
  </snc>
</drv>

  </art>

