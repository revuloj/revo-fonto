<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: baron.xml,v 1.22 2024/10/07 18:36:24 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>baron</rad>/o
</kap>
<drv mrk="baron.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="baron.0o.nobela">
    <dif>
      <ref tip="super" cel="nobel.0o">Nobelo</ref> de rango pli malalta
      ol <ref tip="vid" cel="graf.0o.nobela">grafo</ref>:
      <ekz>
        amaso da mono kaj titolo de <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        peto de <tld/>o estas ordono
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la junaj <tld/>oj &ccirc;iam pafadis per siaj pafarkoj sur la du
        <klr>[portretojn]</klr>
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        amikoj nomis lin &leftquot;<tld/>o&rightquot;, pro lia trankvila sintenado kaj &gcirc;entilaj manieroj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        via <tld/>a mo&scirc;to
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_b;&c_a;&c_r;&c_o;&c_n;</trd>
  <trdgrp lng="de">
     <trd>Freiherr</trd>,
     <trd>Baron</trd>
  </trdgrp>
  <trd lng="es">bar&oacute;n</trd>
  <trd lng="en">baron</trd>
  <trd lng="fr">baron</trd>
  <trd lng="hu">b&aacute;r&oacute;</trd>
  <trd lng="nl">baron</trd>
  <trd lng="pl">baron</trd>
  <trd lng="pt">bar&atilde;o</trd>
  <trd lng="ro">baron</trd>
  <trd lng="ru">&c_b;&c_a;&c_r;&c_o;&c_n;</trd>
  <trd lng="sk">bar&oacute;n</trd>
  </snc>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B8;&#x5E8;&#x5D5;&#x5B9;&#x5DF;</trd>
  <trdgrp lng="ja">
    <trd>&#30007;&#29237;  <pr>&#12384;&#12435;&#12375;&#12419;&#x304F;</pr></trd>,
    <trd>&#x8AF8;&#x4FAF;  <pr>&#12375;&#12423;&#12371;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_a;&c_r;&#x43E;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#22806;&#x56FD;&#x8D35;&#x65CF; <pr>w&#xE0;igu&#xF3;gu&#xEC;z&#xFA;</pr></trd>,
    <trd>&#30007;&#29237; <pr>n&#xE1;nju&#xE9;</pr></trd>,
    <trd>&#x8CB4;&#x65CF; <pr>gu&#xEC;z&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>
