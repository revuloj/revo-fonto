<fnt><bib>PVEF</bib></fnt></kap>
<drv mrk="forst.0o">
  <kap><tld/>o</kap>
  <snc mrk="forst.0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Metode flegata <ref tip="super" cel="arb.0aro">arbaro</ref>:
      <ekz>
        la nova registaro <klr>[&#8230;]</klr> rikoltis malaprobon de la <tld/>a industrio,
        kiam &gcirc;i decidis ne plu ekspluati la indi&gcirc;enajn arbarojn &ccirc;e la okcidenta
        marbordo de Sudinsulo
        <fnt>
          <aut>Donald Rogers</aut>
            <vrk><url ref="&Monato;/2001/007092.php">&Scirc;an&gcirc;i&gcirc;as la direkto</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 8</lok>
          </fnt>;
      </ekz>
      <ekz>
        en la <tld/>a-ligna industrio laboras 700&#8239;000 a&ubreve;stroj
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010952.php">Lige al ligno</url></vrk>
           <bib>Monato</bib>
          <lok>2013/03, p. 13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru (kultiv-)<ke>arbaro</ke>.
  </rim>

  <trd lng="be">&c_l;&c_je;&c_s; <klr tip="amb">(&c_e;&c_k;&c_s;&c_p;&c_l;&c_ju;&c_a;&c_t;&c_u;&c_je;&c_m;&c_y;)</klr></trd>
  <trd lng="bo">&#3940;&#3954;&#3908;&#3851;&#3923;&#3906;&#3942;&#3851;</trd>
  <trd lng="ca">explotaci&oacute; forestal</trd>
  <trdgrp lng="cs">
    <trd>hvozd</trd>,
    <trd>p&ecaron;st&ecaron;n&yacute; les</trd>
  </trdgrp>
  <trd lng="de">Forst</trd>
  <trd lng="en">cultivated forest</trd>
  <trd lng="es">explotaci&oacute;n forestal</trd>
  <trd lng="fr">exploitation foresti&egrave;re</trd>
  <trd lng="ja">&#32946;&#25104;&#26519;  <pr>&#12356;&#12367;&#12379;&#12356;&#12399;&#12420;&#12375;</pr></trd>
  <trd lng="nl">bosbouw</trd>
  <trd lng="pt">floresta plantada</trd>
  <trd lng="ru">&c_l;&c_je;&c_s; <klr>(&c_e;&c_k;&c_s;&c_p;&c_l;&c_u;&c_a;&c_t;&c_i;&c_r;&c_u;&c_je;&c_m;&c_y;&c_j;)</klr></trd>
  <trd lng="sk">les <klr>(pestovan&yacute;)</klr></trd>
  <trd lng="uk">&#1083;&c_ib;&c_s; (&c_je;&#1082;&c_s;&#1087;&#1083;&c_u;&c_a;&c_t;&#1086;&c_v;&c_a;&#1085;&c_i;&c_j;)</trd>
