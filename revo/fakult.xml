<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>fakultat</rad>/o
</kap>
<drv mrk="fakult.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PED</uzo>
  <snc>
    <dif>
      &Ccirc;iu el la diversaj fakoj de <ref tip="malprt" cel="univer2.0o">universitato</ref>:
      <ekz>
        la <tld/>oj beletra, scienca, jura, medicina;
      </ekz>
      <ekz>lernolibroj uzataj en katolikaj seminarioj kaj teologiaj <tld/>oj<fnt><aut>David Castellano</aut>: <vrk>Vatikano rebatas
(2)</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 4a</lok></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="es">facultad</trd>
  <trdgrp lng="de">
    <trd>Fachbereich</trd>,
    <trd>Fakultas</trd>,
    <trd>Fakult&auml;t</trd>
  </trdgrp>
  <trd lng="fr">facult&eacute;<klr tip="ind"> (d'une universit&eacute;)</klr></trd>
  <trdgrp lng="hu">
    <trd>kar<klr tip="ind"> (egyetemi)</klr></trd>,
    <trd>fakult&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">faculteit</trd>
  <trd lng="ru">&c_f;&c_a;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_je;&c_t;</trd>
</drv>
</art>
