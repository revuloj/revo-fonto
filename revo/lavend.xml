<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>lavend</rad>/o
</kap>
<drv mrk="lavend.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Lavandula</trd>), kreskanta precipe
      en Mediteraneo, kun bonodoraj floretoj uzataj en parfumado.
    </dif>
  </snc>
  <trd lng="de">Lavendel</trd>
  <trd lng="en">lavander</trd>
  <trd lng="nl">lavendel</trd>
  <trd lng="pt">lavanda</trd>
  <trd lng="ru">&c_l;&c_a;&c_v;&c_a;&c_n;&c_d;&c_a;</trd>
  <trd lng="sv">lavendel</trd>
</drv>
</art>
