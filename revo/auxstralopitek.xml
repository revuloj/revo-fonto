<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: auxstralopitek.xml,v 1.11 2023/10/28 08:33:10 revo Exp $">
<kap>
  <rad>a&ubreve;stralopitek</rad>/o <fnt><bib>SPIV</bib></fnt>
</kap>

<drv mrk="auxstralopitek.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxstralopitek.0o.PRA">
    <uzo tip="fak">PRA</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Prahistoria malgranda <ref tip="malprt" cel="homed.0oj.ZOO">homedo</ref> (<trd lng="la">Australopithecus</trd>) el <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref>, kiu vivis en orienta
      kaj
      suda Afriko inter 3 milionoj kaj 1 milionoj da jaroj a.K.:
      <ekz>
        zono de arbaroj <klr>...</klr> estis elirebla de la <tld lit="A"/>o, la plej frua prahomo
        <fnt>
        <aut>Perez-Franco</aut>
        <vrk>Denove: pri kiel istmeto &scirc;an&gcirc;is la mondon</vrk>
        <lok>soc.culture.esperanto, 2003-02-08</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="lst" cel="simi.0o" lst="voko:simioj">simio</ref>
    <trdgrp lng="zh">
    <trd>&#x9732;&#x897F; <pr>l&#xF2;ux&#x12B;</pr></trd>,
    <trd>&#x5357;&#x65B9;&#x53E4;&#x733F; <pr>n&#xE1;nf&#x101;ngg&#x1D4;yu&#xE1;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_w;&c_s;&c_t;&c_r;&c_a;&c_l;&c_a;&c_p;&c_ib;&c_t;&c_e;&c_k;</trd>
  <trd lng="de">Australopithecus</trd>
  <trd lng="fr">australopith&egrave;que</trd>
  <trd lng="it">australopiteco</trd>
  <trd lng="nl">australopitecus</trd>
  <trd lng="pl">australopitek</trd>
  <trd lng="ru">&c_a;&c_v;&c_s;&c_t;&c_r;&c_a;&c_l;&c_o;&c_p;&c_i;&c_t;&c_je;&c_k;</trd>
  <trd lng="uk">&#x430;&#x432;&#x441;&#x442;&#x440;&#x430;&#x43B;&#x43E;&#x43F;&#x456;&#x442;&#x435;&#x43A;</trd>
</drv>
</art>

