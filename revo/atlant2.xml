<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: atlant2.xml,v 1.13 2023/12/28 13:31:44 revo Exp $">
<kap>
  <rad>Atlant</rad>/o
</kap>

<drv mrk="atlant2.A0o">
  <kap>
    <tld lit="A"/>o
  </kap>
  <snc mrk="atlant2.A0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Frato de <ref cel="promete.0o">Prometeo</ref>, la <ref cel="titan.0o">titano</ref>
      tenanta sur siaj &scirc;ultroj la <ref cel="firmam.0o">firmamento</ref>n:
      <ekz>
        la nomo <klr>(<ref cel="atlant.A0o">Atlantiko</ref>)</klr> estas ligita al greka mito
        pri fortegulo <tld lit="A"/>o
        <fnt>
          <lok><url
          ref="http://eo.wikipedia.com/wiki/Nomoj_De_Oceanoj"
          >Nomoj de oceanoj</url></lok>
        </fnt>;
      </ekz>

      <ekz>
        <uzo tip="stl">FIG</uzo>
        ne multaj scias, ke <nom>Pasquale</nom> estis iusence la <tld lit="a"/>o, kiu tenis la
        tutan Oficejon sur siaj &scirc;ultroj: li mastrumis, li riparis, li traktis kun
        provizantoj
        <fnt>
          <vrk><url ref="&Tekstaro;?nomo=ondo-de-esperanto"
              >&Scirc;an&gcirc;oj en la Centra Oficejo</url></vrk>
          <lok>La Ondo de Esperanto, 2001 &ndash; 2004 </lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="atalant.0o">Atalanto</ref>,
       <ref cel="ocean.Atlanta_O0o">Atlanta Oceano</ref>,
       <ref cel="atlantid.0o">Atlantido</ref>
    </refgrp>
  </snc>
  <rim>
    <ref tip="hom" cel="atlant3.0o">Atlanto</ref> estas anka&ubreve; usona urbego.
  </rim>
  <trd lng="be">&c_A;&c_t;&c_l;&c_ja;&c_n;&c_t;<klr tip="amb"> (&c_m;&c_ib;&c_t;.)</klr></trd>
  <trd lng="bg">&c_A;&c_t;&c_l;&c_a;&c_s;<klr tip="amb"> (&c_m;&c_i;&c_t;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;)</klr></trd>
  <trd lng="de">Atlas<klr tip="amb"> (Mythologie)</klr></trd>
  <trd lng="en">Atlas<klr tip="amb"> (myth.)</klr></trd>
  <trd lng="fr">Atlas<klr tip="amb"> (Mythologie)</klr></trd>
  <trd lng="hu">Atlasz<klr tip="amb"> (mitol&oacute;gia)</klr></trd>
  <trd lng="it">Atlante</trd>
  <trd lng="ru">&c_A;&c_t;&c_l;&c_a;&c_n;&c_t;<klr tip="amb"> (&c_m;&c_i;&c_f;.)</klr></trd>
</drv>

<drv mrk="atlant2.0o">
  <kap>
    <tld lit="a"/>o <fnt><bib>PIV1</bib></fnt>
  </kap>
  <snc mrk="atlant2.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Vir<ref tip="super" cel="statu.0o">statuo</ref> subtenanta entablementon,
      balkonon ktp.
    </dif>
    <ref tip="vid" cel="kariatid.0o">kariatido</ref>
    <bld
      lok="&WCU;/thumb/6/68/Dorotheenstr_37_Atlas_unten_rechts.JPG/180px-Dorotheenstr_37_Atlas_unten_rechts.JPG"
      prm="PD"
      ><tld lit="a"/>o, &ccirc;e Dorotheenstr. 37, Berlino
        <fnt>
          <aut>Friedrich R&ouml;hrs, propra verko</aut>
          <lok><url ref="&WCW;/File:Dorotheenstr_37_Atlas_unten_rechts.JPG"
              >&WCW;/File:Dorotheenstr_37_Atlas_unten_rechts.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_l;&c_ja;&c_n;&c_t; <klr tip="amb">(&c_a;&c_r;&c_h;&c_ib;&c_t;.)</klr></trd>
  <trd lng="bg">&c_a;&c_t;&c_l;&c_a;&c_n;&c_t;<klr tip="amb"> (&c_a;&c_r;&c_h;&c_i;&c_t;&c_je;&c_k;&c_t;&c_u;&c_r;&c_a;)</klr></trd>
  <trd lng="de">Atlant</trd>
  <trd lng="en">telamon<klr tip="amb"> (architecture)</klr></trd>
  <trdgrp lng="fr">
    <trd>atlante</trd>,
    <trd>t&eacute;lamon<klr tip="amb"> (architecture)</klr></trd>
  </trdgrp>
  <trd lng="hu">atlasz<klr tip="amb"> (&eacute;p&iacute;t&eacute;szet)</klr></trd>
  <trd lng="it">telamone<klr> (arch.)</klr></trd>
  <trd lng="ru">&c_a;&c_t;&c_l;&c_a;&c_n;&c_t;<klr tip="amb"> (&c_a;&c_r;&c_h;&c_i;&c_t;.)</klr></trd>
</drv>
</art>

