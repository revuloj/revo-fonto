<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ruf.xml,v 1.9 2023/07/12 21:25:59 revo Exp $">
<kap>
    <rad>ruf</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ruf.0a">
  <kap><tld/>a</kap>
  <snc mrk="ruf.0a.KOL">
    <dif>
      Havanta
      har<ref tip="lst" cel="kolor.0o.propre" lst="voko:koloroj">koloron</ref>
      ru&gcirc;e flavan:
      <ekz>
        viro malalta, flame <tld/>a, kun elstara kojnodento
        <fnt>
          <aut>M. A. Bulgakov, trad. S. Pokrovskij</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/MM/19.html">La
          majstro kaj Margarita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bej1.0a.KOL">beja</ref>,
      <ref cel="blond.0a.hararo">blonda</ref>,
      <ref cel="brun.0a">bruna</ref>,
      <ref cel="pig.0a.KOL">piga</ref>,
      <ref cel="ruan.0a.KOL">ruana</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_u;&c_d;&c_y;</trd>,
    <trd>&c_r;&c_y;&c_zh;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>rus&yacute;</trd>,
    <trd>ry&scaron;av&yacute;</trd>,
    <trd>zrzav&yacute;</trd>
  </trdgrp>
  <trd lng="de">rothaarig</trd>
  <trd lng="fr">roux<klr> (adj.)</klr></trd>
  <trd lng="pl">rudy</trd>
  <trdgrp lng="sk">
    <trd>hrdzav&yacute;</trd>,
    <trd>ry&scaron;av&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_u;&c_d;&c_i;&c_j;</trd>
</drv>

<drv mrk="ruf.0ulo">
  <kap><tld/>ulo</kap>
    <snc mrk="ruf.0ulo.KOL">
    <dif>
      <tld lit="R"/>hara homo:
      <ekz>
        la <tld/>ulo rigardis &scirc;in per duonfermitaj okuloj
        <fnt>
          <aut>M. A. Bulgakov, trad. S. Pokrovskij</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/MM/19.html">La
          majstro kaj Margarita</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_u;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_r;&c_u;&c_d;&c_y; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  </trdgrp>
  <trd lng="de">Rotschopf</trd>
  <trd lng="fr">roux<klr> (subst.)</klr></trd>
  <trdgrp lng="pl">
    <trd>rudow&lstroke;osy</trd>,
    <trd>rudzielec</trd>
  </trdgrp>
</drv>

<drv mrk="ruf.0ulino">
  <kap><tld/>ulino</kap>
    <snc mrk="ruf.0ulino.KOL">
    <dif>
      Inseksa <tld/>ulo:
      <ekz>
        li e&ccirc; regule renkontadas Katarinan, <tld/>ulinon el la dua jaro
        <fnt>
          <aut>S. Johansson</aut>,
          <vrk>Marina</vrk>,
          <lok>p. 72a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_u;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_r;&c_u;&c_d;&c_a;&c_ja; <klr>(&c_n;&c_a;&c_z;.)</klr></trd>
  </trdgrp>
  <trd lng="de">Rothaarige</trd>
  <trd lng="fr">rousse</trd>
  <trd lng="pl">rudow&lstroke;osa</trd>
</drv>

</art>

