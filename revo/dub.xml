<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dub</rad>/i
</kap>
<drv mrk="dub.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc>
    <gra><vspec>x</vspec></gra>
    <dif>
      Ne esti certa pri io, ne scii, &ccirc;u &gcirc;i estas vera a&ubreve; ne:
      <ekz>
        <tld/>i ion<fnt>Z</fnt>, pri io<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i la utilecon de io<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi ne <tld/>as pri via sukceso?
      </ekz>
      <ekz>
        mi <tld/>as, &ccirc;u vi estas feli&ccirc;a
        <fnt><bib>F</bib>, <lok>&FE; 26</lok></fnt>;
      </ekz>
      <ekz>
        mi forte <tld/>as, &ccirc;u iu serioze konsilus enkonduki
        tian grandan rompadon en nian aferon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        neniu <tld/>os, ke konstanta regulo estas
        mil fojojn pli bona<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ni ne povis <tld/>i, ke oni devis sen ia &scirc;anceli&gcirc;o
        akcepti<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiu <tld/>as sin detenu!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="demand">sin demandi</ref>,
      <ref cel="hezit.0i">heziti</ref>,
      <ref cel="skrupu.0o">skrupulo</ref>,
      <ref cel="sxance.0igxi">&scirc;anceli&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_m;&c_n;&c_ja;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="de">
    <trd>zweifeln</trd>,
    <trd>bezweifeln</trd>
  </trdgrp>
  <trd lng="en">doubt</trd>
  <trd lng="es">dudar</trd>
  <trd lng="fr">douter</trd>
  <trdgrp lng="hu">
    <trd>k&eacute;telkedik</trd>,
    <trd>k&eacute;ts&eacute;gbe von</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_m;&c_n;&c_je;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="dub.0a">
  <kap><tld/>a, (pri)<tld/>ebla</kap>
  <snc>
    <dif>
      Pri kio oni povas <tld/>i:
      <ekz>
        <tld/>a formo, rezultato;
      </ekz>
      <ekz>
        la respondo ne povas esti <tld/>a;
      </ekz>
      <ekz>
        tre <tld/>a plibona&jcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        en &ccirc;iuj <tld/>aj okazoj, oni preferu la
        nominativon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la deziro al la paco estas ne<tld/>ebla realo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cert.mal0a">malcerta</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="viand.0o">viando</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_m;&c_n;&c_je;&c_w;&c_n;&c_y;</trd>
  <trd lng="de">zweifelhaft</trd>
  <trd lng="es">dudoso</trd>
  <trd lng="fr">douteux<klr tip="ind"> (incertain)</klr></trd>
  <trdgrp lng="hu">
    <trd>k&eacute;ts&eacute;ges</trd>,
    <trd>k&eacute;ts&eacute;gbe vonhat&oacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_m;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="dub.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Intelekta stato de tiu, kiu <tld/>as:
      <ekz>
        senti <tld/>on pri io;
      </ekz>
      <ekz>
        esti en <tld/>o pri<fnt>Z</fnt>;
      </ekz>
      <ekz>
        esti atakita, kaptita de <tld/>o;
      </ekz>
      <ekz>
        tio forigas &ccirc;iujn <tld/>ojn<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="risk">risko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_u;&c_m;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_s;&c_u;&c_m;&c_n;&c_je;&c_w;</trd>
  </trdgrp>
  <trd lng="de">Zweifel</trd>
  <trdgrp lng="fr">
    <trd>doute</trd>,
    <trd>incertitude</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;tely</trd>,
    <trd>k&eacute;ts&eacute;g</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_m;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="dub.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Igi iun <tld/>anta.
    </dif>
    <trd lng="hu">k&eacute;telked&odblac;v&eacute; tesz</trd>
    <trd lng="ru">&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; <ind>&c_s;&c_o;&c_m;&c_n;&c_je;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</ind></trd>
  </snc>
  <snc>
    <dif>
      Igi ion necerta:
      <ekz>
        <tld/>igi la venkon.
      </ekz>
    </dif>
    <trd lng="hu">k&eacute;ts&eacute;gess&eacute; tesz</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>faire <ind>douter</ind></trd>,
    <trd>mettre en <ind>doute</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_v;&c_je;&c_r;&c_g;&c_n;&c_u;&c_t;&c_mol; <ind>&c_s;&c_o;&c_m;&c_n;&c_je;&c_n;&c_i;&c_ju;</ind></trd>
</drv>

<drv mrk="dub.0inda">
  <kap><tld/>inda</kap>
  <snc>
    <dif>
      Pri kiu oni ne povas ne <tld/>i:
      <ekz>
        <tld/>inda aserto, fideleco, venko.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_m;&c_n;&c_je;&c_w;&c_n;&c_y;</trd>
  <trd lng="de">zweifelhaft</trd>
  <trd lng="es">dudoso</trd>
  <trd lng="fr">douteux<klr tip="ind"> (&agrave; mettre en doute)</klr></trd>
  <trd lng="hu">gyan&uacute;s<klr tip="ind"> (k&eacute;ts&eacute;ges)</klr></trd>
  <trd lng="ru">&c_s;&c_o;&c_m;&c_n;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="dub.sen0a">
  <kap>ekster<tld/>a<fnt>Z</fnt>, sen<tld/>a<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Pri kiu oni ne povas <tld/>i:
      <ekz>
        garantio de sen<tld/>a sukceso<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_s;&c_u;&c_m;&c_n;&c_je;&c_w;&c_n;&c_y;</trd>
  <trd lng="de">zweifellos</trd>
  <trdgrp lng="fr">
    <trd>certain<klr tip="ind"> (hors de doute)</klr></trd>,
    <trd>hors de <ind>doute</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;ts&eacute;gtelen</trd>,
    <trd>biztos<klr tip="ind"> (k&eacute;ts&eacute;gtelen)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_je;&c_s;&c_o;&c_m;&c_n;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="dub.sen0e">
  <kap>sen<tld/>e</kap>
  <snc>
    <dif>
      Tute certe:
      <ekz>
        sen<tld/>e vi pravas.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_s;&c_u;&c_m;&c_n;&c_je;&c_n;&c_n;&c_a;</trd>
  <trd lng="de">zweifellos</trd>
  <trdgrp lng="fr">
    <trd>assur&eacute;ment</trd>,
    <trd>sans l'ombre d'une <ind>doute</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&eacute;ts&eacute;gtelen&uuml;l</trd>,
    <trd>biztosan<klr tip="ind"> (k&eacute;ts&eacute;gtelen&uuml;l)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_s;&c_o;&c_m;&c_n;&c_je;&c_n;&c_n;&c_o;</trd>,
    <trd>&c_b;&c_je;&c_z; <ind>&c_s;&c_o;&c_m;&c_n;&c_je;&c_n;&c_i;&c_ja;</ind></trd>
  </trdgrp>
</drv>
</art>
