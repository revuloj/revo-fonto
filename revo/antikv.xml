<vortaro>
<art mrk="$Id: antikv.xml,v 1.42 2025/06/18 16:00:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>antikv</rad>/a
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="antikv.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="antikv.0a.pasinta">
    <dif>
      Iam ekzistinta kaj de longe ne plu ekzistanta; pratempa:
      <ekz>
        la <tld/>a greka lingvo;
      </ekz>
      <ekz>
        en la <ind><tld/>a tempo</ind>
        <fnt><bib>MT</bib>, <lok>&Jeh; 38:17</lok></fnt>;
        <trd lng="ca">els temps antics</trd>
      </ekz>
      <ekz>
        fortuloj, tre famaj de plej <tld/>a tempo
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 6:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj lo&gcirc;antoj de E&ubreve;ropo
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        demeti de si la <tld/>an Adamon
        <klr>(plibonigi sian konduton)</klr><fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="arhxaik.0a">ar&hcirc;aika</ref>.
    </refgrp>
    <trd lng="ca">antic</trd>
  </snc>
  <snc mrk="antikv.0a.malnova">
    <dif>
      Tre malnova, malmoderna:
      <ekz>
        <tld/>a modo;
      </ekz>
      <ekz>
        nigra lana vesto kaj silka <tld/>a manteleto pendis sur la
        malgrasa korpo de la
        maljunulino
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel diras <tld/>a proverbo: de malpiuloj eliras malpia&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 24:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj rakontoj
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 4:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; latina estas malfacila, plena je <tld/>aj formoj senutilaj,
        manka je modernaj
        esprimiloj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        en norda parto paroli&gcirc;as ankora&ubreve; <tld/>a lingvo litova, proksima
        je sanskrita
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>vell</trd>,
      <trd>antiquat</trd>
    </trdgrp>
    <trd lng="zh">&#21476;&#33394;&#21476;&#39321; <pr>g&#468;s&egrave;g&#468;xi&amacron;ng</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_zh;&c_y;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_d;&c_a;&c_w;&c_n;&c_ib;</trd>,
    <trd>&c_a;&c_n;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&#3906;&#3923;&#3936;&#3851;&#3942;&#3988;&#3851;&#3928;&#3964;&#3851;</trd>
  <trdgrp lng="br">
    <trd>hen-</trd>,
    <trd>kozh-kozh</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>antick&yacute;</trd>,
    <trd>starobyl&yacute;</trd>
  </trdgrp>
  <trd lng="de">antik</trd>
  <trd lng="he">&ayin;&#1464;&tav;&#1460;&yod;&qof;</trd>
  <trdgrp lng="ja">
    <trd>&#21476;&#20195;&#12398;  <pr>&#12371;&#12384;&#12356;&#12398;</pr></trd>,
    <trd>&#22823;&#26132;&#12398;  <pr>&#12362;&#12362;&#12416;&#12363;&#12375;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="ms">antik</trd>
  <trdgrp lng="en">
    <trd>ancient</trd>
  </trdgrp>
  <trd lng="es">antiguo</trd>
  <trd lng="fr">antique<klr> (adj.)</klr></trd>
  <trdgrp lng="pl">
    <trd>staro&zdot;ytny</trd>,
    <trd>zabytkowy</trd>
  </trdgrp>
  <trd lng="pt">antigo</trd>
  <trd lng="ro">antic</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_je;&c_v;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_i;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>antick&yacute;</trd>,
    <trd>starod&aacute;vny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_r;&c_je;&c_v;&c_n;&c_ib;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_o;&c_d;&c_a;&c_v;&c_n;&c_ib;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="antikv.0o">
  <kap><tld/>o</kap>
  <snc mrk="antikv.0o.epoko">
    <dif>
      Historia
      <ref tip="lst" cel="epok.0o.periodo" lst="voko:historiaj_epokoj" val="-4000">epoko</ref> inter
      praepoko kaj mezepoko, markita de esti&gcirc;o kaj evoluo de la
      unuaj civilizoj en Mezoriento kaj &ccirc;e Mediteraneo:
      <ekz>
        <tld/>o komenci&gcirc;is je la 4-a jarmilo a.K. kaj fini&gcirc;is
        je la 7-a jarcento;
      </ekz>
      <ekz>
        procedo <klr>[&#8230;]</klr> pli longda&ubreve;ra kaj konsekvence pli
        multekosta ol kanonizo
        per aklamo, kiu sufi&ccirc;is en la juna eklezio de la <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Josef Fliegner</aut>
          <vrk><url ref="&Monato;/2005/008668.php">Sanktuloj kaj ties
          advokatoj</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="epok.malnov0o">malnovepoko</ref>
    <trdgrp lng="de">
      <trd>Antike</trd>,
      <trd>Altertum</trd>
    </trdgrp>
    <trd lng="pl">Staro&zdot;ytno&sacute;&cacute;</trd>
    <trdgrp lng="zh">
    <trd>&#21476;&#20195;&#21490; <pr>g&#468;d&agrave;ish&#464;</pr></trd>,
    <trd>&#21476;&#20195;&#29645;&#21697; <pr>g&#468;d&agrave;izh&emacron;np&#464;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;
  <klr>(&c_p;&c_e;&c_r;&c_y;&c_ja;&c_d; &c_g;&c_ib;&c_s;&c_t;&c_o;&c_r;&c_y;&c_ib;)</klr></trd>
  <trd lng="bo">&#3942;&#3988;&#3851;&#3928;&#3964;&#3851;</trd>
  <trdgrp lng="ca">
    <trd>edat antiga</trd>,
    <trd>antiguitat <klr tip="amb">(per&iacute;ode)</klr></trd>
  </trdgrp>
  <trd lng="cs">antikva</trd>
  <trd lng="sk">antikva</trd>
</drv>

<drv mrk="antikv.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="antikv.0ajxo.KOMUNE">
    <dif>
      Tre malnova objekto:
      <ekz>
        kolektanto de <tld/>a&jcirc;oj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i vere estis io tute alia ol la <tld/>a&jcirc;oj, kiuj estas tiel
        palaj kaj
        senkolori&gcirc;intaj
        <fnt>
          <bib>Fab4</bib>
          <lok>&leftquot;Bela&rightquot;</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas kolektanto de <tld/>a&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2004/008445.html">Kolekti
          antikva&jcirc;ojn</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        por trovi tiajn muzikajn <tld/>a&jcirc;ojn, mi devis multe voja&gcirc;i en
        Portugalio kaj
        Hispanio, ser&ccirc;ante ilin en brokantejoj kaj en privataj domoj
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2007/009252.php">Kantado
          en la gaela lingvo</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni bezonus multege da mono por resta&ubreve;ri tian <tld/>a&jcirc;on
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2008/009326.php">Analoj
          de Bailenambeann</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#21476;&#29609; <pr>g&#468;w&agrave;n</pr></trd>,
    <trd>&#21476;&#29289; <pr>g&#468;w&ugrave;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_k;&c_v;&c_a;&c_r;&c_y;&c_ja;&c_t;</trd>
  <trd lng="br">hendra</trd>
  <trd lng="ca">antiguitat <klr>(objecte)</klr></trd>
  <trd lng="de">Antiquit&auml;t</trd>
  <trd lng="he">&yod;&#1464;&mem;&#1461;&yod; &qof;&#1462;&dalet;&#1462;&fmem;</trd>
  <trdgrp lng="ja">
    <trd>&#21476;&#20195;&#12398;&#36986;&#29289;  <pr>&#12371;&#12384;&#12356;&#12398;&#12356;&#12406;&#12388;</pr></trd>,
    <trd>&#39592;&#33891;&#21697;  <pr>&#12371;&#12387;&#12392;&#12358;&#12402;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="ms">barang antik</trd>
  <trdgrp lng="en">
    <trd>antique</trd>
  </trdgrp>
  <trd lng="es">antig&uuml;edad</trd>
  <trd lng="fr">antiquit&eacute;<klr> (objet)</klr></trd>
  <trdgrp lng="pl">
    <trd>antyk</trd>,
    <trd>zabytek</trd>
  </trdgrp>
  <trd lng="ro">antichit&abreve;ti</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_k;&c_v;&c_a;&c_r;&c_i;&c_a;&c_t;</trd>
  <trd lng="sk">staro&zcaron;itnos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_o;&c_zh;&c_i;&c_t;&c_n;&c_o;&c_s;&c_t;&c_ib;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_o;&c_v;&c_i;&c_n;&c_n;&c_ib; &c_r;&c_je;&c_ch;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="antikv.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="antikv.0eco.stato">
    <dif>
      Eco de tio, kio estas <tld/>a:
      <ekz>
        de kie estus en li la sento de maljuneco a&ubreve; <tld/>eco
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eca monar&hcirc;io
        <fnt>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2000/006852.php">&Ccirc;u vere kristana erao?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 17</lok>
          </fnt>;
      </ekz>
      <ekz>
        <nom>Galway</nom> estas fascina kombina&jcirc;o de <tld/>eco kaj
        moderneco
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008094.html">Galway
          - urbo de triboj</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="br">
      <trd>hended</trd>,
      <trd>hender</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>antiguitat</trd>,
      <trd>vellesa</trd>
    </trdgrp>
    <trd lng="de">Antiquiertheit</trd>
  </snc>
  <snc mrk="antikv.0eco.epoko">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <tld/>a epoko:
      <ekz>
        en <tld/>eco Vi fondis la teron
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 102:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la plej malproksima <tld/>eco, kiu jam de longe elvi&scirc;i&gcirc;is el
        la memoro
        <klr>[&#8230;]</klr> la homa familio disi&gcirc;is kaj &gcirc;iaj membroj &ccirc;esis
        kompreni unu la
        alian
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne
          sur Mer en la 5a de a&ubreve;gusto 1905</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">Henamzer</trd>
    <trdgrp lng="ca">
      <trd>antigor</trd>,
      <trd>edat antiga</trd>
    </trdgrp>
    <trd lng="de">Antike</trd>
    <trdgrp lng="zh">
    <trd>&#21476;&#20195; <pr>g&#468;d&agrave;i</pr></trd>,
    <trd>&#21476;&#20195;&#29645;&#21697; <pr>g&#468;d&agrave;izh&emacron;np&#464;n</pr></trd>,
    <trd>&#21476;&#20856;&#26102;&#26399; <pr>g&#468;di&#462;nsh&iacute;q&#299;</pr></trd>,
    <trd>&#21476;&#20856;&#26178;&#26399; <pr>g&#468;di&#462;nsh&iacute;q&#299;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_w;&c_n;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_zh;&c_y;&c_t;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_n;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>antikvovanost</trd>,
    <trd>anti&ccaron;nost</trd>,
    <trd>starobylost</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>antiquity</trd>
  </trdgrp>
  <trd lng="es">antig&uuml;edad</trd>
  <trd lng="fr">antiquit&eacute;<klr> (qualit&eacute;, &eacute;poque)</klr></trd>
  <trd lng="he">&yod;&#1464;&mem;&#1461;&yod; &qof;&#1462;&dalet;&#1462;&fmem;</trd>
  <trdgrp lng="ja">
    <trd>&#21476;&#20195;  <pr>&#12371;&#12384;&#12356;</pr></trd>,
    <trd>&#22823;&#26132;  <pr>&#12362;&#12362;&#12416;&#12363;&#12375;</pr></trd>,
    <trd>&#21476;&#12373;  <pr>&#12405;&#12427;&#12373;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>antyczno&sacute;&cacute;</trd>,
    <trd>staro&zdot;ytno&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_je;&c_v;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>antikvita</trd>,
    <trd>starobylos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_n;&c_t;&c_i;&c_ch;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_o;&c_d;&c_a;&c_v;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="antikv.0ajxisto">
  <kap><tld/>a&jcirc;isto</kap>
  <snc mrk="antikv.0ajxisto.KOMUNE">
    <dif>
      Vendisto de <tld/>a&jcirc;oj:
      <ekz>
        mi staris post la pordo de butiko de <tld/>a&jcirc;isto
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        mia sendanka, monavida laktofrateto &ccirc;ion vendis al urbaj
        <tld/>a&jcirc;istoj
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 42a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#21476;&#25991;&#29289;&#21830; <pr>g&#468;w&eacute;nw&ugrave;sh&amacron;ng</pr></trd>,
    <trd>&#21476;&#33891;&#21830; <pr>g&#468;d&#466;ngsh&amacron;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_k;&c_v;&c_a;&c_r;</trd>
  <trd lng="br">hendraour</trd>
  <trd lng="ca">antiquari</trd>
  <trd lng="de">Antiquar</trd>
  <trd lng="he">&samekh;&vav;&#1465;&het;&#1461;&resh;
  &ayin;&#1463;&tav;&#1460;&yod;&qof;&vav;&#1465;&tav;</trd>
  <trd lng="ja">&#39592;&#33891;&#21830;  <pr>&#12371;&#12387;&#12392;&#12358;&#12375;&#12423;&#12358;</pr></trd>
  <trd lng="ms">penjual antik</trd>
  <trdgrp lng="en">
    <trd>antique dealer</trd>
  </trdgrp>
  <trd lng="es">anticuario</trd>
  <trd lng="fr">antiquaire</trd>
  <trd lng="pl">antykwariusz</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_k;&c_v;&c_a;&c_r;</trd>
  <trd lng="sk">staro&zcaron;itn&iacute;k</trd>
</drv>
