<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>antikv</rad>/a
</kap>
<drv mrk="antikv.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc num="1">
    <dif>
      Iam ekzistinta k de longe ne plu ekzistanta;  pratempa:
      <ekz>
        la <tld/>a greka lingvo;
      </ekz>
      <ekz>
        en la <tld/>a tempo
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Je&hcirc;ezkel 38, 17</lok>)</fnt>;
      </ekz>
      <ekz>
        demeti de si la <tld/>an Adamon
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
        <klr>(plibonigi sian konduton)</klr>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="arkaik.0a">arkaika</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <dif>
      Tre malnova, malmoderna:
      <ekz>
        <tld/>a modo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">antik</trd>
  <trdgrp lng="ru">
    <trd>&#1076;&#1088;&#1077;&#1074;&#1085;&#1080;&#1081;</trd>,
    <trd>&#1089;&#1090;&#1072;&#1088;&#1080;&#1085;&#1085;&#1099;&#1081;</trd>,
    <trd>&#1072;&#1085;&#1090;&#1080;&#1095;&#1085;&#1099;&#1081;</trd>
  </trdgrp>
</drv>
<drv mrk="antikv.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Tre malnova objekto:
      <ekz>
        kolektanto de <tld/>a&jcirc;oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Antiquit&auml;t</trd>
  <trd lng="ru">&#1072;&#1085;&#1090;&#1080;&#1082;&#1074;&#1072;&#1088;&#1080;&#1072;&#1090;</trd>
</drv>
<drv mrk="antikv.0eco">
  <kap><tld/>eco</kap>
  <snc num="1">
    <dif>
      Eco de tio, kio estas <tld/>a. 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      <tld/>a epoko. 
    </dif>
  </snc>
  <trdgrp lng="ru">
     <trd>&#1076;&#1088;&#1077;&#1074;&#1085;&#1086;&#1089;&#1090;&#1100;</trd>,
     <trd>&#1072;&#1085;&#1090;&#1080;&#1095;&#1085;&#1086;&#1089;&#1090;&#1100;</trd>,
     <trd>&#1089;&#1090;&#1072;&#1088;&#1080;&#1085;&#1072;</trd>
  </trdgrp>
</drv>
<drv mrk="antikv.0ajxisto">
  <kap><tld/>a&jcirc;isto</kap>
  <snc>
    <dif>
      Vendisto de <tld/>a&jcirc;oj.  
    </dif>
  </snc>
  <trd lng="ru">&#1072;&#1085;&#1090;&#1080;&#1082;&#1074;&#1072;&#1088;</trd>
</drv>
</art>
