<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lirik.xml,v 1.27 2025/02/05 08:30:25 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>lirik</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="lirik.0o">
  <kap><tld/>o</kap>
  <snc mrk="lirik.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      <ref tip="super" cel="poezi.0o.arto">Poezio</ref> kantanta la
      personajn impresojn, sentojn kaj pasiojn de la poeto mem:
      <ekz>
        la evoluon de la Esperanta <tld/>o priskribis <nom>William Auld</nom> en sia anta&ubreve;parolo al
        Esperanta Antologio
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        koncerne la lirikon <klr>(esperantan)</klr>, la periodoj ne dividi&gcirc;as klare
        <klr>[&#8230;]</klr> kelkaj poetoj de la unua periodo estas aktivaj ankora&ubreve; hodia&ubreve;
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi legis plejparte <tld/>on, kaj estis tiel, kiel kutime estis &ndash; fine anka&ubreve; mi
        komencis verki poemojn
        <fnt>
          <bib>VRA</bib>
          <lok>Jan Frideg&aring;rd</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lirik.0o.lirikajxo">
    <dif>
      <tld lit="L"/>a&jcirc;o:
      <ekz>
        ni trovas: tradukojn de Patro nia, de kelkaj versoj de Genezo kaj de poemeto de
        <nom>Heine</nom>, kaj du originalajn <tld/>ojn
        <fnt>
          <bib>Plumamikoj</bib>
          <lok>Temposkemo sciencfikcia en reala mondo</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj <tld/>oj restas gipsumitaj: kelkaj vere bonaj, pli multaj &scirc;ablonaj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2003/008065.html"
              >Finaj agordoj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_r;&c_y;&c_k;&c_a;</trd>
  <trd lng="ca">l&iacute;rica</trd>
  <trd lng="de">Lyrik</trd>
  <trdgrp lng="en">
    <trd>lyric</trd>,
    <trd>lyric poetry</trd>,
    <trd>lyrical poem</trd>
  </trdgrp>
  <trd lng="es">l&iacute;rica</trd>
  <trdgrp lng="fa">
    <trd>&a_gh;&a_z;&a_l;</trd>,
    <trd>&a_sh;&a_ayn;&a_r; &a_t;&a_gh;&a_z;&a_l;&f_y;</trd>,
    <trd>&a_sh;&a_ayn;&a_r; &a_gh;&a_n;&a_A;&f_y;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>po&eacute;sie <ind>lyrique</ind></trd>
  </trdgrp>
  <trd lng="hu">l&iacute;ra</trd>
  <trd lng="ja">&#21465;&#24773;&#35433;  <pr>&#12376;&#12423;&#12376;&#12423;&#12358;&#12375;</pr></trd>
  <trd lng="nl">lyriek</trd>
  <trd lng="pl">liryka</trd>
  <trd lng="pt">l&iacute;rica</trd>
  <trd lng="ro">genul liric</trd>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_ib;&c_r;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_ib;&c_r;&c_i;&c_ch;&c_n;&c_a; &c_p;&c_o;&c_je;&c_z;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25234;&#24773;&#35433; <pr>sh&umacron;q&iacute;ngsh&imacron;</pr></trd>,
    <trd>&#25234;&#24773;&#35799; <pr>sh&umacron;q&iacute;ngsh&imacron;</pr></trd>,
    <trd>&#35433; <pr>sh&imacron;</pr></trd>,
    <trd>&#35799; <pr>sh&imacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lirik.0a">
  <kap><tld/>a</kap>
  <snc mrk="lirik.0a.POE">
    <dif>
      Konvena al <tld/>o:
      <ekz>
        <tld/>a entuziasmo;
      </ekz>
      <ekz>
        plena&gcirc;uloj &gcirc;uas liajn <tld/>ajn versa&jcirc;ojn, grandajn poemojn
        kaj
        proza&jcirc;ojn
        <fnt><aut>Boris Kolker</aut>, <vrk>Senmorta Pu&scirc;kin
        (Aleksandr Pu&scirc;kin, 200 jaroj, elektitaj verkoj)</vrk>,
        <bib>Monato</bib>, <lok>jaro 2001a, numero 1a, p.
        32a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">l&iacute;ric</trd>
  <trd lng="cs">lyrick&yacute;</trd>
  <trd lng="de">lyrisch</trd>
  <trd lng="en">lyrical</trd>
  <trd lng="es">l&iacute;rico</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_gh;&a_z;&a_l;&f_y;</trd>,
    <trd>&a_gh;&a_n;&a_A;&f_y;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>lyrique</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>l&iacute;rai</trd>,
    <trd>l&iacute;rikus</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21465;&#24773;&#35433;&#12398;  <pr>&#12376;&#12423;&#12376;&#12423;&#12358;&#12375;&#12398;</pr></trd>,
    <trd>&#21465;&#24773;&#30340;&#12394;  <pr>&#12376;&#12423;&#12376;&#12423;&#12358;&#12390;&#12365;&#12394;</pr></trd>
  </trdgrp>
  <trd lng="nl">lyrisch</trd>
  <trd lng="pl">liryczny</trd>
  <trd lng="pt">l&iacute;rico</trd>
  <trd lng="ro">liric</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_i;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_i;&c_r;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">lyrick&yacute;</trd>
  <trd lng="uk">&c_l;&c_ib;&c_r;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#25234;&#24773; <pr>sh&umacron;q&iacute;ng</pr></trd>,
    <trd>&#24773;&#24863;&#20016;&#23500; <pr>q&iacute;ngg&#462;nf&emacron;ngf&ugrave;</pr></trd>,
    <trd>&#24773;&#24863;&#35920;&#23500; <pr>q&iacute;ngg&#462;nf&emacron;ngf&ugrave;</pr></trd>,
    <trd>&#35433;&#27468;&#19968;&#27171; <pr>sh&imacron;g&emacron;y&iacute;y&agrave;ng</pr></trd>,
    <trd>&#35799;&#27468;&#19968;&#26679; <pr>sh&imacron;g&emacron;y&iacute;y&agrave;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lirik.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="lirik.0eco.POE">
    <dif>
      <tld lit="L"/>a karaktero de io:
      <ekz>
        la frazoj de <nom>Ero&scirc;enko</nom>, duonjarcenton post la morto de la
        a&ubreve;toro, pulsas per tenera <tld/>eco
        <fnt><aut>Spomenka &Scirc;timec</aut>, <vrk>El la japana tra la &ccirc;ina
        (V. Ero&scirc;enko, Cikatro de amo)</vrk>, <bib>Monato</bib>, <lok>jaro
        1997a, numero 3a, p. 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_r;&c_y;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_ib;&c_r;&c_y;&c_z;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">lirisme</trd>
  <trdgrp lng="de">
    <trd>Lyrik</trd>,
    <trd>lyrische <ind>Stimmung</ind></trd>
  </trdgrp>
  <trd lng="en">lyricism</trd>
  <trd lng="es">liricidad</trd>
  <trd lng="fa">&a_t;&a_gh;&a_z;&a_l;</trd>
  <trdgrp lng="fr">
    <trd>lyrisme</trd>
  </trdgrp>
  <trd lng="hu">l&iacute;rais&aacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#21465;&#24773;  <pr>&#12376;&#12423;&#12376;&#12423;&#12358;</pr></trd>,
    <trd>&#21465;&#24773;&#21619;  <pr>&#12376;&#12423;&#12376;&#12423;&#12358;&#12354;&#12376;</pr></trd>
  </trdgrp>
  <trd lng="nl">lyriek</trd>
  <trd lng="pl">liryzm</trd>
  <trd lng="pt">liricidade</trd>
  <trd lng="ro">lirism</trd>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="zh">
    <trd>&#35433;&#24847; <pr>sh&imacron;y&igrave;</pr></trd>,
    <trd>&#35799;&#24847; <pr>sh&imacron;y&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="lirik.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Verkanto de <tld/>oj:
      <ekz>
        bonega <tld/>isto, kvankam mallertaj tradukoj faras tian ju&gcirc;on
        nur
        supozo
        <fnt><aut>Nikolao Gudskov</aut>,
        <vrk>Provo ektu&scirc;i serban poezion (Srdj Arandjelovic, Antologio
        de
        la serba poezio (1200-2000))</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2002a, numero 2a, p. 32a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_r;&c_y;&c_k;</trd>
  <trd lng="ca">poeta l&iacute;ric</trd>
  <trd lng="cs">lyrik</trd>
  <trd lng="de">Lyriker</trd>
  <trd lng="en">lyric poet</trd>
  <trd lng="fa">&a_gh;&a_z;&a_l;&a_s;&a_r;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>po&egrave;te <ind>lyrique</ind></trd>
  </trdgrp>
  <trd lng="hu">l&iacute;rikus</trd>
  <trd lng="ja">&#21465;&#24773;&#35433;&#20154;  <pr>&#12376;&#12423;&#12376;&#12423;&#12358;&#12375;&#12376;&#12435;</pr></trd>
  <trd lng="nl">lyricus</trd>
  <trd lng="pl">liryk</trd>
  <trd lng="ro">poet</trd>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_i;&c_k;</trd>
  <trd lng="sk">lyrik</trd>
  <trd lng="uk">&c_l;&c_ib;&c_r;&c_i;&c_k; (&c_p;&c_r;&c_o; &c_p;&c_o;&c_je;&c_t;&c_a;)</trd>
  <trdgrp lng="zh">
    <trd>&#35433;&#20154; <pr>sh&imacron;r&eacute;n</pr></trd>,
    <trd>&#35799;&#20154; <pr>sh&imacron;r&eacute;n</pr></trd>
  </trdgrp>
</drv>
</art>
