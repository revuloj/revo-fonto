<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>lirik</rad>/o
</kap>
<drv mrk="lirik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Poezio kantanta la personajn impresojn, sentojn kaj pasiojn de la
      poeto mem.
    </dif>
  </snc>
  <trd lng="de">Lyrik</trd>
  <trd lng="es">l&iacute;rica</trd>
  <trdgrp lng="fr">
    <trd>po&egrave;me <ind>lyrique</ind></trd>
  </trdgrp>
  <trd lng="nl">liriek</trd>
  <trd lng="pt">l&iacute;rica</trd>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="lirik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konvena al <tld/>o:
      <ekz>
        plena&gcirc;uloj &gcirc;uas liajn <tld/>ajn versa&jcirc;ojn, grandajn poemojn kaj
        proza&jcirc;ojn<fnt><aut>Boris Kolker</aut>: <vrk>Senmorta Pu&scirc;kin
        (Aleksandr Pu&scirc;kin, 200 jaroj, elektitaj verkoj)</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 1a, p. 32a</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>a entuziasmo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">lyrisch</trd>
  <trd lng="es">l&iacute;rico</trd>
  <trdgrp lng="fr">
    <trd>lyrique</trd>
  </trdgrp>
  <trd lng="nl">lirisch</trd>
  <trd lng="pt">l&iacute;rico</trd>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="lirik.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      <tld lit="L"/>a karaktero de io:
      <ekz>
        la frazoj de Ero&scirc;enko, duonjarcenton post la morto de la a&ubreve;toro,
        pulsas per tenera <tld/>eco<fnt><aut>Spomenka &Scirc;timec</aut>:
        <vrk>El la japana tra la &ccirc;ina (V. Ero&scirc;enko, Cikatro de amo)</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 3a, p. 21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Lyrik</trd>,
     <trd>lyrische <ind>Stimmung</ind></trd>
  </trdgrp>
  <trd lng="es">liricidad</trd>
  <trdgrp lng="fr">
    <trd>lyrisme</trd>
  </trdgrp>
  <trd lng="pt">liricidade</trd>
  <trd lng="ru">&c_l;&c_i;&c_r;&c_i;&c_ch;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="lirik.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      Verkanto de <tld/>oj:
      <ekz>
        bonega <tld/>isto, kvankam mallertaj
        tradukoj faras tian ju&gcirc;on nur supozo<fnt><aut>Nikolao
        Gudskov</aut>: <vrk>Provo ektu&scirc;i serban poezion (Srdj
        Arandjelovic, Antologio de la serba poezio (1200-2000))</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 2a, p. 32a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Lyriker</trd>
  <trdgrp lng="fr">
    <trd>po&egrave;te <ind>lyrique</ind></trd>
  </trdgrp>
</drv>
</art>
