<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>balast</rad>/o
</kap>
<drv mrk="balast.0o">
  <kap><tld/>o</kap>
  <snc mrk="balast.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      &Scirc;tonoj, sablo a&ubreve; aliaj pezaj materialoj, per kiuj oni
      ekzemple &scirc;ar&gcirc;as &scirc;ipojn por konservi ekvilibron, a&ubreve;
      aerostatojn por plipezigi ilin:
      <ekz>
        marakva <tld/>o de submaraj &scirc;ipoj;
      </ekz>
      <ekz>
        ni ne ricevis kargon, kaj oni ordonis, ke ni veturu hejmen kun
        <tld/>o
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>unua libro, &ccirc;apitro 1a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Ballast</trd>
    <trdgrp lng="hu">
      <trd>ballaszt</trd>,
      <trd>holts&uacute;ly</trd>
    </trdgrp>
  </snc>
  <snc mrk="balast.0o.FER">
    <uzo tip="fak">FER</uzo>
    <dif>
      Samspeca &scirc;ar&gcirc;a&jcirc;o &scirc;utita sur fervoja grundo, por
      firmigi la transversajn trabojn.
    </dif>
    <trdgrp lng="de">
      <trd>Schotter</trd>,
      <trd>Sch&uuml;ttung</trd>,
      <trd>Bettung</trd>
    </trdgrp>
    <trd lng="hu">kavics&aacute;gyazat</trd>
  </snc>
  <snc mrk="balast.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senutila superflua&jcirc;o, superflua malfacila&jcirc;o:
      <ekz>
        plej multaj neologismoj estas nur <tld/>o por la lingvo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rub.0o">rubo</ref>
    </refgrp>
    <trd lng="de">Ballast</trd>
    <trdgrp lng="hu">
      <trd>ballaszt</trd>,
      <trd>felesleges teher</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_l;&c_ja;&c_s;&c_t;</trd>
  <trd lng="en">ballast</trd>
  <trdgrp lng="fr">
    <trd>ballast</trd>,
    <trd>surcharge<klr tip="amb"> (fig.)</klr></trd>,
    <trd>pl&eacute;thore</trd>
  </trdgrp>
  <trd lng="nl">ballast</trd>
  <trd lng="ru">&c_b;&c_a;&c_l;&c_l;&c_a;&c_s;&c_t;</trd>
</drv>

<drv mrk="balast.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="balast.0i.KOMUNE">
    <dif>
      &Scirc;ar&gcirc;i per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_g;&c_r;&c_u;&c_z;&c_ib;&c_c;&c_mol; <ind>&c_b;&c_a;&c_l;&c_ja;&c_s;&c_t;</ind></trd>
  <trdgrp lng="de">
    <trd>beballasten</trd>,
    <trd>belasten</trd>,
    <trd>Ballast nehmen lassen</trd>,
    <trd>schottern<klr> [Bahn]</klr></trd>,
    <trd>beschottern<klr> [Bahn]</klr></trd>
  </trdgrp>
  <trd lng="en">ballast</trd>
  <trd lng="fr">ballaster</trd>
  <trd lng="hu">holts&uacute;llyal <ind>terhel</ind></trd>
  <trd lng="nl">ballasten</trd>
  <trd lng="ru">&c_z;&c_a;&c_g;&c_r;&c_u;&c_z;&c_i;&c_t;&c_mol; <ind>&c_b;&c_a;&c_l;&c_l;&c_a;&c_s;&c_t;</ind></trd>
</drv>

<drv mrk="balast.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="balast.sen0igi.KOMUNE">
    <dif>
      Forigi de io la <tld/>on, malembarasi.
      <ekz>
        la Majstro emis pli k<klr>aj</klr> pli sen<tld/>igi sian stilon
        <fnt><bib>Lanti</bib>, <lok>Pri la Biblio</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kribr.0i">kribri</ref>,
      <ref cel="pez">malpezigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_ib;&c_n;&c_u;&c_c;&c_mol; <ind>&c_b;&c_a;&c_l;&c_ja;&c_s;&c_t;</ind></trd>,
    <trd>&c_p;&c_a;&c_z;&c_b;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; <ind>&c_b;&c_a;&c_l;&c_ja;&c_s;&c_t;&c_u;</ind></trd>,
    <trd>&c_a;&c_b;&c_l;&c_je;&c_g;&c_ch;&c_y;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>entballasten</trd>,
    <trd>vom Ballast befreien</trd>,
    <trd>freilegen<klr> [Bahn]</klr></trd>
  </trdgrp>
  <trd lng="en">release <ind>ballast</ind></trd>
  <trdgrp lng="fr">
    <trd>all&eacute;ger</trd>,
    <trd>d&eacute;ballaster</trd>
  </trdgrp>
  <trd lng="hu">felesleges tehert&odblac;l <ind>megszabad&iacute;t</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_b;&c_r;&c_o;&c_s;&c_i;&c_t;&c_mol; <ind>&c_b;&c_a;&c_l;&c_l;&c_a;&c_s;&c_t;</ind></trd>,
    <trd>&c_o;&c_b;&c_l;&c_je;&c_g;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="balast.lastesedne0e">
  <kap>laste sed ne <tld/>e</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="balast.lastesedne0e.KOMUNE">
    <dif>
      En la fino, sed tamen grave:
      <ekz>
        &ccirc;irka&ubreve; mil personoj <klr>...</klr> laboristoj,
        mediaktivuloj, studentoj kaj artistoj, kaj laste sed ne <tld/>e
        sep maldekstraj senatanoj
        <fnt>
        <aut>&Gcirc;angalo</aut>,
        <vrk><url
        ref="http://gxangalo.com/modules/news/article.php?storyid=1117">Protestantoj
        en Tajlando metas maskon sur la bu&scirc;on por oponi al
        Bu&scirc;</url></vrk>,
        <lok>2003-10-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">last but not least</trd>
  <trd lng="es">por &uacute;ltimo pero no menos importante</trd>
  <trdgrp lng="fr">
    <trd>en dernier lieu mais en premi&egrave;re place</trd>,
    <trd>last but not least</trd>
  </trdgrp>
</drv>
</art>
