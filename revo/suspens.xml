<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: suspens.xml,v 1.12 2023/05/06 14:26:05 revo Exp $">
<kap>
  <rad>suspens</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="suspens.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Anim<ref tip="super" cel="strecx.0o">stre&ccirc;o</ref> pro atendo de
      sekvaj okaza&jcirc;oj, precipe en rakonto:
      <ekz>
        la <klr>[&#8230;]</klr> re&gcirc;isoro sukcese kreas <tld/>on ekde la unua
        sceno
        <fnt>
          <aut>Marteno Ecott</aut>,
          <vrk>Inkubo en kubo</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2000a, numero 3a, p. 17a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu &ccirc;apitro fini&gcirc;as per surprizo a&ubreve; originala turni&gcirc;o, kiu plialtigas la nivelon
        de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Lode van de Velde</aut>
          <vrk><url ref="&Monato;/2005/008430.php"
              >Suspenso de komenco &gcirc;is fino</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estu permesita al iu ajn a&ubreve;toro de simple distra, senbrida <ind><tld/>romano</ind>,
        kondi&ccirc;e ke li anta&ubreve;deklaru, ke personoj, lokoj kaj eventoj de li
        prirakontotaj estas nuraj fantazi-&jcirc;ongla&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Karlo &Gcirc;elozo</aut>
          <vrk><url ref="&Monato;/2005/008695.php"
              >Interesa recenzo, sed kompletigebla</url></vrk>
          <lok>2005</lok>
        </fnt>.
        <trd lng="de">Thriller</trd>
        <trd lng="en">thriller</trd>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="strecx.0o">stre&ccirc;o</ref>,
      <ref cel="tensi.0o.strecxo">tensio<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="atend.0o">atendo</ref>,
      <ref cel="sensac1.0o">sensacio</ref>,
      <ref cel="stres.0o">streso</ref>
    </refgrp>
  </snc>
  <trd lng="ca">suspens<klr> (cin./lit.)</klr></trd>
  <trd lng="cs">chv&iacute;le nap&ecaron;t&iacute; v d&ecaron;je <klr>(zvl. ve filmu| dramatu)</klr></trd>
  <trd lng="de">Spannung <klr>(Erwartung)</klr></trd>
  <trd lng="en">thrill</trd>
  <trd lng="fr">suspense<klr> (r&eacute;cit etc.)</klr></trd>
  <trd lng="hu">(dr&aacute;mai) <ind>fesz&uuml;lts&eacute;g</ind></trd>
  <trd lng="it">suspense</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_o;&c_je; &c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_v;&c_o;&c_zh;&c_n;&c_o;&c_je; &c_o;&c_zh;&c_i;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_a;&c_s;&c_p;&c_je;&c_n;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chv&iacute;&lcaron;a nap&auml;tia v deji <klr>(filmu, dr&aacute;my)</klr></trd>,
    <trd>chv&iacute;&lcaron;a nap&auml;tia v deji <klr>(filmu| dr&aacute;my)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_je; &c_o;&c_ch;&c_ib;&c_k;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_m;&c_o;&c_m;&c_je;&c_n;&c_t; &c_n;&c_a;&c_p;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_o;&c_g;&c_o; &c_o;&c_ch;&c_ib;&c_k;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="suspens.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ka&ubreve;zanta <ref cel="strecx.0a.postulema">stre&ccirc;a</ref>n sciavidon pri la sekvo:
      <ekz>
        &gcirc;uste en &ccirc;i <tld/>a momento, la a&ubreve;toro de &ccirc;i historio
        interrompis
        la priskribon de la duelo
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 8a, p.
        71a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kapt.atento0a">atentokapta</ref>,
      <ref cel="fascin.0a">fascina</ref>,
      <ref cel="intere.0a">interesa</ref>,
      <ref cel="sensac1.0a">sensacia</ref>,
      <ref cel="spir.0kapta">spirkapta</ref>
    </refgrp>
  </snc>
  <trd lng="ca">de suspens</trd>
  <trd lng="de">spannend</trd>
  <trd lng="en">thrilling</trd>
  <trd lng="hu">fesz&uuml;lt</trd>
  <trd lng="it">avvincente</trd>
  <trd lng="ru">&c_n;&c_a;&c_p;&c_r;&c_ja;&c_zh;&c_jo;&c_n;&c_n;&c_y;&c_j;<klr tip="amb"> (&c_o;
  &c_s;&c_c;&c_je;&c_n;&c_je;)</klr></trd>
</drv>
</art>

