<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ej.xml,v 1.30 2024/03/14 17:24:26 revo Exp $">
<kap>
<ofc>*</ofc>-<rad>ej</rad>
<fnt> <bib>UV</bib></fnt>
</kap>
<subart>
  <drv mrk="ej.0">
  <kap>-<tld/></kap>
  <snc mrk="ej.0.sufikso">
    <dif>
      <ref tip="super" cel="sufiks.0o">Sufikso</ref> montranta
      <ref tip="sin" cel="lok.0o.spaca">loko</ref>n difinitan
      por io a&ubreve; karakterizatan per io:
      <ekz>
        <ref cel="herb.0ejo">herb<tld/>o</ref>j apudakvaj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 19:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="dens.0ejo">dens<tld/>o</ref> de arbaro
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 9:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="tomb.0ejo">tomb<tld/>o</ref>
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        pastron nutras <ref cel="pregx.0ejo">pre&gcirc;<tld/>o</ref>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&gcirc;i havis]</klr> bonegan <ref cel="kuir.0ejo">kuir<tld/>o</ref>n
        kaj <ref cel="proviz.0ejo">proviz<tld/>o</ref>n
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>dorm<tld/>o</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 25:8</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Schlafplatz</trd>,
          <trd>Schlafstatt</trd>,
          <trd>Schlafraum</trd>
        </trdgrp>
      </ekz>
      <ekz>
        knaba <ref cel="lern.0ejo">lern<tld/>o</ref>
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la <ref cel="cxeval.0ejo">&ccirc;eval<tld/>o</ref> blekis la nigraj &ccirc;evaloj
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="kusx.0ejo">ku&scirc;<tld/>o</ref>
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 15:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>noktohalt<tld/>o</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 43:21</lok>
        </fnt>;
        <trd lng="de">Nachtlager</trd>
      </ekz>
      <ekz>
        <ind>mia lo&gcirc;<tld/>o &dash; mia re&gcirc;<tld/>o</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
        <trd lng="de">mein Haus ist mein <ind>K&ouml;nigreich</ind></trd>
      </ekz>
      <ekz>
        la transir<tld/>o de <nom>Jordan</nom>
        <klr>(vd <ref cel="vad.tra0ejo">travadejo</ref>)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 12:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="bord.al0igxejo">albordi&gcirc;<tld/>o</ref> de &scirc;ipoj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 49:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="rifugx.0ejo">rifu&gcirc;<tld/>o</ref> kontra&ubreve; ven&gcirc;anto
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 35:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> malliberuloj <klr>[&#8230;]</klr> estis en
        la <ref cel="liber.mal0ejo">malliber<tld/>o</ref>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 39:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kurteno faru por vi apartigon inter la
        <ref cel="sankt.0ejo">sankt<tld/>o</ref> kaj la
        <ref cel="sankt.plej0ejo">plejsankt<tld/>o</ref>
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 26:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <ref cel="ir.en0ejo">enir<tld/>o</ref> de la domo
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 11:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la enir<tld/>o de la pordego
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 10:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>alir<tld/>o</ind>
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 26:18</lok>
        </fnt>;
        <trd lng="de">Zugang</trd>
      </ekz>
      <ekz>
        <ind>vinber<tld/>o</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 23:11</lok>
        </fnt>;
        <trd lng="de">Weinberg</trd>
      </ekz>
      <ekz>
        <ind>olivarb<tld/>o</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 23:11</lok>
        </fnt>;
        <trd lng="de">Olivenhain</trd>
      </ekz>
      <ekz>
        <ref cel="drasx.0ejo">dra&scirc;<tld/>o</ref>
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 16:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref cel="garb.0ejo">garb<tld/>o</ref>
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 18:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>vinprem<tld/>o</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 18:27</lok>
        </fnt>;
        <trd lng="de">Kelterei</trd>
      </ekz>
      <ekz>
        dudek bovoj de <ref cel="pasxt.0ejo">pa&scirc;tejo</ref>
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 4:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&gcirc;i]</klr> naski&gcirc;is en anas<tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        dando el re&gcirc;a hund<tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        cindr<tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 4:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> renkonti&gcirc;is <klr>[&#8230;]</klr> &ccirc;e la <ref cel="akv.0ejo">akvejo</ref>
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 2:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dezerto kaj la <ind>senakv<tld/>o</ind>
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 35:1</lok>
        </fnt>;
        <trd lng="de">&Ouml;dnis</trd>
      </ekz>
      <ekz>
        tiam <klr>[&#8230;]</klr> dezerti&gcirc;os lia deven<tld/>o kaj li disrabos la
        trezor<tld/>on de &ccirc;iuj grandvalora&jcirc;oj
        <fnt>
          <bib>MT</bib>
          <lok>Ho&scirc;ea 13:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        po&scirc;taj <ref cel="ofic.0ejo">ofic<tld/>o</ref>j
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      En kelkaj okazoj la karakteriza&jcirc;o ne difinas lokon, en kiu &gcirc;i okazas, sed
      eksteran limon: <ref cel="krut.inter0ejo">interkrutejo</ref>, <ref
      cel="tegmen.sub0o">subtegmentejo</ref>.
      Gramatikistoj foje argumentas, ke tie &leftquot;-ej&rightquot; ne rolas kiel sufikso, sed kiel memstara vorto.
      &Z; miascie
      &gcirc;enerale ne faris tian distingon inter afiksoj kaj memstaraj vortetoj, sed &F; limigas la
      sufikson origine al &leftquot;loko por ...&rightquot;, kiu ne kovras vortojn kiel &leftquot;densejo&rightquot;, &leftquot;interkrutejo&rightquot;.
      <aut>Wolfram Diestel</aut>
    </rim>
    <trd lng="sgn">M509x510S16d20492x490</trd>
  </snc>
    <trd lng="ca">(sufix de lloc, de local)</trd>
    <trdgrp lng="de">
      <trd>-<ind>ei</ind><klr> (z.B. B&auml;ckerei)</klr></trd>,
      <trd>-<ind>statt</ind><klr> (z.B. Werkstatt)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>-<ind>hely</ind></trd>,
      <trd>-<ind>da</ind><klr> (helyis&eacute;g)</klr></trd>
    </trdgrp>
    <trd lng="io">-ey</trd>
    <trd lng="pt">-<ind>aria</ind><klr> (sufixo que indica
    lugar)</klr></trd>
  </drv>
</subart>
<subart>
  <dif>
    Samsignifa memstara vortero:
  </dif>
  <drv mrk="ej.0o">
    <kap><tld/>o</kap>
    <snc mrk="ej.0o.loko">
      <ekz>
        vasta <tld/>o
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi amas la <tld/>on de via domo
        <fnt><bib>MT</bib>, <lok>&Psa; 26:8</lok></fnt>;
      </ekz>
      <ekz>
        tiuj <tld/>oj estis en la orienta parto de l' urbo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Marta</nom> eniris en tiun <tld/>on kaj petis, ke oni alportadu al &scirc;i tagman&gcirc;ojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        se la oratoro jam konas la <tld/>on, <klr>[&#8230;]</klr> tio sendube plifaciligas la
        komencan fikson de la vo&ccirc;alteco
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
      <rim>
        <em>ejo</em> diferencas de <em>ujo</em> per tio, ke <em>ujo</em>
        estas efektiva, materia, fizika objekto, entenanta ion, kiel meblo,
        kesto, sako, ktp, dum <em>ejo</em> estas loko, spaco, karakterizata per
        io konkreta a&ubreve; abstrakta.
      </rim>
    </snc>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_m;&c_ja;&c_sh;&c_k;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_m;&c_je;&c_s;&c_c;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>indret</trd>,
      <trd>lloc</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>m&iacute;stnost</trd>,
      <trd>m&iacute;sto</trd>,
      <trd>prostor</trd>,
      <trd>p&uring;sobi&scaron;t&ecaron;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Ort</trd>,
      <trd>Platz</trd>,
      <trd>Stelle</trd>,
      <trd>St&auml;tte</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>endroit</trd>,
      <trd>lieu</trd>,
      <trd>local<klr tip="ind"> (subst.)</klr></trd></trdgrp>
    <trdgrp lng="hu">
      <trd>hely</trd>,
      <trd>helyis&eacute;g</trd>
    </trdgrp>
  <trd lng="ja">&#22580;&#25152;  <pr>&#12400;&#12375;&#12423;</pr></trd>
  <trd lng="ne">&#2313;&#2344;&#2368;&#2346;&#2331;&#2367; &#2344;&#2351;&#2366;&#2305;</trd>
  <trd lng="ru">&c_p;&c_o;&c_m;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">miestnos&tcaron;</trd>
  <trdgrp lng="zh">
    <trd>&#22788;&#25152; <pr>ch&ugrave;su&#466;</pr></trd>,
    <trd>&#25152;&#22312; <pr>su&#466;z&agrave;i</pr></trd>,
    <trd>&#20301; <pr>w&egrave;i</pr></trd>,
    <trd>&#22320; <pr>d&igrave;</pr></trd>
  </trdgrp>
</drv>
</subart>
</art>
