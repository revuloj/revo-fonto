<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxlemil.xml,v 1.4 2023/03/24 08:30:55 revo Exp $">
<kap>
  <rad>&scirc;lemil</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sxlemil.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="sxlemil.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="sort.malbon0ulo">Malbonsortulo</ref>, malprosperulo:
      <ekz>
        li nur tenis sin <tld/>o, havanta nenian pretendon pri ri&ccirc;a&jcirc;o
        <fnt>
          <aut>Pu Songling, trad. Guozhu</aut>,
          <vrk><url ref="http://www.everk.it/index.php?id=19,81,0,0,1,0">
          A&ubreve;guro per moneroj</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Tim Burton</nom> <klr>[&#8230;]</klr> helpis lin disvolvi&gcirc;i kiel respektata aktoro
        <klr>[anstata&ubreve;]</klr> pario, <tld/>o, alia peco de holivuda inter&scirc;an&gcirc;ebla
        viando
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=38784">Johnny Depp</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="kapr.propeka0o">propeka kapro</ref>,
       <ref tip="vid" cel="pari.0o">pario</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ib;&c_k;&c_ch;&c_e;&c_m;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_n;&c_ja;&c_w;&c_d;&c_a;&c_h;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schlemihl</trd>,
    <trd>Ungl&uuml;cksrabe</trd>,
    <trd>Pechvogel</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;veinard</trd>,
    <trd>rat&eacute;<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_z;&c_v;&#x43E;&#x43B;&#x44C;&#x43D;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_z;&#x43F;&#x43E;&c_r;&c_a;&c_d;&#x43D;&c_a; &#x43B;&#x44E;&c_d;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_j;&#x43E;&#x43B;&#x43E;&#x43F;</trd>,
    <trd>&c_b;&c_je;&c_v;&c_z;&c_je;&#x43D;&#x44C;</trd>,
    <trd>&c_t;&c_je;&#x43B;&c_je;&#x43F;&c_je;&#x43D;&#x44C;</trd>
  </trdgrp>
</drv>
</art>

