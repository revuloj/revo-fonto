<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>map</rad>/o
</kap>

<drv mrk="map.0o">
  <kap><tld/>o</kap>
  <snc mrk="map.0o.GEOG">
    <uzo tip="fak">AST</uzo>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Desegno figuranta regionon per simboloj de &gcirc;iaj objektoj
      (vojoj,
      reliefo, grundoj, vetero...) (geografia <tld/>o, land<tld/>o),
      a&ubreve; la
      &ccirc;ielon kun &gcirc;iaj astroj (astronomia <tld/>o):
      <ekz>
        la nomo de la &ccirc;efurbo aperu sur veterprognoza <tld/>o
        <fnt><aut>Raita Pyh&auml;l&auml;</aut>,
        <vrk>&leftquot;Stelo&rightquot; &ccirc;e
        la urbodomo</vrk>, <bib>Monato</bib>, <lok>jaro 2002a, numero
        6a,
        p. 13a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kart.0o.GEOG_AST">karto<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="projek.0o.GEOG">projekcio <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_t;&c_a;
    <klr>(&c_g;&c_je;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ch;&c_n;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_m;&c_a;&c_p;&c_a;</trd>
  </trdgrp>
  <trd lng="br">kartenn</trd>
  <trd lng="ca">mapa</trd>
  <trdgrp lng="de">
    <trd>Karte<klr tip="ind"> (geogr.)</klr></trd>,
    <trd>Landkarte</trd>
  </trdgrp>
  <trd lng="en">map</trd>
  <trd lng="es">mapa</trd>
  <trdgrp lng="fr">
    <trd>carte<klr tip="ind"> (plan)</klr></trd>,
    <trd>plan<klr tip="ind"> (carte)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&eacute;rk&eacute;p</trd>
  <trdgrp lng="nl">
    <trd>kaart<klr tip="ind"> (geogr.)</klr></trd>,
    <trd>sterrenkaart</trd>,
    <trd>landkaart</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mapa</trd>,
    <trd>plan<klr> (np. miasta)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_a; <klr
  tip="amb">(&c_g;&c_je;&c_o;&c_g;&c_r;.,
  &c_a;&c_s;&c_t;&c_r;.)</klr></trd>
  <trd lng="sk">mapa</trd>
  <trd lng="sv">karta</trd>
</drv>

<drv mrk="map.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="map.0aro.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Libroforma <ref tip="super" cel="kolekt.0o.libr">kolekto</ref>
      da <ref tip="prt" cel="map.0o"><tld/>oj</ref>, kutime kun indeksoj
      kaj krominformoj:
      <ekz>
        <klr>(la revuo LOdE)</klr> instigas min trovi en la
        <tld/>aro anta&ubreve;e nekonatajn loknomojn, ekzemple,
        <em>Jenisejo</em>
        <fnt>
        <aut>A. P. Warren</aut>,
        <bib>LOdE</bib>, <lok>numero 71 (2000:8&ndash;9)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_l;&c_a;&c_s;
  <klr>(&c_g;&c_je;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ch;&c_n;&c_y;)</klr></trd>
  <trd lng="br">atlas</trd>
  <trdgrp lng="ca">
    <trd>atlas</trd>,
    <trd>atles</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Atlas</trd>,
    <trd>Kartenwerk</trd>
  </trdgrp>
  <trd lng="en">atlas</trd>
  <trd lng="es">atlas</trd>
  <trd lng="fr">atlas<klr tip="amb"> (g&eacute;ographique)</klr></trd>
  <trdgrp lng="pt">
    <trd>atlas</trd>,
    <trd>mapoteca</trd>
  </trdgrp>
  <trd lng="pl">atlas</trd>
  <trd lng="ru">&c_a;&c_t;&c_l;&c_a;&c_s;<klr tip="amb">
  (&c_g;&c_je;&c_o;&c_g;&c_r;.)</klr></trd>
  <trd lng="sk">atlas</trd>
</drv>

<drv mrk="map.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="map.0igi.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Prezenti iun spacon, farante <tld/>on de &gcirc;i:
      <ekz>
        oni anka&ubreve; sufi&ccirc;e detale <tld/>igis la marprofundon
        <fnt>
        <aut>krab</aut>
        <vrk>Re: (vetero) varmeco en la norda duonglobo</vrk>
        <lok>soc.culture.esperanto, 1995-01-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">kartographieren</trd>
  <trdgrp lng="es">
     <trd>hacer un mapa</trd>,
     <trd>cartografiar</trd>
  </trdgrp>
  <trd lng="fr">dessiner la <ind>carte</ind> de</trd>
  <trdgrp lng="pl">
    <trd>rysowa&cacute; <ind>map&eogonek;</ind></trd>,
    <trd>nanosi&cacute; na <ind>map&eogonek;</ind></trd>,
    <trd>rysowa&cacute; <ind>plan</ind></trd>,
    <trd>nanosi&cacute; na <ind>plan</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_n;&c_je;&c_s;&c_t;&c_i; &c_n;&c_a;
  <ind>&c_k;&c_a;&c_r;&c_t;&c_u;</ind></trd>
</drv>

<drv mrk="map.mond0o">
  <kap>mond<tld/>o</kap>
  <snc mrk="map.mond0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="super" cel="map.0o.GEOG"><tld lit="M"/>o</ref>
      prezentanta
      la tutan konatan mondon, la tutan teron:
      <ekz>
        vi, kiel la magistro de la universitato, plej bone konas la
        mond<tld/>on de Cosmas
        <fnt><bib>SatirRak</bib>, <lok>supersti&ccirc;o</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="glob.plat0o">platglobo</ref>
    </refgrp>
  </snc>
  <trd lng="de">Weltkarte</trd>
  <trdgrp lng="fr">
    <trd>carte du monde</trd>,
    <trd>planisph&egrave;re</trd>
  </trdgrp>
  <trd lng="pl">mapa &sacute;wiata</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_t;&c_a; &c_m;&c_i;&c_r;&c_a;</trd>
</drv>

<drv mrk="map.0klarigo">
  <kap>
    <tld/>klarigo
  </kap>
  <snc mrk="map.0klarigo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de <tld/> kie estas klarigitaj la uzitaj simboloj kaj la
      skalo.
    </dif>
    <trd lng="pl">legenda<klr> (mapy)</klr></trd>
  </snc>
</drv>
</art>
