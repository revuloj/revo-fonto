<drv mrk="map.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="map.0aro.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Libroforma <ref tip="super" cel="kolekt.0o.libr">kolekto</ref>
      da <ref tip="prt" cel="map.0o"><tld/>oj</ref>, kutime kun indeksoj
      kaj krominformoj:
      <ekz>
        <klr>(la revuo LOdE)</klr> instigas min trovi en la
        <tld/>aro anta&ubreve;e nekonatajn loknomojn, ekzemple,
        <em>Jenisejo</em>
        <fnt>
        <aut>A. P. Warren</aut>,
        <bib>LOdE</bib>, <lok>numero 71 (2000:8&ndash;9)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_l;&c_a;&c_s;
  <klr>(&c_g;&c_je;&c_a;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ch;&c_n;&c_y;)</klr></trd>
  <trd lng="br">atlas</trd>
  <trd lng="ca">atles</trd>
  <trd lng="cs">atlas <klr>(map)</klr></trd>
  <trdgrp lng="de">
