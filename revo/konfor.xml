<drv mrk="konfor.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="konfor.0ismo.KOMUNE">
    <dif>
      Sinteno <tld/>a al la ordinaraj moroj, harmonia kun la medio:
      <ekz>
        la kalifornia urbo <nom>Berkeley</nom> restas fama pro siaj toleremo kaj
        ne-<tld/>ismo
        <fnt>
          <aut>I. Onet</aut>
          <vrk><url ref="http://www.eventoj.hu/arkivo/eve-098.htm">Rajton je nuda brusto!</url></vrk>,
          <lok>Eventoj, 1996-03, numero 98a</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne<tld/>ismaj programoj estas barataj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006909.html">Japanio: Takasugi Sinsaku: Novjara kla&ccirc;o super konjako</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ismo estus la&ubreve; mi fermi la okulojn pri iuj problemoj, ofte pro
        <klr>[&#x2026;]</klr> pura memcenzuro
        <fnt>
          <bib>Monato</bib>
          <aut>Aimo Rantanen</aut>
          <vrk><url ref="&Monato;/2004/008379.html">Senkura&gcirc;o malbonas kiel konsilanto</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="ant">
      <ref cel="ekstra1.0o">ekstravaganco</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="formal.0ismo">formalismo</ref>
    </refgrp>
    <trd lng="de">Konformismus</trd>
  </snc>
  <trd lng="cs">konformizmus</trd>
  <trd lng="fr">conformisme</trd>
  <trd lng="pl">konformizm</trd>
  <trdgrp lng="ro">
    <trd>conformism</trd>,
    <trd>conformitate</trd>
  </trdgrp>
  <trd lng="sk">konformizmus</trd>
</drv>

<drv mrk="konfor.sin0igi">
  <kap>sin <tld/>igi, <var><kap>sin al<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konfor.sin0igi.KOMUNE">
    <dif>
      Igi sin <ref cel="konfor.0a.obea"><tld/>a <sncref/></ref>:
      <ekz>
        la presejoj kaj telegrafoj devas sin <tld/>igi al la lingvo, kiun ili uzas
        <fnt>
          <bib>LR</bib>
          <lok>1. Alfabeto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni devas peni <tld/>igi nin al la ekzistantaj kutimoj
        <fnt>
          <bib>LR</bib>
          <lok>11. Verboj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili al<tld/>igis sin al la lo&gcirc;ejo
        <fnt>
          <bib>Fab3</bib>
          <lok>Kvin en unu silikvo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>conformer</ind></trd>,
    <trd>se mettre en <ind>conformit&eacute;</ind></trd>,
    <trd>s'<ind>accorder</ind><klr tip="ind"> (s'adapter)</klr></trd>,
    <trd>ob&eacute;ir<klr tip="ind"> (se mettre en conformit&eacute;)</klr></trd>,
    <trd>se <ind>plier</ind><klr tip="ind"> (se conformer)</klr></trd>,
    <trd>se <ind>r&eacute;gler</ind><klr tip="ind"> (se conformer)</klr></trd>,
    <trd>se <ind>soumettre</ind><klr tip="ind"> (se
    conformer)</klr></trd>
  </trdgrp>
  <trd lng="pl">przystosowywa&cacute; si&eogonek;</trd>     
  <trd lng="ro">adapta</trd>
</drv>

<drv mrk="konfor.0igxi">
  <kap><tld/>i&gcirc;i, <var><kap>al<tld/>i&gcirc;i</kap></var></kap>
  <snc mrk="konfor.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <ref cel="konfor.0a.KOMUNE"><tld/>a <sncref/></ref>:
      <ekz>
        ne <tld/>i&gcirc;u al &ccirc;i tiu mondo, sed aliformi&gcirc;u per renovigado de via menso
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 12:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i al la higienaj postuloj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;i <klr>[&#x2026;]</klr> al la opinioj kaj deziroj de la plimulto
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <klr>[&#x2026;]</klr> komencis danci, sed &scirc;i tute ne povis al<tld/>i&gcirc;i al la takto
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta &scirc;tato <tld/>i&gcirc;as al lia re&gcirc;a volo
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 5a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akord.0igxi">akordi&gcirc;i</ref>,
      <ref cel="ced.0i">cedi</ref>,
      <ref cel="fleks.0igxi.cedi">fleksi&gcirc;i</ref>,
      <ref cel="met.sub0igxi">submeti&gcirc;i</ref>
    </refgrp>
    <trd lng="de">sich <ind>anpassen</ind></trd>
  </snc>
  <trdgrp lng="fr">
    <trd>se <ind>conformer</ind></trd>,
    <trd>se mettre en <ind>conformit&eacute;</ind></trd>,
    <trd>s'<ind>accorder</ind><klr tip="ind"> (s'adapter)</klr></trd>,
    <trd>ob&eacute;ir<klr tip="ind"> (&ecirc;tre en conformit&eacute;)</klr></trd>,
    <trd>se <ind>plier</ind><klr tip="ind"> (se conformer)</klr></trd>,
    <trd>se <ind>r&eacute;gler</ind><klr tip="ind"> (se conformer)</klr></trd>,
    <trd>se <ind>soumettre</ind><klr tip="ind"> (se
    conformer)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>illeszkedik</trd>,
    <trd>&ouml;sszhangba ker&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>adaptowa&cacute;</trd>,
    <trd>przystosowywa&cacute;</trd>
  </trdgrp> 
  <trd lng="ro">adapta</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_g;&c_l;&c_a;&c_s;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_s; &c_ch;&c_je;&c_m;-&c_l;.)</klr></trd>,
    <trd>&c_s;&c_o;&c_o;&c_b;&c_r;&c_a;&c_z;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_s; &c_ch;&c_je;&c_m;-&c_l;.)</klr></trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_p;&c_o;&c_s;&c_a;&c_b;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_k; &c_ch;&c_je;&c_m;&c_u;-&c_l;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="konfor.cel0a">
