<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>grimac</rad>/o
</kap>
<drv mrk="grimac.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Vola a&ubreve; nevola stre&ccirc;o de la viza&gcirc;aj muskoloj, kiu
      misformanta la trajtojn donante al ili neordinaran kaj ofte ridindan
      aspekton:
      <ekz>
        <tld/>o de moko, de doloro.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Grimmasse</trd>
  <trd lng="en">grimace</trd>
  <trd lng="fr">grimace</trd>
  <trd lng="ru">&c_g;&c_r;&c_i;&c_m;&c_a;&c_s;&c_a;</trd>
</drv>
<drv mrk="grimac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>ojn:
      <ekz>
        kiu <tld/>as per la okulo ka&ubreve;zas suferon
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Sentencoj 10, 10</lok>)</fnt>;
      </ekz>
      <ekz>
        <tld/>anta masko.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Grimmassen ziehen</trd>
  <trd lng="fr">grimacer</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; &c_g;&c_r;&c_i;&c_m;&c_a;&c_s;&c_y;</trd>,
    <trd>&c_g;&c_r;&c_i;&c_m;&c_a;&c_s;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
<drv mrk="grimac.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Kutimanta <tld/>i.
    </dif>
  </snc>
  <trd lng="fr">grimacier</trd>
</drv>
</art>

