<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: grimac.xml,v 1.22 2022/07/05 06:35:26 revo Exp $">
<kap><ofc>*</ofc>
<rad>grimac</rad>/o</kap>
<drv mrk="grimac.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Vola a&ubreve; nevola stre&ccirc;o de la viza&gcirc;aj muskoloj,
      kiu misformanta la trajtojn donante al ili neordinaran kaj ofte
      ridindan aspekton:
      <ekz>
        <tld/>o de abomeno
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la grafo <klr>[&#8230;]</klr> faris <tld/>on de doloro
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kio estas al vi? kial vi faras <tld/>ojn?
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo, la orazeno kaj bastono el sako</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi &ccirc;iam devas gaje petoli, fari <tld/>ojn por ke la homoj ridu
        <fnt>
          <bib>ElektFab</bib>
          <lok>La vivolongo</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la mallar&gcirc;aj lipoj ironio anstata&ubreve;is la maldol&ccirc;an <tld/>on
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        trae de &ccirc;iuj viaj <tld/>oj mi vidis la verecon <klr>[&#8230;]</klr> kiom da respekto
        vi havas
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li metante la monon en sian bu&scirc;on, montris per penaj ma&ccirc;<tld/>oj, ke el tiu ne
        eblas kuiri man&gcirc;a&jcirc;on
        <fnt>
          <bib>VaK</bib>
          <lok>Dua &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="karika.0o">karikaturo</ref>,
      <ref cel="mask.0o">masko</ref>,
      <ref cel="mien.0o">mieno</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_y;&c_m;&c_a;&c_s;&c_a;</trd>
  <trd lng="bg">&c_g;&c_r;&c_i;&c_m;&c_a;&c_s;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>ganyota</trd>,
    <trd>carassa</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>grimasa</trd>,
    <trd>pitvo&rcaron;en&iacute;</trd>,
    <trd>&uacute;&scaron;klebek</trd>,
    <trd>&scaron;kleben&iacute;</trd>
  </trdgrp>
  <trd lng="de">Grimmasse</trd>
  <trd lng="en">grimace</trd>
  <trd lng="es">mueca</trd>
  <trd lng="fr">grimace</trd>
  <trdgrp lng="hu">
    <trd>fintor</trd>,
    <trd>grimasz</trd>
  </trdgrp>
  <trd lng="nl">grimas</trd>
  <trdgrp lng="pl">
    <trd>grymas</trd>,
    <trd>d&aogonek;s</trd>,
    <trd>foch</trd>
  </trdgrp>
  <trd lng="pt">careta</trd>
  <trd lng="ru">&c_g;&c_r;&c_i;&c_m;&c_a;&c_s;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>grimasa</trd>,
    <trd>&uacute;&scaron;k&lcaron;abok</trd>
  </trdgrp>
  <trd lng="uk">&c_gu;&c_r;&c_i;&c_m;&c_a;&c_s;&c_a;</trd>
</drv>

<drv mrk="grimac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>ojn:
      <ekz>
        kiu <tld/>as per la okulo ka&ubreve;zas suferon
        <fnt><bib>MT</bib>, <lok>&Sen; 10:10</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>anta statuo
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsekorelaj buboj <klr>[&#8230;]</klr> plor<tld/>as pri la venkoj de Scipiono, &ccirc;ar
        ili devas raporti pri ili lecionojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        Maziero <klr>[&#8230;]</klr> ne povis a&ubreve;di tiun cinikan rakonton, kaj samtempe ne
        <tld/>i pro na&ubreve;zo
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is pro tedo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam Matildo montris al siaj gepatroj sian novan protektiton, &ccirc;i tiuj unue
        ek<tld/>is pro mal&scirc;ato
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mien.0i">mieni</ref>,
      <ref cel="pauxt.0i">pa&ubreve;ti</ref>,
      <ref cel="rikan.0i">rikani</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_y;&c_m;&c_a;&c_s;&c_mol;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_y;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_g;&c_r;&c_i;&c_m;&c_a;&c_s;&c_n;&c_i;&c_ch;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_ja; &c_g;&c_r;&c_i;&c_m;&c_a;&c_s;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">fer ganyotes</trd>
  <trdgrp lng="cs">
    <trd>d&ecaron;lat &uacute;&scaron;klebky</trd>,
    <trd>grimasovat</trd>,
    <trd>pitvo&rcaron;it se</trd>,
    <trd>u&scaron;kl&iacute;bat se</trd>,
    <trd>u&scaron;kl&iacute;bnout se</trd>,
    <trd>&scaron;klebit se</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>eine <ind>Grimmasse</ind> ziehen</trd>,
    <trd>zur Grimmasse <ind>verzerren</ind></trd>
  </trdgrp>
  <trd lng="es">hacer muecas</trd>
  <trd lng="fr">grimacer</trd>
  <trdgrp lng="hu">
    <trd>fintorog</trd>,
    <trd>grimaszol</trd>
  </trdgrp>
  <trd lng="nl">grimassen maken</trd>
  <trdgrp lng="pl">
    <trd>grymasi&cacute;</trd>,
    <trd>d&aogonek;sa&cacute; si&eogonek;</trd>,
    <trd>stroi&cacute; <ind>fochy</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>fazer <ind>careta</ind></trd>, 
   <trd>caretear</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
    &c_g;&c_r;&c_i;&c_m;&c_a;&c_s;&c_y;</trd>,
    <trd>&c_g;&c_r;&c_i;&c_m;&c_a;&c_s;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">robi&tcaron; grimasy</trd>
  <trdgrp lng="uk">
    <trd>&c_gu;&c_r;&c_i;&c_m;&c_a;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_v;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_k;&c_r;&c_i;&c_v;&c_l;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="grimac.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Kutimanta <tld/>i.
    </dif>
  </snc>
  <trd lng="ca">ganyotaire</trd>
  <trd lng="fr">grimacier</trd>
  <trdgrp lng="pl">
    <trd>gryma&sacute;ny</trd>
  </trdgrp>
  <trd lng="pt">careteiro</trd>
</drv>
</art>
