<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kask</rad>/o
</kap>
<drv mrk="kask.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Metala a&ubreve; leda kap&scirc;irmilo por soldatoj, ministoj, konstrulaboristoj,
      motorciklantoj kaj aliaj:
      <ekz>
        barcelonaj oficiroj kun brilaj uniformoj kaj blankaj plumoj sur la
        <tld/>oj
        <fnt><bib>Aventuroj</bib>, <lok>&ccirc;apitro 14a, p. 41a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_o;&c_m;</trd>
  </trdgrp>                  
  <trd lng="br">tokarn</trd>
  <trd lng="ca">casc</trd>
  <trd lng="de">Helm</trd>
  <trd lng="en">helmet</trd>
  <trd lng="es">casco</trd>
  <trd lng="fr">casque</trd>
  <trd lng="hu">sisak</trd>
  <trd lng="nl">helm</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_l;&c_je;&c_m;</trd>
  </trdgrp>
</drv>
</art>
