<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: helium.xml,v 1.33 2025/07/20 08:00:23 revo Exp $">
<kap>
  <rad>helium</rad>/o <fnt><bib>JED</bib></fnt>,
  <var><kap><rad var="i">heli</rad>/o <fnt><bib>VdE</bib></fnt></kap></var>
</kap>
<drv mrk="helium.0o">
  <kap>
    <tld/>o,
    <var><kap><tld var="i"/>o</kap></var>
  </kap>
  <mlg kod="IUPAC">He</mlg>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="2">elemento</ref>
      kun
      atomnumero 2, atompezo 4,0026;
      <ref tip="super" cel="gas.nobla0o">nobla gaso</ref>, senkolora,
      nebrulebla, malpli peza ol aero kaj pro tio preferata gaso por
      zepelinoj:
      <ekz>
        alfa-radiado konsistas el rapidflugaj atomkernoj de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2011/010370.php">Moderna minaco: radioaktiveco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="helios1.0o">Helio (sundio)</ref>
  <trd lng="be">&c_g;&c_je;&c_l;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_h;&c_je;&c_l;&c_i;&c_j;</trd>
  <trd lng="br">hiliom</trd>
  <trd lng="ca">heli</trd>
  <trd lng="cv">&c_g;&c_je;&c_l;&c_i;</trd>
  <trd lng="de">Helium</trd>
  <trd lng="el">&eta_ton;&lambda;&jota;&omikron;</trd>
  <trd lng="en">helium</trd>
  <trd lng="es">helio</trd>
  <trd lng="fa">&a_h;&a_l;&f_y;&a_damma;&a_m;</trd>
  <trd lng="fr">h&eacute;lium</trd>
  <trd lng="hu">h&eacute;lium</trd>
  <trd lng="id">helium</trd>
  <trd lng="it">elio</trd>
  <trd lng="ja">&#12504;&#12522;&#12454;&#12512;</trd>
  <trd lng="la">Helium</trd>
  <trd lng="nl">helium</trd>
  <trd lng="pl">hel</trd>
  <trd lng="pt">h&eacute;lio</trd>
  <trd lng="ru">&c_g;&c_je;&c_l;&c_i;&c_j;</trd>
  <trd lng="sv">helium</trd>
  <trd lng="tr">helyum</trd>
  <trd lng="uk">&c_g;&c_je;&c_l;&c_ib;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#27686; <pr>h&agrave;i</pr></trd>,
    <trd>&#27686;&#27668; <pr>h&agrave;iq&igrave;</pr></trd>
  </trdgrp>
</drv>
</art>
