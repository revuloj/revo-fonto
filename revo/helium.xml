<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>helium</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="helium.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun atomnumero 2, atompezo 4,0026, simbolo He. Alia ofta E-a nomo: helio.
      <ref tip="super" cel="gas.nobla0o">Nobla gaso</ref>, senkolora, nebrulebla, malpli peza ol aero kaj pro tio preferata gaso por zepelinoj.
    </dif>
  </snc>
  <trd lng="de">Helium</trd>
  <trd lng="en">helium</trd>
  <trd lng="es">helio</trd>
  <trd lng="fr">h&eacute;lium</trd>
  <trd lng="it">elio</trd>
  <trd lng="nl">helium</trd>
  <trd lng="ru">&c_g;&c_je;&c_l;&c_i;&c_j;</trd>
  <trd lng="tr">helyum</trd>
</drv>
</art>
