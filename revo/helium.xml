<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>helium</rad>/o
  <fnt>Z</fnt>
</kap>
<mlg>He</mlg>
<drv mrk="helium.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj">elemento</ref> kun
      atomnumero 2, atompezo 4,0026, simbolo <frm>He</frm>.
      <ref tip="super" cel="gas.nobla0o">Nobla gaso</ref>, senkolora,
      nebrulebla, malpli peza ol aero kaj pro tio preferata gaso por
      zepelinoj.
    </dif>
    <ref tip="sin" cel="heli.0o">helio.</ref>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_l;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_h;&c_je;&c_l;&c_i;&c_j;</trd>
  <trd lng="br">hiliom</trd>
  <trd lng="de">Helium</trd>
  <trd lng="el">&eta_ton;&lambda;&jota;&omikron;</trd>
  <trd lng="en">helium</trd>
  <trd lng="es">helio</trd>
  <trd lng="fa">&a_h;&a_l;&f_y;&a_damma;&a_m;</trd>
  <trd lng="fr">h&eacute;lium</trd>
  <trd lng="hu">h&eacute;lium</trd>
  <trd lng="it">elio</trd>
  <trd lng="la">Helium</trd>
  <trd lng="nl">helium</trd>
  <trd lng="pl">hel</trd>
  <trd lng="pt">h&eacute;lio</trd>
  <trd lng="ru">&c_g;&c_je;&c_l;&c_i;&c_j;</trd>
  <trd lng="sv">helium</trd>
  <trd lng="tr">helyum</trd>
</drv>
</art>
