<drv mrk="manife.sin0i">
  <kap>sin <tld/>i, <var><kap><tld/>i&gcirc;i</kap></var></kap>
  <snc mrk="manife.sin0i.KOMUNE">
    <dif>
      Sin <ref cel="montr.0igxi">montri</ref> klare, <ref tip="super" cel="aper.0i">aperi</ref>
      evidente:
      <ekz>
        la konfesantoj komencis kunvenadi, por vidi tiun, al kiu klare <tld/>i&gcirc;is la
        favoro de Kristo
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        novnaciismo <klr>[&#8230;]</klr> <tld/>i&gcirc;is en diversaj landoj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        la evoluo de Esperanto precipe <tld/>i&gcirc;is en la plimulti&gcirc;o de la radikaro
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        ju pli oni tiel esploris la esprimeblecojn, des pli sin <tld/>is ri&ccirc;e variaj kaj
        koncize trafaj parolturnoj
        <fnt>
          <bib>Plumamikoj</bib>
        </fnt>;
      </ekz>
      <ekz>
        entuziasmo eksplode <tld/>i&gcirc;as per apla&ubreve;degoj
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi priskribu detale, en kio <tld/>i&gcirc;as la sindono en la amo
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aper.el0i">elaperi</ref>,
      <ref cel="emergx.0i">emer&gcirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_ja;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_y;&c_r;&c_a;&c_zh;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>sich <ind>manifestieren</ind></trd>,
    <trd>sich <ind>zeigen</ind></trd>,
    <trd>in <ind>Erscheinung</ind> treten</trd>
  </trdgrp>
  <trd lng="en">manifest</trd>
  <trd lng="fr">se <ind>manifester</ind></trd>
  <trdgrp lng="hu">
    <trd>megnyilv&aacute;nul</trd>,
    <trd>kifejez&eacute;sre jut</trd>,
    <trd>kifejez&odblac;dik</trd>,
    <trd>manifeszt&aacute;l&oacute;dik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x660E;&#x793A;&#12373;&#x308C;&#x308B;  <pr>&#12417;&#12356;&#12376;&#12373;&#x308C;&#x308B;</pr></trd>,
    <trd>&#34920;&#x308C;&#x308B;  <pr>&#12354;&#12425;&#x308F;&#x308C;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">zich <ind>manifesteren</ind></trd>
  <trdgrp lng="pl">
    <trd>manifestowa&cacute; si&eogonek;</trd>,
    <trd>przejawia&cacute; si&eogonek;</trd>,
    <trd>widnie&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
