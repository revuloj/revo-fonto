  </trdgrp>
  <trdgrp lng="ro">
    <trd>se manifest&abreve;</trd>,
    <trd>se ivi</trd>,
    <trd>ar&abreve;ta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_ja;&c_v;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_n;&c_a;&c_r;&c_u;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
