<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: benefi.xml,v 1.18 2023/08/31 08:48:50 revo Exp $">
<kap>
  <rad>benefic</rad>/o
</kap>
<drv mrk="benefi.0o">
  <kap><tld/>o</kap>
  <snc mrk="benefi.0o.avantagxo">
    <dif>
      <ref tip="super" cel="avanta.0o">Avanta&gcirc;o</ref>, ricevata anstata&ubreve; 
      a&ubreve; aldone de salajro por iu ofico:
      <ekz>
        ne ekzistas ofico sen <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        laborista <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=99652">Enspezo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la papo insistis ke li estu ordinata, kaj pro tio li demisiis, tamen li da&ubreve;re
        retenis multajn ekleziajn <tld/>ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=272610">Gotfredo de Jorko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Geldwerter <ind>Vorteil</ind></trd>
  </snc>
  <snc mrk="benefi.0o.prezento">
    <dif>
      <ref tip="super" cel="prezen1.0ado">Prezentado</ref>
      (spektaklo, sporto, koncerto) profite al iu aktoro
      a&ubreve; socia, filantropia entrepreno:
      <ekz>
        <nom>Emily Barlaston</nom> <klr>[&#x2026;]</klr> partoprenis en aidoso-<tld/>oj kaj aperis en la
        fama Roza Sabato en <nom>Musis Sacrum</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1327">Emily Barlaston</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_e;&c_n;&c_e;&c_f;&c_ib;&c_s;</trd>
    <trd lng="bg">&c_b;&c_je;&c_n;&c_je;&c_f;&c_i;&c_s;</trd>
    <trdgrp lng="cs">
      <trd>benefice</trd>,
      <trd>benefi&ccaron;n&iacute; p&rcaron;edstaven&iacute;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Benefizveranstaltung</trd>,
      <trd>Wohlt&auml;tigkeitsveranstaltung</trd>
    </trdgrp>
    <trd lng="en">benefit</trd>
    <trd lng="es">funci&oacute;n ben&eacute;fica</trd>
    <trd lng="fr">spectacle de bienfaisance</trd>
    <trd lng="hu">j&oacute;t&eacute;konys&aacute;gi rendezv&eacute;ny</trd>
    <trd lng="pl">benefis</trd>
    <trd lng="sk">benefi&ccaron;n&eacute; predstavenie</trd>
    <trd lng="ru">&c_b;&c_je;&c_n;&c_je;&c_f;&c_i;&c_s;</trd>
  </snc>
  <trd lng="he">&#x5D4;&#x5E6;&#x5D2;&#x5EA; &#x5DB;&#x5D1;&#x5D5;&#x5D3;</trd>
  <trdgrp lng="ja">
    <trd>&#x6148;&#x5584;&#x8208;&#x884C;  [<ind>&#x3058;&#x305C;&#x3093;&#x3053;&#x3046;&#x304E;&#x3087;&#x3046;</ind>]</trd>,
    <trd>&#x7279;&#x5178;  [<ind>&#x3068;&#x304F;&#x3066;&#x3093;</ind>]</trd>,
    <trd>&#x7279;&#x6A29;  [<ind>&#x3068;&#x3063;&#x3051;&#x3093;</ind>]</trd>
  </trdgrp>
  <trd lng="uk">&#x431;&#x435;&#x43D;&#x435;&#x444;&#x456;&#x441;</trd>
</drv>
</art>
