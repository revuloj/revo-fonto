<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kahel.xml,v 1.27 2015/10/13 13:10:20 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>kahel</rad>/o
</kap>
<drv mrk="kahel.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">KON</uzo>
   <snc>
     <dif>
       Plato el bakita kaj glazurita argilo, a&ubreve; simila
       materialo, kiun oni uzas por pavimi a&ubreve; tegi (surfacojn
       en domo, fornon ktp):
       <ekz>
         la nobla sinjorino akceptis la filon en kabineto,
         ornamita per fajencaj <tld/>oj
         <fnt><bib>Far1</bib><lok>&ccirc;ap. 7</lok></fnt>;
       </ekz>
       <ekz>
         sur unu muro, kovrita per emajlitaj <tld/>oj, trovi&gcirc;is
         brilkolora bildo de granda monstro
         <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 6</lok></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="cerami.0o">ceramiko</ref>,
       <ref cel="fajenc.0o">fajenco</ref>.
     </refgrp>
     <rim>
       La difino en <fnt><bib>PV</bib></fnt> estas pli preciza
       kaj pli vasta:
       <ctl>plato el ia ajn materi[al]o, krom ligno...</ctl>
     </rim>
   </snc>
  <trd lng="be">&c_k;&c_a;&c_f;&c_l;&c_ja;</trd>
  <trd lng="br">karrell</trd>
  <trdgrp lng="cs">
    <trd>kachel</trd>,
    <trd>kachl&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Kachel</trd>
  <trdgrp lng="en">
     <trd>tile</trd>,
     <trd>paving block</trd>,
     <trd>paver</trd>,
     <trd>flag</trd>
   </trdgrp>
  <trdgrp lng="es">
     <trd>baldosa</trd>,
     <trd>losa</trd>
   </trdgrp>
  <trdgrp lng="fi">
     <trd><klr tip="amb">(keramiikka)</klr>laatta</trd>,
     <trd>kaakeli</trd>
   </trdgrp>
  <trdgrp lng="fr">
     <trd>carreau<klr tip="ind"> (de carrelage)</klr></trd>,
     <trd>dalle<klr tip="ind"> (pour un dallage)</klr></trd>
   </trdgrp>
  <trd lng="hu">csempe</trd>
  <trdgrp lng="nl">
     <trd>tegel</trd>,
     <trd>tichel</trd>
   </trdgrp>
  <trdgrp lng="pl">
     <trd>kafel<klr> (du&zdot;y ok&lstroke;adzinowy lub piecowy)</klr>
     </trd>,
     <trd>kafelek<klr> (rzadko piecowy)</klr></trd>
   </trdgrp>
  <trdgrp lng="pt">
     <trd>ladrilho</trd>,
     <trd>azulejo</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_k;&c_a;&c_f;&c_je;&c_l;&c_mol;</trd>,
     <trd>&c_i;&c_z;&c_r;&c_a;&c_z;&c_je;&c_c;</trd>,
     <trd>&c_p;&c_l;&c_i;&c_t;&c_k;&c_a;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>dla&zcaron;dica</trd>,
    <trd>kachli&ccaron;ka</trd>,
    <trd>kach&lcaron;a</trd>,
    <trd>obklada&ccaron;ka</trd>
  </trdgrp>
  <trd lng="sv">kakel</trd>
  <trd lng="tr">d&ouml;&scedil;eme ta&scedil;&inodot;</trd>
</drv>

<drv mrk="kahel.0a">
   <kap><tld/>a</kap>
   <snc>
     <dif>
       Rilata al <tld/>o.
     </dif>
     <trd lng="hu">csempe-</trd>
     <trd lng="nl">tegel-</trd>
     <trdgrp lng="pl">
       <trd>kaflowy <klr> (piec)</klr></trd>,
       <trd>kafelkowy</trd>
     </trdgrp>
   </snc>
   <snc>
     <dif>
       Tegita per <tld/>oj:
       <ekz>
         la poeto sidis varme kaj komforte &ccirc;e sia <tld/>a forno
         <fnt><bib>Fab1</bib><lok>Malbonkonduta knabo</lok></fnt>.
       </ekz>
     </dif>
     <trd lng="br">karrellet</trd>
     <trdgrp lng="de">
       <trd>gekachelt</trd>,
       <trd>Kachel-</trd>
     </trdgrp>
     <trdgrp lng="fi">
       <trd>laatta-</trd>,
       <trd>laatoitettu</trd>,
       <trd>kaakeli-</trd>
     </trdgrp>
     <trd lng="fr">carrel&eacute;</trd>
     <trd lng="hu">csemp&eacute;zett</trd>
     <trd lng="nl">betegeld</trd>
     <trdgrp lng="pl">
       <trd>kaflowy<klr> (piec)</klr></trd>,
       <trd>kafelkowany</trd>,
       <trd>wykafelkowany</trd>,
       <trd>okaflowany</trd>
     </trdgrp>
   </snc>
   <trd lng="en">tiled</trd>
   <trd lng="ru">&c_k;&c_a;&c_f;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="kahel.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <fnt><bib>PIV1</bib></fnt>
   <snc>
     <dif>
       Tegi per <tld/>oj.
     </dif>
   </snc>
   <trd lng="br">karrella&ntilde;</trd>
   <trd lng="en">to tile</trd>
   <trd lng="es">enlosar</trd>
   <trd lng="fr">carreler</trd>
   <trd lng="fi">laatoittaa</trd>
   <trd lng="hu">csemp&eacute;z</trd>
   <trd lng="nl">betegelen</trd>
   <trdgrp lng="pl">
     <trd>kaflowa&cacute;<klr> (piec)</klr></trd>,
     <trd>kafelkowa&cacute;<klr>
     (pod&lstroke;og&eogonek;, &sacute;cian&eogonek;)</klr></trd>,
     <trd>uk&lstroke;ada&cacute; <ind>kafelki</ind><klr>
     (na pod&lstroke;odze, &sacute;cianie)</klr></trd>
   </trdgrp>
   <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;
   <ind>&c_k;&c_a;&c_f;&c_je;&c_l;&c_je;&c_m;</ind></trd>
   <trd lng="tr">ta&scedil; d&ouml;&scedil;emek</trd>
</drv>

<drv mrk="kahel.0aro">
   <kap><tld/>aro</kap>
   <uzo tip="fak">KON</uzo>
   <fnt><bib>PIV1</bib></fnt>
   <snc>
     <dif>
       Tuto de la <tld/>oj, tegantaj iun surfacon.
     </dif>
   </snc>
  <trd lng="br">karrelladur</trd>
  <trd lng="cs">dla&zcaron;ba</trd>
  <trd lng="en">tiling</trd>
  <trd lng="fi">laatoitus <klr tip="amb"> (laatoitettu pinta)</klr>
   </trd>
  <trd lng="fr">carrelage</trd>
  <trd lng="hu">csempeburkolat</trd>
  <trd lng="nl">tegelwerk</trd>
  <trd lng="pl">kafelkowanie</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_t;&c_i;&c_l;
   &c_i;&c_z; &c_k;&c_a;&c_f;&c_je;&c_l;&c_ja;</trd>
  <trd lng="sk">dla&zcaron;ba</trd>
  <trd lng="tr">ta&scedil; d&ouml;&scedil;eme</trd>
</drv>
</art>
