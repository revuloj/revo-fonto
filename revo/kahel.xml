<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kahel</rad>/o
</kap>
<drv mrk="kahel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Plato el ia ajn materio, krom ligno, kiun oni uzas por
      pavimi a&ubreve; tegi internon de domo, fornon ktp. 
    </dif>
    <refgrp tip="sin">
      <ref cel="cerami.0o">ceramiko</ref>,
      <ref cel="fajenc.0o">fajenco</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Kachel</trd>
  <trd lng="nl">tegel</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_f;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_i;&c_z;&c_r;&c_a;&c_z;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="sv">kakel</trd>
</drv>
</art>
