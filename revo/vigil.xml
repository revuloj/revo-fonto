<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vigil.xml,v 1.11 2024/08/04 21:25:44 revo Exp $">
<kap>
  <rad>vigil</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="vigil.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ke><ref tip="dif" cel="vigl.0i.maldormi">Vigli<sncref/></ref></ke>,
      <ref tip="super" cel="dorm.mal0i">maldormi</ref> pro vola sindeteno:
      <ekz>
        &ccirc;i-nokte en la kapelo de &ccirc;i kastelo mi <tld/>os
        <fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 3a, p. 39a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gvat.0i.dejxore">gvati</ref>,
      <ref cel="vacx.0i">va&ccirc;i</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja; <ind>&c_s;&c_p;&c_a;&c_c;&c_mol;</ind></trd>,
    <trd>&c_d;&c_b;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bd&#xED;t</trd>,
    <trd>br&#xE1;nit se sp&#xE1;nku</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wachen</trd>,
    <trd><ind>wach</ind> bleiben</trd>,
    <trd>aufbleiben</trd>
  </trdgrp>
  <trd lng="es">velar</trd>
  <trd lng="fr">veiller<klr> (s'abstenir de dormir)</klr></trd>
  <trd lng="hu">virraszt</trd>
  <trd lng="pl">czuwa&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_o;&c_d;&c_r;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_d;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_v;&c_o;&c_z;&c_d;&c_je;&c_r;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_o;&c_t; &c_s;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="sk">bdie&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43D;&c_je; &c_s;&#x43F;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x5F85;&#x547D; <pr>d&#xE0;im&#xEC;ng</pr></trd>
</drv>
</art>

