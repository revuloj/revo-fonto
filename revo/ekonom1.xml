  <trd lng="fr">&eacute;conomiste</trd>
  <trd lng="nl">econoom</trd>
  <trd lng="no">&oslash;konom</trd>
  <trd lng="pl">ekonomista</trd>
  <trd lng="ro">economist</trd>
  <trd lng="sk">ekon&oacute;m</trd>
  <trd lng="uk">&#x433;&#x43E;&#x441;&#x43F;&#x43E;&#x434;&#x430;&#x440;&#x43D;&#x438;&#x43A;</trd>
</drv>

<drv mrk="ekonom1.merkat0o">
  <kap>merkat<tld/>o</kap>
  <snc mrk="ekonom1.merkat0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="E"/>o, en kiu regas precipe merkataj proponoj kaj
      postuloj, kaj kompaniaj decidoj:
      <ekz>
        merkat<tld/>o altnivele konkurkapabla
        <fnt>
        <aut>T. Lemaheux, trad. P. Levi</aut>,
        <vrk><url ref="http://satesperanto.free.fr/satkulturo/article.php3?id_article=153">Dek
        motivoj por rifuzi...</url></vrk>,
        <lok>L'Humanit&eacute;, 2004-10</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u la hodia&ubreve;a libera merkat<tld/>o neniel bremsas a&ubreve; e&ccirc;
        misgvidas la demokration
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://www.esperanto.be/fel/mon/007242.html">Supra&jcirc;e
          rozkolora</url></vrk>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_n;&c_k;&c_a;&c_v;&c_a;&c_ja;
  <ind>&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_k;&c_a;</ind></trd>
  <trd lng="ca"><ind>economia</ind> de mercat</trd>
  <trdgrp lng="cs">
    <trd>tr&zcaron;n&iacute; ekonomie</trd>,
    <trd>tr&zcaron;n&iacute; hospod&aacute;&rcaron;stv&iacute;</trd>
  </trdgrp>
  <trd lng="fr">&eacute;conomie de march&eacute;</trd>
  <trd lng="nl">markteconomie</trd>
  <trd lng="no">markeds&oslash;konomi</trd>
  <trd lng="pl">gospodarka rynkowa</trd>
  <trd lng="sk">tr&zcaron;n&eacute; hospod&aacute;rstvo</trd>
  <trd lng="uk">&#x440;&#x438;&#x43D;&#x43A;&#x43E;&#x432;&#x430; &#x435;&#x43A;&#x43E;&#x43D;&#x43E;&#x43C;&#x456;&#x43A;&#x430;</trd>
</drv>

<drv mrk="ekonom1.plan0o">
  <kap>plan<tld/>o</kap>
  <snc mrk="ekonom1.plan0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="E"/>o, en kiu precipe la &scirc;tato planas kaj direktas
      la
      produktadon kaj &gcirc;ian evoluon:
      <ekz>
        La diferenco inter plan<tld/>o kaj merkatekonomio fakte ja ne
        konsistas en tio, ke la unua planas, kaj la dua ne, sed la
        maniero
        de la planado
        <fnt>
        <aut>M. Behr k. a.</aut>,
        <vrk><url ref="http://www.fw.hu/eventoj/steb/direktado_kaj_ekonomio/smerkat.htm">Enkonduko
        en la socialan merkat-ekonomion</url></vrk>,
        <lok><url ref="http://www.fw.hu/eventoj/steb/">Steb</url>,
        1993</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kuba &scirc;tato ali&gcirc;as al la socialismaj principoj
        <klr>[...]</klr> 
        de <klr>[...]</klr> plejparte &scirc;tatkontrolita planekonomio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2224">Kubo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_l;&c_ja;&c_n;&c_a;&c_v;&c_a;&c_ja;
  <ind>&c_e;&c_k;&c_a;&c_n;&c_o;&c_m;&c_ib;&c_k;&c_a;</ind></trd>
  <trd lng="ca"><ind>economia</ind> planificada</trd>
  <trd lng="fr">&eacute;conomie planifi&eacute;e</trd>
  <trd lng="nl">planeconomie</trd>
  <trd lng="no">plan&oslash;konomi</trd>
  <trd lng="pl">gospodarka planowa</trd>
  <trd lng="uk">&#x43F;&#x43B;&#x430;&#x43D;&#x43E;&#x432;&#x430; &#x435;&#x43A;&#x43E;&#x43D;&#x43E;&#x43C;&#x456;&#x43A;&#x430;</trd>
</drv>

<drv mrk="ekonom1.O0ajKE">
  <kap>Organiza&jcirc;o por <tld lit="E"/>aj Kunlaboro kaj Evoluigo
  <fnt><bib>ManPol</bib></fnt></kap>
  <mlg>OEKE</mlg>
  <snc mrk="ekonom1.O0ajKE.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Speciala <ref tip="lst" cel="organi2.0ajxo" lst="voko:organiza&jcirc;oj">organiza&jcirc;o</ref>
      de <ref tip="malprt" cel="naci.Unuigxintaj_0oj">Unui&gcirc;intaj
      Nacioj</ref>, kiu celas plani, kunordigi kaj profundigi <tld/>ajn
      kunlaboron kaj evoluigon:
      <ekz>
        la listo de landoj, kiuj disponigas favorajn impostojn, estas
        kunmetita de Organiza&jcirc;o por <tld lit="E"/>aj Kunlaboro kaj
        Evoluigo (OEKE), kriterio estis aliro al la imposta konkurenco,
        kiu en tiuj landoj, la&ubreve; opinio de OEKE, estas maljusta
        kaj nelojala
        <fnt>
        <aut>Evgeni GEORGIEV</aut>,
        <vrk><url ref="&Monato;/007128.html">&Ccirc;u venos fino de la
        impostaj paradizoj?</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000-10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;
    &c_e;&c_k;&c_a;&c_n;&c_a;&c_m;&c_ib;&c_ch;&c_n;&c_a;&c_g;&c_a;
    &c_s;&c_u;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;
    &c_ib;
    &c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_c;&c_mol;&c_c;&c_ja;</trd>,
    <trd>&c_A;&c_E;&c_S;&c_R;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>Organitzaci&oacute; de Cooperaci&oacute; i Desenvolupament Econ&ograve;mic</trd>,
    <trd>OCDE</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Organisation f&uuml;r Wirtschaftliche Zusammenarbeit</trd>,
    <trd>OECD</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>Organisation for <ind>Economic</ind> Co-operation and
    Development</trd>,
    <trd>OECD</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Organisation de coop&eacute;ration et de d&eacute;veloppement
    <ind>&eacute;conomique</ind></trd>,
    <trd>OCDE</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Gazdas&aacute;gi Egy&uuml;ttm&udblac;k&ouml;d&eacute;si
    &eacute;s
    Fejleszt&eacute;si Szervezet</trd>,
    <trd>OECD</trd>
  </trdgrp><trdgrp lng="nl">
    <trd>Organisatie voor Economische Samenwerking en
    Ontwikkeling</trd>,
    <trd>OESO</trd>
  </trdgrp>
  <trdgrp lng="no">
    <trd>Organisasjon for &Oslash;konomisk Samarbeid og Utvikling</trd>,
    <trd>OECD</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>Organizacja Wsp&oacute;&lstroke;pracy <ind>Gospodarczej</ind> i
    Rozwoju</trd>,
    <trd>OECD</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>Organiza&ccedil;&atilde;o para a Coopera&ccedil;&atilde;o
    <ind>Econ&ocirc;mica</ind> e o
    Desenvolvimento</trd>,
    <trd>OCED</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_O;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;
    &c_e;&c_k;&c_o;&c_n;&c_o;&c_m;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_g;&c_o;
    &c_s;&c_o;&c_t;&c_r;&c_u;&c_d;&c_n;&c_i;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_a;
    &c_i; &c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_i;&c_ja;</trd>,
    <trd>&c_O;&c_E;&c_S;&c_R;</trd>
  </trdgrp>
</drv>
</art>
