<drv mrk="ekonom1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        kiel <tld/>istoj <klr>[...]</klr> &gcirc;i rigardas homojn kaj naciojn
        kiel raciaj kaj egoismaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4098">The
          Economist</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        okupoj: sociologo kaj <tld/>isto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=301092">Max
          Weber</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_a;&c_n;&c_a;&c_m;&c_ib;&c_s;&c_t;</trd>
