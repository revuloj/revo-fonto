<art mrk="$Id: arnold.xml,v 1.11 2023/10/28 08:33:09 revo Exp $">
<kap>
  <rad>Arnold</rad>/o
</kap>
<drv mrk="arnold.0o">
  <kap><tld/>o</kap>
  <snc mrk="arnold.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        &leftquot;<tld/>o, mi devas diri al vi, ke mi &scirc;atas vian
        diligentecon&rightquot;
        <fnt>
          <aut>-</aut>,
          <vrk>Malfrua al la laborejo</vrk>,
          <lok>
    <url ref="http://www.gazetejo.org/system/files/gazetoj/IN0704.pdf">
          Inter ni, 2007-04</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        li similas al la fa&ubreve;no el la pentra&jcirc;o de <tld/>o
        <nom>B&ouml;cklin</nom>
        <fnt>
          <aut>A. Grin, trad. J. Finkel</aut>,
          <vrk>
          <url ref="http://www.esperanto.mv.ru/Grin/skarlataj.html">
          Skarlataj veloj</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x963F;&#x8AFE;&#x5FB7; <pr>&#x101;nu&#xF2;d&#xE9;</pr></trd>,
    <trd>&#x963F;&#x8BFA;&#x5FB7; <pr>&#x101;nu&#xF2;d&#xE9;</pr></trd>,
    <trd>&#x963F;&#x5C14;&#x8BFA;&#x5FB7; <pr>&#x101;&#x11B;rnu&#xF2;d&#xE9;</pr></trd>,
    <trd>&#x963F;&#x723E;&#x8AFE;&#x5FB7; <pr>&#x101;&#x11B;rnu&#xF2;d&#xE9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_A;&c_r;&c_n;&c_o;&c_l;&c_mol;&c_d;</trd>
  <trd lng="de">Arnold</trd>
  <trd lng="fr">Arnaud</trd>
  <trd lng="hu">Arnold</trd>
  <trdgrp lng="it">
    <trd>Arnoldo</trd>,
    <trd>Arnaldo</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Aart</trd>,
    <trd>Arend</trd>,
    <trd>Arnout</trd>,
    <trd>Arnoud</trd>
  </trdgrp>
  <trd lng="pl">Arnold</trd>
  <trd lng="uk">&c_A;&c_r;&c_n;&c_o;&c_l;&c_mol;&c_d;&nbsp;(&c_ib;&c_m;&#x2019;&c_ja;)</trd>
</drv>
</art>

