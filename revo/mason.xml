<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>mason</rad>/i
</kap>
<drv mrk="mason.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="mason.0i.konstrui">
    <dif>
      Fortike konstrui ion el &scirc;tonoj a&ubreve; brikoj kunigante ilin
      per mortero a&ubreve; cemento:
      <ekz>
        <tld/>i muron, domon.
      </ekz>
    </dif>                       
    <trd lng="br">ma&ntilde;sonat</trd>
    <trd lng="de">mauern</trd>
    <trd lng="fr">ma&ccedil;onner</trd>
    <trd lng="nl">metselen</trd>
  </snc>
  <snc mrk="mason.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fortike kaj firme starigi ion abstraktan:
      <ekz>
        nur la interkompreno povas <tld/>i la pacon.
      </ekz>
    </dif>
    <trd lng="nl">bouwen<klr> (figuurlijk)</klr></trd>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_z; &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ja;, &c_c;&c_e;&c_g;&c_l;&c_y;)</klr></trd>
  <trd lng="en">build</trd>
  <trdgrp lng="hu">
    <trd>falaz</trd>,
    <trd>&eacute;p&iacute;t</trd>,
    <trd>emel <klr tip="ind">(falat)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_i;&c_z; &c_k;&c_a;&c_m;&c_n;&c_ja;, &c_k;&c_i;&c_r;&c_p;&c_i;&c_ch;&c_a;)</klr></trd>
</drv>
</art>
