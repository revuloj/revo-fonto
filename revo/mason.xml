<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mason.xml,v 1.28 2024/01/25 20:10:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mason</rad>/i
</kap>

<drv mrk="mason.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KON</uzo>
  <snc mrk="mason.0i.konstrui">
    <dif>
      Fortike <ref tip="super" cel="konstr.0i">konstrui</ref>
      ion el &scirc;tonoj a&ubreve; brikoj kunigante ilin per mortero 
      a&ubreve; cemento:
      <ekz>
        <tld/>i muron, domon;
      </ekz>
      <ekz>
        traba domo kun <tld/>ita kelo
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        strato konstruita de lia frato, la <tld/>a majstro
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili revenis supren kaj same zorge <tld/>is la eniron en la puton
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        flava <tld/>ita pre&gcirc;ejo kun verda tegmento
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiu korto estis <klr>[&#x2026;]</klr> de kvar flankoj 
        &ccirc;irka&ubreve;ita de altaj <tld/>itaj konstrua&jcirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        eksperimentoj kun konstrueroj, faritaj el malplenaj 
        ladskatoloj, kaj muroj, <tld/>itaj el malplenaj boteloj
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2004/008236.html">La rikolto de la nova
          tempo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        trulo <klr>[estas]</klr> &ccirc;erpilo kaj albatilo por <tld/>pasto
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> tombon <klr>[&#x2026;]</klr> oni <tld/>fermis
        <fnt>
          <bib>Fab2</bib>
          <lok>El fenestro en Vartou</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">ma&ntilde;sonat</trd>
    <trd lng="ca">bastir</trd>
    <trd lng="de">mauern</trd>
    <trd lng="en">mason</trd>
    <trd lng="fr">ma&ccedil;onner</trd>
    <trd lng="nl">metselen</trd>
    <trdgrp lng="pt">
      <trd>fazer obra de <ind>alvenaria</ind></trd>,
      <trd>trabalhar de <ind>pedreito</ind></trd>
    </trdgrp>
    <trd lng="zh">&#x780C; <pr>q&#xEC;</pr></trd>
</snc>
<!-- ne trovis pruvon de figura uzo...
  <snc mrk="mason.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fortike kaj firme starigi ion abstraktan:
      <ekz>
        nur la interkompreno povas <tld/>i la pacon.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>construir</trd>,
      <trd>edificar</trd>
    </trdgrp>
    <trd lng="de">untermauern<klr> (fig.)</klr></trd>
    <trd lng="nl">bouwen<klr> (figuurlijk)</klr></trd>
  </snc>
-->
  <trd lng="be">&c_b;&c_u;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_z;
&c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ja;,
&c_c;&c_e;&c_g;&c_l;&c_y;)</klr></trd>
  <trd lng="de">mauern</trd>
  <trd lng="en">build</trd>
  <trdgrp lng="es">
    <trd>construir</trd>,
    <trd>edificar</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>falaz</trd>,
    <trd>&eacute;p&iacute;t</trd>,
    <trd>emel <klr tip="ind">(falat)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x7BC9;&#x304F;  <pr>&#x304D;&#x305A;&#x304F;</pr></trd>
  <trd lng="pl">murowa&cacute; </trd>
  <trd lng="pt">cimentar <klr tip="ind">(fig.)</klr></trd>
  <trd lng="ro">construi </trd>
  <trd lng="ru">&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_i;&c_z; &c_k;&c_a;&c_m;&c_n;&c_ja;,&c_k;&c_i;&c_r;&c_p;&c_i;&c_ch;&c_a;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_u;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i; &c_z; &#x43A;&c_a;&#x43C;&c_je;&#x43D;&#x44E;</trd>
  </trdgrp>
</drv>


<drv mrk="mason.0ajxo">
  <kap>
    <tld/>a&jcirc;o
  </kap>
  <uzo tip="fak">KON</uzo>
  <snc>
    <dif>
      Io <tld/>ita: <ref tip="sub" cel="mur.0o.ARKI">muro</ref>, 
      <ref cel="konstr.0ajxo">konstrua&jcirc;o</ref> kc:
      <ekz>
        ili supreniris mallar&gcirc;an, krutan &scirc;tuparon, kondukantan tra la <klr>[&#x2026;]</klr>
        masiva <tld/>a&jcirc;o de la sesa eta&gcirc;o al la sepa
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        sin klinante &gcirc;is la zono ekster la fenestro, <klr>[&#x2026;]</klr> Viktoro palpis per
        la mano kaj ser&ccirc;is en kiu stato <klr>[&#x2026;]</klr> trovi&gcirc;as la <tld/>a&jcirc;o
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_r;&c_o;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_u;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_k;&c_l;&c_a;&c_d;&c_k;&c_a; <klr tip="amb">(&c_z; &c_k;&c_a;&c_m;&c_je;&c_n;&c_mol;&c_n;&c_ja;, &c_c;&c_e;&c_g;&c_l;&c_y;)</klr></trd>
  </trdgrp>
  <trd lng="de">Mauerwerk</trd>
  <trd lng="en">masonry</trd>
  <trd lng="fr">ma&ccedil;onnerie</trd>
  <trdgrp lng="ja">
    <trd>&#x77F3;&#x7A4D;&#x307F;  <pr>&#x3044;&#x3057;&#x3065;&#x307F;</pr></trd>,
    <trd>&#x30EC;&#x30F3;&#x30AC;&#x7A4D;&#x307F;  <pr>&#x308C;&#x3093;&#x304C;&#x3064;&#x307F;</pr></trd>,
    <trd>&#x77F3;&#x9020;&#x5EFA;&#x7BC9;  <pr>&#x3044;&#x3057;&#x3064;&#x304F;&#x3051;&#x3093;&#x3061;&#x304F;</pr></trd>,
    <trd>&#x7149;&#x74E6;&#x5EFA;&#x7BC9;  <pr>&#x308C;&#x3093;&#x304C;&#x3051;&#x3093;&#x3061;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="uk">&#x43A;&c_a;&#x43C;&#x2019;&#x44F;&#x43D;&c_a; &#x43A;&#x43B;&c_a;&c_d;&#x43A;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x6CE5;&#x5DE5; <pr>n&#xED;g&#x14D;ng</pr></trd>,
    <trd>&#x77F3;&#x5DE5; <pr>sh&#xED;g&#x14D;ng</pr></trd>
  </trdgrp>
</drv>


<drv mrk="mason.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:metioj" cel="meti.0isto">Metiisto</ref> 
      de <tld/>ado:
      <ekz>
        <klr>[li sendis]</klr> cedrajn arbojn, kaj 
        &ccirc;arpentistojn kaj <tld/>istojn kaj
        ili konstruis domon
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 5:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>istoj kaj &ccirc;arpentistoj faris anta&ubreve;kalkulon, la&ubreve; kiu la kostoj prezentis
        la sumon de ankora&ubreve; dumil spesmiloj
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        malanta&ubreve; tia domo mi vidis <tld/>istan trabaron, kiu kovris sufi&ccirc;e bone
        aspektantan, konstruatan domon
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_l;&c_ja;&c_r;</trd>
  <trd lng="de">Maurer</trd>
  <trd lng="en">bricklayer</trd>
  <trd lng="fr">ma&ccedil;on</trd>
  <trdgrp lng="ja">
    <trd>&#x77F3;&#x5DE5;  <pr>&#x3044;&#x3057;&#x304F;</pr></trd>,
    <trd>&#x30EC;&#x30F3;&#x30AC;&#x8077;&#x4EBA;  <pr>&#x308C;&#x3093;&#x304C;&#x3057;&#x3087;&#x304F;&#x306B;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="uk">&#x43C;&c_u;&#x43B;&#x44F;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#x6CE5;&#x74E6;&#x5320; <pr>n&#xED;w&#x1CE;ji&#xE0;ng</pr></trd>,
    <trd>&#x74E6;&#x5320; <pr>w&#x1CE;jiang</pr></trd>,
    <trd>&#x6CE5;&#x6C34;&#x5320; <pr>n&#xED;shu&#x1D0;ji&#xE0;ng</pr></trd>,
    <trd>&#x74E6;&#x5DE5; <pr>w&#x1CE;g&#x14D;ng</pr></trd>
  </trdgrp>
</drv>




