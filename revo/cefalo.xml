<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>cefalopod</rad>/oj
</kap>
<drv mrk="cefalo.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="molusk.0oj">moluskoj</ref>
      (<trd lng="la">Cephalopoda</trd>), kies kapo
      estas &ccirc;irka&ubreve;ata de brakoj servantaj por kapti kaj porti la
      nutra&jcirc;on al la bu&scirc;o.
    </dif>
    <ref tip="sin" cel="pied.kapo0uloj">kapopieduloj</ref>
    <refgrp tip="sub">
     <ref cel="polp.0o">polpo</ref>,
     <ref cel="sepi.0o">sepio</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Kopff&uuml;&szlig;er</trd>
  <trd lng="el">&kappa;&epsilon;&phi;&alfa;&lambda;&omikron_ton;&pi;&omikron;&delta;&alfa;</trd>
  <trd lng="es">cefal&oacute;podos</trd>
  <trd lng="fr">c&eacute;phalopodes</trd>
  <trd lng="pt">cefal&oacute;podes</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_o;&c_n;&c_o;&c_g;&c_i;&c_je;</trd>
</drv>
</art>
