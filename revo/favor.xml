<drv mrk="favor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="favor.0i.KOMUNE">
    <dif>
      Esti <tld/>a, agi <tld/>e al, 
      <ref tip="sub" cel="prefer.0i">preferi</ref>:
      <ekz>
        <tld/>i iun, al iu;
      </ekz>
      <ekz>
        <tld/>i ian entreprenon, al entrepreno;
      </ekz>
      <ekz>
        ni estis <tld/>itaj per bela vetero;
      </ekz>
      <ekz>
        kiun <tld/>as la sorto, por tiu e&ccirc; koko estas
        ovoporta<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la dioj <tld/>is vin per granda virto
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 5a</lok></fnt>;
      </ekz>
      <ekz>
        ne estas bone <tld/>i malvirtulon, por faligi virtulon &ccirc;e la
        ju&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 18:5</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akcel.0i">akceli</ref>,
      <ref cel="ben.0i">beni</ref>,
      <ref cel="kares.0i">karesi</ref>,
      <ref cel="rid.0i">ridi</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>kedvez vkinek</trd>,
      <trd>kegyes vkihez</trd>,
      <trd>kedvezm&eacute;nyez</trd>,
      <trd>favoriz&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">begunstigen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_y;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; &c_z;
    <ind>&c_p;&c_r;&c_y;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ju;</ind></trd>
  </trdgrp>
  <trd lng="bo">&t_da;&t_ga;&amp;t_-a;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>b&yacute;t naklon&ecaron;n</trd>,
    <trd>p&rcaron;&aacute;t</trd>,
    <trd>stranit <klr>(n&ecaron;komu)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beg&uuml;nstigen</trd>,
    <trd>bevorzugen</trd>,
    <trd>f&ouml;rdern</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>favour</trd>,
    <trd>favor</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ser <ind>favorable</ind> a</trd>,
    <trd>favorecer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>favorable</ind> (&agrave;)</trd>,
    <trd>favoriser</trd>,
    <trd>gratifier</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_v;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>favorizova&tcaron;</trd>,
    <trd>uprednostni&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
