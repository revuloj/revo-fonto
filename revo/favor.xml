<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>favor</rad>/a</kap>
<drv mrk="favor.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Bonvola, helpopreta, indulga <klr>(ofte pro aparta arbitra
      prefero)</klr>:
      <ekz>
        estro <tld/>a al <klr>(por)</klr> siaj subuloj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komple4.0ema">komplezema</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_o;&c_b;&c_r;&c_a;&c_z;&c_y;&c_ch;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="de">gro&szlig;z&uuml;gig</trd>
    <trdgrp lng="hu">
      <trd>kegy</trd>,
      <trd>j&oacute;indulat</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_t;&c_i;&c_v;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_k;&c_l;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_zh;&c_je;&c_l;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Helpota&ubreve;ga, faciliga, sukcesiga:
      <ekz>
        <ind><tld/>a vento</ind>, vetero, cirkonstanco;
        <trd lng="fr">vent favorable</trd>
        <trd lng="hu">kedvez&odblac; sz&eacute;l</trd>
        <trd lng="ru">&c_p;&c_o;&c_p;&c_u;&c_t;&c_n;&c_y;&c_j; &c_v;&c_je;&c_t;&c_je;&c_r;</trd>
      </ekz>
      <ekz>
        <tld/>a argumento
        <klr>(<ref tip="vid" cel="pled.0i">pledi</ref>)</klr>;
      </ekz>
      <ekz>
        tiu grundo estas <tld/>a por greno;
      </ekz>
      <ekz>
        regas <ind>mal<tld/>a</ind> opinio pri li.
        <trdgrp lng="fr">
          <trd>d&eacute;favorable</trd>,
          <trd>n&eacute;gatif</trd>,
          <trd>hostile</trd>
        </trdgrp>
        <trd lng="hu">kedvez&odblac;tlen</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="felicx.0a">feli&ccirc;a</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_p;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trd lng="de">g&uuml;nstig</trd>
    <trd lng="hu">kedvez&odblac;</trd>
    <trd lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>bienveillant</trd>,
    <trd>favorable</trd>,
    <trd>propice</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>favorable</trd>,
    <trd>propicio, -a</trd>
  </trdgrp>
  <trd lng="nl">gunstig</trd>
</drv>

<drv mrk="favor.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Bonvola, <tld/>a inklino <klr>(ofte pro aparta arbitra
      prefero)</klr>:
      <ekz>
        altiri la <tld/>on de iu;
      </ekz>
      <ekz>
        trovi <tld/>on &ccirc;e iu<fnt>Z</fnt>;
      </ekz>
      <ekz>
        suferi <ind>mal<tld/>o</ind>n.
        <trdgrp lng="fr">
          <trd>d&eacute;faveur</trd>,
          <trd>disgr&acirc;ce</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <snc mrk="favor.0o.0ajxo">
    <dif>
      <tld lit="F"/>a ago, helpo:
      <ekz>
        super&scirc;uti iun per <tld/>oj;
      </ekz>
      <ekz>
        peti <tld/>on.
      </ekz>
    </dif>
    <ref tip="sin" cel="favor.0ajxo"><tld/>a&jcirc;o</ref>
    <refgrp tip="vid">
      <ref cel="intere.0o">intereso</ref>,
      <ref cel="komple4.0o">komplezo</ref>,
      <ref cel="patron">patronado</ref>,
      <ref cel="protek">protekto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_r;&c_a;&c_z;&c_y;&c_ch;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Gunst</trd>
  <trdgrp lng="fr">
    <trd>bienfait</trd>,
    <trd>bienveillance</trd>,
    <trd>faveur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kedvezm&eacute;ny</trd>,
    <trd>kedvez&eacute;s</trd>,
    <trd>kegy</trd>
  </trdgrp>
  <trd lng="nl">gunst</trd>
  <trd lng="es">favor</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_v;&c_o;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_k;&c_l;&c_o;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je;<klr tip="ind"> (&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_k;&c_l;&c_o;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="favor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="favor.0i.KOMUNE">
    <dif>
      Esti <tld/>a, agi <tld/>e al:
      <ekz>
        <tld/>i iun, al iu;
      </ekz>
      <ekz>
        <tld/>i ian entreprenon, al entrepreno;
      </ekz>
      <ekz>
        kiun <tld/>as la sorto, por tiu e&ccirc; koko estas
        ovoporta<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la dioj <tld/>is vin per granda virto
        <fnt><bib>Far1</bib>, <lok>&ccirc;apitro 5a</lok></fnt>;
      </ekz>
      <ekz>
        ni estis <tld/>itaj per bela vetero.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akcel.0i">akceli</ref>,
      <ref cel="ben.0i">beni</ref>,
      <ref cel="kares.0i">karesi</ref>,
      <ref cel="rid.0i">ridi</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>kedvez vkinek</trd>,
      <trd>kegyes vkihez</trd>,
      <trd>kedvezm&eacute;nyez</trd>,
      <trd>favoriz&aacute;l</trd>
    </trdgrp>
    <trd lng="nl">begunstigen</trd>
  </snc>
  <snc>
    <dif>
      Esti prefere <tld/>a al:
      <ekz>
        <tld/>i siajn amikojn, al siaj amikoj.
      </ekz>
    </dif>
    <trd lng="hu">el&odblac;nyben r&eacute;szes&iacute;t</trd>
    <trd lng="es">preferir</trd>
    <trd lng="nl">bevoordeligen</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_y;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_ib;&c_c;&c_c;&c_a; &c_z; <ind>&c_p;&c_r;&c_y;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ju;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beg&uuml;nstigen</trd>,
    <trd>bevorzugen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>favorable</ind> (&agrave;)</trd>,
    <trd>favoriser</trd>,
    <trd>gratifier</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>ser <ind>favorable</ind> a</trd>,
    <trd>favorecer</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_v;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="favor.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="favor.0ajxo.KOMUNE">
      <ref tip="dif" cel="favor.0o.0ajxo"><tld/>o
      <sncref ref="favor.0o.0ajxo"/></ref>
      <ekz>
        montru al ni la eniron en la urbon, kaj ni faros al vi <tld/>a&jcirc;on
        <fnt><bib>MT</bib>, <lok>&Jug; 7:24</lok></fnt>.
      </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>faveur<klr tip="ind"> (bienfait)</klr></trd>,
    <trd>bienfait</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kedvezm&eacute;ny</trd>,
    <trd>el&odblac;ny</trd>,
    <trd>kegyes cselekedet</trd>
  </trdgrp>
  <trd lng="nl">gunst</trd>
  <trd lng="es">favor</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_v;&c_o;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_k;&c_l;&c_o;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="favor.0ato">
  <kap><tld/>ato</kap>
  <snc mrk="favor.0ato.KOMUNE">
    <dif>
      Tiu, kiu &gcirc;uas apartan <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_v;&c_a;&c_r;&c_y;&c_t;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_ib;&c_m;&c_je;&c_c;</trd>,
    <trd>&c_u;&c_l;&c_ju;&c_b;&c_jo;&c_n;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="de">G&uuml;nstling</trd>
  <trdgrp lng="fr">
    <trd>chouchou</trd>,
    <trd>favori<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kedvenc</trd>,
    <trd>kegyenc</trd>
  </trdgrp>
  <trd lng="nl">begunstigde</trd>
  <trdgrp lng="es">
    <trd>favorecido</trd>,
    <trd>favorito</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_v;&c_o;&c_r;&c_i;&c_t;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_i;&c_m;&c_je;&c_c;</trd>
  </trdgrp>
</drv>

<drv mrk="favor.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="favor.0igi.KOMUNE">
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi la publikon al ideo;
      </ekz>
      <ekz>
        <ind><tld/>igi al si</ind> la humoron de la re&gcirc;o;
        <trd lng="fr">se <ind>concilier</ind></trd>
      </ekz>
      <ekz>
        rekomendi <ind><tld/>igo</ind>n al la disvastigo de humanismo.
        <trd lng="fr">ralliement<klr tip="ind"> (adh&eacute;sion)</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">rallier<klr tip="ind"> (faire adopter une id&eacute;e)</klr></trd>
  <trd lng="es">favorecer<klr> (hacer favorable algo)</klr></trd>
  <trd lng="hu">megnyer vminek</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_k;&c_o;&c_g;&c_o;-&c_l;. &c_k; &c_ch;&c_je;&c_m;&c_u;-&c_l;.)</klr></trd>,
    <trd>&c_s;&c_n;&c_i;&c_s;&c_k;&c_a;&c_t;&c_mol; &c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_k;&c_l;&c_o;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="favor.kor0i">
  <kap>kor<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="favor.kor0i.BIB">
    <dif>
      Bonkore kompati iun:
      <ekz>
        turnu Vin al mi kaj kor<tld/>u min
        <fnt><bib>MT</bib>, <lok>&Psa; 25:16</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">avoir <ind>piti&eacute;</ind> de</trd>
  <trd lng="es">compadecerse de</trd>
  <trdgrp lng="hu">
    <trd>sz&iacute;vlel</trd>,
    <trd>j&oacute; <ind>sz&iacute;v</ind>vel van ir&aacute;nta</trd>,
    <trd>j&oacute;indulattal van ir&aacute;nta</trd>
  </trdgrp>
  <trd lng="nl">behartigen</trd>
  <trd lng="ru">&c_s;&c_o;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="favor.kor0o">
  <kap>kor<tld/>o</kap>
  <snc mrk="favor.kor0o.BIB">
    <ref tip="dif" cel="kor.favor0eco"><tld/>koreco</ref>
    <ekz>
      Li helpis Sian servanton Izrael, memorante Sian kor<tld/>on
      <fnt><bib>NT</bib>, <lok>&Mar; 1:54</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ib;&c_t;&c_a;&c_s;&c_c;&c_ib;&c_v;&c_a;&c_s;&c_c;&c_mol;</trd>,
    <trd>&c_m;&c_ib;&c_l;&c_a;&c_s;&c_e;&c_r;&c_n;&c_a;&c_s;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>mis&eacute;ricorde</trd>,
    <trd>piti&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
   <trd>kegy</trd>,
   <trd>j&oacute;indulat</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_o;&c_s;&c_je;&c_r;&c_d;&c_i;&c_je;</trd>
</drv>

<drv mrk="favor.san0a">
  <kap>tld</kap>
  <snc mrk="favor.san0a.MED">
    <dif>
      Kiu bone konservas a&ubreve; protektas <tld/>on:
      <ekz>
        atentu pri <klr>...</klr> la san<tld/>a, naturkonforma 
        vivmaniero por longe vivi sane kaj trankvile 
        <fnt>
        <vrk><url ref="http://users.pandora.be/walkme/longvivo/longvivo.html"> La aktiva longvivado</url></vrk>,
        <lok>2003-12-28</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>bon pour la sant&eacute;</trd>,
    <trd>sain<klr> (bon pour la sant&eacute;)</klr></trd>,
    <trd>salubre</trd>
  </trdgrp>
</drv>
</art>
