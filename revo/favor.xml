<drv mrk="favor.0a">
  <kap><tld/>a</kap>
  <snc mrk="favor.0a.helpema">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Bonvola, helpopreta, indulga <klr>(ofte pro aparta arbitra
      prefero)</klr>:
      <ekz>
        estro <tld/>a al <klr>(por)</klr> siaj subuloj;
      </ekz>
      <ekz>
        multaj junaj pastroj estas <tld/>aj al la faraono kaj konigas al
        li &ccirc;ion
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        via Asocio ricevis la plej <tld/>an akcepton en nia movado
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la malferma kunveno de UEA &ccirc;e la 
          Kvina Kongreso Esperantista en Barcelona en la 7a de septembro
          1909</vrk>
        </fnt>;
      </ekz>
      <ekz>
        por kio vi estas tiel <tld/>a al mi?
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="komple4.0ema">komplezema</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_d;&c_o;&c_b;&c_r;&c_a;&c_z;&c_y;&c_ch;&c_l;&c_ib;&c_v;&c_y;</trd>,
      <trd>&c_p;&c_r;&c_y;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="de">gro&szlig;z&uuml;gig</trd>
    <trdgrp lng="en">
      <trd>favoring</trd>,
      <trd>favouring</trd>,
      <trd>partial</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kegy</trd>,
      <trd>j&oacute;indulat</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_t;&c_i;&c_v;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_k;&c_l;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_zh;&c_je;&c_l;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="favor.0a.prosperiga">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Helpota&ubreve;ga, faciliga, sukcesiga:
      <ekz>
        <ind><tld/>a vento</ind>, vetero, cirkonstanco;
        <trd lng="es">viento favorable</trd>
        <trd lng="fr">vent favorable</trd>
        <trd lng="hu">kedvez&odblac; sz&eacute;l</trd>
        <trd lng="ru">&c_p;&c_o;&c_p;&c_u;&c_t;&c_n;&c_y;&c_j;
        &c_v;&c_je;&c_t;&c_je;&c_r;</trd>
      </ekz>
      <ekz>
        <tld/>a argumento
        <klr>(<ref tip="vid" cel="pled.0i">pledi</ref>)</klr>;
      </ekz>
      <ekz>
        tiu grundo estas <tld/>a por greno;
      </ekz>
      <ekz>
        la ju&gcirc;o de dioj estos <tld/>a por li
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne estis jam por &scirc;i tempo <tld/>a por lernado, kaj tamen &scirc;i
        lernis
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sentis, kian por &scirc;i ne<tld/>oran impreson &scirc;ia ludado faris
        sur la virino
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sxanc.bon0a.donanta">bon&scirc;anca<sncref/></ref>,
      <ref cel="felicx.0a.favora">feli&ccirc;a<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_s;&c_p;&c_r;&c_y;&c_ja;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>g&uuml;nstig</trd>,
      <trd>f&ouml;rderlich</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>advantageous</trd>,
      <trd>favourable</trd>,
      <trd>favourite</trd>,
      <trd>favorable</trd>,
      <trd>favorite</trd>
    </trdgrp>
    <trd lng="hu">kedvez&odblac;</trd>
    <trd lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="cs">p&rcaron;&iacute;zniv&yacute;</trd>
  <trdgrp lng="es">
    <trd>favorable</trd>,
    <trd>propicio, -a</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bienveillant</trd>,
    <trd>favorable</trd>,
    <trd>propice</trd>
  </trdgrp>
  <trd lng="nl">gunstig</trd>
  <trdgrp lng="pt">
    <trd>favor&aacute;vel</trd>,
    <trd>prop&iacute;cio</trd>
    <trd>benevolente</trd>
    <trd>benigno</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>priazniv&yacute;</trd>,
    <trd>v&yacute;hodn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_h;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_r;&c_o;&c_z;&c_i;&c_ch;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_t;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_p;&c_u;&c_t;&c_n;&c_i;&c_j; (&c_p;&c_r;&c_o; &c_v;&c_ib;&c_t;&c_je;&c_r;)</trd>
  </trdgrp>
</drv>

<drv mrk="favor.0o">
  <kap><tld/>o</kap>
  <snc mrk="favor.0o.KOMUNE">
    <dif>
      Bonvola, <tld/>a inklino <klr>(ofte pro aparta arbitra
      prefero)</klr>:
      <ekz>
        altiri la <tld/>on de iu;
      </ekz>
      <ekz>
        a&ccirc;eti la <tld/>on de la le&gcirc;o
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li trovis <tld/>on &ccirc;e pastrino kaj la&ubreve; &scirc;ia konsilo forveturis
        <klr>[...]</klr>
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i akiris &ccirc;e li simpation kaj <tld/>on pli ol &ccirc;iuj virgulinoj, 
        kaj li <klr>[&#8230;]</klr> faris &scirc;in re&gcirc;ino 
        <fnt>
          <bib>MT</bib>
          <lok>Ester 2:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        parioj, kiuj rondiras sen <tld/>o kaj sen protekto
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
        </fnt>;
      </ekz>
      <ekz>
        suferi <ind>mal<tld/>o</ind>n;
        <trdgrp lng="es">
          <trd>disfavor</trd>,
          <trd>desgracia</trd>
        </trdgrp>
        <trdgrp lng="fr">
          <trd>d&eacute;faveur</trd>,
          <trd>disgr&acirc;ce</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind>klopodi pri &ccirc;ies <tld/>o estas pleje malsa&gcirc;a laboro</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">allen Leuten <ind>recht</ind> getan ist 
          eine Kunst die keiner kann</trd>
      </ekz>
    </dif>
  </snc>
  <snc mrk="favor.0o.0ajxo">
    <dif>
      <tld lit="F"/>a ago, helpo;
      <ref tip="sin" cel="favor.0ajxo"><tld/>a&jcirc;o</ref>:
      <ekz>
        super&scirc;uti iun per <tld/>oj;
      </ekz>
      <ekz>
        peti <tld/>on;
      </ekz>
      <ekz>
        vi faras grandan <tld/>on al <klr>[ili]</klr> provizante ilin je
        &ccirc;io necesa
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efoj <klr>[...]</klr> lin instigis, petante de li <tld/>on 
        kontra&ubreve; Pa&ubreve;lo, ke li venigu lin <klr>[...]</klr> por mortigi lin
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 25:3</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="intere.0o">intereso</ref>,
      <ref cel="komple4.0o">komplezo</ref>,
      <ref cel="patron">patronado</ref>,
      <ref cel="protek">protekto</ref>
    </refgrp>
  </snc>
