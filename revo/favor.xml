<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>favor</rad>/a
</kap>
<drv mrk="favor.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc num="1">
  <uzo tip="klr">(iu)</uzo>
    <dif>
      Bonvola, helpopreta, indulga (ofte pro aparta arbitra prefero):
      <ekz>
        estro <tld/>a al <klr>(por)</klr> siaj subuloj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="komple4">kompleza</ref>.
    </refgrp>
    <trd lng="de">gro&szlig;z&uuml;gig</trd>
    <trdgrp lng="ru">
      <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_t;&c_i;&c_v;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_k;&c_l;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_zh;&c_je;&c_l;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="klr">(io)</uzo>
    <dif>
      Helpota&ubreve;ga, faciliga, sukcesiga:
      <ekz>
        <ind><tld/>a vento</ind><trd lng="ru">&c_p;&c_o;&c_p;&c_u;&c_t;&c_n;&c_y;&c_j; &c_v;&c_je;&c_t;&c_je;&c_r;</trd>,
        vetero, cirkonstanco;
      </ekz>
      <ekz>
        <tld/>a argumento
        (<ref tip="vid" cel="pled.0i">pledi</ref>);
      </ekz>
      <ekz>
        tiu grundo estas <tld/>a por greno. 
      </ekz>
    </dif>
    <trd lng="de">g&uuml;nstig</trd>
    <trd lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>
<drv mrk="favor.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Bonvola, <tld/>a inklino (ofte pro aparta arbitra prefero):
      <ekz>
        altiri la <tld/>on de iu;
      </ekz>
      <ekz>
        trovi <tld/>on &ccirc;e iu<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      <tld/>a ago, helpo; <tld/>a&jcirc;o:
      <ekz>
        super&scirc;uti iun per <tld/>oj;
      </ekz>
      <ekz>
        peti <tld/>on;
      </ekz>
      <ekz>
        suferi mal<tld/>on
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="komple4.0o">komplezo</ref>,
      <ref cel="protek">protekto</ref>,
      <ref cel="patron">patronado</ref>,
      <ref cel="intere.0o">intereso</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Gunst</trd>
  <trdgrp lng="ru">
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_v;&c_o;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_s;&c_k;&c_l;&c_o;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="favor.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc num="1">
    <dif>
      Esti <tld/>a al: 
      <ekz>
        <tld/>i iun, al iu; 
      </ekz>
      <ekz>
        <tld/>i ian entreprenon, al entrepreno; 
      </ekz>
      <ekz>
        kiun <tld/>as la sorto, por tiu e&ccirc; koko estas ovoporta
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>; 
      </ekz>
      <ekz>
        la dioj <tld/>is vin per granda virto
        <fnt>(<aut>Prus, trad. Kabe</aut>: <vrk>La Faraono, 1a vol.</vrk>,
        <lok>&ccirc;ap. 5</lok>)</fnt>; 
      </ekz>
      <ekz>
        ni estis <tld/>itaj per bela vetero
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="akcel.0i">akceli</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="rid.0i">ridi</ref>, 
      <ref cel="kares.0i">karesi</ref>, 
      <ref cel="ben.0i">beni</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <dif>
      Esti prefere <tld/>a al: 
      <ekz>
        <tld/>i siajn amikojn, al siaj amikoj. 
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>beg&uuml;nstigen</trd>,
    <trd>bevorzugen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_v;&c_o;&c_l;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_l;&c_a;&c_g;&c_o;&c_p;&c_r;&c_i;&c_ja;&c_t;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="favor.0ato">
  <kap><tld/>ato</kap>
  <snc>
    <dif>
      Tiu, kiu &gcirc;uas apartan <tld/>on. 
    </dif>
  </snc>
  <trd lng="de">G&uuml;nstling</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_a;&c_v;&c_o;&c_r;&c_i;&c_t;</trd>,
    <trd>&c_l;&c_ju;&c_b;&c_i;&c_m;&c_je;&c_c;</trd>
  </trdgrp>
</drv>
<drv mrk="favor.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a:
      <ekz>
        rekomendi <tld/>igon al la disvastigo de humanismo.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="favor.0korecokor0o">
  <kap><tld/>koreco, kor<tld/>o</kap>
  <snc num="1">
    <dif>
      Bonvola bonkora sento al iu. 
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Graco.  
    </dif>
  </snc>
  <trd lng="ru">&c_m;&c_i;&c_l;&c_o;&c_s;&c_je;&c_r;&c_d;&c_i;&c_je;</trd>
</drv>
</art>
