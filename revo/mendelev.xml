    <kap><rad>Mendelev</rad>/o <fnt><bib>PIV2</bib></fnt></kap>
 
<drv mrk="mendelev.M0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Rusa <ref tip="lst" lst="voko:historiaj_personoj" cel="hxemi.0isto">&hcirc;emiisto</ref>
      Demetrio <tld/>o, (1834&ndash;1907), a&ubreve;toro de la
      <ref cel="period.0atabelo">Perioda tabelo</ref>:
      <ekz>
        venis pozitiva respondo: en la liverita kvanto estas 0,6 gramoj
        da oro, anka&ubreve;
        volframo kaj multaj aliaj elementoj el la tabelo de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2010/009963.php">Ora febro en Litovio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="mendelevi.0o">mendelevio</ref>
  <trd lng="be">&c_M;&c_je;&c_n;&c_mol;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_je;&c_w;</trd>
  <trdgrp lng="ca">
    <trd>Mendel&eacute;iev</trd>,
    <trd>Mendelejev</trd>
  </trdgrp>
  <trd lng="de">Mendelejew</trd>
  <trd lng="en">Mendeleev</trd>
  <trd lng="fr">Mendele&iuml;ev</trd>
  <trd lng="hu">Mengyelejev</trd>
  <trd lng="it">Mendeleev</trd>
  <trd lng="ja">&#x30E1;&#x30F3;&#x30C7;&#x30EC;&#x30FC;&#x30A8;&#x30D5;</trd>
  <trd lng="nl"><klr>Dmitri </klr>Mendelejev</trd>
  <trd lng="pt">Mendeleyev, Dimitri Ivanovich</trd>
  <trd lng="ru"><klr>&c_D;&c_m;&c_i;&c_t;&c_r;&c_i;&c_j; &c_I;&c_v;&c_a;&c_n;&c_o;&c_v;&c_i;&c_ch;
  </klr>&c_M;&c_je;&c_n;&c_d;&c_je;&c_l;&c_je;&c_je;&c_v;</trd>
  <trd lng="sk">Mendelejev</trd>
  <trd lng="cs">Mend&ecaron;lejev</trd>
  <trdgrp lng="zh">
    <trd>&#x9580;&#x6377;&#x5217;&#x592B; <pr>M&#xE9;nji&#xE9;li&#xE8;f&#x16B;</pr></trd>,
    <trd>&#x95E8;&#x6377;&#x5217;&#x592B; <pr>M&#xE9;nji&#xE9;li&#xE8;f&#x16B;</pr></trd>
  </trdgrp>
