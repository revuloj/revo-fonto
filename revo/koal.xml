<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: koal.xml,v 1.13 2023/12/03 09:58:51 revo Exp $">
<kap>
  <rad>koal</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="koal.0o">
  <kap><tld/>o</kap>
  <snc mrk="koal.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda arbogrimpa <ref tip="malprt" cel="marsup.0uloj">marsupiulo</ref>
      vivanta en orienta kaj suda <ref tip="malprt" cel="auxstr1.A0o.kontinento">A&ubreve;stralio</ref>,
      kiu man&gcirc;as foliojn de iuj <ref cel="euxkal.0o">e&ubreve;kalipto</ref>j;
      sola vivanta <ref tip="lst" lst="voko:zoologiaj_specioj" cel="speci.0o.BIO">specio</ref>
      el sia genro (<trd lng="la">Phascolarctos cinereus</trd>):
      <ekz>
        inter la <tld/>oj &scirc;i ne vidis sian filon
        <fnt>
          <aut>C. Eaton, trad. E. Mayer</aut>,
          <vrk><url ref="http://cindymckee.com/librejo/Bib.pdf">
          Bib la Koalido</url></vrk>,
          <lok>1984</lok>
        </fnt>;
      </ekz>
      <ekz>
        pando kaj <tld/>o dancantaj man-en-mane
        <fnt>
          <vrk><url ref="http://esperanto.china.org.cn/2017-01/03/content_40030103.htm">Granda pando kaj koalo bonvenigas la &Ccirc;ina-A&ubreve;stralian ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/25/Friendly_Female_Koala.JPG/351px-Friendly_Female_Koala.JPG" prm="CC BY-SA 3.0"><tld/>ino, Sova&gcirc;parko en Port Macquarie, A&ubreve;stralio
        <fnt>
          <aut>Quartl, propra verko</aut>
          <lok><url ref="&WCW;/File:Friendly_Female_Koala.JPG">&WCW;/File:Friendly_Female_Koala.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_a;&c_l;&c_a;</trd>
  <trd lng="bg">&c_k;&c_o;&c_a;&c_l;&c_a;</trd>
  <trd lng="cs">koala</trd>
  <trd lng="de">Koala</trd>
  <trd lng="en">koala</trd>
  <trd lng="fr">koala</trd>
  <trd lng="id">koala</trd>
  <trd lng="it">koala</trd>
  <trdgrp lng="ja">
    <trd>&#x30B3;&#x30A2;&#x30E9;  <pr>&#x3053;&#x3042;&#x3089;</pr></trd>,
    <trd>&#x30D5;&#x30AF;&#x30ED;&#x30B0;&#x30DE;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mi&sacute; <ind>koala</ind> <klr>(pot.)</klr></trd>,
    <trd>koala</trd>
  </trdgrp>
  <trd lng="sk">koala</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x430;&#x43B;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x65E0;&#x5C3E;&#x718A; <pr>w&#xFA;w&#x11B;ixi&#xF3;ng</pr></trd>,
    <trd>&#x6811;&#x888B;&#x718A; <pr>sh&#xF9;d&#xE0;ixi&#xF3;ng</pr></trd>
  </trdgrp>
</drv>
</art>

