<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akar.xml,v 1.29 2023/10/28 08:33:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>akar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="akar.0o.ZOO">
    <dif>
      Iu el <ref tip="malprt" cel="akar.0oj.ZOO"><tld/>oj</ref>, 
      kies unu specio vivanta enha&ubreve;te ka&ubreve;zas la 
      <ref tip="dif" cel="skabi.0o">skabion</ref>:
      <ekz>
        la varroa <tld/>o <klr>...</klr> estas parazito
        vivanta de insekta sango
        <fnt>
          <aut>F. van Hertrooij</aut>
          <vrk><url ref="https://scivolemo.wordpress.com/2017/08/22/la-kodo-de-la-abela-danco/">
          La kodo de la abela&nbsp;danco</url></vrk>
          <lok>Scivolemo, 2017-08-22</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x8731; <pr>p&#xED;</pr></trd>
</snc>
  <ref tip="sub" cel="iksod.0o">iksodo</ref>
  <trd lng="be">&c_k;&c_l;&c_je;&c_sh;&c_ch;</trd>
  <trd lng="bg">&c_k;&c_malmol;&c_r;&c_l;&c_je;&c_zh;</trd>
  <trd lng="br">pre&ntilde;v ar gal</trd>
  <trd lng="de">Milbe</trd>
  <trd lng="en">mite</trd>
  <trd lng="fr">acarien</trd>
  <trd lng="he">&qof;&resh;&dalet;&yod;&tav;</trd>
  <trd lng="hu">atka</trd>
  <trdgrp lng="ja">
    <trd>&#x30C0;&#x30CB;</trd>,
    <trd>&#x30B3;&#x30CA;&#x30C0;&#x30CB;</trd>
  </trdgrp>
  <trd lng="nl">mijt</trd>
  <trd lng="es">&aacute;caro</trd>
  <trd lng="pt">&aacute;caro</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_shch;</trd>
  <trd lng="sk">rozto&ccaron; svrabov&yacute;</trd>
  <trd lng="cs">rozto&ccaron;</trd>
  <trd lng="uk">&#x43A;&#x43B;&c_ib;&c_shch;</trd>
</drv>

<drv mrk="akar.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="akar.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="grup.0o.BIO" lst="voko:zoologiaj_grupoj">Grupo</ref>
      de <ref tip="malprt" cel="arane.0uloj.ZOO">araneuloj</ref>
      (<trd lng="la">Acari</trd>)
      kun proksimume 50000 konataj specioj,
      tre malgrandaj inter 0,1 kaj kelkaj milimetroj.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_sh;&c_ch;&c_y; <klr tip="amb">(&c_p;&c_a;&c_d;&c_k;&c_l;&c_ja;&c_s;)</klr></trd>
  <trd lng="fr">acariens</trd>
</drv>

</art>
