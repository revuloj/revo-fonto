<drv mrk="songx.prem0o">
  <kap>prem<tld/>o</kap>
  <snc mrk="songx.prem0o.inkubo">
    <dif>
      <ref tip="dif" cel="inkub.0o.songxo">Inkubo</ref>:
      <ekz>
        &ccirc;iunokte mi havas prem<tld/>on &ndash; &ccirc;iam la saman
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008265.html">Parakosmo</url></vrk>
          <bib>Monato</bib>
          <lok>2004/05, p. 27</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estas a&ubreve; prem<tld/>o a&ubreve; malsana fantazio
        <fnt>
          <aut>Boris Kolker</aut>
          <vrk><url ref="&Monato;/2003/007812.html">Estas dan&gcirc;ere esti poeto</url></vrk>
          <bib>Monato</bib>
          <lok>2003/11, p. 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite &scirc;i veki&gcirc;is, kvaza&ubreve; el morta prem<tld/>o
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 22</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kosxmar.0o">ko&scirc;maro</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_sh;&c_m;&c_a;&c_r;</trd>
  <trd lng="bg">&c_k;&c_o;&c_sh;&c_m;&c_a;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Alptraum</trd>,
    <trd>Albtraum</trd>,
    <trd>Alpdr&uuml;cken</trd>,
    <trd>Alpdruck</trd>
  </trdgrp>
  <trd lng="en">nightmare</trd>
  <trd lng="es">pesadilla</trd>
  <trdgrp lng="fr">
    <trd>cauchemar</trd>,
    <trd>mauvais <ind>r&ecirc;ve</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lid&eacute;rcnyom&aacute;s</trd>,
    <trd>r&eacute;m&aacute;lom</trd>
  </trdgrp>
