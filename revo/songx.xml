<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>son&gcirc;</rad>/o
</kap>
<drv mrk="songx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Bildoj kaj ideoj, plej ofte senordaj kaj senlogikaj, aperantaj en
      nia imago dum dormo:
      <ekz>
        mi havis tre strangan <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>o teruras, <tld/>o forkuras
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="delir.0o">deliro</ref>,
      <ref cel="fantaz.0o">fantazio</ref>,
      <ref cel="rev">revo</ref>,
      <ref cel="iluzi.0o">iluzio</ref>,
      <ref cel="imag.0o0ado0emo0povo">imago</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">sen</trd>
  <trd lng="de">Traum</trd>
  <trd lng="en">dream</trd>
  <trd lng="it">sogno</trd>
  <trd lng="nl">droom</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_n;<klr tip="ind"> (&c_s;&c_n;&c_o;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_s;&c_n;&c_o;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">dr&ouml;m</trd>
</drv>

<drv mrk="songx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Vidi image dum la dormo:
      <ekz>
        kiel malsatanto <tld/>as, ke li man&gcirc;as<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>is kvaza&ubreve; &scirc;i estas ankora&ubreve; juna knabino<fnt>K</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">tr&auml;umen</trd>
  <trd lng="it">sognare</trd>
  <trd lng="nl">dromen</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_d;&c_je;&c_t;&c_mol; <ind>&c_s;&c_o;&c_n;</ind></trd>,
    <trd>&c_v;&c_i;&c_d;&c_je;&c_t;&c_mol; &c_v;&c_o; <ind>&c_s;&c_n;&c_je;</ind> <klr>(&c_ch;&c_t;&c_o;-&c_l;.)</klr></trd>
  </trdgrp>
  <trd lng="ru"></trd>
  <trd lng="sv">dr&ouml;mma</trd>
</drv>
</art>
