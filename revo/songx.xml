    <trd>r&ecirc;ve</trd>,
    <trd>songe</trd>
  </trdgrp>
  <trd lng="hu">&aacute;lom</trd>
  <trdgrp lng="id">
    <trd>mimpi</trd>,
    <trd>bunga tidur</trd>
  </trdgrp>
  <trd lng="it">sogno</trd>
  <trd lng="ja">&#22818;  <pr>&#12422;&#12417;</pr></trd>
  <trd lng="ne">&#2360;&#2346;&#2344;&#2366;</trd>
  <trd lng="nl">droom</trd>
  <trdgrp lng="pl">
    <trd>marzenie  <klr>(senne)</klr></trd>,
    <trd>sen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_n;<klr tip="ind">
    (&c_s;&c_n;&c_o;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_s;&c_n;&c_o;&c_v;&c_i;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">dr&ouml;m</trd>
  <trdgrp lng="uk">
