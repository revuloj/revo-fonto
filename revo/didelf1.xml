<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: didelf1.xml,v 1.16 2023/11/04 14:29:01 revo Exp $">
<kap>
  <rad>didelfed</rad>/oj <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="didelf1.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="didelf1.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgrandaj amerikaj arbogrimpaj mamuloj;
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">familio</ref>
      el la subklaso <ref tip="malprt" cel="marsup.0uloj.ZOO">marsupiuloj</ref>
      (<trd lng="la">Didelphidae</trd>):
      <ekz>
        Amerika harpio: <klr>[&#x2026;]</klr> &ccirc;efa predo estas arbolo&gcirc;antaj mamuloj kiaj
        simioj, koatoj, bradipoj, histrikoj, kinkajuoj, formikoman&gcirc;uloj kaj <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=108774">Amerika harpio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="prt" cel="didelf.0o">didelfo</ref>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_o;&c_s;&c_u;&c_m;&c_y; <klr>(&c_s;&c_ja;&c_m;&#x2019;&c_ja;)</klr></trd>
  <trd lng="ca">did&egrave;lfids</trd>
  <trd lng="cs">va&ccaron;icovit&iacute;</trd>
  <trd lng="de">Beutelratten</trd>
  <trd lng="fr">didelphid&eacute;s</trd>
  <trd lng="hu">ersz&eacute;nyes <ind>patk&aacute;nyok</ind></trd>
  <trdgrp lng="pl">
    <trd>dydelfowate</trd>,
    <trd>szczury workowate</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>didelf&iacute;deos</trd>,
    <trd>didelfi&iacute;deos</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_p;&c_o;&c_s;&c_s;&c_u;&c_m;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">va&ccaron;icovit&eacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x43F;&#x43E;&c_s;&c_u;&#x43C;&#x43E;&c_v;&c_ib;</trd>,
    <trd>&c_d;&c_v;&#x43E;&c_u;&c_t;&c_r;&#x43E;&c_b;&#x43A;&#x43E;&c_v;&c_ib;</trd>
  </trdgrp>
  <trd lng="zh">&#x8CA0;&#x9F20; <pr>f&#xF9;sh&#x1D4;</pr></trd>
</drv>
</art>

