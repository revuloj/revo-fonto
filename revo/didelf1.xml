  <trd lng="de">Beutelratten</trd>
  <trd lng="fr">didelphid&eacute;s</trd>
  <trd lng="hu">ersz&eacute;nyes <ind>patk&aacute;nyok</ind></trd>
  <trd lng="ru">&c_o;&c_p;&c_o;&c_s;&c_s;&c_u;&c_m;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

