<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>didelfed</rad>/oj <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="didelf1.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="didelf1.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgrandaj amerikaj arbogrimpaj mamuloj, kies inoj havas
      sur ventro po&scirc;on por porti la idojn;
      <ref tip="lst" cel="famili.0o.BIO"
      lst="voko:zoologiaj_familioj">familio</ref> el la subklaso <ref
      tip="malprt"
      cel="marsup.0oj">marsupiuloj</ref>
      (<trd lng="la">Didelphidae</trd>).
      </dif>
  </snc>
  <trd lng="ca">did&egrave;lfids</trd>
  <trd lng="de">Beutelratten</trd>
  <trd lng="fr">didelphid&eacute;s</trd>
  <trd lng="hu">ersz&eacute;nyes <ind>patk&aacute;nyok</ind></trd>
  <trdgrp lng="pl">
    <trd>dydelfowate</trd>,
    <trd>szczury workowate</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>didelf&iacute;deos</trd>,
    <trd>didelfi&iacute;deos</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_p;&c_o;&c_s;&c_s;&c_u;&c_m;&c_y; <klr
  tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sk">vačicovité</trd>
</drv>
</art>

