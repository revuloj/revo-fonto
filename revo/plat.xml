<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>plat</rad>/a
</kap>
<drv mrk="plat.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Prezentanta malgrandan dikecon kompare kun la du aliaj dimensioj:
      <ekz>
        <tld/>a po&scirc;horlo&gcirc;o, &scirc;tono, libro;
      </ekz>
      <ekz>
        <tld/>igi sian nazon sur la fenestra vitro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>igi klak&ccirc;apelon.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Prezentanta nur malaltajn reliefojn:
      <ekz>
        <tld/>a viza&gcirc;o;
      </ekz>
      <ekz>
        virino kun <tld/>a brusto;
      </ekz>
      <ekz>
        <tld/>a telero, skulpta&jcirc;o
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a <klr>(tre banala)</klr> prozo, verso.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="eben.0a">ebena</ref>,
      <ref cel="glat.0a">glata</ref>,
      <ref cel="relief.sen0a">senreliefa</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>flach</trd>,
    <trd>platt</trd>
  </trdgrp>
  <trd lng="en">flat</trd>
  <trd lng="fr">plat<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="he">&shin;&tet;&vav;&het;</trd>
  <trd lng="hu">lapos</trd>
  <trd lng="nl">plat</trd>
  <trd lng="pl">p&lstroke;aski</trd>
  <trd lng="ru">&c_p;&c_l;&c_o;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="plat.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Objekto malpli dika ol lar&gcirc;a kaj longa:
      <ekz>
        <tld/>o de glavo;
      </ekz>
      <ekz>
        &scirc;tala, lada, vitra, fotografa <tld/>o;
      </ekz>
      <ekz>
        &scirc;tona <tld/>o de tombo;
      </ekz>
      <ekz>
        <ind>fermo<tld/>o</ind> de pordo<fnt>Z</fnt>
        <klr>(<ref cel="flug.0ilo">flugilo</ref>)</klr>.
        <trd lng="fr">battant</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kling.0o">klingo</ref>,
      <ref cel="lamen.0o">lameno</ref>.
    </refgrp>
  </snc>
  <adm>
    La difinoj ne konvinkas min. Ekz-e man~o aux ~o de glavo pli
    bone kongruas kun la difino de ~ajxo. La francaj trd ne estas
    tre certaj.
    <aut>MB</aut>
  </adm>
  <trdgrp lng="fr">
    <trd>plat<klr tip="ind"> (partie plate de qc)</klr></trd>,
    <trd>plaque</trd>
  </trdgrp>
  <trd lng="he">&lamed;&vav;&het;</trd>
  <trd lng="hu">lap<klr tip="ind"> (lemez)</klr></trd>
  <trd lng="nl">plaat</trd>
  <trd lng="pl">p&lstroke;yta</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_a;&c_s;&c_t;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="plat.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld lit="P"/>a parto de objekto:
      <ekz>
        <uzo tip="fak">TEK</uzo>
        intereta&gcirc;a <tld/>a&jcirc;o de &scirc;tuparo;
      </ekz>
      <ekz>
        la tegmento prezentas <tld/>a&jcirc;on;
      </ekz>
      <ekz>
        <uzo tip="fak">GEOG</uzo>
        la <tld/>a&jcirc;o de Tibeto
        <klr>(altebena&jcirc;o)</klr>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>palier</trd>,
    <trd>plate-forme</trd>,
    <trd>plat<klr tip="ind"> (partie plate de qc)</klr></trd>,
    <trd>plateau<klr tip="ind"> (techn.)</klr></trd>
  </trdgrp>
  <trd lng="he">&mem;&yod;&shin;&vav;&resh;</trd>
  <trdgrp lng="hu">
    <trd>lapos r&eacute;sz</trd>,
    <trd>lap&aacute;ly</trd>
  </trdgrp>
  <trd lng="nl">plateau</trd>
  <trd lng="pl">pomost</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_i;&c_t;&c_a; <klr>(TEK)</klr></trd>,
    <trd>&c_p;&c_l;&c_a;&c_t;&c_o; <klr>(GEOG)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="plat.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>a:
      <ekz>
        <tld/>igi sian nazon sur la fenestra vitro<fnt>B</fnt>;
      </ekz>
      <ekz>
        li derompis la rozon, prem<tld/>igis &gcirc;in en libro kaj forportis
        &gcirc;in tiamaniere kun si
        <fnt><bib>Fab2</bib>, <lok>rozo de la tombo de Homero</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">aplatir</trd>
  <trd lng="he">&lamed;<ind>&shin;&tet;&het;</ind></trd>
  <trdgrp lng="hu">
    <trd>lap&iacute;t<klr tip="ind"> (lapos&iacute;t)</klr></trd>,
    <trd>lapos&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">pletten</trd>
  <trd lng="pl">sp&lstroke;aszcza&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_o;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="plat.cifer0o">
  <kap>cifer<tld/>o</kap>
  <snc mrk="plat.cifer0o.TEK">
    <dif>
      Ekstera <tld/>o de <ref tip="malprt" cel="horlog.0o">horlo&gcirc;o</ref>,
      mezurilo a&ubreve; alia aparato, sur kiu estas surskribitaj ciferoj.
    </dif>
  </snc>
  <trd lng="de">Ziffernblatt</trd>
  <trd lng="fr">cadran</trd>
  <trd lng="hu">sz&aacute;mlap</trd>
  <trd lng="nl">cijferplaat</trd>
  <trd lng="pl">cyferblat</trd>
  <trd lng="ru">&c_c;&c_i;&c_f;&c_je;&c_r;&c_b;&c_l;&c_a;&c_t;</trd>
</drv>

<drv mrk="plat.licenc0o">
  <kap>licenc<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="plat.licenc0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Videbla &scirc;ildo sur a&ubreve;to a&ubreve; alia veturilo portanta
      &gcirc;ian identigan numeron:
      <ekz>
        ekzistas satelitoj kapablaj legi e&ccirc; ciferojn sur licenc<tld/>o de
        a&ubreve;tomobilo
        <fnt>
        <aut>Xu Jinming</aut>,
        <vrk>Kial?</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1999a, numero 9a, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>plaque d'immatriculation</trd>,
    <trd>plaque min&eacute;ralogique</trd>
  </trdgrp>
  <trd lng="he">&lamed;&vav;&het;&yod;&tav; &resh;&yod;&shin;&vav;&yod;</trd>
  <trd lng="pl">tablica rejestracyjna</trd>
</drv>

<drv mrk="plat.man0o">
  <kap>man<tld/>o</kap>
  <snc mrk="plat.man0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Interna flanko de <ref tip="malprt" cel="man.0o.ANA">mano</ref>:
      <ekz>
        la me&hcirc;aniko de la lingvo estis anta&ubreve; mi kvaza&ubreve; sur
        man<tld/>o<fnt>Z</fnt>
        <klr>(tute je mia dispono)</klr>;
      </ekz>
      <ekz>
        Trajan elprenis la tabulon, vi&scirc;is &gcirc;in per manplato
        <fnt>
        <aut>Julian Modest</aut>,
        <vrk>La ikono</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 2001a, numero 12a, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kav.man0o">mankavo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_o;&c_n;&c_mol;</trd>
  <trd lng="de">Handfl&auml;che</trd>
  <trd lng="es">palma <klr>(de la mano)</klr></trd>
  <trd lng="fr">paume</trd>
  <trd lng="hu">teny&eacute;r</trd>
  <trd lng="nl">handpalm</trd>
  <trd lng="pl">d&lstroke;o&nacute;</trd>
  <trd lng="ru">&c_l;&c_a;&c_d;&c_o;&c_n;&c_mol;</trd>
  <trd lng="sv">handflata</trd>
</drv>
</art>
