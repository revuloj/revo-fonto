<drv mrk="plat.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="plat.0ajxo.objekto">
    <dif>
      <tld lit="P"/>a objekto a&ubreve; parto de &gcirc;i:
      <ekz>
        ar&gcirc;enta <tld/>a&jcirc;eto kun la Dipatrino pendis sur lia brusto
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;apelo, kiu povis estis kunpremata, tiel ke oni povis vidi nur la supran
        <tld/>a&jcirc;on kaj la randojn
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>;
      </ekz>
      <ekz>
        tabletoj e&ccirc; kun marmoraj <tld/>a&jcirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tablo kun multaj fakoj super la skriba <tld/>a&jcirc;o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">TEK</uzo>
        intereta&gcirc;a <tld/>a&jcirc;o de &scirc;tuparo;
      </ekz>
      <ekz>
        la tegmento prezentas <tld/>a&jcirc;on;
      </ekz>
      <ekz>
        <uzo tip="fak">GEOG</uzo>
        la <tld/>a&jcirc;o de Tibeto
        <klr>(altebena&jcirc;o)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="eben.0o">ebeno</ref>,
       <ref cel="surfac.0o.KOMUNE">surfaco</ref>
    </refgrp>
    <trd lng="de">Fl&auml;che</trd>
  </snc>
  <trdgrp lng="en">
    <trd>level part</trd>,
    <trd>plateau</trd>,
    <trd>shelf</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>parte plana</trd>,
    <trd>plataforma</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>palier</trd>,
    <trd>plate-forme</trd>,
    <trd>plat<klr tip="ind"> (partie plate de qc)</klr></trd>,
    <trd>plateau<klr tip="ind"> (techn.)</klr></trd>
  </trdgrp>
  <trd lng="he">&mem;&yod;&shin;&vav;&resh;</trd>
  <trdgrp lng="hu">
    <trd>lapos r&eacute;sz</trd>,
    <trd>lap&aacute;ly</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>piatto<klr> (parte piatta)</klr></trd>,
    <trd>ripiano</trd>,
    <trd>schiacciatura</trd>,
    <trd>tavolato</trd>,
    <trd>tavellone</trd>,
    <trd>tavolato<klr> (geog.)</klr></trd>,
    <trd>tavoliere<klr> (geog.)</klr></trd>,
    <trd>pianoro<klr> (geog.)</klr></trd>
  </trdgrp>
  <trd lng="nl">plateau</trd>
  <trd lng="pl">pomost</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_i;&c_t;&c_a; <klr>(TEK)</klr></trd>,
    <trd>&c_p;&c_l;&c_a;&c_t;&c_o; <klr>(GEOG)</klr></trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>plato</trd>,
    <trd>ova</trd>
  </trdgrp>
</drv>

<drv mrk="plat.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plat.0igi.KOMUNE">
    <dif>
      Igi <tld/>a:
      <ekz>
        lar&gcirc;a vosto, platigita de la flankoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ordigis siajn pantoflojn, &ccirc;ar tio estis &scirc;uoj, kiujn li estis tret<tld/>iginta
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li derompis la rozon, prem<tld/>igis &gcirc;in en libro kaj
        forportis &gcirc;in tiamaniere kun si
        <fnt><bib>Fab2</bib>,
        <lok>rozo de la tombo de Homero</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">platt machen</trd>
  </snc>
  <trd lng="cs">zplo&scaron;tit</trd>
  <trd lng="en">flatten</trd>
  <trdgrp lng="es">
    <trd>aplanar</trd>,
    <trd>allanar</trd>
  </trdgrp>
  <trd lng="fr">aplatir</trd>
  <trd lng="he">&lamed;<ind>&shin;&tet;&het;</ind></trd>
  <trdgrp lng="hu">
    <trd>lap&iacute;t<klr tip="ind"> (lapos&iacute;t)</klr></trd>,
    <trd>lapos&iacute;t</trd>
  </trdgrp>
  <trd lng="it">appiattire</trd>
  <trd lng="nl">pletten</trd>
  <trd lng="pl">sp&lstroke;aszcza&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_o;&c_v;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">splo&scaron;ti&tcaron;</trd>
  <trd lng="tr">yass&inodot;la&scedil;t&inodot;rmak</trd>
</drv>

<drv mrk="plat.dis0igxi">
  <kap>dis<tld/>i&gcirc;i</kap>
  <snc mrk="plat.dis0igxi.KOMUNE">
    <dif>
       Frakasi&gcirc;i kaj plati&gcirc;i, albati&gcirc;ante al ia surfaco:
       <ekz>
        post longa flugado li atingis la teron, kie li
        kompreneble dis<tld/>i&gcirc;is kiel ova&jcirc;o
        <fnt>
          <aut>J. Dorosmai, trad. J. Dorosmai jun. kaj J. Horvath</aut>
          <vrk>Fabloj kaj Aforismoj</vrk>
          <lok>2002</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">s'<ind>&eacute;craser</ind></trd>
</drv>

<drv mrk="plat.cifer0o">
  <kap><ofc>*</ofc> cifer<tld/>o <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-4992">cifer'plat'</url></lok></fnt></kap>
  <snc mrk="plat.cifer0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ekstera <ref tip="super" cel="plat.0o.KOMUNE"><tld/>o</ref> de
      <ref tip="malprt" cel="horlog.0o">horlo&gcirc;o</ref>, mezurilo
      a&ubreve; alia aparato, sur kiu estas surskribitaj ciferoj:
      <ekz>
        la astronomia horlo&gcirc;o enhavas klasikan horlo&gcirc;an cifer<tld/>on, kalendaron,
        astronomian parton kaj planedan parton
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=290483">&Ccirc;ambra astronomia horlo&gcirc;o de Jan Ma&scaron;ek</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        centre de konveksa regpanelo elstaris lar&gcirc;a kaj purpura cifer<tld/>o
        <fnt>
          <aut>Ivan Efremov</aut>
          <vrk><url ref="https://esperanto.mv.ru/Efremov/andromedo_01.html">La nebulozo de Andromedo</url></vrk>
          <lok>Chapitro 1. Fera stelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;to-cifer<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="https://www.monato.be/2011/010446.php">Lingvo a&ubreve;to-didakta</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>cifern&iacute;k</trd>,
    <trd>&ccaron;&iacute;selnice</trd>,
    <trd>&ccaron;&iacute;seln&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Ziffernblatt</trd>
  <trdgrp lng="en">
    <trd>clock face</trd>,
    <trd>dial</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>esfera <klr>(del reloj)</klr></trd>
  </trdgrp>
  <trd lng="fr">cadran</trd>
  <trd lng="hu">sz&aacute;mlap</trd>
  <trd lng="it">quadrante<klr> (di orologio o sim.)</klr></trd>
  <trd lng="nl">cijferplaat</trd>
  <trd lng="pl">cyferblat</trd>
  <trd lng="ru">&c_c;&c_i;&c_f;&c_je;&c_r;&c_b;&c_l;&c_a;&c_t;</trd>
  <trd lng="sk">cifern&iacute;k</trd>
  <trd lng="tr">kadran</trd>
</drv>

<drv mrk="plat.farbo0o">
  <kap>farbo<tld/>o</kap>
  <snc mrk="plat.farbo0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="dif" cel="paletr.0o.BELA">Paletro</ref>:
      <ekz>
        la&ubreve;stile liaj pentra&jcirc;oj komence tre similas al tiuj de <nom>Rembrandt</nom>, <klr>[sed
        poste]</klr> lia farbo<tld/>o heli&gcirc;as kaj fari&gcirc;as pli multkolora, per kio
        liaj bildoj efikas pli serenaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=340057">Govaert Flinck</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiu premrulilo minacas forvi&scirc;i &ccirc;iujn kolorojn de la
        farbo<tld/>o de l' homaro
        <fnt>
          <aut>A. Sol&jcirc;enicin</aut>
          <bib>Viki</bib>
          <lok>cito en artikolo &leftquot;naciismo&rightquot;,
          2008-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_t;&c_r;&c_a;</trd>
    <trd lng="ca">paleta</trd>
    <trd lng="de">Palette</trd>
    <trd lng="en">palette</trd>
    <trd lng="es">paleta<klr> (de pintura o
    colores)</klr></trd>
    <trd lng="fr">palette<klr tip="ind"> (de peintre)</klr></trd>
    <trd lng="hu">paletta</trd>
    <trd lng="it">tavolozza</trd>
    <trd lng="nl">palet</trd>
    <trd lng="pl">paleta</trd>
    <trdgrp lng="pt">
      <trd>paleta</trd>,
      <trd>palheta</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_a;&c_l;&c_i;&c_t;&c_r;&c_a;</trd>
  <trdgrp lng="tr">
    <trd>palet</trd>,
    <trd>renk paleti</trd>
  </trdgrp>
</drv>

<drv mrk="plat.licenc0o">
  <kap>licenc<tld/>o</kap> <fnt><bib>EBV</bib></fnt>
  <snc mrk="plat.licenc0o.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Videbla identiga numer<ref tip="super" cel="plat.0o.KOMUNE"><tld/>o</ref> sur veturilo:
      <ekz>
        vantaj a&ubreve;tomobilistoj povis elekti licenc-<tld/>ojn kun siaj personaj
        bon&scirc;anco-ciferoj
        <fnt>
          <bib>Monato</bib>
          <aut>MNL</aut>
          <vrk><url ref="https://www.monato.be/2004/008441.html">Filipinoj a&ubreve;kcias licenc-platojn de a&ubreve;toj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekzistas satelitoj kapablaj legi e&ccirc; ciferojn sur
        licenc<tld/>o de a&ubreve;tomobilo
        <fnt>
          <aut>Xu Jinming</aut>,
          <vrk>Kial?</vrk>,
          <bib>Monato</bib>,
          <lok>1999:9, p. 10a</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="plat.numer0o">numerplato</ref>
    <trd lng="de">Nummernschild <klr>(Auto)</klr></trd>
  </snc>
  <trd lng="cs">st&aacute;tn&iacute; pozn&aacute;vac&iacute; zna&ccaron;ka auta</trd>
  <trd lng="en">license plate</trd>
  <trd lng="es">matr&iacute;cula<klr> (de autom&oacute;vil)</klr></trd>
  <trdgrp lng="fr">
    <trd>plaque d'immatriculation</trd>,
    <trd>plaque min&eacute;ralogique</trd>
  </trdgrp>
  <trd lng="he">&lamed;&vav;&het;&yod;&tav; &resh;&yod;&shin;&vav;&yod;
  </trd>
  <trd lng="it">targa<klr> (di automobile)</klr></trd>
  <trd lng="pl">tablica rejestracyjna</trd>
  <trd lng="ru">
  &c_n;&c_o;&c_m;&c_je;&c_r;&c_n;&c_o;&c_j; &c_z;&c_n;&c_a;&c_k;<klr>
  (&c_a;&c_v;&c_t;&c_o;&c_m;&c_o;&c_b;&c_i;&c_l;&c_ja;)</klr></trd>
  <trd lng="sk">&scaron;t&aacute;tna pozn&aacute;vacia zna&ccaron;ka auta</trd>
  <trdgrp lng="tr">
    <trd>plaka</trd>,
    <trd>ara&ccedil; plakas&inodot;</trd>
  </trdgrp>
</drv>

<drv mrk="plat.man0o">
  <kap><ofc>*</ofc>man<tld/>o <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5099">man'plat'</url></lok></fnt></kap>
  <snc mrk="plat.man0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Interna flanko de <ref tip="malprt" cel="man.0o.ANA">mano</ref>:
      <ekz>
        la pastro prenos iom <klr>[&#x2026;]</klr> da oleo kaj ver&scirc;os sur sian maldekstran
        man<tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 14:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        peco da tero dudek-hektara, formanta malgrandan kvadraton, kiun de la supro de
        l' domo oni povis &ccirc;irka&ubreve;preni per la rigardo, kvaza&ubreve; sur man<tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la me&hcirc;aniko de la lingvo estis anta&ubreve; mi 
        kvaza&ubreve; sur la man<tld/>o
        <fnt>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0015/0022.php">Lingvaj respondoj</url></vrk>
        </fnt>
        <klr>(tute je mia dispono)</klr>;
      </ekz>
      <ekz>
        la juvelisto jam per amba&ubreve; man<tld/>oj ekfrotis siajn harojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sub la man<tld/>o, per kiu &scirc;i &scirc;irmis al si la okulojn, oni povis vidi, ke &scirc;ia
        viza&gcirc;o estas pala kaj senmova
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        li havas truon en la man<tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Trajan</nom> elprenis la tabulon, vi&scirc;is &gcirc;in per manplato
        <fnt>
        <aut>J. Modest</aut>,
        <vrk>La ikono</vrk>,
        <bib>Monato</bib>,
        <lok>2001:12, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kav.man0o">mankavo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_o;&c_n;&c_mol;</trd>
  <trd lng="bo">&#xF50;&#xF63;&#xF0B;&#xF58;&#xF7C;&#xF0B;</trd>
  <trd lng="cs">dla&ncaron;</trd>
  <trd lng="de">Handfl&auml;che</trd>
  <trd lng="en">palm</trd>
  <trd lng="es">palma <klr>(de la mano)</klr></trd>
  <trdgrp lng="fr">
    <trd>plat de la main</trd>,
    <trd>paume</trd>
  </trdgrp>
  <trd lng="hu">teny&eacute;r</trd>
  <trd lng="it">palmo</trd>
  <trd lng="nl">handpalm</trd>
  <trd lng="pl">d&lstroke;o&nacute;</trd>
  <trd lng="ru">&c_l;&c_a;&c_d;&c_o;&c_n;&c_mol;</trd>
  <trd lng="sk">dla&ncaron;</trd>
  <trd lng="sv">handflata</trd>
  <trdgrp lng="tr">
    <trd>avu&ccedil;</trd>,
    <trd>avu&ccedil; i&ccedil;i</trd>,
    <trd>aya</trd>
  </trdgrp>
</drv>

<drv mrk="plat.numer0o">
  <kap>numer<tld/>o</kap>
  <snc mrk="plat.numer0o.KOMUNE">
    <dif>
      <ref tip="super" cel="plat.0o.KOMUNE"><tld lit="P"/>o</ref>
      surhavanta la numeron de la koncerna objekto:
      <ekz>
        iam iu koliziis kontra&ubreve; mia parkumita a&ubreve;to kaj forfu&gcirc;is, sed mia turka najbaro
        rapidis al mia apartamento kun la notita numer<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="https://www.monato.be/2012/010808.php">Rajtoj pri ritoj</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter&scirc;an&gcirc;i ne plu uzatajn a&ubreve;tajn
        numer<tld/>ojn
        <fnt>
          <aut>C. Zervic</aut>,
          <vrk>A&ubreve;taj Numerplatoj</vrk>,
          <lok><url ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">
          soc.culture.esperanto</url>,
          2003-11-19</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Nummernschild</trd>
  </snc>
  <trd lng="cs">st&aacute;tn&iacute; pozn&aacute;vac&iacute; zna&ccaron;ka</trd>
  <trd lng="es">placa de matriculaci&oacute;n</trd>
  <trd lng="fr">plaque d'immatriculation</trd>
  <trdgrp lng="it">
     <trd>targa<klr> (di auto)</klr></trd>,
     <trd>targhetta<klr> (matricola)</klr></trd>
  </trdgrp>
  <trd lng="sk">&scaron;t&aacute;tna pozn&aacute;vacia zna&ccaron;ka</trd>
  <trd lng="tr">plaka</trd>
</drv>

<drv mrk="plat.pied0o">
  <kap>pied<tld/>o</kap>
  <snc mrk="plat.pied0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      <ref tip="dif" cel="pland.0o.ANA">Plando</ref>:
      <ekz>
        <nom>Mahdi</nom> ku&scirc;is sur mola divano, &ccirc;irka&ubreve;ita de siaj
        edzinoj, el
        kiuj du ventumis lin per grandaj strutaj plumoj, kaj du aliaj
        gratis delikate liajn pied<tld/>ojn
        <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj
        praarbaro</url></vrk>, <lok>Varsovio, 1978</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Fu&szlig;sohle</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_e;&c_sh;&c_v;&c_a;
  <klr>(&c_n;&c_a;&c_g;&c_ib;)</klr></trd>
  
  <trd lng="en">sole</trd>
  <trd lng="es">planta del pie</trd>
  <trd lng="fr">plante du pied</trd>
  <trd lng="hu">talp</trd>
  <trd lng="it">pianta<klr> (del piede)</klr></trd>
  <trdgrp lng="nl">
    <trd>zool</trd>,
    <trd>voetzool</trd>
  </trdgrp>
  <trd lng="pl">podeszwa <klr>(stopy)</klr></trd>
  <trd lng="pt">planta do <klr tip="amb">p&eacute;</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_sh;&c_v;&c_a;
  <klr tip="amb">(&c_n;&c_o;&c_g;&c_i;)</klr></trd>
  <trd lng="tr">taban</trd>
</drv>

<drv mrk="plat.sxton0o">
  <kap>&scirc;ton<tld/>o</kap>
  <snc mrk="plat.sxton0o.KON">
    <uzo tip="fak">KON</uzo>
    <dif>
      <tld lit="P"/>o fasonita en &scirc;tono:
      <ekz>
        &ccirc;e fontano estas &scirc;ton<tld/>o, sur kiu estas gravurita jena noto <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="https://www.monato.be/2008/009358.php">Nagasako</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu &scirc;ton<tld/>o sur la tombo estas monumento
        de la ka&scirc;ita ateismo, por vera kristano decas
        nur ligna krucifikso
        <fnt><aut>V. O&scaron;lak</aut>, <vrk>Foje e&ccirc; pensi estas &gcirc;uo</vrk>, 
        <lok>2003</lok></fnt>;
      </ekz> 
      <ekz>
        <klr>[&scirc;i]</klr> nudpiede ekpa&scirc;is sur la &scirc;ton<tld/>a kuireja planko
        <fnt>
          <bib>Monato</bib>
          <aut>Julian Modest</aut>
          <vrk><url ref="https://www.monato.be/2015/011473.php">Budape&scirc;ta a&ubreve;tuno</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="slab.0o.KON">slabo</ref>,
      <ref cel="kahel.0o">kahelo</ref>,
      <ref cel="pavim.0o">pavimo</ref>,
      <ref cel="trotua.0o">trotuaro</ref>
    </refgrp>
    <trd lng="de">Steinplatte</trd>
  </snc>
  <trd lng="es">losa de piedra</trd>
  <trd lng="fr">dalle de pierre</trd>
  <trd lng="tr">parke ta&scedil;&inodot;</trd>
</drv>
