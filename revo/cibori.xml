<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cibori.xml,v 1.8 2022/08/23 07:35:34 revo Exp $">
<kap>
  <rad>cibori</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="cibori.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="cibori.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <ref tip="dif" cel="hosti.0ujo">Hostiujo</ref>:
      <ekz>
        kiam la pastro levis la <tld/>on, ni diris: <ctl>Mia vivo estas
        via</ctl>
        <fnt><aut>B. Vaha</aut>, <vrk>Adolesko</vrk>,
        <lok>1987</lok></fnt>;
      </ekz>
      <ekz>
        sur la altaro estas anka&ubreve; kruco kaj <tld/>o
        <fnt>
          <aut>I. Smykov</aut>,
          <vrk><url
          ref="http://groups.yahoo.com/group/kristana-ortodoksa-rondo-esperantista/message/766">
          Medite pri la Nova Tabelo</url></vrk>,
          <lok>kristana-ortodoksa-rondo-esperantista, 2006-09-17</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter la rimarkinda&jcirc;oj: <tld/>o (12a jarcento),
        <fnt>
          <vrk><url ref="https://www.perigueux-maap.fr/files/2012/03/plan-en-esperanto.pdf">Muzeo
          Arta kaj Arkeologia de Perigordo</url></vrk>
          <lok>perigueux-maap.fr</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_b;&c_o;&c_r;&c_y;&c_j;</trd>
  <trd lng="cs">ciborium</trd>
  <trd lng="de">Ziborium</trd>
  <trd lng="fr">ciboire</trd>
  <trd lng="it">ciborio</trd>
  <trdgrp lng="sk">
    <trd>cib&oacute;rium</trd>,
    <trd>kalich na hostie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_i;&c_v;&#x43E;&c_r;&c_ib;&c_j;</trd>,
    <trd>&c_d;&c_a;&c_r;&#x43E;&#x43D;&#x43E;&c_s;&c_i;&c_c;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

