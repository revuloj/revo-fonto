<art mrk="$Id: pub.xml,v 1.20 2023/12/06 07:24:30 revo Exp $">
<kap>
  <ofc>9</ofc><rad>pub</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pub.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">ANA</uzo>
    <dif>
      Malsupra parto de la ventro, kiu kovras la <ref cel="ost.pub0o"><tld/>oston</ref>:
      <ekz>
        kresko de <klr>[vira]</klr> hararo sur la viza&gcirc;o (barbo kaj
        lipharoj)
        <klr>[estas]</klr> pli dens<klr>[a]</klr> kaj long<klr>[a]</klr>
        ol <klr>[&ccirc;e]</klr> virinoj
        anka&ubreve; &ccirc;e aliaj partoj de la korpo (brakoj, kruroj, brusto,
        abdomeno, akselo, kaj <tld/>o)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=40624">Viro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        iuj &scirc;losilaj nov-evolua&jcirc;oj de la alosa&ubreve;redoj estas
        <klr>[&#8230;]</klr> longaj,
        kompaktaj kruroj, rigidaj vostoj, kaj pligrandigita &leftquot;boto&rightquot; fine
        de la pubo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=202625">Alosa&ubreve;redoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="en">pubic region</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_sh;&c_m;&c_a;&c_k;</trd>,
    <trd>&c_l;&c_a;&c_b;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Schamgegend</trd>
  <trd lng="fr">pubis<klr> (r&eacute;gion du)</klr></trd>
  <trdgrp lng="hu">
    <trd>szem&eacute;remt&aacute;j&eacute;k</trd>,
    <trd>szem&eacute;remdomb</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>pube</trd>,
    <trd>zona pubica</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24677;&#x9AA8;&#x90E8;  <pr>&#x306F;&#12376;&#12371;&#12388;&#12406;</pr></trd>,
    <trd>&#24677;&#x90E8;  <pr>&#12385;&#12406;</pr></trd>,
    <trd>&#38512;&#x90E8;  <pr>&#12356;&#12435;&#12406;</pr></trd>
  </trdgrp>
  <trd lng="nl">schaamstreek</trd>
  <trd lng="pl">&lstroke;ono <klr>(anatomia)</klr></trd>
  <trd lng="ru">&c_l;&c_o;&c_b;&c_o;&c_k;</trd>
  <trd lng="uk">&#x43B;&#x43E;&c_b;&#x43E;&#x43A;</trd>
</drv>

</art>
