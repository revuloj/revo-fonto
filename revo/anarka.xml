<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>anakadi</rad>/acoj
</kap>
<drv mrk="anarka.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="anakar.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Anacardium</trd>), kies unu specio estas
      la <ref tip="sub" cel="akajxu.0ujo">aka&jcirc;uujo</ref>.
    </dif>
  </snc>
</drv>
<drv mrk="anarka.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Anacardiaceae</trd>), tropikaj
      lignoplantoj.
    </dif>
  </snc>
  <trd lng="de">Sumachgew&auml;chse</trd>
</drv>
</art>
