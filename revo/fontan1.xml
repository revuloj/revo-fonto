<kap><rad>fontanel</rad>/o<fnt>Z</fnt></kap>
<drv mrk="fontan1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Mola parto de la <ref tip="malprt" cel="krani.0o.ANA">kranio</ref>
      &ccirc;e tre junaj infanoj:
      <ekz>
        la osti&gcirc;o de la kraniostoj igas la anta&ubreve;an <tld/>on fermi&gcirc;i en a&gcirc;o de 18 &gcirc;is 24
        monatoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=427981">Fontanelo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_n;&c_ib;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
  <trd lng="cs">tem&iacute;nko</trd>
  <trd lng="de">Fontanelle</trd>
  <trd lng="en">fontanel(le)</trd>
  <trd lng="es">fontanela</trd>
  <trd lng="fr">fontanelle</trd>
  <trdgrp lng="hu">
    <trd>kutacs</trd>,
    <trd>fontanella</trd>
  </trdgrp>
  <trd lng="it">fontanella<klr> (anat.)</klr></trd>
  <trd lng="ja">&#x6CC9;&#38272;  <pr>&#x305B;&#12435;&#12418;&#12435;</pr></trd>
  <trd lng="nl">fontanel</trd>
  <trd lng="pl">ciemi&aogonek;czko</trd>
  <trdgrp lng="pt">
    <trd>fontanela</trd>,
    <trd>moleira</trd>,
    <trd>font&iacute;culo</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_n;&c_i;&c_ch;&c_o;&c_k;</trd>
  <trd lng="sk">fontanela</trd>
  <trd lng="uk">&c_t;&c_ib;&#x43C;&#8217;&#x44F;&c_ch;&#x43A;&#x43E;</trd>
</drv>
