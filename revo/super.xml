  <drv mrk="super.al0">
    <kap>al <tld/>, <var><kap><tld/> ...-n</kap></var></kap>
    <snc>
      <dif>
        <ref cel="al.0">Al</ref> loko situanta
        <ref cel="super.0.loko"><tld/> <sncref/></ref>:
        <ekz>
          la fi&scirc;oj elsaltis <tld/> la akvon<fnt>Z</fnt>;
        </ekz>
        <ekz>
          etendu vian manon <tld/> la riverojn kaj la
          lagojn<fnt>Z</fnt>;
        </ekz>
        <ekz>
          la suno levi&gcirc;is <tld/> la teron<fnt>Z</fnt>;
        </ekz>
        <ekz>
          la akvo &scirc;prucas alte super nin<fnt>Z</fnt>;
        </ekz>
        <ekz>
          la aer&scirc;ipo flugas <tld/> la landon<fnt>Z</fnt>;
        </ekz>
        <ekz>
          la potenca metala sono trakuris la kortojn, resaltis de la
          dikaj
          muroj de l' templo kaj forflugis <tld/> la tritikajn
          kampojn<fnt>B</fnt>;
        </ekz>
        <ekz>
          dume Dio, la Eternulo, pretigis hederon, kaj &gcirc;i
          elkreskis <tld/> lian kapon, por ke li havu ombron
          <tld/> sia kapo kaj liberi&gcirc;u el sia suferado
          <fnt><bib>MT</bib>, <lok>&Jon; 4:6</lok></fnt>.
        </ekz>
      </dif>
      <rim>
        Pri la prepoziciaj kuna&jcirc;oj kun <em>al</em>,
        vidu rimarkon sub
        <ref cel="cxe.al0.kunajxo_kun_al">al &ccirc;e</ref>.
      </rim>
    </snc>
    <trd lng="hu">f&ouml;l&eacute;</trd>
  <trdgrp lng="th">
    <trd>&#3586;&#3638;&#3657;&#3609;&#3652;&#3611;&#3586;&#3657;&#3634;&#3591;&#3610;&#3609;</trd>,
    <trd>&#3586;&#3638;&#3657;&#3609;&#3652;&#3611;&#3648;&#3627;&#3609;&#3639;&#3629;</trd>
  </trdgrp>
  </drv>
  <drv mrk="super.de0">
    <kap>de <tld/></kap>
    <snc>
      <dif>
        <ref cel="de.0">De</ref>
        <sncref ref="de.0.spaco"/>,<sncref ref="de.0.origino"/>
        loko situanta
        <ref cel="super.0"><tld/></ref>:
        <ekz>
          levi&gcirc;is la nubo de <tld/> la tabernaklo de atesto
          <fnt><bib>MT</bib><lok>&Nom; 10:11</lok></fnt>;
        </ekz>
        <ekz>
          <klr>[la virino]</klr> levis la kapon de <tld/> la laboro
          <fnt><bib>Marta</bib></fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="hu">f&ouml;l&uuml;l</trd>
  </drv>
