  <drv mrk="super.0i">
  <kap><ofc>*</ofc><tld/>i <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5212">super'i</url></lok></fnt></kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>a je io a&ubreve; iu:
      <ekz>
        la re&gcirc;o Salomono <tld/>is &ccirc;iujn re&gcirc;ojn de la tero per ri&ccirc;eco kaj per sa&gcirc;eco
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 10:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        miaj krimoj <tld/>as mian kapon <klr>[&#8230;]</klr> ili estas tro pezaj por mi
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 38:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu sento <klr>[&#8230;]</klr> <tld/>is &ccirc;iujn ceterajn
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la varmo ne <tld/>is 15 gradojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        ebena&jcirc;o, kies supra&jcirc;o ne <tld/>as unu kvadratan kilometron
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        mem <klr>[&#8230;]</klr> aran&gcirc;i &ccirc;iujn aferojn, tio <tld/>as la homajn fortojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la laci&gcirc;o <tld/>is la eksciton
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        malbona virino diablon <tld/>as
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        pardonemeco <tld/>as justecon
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
