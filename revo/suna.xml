<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Suna</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="suna.0o">
  <kap><tld/>o</kap>
  <snc mrk="suna.0o.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      Tuto de la solvoj, donitaj de
      <ref tip="vid" cel="muhama.0o.profeto">Muhamado</ref>
      al la aferoj, submetitaj al lia ju&gcirc;ado; tiujn solvojn
      parkeris kaj kolektis liaj posteuloj en tradician doktrinon, por
      kompletigi la <ref tip="vid" cel="koran.0o">Koranon</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_n;&c_a;</trd>
  <trd lng="ca">sunna</trd>
  <trd lng="de">Sunna</trd>
  <trd lng="fr">sunna</trd>
  <trd lng="hu">szunna</trd>
  <trd lng="id">sunah</trd>
  <trd lng="it">sunna</trd>
  <trd lng="nl">soenna</trd>
  <trd lng="ru">&c_s;&c_u;&c_n;&c_n;&c_a;</trd>
</drv>

<drv mrk="suna.0ismo">
  <kap><tld lit="s"/>ismo</kap>
  <snc mrk="suna.0ismo.ISL">
    <uzo tip="fak">ISL</uzo>
    <dif>
      Nomo de la ortodoksa formo de
      <ref tip="super" cel="islam.0o">islamo</ref>, agnoskanta
      <tld/>on.
    </dif>
    <ref tip="vid" cel="sxijaism.0o">&scirc;ijaismo</ref>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trd lng="de">Sunnismus</trd>
  <trd lng="fr">sunnisme</trd>
  <trd lng="hu">szunnitizmus</trd>
  <trd lng="id">Sunni</trd>
  <trd lng="it">sunnismo</trd>
  <trd lng="nl">soennisme</trd>
  <trd lng="ru">&c_s;&c_u;&c_n;&c_n;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
