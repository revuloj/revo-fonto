<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Suna</rad>/o
</kap>
<drv mrk="suna.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ISL</uzo>
  <snc>
    <dif>
      Tuto de la solvoj, donitaj de
      <ref tip="vid" cel="muhama.0o.profeto">Muhamado</ref>
      al la aferoj, submetitaj al lia ju&gcirc;ado; tiujn solvojn transdonis
      parkere liaj kunuloj, kaj kolektis liaj posteuloj en korpon de
      doktrino, por kompletigi la
      <ref tip="vid" cel="koran.0o">Koranon</ref>.      
    </dif>
  </snc>
  <trd lng="de">Sunna</trd>
  <trd lng="nl">soenna</trd>
  <trd lng="ru">&c_s;&c_u;&c_n;&c_n;&c_a;</trd>
</drv>
<drv mrk="suna.0ismo">
  <kap><tld lit="s"/>ismo</kap>
  <uzo tip="fak">ISL</uzo>
  <snc>
    <dif>
      Nomo de la ortodoksa formo de
      <ref tip="super" cel="islam.0o">islamo</ref>, agnoskanta
      <tld/>on.
    </dif>  
    <ref tip="vid" cel="sxijaism.0o">&scirc;ijaismo</ref>
  </snc>
  <trd lng="de">Sunnismus</trd>
  <trd lng="nl">soennisme</trd>
  <trd lng="ru">&c_s;&c_u;&c_n;&c_n;&c_i;&c_z;&c_m;</trd>
</drv>
</art>

