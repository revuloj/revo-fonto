<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: majoran.xml,v 1.18 2018/10/07 17:10:25 revo Exp $">
<kap>
<rad>majoran</rad>/o
</kap>
<drv mrk="majoran.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="spic.0o" lst="voko:spicoj">Spico</ref> el
      sekigitaj plantoj de
      <tld/>a <ref tip="vid" cel="origan.0o">origano</ref>
      (<trd lng="la">Origanum majorana</trd>):
      <ekz>
        <tld/>o fajnigas &ccirc;efe grasajn vianda&jcirc;ojn, kolbasojn,
        terpoma&jcirc;ojn kaj sa&ubreve;cojn
        <fnt><bib>Viki</bib>, <lok>majorano</lok></fnt>.
      </ekz>
    </dif>
    <bld
       lok="&WCU;/thumb/5/5c/Origanum_majorana_002.JPG/450px-Origanum_majorana_002.JPG"
       prm="CC BY-SA 3.0"
       >floraro de <tld/>a origano (Origanum majorana), Botanika &Gcirc;ardeno KIT, Karlsruhe, Germanujo
         <fnt>
           <aut>H. Zell, propra verko</aut>
           <lok><url ref="&WCW;/File:Origanum_majorana_002.JPG"
               >&WCW;/File:Origanum_majorana_002.JPG</url></lok>
         </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_ja;&c_r;&c_a;&c_n;</trd>
  <trd lng="bg">&c_m;&c_a;&c_j;&c_o;&c_r;&c_a;&c_n;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>marduix</trd>,
    <trd>am&agrave;rac</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>major&aacute;n</trd>,
    <trd>major&aacute;nka</trd>
  </trdgrp>
  <trd lng="de">majoran</trd>
  <trdgrp lng="en">
    <trd>marjoram</trd>,
    <trd>sweet marjoram</trd>
    <trd>knotted marjoram</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>mejorana</trd>,
    <trd>mayorana</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_r;&a_z;&a_n;&f_g;&a_w;&a_sh;</trd>
  <trd lng="fr"> marjolaine</trd>
  <trd lng="hu">major&aacute;nna</trd>
  <trd lng="nl">marjolein</trd>
  <trd lng="pl">majeranek</trd>
  <trd lng="pt">manjerona</trd>
  <trd lng="ru">&c_m;&c_a;&c_j;&c_o;&c_r;&c_a;&c_n;</trd>
  <trd lng="sk">major&aacute;nka</trd>
</drv>
</art>

