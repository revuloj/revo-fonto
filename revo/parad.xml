<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: parad.xml,v 1.29 2023/09/10 12:37:54 revo Exp $">
<kap>
  <ofc>*</ofc><rad>parad</rad>/o <fnt><bib>PV</bib></fnt>
 </kap>

<drv mrk="parad.0o">
  <kap><tld/>o</kap>
  <snc mrk="parad.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Solena militista <ref tip="super" cel="marsx.0o.ago">mar&scirc;o</ref>:
      <ekz>
        en la sekvanta tago <klr>[&#8230;]</klr> venis <nom>Agripo</nom> kaj <nom>Bernike</nom> kun
        grandioza
        <tld/>o, kaj ili eniris en la a&ubreve;ditorion kun la &ccirc;efkapitanoj kaj la
        eminentuloj el la urbo
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 25:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        la soldatoj kun etenditaj standardoj kaj brilantaj bajonetoj staris orditaj en
        <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        vespere la tuta urbo estis feste iluminita,
        la kanonoj tondris <ctl>bum!</ctl> kaj la
        soldatoj faris <tld/>on
        <fnt><bib>Fab2</bib>, <lok>La ombro</lok></fnt>;
     </ekz>
      <ekz>
        la funebra soleno estas aran&gcirc;ata en plena <tld/>o <klr>[&#8230;]</klr>, kaj ni
        elmar&scirc;is <klr>[&#8230;]</klr> havante lanternon en unu mano kaj niajn spadojn en la
        alia
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="revu.0o.MIL">revuo<sncref/></ref>
    </refgrp>
    <trd lng="br">gweladeg</trd>
    <trdgrp lng="hu">
      <trd>d&iacute;sszemle</trd>,
      <trd>katonai par&aacute;d&eacute;</trd>
    </trdgrp>
    <trd lng="pl">parada</trd>
  </snc>
  <snc mrk="parad.0o.procesio">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Alia impona mar&scirc;ado,
      <ref tip="sin" cel="proces1.0o">procesio</ref>:
      <ekz>
        karnavala <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;tono de la sa&gcirc;uloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la 4a <tld lit="P"/>o de Geja Fiero
        <fnt>
          <vrk>Esperanto en geja parado</vrk>,
          <bib>LOdE</bib>, <lok>N-ro 71 (2000:8&ndash;9)</lok>.
        </fnt>.
      </ekz>
   </dif>
   <trdgrp lng="de">
      <trd>Aufmarsch</trd>,
      <trd>Festumzug</trd>,
      <trd>Umzug <klr>(Parade)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="parad.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Festena, solena, pompa a&ubreve; fanfarona aran&gcirc;o, elmontrado,
      sinteno:
      <ekz>
        la pomarboj kaj persikarboj <klr>[&#8230;]</klr> staris en plena <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de la jaro</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la palaco &ccirc;io estis en festa <tld/>o
        <fnt><bib>Fab2</bib>, <lok>La najtingalo</lok></fnt>;
     </ekz>
      <ekz>
        &scirc;i venis vizite <klr>[&#8230;]</klr>, en plena <tld/>o &scirc;i tie staris, kaj <nom>Ib</nom> havis sur
        si siajn malnovajn &ccirc;iutagajn vestojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        re&gcirc;a rigardo <klr>[&#8230;]</klr>, kiu frakasas la lukson, la <tld/>on, la triumfojn
        de la granduloj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        senenhava vort<tld/>o
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
     </ekz>
   </dif>
    <trd lng="br">bragerezh</trd>
    <trdgrp lng="de">
      <trd>Aufzug <klr>(Pomp)</klr></trd>,
      <trd>Pomp</trd>
    </trdgrp>
    <trd lng="hu">par&aacute;d&eacute;</trd>
    <trd lng="pl">popisy</trd>
 </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_d;</trd>
  <trd lng="bo">&t_kha;&t_rata;&t_naro;&t_ma;&t_tsheg;&t_ba;&t_sa;&t_kata;&t_naro;&t_ra;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>par&aacute;da</trd>,
    <trd>slav. p&rcaron;ehl&iacute;dka</trd>
 </trdgrp>
  <trd lng="de">Parade</trd>
  <trd lng="fr">parade<klr tip="ind"> (mil.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x95B2;&#20853;&#x5F0F;  [<ind>&#12360;&#12387;&#12410;&#12356;&#12375;&#12365;</ind>]</trd>,
    <trd>&#x30D1;&#x30EC;&#x30FC;&#x30C9;  [<ind>&#12401;&#x308C;&#x30FC;&#12393;</ind>]</trd>,
    <trd>&#x8A87;&#x793A;  [<ind>&#12371;&#12376;</ind>]</trd>,
    <trd>&#x898B;&#x305B;&#12403;&#12425;&#x304B;&#12375;  [<ind>&#x307F;&#x305B;&#12403;&#12425;&#x304B;&#12375;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">parade</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_a;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>par&aacute;da</trd>,
    <trd>sl&aacute;vnostn&aacute; prehliadka</trd>
 </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&c_r;&c_a;&c_d;</trd>,
    <trd>&#x43E;&c_g;&#x43B;&#x44F;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="parad.0a">
  <kap><tld/>a</kap>
  <snc mrk="parad.0a.KOMUNE">
    <dif>
      Elmontra, destinita al <tld/>o:
      <ekz>
        &ccirc;iuj homoj kriis hura, la <tld/>a gardistaro eksonigis muzikon sur la stratoj,
        la sonoriloj sonoris
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o iris en <tld/>a mar&scirc;o sub la belega baldakeno, kaj &ccirc;iuj homoj sur la
        stratoj kaj en la fenestroj kriis
        <fnt>
          <bib>Fab1</bib>
          <lok>La novaj vestoj de la re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        servistoj en <tld/>a livreo
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
         en plene <tld/>aj uniformoj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_d;&c_n;&c_y;</trd>
  <trdgrp lng="fr">
    <trd>de <ind>parade</ind></trd>,
    <trd>de <ind>prestige</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x95B2;&#20853;&#x5F0F;&#x306E;  [<ind>&#x95B2;&#20853;&#12375;&#x304D;&#x306E;</ind>]</trd>,
    <trd>&#x30D1;&#x30EC;&#x30FC;&#x30C9;&#x306E;  [<ind>&#12401;&#x308C;&#x30FC;&#12393;&#x306E;</ind>]</trd>,
    <trd>&#x8A87;&#x793A;&#x306E;  [<ind>&#12371;&#12376;&#x306E;</ind>]</trd>,
    <trd>&#x898B;&#x305B;&#12403;&#12425;&#x304B;&#12375;&#x306E;  [<ind>&#x307F;&#x305B;&#12403;&#12425;&#x304B;&#12375;&#x306E;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&c_r;&c_a;&c_d;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_u;&c_r;&#x43E;&c_ch;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="parad.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="parad.0i.MIL">
    <dif>
      La&ubreve;vice kaj ceremonie <ref tip="super" cel="marsx.0i.orde">mar&scirc;i</ref> &ccirc;e revuo:
      <ekz>
        soldatoj el ekskolonioj <tld/>as en Parizo okaze de la nacia festotago
        <fnt>
          <bib>MD08</bib>
          <vrk>Kvindek jarojn post la sendependi&gcirc;o de la &leftquot;francparolanta&rightquot; Afriko</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la&ubreve; la stratoj de la urbo <tld/>as miloj da bretonoj, kun ri&ccirc;aj folkloraj
        kostumoj kune kun tradiciaj muzikinstrumentoj
        <fnt>
          <aut>Josette Ducloyer</aut>
          <vrk><url ref="&Monato;/2000/006851.php">Quimper</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 16</lok>
        </fnt>.
      </ekz>
   </dif>
    <ref tip="vid" cel="defil.0i">defili</ref>
    <trd lng="be">&c_ib;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_p;&c_a;&c_r;&c_a;&c_d;&c_a;&c_m;</ind></trd>
    <trd lng="br">dibuna&ntilde;<klr tip="ind"> (soudarded)</klr></trd>
    <trdgrp lng="de">
      <trd>aufmarschieren</trd>,
      <trd>paradieren</trd>
  </trdgrp>
    <trdgrp lng="hu">
      <trd>d&iacute;szmenetben <ind>felvonul</ind></trd>,
      <trd>d&iacute;szmenetben <ind>elvonul</ind></trd>
  </trdgrp>
    <trd lng="nl">paraderen</trd>
    <trd lng="pl">paradowa&cacute;</trd>
    <trdgrp lng="pt">
      <trd>desfilar</trd>,
      <trd>marchar em coluna <klr>(numa parada&gt;</klr></trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_sh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_s;&c_t;&c_r;&c_o;&c_je;&c_m;</trd>
   </trdgrp>
 </snc>
  <snc mrk="parad.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fiere kaj afekte fanfaroni,
      <ref tip="vid" cel="pav.0i.FIG">pavi</ref>,
      <ref tip="vid" cel="dand.0i">dandi</ref>:
      <ekz>
        li ricevis botojn, kun kiuj oni efektive povus <tld/>i, kaj anka&ubreve; plej
        bonspecajn vestojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        servistaro <klr>[&#8230;]</klr> <tld/>is en silkaj &scirc;trumpoj, tiuj fieraj homoj
        <fnt>
          <bib>Fab3</bib>
          <vrk>&Ccirc;io sur sian &gcirc;ustan lokon!</vrk>
        </fnt>;
      </ekz>
      <ekz>
        homoj <klr>[&#8230;]</klr> <tld/>as siajn talentojn
        anta&ubreve; konatuloj
        <fnt><aut>J. Francis</aut>,
        <vrk>La Granda Kaldrono</vrk>,
        <lok>1978</lok></fnt>
      </ekz>
   </dif>
   <trdgrp lng="be">
     <trd>&c_v;&c_y;&c_h;&c_v;&c_a;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_v;&c_y;&c_s;&c_t;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; <ind>&c_n;&c_a;&c_p;&c_a;&c_k;&c_a;&c_z;</ind></trd>
   </trdgrp>
    <trd lng="br">bragal</trd>
    <trdgrp lng="de">
      <trd>zur <ind>Schau</ind> stellen</trd>,
      <trd>prunken</trd>,
      <trd>vorf&uuml;hren</trd>,
      <trd>prahlen</trd>,
      <trd>protzen</trd>,
      <trd>strunzen<klr> [s&uuml;ddt]</klr></trd>,
      <trd><ind>Eindruck</ind> machen</trd>
    </trdgrp>
    <trd lng="hu">par&aacute;d&eacute;zik</trd>
    <trd lng="nl">pronken</trd>
    <trdgrp lng="pl">
      <trd>paradowa&cacute;</trd>,
      <trd>popisywa&cacute; si&eogonek;</trd>
   </trdgrp>
    <trdgrp lng="pt">
      <trd>alardear</trd>,
      <trd>pavonear-se</trd>
   </trdgrp>
 </snc>
  <trd lng="bo">&t_kha;&t_rata;&t_naro;&t_ma;&t_tsheg;&t_ba;&t_sa;&t_kata;&t_naro;&t_ra;&t_tsheg;&t_ba;&t_yata;&t_drengbu;&t_da;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>chlubit se</trd>,
    <trd>defilovat</trd>,
    <trd>fintit se</trd>,
    <trd>honosit se</trd>,
    <trd>par&aacute;dit se</trd>,
    <trd>p&rcaron;edv&aacute;d&ecaron;t se</trd>,
    <trd>slavnostn&ecaron; pochodovat</trd>
 </trdgrp>
  <trd lng="en">parade</trd>
  <trd lng="fr">parader</trd>
  <trdgrp lng="ja">
    <trd>&#20998;&#21015;&#x884C;&#36914;&#12377;&#x308B;  [<ind>&#12406;&#12435;&#x308C;&#12388;&#12371;&#12358;&#12375;&#12435;&#12377;&#x308B;</ind>]</trd>,
    <trd>&#x884C;&#21015;&#12375;&#12390;&#x884C;&#36914;&#12377;&#x308B;  [<ind>&#x304E;&#12423;&#12358;&#x308C;&#12388;&#12375;&#12390;&#12371;&#12358;&#12375;&#12435;&#12377;&#x308B;</ind>]</trd>,
    <trd>&#x8A87;&#x793A;&#12377;&#x308B;  [<ind>&#12371;&#12376;&#12377;&#x308B;</ind>]</trd>,
    <trd>&#x898B;&#x305B;&#12403;&#12425;&#x304B;&#12377;  [<ind>&#x307F;&#x305B;&#12403;&#12425;&#x304B;&#12377;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>defilova&tcaron;</trd>,
    <trd>honosi&tcaron; sa</trd>,
    <trd>predv&aacute;dza&tcaron; sa</trd>,
    <trd>predv&aacute;dza&tcaron; sa</trd>,
    <trd>sl&aacute;vanostne pochodova&tcaron;</trd>
 </trdgrp>
   <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_h;&#x43E;&c_d;&c_i;&c_t;&c_i; &c_s;&c_t;&c_r;&#x43E;&c_jeu;&#x43C;</trd>,
    <trd>&#x43C;&c_a;&c_r;&c_sh;&c_i;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_a;&c_r;&c_a;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>,
    <trd>&c_h;&c_i;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="parad.por0o">
  <kap>por <tld/>o</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="parad.por0o.KOMUNE">
    <dif>
      Elmontre, &scirc;ajnige, por imponi:
      <ekz>
        li havas dentojn ne por <tld/>o
        <klr>(sed por mordi)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la direktoro nur saltetis &ccirc;irka&ubreve;e kun la fajfilo
        kaj de tempo al tempo li por <tld/>o admonis:
        <ctl>mi petas, mezen!</ctl>
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
       </fnt>.
     </ekz>
   </dif>
   <trd lng="de">um <ind>Einduck</ind> zu machen</trd>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_p;&c_a;&c_k;&c_a;&c_z;</trd>
  <trd lng="fr">pour la galerie</trd>
 </drv>
 </art>
