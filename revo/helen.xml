<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: helen.xml,v 1.25 2023/11/15 16:45:18 revo Exp $">
<kap>
  <rad>helen</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="helen.0o">
  <kap><tld/>o</kap>
  <snc mrk="helen.0o.ETN">
    <dif>
      Antikva nomo de ano de la hinde&ubreve;ropaj triboj,
      kiuj koloniis Grekujon:
      <ekz>
        trans maro blua de l' <tld lit="H"/>oj ku&scirc;as antikva Palestino
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        evidente pravis <tld/>oj, kiuj diris, ke &leftquot;<nac>panta rhei</nac>&rightquot; &ndash; &ccirc;io
        fluas, evoluas
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008798.php">Migrobirdoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        al la <tld/>oj oni &scirc;uldas ne nur la latinan alfabeton, sed anka&ubreve; la cirilan
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010261.php">Estis iam alfabeto ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi timas <tld/>ojn, e&ccirc; kiam ili alportas donacojn
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2010/009936.php">Franda&jcirc;o por kleruloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>hellene</trd>,
      <trd>Greek (ancient person)</trd>
    </trdgrp>
  </snc>
  <ref tip="hom" cel="helen1.0o">Heleno (virina nomo)</ref>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_g;&c_r;&c_e;&c_k; <klr tip="amb">(&c_s;&c_t;&c_a;&c_r;&c_a;&c_zh;&c_y;&c_t;&c_n;&c_y;)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>Hel&eacute;n</trd>,
    <trd>antick&yacute; &Rcaron;ek</trd>,
    <trd>star&yacute; &Rcaron;ek</trd>
  </trdgrp>
  <trd lng="de">Hellene</trd>
  <trdgrp lng="fr">
    <trd>Hell&egrave;ne</trd>,
    <trd>Grec</trd></trdgrp>
  <trd lng="hu">hell&eacute;n</trd>
  <trd lng="it">elleno</trd>
  <trd lng="ja">&#x53E4;&#x4EE3;&#x30AE;&#x30EA;&#x30B7;&#x30A2;&#x4EBA;  <pr>&#x3053;&#x3060;&#x3044;&#x30AE;&#x30EA;&#x30B7;&#x30A2;&#x3058;&#x3093;</pr></trd>
  <trdgrp lng="nl">
    <trd>Helleen</trd>,
    <trd>Griek<klr tip="ind"> (antique)</klr></trd>
  </trdgrp>
  <trd lng="pl">Grek</trd>
  <trdgrp lng="pt">
   <trd>heleno</trd>, 
   <trd>grego</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_e;&c_l;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_d;&c_r;&c_je;&c_v;&c_n;&c_i;&c_j; &c_g;&c_r;&c_je;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>Hel&eacute;n</trd>,
    <trd>star&yacute; Gr&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&c_l;&c_l;&c_ib;&c_n;</trd>,
    <trd>&c_g;&c_r;&c_je;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="helen.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="helen.0ismo.KOMUNE">
    <dif>
      La <tld/>a kulturo:
      <ekz>
        grave influis la armenan kulturon <tld/>ismo
        <fnt>
          <bib>Monato</bib>
          <aut>Lida Elbakjan</aut>
          <vrk><url ref="&Monato;/2003/007590.html">Armenio festis 1700 jarojn da
          kristanismo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Aleksandrio <klr>[&#x2026;]</klr> fari&gcirc;is unu de la plej grandaj urboj de la <tld lit="H"/>isma
        civilizo, kaj estis la sidejo de la Ptolemea dinastio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17192">Aleksandrio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="helen.0ismo.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <tld lit="H"/>a parolturno.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="helenistik.0a.HIS">helenistika</ref>
  </refgrp>
  <trd lng="be">&c_e;&c_l;&c_ib;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trd lng="de">Hellenismus</trd>
  <trd lng="en">Hellenism</trd>
  <trd lng="fr">hell&eacute;nisme</trd>
  <trd lng="hu">hellenizmus</trd>
  <trd lng="it">ellenismo</trd>
  <trdgrp lng="ja">
    <trd>&#x30D8;&#x30EC;&#x30CB;&#x30BA;&#x30E0;</trd>,
    <trd>&#x30AE;&#x30EA;&#x30B7;&#x30A2;&#x8A9E;&#x98A8;&#x306E;&#x8868;&#x73FE;  <pr>&#x30AE;&#x30EA;&#x30B7;&#x30A2;&#x3054;&#x304B;&#x305C;&#x306E;&#x3072;&#x3087;&#x3046;&#x3052;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">Hellenisme</trd>
  <trdgrp lng="pl">
    <trd>hellenizm</trd>,
    <trd>aleksandryzm</trd>
  </trdgrp>
  <trd lng="pt">helenismo</trd>
  <trd lng="ru">&c_e;&c_l;&c_l;&c_i;&c_n;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">helenizmus</trd>
  <trdgrp lng="cs">
    <trd>hel&eacute;nizmus</trd>,
    <trd>hel&eacute;nstv&iacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_je;&c_l;&c_l;&c_ib;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trdgrp lng="zh">
    <trd>&#x5E0C;&#x814A;&#x4E3B;&#x4E49; <pr>x&#x12B;l&#xE0;zh&#x1D4;y&#xEC;</pr></trd>,
    <trd>&#x5E0C;&#x814A;&#x5316;&#x65F6;&#x4EE3; <pr>x&#x12B;l&#xE0;hu&#xE0;sh&#xED;d&#xE0;i</pr></trd>
  </trdgrp>
</drv>
</art>
