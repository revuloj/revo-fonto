<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>helen</rad>/o
</kap>
<drv mrk="helen.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Antikva nomo de ano de la hinde&ubreve;ropaj triboj, kiuj fiksi&gcirc;is en
      Grekujo.
    </dif>
  </snc>
  <trd lng="de">Hellene</trd>
  <trdgrp lng="fr">
    <trd>Hell&egrave;ne</trd>,
    <trd>Grec</trd></trdgrp>
  <trd lng="hu">hell&eacute;n</trd>
  <trdgrp lng="nl">
    <trd>Helleen</trd>,
    <trd>Griek<klr tip="ind"> (antique)</klr></trd>
  </trdgrp>
  <trd lng="pt">heleno</trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_l;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_d;&c_r;&c_je;&c_v;&c_n;&c_i;&c_j; &c_g;&c_r;&c_je;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="helen.0ismo">
  <kap>
    <tld/>ismo
  </kap>
  <snc>
    <dif>
      <tld lit="H"/>a koncepto pri civilizo.
    </dif>
  </snc>
  <trd lng="de">Hellenismus</trd>
  <trd lng="fr">hell&eacute;nisme</trd>
  <trd lng="hu">hell&eacute;nizmus</trd>
  <trd lng="nl">Hellenisme</trd>
  <trd lng="pt">helenismo</trd>
  <trd lng="ru">&c_e;&c_l;&c_l;&c_i;&c_n;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
