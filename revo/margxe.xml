  <trd lng="nl">marge</trd>
  <trd lng="pl">margines</trd>
  <trd lng="pt">margem <klr tip="amb">(de livro)</klr></trd>
  <trd lng="ro">margine</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_je;<klr tip="amb">
