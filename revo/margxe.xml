<drv mrk="margxe.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="margxe.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne <ref tip="ant" cel="grav.0a">grava</ref>,
      ne <ref tip="ant" cel="cxef.0a.KOMUNE">&ccirc;efa</ref>:
      <ekz>
        aliaj etnaj lingvoj ludas ege <tld/>an rolon
        <fnt>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2003/008051.html">Bruselo-Babelo: lingvo-kulturo en
          E&ubreve;ropa Unio</url></vrk>
           <bib>Monato</bib>
          <lok>2003/02, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        Esperanto <klr>[&#x2026;]</klr> ankora&ubreve; ne estas pli ol <tld/>e uzata tra la mondo
        <fnt>
          <aut>Kep Enderby</aut>
          <vrk><url ref="&Monato;/2002/007693.html">Libroj &dash; Unu grandan
          rondon familian</url></vrk>
          <bib>Monato</bib>
          <lok>2002/09, p. 21</lok>
        </fnt>;
      </ekz>
      <ekz>
        kompare kun veraj viktimoj mi estas nur <tld/>a suferanto
        <fnt>
          <aut>Gotoo Hitosi</aut>
          <vrk><url ref="&Monato;/2011/010354.php">Frustri&gcirc;o de mar&gcirc;ena
          suferanto</url></vrk>
          <bib>Monato</bib>
          <lok>2011/06, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> rezultoj estas <tld/>aj kaj nekontentigaj
        <fnt>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009681.php">Politika sukceso, scienca
          fiasko</url></vrk>
           <bib>Monato</bib>
          <lok>2009/01, p. 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        2013 montri&gcirc;os <klr>(nur)</klr> <tld/>a kresko kompare al la anta&ubreve;a jaro
        <fnt>
          <aut>Lenio Marobin</aut>
          <vrk><url ref="&Monato;/2013/011053.php">Ekonomia fato</url></vrk>
          <bib>Monato</bib>
          <lok>2013/08, p. 24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_r;&c_g;&c_ib;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">marginal</trd>
    <trdgrp lng="de">
      <trd>geringf&uuml;gig</trd>,
      <trd>unbedeutend</trd>,
      <trd>marginal</trd>,
      <trd>peripher</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6B04;&#x5916;&#x306E;  <pr>&#x3089;&#x3093;&#x304C;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x4F59;&#x767D;&#x306E;  <pr>&#x3088;&#x306F;&#x304F;&#x306E;</pr></trd>,
    <trd>&#x9650;&#x754C;&#x306E;  <pr>&#x3052;&#x3093;&#x304B;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x304E;&#x308A;&#x304E;&#x308A;&#x306E;</trd>
  </trdgrp>
  <trd lng="nl">marginaal</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_a;&#x43F;&c_i;&c_s;&c_a;&#x43D;&c_i;&c_j; &#x43D;&c_a; &#x43F;&#x43E;&#x43B;&#x44F;&c_h;/&c_b;&c_je;&c_r;&c_je;&c_g;&c_a;&c_h;</trd>,
    <trd>&c_z;&c_r;&#x43E;&c_b;&#x43B;&c_je;&#x43D;&c_i;&c_j; &#x43D;&c_a; &#x43F;&#x43E;&#x43B;&#x44F;&c_h;/&c_b;&c_je;&c_r;&c_je;&c_g;&c_a;&c_h;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5916;&#x56F4; <pr>w&#xE0;iw&#xE9;i</pr></trd>,
    <trd>&#x5916;&#x570D; <pr>w&#xE0;iw&#xE9;i</pr></trd>,
    <trd>&#x5FAE;&#x4E0D;&#x8DB3;&#x9053; <pr>w&#xE9;ib&#xF9;z&#xFA;d&#xE0;o</pr></trd>,
    <trd>&#x4E34;&#x754C; <pr>l&#xED;nji&#xE8;</pr></trd>,
    <trd>&#x81E8;&#x754C; <pr>l&#xED;nji&#xE8;</pr></trd>,
    <trd>&#x59D4;&#x7410; <pr>w&#x11B;isu&#x1D2;</pr></trd>,
    <trd>&#x59D4;&#x7463; <pr>w&#x11B;isu&#x1D2;</pr></trd>,
    <trd>&#x5C0F;&#x560E;&#x5623;&#x8C46; <pr>xi&#x1CE;og&#x101;b&#x113;ngdou</pr></trd>,
    <trd>&#x65E0;&#x5173;&#x91CD;&#x8981; <pr>w&#xFA;gu&#x101;nzh&#xF2;ngy&#xE0;o</pr></trd>,
    <trd>&#x7121;&#x95DC;&#x91CD;&#x8981; <pr>w&#xFA;gu&#x101;nzh&#xF2;ngy&#xE0;o</pr></trd>
  </trdgrp>
