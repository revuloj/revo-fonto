<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>mar&gcirc;en</rad>/o
    </kap>

    <drv mrk="margxe.0o">
      <kap><tld/>o</kap>
      <snc>
	<dif>
	  Randparto de pa&gcirc;o destinita por resti libera je skribo a&ubreve; preso:
	  <ekz>
	    <uzo tip="stl">FIG</uzo>
	    la propraj nomoj staras &ccirc;e la <tld/>oj
	    <klr>(apude sed ne interne)</klr> de la lingvo.
	  </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_p;&c_o;&c_l;&c_je; <klr>(&c_s;&c_t;&c_a;&c_r;&c_o;&c_n;&c_k;&c_ib;)</klr></trd>
      <trdgrp lng="de">
		<trd>Rand</trd>,
		<trd>Seitenrand</trd>
      </trdgrp>
      <trd lng="en">margin</trd>
      <trdgrp lng="hu">
	<trd>marg&oacute;</trd>,
	<trd>lapsz&eacute;l</trd>
      </trdgrp>
      <trd lng="nl">marge</trd>
      <trd lng="ru">&c_p;&c_o;&c_l;&c_je;<klr tip="amb"> (&c_s;&c_t;&c_r;&c_a;&c_n;&c_i;&c_c;&c_y;)</klr></trd>
    </drv>

    <drv mrk="margxe.krom0o">
      <kap>krom<tld/>o</kap>
      <uzo tip="fak">TIP</uzo>
      <snc>
	<dif>
	  En teksto, &scirc;ovo de la linikomencoj dekstren (enen en la
	  tekston):
	  <ekz>
	    marki la cita&jcirc;ojn per krom<tld/>o.
	  </ekz>
	</dif>
      </snc>
      <trd lng="en">indentation</trd>
      <trd lng="ru">&c_v;&c_t;&c_ja;&c_zh;&c_k;&c_a;<klr tip="amb"> (&c_t;&c_je;&c_k;&c_s;&c_t;&c_a;)</klr></trd>
    </drv>
  </art>
