<vortaro>
<art mrk="$Id: infrastruktur.xml,v 1.10 2021/04/12 07:08:01 revo Exp $">
<kap>
  <rad>infrastruktur</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="infrastruktur.0o">
  <kap><tld/>o</kap>
  <snc mrk="infrastruktur.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Baza <ref tip="vid" cel="ekip.0ajxo">ekipa&jcirc;o</ref> por
      funkciado de domoj, industrio, moderna vivo:
      <ekz>
        la elementoj de la <tld/>o estas en <klr>...</klr> malbonaj
        kondi&ccirc;oj, tiel ke e&ccirc; fari telefonan vokon sukcese
        estas mirinda&jcirc;o
        <fnt>
        <aut>Orlando</aut>,
        <vrk>Gravaj informoj pri la retligo al Kubo</vrk>,
        <lok><url ref=
        "http://groups.google.be/group/soc.culture.esperanto?hl=eo">
        soc.culture.esperanto</url>,
        1993-11-05</lok>
        </fnt>.
      </ekz>
      <ekz>
        en la lando estas ruinigita la tuta <tld/>o, la transporto,
        energio, akvo, nutra&jcirc;oj
        <fnt>
        <aut>Sergio Pokrovskij</aut>,
        <vrk>Elektu pacon!</vrk>,
        <lok><url ref=
        "http://groups.google.be/group/soc.culture.esperanto?hl=eo">
        soc.culture.esperanto</url>,
        1999-05-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_f;&c_r;&c_a;&c_s;&c_t;&c_r;&c_u;&c_k;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="ca">infraestructura</trd>
  <trd lng="cs">infrastruktura</trd>
  <trd lng="en">infrastructure</trd>
  <trdgrp lng="fa">
    <trd>&a_z;&f_y;&a_r;&a_s;&a_A;&a_kh;&a_t;</trd>,
    <trd>&a_z;&f_y;&a_r;&a_b;&a_n;&a_A;</trd>
  </trdgrp>
  <trd lng="fr">infrastructure</trd>
  <trd lng="pl">infrastruktura</trd>
  <trd lng="pt">infra-estrutura</trd>
  <trd lng="sk">infra&scaron;trukt&uacute;ra</trd>
</drv>
</art>

