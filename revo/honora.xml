<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: honora.xml,v 1.25 2023/11/15 17:08:01 revo Exp $">
<kap>
  <ofc>6</ofc>
  <rad>honorari</rad>/o
  <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="honora.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      Neperioda salajro por iu servo de personoj praktikantaj liberan
      profesion:
      <ekz>
        <tld/>o de kuracisto, de advokato.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_n;&c_a;&c_r;&c_a;&c_r;</trd>,
    <trd>&c_g;&c_r;&c_a;&c_sh;&c_o;&c_v;&c_a;&c_je; &c_w;&c_z;&c_n;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_zh;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">honoraris</trd>
  <trd lng="cs">honor&aacute;&rcaron;</trd>
  <trd lng="de">Honorar</trd>
  <trdgrp lng="en">
    <trd>honorarium</trd>,
    <trd>fee</trd>,
    <trd>remuneration</trd>
  </trdgrp>
  <trd lng="es">honorarios</trd>
  <trdgrp lng="fa">
    <trd>&a_d;&a_s;&a_t;&a_m;&a_z;&a_d;</trd>,
    <trd>&a_A;&a_j;&a_r;&a_t;</trd>,
    <trd>&a_H;&a_q;&zwnj;&a_A;&a_l;&a_z;&a_H;&a_m;&a_h;</trd>,
    <trd>&a_w;&f_y;&a_z;&f_y;&a_t;</trd>,
    <trd>&a_H;&a_q;&zwnj;&a_A;&a_l;&a_w;&f_k;&a_A;&a_l;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">honoraires</trd>
  <trdgrp lng="hu">
    <trd>tiszteletd&iacute;j</trd>,
    <trd>honor&aacute;rium</trd>
  </trdgrp>
  <trd lng="it">onorario<klr> (sost.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x8B1D;&#x793C;&#x91D1;  <pr>&#x3057;&#x3083;&#x308C;&#x3044;&#x304D;&#x3093;</pr></trd>,
    <trd>&#x5831;&#x916C;  <pr>&#x307B;&#x3046;&#x3057;&#x3085;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">honorarium</trd>
  <trd lng="pt">honor&aacute;rios</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_n;&c_o;&c_r;&c_a;&c_r;</trd>,
    <trd>&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_o;&c_je; &c_v;&c_o;&c_z;&c_n;&c_a;&c_g;&c_r;&c_a;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">honor&aacute;r</trd>
  <trdgrp lng="uk">
    <trd>&#x433;&#x43E;&#x43D;&#x43E;&#x440;&#x430;&#x440;</trd>,
    <trd>&#x432;&#x438;&#x43D;&#x430;&#x433;&#x43E;&#x440;&#x43E;&#x434;&#x430; <klr>(&#x433;&#x440;&#x43E;&#x448;&#x43E;&#x432;&c_a;)</klr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x916C;&#x91D1; <pr>ch&#xF3;uj&#x12B;n</pr></trd>,
    <trd>&#x8B1D;&#x5100; <pr>xi&#xE8;y&#xED;</pr></trd>,
    <trd>&#x8B1D;&#x79AE; <pr>xi&#xE8;l&#x1D0;</pr></trd>,
    <trd>&#x7A3F;&#x8CBB; <pr>g&#x1CE;of&#xE8;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="honora.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      Pagi <tld/>on al iu.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_l;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol; <ind>&c_g;&c_a;&c_n;&c_a;&c_r;&c_a;&c_r;</ind></trd>
  <trdgrp lng="cs">
    <trd>honorovat</trd>,
    <trd>odm&ecaron;nit</trd>,
    <trd>platit honor&aacute;&rcaron;</trd>
  </trdgrp>
  <trd lng="de"><klr>ein </klr>Honorar <ind>bezahlen</ind></trd>
  <trdgrp lng="en">
    <trd>pay a honorarium</trd>,
    <trd>pay a fee</trd>,
    <trd>remunerate</trd>
  </trdgrp>
  <trd lng="es">pagar <ind>honorarios</ind></trd>
  <trd lng="fa">&a_d;&a_s;&a_t;&a_m;&a_z;&a_d; &f_p;&a_r;&a_d;&a_A;&a_kh;&a_t;&a_n;<klr> (&a_b;&a_h;)</klr></trd>
  <trd lng="fr">payer des <ind>honoraires</ind> (&agrave; qn)</trd>
  <trdgrp lng="hu">
    <trd>tiszteletd&iacute;jat fizet</trd>,
    <trd>honor&aacute;riumot fizet</trd>
  </trdgrp>
  <trd lng="it">pagare l'<ind>onorario</ind></trd>
  <trd lng="ja">&#x8B1D;&#x793C;&#x91D1;&#x3092;&#x6255;&#x3046;  <pr>&#x3057;&#x3083;&#x308C;&#x3044;&#x304B;&#x306D;&#x3092;&#x306F;&#x3089;&#x3046;</pr></trd>
  <trd lng="nl">honoreren</trd>
  <trd lng="pt">pagar <ind>honor&aacute;rios</ind></trd>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol; &c_g;&c_o;&c_n;&c_o;&c_r;&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>honorova&tcaron;</trd>,
    <trd>plati&tcaron; honor&aacute;r</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43B;&#x430;&#x442;&#x438;&#x442;&#x438; &#x433;&#x43E;&#x43D;&#x43E;&#x440;&#x430;&#x440;</trd>
</drv>
</art>
