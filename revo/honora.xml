<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>honorari</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="honora.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      Neperioda salajro por iu servo de personoj praktikantaj liberan
      profesion:
      <ekz>
        <tld/>o de kuracisto, de advokato.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Honorar</trd>
  <trd lng="es">honorarios</trd>
  <trd lng="fr">honoraires</trd>
  <trdgrp lng="hu">
    <trd>tiszteletd&iacute;j</trd>,
    <trd>honor&aacute;rium</trd>
  </trdgrp>
  <trd lng="nl">honorarium</trd>
  <trd lng="pt">honor&aacute;rios</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_n;&c_o;&c_r;&c_a;&c_r;</trd>,
    <trd>&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_o;&c_je; &c_v;&c_o;&c_z;&c_n;&c_a;&c_g;&c_r;&c_a;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="honora.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>ZR</bib></fnt>
  <snc>
    <dif>
      Pagi <tld/>on al iu.
    </dif>
  </snc>
  <trd lng="de"><klr>ein </klr>Honorar <ind>bezahlen</ind></trd>
  <trd lng="nl">honoreren</trd>
  <trd lng="es">pagar <ind>honorarios</ind></trd>
  <trd lng="fr">payer des <ind>honoraires</ind> (&agrave; qn)</trd>
  <trdgrp lng="hu">
    <trd>tiszteletd&iacute;jat fizet</trd>,
    <trd>honor&aacute;riumot fizet</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_l;&c_a;&c_t;&c_i;&c_t;&c_mol; &c_g;&c_o;&c_n;&c_o;&c_r;&c_a;&c_r;</trd>
</drv>
</art>
