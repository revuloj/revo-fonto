<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lazurit.xml,v 1.18 2025/12/31 22:34:09 revo Exp $">
<kap>
  <rad>lazurit</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="lazurit.0o">
  <kap><tld/>o</kap>
  <snc mrk="lazurit.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="dif" cel="sxton.lazur0o.MIN">Lazur&scirc;tono</ref>:
      <ekz>
        nomoj rilatas al rimarkeblaj ecoj de la &scirc;tonoj <klr>[&#x2026;]</klr>
        ekzemple <tld/>o la&ubreve; blua koloro
        <fnt>
          <aut>Galina Beliakova</aut>,
          <vrk><url ref="http://www.linabel.de/Gem/Eo/Gemmologie800_eo.html">gemologio</url></vrk>,
          <lok>[vidita je 2006-02-17]</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> lumis karmezinru&gcirc;e, <klr>[&#x2026;]</klr> terbrune,
        hindflave,
        hidrargosulfidru&gcirc;e kaj <tld/>blue
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Goabo, la dezerto de la Koloroj</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">lazurite</trd>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>lazur&scirc;tono</ke>.
  </rim>

  <trd lng="be">&c_l;&c_ja;&c_z;&c_u;&c_r;&c_y;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>azurit</trd>,
    <trd>lazur</trd>,
    <trd>lazurit</trd>
  </trdgrp>
  <trd lng="de">Lapislazuli</trd>
  <trd lng="fr">lapis lazuli</trd>
  <trdgrp lng="hu">
    <trd>lazurk&odblac;</trd>,
    <trd>lazurit</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x9752;&#x91D1;&#x77F3;  <pr>&#x305B;&#x3044;&#x304D;&#x3093;&#x305B;&#x304D;</pr></trd>,
    <trd>&#x30E9;&#x30D4;&#x30B9;&#x30E9;&#x30BA;&#x30EA;</trd>,
    <trd>&#x7460;&#x7483;  <pr>&#x308B;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lazuursteen</trd>,
    <trd>lapis lazuli</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lazuryt</trd>,
    <trd>lapis lazuli</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>lazurita</trd>,
    <trd>l&aacute;pis-laz&uacute;li</trd>
  </trdgrp>
  <trd lng="ro">lapis lazuli</trd>
  <trdgrp lng="sk">
    <trd>lazurit</trd>,
    <trd>lazurit,</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_z;&c_u;&c_r;&c_i;&c_t;</trd>,
    <trd>&c_l;&c_a;&c_z;&c_u;&c_r;&c_o;&c_v;&c_i;&c_j; &c_k;&c_a;&c_m;&c_ib;&c_n;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5929;&#x9752;&#x77F3; <pr>ti&amacron;nq&imacron;ngsh&iacute;</pr></trd>,
    <trd>&#x9752;&#x91D1;&#x77F3; <pr>q&imacron;ngj&imacron;nsh&iacute;</pr></trd>
  </trdgrp>
</drv>
</art>

