<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lazurit.xml,v 1.17 2025/04/02 05:08:27 revo Exp $">
<kap>
  <rad>lazurit</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>

<drv mrk="lazurit.0o">
  <kap><tld/>o</kap>
  <snc mrk="lazurit.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      <ref tip="dif" cel="sxton.lazur0o.MIN">Lazur&scirc;tono</ref>:
      <ekz>
        nomoj rilatas al rimarkeblaj ecoj de la &scirc;tonoj <klr>[&#8230;]</klr>
        ekzemple <tld/>o la&ubreve; blua koloro
        <fnt>
          <aut>Galina Beliakova</aut>,
          <vrk><url ref="http://www.linabel.de/Gem/Eo/Gemmologie800_eo.html">gemologio</url></vrk>,
          <lok>[vidita je 2006-02-17]</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> lumis karmezinru&gcirc;e, <klr>[&#8230;]</klr> terbrune,
        hindflave,
        hidrargosulfidru&gcirc;e kaj <tld/>blue
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Goabo, la dezerto de la Koloroj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>lazur&#349;tono</ke>.
  </rim>

  <trd lng="be">&c_l;&c_ja;&c_z;&c_u;&c_r;&c_y;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>azurit</trd>,
    <trd>lazur</trd>,
    <trd>lazurit</trd>
  </trdgrp>
  <trd lng="de">Lapislazuli</trd>
  <trd lng="fr">lapis lazuli</trd>
  <trdgrp lng="hu">
    <trd>lazurk&odblac;</trd>,
    <trd>lazurit</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#38738;&#37329;&#30707;  <pr>&#12379;&#12356;&#12365;&#12435;&#12379;&#12365;</pr></trd>,
    <trd>&#12521;&#12500;&#12473;&#12521;&#12474;&#12522;</trd>,
    <trd>&#29792;&#29827;  <pr>&#12427;&#12426;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lazuursteen</trd>,
    <trd>lapis lazuli</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lazuryt</trd>,
    <trd>lapis lazuli</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>lazurita</trd>,
    <trd>l&aacute;pis-laz&uacute;li</trd>
  </trdgrp>
  <trd lng="ro">lapis lazuli</trd>
  <trdgrp lng="sk">
    <trd>lazurit</trd>,
    <trd>lazurit,</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1083;&c_a;&c_z;&c_u;&c_r;&c_i;&c_t;</trd>,
    <trd>&#1083;&c_a;&c_z;&c_u;&c_r;&#1086;&c_v;&c_i;&c_j; &#1082;&c_a;&#1084;&c_ib;&#1085;&#1100;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22825;&#38738;&#30707; <pr>ti&amacron;nq&#299;ngsh&#237;</pr></trd>,
    <trd>&#38738;&#37329;&#30707; <pr>q&#299;ngj&#299;nsh&#237;</pr></trd>
  </trdgrp>
</drv>
</art>

