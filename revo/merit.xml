<kap>
  <ofc>*</ofc>
  <rad>merit</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="merit.0i">
  <kap><ofc>*</ofc><tld/>i <fnt><bib>PV</bib></fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="merit.0i.KOMUNE">
    <dif>
      Havi pro sia konduto a&ubreve; pro siaj ecoj rajton pri justa rekompenco
      a&ubreve; puno; esti inda je:
      <ekz>
        <tld/>i dankon, gratulon, estimon, atenton, sian sorton;
      </ekz>
      <ekz>
        tiuj ostoj <tld/>is nenion alian, ol ke oni ludu per ili
        keglojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiel li <tld/>is, tiel li profitis
        <fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        esti <tld/>ite apla&ubreve;dita.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_s;&c_l;&c_u;&c_g;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol;
    <ind>&c_v;&c_a;&c_r;&c_t;&c_y;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>mer&egrave;ixer</trd>,
    <trd>meritar</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>zasluhovat</trd>,
    <trd>zaslou&zcaron;it</trd>,
    <trd>zaslou&zcaron;it si</trd>
  </trdgrp>
  <trd lng="de">verdienen</trd>
  <trdgrp lng="en">
    <trd>merit</trd>,
    <trd>deserve</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>merecerse<klr> (algo)</klr></trd>,
    <trd>merecer<klr> (algo)</klr></trd>,
    <trd>tener m&eacute;ritos de</trd>,
    <trd>hacer m&eacute;ritos para</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;riter</trd>
  <trdgrp lng="hu">
    <trd>meg&eacute;rdemel</trd>,
    <trd>ki&eacute;rdemel</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4FA1;&#12377;&#x308B;  <pr>&#12354;&#x305F;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#12405;&#12373;&#x308F;&#12375;&#12356;</trd>,
    <trd>&#x5F53;&#28982;&#12391;&#12354;&#x308B;  <pr>&#12392;&#12358;&#x305C;&#12435;&#12391;&#12354;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verdienen<klr tip="ind"> (waardig zijn)</klr></trd>,
    <trd>waardig zijn</trd>
  </trdgrp>
  <trd lng="pl">zas&lstroke;u&zdot;y&cacute;</trd>
  <trdgrp lng="pt">
    <trd>merecer</trd>,
    <trd>ser <ind>digno</ind> de</trd>,
    <trd>fazer <ind>jus</ind> a</trd>,
    <trd>valer</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>merita</trd>
    <trd>cuveni</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_y;&c_t;&c_mol;
    &c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">zasl&uacute;&zcaron;i&tcaron; si</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&#x43B;&c_u;&c_g;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_v;&c_a;&c_r;&c_t;&c_i;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="merit.0o">
  <kap><tld/>o</kap>
  <snc mrk="merit.0o.KOMUNE">
    <dif>
      Ago, eco danke al kiu oni <tld/>as ion:
      <ekz>
        &ccirc;iuj enterigitoj estas plenaj de <tld/>oj
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        la&ubreve; la <tld/>o de siaj manoj homo ricevas redonon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        prave punite la&ubreve;<tld/>e
        <fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_s;&c_l;&c_u;&c_g;&c_a;</trd>
  <trd lng="bo">&#xF51;&#xF42;&#xF7A;&#xF0B;&#xF56;&#xF0B;</trd>
  <trd lng="ca">m&egrave;rit</trd>
  <trd lng="cs">z&aacute;sluha</trd>
  <trd lng="de">Verdienst</trd>
  <trdgrp lng="en">
    <trd>merit</trd>,
    <trd>worth</trd>
  </trdgrp>
  <trd lng="es">m&eacute;rito(s)</trd>
  <trd lng="fr">m&eacute;rite</trd>
  <trd lng="hu">&eacute;rdem</trd>
  <trdgrp lng="ja">
    <trd>&#x529F;&#x7E3E;  <pr>&#12371;&#12358;&#x305B;&#x304D;</pr></trd>,
    <trd>&#x624B;&#x67C4;  <pr>&#12390;&#x304C;&#12425;</pr></trd>,
    <trd>&#x53D6;&#x308A;&#x67C4;  <pr>&#12392;&#x308A;&#12360;</pr></trd>,
    <trd>&#38263;&#25152;  <pr>&#12385;&#12423;&#12358;&#12375;&#12423;</pr></trd>,
    <trd>&#x4FA1;&#20516;  <pr>&#x304B;&#12385;</pr></trd>,
    <trd>&#x529F;&#x7F6A;  <pr>&#12371;&#12358;&#12374;&#12356;</pr></trd>,
    <trd>&#x529F;&#x5FB3;  <pr>&#x304F;&#12393;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">verdienste</trd>
  <trd lng="pl">godno&sacute;&cacute; <klr>(zas&lstroke;uga)</klr></trd>
  <trd lng="ro">demnitate <klr>(merit)</klr></trd>
  <trd lng="ru">&c_z;&c_a;&c_s;&c_l;&c_u;&c_g;&c_a;</trd>
  <trd lng="sk">z&aacute;sluha</trd>
  <trd lng="uk">&c_z;&c_a;&c_s;&#x43B;&c_u;&c_g;&c_a;</trd>
</drv>

<drv mrk="merit.0a">
  <kap><tld/>a</kap>
  <snc mrk="merit.0a.KOMUNE">
    <dif>
      <tld lit="M"/>oplena, mult<tld/>a:
      <ekz>
        <tld/>a homo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        konduto;
      </ekz>
      <ekz>
        servo ne petita, ne estas <tld/>a
        <fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_a;&c_n;&c_y;</trd>,
      <trd>&c_v;&c_a;&c_r;&c_t;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>meritori</trd>,
      <trd>merit&oacute;s</trd>
    </trdgrp>
    <trd lng="de">verdienstvoll</trd>
    <trd lng="es">meritorio</trd>
    <trd lng="fr">m&eacute;ritant</trd>
    <trdgrp lng="hu">
      <trd>&eacute;rdemes</trd>,
      <trd>m&eacute;lt&oacute;</trd>
    </trdgrp>
    <trd lng="nl">verdienstelijk</trd>

  </snc>
  <snc mrk="merit.0a.meritita">
    <dif>
      <tld/>ita:
      <ekz>
        laboro finita, ripozo <tld/>a
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_a;&c_n;&c_y;</trd>
    <trd lng="ca">merescut</trd>
    <trd lng="de">verdient</trd>
    <trd lng="fr">m&eacute;rit&eacute;</trd>
    <trd lng="hu">meg&eacute;rdemelt</trd>
    <trd lng="nl">verdiend</trd>
    <trd lng="es">merecido</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>zaslou&zcaron;il&yacute;</trd>,
    <trd>z&aacute;slu&zcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>meritorious</trd>,
    <trd>worthy</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x529F;&#x7E3E;&#x306E;&#12354;&#x308B;  <pr>&#12371;&#12358;&#x305B;&#x304D;&#x306E;&#12354;&#x308B;</pr></trd>,
    <trd>&#x79F0;&#x8CDB;&#x306B;&#20516;&#12377;&#x308B;  <pr>&#12375;&#12423;&#12358;&#12373;&#12435;&#x306B;&#12354;&#x305F;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x5F53;&#28982;&#x53D7;&#12369;&#x308B;&#12409;&#x304D;  <pr>&#12392;&#12358;&#x305C;&#12435;&#12358;&#12369;&#x308B;&#12409;&#x304D;</pr></trd>,
    <trd>&#12405;&#12373;&#x308F;&#12375;&#12356;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>godny</trd>
    <trd>zas&lstroke;u&zdot;ony</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>demn</trd>
    <trd>meritat</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>zasl&uacute;&zcaron;il&yacute;</trd>,
    <trd>z&aacute;slu&zcaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_z;&c_a;&c_s;&#x43B;&c_u;&c_zh;&c_je;&#x43D;&c_i;&c_j;</trd>
</drv>

<drv mrk="merit.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="merit.0ajxo.KOMUNE">
    <dif>
      Tio, kion oni <tld/>is:
      <ekz>
        redoni al iu la <tld/>a&jcirc;on<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_n;&c_a;&c_g;&c_a;&c_r;&c_o;&c_d;&c_a;
  <klr>(&c_n;&c_je;&c_sh;&c_t;&c_a;
  &c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_a;&c_n;&c_a;&c_je;)</klr></trd>
  <trdgrp lng="ca">
    <trd>premi</trd>,
    <trd>gratificaci&oacute;</trd>,
    <trd>all&ograve; merescut</trd>
  </trdgrp>
  <trd lng="cs">zaslou&zcaron;en&aacute; v&ecaron;c</trd>
  <trdgrp lng="en">
    <trd>just desserts</trd>,
    <trd>appropriate compensation</trd>,
    <trd>reward</trd>,
    <trd>punishment</trd>
  </trdgrp>
  <trd lng="es">recompensa</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rdemd&iacute;j</trd>,
    <trd>meg&eacute;rdemelt jutalom</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#22577;&#12356;  <pr>&#12416;&#x304F;&#12356;</pr></trd>,
    <trd>&#x8CDE;&#x7F70;  <pr>&#12375;&#12423;&#12358;&#12400;&#12388;</pr></trd>
  </trdgrp>
  <trd lng="nl">verdiende <ind>loon</ind></trd>
  <trdgrp lng="pl">
    <trd>rzecz zas&lstroke;u&zdot;ona</trd>
    <trd>rzecz nale&zdot;na</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>bine meritat</trd>
    <trd>datorat</trd>
  </trdgrp>
  <trd lng="sk">zasl&uacute;&zcaron;en&aacute; vec</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_s;&#x43B;&c_u;&c_g;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&#x43B;&c_u;&c_zh;&c_je;&#x43D;&c_je; (&c_ib;&#x43C;.)</trd>
  </trdgrp>
</drv>

<drv mrk="merit.sen0a">
  <kap>sen<tld/>a</kap>
  <uzo tip="klr">(pri homo)</uzo>
  <snc mrk="merit.sen0a.KOMUNE">
    <dif>
      Kiu <tld/>on ne havas, kiu estas
      malgrandvalora:
      <ekz>
        la Mastro troe honoras maljunan sen<tld/>an sklavon
        <fnt><aut>E. de Zilah</aut>, <vrk>La Princo &ccirc;e la Hunoj</vrk>,
        <lok>2011</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_a;&c_n;&c_y;</trd>,
    <trd>&c_b;&c_je;&c_z; <ind>&c_z;&c_a;&c_s;&c_l;&c_u;&c_g;&c_a;&c_w;</ind></trd>,
    <trd>&c_n;&c_ja;&c_v;&c_a;&c_r;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bez z&aacute;sluh</trd>,
    <trd>nez&aacute;slu&zcaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="en">undeserving</trd>
  <trdgrp lng="fr">
    <trd>insignifiant<klr> (sans m&eacute;rite)</klr></trd>,
    <trd>modeste<klr> (sans m&eacute;rite)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x529F;&#x7E3E;&#x306E;&#x306A;&#12356;  <pr>&#12371;&#12358;&#x305B;&#x304D;&#x306E;&#x306A;&#12356;</pr></trd>,
    <trd>&#x53D6;&#x308A;&#x67C4;&#x306E;&#x306A;&#12356;  <pr>&#12392;&#x308A;&#12360;&#x306E;&#x306A;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niezas&lstroke;u&zdot;ony</trd>
    <trd>bez zas&lstroke;ugi</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>ne&icirc;nvins</trd>
    <trd>f&abreve;r&abreve; merit</trd>
  </trdgrp>
  <trd lng="sk">bez z&aacute;sluh</trd>
  <trdgrp lng="uk">
    <trd>&#x43D;&c_je;&c_z;&c_a;&c_s;&#x43B;&c_u;&c_zh;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z; &c_z;&c_a;&c_s;&#x43B;&c_u;&c_g;</trd>
  </trdgrp>
</drv>
</art>
