<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kumin.xml,v 1.30 2023/12/03 09:59:03 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>kumin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kumin.0o">
  <kap><tld/>o</kap>
  <snc mrk="kumin.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Jara herba vegeta&jcirc;o el orienta Mediteraneo &gcirc;is orienta
      Hindio,
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio <ref tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Cuminum</trd>).
    </dif>
  </snc>
  <snc mrk="kumin.0o.spico">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="lst" cel="spic.0o.KUI" lst="voko:spicoj">Spico</ref> el
      la semoj
      de unu specio de <ref tip="malprt" cel="kumin.0o.genro"><tld/>o<sncref/></ref>
      (<trd lng="la">Cuminum cyminum</trd>):
      <ekz>
        sed per bastono oni elbatas <ref cel="nigel.0o.spico">nigelon</ref>
        kaj <tld/>on per vergo <fnt><bib>MT</bib>, <lok>&Jes;
        28:26</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;ar vi pagas dekona&jcirc;ojn de mento kaj de anizo kaj de
        <tld/>o <fnt><bib>NT</bib>, <lok>&Mat; 23:23</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_m;&c_je;&c_n;</trd>
  <trd lng="bg">&c_k;&c_i;&c_m;&c_i;&c_o;&c_n;</trd>
  <trd lng="cs">km&iacute;n &rcaron;&iacute;msk&yacute;</trd>
  <trd lng="de">Kreuzk&uuml;mmel</trd>
  <trd lng="en">cumin</trd>
  <trd lng="es">cumino</trd>
  <trdgrp lng="fi">
    <trd>roomankumina</trd>,
    <trd>juustokumina</trd>
  </trdgrp>
  <trd lng="fr">cumin</trd>
  <trd lng="hi">&#x91C;&#x940;&#x930;&#x93E; [<ind>j&#x12B;r&#x101;</ind>]</trd>
  <trd lng="hu">borsosk&ouml;m&eacute;ny</trd>
  <trdgrp lng="it">
    <trd>cumino</trd>,
    <trd>cimino</trd>
  </trdgrp>
  <trd lng="ja">&#x30AF;&#x30DF;&#x30F3;</trd>
  <trdgrp lng="nl">
    <trd>komijn</trd>
    <trd>kummel</trd>,
    <trd>karwij</trd>
  </trdgrp>
  <trd lng="pl">kminek</trd>
  <trd lng="pt">cominho</trd>
  <trd lng="ru">&c_t;&c_m;&c_i;&c_n;</trd>
  <trd lng="sk">km&iacute;n</trd>
  <trd lng="sv">spiskummin</trd>
  <trd lng="tr">kimyon</trd>
  <trd lng="uk">&#x43A;&#x43C;&#x438;&#x43D;</trd>
  <trd lng="zh">&#x5B5C;&#x7136; <pr>z&#x12B;r&#xE1;n</pr></trd>
</drv>
</art>
