<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>kumin</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="kumin.0o">
  <kap>
  <tld/>o
  </kap>
  <snc mrk="kumin.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Cuminum</trd>).
    </dif>
  </snc>
  <snc mrk="kumin.0o.spico">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="spic.0o">Spico</ref> el la semoj
      de unu specio de <ref tip="malprt"
      cel="kumin.0o.genro"><tld/>o<sncref/></ref>
      (<trd lng="la">Cuminum cyminum</trd>):
      <ekz>
        sed per bastono oni elbatas <ref cel="nigel.0o.spico">nigelon</ref>
        kaj kuminon per vergo <fnt><bib>MT</bib>, <lok>Jesaja 28:26</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;ar vi pagas dekona&jcirc;ojn de mento kaj de anizo kaj de
        kumino <fnt><bib>NT</bib>, <lok>Mateo 23:23</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_m;&c_je;&c_n;</trd>
  <trd lng="de">Kreuzk&uuml;mmel</trd>
  <trd lng="en">cumin</trd>
  <trd lng="es">cumino</trd>
  <trdgrp lng="fi">
    <trd>roomankumina</trd>,
    <trd>juustokumina</trd>
  </trdgrp>
  <trd lng="fr">cumin</trd>
  <trdgrp lng="it">
    <trd>cumino</trd>,
    <trd>cimino</trd>
  </trdgrp>
  <trd lng="hu">borsosk&ouml;m&eacute;ny</trd>
  <trd lng="nl">komijn</trd>
  <trd lng="pt">cominho</trd>
  <trd lng="ru">&c_t;&c_m;&c_i;&c_n;</trd>
  <trd lng="sv">spiskummin</trd>
  <trd lng="tr">kimyon</trd>
</drv>
</art>
