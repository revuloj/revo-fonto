<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kumin</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kumin.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Cuminum</trd>), kies semoj estas uzataj kiel
      spica&jcirc;o:
      <ekz>
        sed per bastono oni elbatas nigelon kaj kuminon per vergo
        <fnt>(<aut>trad. Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Jesaja 28, 26</lok>)</fnt>;
      </ekz>
      <ekz>
        &ccirc;ar vi pagas dekona&jcirc;ojn de mento kaj de anizo kaj de kumino
        <fnt>(<vrk>Nova testamento</vrk>, <lok>Mateo 23, 23</lok>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kreuzk&uuml;mmel</trd>
  <trd lng="en">cumin</trd>
  <trd lng="es">cumino</trd>
  <trdgrp lng="fi">
    <trd>roomankumina</trd>,
    <trd>juustokumina</trd>
  </trdgrp>
  <trd lng="fr">cumin</trd>
  <trdgrp lng="it">
    <trd>cumino</trd>,
    <trd>cimino</trd>
  </trdgrp>
  <trd lng="nl">komijn</trd>
  <trd lng="pt">cominho</trd>
  <trd lng="ru">&c_t;&c_m;&c_i;&c_n;</trd>
  <trd lng="sv">spiskummin</trd>
  <trd lng="tr">kimyon</trd>
</drv>
</art>
