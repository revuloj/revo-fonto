<drv mrk="konver2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="konver2.0i.KOMUNE">
    <dif>
      Familiare
      <ref tip="super" cel="parol.inter0i.KOMUNE">interparoladi</ref>
      kun iu:
      <ekz>
        <tld/>i pri la vetero kaj ceteraj aferoj
        <fnt>
          <bib>IdO</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i tute ne volis <tld/>i kun personoj de sia rango
        <fnt>
          <bib>IdO</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne estis grandmonda sinjorino, kiu sur velura sofo de salono sprite <tld/>as
        pri emancipado de virinoj
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="babil.0i">babili</ref>,
      <ref cel="diskut.0i">diskuti</ref>,
      <ref cel="klacx.0i">kla&ccirc;i</ref>,
      <ref cel="palavr.0i">palavri</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_t;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>sich <ind>unterhalten</ind></trd>
  </trdgrp>
