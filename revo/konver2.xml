<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>konversaci</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="konver2.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Familiara, ordinare nur distriga interparolado:
      <ekz>
        la legado de bonaj libroj estas kvaza&ubreve; <tld/>o kun plej kleraj
        personoj de la pasintaj jarcentoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Unterhaltung</trd>,
     <trd>Gespr&auml;ch</trd>,
     <trd>Konversation</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conversation</trd>,
    <trd>chat</trd>,
    <trd>talk</trd>
  </trdgrp>
  <trd lng="cs">rozhovor</trd>
  <trdgrp lng="es">
    <trd>conversaci&oacute;n</trd>,
    <trd>charla</trd>,
    <trd>pl&aacute;tica</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_je;&c_d;&c_a;</trd>
</drv>

<drv mrk="konver2.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Familiare interparoladi kun iu.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>sich <ind>unterhalten</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>converse</trd>,
    <trd>chat</trd>,
    <trd>talk</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>conversar</trd>,
    <trd>charlar</trd>,
    <trd>platicar</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
