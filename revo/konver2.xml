<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konver2.xml,v 1.21 2021/05/30 19:16:16 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>konversaci</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="konver2.0o">
  <kap><tld/>o</kap>
  <snc mrk="konver2.0o.KOMUNE">
    <dif>
      Familiara, ordinare nur distriga interparolado:
      <ekz>
        la legado de bonaj libroj estas kvaza&ubreve; <tld/>o kun plej kleraj
        personoj de la pasintaj jarcentoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="am">&#x121B;&#x12CD;&#x122B;&#x1275;</trd>
  <trd lng="ar">&#x627;&#x644;&#x62D;&#x62F;&#x64A;&#x62B;</trd>
  <trd lng="be">&c_g;&c_u;&c_t;&c_a;&c_r;&c_k;&c_a;</trd>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_g;&c_o;&c_v;&c_o;&c_r;</trd>
  <trd lng="bo">&#xF66;&#xF90;&#xF51;&#xF0B;&#xF46;</trd>
  <trd lng="cs">rozhovor</trd>
  <trdgrp lng="de">
     <trd>Unterhaltung</trd>,
     <trd>Gespr&auml;ch</trd>,
     <trd>Konversation</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>conversation</trd>,
    <trd>chat</trd>,
    <trd>talk</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>conversaci&oacute;n</trd>,
    <trd>charla</trd>,
    <trd>pl&aacute;tica</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_g;&a_f;&a_t;&f_g;&a_w;</trd>,
    <trd>&a_m;&f_k;&a_A;&a_l;&a_m;&a_h;</trd>,
    <trd>&a_S;&a_H;&a_b;&a_t;</trd>
  </trdgrp>
  <trd lng="fr">conversation</trd>
  <trd lng="hu">t&aacute;rsalg&aacute;s</trd> 
  <trdgrp lng="pl">
    <trd>konwersacja</trd>,
    <trd>rozmowa</trd>
  </trdgrp>    
  <trdgrp lng="pt">
    <trd>conversa</trd>,
    <trd>col&oacute;quio</trd>
  </trdgrp>
  <trd lng="ro">conversa&#x21B;ie</trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_je;&c_d;&c_a;</trd>
  <trd lng="sq">flasim</trd>
</drv>

<drv mrk="konver2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="konver2.0i.KOMUNE">
    <dif>
      Familiare
      <ref tip="vid" cel="parol.inter0i.KOMUNE">interparoladi</ref>
      kun iu.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_t;&c_a;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>sich <ind>unterhalten</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>converse</trd>,
    <trd>chat</trd>,
    <trd>talk</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>conversar</trd>,
    <trd>charlar</trd>,
    <trd>platicar</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_g;&a_f;&a_t;&f_g;&a_w; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_S;&a_H;&a_b;&a_t; &f_k;&a_r;&a_d;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">converser</trd>
  <trd lng="hu">t&aacute;rsalog</trd> 
  <trdgrp lng="pl">
    <trd>konwersowa&cacute;</trd>,
    <trd>rozmawia&cacute;</trd>
  </trdgrp>    
  <trd lng="ro">s&abreve; vorbe&#x219;ti</trd>  
  <trd lng="ru">&c_b;&c_je;&c_s;&c_je;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
