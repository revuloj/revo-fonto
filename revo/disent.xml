<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>disenteri</rad>/o
</kap>
<drv mrk="disent.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Infekta inflamo de la intesto, ka&ubreve;zanta da&ubreve;ran lakson:
      <ekz>
        en Samarkando, Uzbekio, [...] oni aplikis bakteriofagojn por
        preventado de <tld/>o<fnt><aut>Vladimir Lemelev</aut>:
        <vrk>Malamikoj de bakterioj</vrk>, <bib>Monato</bib><lok>jaro
        2000a, numero 1a, p. 14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_z;&c_e;&c_n;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Ruhr</trd>,
    <trd>Dysenterie<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="es">disenter&iacute;a</trd>
  <trd lng="fr">dysenterie</trd>
  <trdgrp lng="hu">
    <trd>dizent&eacute;ria</trd>,
    <trd>v&eacute;rhas</trd>
  </trdgrp>
  <trd lng="pt">disenteria</trd>
  <trd lng="ru">&c_d;&c_i;&c_z;&c_je;&c_n;&c_t;&c_je;&c_r;&c_i;&c_ja;</trd>
</drv>
</art>
