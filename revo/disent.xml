<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: disent.xml,v 1.23 2024/01/02 23:25:47 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>disenteri</rad>/o
</kap>
<drv mrk="disent.0o">
  <kap><tld/>o</kap>
  <snc mrk="disent.0o.MED">
     <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="san.mal0o" lst="voko:malsanoj">Malsano</ref>,
      infekta inflamo de la intesto, ka&ubreve;zanta da&ubreve;ran
      <ref tip="vid" cel="laks.0o">lakson</ref>:
      <ekz>
        estas eble <tld/>o &mdash; &gcirc;i montri&gcirc;as &ccirc;iujare en somero
        <fnt>
          <aut>B. von Suttner, trad. A. Caumont kaj F. Luin</aut>
          <vrk>For la batalilojn!</vrk>
          <lok>1914, 2001</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Samarkando, Uzbekio, [...] oni aplikis bakteriofagojn por
        preventado de <tld/>o
        <fnt>
        <aut>V. Lemelev</aut>
        <vrk>Malamikoj de bakterioj</vrk>
        <bib>Monato</bib>
        <lok>2000/1, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_z;&c_e;&c_n;&c_t;&c_e;&c_r;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>dysenterie</trd>,
    <trd>&uacute;plavice st&rcaron;evn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ruhr</trd>,
    <trd>Dysenterie<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="en">dysentery</trd>
  <trd lng="es">disenter&iacute;a</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_w;&a_n;&a_r;&a_w;&a_sh;</trd>,
    <trd>&a_A;&a_s;&a_h;&a_A;&a_l; &a_kh;&a_w;&a_n;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">dysenterie</trd>
  <trdgrp lng="hu">
    <trd>dizent&eacute;ria</trd>,
    <trd>v&eacute;rhas</trd>
  </trdgrp>
  <trd lng="ja">&#x8D64;&#x75E2;  <pr>&#x305B;&#x304D;&#x308A;</pr></trd>
  <trdgrp lng="pl">
    <trd>dyzenteria</trd>,
    <trd>czerwonka</trd>
  </trdgrp>
  <trd lng="pt">disenteria</trd>
  <trd lng="ru">&c_d;&c_i;&c_z;&c_je;&c_n;&c_t;&c_je;&c_r;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>dyzent&eacute;ria</trd>,
    <trd>hna&ccaron;ka</trd>,
    <trd>&uacute;plavica</trd>
  </trdgrp>
  <trd lng="uk">&c_d;&c_i;&c_z;&c_je;&#x43D;&c_t;&c_je;&c_r;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x75E2; <pr>l&#xEC;</pr></trd>,
    <trd>&#x75B6; <pr>xi&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>
