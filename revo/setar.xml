<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>setar</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="setar.0o">
  <kap><tld/>o</kap>
  <snc mrk="setar.0o.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      Persa <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>
      liuteto origine trikorda, de la 1900aj jaroj kun plusa
      kunvibra kordo, kun ligna duongutforma sonkesto, longa 25-freta kolo.
    </dif>
  </snc>
  <rim mrk="setar.0o.RIM_PARONIMO">
    Ne konfuzu kun la hinda <ref tip="vid" cel="sitar.0o">sitaro</ref>, 
    kiu probable ricevis sian nomon de setaro, sed en sia nuntempa formo 
    sufi&ccirc;e malsimilas.
  </rim>
  <adm>
    2006-06 - kelkaj tradukoj efektive konfuzas.
    <aut>&jcirc;v</aut>
  </adm>
  <trd lng="be">&c_s;&c_ib;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">c&iacute;tara</trd>
  <trd lng="en">sitar</trd>
  <trd lng="es">c&iacute;tara</trd>
  <trdgrp lng="fr">
    <trd>set&acirc;r</trd>
  </trdgrp>
  <trd lng="hu">szit&aacute;r</trd>
  <trd lng="ru">&c_s;&c_i;&c_t;&c_a;&c_r;</trd>
</drv>
</art>

