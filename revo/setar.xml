<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>setar</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="setar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Persa <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>
      liuteto origine trikorda, de la 1900aj jaroj kun plusa
      kunvibra kordo, kun ligna duongutforma sonkesto, longa 25-freta kolo.
    </dif>
  </snc>
</drv>
<trd lng="en">sitar</trd>
<trd lng="es">sitar</trd>
<trdgrp lng="fr">
  <trd>set&acirc;r</trd>
</trdgrp>
</art>

