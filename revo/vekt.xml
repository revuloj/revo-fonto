<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vekt.xml,v 1.24 2023/10/21 16:50:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vekt</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vekt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vekt.0o.pesila">
    <dif>
      <ref tip="super" cel="stang.0eto">Stangeto</ref> en <ref tip="malprt" cel="pes.0ilo">pesilo</ref>, 
      kiu balanci&gcirc;as &ccirc;irka&ubreve; sia akso kaj portas la telerojn:
      <ekz>
        <tld/>a pesilo kun du tasoj
        <fnt>
          <vrk><url ref="https://lernu.net/lt/forumo/temo/2115">peso a&ubreve; pezo?</url></vrk>
          <lok>forumo de lernu.net</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Waagebalken</trd>,
      <trd>W&auml;gebalken</trd>,
      <trd>Wiegebalken</trd>
    </trdgrp>
    <trd lng="fr">fl&eacute;au<klr> (de balance)</klr></trd>
    <trd lng="hu">m&eacute;rlegkar</trd>
    <trdgrp lng="pt">
     <trd>travess&atilde;o (de balan&ccedil;a)</trd>
    </trdgrp>
  </snc>
  <snc mrk="vekt.0o.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Balanci&gcirc;a trabo<ref tip="super" cel="stang.0o">stango</ref> en ma&scirc;ino:
      <ekz>
        en Oklahomo anka&ubreve; staris arganoj kaj ritme balanci&gcirc;is <tld/>oj
        <fnt>
          <aut>Ilja Ilf, Evgenij Petrovi&ccirc; Katajev, trad. Vladimir Okc </aut>
          <vrk><url ref="http://www.vladimir-okc.narod.ru/UU-html/truu20.htm">Unueta&gcirc;a
          Usono</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="baskul.0o.TEK">baskulo<sncref/></ref>,
      <ref cel="brak.0o.FIG">brako<sncref/></ref>,
      <ref cel="kantilevr.0o">kantilevro</ref>,
      <ref cel="stang.lev0o">levstango</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Schwingbalken</trd>,
      <trd>Balancier</trd>
    </trdgrp>
    <trd lng="en">double overhanging <ind>beam</ind></trd>
    <trd lng="fr">balancier</trd>
    <trd lng="hu">himba</trd>
  </snc>
  <snc mrk="vekt.0o.portovekto">
    <dif>
      <ref tip="dif" cel="vekt.porto0o.KOMUNE">Porto<tld/>o</ref>:
      <ekz>
        kamparanino per rapidaj pa&scirc;etoj mar&scirc;as kun vekto lokita sur <klr>[s]</klr>iaj
        &scirc;ultroj
        <fnt>
       <aut>Maryvonne kaj Bruno Robineau</aut>
          <vrk><url ref="&Monato;/2000/006831.php">La&ubreve; ritmo de la rizejoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/02, p. 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        olda laktoliverantino kun siteloj kaj <tld/>o eliranta estis el la pordo por akvo
        <fnt>
          <aut> Gajdar Arkadij</aut>
          <vrk><url ref="https://www.litmir.me/br/?b=87722&amp;p=5">Timur kaj lia
          tachmento</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Tragjoch</trd>,
      <trd>Schulterjoch</trd>
    </trdgrp>
    <trd lng="fr">palanche</trd>
    <trdgrp lng="hu">
      <trd>v&iacute;zhord&oacute; r&uacute;d</trd>,
      <trd>v&aacute;llj&aacute;rom</trd>
    </trdgrp>    
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_o;&c_m;&c_y;&c_s;&c_je;&c_l;</trd>
  <trd lng="cs">j&aacute;dro elektromagnetu</trd>
  <trd lng="en"><ofc>*</ofc>flail</trd>
  <trdgrp lng="ja">
    <trd>&#x5929;&#x79E4;&#x68D2;  <pr>&#x3066;&#x3093;&#x3073;&#x3093;&#x307C;&#x3046;</pr></trd>,
    <trd>&#x3055;&#x304A;</trd>
  </trdgrp>
  <trd lng="nl">juk</trd>
  <trd lng="pl"><ofc>*</ofc>bela wagowa</trd>
  <trd lng="ru"><ofc>*</ofc>&c_k;&c_o;&c_r;&c_o;&c_m;&c_y;&c_s;&c_l;&c_o;</trd>
  <trd lng="sk">jadro elektromagnetu</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43E;&c_r;&#x43E;&#x43C;&c_i;&c_s;&#x43B;&#x43E; (&c_d;&#x43B;&#x44F; &c_v;&c_ib;&c_d;&c_je;&c_r;</trd>,
    <trd>&c_u; &c_v;&c_a;&c_g;&c_a;&c_h;)</trd>,
    <trd>&#x44F;&c_r;&#x43C;&#x43E; (&c_v; &c_je;&#x43B;&c_je;&#x43A;&c_t;&c_r;&#x43E;&#x43C;&c_a;&c_g;&#x43D;&c_ib;&c_t;&c_ib;)</trd>
  </trdgrp>
</drv>

<drv mrk="vekt.porto0o">
  <kap>porto<tld/>o</kap>
  <snc mrk="vekt.porto0o.KOMUNE">
    <dif>
      Kurba sur&scirc;ultra ligna&jcirc;o por porti du peza&jcirc;ojn
      (korbojn, sitelojn...):
      <ekz>
        li a&ccirc;etis paron da korboj kaj havigis al si 
        porto<tld/>on, por kolporti melonojn, fruktojn, 
        ternuksojn kaj cigardojn
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="jug.0o">jugo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_o;&c_m;&c_y;&c_s;&c_je;&c_l;</trd>
  <trd lng="de">Tragjoch</trd>
  <trd lng="fr">palanche</trd>
  <trdgrp lng="hu">
    <trd>v&iacute;zhord&oacute; r&uacute;d</trd>,
    <trd>v&aacute;llj&aacute;rom</trd>
  </trdgrp>    
</drv>
</art>
