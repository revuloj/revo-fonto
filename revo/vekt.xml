<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vekt</rad>/o
</kap>
<drv mrk="vekt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Stangeto en pesilo, kiu balanci&gcirc;as &ccirc;irka&ubreve; sia akso kaj portas la
      telerojn.
    </dif>
    <trd lng="fr">fl&eacute;au<klr> (de balance)</klr></trd>
    <trd lng="hu">m&eacute;rlegkar</trd>
  </snc>
  <snc>
    <dif>
      Kurba sur&scirc;ultra ligna&jcirc;o por porti du pendantajn sitelojn.
    </dif>
    <trd lng="fr">palanche</trd>
    <trdgrp lng="hu">
      <trd>v&iacute;zhord&oacute; r&uacute;d</trd>,
      <trd>v&aacute;llj&aacute;rom</trd>
    </trdgrp>    
  </snc>
  <snc>
    <dif>
      Balanci&gcirc;a trabostango en ma&scirc;ino.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_o;&c_m;&c_y;&c_s;&c_je;&c_l;</trd>
  <trd lng="hu">himba</trd>
  <trd lng="nl">juk</trd>
  <trd lng="ru">&c_k;&c_o;&c_r;&c_o;&c_m;&c_y;&c_s;&c_l;&c_o;</trd>
</drv>
</art>
