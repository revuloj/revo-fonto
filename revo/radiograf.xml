<kap>
   <ofc>9</ofc><rad>radiograf</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="radiograf.0i">
  <kap><ofc>9</ofc><tld/>i</kap>
  <snc mrk="radiograf.0i.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Fari foton de organo per iks-radioj.
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>radiobildigi</ke>.
  </rim>

  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; &c_r;&c_e;&c_n;&c_t;&c_g;&c_je;&c_n;&c_a;&c_w;&c_s;&c_k;&c_ib; <ind>&c_z;&c_d;&c_y;&c_m;&c_a;&c_k;</ind></trd>
  <trd lng="fr">radiographier</trd>
  <trd lng="ja">&#65336;&#32218;&#20889;&#30495;&#12395;&#25774;&#12427;
  <pr>&#12360;&#12387;&#12367;&#12377;&#12379;&#12435;&#12375;&#12419;&#12375;&#12435;&#12395;&#12392;&#12427;</pr></trd>
  <trd lng="nl">een <ind>r&ouml;ntgenopname</ind> maken</trd>
  <trd lng="pl">robi&cacute;  <ind>prze&sacute;wietlenie</ind></trd>
  <trd lng="uk">&c_r;&#1086;&c_b;&c_i;&c_t;&c_i; &c_r;&c_je;&#1085;&c_t;&c_gu;&c_je;&#1085;&c_ib;&c_v;&c_s;&#1100;&#1082;&c_i;&c_j;
  &c_z;&#1085;&c_ib;&#1084;&#1086;&#1082;</trd>
</drv>

<drv mrk="radiograf.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="radiograf.0ajxo.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Foto de organo per iks-radioj:
      <ekz>
        unu asistanto forportis <tld/>a&jcirc;ojn al la fotolaboratorio
        <fnt>
          <aut>B. Golden</aut>
          <vrk><url ref="http://www.autodidactproject.org/other/golden_kosma.html">
          Kosma Ruba&jcirc;o</url></vrk>
          <lok>Monda Karuselo, 1977:10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_n;&c_t;&c_g;&c_je;&c_n;&c_a;&c_w;&c_s;&c_k;&c_ib; <ind>&c_z;&c_d;&c_y;&c_m;&c_a;&c_k;</ind></trd>
  <trd lng="fr">radiographie</trd>
  <trd lng="ja">&#65336;&#32218;&#20889;&#30495;  <pr>&#12360;&#12387;&#12367;&#12377;&#12379;&#12435;&#12375;&#12419;&#12375;&#12435;</pr></trd>
  <trdgrp lng="nl">
    <trd>radiografie</trd>,
    <trd>r&ouml;ntgenopname</trd>
  </trdgrp>
  <trd lng="pl">zdj&eogonek;cie  <ind>rentgenowskie</ind></trd>
  <trd lng="uk">&c_r;&c_je;&#1085;&c_t;&c_gu;&c_je;&#1085;&c_ib;&c_v;&c_s;&#1100;&#1082;&c_i;&c_j; &c_z;&#1085;&c_ib;&#1084;&#1086;&#1082;</trd>
</drv>

<drv mrk="radiograf.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="radiograf.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Specialisto pri <tld/>ado.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_n;&c_t;&c_g;&c_je;&c_n;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="fr">radiographiste</trd>
  <trd lng="ja">&#65336;&#32218;&#25216;&#24107;  <pr>&#12360;&#12387;&#12367;&#12377;&#12379;&#12435;&#12366;&#12375;</pr></trd>
  <trd lng="nl">radioloog</trd>
  <trd lng="pl">radiolog</trd>
  <trd lng="uk">&c_r;&c_je;&#1085;&c_t;&c_gu;&c_je;&#1085;&#1086;&#1083;&#1086;&c_g;</trd>
</drv>

</art>

