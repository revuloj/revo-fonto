<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: interv1.xml,v 1.28 2025/04/06 19:37:31 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>intervju</rad>/o <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="interv1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="interv1.0i.INF">
    <dif>
      Demandadi iun pri vivo, opinioj kaj similaj
      por posta, ofte publika raporto:
      <ekz>
        imamo de roterdama moskeo estis <tld/>ita televide
        <fnt><aut>G. Berveling</aut>:
        <vrk>Samseksemo kaj islamo</vrk>,
        <bib>Monato</bib>, <lok>2001:10, p.
        15a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_r;&c_v;&c_ib;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">interviewovat</trd>
  <trdgrp lng="de">
    <trd>befragen</trd>,
    <trd>interviewen</trd>
  </trdgrp>
  <trd lng="en">interview</trd>
  <trd lng="es">entrevistar</trd>
  <trd lng="fa">&a_m;&a_S;&a_A;&a_H;&a_b;&a_h; &f_k;&a_r;&a_d;&a_n;<klr> (&a_b;&a_A;)</klr></trd>
  <trdgrp lng="fr">
    <trd>interviewer</trd>,
    <trd>interroger<klr tip="ind"> (interviewer)</klr></trd>
  </trdgrp>
  <trd lng="hu">interj&uacute;vol</trd>
  <trdgrp lng="ja">
    <trd>&#12452;&#12531;&#12479;&#12499;&#12517;&#12540;&#12377;&#12427;  <pr>&#12356;&#12435;&#12383;&#12403;&#12421;&#12540;&#12377;&#12427;</pr></trd>,
    <trd>&#38754;&#25509;&#12377;&#12427;  <pr>&#12417;&#12435;&#12379;&#12388;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">interviewen</trd>
  <trd lng="pt">entrevistar</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_mol;&c_ju;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">interviewova&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_n;&c_t;&c_je;&c_r;&c_v;&#8217;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_i; &c_ib;&c_n;&c_t;&c_je;&c_r;&c_v;&#8217;&c_ju;</trd>
  </trdgrp>
  <trd lng="zh">&#35370;&#35527; <pr>f&#462;ngt&aacute;n</pr></trd>
</drv>

<drv mrk="interv1.0o">
  <kap><tld/>o</kap>
  <snc mrk="interv1.0o.INF">
    <dif>
      Interparolo konsistanta el demandoj kaj respondoj
      pri ies vivo, agoj a&ubreve; opinioj por raporto:
      <ekz>
        <ctl>Pri kelkaj problemoj de interlingvistiko</ctl> estas
        surpaperigo de <tld/>o kun la franca lingvisto Andr&eacute;
        Martinet
        <fnt><aut>PP</aut>, <vrk>Fascina bro&scirc;uro</vrk>,
        <bib>Monato</bib>, <lok>199311, p.
        23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ja">
    <trd>&#12452;&#12531;&#12479;&#12499;&#12517;&#12540;  <pr>&#12356;&#12435;&#12383;&#12403;&#12421;&#12540;</pr></trd>,
    <trd>&#21462;&#26448;&#35370;&#21839;  <pr>&#12375;&#12421;&#12374;&#12356;&#12411;&#12358;&#12418;&#12435;</pr></trd>,
    <trd>&#38754;&#25509;  <pr>&#12417;&#12435;&#12379;&#12388;</pr></trd>
  </trdgrp>
  <trdgrp lng="lat">
    <trd>colloquium, ii percontatiuum</trd>,
    <trd>colloquium, ii interrogatiuum</trd>
  </trdgrp>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_r;&c_v;&c_ib;&c_ju;</trd>
  <trd lng="cs">interview</trd>
  <trdgrp lng="de">
    <trd>Befragung</trd>,
    <trd>Interview</trd>
  </trdgrp>
  <trd lng="en">interview</trd>
  <trd lng="es">entrevista</trd>
  <trd lng="fa">&a_m;&a_S;&a_A;&a_H;&a_b;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>interview</trd>,
    <trd>entrevue<klr tip="ind"> (interview)</klr></trd>
    <trd>entretien</trd>
  </trdgrp>
  <trd lng="hu">interj&uacute;</trd>
  <trd lng="nl">interview</trd>
  <trd lng="pt">entrevista</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_mol;&c_ju;</trd>
  <trd lng="sk">interview</trd>
  <trd lng="uk">&c_ib;&c_n;&c_t;&c_je;&c_r;&c_v;&#8217;&c_ju;</trd>
  <trdgrp lng="zh">
    <trd>&#38754;&#35430; <pr>mi&agrave;nsh&igrave;</pr></trd>,
    <trd>&#35370;&#35527; <pr>f&#462;ngt&aacute;n</pr></trd>,
    <trd>&#26212;&#35527; <pr>w&ugrave;t&aacute;n</pr></trd>,
    <trd>&#38754;&#35527; <pr>mi&agrave;nt&aacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="interv1.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="interv1.0igxi.INF">
    <dif>
      Respondi al demandoj de <tld/>o:
      <ekz>
        &ccirc;u vi venas <klr>...</klr> al nia &ccirc;efsidejo por
        <tld/>i&gcirc;i?
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>
          <vrk>Montara Vila&gcirc;o</vrk>
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_ib;&c_n;&c_t;&c_e;&c_r;&c_v;&c_ib;&c_ju;</ind></trd>
  <trd lng="en">be <ind>interview</ind>ed</trd>
  <trdgrp lng="fr">
    <trd>donner un <ind>entretien</ind></trd>,
    <trd>se faire <ind>interviewer</ind></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12452;&#12531;&#12479;&#12499;&#12517;&#12540;&#12434;&#21463;&#12369;&#12427;  <pr>&#12356;&#12435;&#12383;&#12403;&#12421;&#12540;&#12434;&#12358;&#12369;&#12427;</pr></trd>,
    <trd>&#38754;&#25509;&#12434;&#21463;&#12369;&#12427;  <pr>&#12417;&#12435;&#12379;&#12388;&#12434;&#12358;&#12369;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#38754;&#35430; <pr>mi&agrave;nsh&igrave;</pr></trd>,
    <trd>&#21463;&#35370; <pr>sh&ograve;uf&#462;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="interv1.dung0o">
  <kap>dung<tld/>o,
  <var><kap>labor<tld/>o</kap></var>
  </kap>
  <snc mrk="interv1.dung0o.EKON">
    <dif>
      <tld lit="I"/>o per kiu proponanto de laboro esploras la pretecon de kandidato:
      <ekz>
        imagu, kion mi a&ubreve;dis dum dung<tld/>o:
        &leftquot;&ccirc;u vi havas sperton?&#8221;
        &leftquot;jes, multan, jam 50 <tld/>ojn.&#8221;
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://tatoeba.org/tgl/sentences/show/6736127">Frazo 6736127a</url></vrk>
          <lok>Tatoeba, [2018?]</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi sukcesis pri la unua labor<tld/>o
        <fnt>
          <aut>noktema</aut>
          <vrk><url ref="https://www.youtube.com/watch?v=rB2ZE3dyyeA">
          E-bloga&jcirc;o - saluton ree</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_r;&c_a;&c_z;&c_m;&c_o;&c_w;&c_je; <klr>(&c_p;&c_r;&c_y; &c_p;&c_r;&c_y;&c_jo;&c_m;&c_je; &c_n;&c_a; &c_p;&c_r;&c_a;&c_c;&c_u;)</klr></trd>
  <trd lng="en">employment <ind>interview</ind></trd>
  <trd lng="fr">entretien d'embauche</trd>
  <trd lng="zh">&#38754;&#35430; <pr>mi&agrave;nsh&igrave;</pr></trd>
</drv>

</art>
