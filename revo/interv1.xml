<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>intervju</rad>/o
</kap>
<drv mrk="interv1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Interparolado de gazetisto kun fama persono por demandi lin pri
      lia vivo, agoj a&ubreve; opinioj kaj raporti en gazeto. 
    </dif>
  </snc>
  <trd lng="de">Interview</trd>
  <trd lng="en">interview</trd>
  <trdgrp lng="fr">
    <trd>interview</trd>,
    <trd>entretien</trd>
  </trdgrp>
  <trdgrp lng="lat">
    <trd>colloquium, ii percontatiuum</trd>,
    <trd>colloquium, ii interrogatiuum</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_mol;&c_ju;</trd>
</drv>
<drv mrk="interv1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Demandi iun per <tld/>o.  
    </dif>
  </snc>
  <trd lng="de">interviewen</trd>
  <trd lng="fr">interviewer</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_mol;&c_ju;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
