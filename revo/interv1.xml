<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>intervju</rad>/o
</kap>
<drv mrk="interv1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Interparolo kun fama persono por demandi lin pri
      lia vivo, agoj a&ubreve; opinioj kaj publike raporti, 
      ekz-e en gazeto:
      <ekz>
        <ctl>Pri kelkaj problemoj de interlingvistiko</ctl> estas
        surpaperigo de <tld/>o kun la franca lingvisto Andr&eacute;
        Martinet
        <fnt><aut>PP</aut>: <vrk>Fascina bro&scirc;uro</vrk>,
        <bib>Monato</bib><lok>jaro 1993a, numero 11a, p. 23a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_r;&c_v;&c_ib;&c_ju;</trd>
  <trdgrp lng="de">
    <trd>Befragung</trd>,
    <trd>Interview</trd>
  </trdgrp>
  <trd lng="en">interview</trd>
  <trd lng="es">entrevista</trd>
  <trdgrp lng="fr">
    <trd>interview</trd>,
    <trd>entrevue<klr tip="ind"> (interview)</klr></trd>
    <trd>entretien</trd>
  </trdgrp>
  <trd lng="hu">interj&uacute;</trd>
  <trdgrp lng="lat">
    <trd>colloquium, ii percontatiuum</trd>,
    <trd>colloquium, ii interrogatiuum</trd>
  </trdgrp>
  <trd lng="nl">interview</trd>
  <trd lng="pt">entrevista</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_mol;&c_ju;</trd>
</drv>

<drv mrk="interv1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Demandadi iun <tld/>e:
      <ekz>
        imamo de roterdama moskeo estis <tld/>ita televide
        <fnt><aut>Gerrit Berveling</aut>: 
        <vrk>Samseksemo kaj islamo</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 10a, p. 15a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_r;&c_v;&c_ib;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>befragen</trd>,
    <trd>interviewen</trd>
  </trdgrp>
  <trd lng="es">entrevistar</trd>
  <trdgrp lng="fr">
    <trd>interviewer</trd>,
    <trd>interroger<klr tip="ind"> (interviewer)</klr></trd>
  </trdgrp>
  <trd lng="hu">interj&uacute;vol</trd>
  <trd lng="nl">interviewen</trd>
  <trd lng="pt">entrevistar</trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_v;&c_mol;&c_ju;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
