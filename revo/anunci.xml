<art mrk="$Id: anunci.xml,v 1.21 2023/10/12 18:05:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>Anunciaci</rad>/o
</kap>
<drv mrk="anunci.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="anunci.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      <ref tip="lst" lst="voko:festoj" cel="fest.0o.KAL">Festo</ref>
      memoriganta
      la anoncon pri la naski&gcirc;o de Jesuo, faritan de an&gcirc;elo al Maria;
      resp. tiu evento mem:
      <ekz>
        Bernardo el Turingujo en la jaro 960 anta&ubreve;diris proksiman finon
        de la mondo, li e&ccirc; difinis la tempon,
        nome, kiam la festo de <tld/>o falos sur la Grandan Vendredon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam oni promenas en la centro de &Hcirc;arkivo anta&ubreve; la okuloj aperas
        eble la plej
        impona konstrua&jcirc;o &ndash; katedralo de <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk><url ref="&Monato;/2009/009672.php">Ortodoksaj pre&gcirc;ejoj en la &hcirc;arkiva regiono</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        pia tradicio diras, ke &gcirc;i efektive estas la dometo, lo&gcirc;ita de
        Maria en Nazareto,
        kie &scirc;i estis salutata de An&gcirc;elo Gabrielo en la <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2015/011299.php">La Loreta Vojo: La&ubreve; la spuroj de antikvaj
              pilgrimantoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_Z;&c_mol;&c_v;&c_je;&c_s;&c_t;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_D;&c_a;&c_b;&c_r;&c_a;&c_v;&c_je;&c_sh;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="br">gouel Maria-Veurzh</trd>
  <trd lng="ca">Anunciaci&oacute;</trd>
  <trd lng="de">Mari&auml; <ind>Verk&uuml;ndigung</ind></trd>
  <trd lng="en">annunciation</trd>
  <trd lng="es">Anunciaci&oacute;n</trd>
  <trd lng="fr">Annonciation</trd>
  <trd lng="hu">Gy&uuml;m&ouml;lcsolt&oacute; Boldogasszony<klr tip="ind">
  (&uuml;nnep)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x53D7;&#x80CE;&#x544A;&#x77E5;  <pr>&#x3058;&#x3085;&#x305F;&#x3044;&#x3053;&#x304F;&#x3061;</pr></trd>,
    <trd>&#x304A;&#x544A;&#x3052;&#x306E;&#x795D;&#x65E5;  <pr>&#x304A;&#x3064;&#x3052;&#x306E;&#x3057;&#x3085;&#x304F;&#x3058;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_l;&c_a;&c_g;&c_o;&c_v;&c_je;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="uk">&#x411;&#x43B;&#x430;&#x433;&#x43E;&#x432;&#x456;&#x449;&#x435;&#x43D;&#x43D;&#x44F;</trd>
</drv>
