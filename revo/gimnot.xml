<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>gimnot</rad>/o
</kap>
<drv mrk="gimnot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc mrk="gimnot.0o.genro">
    <dif>
      Genro el familio <ref tip="super" cel="gimnot.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Gymnotus</trd>), simila al angilo, kun organo,
      produktantan malfortajn elektrajn mal&scirc;argojn uzatajn por orienti&gcirc;o.
    </dif>
    <trd lng="de">Messeraal</trd>
  </snc>
  <snc mrk="gimnot.0o.evitinda">
    <uzo tip="klr">(evitinda)</uzo>
    <dif>
      Simila fi&scirc;o (<trd lng="la">Electrophorus</trd>), produktanta
      tre fortajn elektrajn mal&scirc;argojn:
      <ekz>
        la elektraj mal&scirc;argoj de <tld/>o povas paralizi sufi&ccirc;e
        grandan beston.
      </ekz>
    </dif>
    <trd lng="de">Zitteraal</trd>
  </snc>
</drv>
<drv mrk="gimnot.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="ciprin.0oformaj">ciprinoformaj
      fi&scirc;oj</ref> (<trd lng="la">Gymnotidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Messeraale <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
