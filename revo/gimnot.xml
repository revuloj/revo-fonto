<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gimnot.xml,v 1.22 2023/08/31 14:29:53 revo Exp $">
<kap>
  <rad>gimnot</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gimnot.0o">
  <kap><tld/>o</kap>
  <snc mrk="gimnot.0o.familio">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Angilosimila <ref tip="lst" cel="fisx.0o" lst="voko:fi&scirc;oj">fi&scirc;o</ref>
      el familio <ref tip="malprt" cel="gimnot1.0oj">gimnotedoj</ref>,
      kun klingoforma konturo, vivanta en malrapidfluaj regionoj de
      grandaj 
      riveroj kaj mar&ccirc;oj kaj kapabla produkti elektrajn mal&scirc;argojn per
      adaptitaj muskol&ccirc;eloj:
    </dif>
      <subsnc mrk="gimnot.0o.genro">
        <dif>
          malfortajn por orienti&gcirc;o kaj interkomunikado
          (<ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> 
            <trd lng="la">Gymnotus</trd>);
        </dif>
        <trd lng="ca">gimnot</trd>
        <trd lng="de">Messeraal</trd>
        <trd lng="fr">gymnote</trd>
        <trdgrp lng="hu">
          <trd>villamos <ind>angolna</ind></trd>,
          <trd>sajgat&oacute;hal</trd></trdgrp> 
        <trd lng="nl">mesaal</trd>
      </subsnc>
      <subsnc mrk="gimnot.0o.elktra">
        <dif>
          fortajn por &ccirc;asado (genro <trd lng="la">Electrophorus</trd>): 
          <ekz>
            la elektraj mal&scirc;argoj de <tld/>o povas paralizi sufi&ccirc;e
            grandan beston.
          </ekz>
        </dif>
        <trd lng="ca"><ind>anguila</ind> el&egrave;ctrica</trd>
        <trd lng="de">Zitteraal</trd>
        <trdgrp lng="fr">
          <trd>anguille &eacute;lectrique</trd>,
          <trd>gymnote &eacute;lectrique</trd></trdgrp>
        <trd lng="nl">sidderaal</trd>
     </subsnc>
  </snc>
  <trd lng="cs">elektrick&yacute; &uacute;ho&rcaron;</trd>
  <trd lng="ja">&#x30AE;&#x30E0;&#x30CE;&#x30FC;&#x30C8;&#x30A5;&#x30B9;</trd>
  <trdgrp lng="pt">
    <trd>gimnoto</trd>,
    <trd>ginoto</trd>,
    <trd>peixe-el&eacute;trico</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_u;&c_g;&c_o;&c_r;&c_mol;</trd>
  <trd lng="sk">elektrick&yacute; &uacute;hor</trd>
  <trd lng="uk">&#x435;&#x43B;&#x435;&#x43A;&#x442;&#x440;&#x438;&#x447;&#x43D;&#x438;&#x439; &#x432;&#x443;&#x433;&#x43E;&#x440;</trd>
</drv>
</art>

