<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&Gcirc;enov</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gxenov.0o">
  <kap><tld/>o</kap>
  <snc mrk="gxenov.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Italuja <ref tip="lst" cel="urb.0o" lst="voko:urboj">urbo</ref>, 
      &ccirc;efurbo de <ref tip="malprt" cel="ligur.0ujo">Ligurujo</ref>:
      <ekz>
        &Ccirc;i&ccirc;erin preparis la planon por agado en <tld/>o
        <fnt>
          <aut>I. Lapenna</aut>,
          <vrk><url ref="https://www.youtube.com/watch?v=ZN-MZQEq8xs">
          [prelego pri la komunismo kaj Stalin]</url></vrk>,
          <lok>[1986]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_je;&c_n;&c_u;&c_ja;</trd>
  <trd lng="it">Genova</trd>
  <trd lng="en">Genoa</trd>
  <trd lng="fr">G&ecirc;nes</trd>
  <trd lng="kk">&c_G;&c_je;&c_n;&c_u;&c_ja;</trd>
  <trd lng="la">Genua</trd>
  <trd lng="lat">Genua</trd>
  <trd lng="mn">&c_G;&c_je;&c_n;&c_u;&c_ja;</trd>
  <trd lng="ru">&c_G;&c_je;&c_n;&c_u;&c_ja;</trd>
  <trd lng="uk">&c_G;&c_je;&c_n;&c_u;&c_ja;</trd>
</drv>
</art>

