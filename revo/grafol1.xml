<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>grafologi</rad>/o
</kap>
<drv mrk="grafol1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Arto ekkoni ies karakteron per ekzameno de lia skribo.  
    </dif>
  </snc>
  <trd lng="de">Graphologie</trd>
  <trdgrp lng="en">
    <trd>graphology</trd>,
    <trd>handwriting analysis</trd>
  </trdgrp>
  <trd lng="fr">graphologie</trd>
  <trd lng="nl">grafologie</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_f;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>
<drv mrk="grafol1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Specialisto pri <tld/>o.
    </dif>
    <ref tip="sin" cel="grafol.0o">grafologo</ref>
  </snc>
  <trd lng="de">Graphologe</trd>
  <trdgrp lng="en">
    <trd>graphologist</trd>,
    <trd>handwirting analyst</trd>,
    <trd>handwriting expert</trd>
  </trdgrp>
  <trd lng="fr">graphologue</trd>
  <trd lng="nl">grafoloog</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_f;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
