<art mrk="$Id: metret.xml,v 1.4 2023/11/12 15:25:22 revo Exp $">
<kap>
    <rad>metret</rad>/o <fnt><bib>NT</bib></fnt>
</kap>
<drv mrk="metret.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="metret.0o.mezurunuo">
    <dif>
      Malnova greka mezurunuo de volumeno, proksimume du siteloj (39l):
      <ekz>
        ses akvokuvoj &scirc;tonaj <klr>[...]</klr> enhavantaj po du &gcirc;is tri
        metretoj
        <fnt>
          <bib>NT</bib>
          <lok>Johano 2:6</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_r;&c_e;&c_t;</trd>
  <trd lng="de">Metretes</trd>
  <trd lng="fr">m&eacute;tr&egrave;te</trd>
  <trd lng="uk">&#x43C;&c_je;&c_t;&c_r;&c_je;&c_t;</trd>
</drv>
</art>

