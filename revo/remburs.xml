<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: remburs.xml,v 1.3 2023/09/03 06:29:19 afido Exp $">
    <kap>
      <rad>remburs</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="remburs.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="sum.mon0o">Monsumo</ref> pagata de akceptanto de senda&jcirc;o,
      kaj kiun la po&scirc;to pagas la la sendinto:
      <ekz>
        en kazo de mendo kontra&ubreve; <tld/>o ne eblis devige konfirmi la mendon (erare
        indikita telefonnumero, neatingebla, ne respondas al retpo&scirc;to, ktp.)
        <fnt>
          <vrk><url ref="https://www.espero.sk/eo/obchodne-podmienky/"
              >Uzkondi&ccirc;oj de la vendejo</url></vrk>
          <lok>Nuligo de mendo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de internaciaj biletoj <klr>[&#8230;]</klr> pere de la reciproka monkompenso
        <fnt>
          <vrk><url ref="http://ifef.free.fr/bultenoj/IF%201959-10%20(12-1).pdf"
              >La remburso de internaciaj biletoj</url></vrk>
          <lok>La Internacia Fervojisto, n-ro 1, okt. 1959, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Nachnahme</trd>
    <trd lng="en">cash on delivery</trd>
    <trd lng="fr">remboursement</trd>
    <trd lng="nl">rembours</trd>
  </snc>
</drv>

  </art>

