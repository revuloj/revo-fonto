<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Kasiope</rad>/o
</kap>
<drv mrk="kasiope.0o">
  <kap><tld/>o</kap>
  <mlg>Cas</mlg>
  <snc mrk="kasiope.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera
      <ref tip="super" cel="stel.0figuro">stelfiguro</ref> 
      (<trd lng="la">Cassiopeia</trd>).
    </dif>
  </snc>
  <snc mrk="kasiope.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <dif>
      En la helena mito, vantema 
      <ref tip="vid" cel="nimf.0o.MIT">nimfo</ref>, patrino de 
      <ref tip="vid" cel="andromed.0o.MIT">Andromedo</ref>, tranformita
      en la samnoma stelaro.
    </dif>
  </snc>
  <trd lng="de">Cassiopeia</trd>
  <trd lng="en">Cassiopeia</trd>
  <trd lng="es">Casiopea</trd>
  <trd lng="fr">Cassiop&eacute;e</trd>
  <trd lng="it">Cassiopea</trd>
  <trd lng="pl">Kasiopea</trd>
</drv>
</art>
