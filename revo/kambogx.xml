<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kambogx.xml,v 1.14 2023/12/02 21:20:05 revo Exp $">
<kap>
  <rad>Kambo&gcirc;</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="kambogx.K0o">
  <kap>
    <tld lit="K"/>o
  </kap>
  <mlg kod="ISO-3166">KH</mlg>
  <snc mrk="kambogx.K0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o.POL">&Scirc;tato</ref>
      en la sudorienta Azio kun &ccirc;efurbo Pnompeno; teritorio de Regno <tld/>o,
      limanta kun
      <ref tip="vid" cel="laos.L0o.GEOG">Laoso</ref>,
      <ref tip="vid" cel="taj.T0lando.GEOG">Tajlando</ref>,
      <ref tip="vid" cel="vjetnam.V0ujo.GEOG">Vjetnamujo</ref>
      kaj Taja Golfo:
      <ekz>
        <nom>Henry Kissinger</nom> &ndash; gajninto de Nobelpremio en 1973 &ndash; respondecis pri la
        bombardadoj en <tld/>o en 1969 kaj 1970, dum kiuj proksimume 800&#x202F;000
        civiluloj pereis
        <fnt>
          <bib>Monato</bib>
          <aut>Edward Kusters</aut>
          <vrk><url ref="&Monato;/2006/009100.php">Alfred Nobel: nobelo inter la sciencistoj kaj lingvistoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas konstitucia monarkio
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009660.php">Du mil ok</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la munda lingvaro <klr>[&#x2026;]</klr> havas rilaton kun a&ubreve;stral-aziaj lingvoj de
        Asamo kaj Hindo&ccirc;inio (inkluzive de la &hcirc;mera parolata en <tld/>o)
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011358.php">Notoj indaj pri lingvaroj hindaj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_K;&c_a;&c_m;&c_b;&c_o;&c_d;&c_zh;&c_a;</trd>,
    <trd>&c_K;&c_a;&c_m;&c_p;&c_u;&c_ch;&c_y;&c_ja;</trd>
  </trdgrp>
  <trd lng="bg">&c_K;&c_a;&c_m;&c_b;&c_o;&c_d;&c_zh;&c_a;</trd>
  <trd lng="ca">Cambodja</trd>
  <trd lng="de">Kambodscha</trd>
  <trd lng="en">Cambodia</trd>
  <trdgrp lng="fr">
    <trd>Cambodge</trd>,
    <trd>Royaume du Cambodge</trd>
  </trdgrp>
  <trd lng="id">Kamboja</trd>
  <trd lng="ja">&#x30AB;&#x30F3;&#x30DC;&#x30B8;&#x30A2;  <pr>&#x304B;&#x3093;&#x307C;&#x3058;&#x3042;</pr></trd>
  <trdgrp lng="pl">
    <trd>Kambod&zdot;a</trd>,
    <trd>Kr&oacute;lestwo <ind>Kambod&zdot;y</ind></trd>
  </trdgrp>
  <trd lng="pt">Camboja</trd>
  <trd lng="uk">&#x41A;&#x430;&#x43C;&#x431;&#x43E;&#x434;&#x436;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x67EC;&#x57D4;&#x5BE8; <pr>Ji&#x1CE;np&#x1D4;zh&#xE0;i</pr></trd>,
    <trd>&#x67EC;&#x57D4;&#x5BE8; <pr>ji&#x1CE;np&#x1D4;zh&#xE0;i</pr></trd>,
    <trd>&#x9AD8;&#x68C9; <pr>g&#x101;omi&#xE1;n</pr></trd>
  </trdgrp>
</drv>

</art>

