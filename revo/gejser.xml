<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gejser.xml,v 1.26 2023/11/11 10:00:39 revo Exp $">
<kap>
  <rad>gejser</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gejser.0o">
  <kap><tld/>o</kap>
  <snc mrk="gejser.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="super" cel="font.0o.loko">Fonto</ref>, el&scirc;prucanta intermite 
      varman akvon, eventuale anka&ubreve; vaporon a&ubreve; &scirc;limon:
      <ekz>
        la <tld/>o skande funkciis dum almena&ubreve; 10 minutoj
        <fnt>
          <bib>ChM</bib>
        </fnt>;
      </ekz>
      <ekz>
        en Islando precizege kiel horlo&gcirc;oj &jcirc;etas sin la
        <tld/>oj al la malserena &ccirc;ielo
        <fnt><aut>I. Nemere</aut>, <vrk>Sur Kampo Granita</vrk>, 
        <lok>1983</lok></fnt>;
      </ekz>
      <ekz>
        la vila&gcirc;o fieras pri e&ubreve;ropa unika&jcirc;o &ndash; malvarmega <tld/>o, kiu sputas akvon el
        profundo de 405&#x202F;m &gcirc;is alteco de 30&#x202F;-&#x202F;40&#x202F;m
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010472.php">Varmaj vortoj por malvarmaj akvoj</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="fontan.0o.konstruajxo">fontano<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_j;&c_z;&c_e;&c_r;</trd>
  <trd lng="bg">&c_g;&c_je;&c_j;&c_z;&c_je;&c_r;</trd>
  <trd lng="ca">gu&egrave;iser</trd>
  <trd lng="cs">gejz&iacute;r</trd>
  <trdgrp lng="de">
    <trd>Geiser</trd>,
    <trd>Geysir</trd>
  </trdgrp>
  <trd lng="en">geyser</trd>
  <trd lng="es">g&eacute;iser</trd>
  <trd lng="fa">&a_A_madda;&a_b;&a_f;&a_sh;&a_A;&a_n;</trd>
  <trd lng="fr">geyser</trd>
  <trd lng="hu">gejz&iacute;r</trd>
  <trd lng="ja">&#x9593;&#x6B20;&#x6CC9;  <pr>&#x304B;&#x3093;&#x3051;&#x3064;&#x305B;&#x3093;</pr></trd>
  <trd lng="nl">geiser<klr tip="ind"> (warmwaterbron)</klr></trd>
  <trd lng="pt">g&ecirc;iser</trd>
  <trd lng="ru">&c_g;&c_je;&c_j;&c_z;&c_je;&c_r;</trd>
  <trd lng="sk">gejz&iacute;r</trd>
  <trd lng="uk">&#x433;&#x435;&#x439;&#x437;&#x435;&#x440;</trd>
  <trdgrp lng="zh">
    <trd>&#x70E7;&#x6C34;&#x9505;&#x7089; <pr>sh&#x101;oshu&#x1D0;gu&#x14D;l&#xFA;</pr></trd>,
    <trd>&#x9593;&#x6B47;&#x6CC9; <pr>ji&#xE0;nxi&#x113;qu&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>
