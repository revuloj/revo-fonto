<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc> <rad>raci</rad>/o</kap>
<drv mrk="raci.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Kapablo decidi per logika rezonado:
      <ekz>
        la lingvo fari&gcirc;as io sankta, io fea, io fabela, io mita
        &dash; io, kio situas ekster la kampo de <tld/>o
        <fnt>
        <aut>Claude Piron</aut>:
        <vrk>Psikologiaj aspektoj de la monda lingvoproblemo
        kaj de Esperanto</vrk>,
        <lok>
        Prelego prezentita en Bazelo dum la Trilanda
        Renkonti&gcirc;o, 1998-03-21.</lok></fnt>;
      </ekz>
      <ekz>
        en la sfero de la pura <tld/>o la ekzisto de Dio estas
        nepruvebla
        <fnt><bib>MkM</bib>, <lok>P.10.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_z;&c_u;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Vernunft</trd>,
    <trd>Verstand</trd>,
    <trd>Ratio</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>raison<klr> (capacit&eacute; de raisonner)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;sz</trd>,
    <trd>r&aacute;ci&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>rede</trd>,
    <trd>ratio</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>raz&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_s;&c_u;&c_d;&c_o;&c_k;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_u;&c_m;</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rnuft</trd>
</drv>

<drv mrk="raci.0a">
  <kap><tld/>a</kap>
  <snc>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Posedanta la <tld/>on:
      <ekz>
        homo estos <tld/>a esta&jcirc;o.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_u;&c_m;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>vernunftbegabt</trd>,
      <trd>vern&uuml;nftig</trd>
    </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;sz-</trd>,
    <trd>racion&aacute;lis</trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_u;&c_m;&c_n;&c_y;&c_j;</trd>;
      <trd>&c_r;&c_a;&c_s;&c_s;&c_u;&c_d;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>f&ouml;rnuftig</trd>,
      <trd>rationell</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Bazita sur la <tld/>o, konforma al la <tld/>o:
      <ekz>
        <tld/>a rezono, decido;
      </ekz>
      <ekz>
        <tld/>a lingvo internacia<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_u;&c_m;&c_n;&c_y;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_u;&c_m;&c_o;&c_v;&c_y;</trd>,
      <trd>&c_r;&c_a;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>vern&uuml;nftig</trd>,
      <trd>logisch begr&uuml;ndet</trd>
    </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;szszer&udblac;</trd>,
    <trd>racion&aacute;lis</trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_u;&c_m;&c_n;&c_y;&c_j;</trd>,

<trd>&c_r;&c_a;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="sv">
      <trd>f&ouml;rnuftsenlig</trd>,
      <trd>rationell</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>raisonnable</trd>,
    <trd>rationnel</trd>
  </trdgrp>
  <trd lng="nl">redelijk</trd>
</drv>

<drv mrk="raci.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Konformeco al <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_z;&c_u;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,

<trd>&c_r;&c_a;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Vernunftgem&auml;&szlig;heit</trd>,
    <trd>Rationalit&auml;t</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>rationalit&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;szszer&udblac;s&eacute;g</trd>,
    <trd>racionalizmus</trd>
  </trdgrp>
  <trd lng="nl">redelijkheid</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_u;&c_m;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,

<trd>&c_r;&c_a;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sv">f&ouml;rnuftighet</trd>
</drv>

<drv mrk="raci.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi konforma al la <tld/>o.
    </dif>
    <trd lng="de">vern&uuml;nftig gestalten</trd>
  </snc>
  <snc>
    <dif>
      Metode plibonigi kaj pliefikigi:
      <ekz>
        <tld/>igi la laboron en la industrio,
        <tld/>igi la produktadon.
      </ekz>
    </dif>
    <trd lng="de">rationalisieren</trd>
    <trdgrp lng="fr">
      <trd>rationaliser</trd>
    </trdgrp>
    <trd lng="nl">rationaliseren</trd>
  </snc>
  <trd
lng="be">&c_r;&c_a;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;szszer&udblac;s&iacute;t</trd>,
    <trd>racionaliz&aacute;l</trd>
  </trdgrp>
  <trd
lng="ru">&c_r;&c_a;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="raci.0ismo">
  <kap><tld/>ismo</kap>
  <uzo tip="fak">FIL</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="racion.0ismo">Racionalismo</ref>.
    </dif>
  </snc>
  <trd
lng="be">&c_r;&c_a;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="de">Rationalismus</trd>
  <trd lng="fr">rationalisme</trd>
  <trd lng="hu">racionalizmus</trd>
  <trd lng="nl">rationalisme</trd>
  <trd
lng="ru">&c_r;&c_a;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="sv">rationalism</trd>
</drv>
</art>
