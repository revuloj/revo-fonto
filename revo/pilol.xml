<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pilol.xml,v 1.30 2024/02/17 06:43:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pilol</rad>/o
</kap>

<drv mrk="pilol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pilol.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Medikamento sub formo de glutebla buleto:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni devas gluti la maldol&ccirc;an <tld/>on
        <klr>(ni devas rezigneme elporti tiun
        neeviteblan malagrabla&jcirc;on)</klr>;
      </ekz>
      <ekz>
        tiun noktmezon enlite, sentante doloron en la brusto, li metis
        du <tld/>ojn sub la langon
        <fnt>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2002/pdf/monato200201.pdf#page=28">Koratako</url></vrk>
          <bib>Monato</bib>
          <lok>2002/01, p. 28-34</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kapsul.0o.MED">kapsulo</ref>,
      <ref cel="oblat.0o.MED">oblato</ref>,
      <ref cel="pastel.0o.MED">pastelo</ref>,
      <ref cel="prem.0ajxo.MED">prema&jcirc;o</ref>,
      <ref cel="tablojd.0o">tablojdo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_l;&c_ju;&c_l;&c_ja;</trd>
  <trdgrp lng="bg">
    <trd>&c_t;&c_a;&c_b;&c_l;&c_je;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_h;&c_a;&c_p;&c_ch;&c_je;</trd>
  </trdgrp>
  <trd lng="bo">&#xF62;&#xF72;&#xF63;&#xF0B;&#xF56;&#xF74;&#xF0B;</trd>
  <trd lng="br">pilulenn</trd>
  <trd lng="ca">p&iacute;ndola</trd>
  <trdgrp lng="cs">
    <trd>l&eacute;k v pilulk&aacute;ch</trd>,
    <trd>pilulka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pille</trd>,
    <trd>Tablette</trd>
  </trdgrp>
  <trd lng="en">pill</trd>
  <trd lng="es">p&iacute;ldora</trd>
  <trd lng="fr">pilule</trd>
  <trdgrp lng="hu">
    <trd>pirula</trd>,
    <trd>labdacs</trd>
  </trdgrp>
  <trd lng="id">pil</trd>
  <trdgrp lng="ja">
    <trd>&#x9320;&#x5264;  <pr>&#x3058;&#x3087;&#x3046;&#x3056;&#x3044;</pr></trd>,
    <trd>&#x4E38;&#x85AC;  <pr>&#x304C;&#x3093;&#x3084;&#x304F;</pr></trd>,
    <trd>&#x30D4;&#x30EB;</trd>
  </trdgrp>
  <trd lng="nl">pil</trd>
  <trdgrp lng="pl">
    <trd>pigu&lstroke;ka</trd>,
    <trd>tabletka</trd>
  </trdgrp>
  <trd lng="pt">p&iacute;lula</trd>
  <trd lng="ru">&c_p;&c_i;&c_l;&c_ju;&c_l;&c_ja;</trd>
  <trd lng="sk">tabletka</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_a;&c_b;&#x43B;&c_je;&c_t;&#x43A;&c_a;</trd>,
    <trd>&#x43F;&c_ib;&#x43B;&#x44E;&#x43B;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E38; <pr>w&#xE1;n</pr></trd>,
    <trd>&#x836F;&#x4E38; <pr>y&#xE0;ow&#xE1;n</pr></trd>,
    <trd>&#x85E5;&#x4E38; <pr>y&#xE0;ow&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>
