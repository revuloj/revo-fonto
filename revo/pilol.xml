<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pilol</rad>/o
</kap>
<drv mrk="pilol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">FAR</uzo>
  <snc>
    <dif>
      Medikamento sub formo de buleto:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni devas gluti la maldol&ccirc;an <tld/>on
        <klr>(rezignacie elporti tiun
        neeviteblan malagrabla&jcirc;on)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Pille</trd>
  <trd lng="nl">pil</trd>
  <trd lng="ru">&c_p;&c_i;&c_l;&c_ju;&c_l;&c_ja;</trd>
</drv>
</art>
