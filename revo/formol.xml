<kap>
  <rad>formol</rad>/o
</kap>
<drv mrk="formol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Akva <ref tip="super" cel="solv.0ajxo">solva&jcirc;o</ref>
      de <ref tip="vid" cel="formal1.0o">formaldehido</ref>:
      <ekz>
        la konsumantoj malfidas la produktojn, pro la ripetataj skandaloj: falsita
        lakto, infektita porka&jcirc;o, brasiko kun <tld/>o, ktp
        <fnt>
          <aut>Martine Bulard</aut>
          <vrk><url ref="&Mondediplo;article2297.html">La limoj de la malkolektivigo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kelkaj ideoj <klr>[&#x2026;]</klr> &scirc;vebas en azoto, a&ubreve; en <tld/>o, sen ajna ligo kun la
        sociaj kondi&ccirc;oj kiuj anta&ubreve;enigus ilin
        <fnt>
          <aut>Serge Halimi</aut>
          <vrk><url ref="&Mondediplo;article1299.html">Oligar&hcirc;io, Socialista Partio kaj
          Bernard-Henri L&eacute;vy</url></vrk>
        </fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_r;&c_m;&c_a;&c_l;&c_ib;&c_n;</trd>
  <trd lng="bg">&c_f;&c_o;&c_r;&c_m;&c_a;&c_l;&c_i;&c_n;</trd>
  <trd lng="cs">formol</trd>
  <trd lng="de">Formalin</trd>
  <trd lng="en">formalin</trd>
  <trd lng="es">formol</trd>
  <trd lng="fr">formol</trd>
  <trd lng="hu">formalin</trd>
  <trd lng="ja">&#x30DB;&#x30EB;&#x30DE;&#x30EA;&#x30F3;</trd>
  <trd lng="nl">formol</trd>
  <trd lng="pl">formalina</trd>
  <trdgrp lng="pt">
    <trd>formol</trd>,
    <trd>formalina</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_o;&c_r;&c_m;&c_a;&c_l;&c_i;&c_n;</trd>
  <trd lng="sk">formal&iacute;n</trd>
  <trd lng="uk">&c_f;&#x43E;&c_r;&#x43C;&c_a;&#x43B;&c_ib;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x798F;&#x99AC;&#x6797; <pr>f&#xFA;m&#x1CE;l&#xED;n</pr></trd>,
    <trd>&#x7532;&#x919B;&#x6C34; <pr>ji&#x1CE;qu&#xE1;nshu&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>
