<vortaro>
  <art mrk="$Id: palestin.xml,v 1.11 2023/10/20 14:25:19 revo Exp $">
    <kap>
      <rad>Palestin</rad>/o
    </kap>

<drv mrk="palestin.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo><uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">Regiono</ref> amba&ubreve;flanke de la nuna
      limo
      inter <ref tip="malprt" cel="israel.I0o">Israelo</ref> kaj <ref tip="malprt" cel="jordan.J0o">Jordanio</ref>:
      <ekz>
        ni vokos al <tld/>o &ccirc;ion, kio ne man&gcirc;as porkan viandon
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        hebreaj patriotoj diras, ke en siaj landoj de naski&gcirc;o ili devas
        esti plenrajtaj
        regnanoj kaj tamen en <tld/>o iam devas mastrumi nur la hebreoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Pri la homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        trans maro blua de l' Helenoj ku&scirc;as antikva <tld/>o
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        la romkatolika papo vizitas <tld/>on kaj Israelon, lulejon kaj
        ludejon de tiom
        da religioj kaj eklezioj
        <fnt>
	<aut>Jorge Camacho</aut>
	<vrk><url ref="&Monato;/2000/006820.php">Plia libro biblia</url></vrk>
	<bib>Monato</bib>
	<lok>2000/08, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne legas ion ajn kritikantan la uzurpon de <tld/>o fare de la
        judoj
        <fnt>
	<aut>Stefan Maul</aut>
	<vrk><url ref="&Monato;/2000/006825.php">Estimataj gelegantoj,</url></vrk>
	<bib>Monato</bib>
	<lok>2000/02, p. 3</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_P;&c_a;&c_l;&c_je;&c_s;&c_t;&c_y;&c_n;&c_a;</trd>
    <trd lng="en">Palestine</trd>
    <trd lng="de">Pal&auml;stina</trd>
    <trd lng="nl">Palestina</trd>
    <trd lng="pl">Palestyna</trd>
    <trd lng="ro">Palestina</trd>
  </snc>
  <trd lng="ja">&#x30D1;&#x30EC;&#x30B9;&#x30C1;&#x30CA;  <pr>&#x3071;&#x308C;&#x3059;&#x3061;&#x306A;</pr></trd>
  <trd lng="uk">&#x41F;&c_a;&#x43B;&c_je;&c_s;&c_t;&c_i;&#x43D;&c_a;</trd>
</drv>

  </art>

