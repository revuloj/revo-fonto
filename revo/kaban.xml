<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kaban.xml,v 1.33 2024/07/31 09:30:44 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>kaban</rad>/o
</kap>
<drv mrk="kaban.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Malri&ccirc;a <ref tip="super" cel="dom.0eto">dometo</ref>, konstruita el
       krudaj kaj ne &ccirc;iam solidaj materialoj:
      <ekz>
        por siaj brutoj li faris <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 33:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi vidas malnovajn, mizerajn, kadukajn <tld/>ojn, kvaza&ubreve; mi trovi&gcirc;us en provinca
        urbeto
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la ne&gcirc;o ku&scirc;is ekstere <klr>[&#8230;]</klr> la <tld/>o preska&ubreve; estis ka&scirc;ita
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        la muroj de la <ind>ne&gcirc;a <tld/>o</ind> <klr>(vd <ref cel="igl.0o">iglo</ref>)</klr> falis
        <fnt>
          <bib>Fab3</bib>
          <lok>&Ccirc;e la plej ekstrema maro</lok>
        </fnt>;
        <trd lng="de">Schneeh&uuml;tte</trd>
      </ekz>
      <ekz>
        <ind>pajl<tld/>o</ind>
        <fnt>
          <bib>LSB</bib>
          <lok>Angkor'A&Ubreve;!</lok>
        </fnt>;
        <trd lng="de">Strohh&uuml;tte</trd>
      </ekz>
      <ekz>
        sur teraso de <ind>ski<tld/>o</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Jomo Ipfelkofer</aut>
          <vrk><url ref="&Monato;/2006/009020.php">Bavario funebras pro Beppo Bruno</url></vrk>
          <lok>2006</lok>
        </fnt>;
        <trd lng="de">Skih&uuml;tte</trd>
      </ekz>
      <ekz>
        du ekskursantoj eliris el la <ind>rifu&gcirc;<tld/>o</ind>
        <klr>(kp <ref cel="sxirm.0ejo">&scirc;irmejo</ref>, <ref cel="rifugx.0ejo">rifu&gcirc;ejo</ref>)</klr>
        <fnt>
          <bib>ChV</bib>
          <lok>26</lok>
        </fnt>;
        <trd lng="de">Schutzh&uuml;tte</trd>
      </ekz>
      <ekz>
        en la arbaro ili konstruas <tld/>ojn el bran&ccirc;oj
        <klr>(kp <ref cel="tend.0o">tendo</ref>, <ref cel="lauxb.0o">la&ubreve;bo</ref>)</klr>
        <fnt>
          <bib>HsT</bib>
          <lok>La nepo</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni lo&gcirc;os en bona <ind>karbigista <tld/>o</ind>
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
        <trd lng="de">K&ouml;hlerh&uuml;tte</trd>
      </ekz>
      <ekz>
        li volis tiam montri, ke liaj brakoj malgra&ubreve; la sepdek jaroj ne perdis sian
        viglecon &ndash; kun la rezulto, ke li preska&ubreve; faligis la malstabilan <ind>ban<tld/>o</ind>n
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>&Ccirc;e Lev Tolstoj en Jasnaja Poljana</lok>
        </fnt>!
        <trd lng="de">Badeh&auml;uschen</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="barak.0o">barako</ref>,
      <ref cel="bud.0o">budo</ref>,
      <ref cel="dacx.0o">da&ccirc;o</ref>,
      <ref cel="sxed.0o">&scirc;edo</ref>,
      <ref cel="stal.0o">stalo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
     <trd>&c_h;&c_a;&c_c;&c_ib;&c_n;&c_a;</trd>,
     <trd>&c_h;&c_a;&c_l;&c_u;&c_p;&c_a;</trd>,
     <trd>&c_h;&c_a;&c_t;&c_a;</trd>
   </trdgrp>
  <trdgrp lng="br">
     <trd>kabanenn</trd>,
     <trd>loch</trd>,
     <trd>lochenn</trd>,
     <trd>log</trd>,
     <trd>logenn</trd>
   </trdgrp>
  <trd lng="ca">cabana</trd>
  <trdgrp lng="cs">
    <trd>bouda</trd>,
    <trd>chalupa</trd>,
    <trd>chata</trd>,
    <trd>ch&yacute;&scaron;e</trd>,
    <trd>provizorn&iacute; obydl&iacute;</trd>
  </trdgrp>
  <trd lng="de">H&uuml;tte</trd>
  <trdgrp lng="en">
     <trd>cabin</trd>,
     <trd>hut</trd>
   </trdgrp>
  <trdgrp lng="es">
     <trd>caba&ntilde;a</trd>,
     <trd>choza</trd>
   </trdgrp>
  <trdgrp lng="fa">
     <trd>&a_A_madda;&a_l;&a_w;&a_n;&f_k;</trd>,
     <trd>&f_k;&a_l;&a_b;&a_h;</trd>
   </trdgrp>
  <trd lng="fi">maja</trd>
  <trdgrp lng="fr">
     <trd>cabane</trd>,
     <trd>cahute</trd>,
     <trd>hutte</trd>
   </trdgrp>
  <trd lng="hu">kunyh&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x5C0F;&#x5C4B;  <pr>&#12371;&#12420;</pr></trd>,
    <trd>&#25496;&#x7ACB;&#12390;&#x5C0F;&#x5C4B;  <pr>&#x307B;&#x305F;&#12390;&#12371;&#12420;</pr></trd>,
    <trd>&#12354;&#12400;&#12425;&#x5C4B;  <pr>&#12354;&#12400;&#12425;&#12420;</pr></trd>
  </trdgrp>
  <trd lng="nl">hut</trd>
  <trdgrp lng="pl">
    <trd>cha&lstroke;upa</trd>,
    <trd>chata</trd>
  </trdgrp>
  <trdgrp lng="pt">
     <trd>cabana</trd>,
     <trd>barrac&atilde;o</trd>
   </trdgrp>
   <trdgrp lng="ro">
    <trd>colib&abreve;</trd>,
    <trd>c&abreve;su&#x21B;&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_h;&c_i;&c_zh;&c_i;&c_n;&c_a;</trd>,
     <trd>&c_l;&c_a;&c_ch;&c_u;&c_g;&c_a;</trd>,
     <trd>&c_i;&c_z;&c_b;&c_u;&c_sh;&c_k;&c_a;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>chalupa</trd>,
    <trd>chata</trd>,
    <trd>chy&zcaron;a</trd>
  </trdgrp>
  <trd lng="sv">hydda</trd>
  <trd lng="tr">kul&uuml;be</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_a;&c_t;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_h;&c_a;&c_l;&c_u;&c_p;&c_a;</trd>,
    <trd>&c_h;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_b;&c_u;&c_d;&c_i;&c_n;&c_o;&c_ch;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5E90; <pr>l&#xFA;</pr></trd>,
    <trd>&#x5C0F;&#x5C4B; <pr>xi&#x1CE;ow&#x16B;</pr></trd>,
    <trd>&#x5BEE; <pr>li&#xE1;o</pr></trd>,
    <trd>&#x5BEE;&#x623F; <pr>li&#xE1;of&#xE1;ng</pr></trd>,
    <trd>&#33541;&#x820D; <pr>m&#xE1;osh&#xE8;</pr></trd>,
    <trd>&#x53AA; <pr>j&#x1D0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kaban.mont0o">
  <kap>
    mont<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>o en la montaro, uzata de pa&scirc;tistoj, migrantoj ktp:
      <ekz>
        tiuj kiuj preferas pli facilajn promenadojn vizitis pintetojn pli proksime al la
        mont<tld/>o, man&gcirc;is arbarajn berojn kaj sunumis
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj de ni enlo&gcirc;i&gcirc;is en la mont<tld/>o, pli &gcirc;uste monthotelo
        <fnt>
          <vrk><url ref="https://esperanto-maribor.si/srecanje-na-lisci/">Sre&ccaron;anje na Lisci -
          Esperantsko dru&scaron;tvo Maribor</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="alberg.0o">albergo</ref>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_r;&c_n;&c_a;&c_ja; <ind>&c_h;&c_a;&c_c;&c_ib;&c_n;&c_a;</ind></trd>
  <trd lng="de">Bergh&uuml;tte</trd>
</drv>

</art>
