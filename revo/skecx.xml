<art mrk="$Id: skecx.xml,v 1.11 2024/03/25 06:40:50 revo Exp $">
<kap>
  <ofc>9</ofc><rad>ske&ccirc;</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="skecx.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="skecx.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Mallonga, facila <ref tip="super" cel="teatr.0ajxo">teatra&jcirc;eto</ref>, ofte prezentata kiel ero
      de
      <ref tip="malprt" cel="variet.0o">varieteo</ref>:
      <ekz>
        originalajn unuakta&jcirc;ojn a&ubreve; <tld/>ojn oni
        sufi&ccirc;e ofte
        prezentas en
        Universalaj Kongresoj
        <fnt><bib>Plumamikoj</bib>, <lok>dramoj en
        esperanto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_je;&c_t;&c_ch;</trd>
  <trdgrp lng="en">
    <trd>sketch</trd>,
    <trd>skit</trd>,
    <trd>act</trd>
  </trdgrp>
  <trd lng="fr">sketch</trd>
  <trd lng="ja">&#x5BF8;&#x5287;  <pr>&#x3059;&#x3093;&#x3052;&#x304D;</pr></trd>
  <trd lng="nl">sketch</trd>
  <trd lng="pl">skecz</trd>
  <trd lng="pt">esquete</trd>
  <trd lng="ru">&c_s;&c_k;&c_je;&c_t;&c_ch;</trd>
  <trd lng="uk">&c_s;&#x43A;&c_je;&c_t;&c_ch;</trd>
  <trdgrp lng="zh">
    <trd>&#x76F8;&#x58F0; <pr>xi&#xE0;ngsheng</pr></trd>,
    <trd>&#x76F8;&#x8072; <pr>xi&#xE0;ngsheng</pr></trd>,
    <trd>&#x5C0F;&#x54C1; <pr>xi&#x1CE;op&#x1D0;n</pr></trd>
  </trdgrp>
</drv>
</art>

