<drv mrk="butik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="butik.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Strata <ref tip="super" cel="vend.0ejo">vendejo</ref>, ordinare
      tereta&gcirc;a, kie oni vendas podetale:
      <ekz>
        &ccirc;u anka&ubreve; vi jam spertis, ke via po&scirc;telefono
        a&ubreve; radio apena&ubreve; estas
        uzebla en tuneloj, <tld/>oj, gara&gcirc;oj
        <fnt>
        <aut>-</aut>,
        <vrk>Nova! Signal-fortigilo...</vrk>,
        <bib>Monato</bib>, <lok>jaro 2002a, numero 5a, p. 32a</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_m;&c_a;</trd>,
    <trd>&c_b;&c_u;&c_c;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_u;&c_t;&c_i;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>boutique</trd>,
    <trd>butik</trd>,
    <trd>kr&aacute;m</trd>,
    <trd>mal&yacute; obchod</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Boutique</trd>,
    <trd>Laden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>shop</trd>,
    <trd>store</trd>
  </trdgrp>
  <trd lng="es">tienda</trd>
  <trdgrp lng="fr">
    <trd>boutique</trd>,
    <trd>magasin<klr> (boutique)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5B4;&#x5E7;&#x5B0;&#x5E0;&#x5D5;&#x5B9;&#x5EA;</trd>
  <trd lng="hu">bolt</trd>
  <trdgrp lng="id">
    <trd>toko</trd>,
    <trd>kedai</trd>,
    <trd>warung</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5E97;  <pr>&#x305F;&#x306A;</pr></trd>,
    <trd>&#x5C0F;&#x58F2;&#x5E97;  <pr>&#12371;&#12358;&#x308A;&#12390;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>winkel</trd>,
    <trd>boetiek</trd>,
    <trd>zaak</trd>,
    <trd>shop</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sklep</trd>,
    <trd>butik</trd>,
    <trd>kram</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>loja</trd>,
    <trd>butique</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kr&aacute;m</trd>,
    <trd>obchod</trd>
  </trdgrp>
  <trd lng="tr">d&uuml;kk&acirc;n</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i;&#x43A;</trd>,
    <trd>&#x43A;&c_r;&c_a;&#x43C;&#x43D;&c_i;&c_c;&#x44F;</trd>,
    <trd>&#x43D;&c_je;&c_v;&c_je;&#x43B;&c_i;&#x43A;&c_i;&c_j; &#x43C;&c_a;&c_g;&c_a;&c_z;&c_i;&#x43D;</trd>
  </trdgrp>
  <trd lng="zh">&#x5E97;<klr> [di&agrave;n]</klr></trd>
</drv>

<drv mrk="butik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="butik.0isto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="komerc.0isto">Komercisto</ref>
      kiu funkciigas <tld/>on.
    </dif>
    <trdgrp lng="zh">
    <trd>&#22899;&#x552E;&#x8CA8;&#x54E1; <pr>n&#x1DA;sh&#xF2;uhu&#xF2;yu&#xE1;n</pr></trd>,
    <trd>&#x51FA;&#x552E;&#x8CA8;&#29289;&#30340;&#x4EBA;
    <pr>ch&#x16B;sh&#xF2;uhu&#xF2;w&#xF9;der&#xE9;n</pr></trd>,
    <trd>&#x5E97;&#x4F19; <pr>di&#xE0;nhu&#x1D2;</pr></trd>,
    <trd>&#21334;&#x4E3B; <pr>m&#xE0;izh&#x1D4;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_k;&c_r;&c_a;&c_m;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>drobn&yacute; obchodn&iacute;k</trd>,
    <trd>hokyn&aacute;&rcaron;</trd>,
    <trd>kram&aacute;&rcaron;</trd>,
    <trd>kupec</trd>
  </trdgrp>
  <trd lng="de">Verk&auml;ufer<klr tip="amb"> (in einem
  Laden)</klr></trd>
  <trdgrp lng="en">
    <trd>shopkeeper</trd>,
    <trd>merchant</trd>,
    <trd>retailer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commer&ccedil;ant<klr> (tenancier)</klr></trd>,
    <trd>boutiquier</trd>,
    <trd>g&eacute;rant<klr> (de magasin)</klr></trd>,
    <trd>tenancier<klr> (de magasin)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5D5;&#x5DB;&#x5E8; &#x5D1;&#x5D7;&#x5E0;&#x5D5;&#x5EA;</trd>
  <trd lng="hu">boltos</trd>
  <trdgrp lng="id">
    <trd><baz>jaga</baz>penjaga toko</trd>,
    <trd><baz>jual</baz>penjual</trd>,
    <trd><baz>dagang</baz>pedagang</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5C0F;&#x58F2;&#x5E97;&#x4E3B;  <pr>&#12371;&#12358;&#x308A;&#12390;&#12435;&#12375;&#12421;</pr></trd>,
    <trd>&#x5E97;&#38263;  <pr>&#12390;&#12435;&#12385;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">winkelier</trd>
  <trdgrp lng="pl">
    <trd>sklepowa<klr> {f}</klr></trd>,
    <trd>sprzedawca</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_l;&c_a;&c_d;&c_je;&c_l;&c_je;&c_c;
    <ind>&c_b;&c_u;&c_t;&c_i;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>drobn&yacute; obchodn&iacute;k <klr>(ziskuchtivec)</klr></trd>,
    <trd>kram&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_r;&c_a;&#x43C;&c_a;&c_r;</trd>,
    <trd>&c_d;&c_r;&c_ib;&c_b;&#x43D;&c_i;&c_j; &c_t;&#x43E;&c_r;&c_g;&#x43E;&c_v;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="butik.0umi">
  <kap><tld/>umi</kap>
  <snc>
    <dif>
      Viziti <tld/>ojn por fari diversajn a&ccirc;etojn:
      <ekz>
        mia edzino <tld/>umis en supervendejo
        <fnt>
        <aut>G. MacAoidh</aut>,
        <vrk>Ni hontas kaj postulas</vrk>,
        <bib>Monato</bib>, <lok>jaro 2000a, numero 11a, p. 7a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_h;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol; &c_p;&c_a; <ind>&c_k;&c_r;&c_a;&c_m;&c_a;&c_h;</ind></trd>,
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_c;&c_a; <ind>&c_sh;&c_o;&c_p;&c_ib;&c_n;&c_g;&c_a;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="cs">b&ecaron;hat po obchodech</trd>
  <trdgrp lng="de">
    <trd>einkaufen gehen</trd>,
    <trd>shoppen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>shop</trd>,
    <trd>go shopping</trd>
  </trdgrp>
  <trd lng="es">ir de <ind>compras</ind></trd>
  <trdgrp lng="fr">
    <trd>faire les <ind>magasins</ind></trd>,
    <trd>faire les <ind>courses</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5E2;&#x5E9;&#x5D5;&#x5EA; &#x5E7;&#x5E0;&#x5D9;&#x5D5;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>j&aacute;rja az <ind>&uuml;zleteket</ind></trd>,
    <trd>v&aacute;s&aacute;rolgat</trd>
  </trdgrp>
  <trd lng="id"><baz>belanja</baz>berbelanja</trd>
  <trdgrp lng="ja">
    <trd>&#x8CB7;&#12356;&#29289;&#12434;&#12377;&#x308B;  <pr>&#x304B;&#12356;&#12418;&#x306E;&#12434;&#12377;&#x308B;</pr></trd>,
    <trd>&#x30B7;&#x30E7;&#x30C3;&#x30D4;&#x30F3;&#x30B0;&#12377;&#x308B;  <pr>&#12375;&#12423;&#12387;&#12404;&#12435;&#12368;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>winkelen</trd>,
    <trd>boodschappen doen</trd>,
    <trd>inkopen doen</trd>,
    <trd>shoppen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>robi&cacute; <ind>zakupy</ind></trd>,
    <trd>i&sacute;&cacute; na <ind>zakupy</ind></trd>,
    <trd>chodzi&cacute; po sklepach</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
    <ind>&c_p;&c_o;&c_k;&c_u;&c_p;&c_k;&c_i;</ind></trd>,
    <trd>&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_p;&c_o;
    &c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;&c_a;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja;
    <ind>&c_sh;&c_o;&c_p;&c_p;&c_i;&c_n;&c_g;&c_o;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="sk">beha&tcaron; po obchodoch</trd>
  <trd lng="sv">butik</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&#x43E;&c_d;&c_i;&c_t;&c_i; &#x43F;&#x43E; &#x43A;&c_r;&c_a;&#x43C;&#x43D;&c_i;&c_c;&#x44F;&c_h;</trd>,
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43F;&#x43E;&#x43A;&c_u;&#x43F;&#x43A;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="butik.ret0o">
  <kap>ret<tld/>o</kap>
  <snc mrk="butik.ret0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref tip="super" cel="ret.0ejo.KOMP">Retejo</ref>,
      en kiu uzanto povas elekti, mendi, plejofte anta&ubreve;pagi
      a&ccirc;eta&jcirc;ojn:
      <ekz>
        la Ret<tld/>o de FEL estis kreita la 9-an de februaro 1996
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Flandra_Esperanto-Ligo">
          Flandra Esperanto-Ligo</url>,
          2009-12-24</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_r;&c_n;&c_e;&c_t;-&c_k;&c_r;&c_a;&c_m;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>boutique en ligne</trd>,
    <trd>e-boutique</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><ind>toko</ind> daring</trd>,
    <trd><ind>toko</ind> online</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30A4;&#x30F3;&#x30BF;&#x30FC;&#x30CD;&#x30C3;&#x30C8;&#x30B7;&#x30E7;&#x30C3;&#x30D7;</trd>,
    <trd>&#x30CD;&#x30C3;&#x30C8;&#x30B7;&#x30E7;&#x30C3;&#x30D7;</trd>
  </trdgrp>
  <trd lng="nl">webwinkel</trd>
  <trd lng="pl">sklep internetowy</trd>
  <trdgrp lng="zh">
    <trd>&#x8CFC;&#29289;&#x7DB2;&#x7AD9; <pr>g&#xF2;uw&#xF9;w&#x1CE;ngzh&#xE0;n</pr></trd>,
    <trd>&#x7DB2;&#x5E97; <pr>w&#x1CE;ngdi&#xE0;n</pr></trd>
  </trdgrp>
</drv>
