<drv mrk="butik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="butik.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Strata <ref tip="super" cel="vend.0ejo">vendejo</ref>, ordinare
      tereta&gcirc;a, kie oni vendas podetale:
      <ekz>
        &ccirc;u anka&ubreve; vi jam spertis, ke via po&scirc;telefono
        a&ubreve; radio apena&ubreve; estas
        uzebla en tuneloj, <tld/>oj, gara&gcirc;oj
        <fnt>
        <aut>-</aut>,
        <vrk>Nova! Signal-fortigilo...</vrk>,
        <bib>Monato</bib>, <lok>jaro 2002a, numero 5a, p. 32a</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_m;&c_a;</trd>,
    <trd>&c_b;&c_u;&c_c;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_u;&c_t;&c_i;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>boutique</trd>,
    <trd>butik</trd>,
    <trd>kr&aacute;m</trd>,
    <trd>mal&yacute; obchod</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Boutique</trd>,
    <trd>Laden</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>shop</trd>,
    <trd>store</trd>
  </trdgrp>
  <trd lng="es">tienda</trd>
  <trdgrp lng="fr">
    <trd>boutique</trd>,
    <trd>magasin<klr> (boutique)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5B4;&#x5E7;&#x5B0;&#x5E0;&#x5D5;&#x5B9;&#x5EA;</trd>
  <trd lng="hu">bolt</trd>
  <trd lng="id">toko</trd>
  <trdgrp lng="ja">
    <trd>&#x5E97;  <pr>&#x305F;&#x306A;</pr></trd>,
    <trd>&#x5C0F;&#x58F2;&#x5E97;  <pr>&#x3053;&#x3046;&#x308A;&#x3066;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>winkel</trd>,
    <trd>boetiek</trd>,
    <trd>zaak</trd>,
    <trd>shop</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sklep</trd>,
    <trd>butik</trd>,
    <trd>kram</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>loja</trd>,
    <trd>butique</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kr&aacute;m</trd>,
    <trd>obchod</trd>
  </trdgrp>
  <trd lng="tr">d&uuml;kk&acirc;n</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x443;&#x442;&#x438;&#x43A;</trd>,
    <trd>&#x43A;&#x440;&#x430;&#x43C;&#x43D;&#x438;&#x446;&#x44F;</trd>,
    <trd>&#x43D;&#x435;&#x432;&#x435;&#x43B;&#x438;&#x43A;&#x438;&#x439; &#x43C;&#x430;&#x433;&#x430;&#x437;&#x438;&#x43D;</trd>
  </trdgrp>
  <trd lng="zh">&#x5E97;<klr> [di&agrave;n]</klr></trd>
</drv>

