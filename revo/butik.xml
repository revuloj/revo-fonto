<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>butik</rad>/o
</kap>
<drv mrk="butik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Strata <ref tip="super" cel="vend.0ejo">vendejo</ref>, ordinare
      tereta&gcirc;a, kie oni vendas podetale:
      <ekz>
        &ccirc;u anka&ubreve; vi jam spertis, ke via po&scirc;telefono a&ubreve; radio apena&ubreve; estas
        uzebla en tuneloj, <tld/>oj, gara&gcirc;oj
        <fnt>
        <aut>-</aut>,
        <vrk>Nova! Signal-fortigilo de Big Planet</vrk>,
        <bib>Monato</bib>, <lok>jaro 2002a, numero 5a, p. 32a</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_m;&c_a;</trd>,
    <trd>&c_b;&c_u;&c_c;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Laden</trd>
  <trdgrp lng="en">
    <trd>shop</trd>,
    <trd>store</trd>
  </trdgrp>
  <trd lng="es">tienda</trd>
  <trdgrp lng="fr">
    <trd>boutique</trd>,
    <trd>magasin<klr> (boutique)</klr></trd>
  </trdgrp>
  <trd lng="hu">bolt</trd>
  <trd lng="nl">winkel</trd>
  <trd lng="pl">sklep</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="tr">d&uuml;kk&acirc;n</trd>
</drv>

<drv mrk="butik.0isto">
  <kap><tld/>isto</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="komerc.0isto">Komercisto</ref>
      kiu funkciigas butikon.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>shopkeeper</trd>,
    <trd>merchant</trd>,
    <trd>retailer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>commer&ccedil;ant<klr> (tenancier)</klr></trd>,
    <trd>boutiquier</trd>,
    <trd>g&eacute;rant<klr> (de magasin)</klr></trd>,
    <trd>tenancier<klr> (de magasin)</klr></trd>
  </trdgrp>
  <trd lng="hu">boltos</trd>
  <trd lng="nl">winkelier</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_v;&c_o;&c_ch;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_l;&c_a;&c_d;&c_je;&c_l;&c_je;&c_c; <ind>&c_b;&c_u;&c_t;&c_i;&c_k;&c_a;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="butik.0umi">
  <kap><tld/>umi</kap>
  <snc>
    <dif>
      Viziti <tld/>ojn por fari diversajn a&ccirc;etojn:
      <ekz>
        mia edzino <tld/>umis en supervendejo
        <fnt>
        <aut>Garbhan MacAoidh</aut>,
        <vrk>Ni hontas kaj postulas</vrk>,
        <bib>Monato</bib>, <lok>jaro 2000a, numero 11a, p. 7a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>einkaufen gehen</trd>,
    <trd>shoppen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>shop</trd>,
    <trd>go shopping</trd>
  </trdgrp>
  <trd lng="es">ir de <ind>compras</ind></trd>
  <trdgrp lng="fr">
    <trd>faire les <ind>magasins</ind></trd>,
    <trd>faire les <ind>courses</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>j&aacute;rja az <ind>&uuml;zleteket</ind></trd>,
    <trd>v&aacute;s&aacute;rolgat</trd>
  </trdgrp>
  <trd lng="nl">winkelen</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_p;&c_o;&c_k;&c_u;&c_p;&c_k;&c_i;</ind></trd>,
    <trd>&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_p;&c_o; &c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;&c_a;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja; <ind>&c_sh;&c_o;&c_p;&c_p;&c_i;&c_n;&c_g;&c_o;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="sv">butik</trd>
</drv>
</art>
