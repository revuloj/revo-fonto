<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>butik</rad>/o
</kap>
<drv mrk="butik.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Tereta&gcirc;a <ref tip="super" cel="komerc.0ejo">komercejo</ref>, senpere alirebla el la strato, kie oni vendas podetale.
    </dif>
    <refgrp tip="vid">
      <ref cel="bazar.0o">bazaro</ref>,
      <ref cel="bud.0o">budo</ref>,
      <ref cel="magaze.0o">magazeno</ref>,
      <ref cel="vend.0ejo">vendejo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Laden</trd>
  <trd lng="en">shop</trd>
  <trd lng="fr">boutique</trd>
  <trd lng="nl">winkel</trd>
  <trd lng="pl">sklep</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_a;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_a;&c_z;&c_i;&c_n;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="tr">d&uuml;kk&acirc;n</trd>
</drv>

<drv mrk="butik.0umi">
  <kap>
    <tld/>umi
  </kap>
  <snc>
    <dif>
      Viziti la butikojn por fari diversajn a&ccirc;etojn.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>einkaufen gehen</trd>,
    <trd>shoppen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire les <ind>magasins</ind></trd>,
    <trd>faire les <ind>courses</ind></trd>
  </trdgrp>
  <trd lng="sv">butik</trd>
</drv>
</art>
