<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>skal</rad>/o
</kap>
<drv mrk="skal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Linio dividita per egaldistancaj streketoj, kiuj montras la
      dimensiojn de desegna&jcirc;o rilate al efektiva objekto.
    </dif>
    <trd lng="de">Skala</trd>
    <trd lng="ru">&c_m;&c_a;&c_s;&c_sh;&c_t;&c_a;&c_b;</trd>
  </snc>
  <snc>
    <dif>
      Aro de la streketoj, kiuj montras la gradojn de termometro a&ubreve; de
      alia ajn mezuraparato.
    </dif>
    <trd lng="de">Skala</trd>
    <trd lng="ru">&c_sh;&c_k;&c_a;&c_l;&c_a;</trd>
  </snc>
  <snc>
    <dif>
      Rilato inter la efektivaj dimensioj de objekto kaj la dimensioj de
      desegna&jcirc;o, kiu figuras &gcirc;in.
    </dif>
    <trd lng="de">Ma&szlig;stab</trd>
    <trd lng="ru">&c_m;&c_a;&c_s;&c_sh;&c_t;&c_a;&c_b;</trd>
  </snc>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzika amplekso de iu vo&ccirc;o.
    </dif>
    <trd lng="de">Tonumfang</trd>
    <trd lng="ru">&c_d;&c_i;&c_a;&c_p;&c_a;&c_z;&c_o;&c_n; <klr tip="amb">(&c_m;&c_u;&c_z;.)</klr></trd>
  </snc>
  <trd lng="nl">schaal</trd>
</drv>
</art>
