<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gek</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="gek.0o">
  <kap><tld/>o</kap>
  <snc mrk="gek.0o.familiano">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="geked.0oj"><tld/>edoj</ref>.
    </dif>
    <trdgrp lng="ca">
      <trd>drag&oacute;</trd>,
      <trd>tocai</trd>
    </trdgrp>
    <trd lng="de">Gecko</trd>
    <trd lng="nl">gekko</trd>
  </snc>
  <snc mrk="gek.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="geked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Gekko</trd>).
    </dif>
    <trd lng="ca">g&egrave;nere Gekko</trd>
  <trd lng="de">Tockee</trd>
  <trdgrp lng="nl">
    <trd>gekko</trd>,
    <trd>tokeh</trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_k;&c_o;&c_n;</trd>
  <trd lng="es">geco</trd>
  <trd lng="fr">gecko</trd>
  <trd lng="hu">gekk&oacute;</trd>
  <trdgrp lng="pt">
    <trd>lagartixa</trd>,
    <trd>geco</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_k;&c_k;&c_o;&c_n;</trd>
</drv>
</art>
