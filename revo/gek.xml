<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gek</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="gek.0o">
  <kap><tld/>o</kap>
  <snc mrk="gek.0o.familiano">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="gek.0edoj"><tld/>edoj</ref>.
    </dif>
    <trd lng="de">Gecko</trd>
  </snc>
  <snc mrk="gek.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="gek.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Gekko</trd>).
    </dif>
  </snc>
  <trd lng="de">Tockee</trd>
  <trd lng="ru">&c_g;&c_je;&c_k;&c_k;&c_o;&c_n;</trd>
</drv>
<drv mrk="gek.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="skvam.0uloj">skvamuloj</ref>
      (<trd lng="la">Gekkonidae</trd>), el kiu pluraj specioj povas rampi
      sur vertikalaj surfacoj per elektrostataj teniloj de la fingroj. 
    </dif>
  </snc>
  <trd lng="de">Geckos <klr tip="ind">(Familie)</klr></trd>
  <trd lng="en">geckos</trd>
</drv>
</art>
