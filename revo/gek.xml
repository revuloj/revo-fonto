<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gek.xml,v 1.26 2023/11/11 10:00:39 revo Exp $">
<kap>
  <rad>gek</rad>/o 
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gek.0o">
  <kap><tld/>o</kap>
  <snc mrk="gek.0o.familiano">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Iu el <ref tip="malprt" cel="geked.0oj"><tld/>edoj</ref>:
      <ekz>
        rampas sur la tero <klr>[&#x2026;]</klr> la <tld/>o kaj la monitoro kaj la uromastiko
        kaj la skinko kaj la &hcirc;ameleono
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 11:30</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>drag&oacute;</trd>,
      <trd>tocai</trd>
    </trdgrp>
    <trd lng="de">Gecko</trd>
    <trd lng="nl">gekko</trd>
    <trdgrp lng="zh">
    <trd>&#x6A90;&#x86C7; <pr>y&#xE1;nsh&#xE9;</pr></trd>,
    <trd>&#x86C7;&#x533B; <pr>sh&#xE9;y&#x12B;</pr></trd>,
    <trd>&#x5B88;&#x5BAB; <pr>sh&#x1D2;ug&#x14D;ng</pr></trd>,
    <trd>&#x56DB;&#x811A;&#x86C7; <pr>s&#xEC;ji&#x1CE;osh&#xE9;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="gek.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="reptil.0o" lst="voko:reptilioj">Reptilio</ref>
      kun vertikalaj pupiloj, vivanta sur arboj en Azio;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> 
      el familio <ref tip="malprt" cel="geked.0oj"><tld/>edoj</ref>
      (<trd lng="la">Gekko</trd>):
      <ekz>
        <klr>[li]</klr> lerte malsupren grimpis,
        kvaza&ubreve; <tld/>o sur kalkita muro
        <fnt>
          <aut>Mo Yan, trad. Vejdo</aut>
          <vrk>Travidebla Ru&gcirc;a Rafano</vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/19/Male_Gekko_gecko_(KU_330057)_in_the_outskirts_of_Barangay_Magrafil_-_ZooKeys-266-001-g047.jpg/320px-Male_Gekko_gecko_(KU_330057)_in_the_outskirts_of_Barangay_Magrafil_-_ZooKeys-266-001-g047.jpg" prm="CC BY 3.0"><tld/>o (Gekko gecko) Magrafil, Filipinoj
        <fnt>
          <aut>Brown R, k.a. (2013). &leftquot;The amphibians and reptiles of Luzon Island, Philippines, VIII&rightquot;</aut>
          <lok><url ref="&WCW;/File:Male_Gekko_gecko_(KU_330057)_in_the_outskirts_of_Barangay_Magrafil_-_ZooKeys-266-001-g047.jpg">&WCW;/File:Male_Gekko_gecko_(KU_330057)_in_the_outskirts_of_Barangay_Magrafil_-_ZooKeys-266-001-g047.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="ca">g&egrave;nere Gekko</trd>
    <trd lng="de">Tockee</trd>
    <trdgrp lng="nl">
      <trd>gekko</trd>,
      <trd>tokeh</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_k;&c_o;&c_n;</trd>
  <trd lng="bg">&c_g;&c_je;&c_k;&c_o;&c_n;</trd>
  <trd lng="es">geco</trd>
  <trd lng="fr">gecko</trd>
  <trd lng="hu">gekk&oacute;</trd>
  <trd lng="ja">&#x30E4;&#x30E2;&#x30EA;</trd>
  <trdgrp lng="pt">
    <trd>lagartixa</trd>,
    <trd>geco</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_je;&c_k;&c_k;&c_o;&c_n;</trd>
  <trd lng="uk">&c_g;&c_je;&c_k;&c_o;&c_n;</trd>
</drv>
</art>
