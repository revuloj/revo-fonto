<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>fotogravur</rad>/i</kap>
<drv mrk="fotogr2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Produkti per fotografa procedo gravuritajn kli&scirc;ojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="fotogl.0o">fotogliptiko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="hu">
    <trd>fotograv&uacute;r&aacute;t k&eacute;sz&iacute;t</trd>,
    <trd>heliograv&uacute;r&aacute;t k&eacute;sz&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fotogravar</trd>,
    <trd>fazer <ind>fotogravura</ind></trd>
  </trdgrp>
</drv>

<drv mrk="fotogr2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Ilustra&jcirc;o produktita per <tld/>ado.
    </dif>
  </snc>
  <trd lng="fr">photogravure</trd>
  <trd lng="nl">fotogravure</trd>
  <trdgrp lng="hu">
    <trd>fotograv&uacute;ra</trd>,
    <trd>heliograv&uacute;ra</trd>
  </trdgrp>
  <trd lng="pt">fotogravura</trd>
  <trd lng="ru">&c_f;&c_o;&c_t;&c_o;&c_f;&c_i;&c_l;&c_mol;&c_m;&c_o;&c_p;&c_je;&c_ch;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="fotogr2.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Persono, kies profesio estas <tld/>i.
    </dif>
  </snc>
  <trd lng="fr">photograveur</trd>
  <trdgrp lng="hu">
    <trd>fotograv&uacute;rista</trd>,
    <trd>heliograv&uacute;rista</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>chemigraaf</trd>,
    <trd>clich&eacute;maker</trd>
  </trdgrp>
  <trd lng="pt">fotogravador</trd>
</drv>
</art>
