<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>fotogravur</rad>/i
</kap>
<drv mrk="fotogr2.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Produkti per fotografa procedo gravuritajn kli&scirc;ojn.
    </dif>
    <refgrp tip="vid">
      <ref cel="fotogl.0o">fotogliptiko</ref>.
    </refgrp>
  </snc>
</drv>
<drv mrk="fotogr2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Ilustra&jcirc;o produktita per <tld/>ado.
    </dif>
  </snc>
  <trd lng="fr">photogravure</trd>
</drv>
<drv mrk="fotogr2.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Persono, kies profesio estas <tld/>i.
    </dif>
  </snc>
  <trd lng="fr">photograveur</trd>
</drv>
</art>

