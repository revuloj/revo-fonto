<drv mrk="sxuld.0igi">
  <kap><tld/>igi,
    <var><kap>en<tld/>igi</kap></var>
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari al iu servon, kiu morale devigas lin al <tld/>o a&ubreve; dankemo:
      <ekz>
        tion farante, vi min <tld/>igas senfine
        <fnt><aut>Millidge</aut></fnt>;
      </ekz>
      <ekz>
        mi estas <klr>[&#x2026;]</klr> terure en<tld/>igita
        <fnt>
          <bib>BdV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_z;&c_a;&c_d;&c_malmol;&c_l;&c_zh;&c_a;&c_v;&c_a;&c_m;</trd>
  <trdgrp lng="de">
    <trd>verschulden</trd>,
    <trd>verpflichten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>to obligate</trd>,
    <trd>oblige</trd>
  </trdgrp>
  <trd lng="es">estar en deuda</trd>
  <trd lng="fr">obliger<klr> (qqun, lui cr&eacute;er une
  dette)</klr></trd>
  <trd lng="hu">lek&ouml;telez</trd>
  <trd lng="it">vincolare</trd>
  <trdgrp lng="ja">
    <trd>&#x50B5;&#x52D9;&#x3092;&#x8CA0;&#x308F;&#x305B;&#x308B; 
    <pr>&#x3055;&#x3044;&#x3080;&#x3092;&#x304A;&#x308F;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x7FA9;&#x52D9;&#x3092;&#x8CA0;&#x308F;&#x305B;&#x308B; 
    <pr>&#x304E;&#x3080;&#x3092;&#x304A;&#x308F;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x8CB8;&#x3057;&#x3092;&#x3064;&#x304F;&#x308B;  <pr>&#x304B;&#x3057;&#x3092;&#x3064;&#x304F;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">aan zich <ind>verplichten</ind></trd>
  <trd lng="pl">czyni&cacute; <ind>d&lstroke;u&zdot;nikiem</ind></trd>
  <trd lng="ru">&c_o;&c_b;&c_ja;&c_z;&c_a;&c_t;&c_mol;<klr>
