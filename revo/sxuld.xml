<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;uld</rad>/i
</kap>
<drv mrk="sxuld.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Devi la&ubreve;le&gcirc;e pagi iun sumon:
      <ekz>
        vi <tld/>as al mi mil frankojn;
      </ekz>
      <ekz>
        ri&ccirc;a estas tiu, kiu <tld/>as al neniu
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">tartozik</trd>
  </snc>
  <snc>
    <dif>
      Havi moralan devon esti danka al iu pro ia servo; danki 4:
      <ekz>
        mi <tld/>as al vi grandan dankon;
      </ekz>
      <ekz>
        mi <tld/>as al tiu artikolo mian konverton<fnt>B</fnt>;
      </ekz>
      <ekz>
        al lia senlaca laborado ni <tld/>as la tutan kongreson kaj la
        &gcirc;ojojn, kiujn &gcirc;i alportis al ni<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la lando <tld/>as sian prosperon al la terkulturo.
      </ekz>
    </dif>
    <trd lng="hu">k&ouml;sz&ouml;nhet<klr tip="amb"> (vkinek vmit)</klr></trd>
  </snc>
  <snc>
    <dif>
      Devi fari ion pro morala devo:
      <ekz>
        ni <tld/>as al la anoj klarigojn pri nia sinteno.
      </ekz>
    </dif>
    <trd lng="hu">tartozik<klr tip="amb"> (pl. h&aacute;l&aacute;val)</klr></trd>
  </snc>
  <trd lng="de">schulden</trd>
  <trd lng="el">&omikron;&phi;&epsilon;&jota_ton;&lambda;&omega;</trd>
  <trd lng="en">owe</trd>
  <trd lng="es">deber</trd>
  <trd lng="nl"><klr>(iets)</klr> <ind>schuldig</ind> zijn</trd>
  <trd lng="pt">dever</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_y;&c_t;&c_mol; &c_d;&c_o;&c_l;&c_zh;&c_n;&c_y;&c_m; <klr tip="amb">(&c_k;&c_o;&c_m;&c_u;-&c_l;. &c_d;&c_je;&c_n;&c_mol;&c_g;&c_i;)</klr></trd>,
    <trd>&c_z;&c_a;&c_d;&c_o;&c_l;&c_zh;&c_a;&c_t;&c_mol; <klr>(&c_k;&c_o;&c_m;&c_u;-&c_l;.)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="sxuld.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <dif>
    Tio, kion oni <tld/>as:
  </dif>
  <snc>
    <ekz>
      Droni en la <tld/>oj<fnt>B</fnt>;
    </ekz>
    <ekz>
      sidi en <tld/>oj &gcirc;is super la &scirc;ultroj<fnt>Z</fnt>.
    </ekz>
    <trd lng="de">Schuld</trd>
    <trdgrp lng="hu">
      <trd>ad&oacute;ss&aacute;g</trd>,
      <trd>tartoz&aacute;s</trd>
    </trdgrp>
  </snc>
  <snc>
    <ekz>
      Tio estas por mi sankta dank<tld/>o.
    </ekz>
    <trd lng="hu">tartoz&aacute;s<klr tip="ind"> (erk&ouml;lcsi)</klr></trd>
  </snc>
  <snc>
    <ekz>
      Al vi obei estas mia <tld/>o<fnt>Z</fnt> <klr>(devo)</klr>;
    </ekz>
    <ekz>
      la filo, la&ubreve; la <tld/>o de infano, funebri devas
      kelkan tempon<fnt>Z</fnt>;
    </ekz>
    <ekz>
      amo kaj <tld/>o ordonas &gcirc;in al ni<fnt>Z</fnt>;
    </ekz>
    <ekz>
      miaj <tld/>oj de mastrino min vokas<fnt>K</fnt>.
    </ekz>
    <trd lng="hu">k&ouml;telezetts&eacute;g</trd>
  </snc>
  <trd lng="de">Schuld</trd>
  <trd lng="el">&chi;&rho;&epsilon_ton;&omikron;&sigma_fina;</trd>
  <trd lng="es">deuda</trd>
  <trd lng="nl">schuld</trd>
  <trd lng="pt">d&iacute;vida</trd>
  <trd lng="ru">&c_d;&c_o;&c_l;&c_g; <klr tip="amb">(&c_d;&c_je;&c_n;&c_je;&c_zh;&c_n;&c_y;&c_j;)</klr></trd>
</drv>

<drv mrk="sxuld.0anto">
  <kap><tld/>anto</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Tiu, kiu <tld/>as iun sumon al aliulo:
      <ekz>
        pago de <tld/>anto estas bona en &ccirc;iu kvanto
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tuf.0o">tufo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Schuldner</trd>
  <trd lng="el">&omikron;&phi;&epsilon;&jota;&lambda;&epsilon_ton;&tau;&eta;&sigma_fina;</trd>
  <trd lng="hu">ad&oacute;s</trd>
  <trd lng="nl">schuldenaar</trd>
  <trd lng="pt">devedor</trd>
  <trd lng="ru">&c_d;&c_o;&c_l;&c_zh;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="sxuld.0ato">
  <kap><tld/>ato</kap>
  <snc>
    <dif>
      Tiu, al kiu aliulo <tld/>as iun sumon; kreditoro.
    </dif>
  </snc>
  <trd lng="el">&delta;&alfa;&ny;&epsilon;&jota;&sigma;&tau;&eta_ton;&sigma_fina;</trd>
  <trd lng="hu">hitelez&odblac;</trd>
  <trd lng="nl">schuldeiser</trd>
  <trd lng="pt">credor</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_d;&c_i;&c_t;&c_o;&c_r;</trd>
</drv>

<drv mrk="sxuld.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari al iu servon, kiu morale devigas lin al dankemo:
      <ekz>
        tion farante, vi min <tld/>igas senfine
      </ekz>
      <sup>D</sup>.
    </dif>
  </snc>
  <trd lng="hu">lek&ouml;telez</trd>
  <trd lng="nl">aan zich <ind>verplichten</ind></trd>
</drv>

<drv mrk="sxuld.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Sin devigi; akcepti a&ubreve; ricevi ian devon:
      <ekz>
        tiele vi plenumos la <tld/>i&gcirc;on de via &jcirc;uro<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">lek&ouml;telezi mag&aacute;t</trd>
  <trd lng="nl">zich <ind>verplichten</ind> tot</trd>
</drv>

<drv mrk="sxuld.sen0igimal0igi">
  <kap>sen<tld/>igi, mal<tld/>igi</kap>
  <snc>
    <dif>
      Liberigi iun je tio, kion li <tld/>as:
      <ekz>
        mal<tld/>igi al iu lian <tld/>on<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="hu">elenged<klr> (ad&oacute;ss&aacute;got)</klr></trd>
  <trd lng="nl">een schuld <ind>kwijtschelden</ind></trd>
  <trd lng="de">entschulden</trd>
  <trd lng="pt">perdoar<klr> (uma d&iacute;vida)</klr></trd>
  <trd lng="ru">&c_o;&c_t;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol; &c_d;&c_o;&c_l;&c_g;</trd>
</drv>

<drv mrk="sxuld.0ateco">
  <kap><tld/>ateco<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Stato de tiu, al kiu aliulo <tld/>as dankemon, konsiderata rilate al
      la <tld/>anto.
    </dif>
  </snc>
  <trd lng="nl">schuld</trd>
</drv>
</art>
