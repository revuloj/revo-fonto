  <trd lng="hu">elenged<klr> (ad&oacute;ss&aacute;got)</klr></trd>
  <trd lng="it">svincolare</trd>
  <trdgrp lng="ja">
    <trd>&#x5E33;&#x6D88;&#x3057;&#x306B;&#x3059;&#x308B;  <pr>&#x3061;&#x3087;&#x3046;&#x3051;&#x3057;&#x306B;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x50B5;&#x52D9;&#x3092;&#x514D;&#x305A;&#x308B;  <pr>&#x3055;&#x3044;&#x3080;&#x3092;&#x3081;&#x3093;&#x305A;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">een schuld <ind>kwijtschelden</ind></trd>
  <trdgrp lng="pl">
