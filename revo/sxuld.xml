<drv mrk="sxuld.sen0igi">
  <kap>sen<tld/>igi, <var><kap>mal<tld/>igi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="liber.0igi">Liberigi</ref> iun je tio, kion li <tld/>as:
      <ekz>
        mal<tld/>igi al iu lian <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 5:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        lian landon <klr>[&#x2026;]</klr> la G8-pinto sen<tld/>igis
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2006/008892.php">Diktatoroj dorlotataj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sen<tld/>iga programo
       <fnt>
       <aut>Ji&rcaron;&iacute; Patera</aut>
          <vrk><url ref="&Monato;/2001/007258.html">Post la kunsido de financistoj en
          Prago</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_o;&c_p;&c_r;&c_o;&c_shch;&c_a;&c_v;&c_a;&c_m; (&c_d;&c_malmol;&c_l;&c_g;)</trd>
