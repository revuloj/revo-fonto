<drv mrk="sxuld.0igxi">
  <kap><tld/>i&gcirc;i,
    <var><kap>en<tld/>i&gcirc;i</kap></var>
  </kap>
  <snc>
    <dif>
      Sin devigi; akcepti a&ubreve; ricevi ian devon:
      <ekz>
        la nuna mizera stato en multaj afrikaj landoj grandparte <tld/>i&gcirc;as al mismastrumado fare de
        la afrikanoj mem
        <fnt>
          <bib>Monato</bib>
          <aut>Brian Moon</aut>
          <vrk><url ref="&Monato;/2003/007984.html">Manipulado duobla</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en<tld/>i&gcirc;intaj landoj
        <fnt>
       <aut>Ji&rcaron;&iacute; Patera</aut>
          <vrk><url ref="&Monato;/2001/007258.html">Post la kunsido de financistoj en
          Prago</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la en<tld/>i&gcirc;o de multaj &scirc;tatoj <klr>[&#x2026;]</klr> fari&gcirc;is pro sinri&ccirc;i&gcirc;o de egoismaj
        politikistoj
        <fnt>
          <aut>Werner Schad</aut>
            <vrk><url ref="&Monato;/2001/007311.html">Putrantaj demokratioj</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 7</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
    <trd lng="bg">&c_z;&c_a;&c_d;&c_malmol;&c_l;&c_zh;&c_a;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  <trdgrp lng="de">
    <trd>sich <ind>verschulden</ind></trd>,
    <trd><ind>geschuldet</ind> sein</trd>
  </trdgrp>
  <trd lng="en">to become indebted</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>endetter</ind></trd>,
    <trd>s'<ind>obliger</ind><klr> (se cr&eacute;er une
    obligation)</klr></trd>
  </trdgrp>
  <trd lng="hu">lek&ouml;telezi mag&aacute;t</trd>
  <trdgrp lng="it">
      <trd>vincolarsi</trd>,
      <trd>indebitarsi</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7FA9;&#x52D9;&#x3092;&#x8CA0;&#x3046;  <pr>&#x304E;&#x3080;&#x3092;&#x304A;&#x3046;</pr></trd>,
    <trd>&#x501F;&#x308A;&#x3092;&#x3064;&#x304F;&#x308B;  <pr>&#x304B;&#x308A;&#x3092;&#x3064;&#x304F;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">zich <ind>verplichten</ind> tot</trd>
  <trd lng="pl">zad&lstroke;u&zdot;a&cacute; si&eogonek;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&c_t;&c_i; &c_b;&#x43E;&c_r;&c_zh;&#x43D;&c_i;&#x43A;&#x43E;&#x43C;</trd>,
    <trd>&c_z;&c_a;&c_b;&#x43E;&c_r;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&#x43E;&c_b;&#x43E;&c_v;&#x2019;&#x44F;&c_z;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="sxuld.sen0a">
