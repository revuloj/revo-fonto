<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Kebek</rad>/o
</kap>

<drv mrk="kebek.K0o">
  <kap><tld/>o</kap>
  <snc mrk="kebek.K0o.provinco">
    <fnt><bib>LanLin</bib></fnt>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Provinco de <ref tip="malprt" cel="kanad.K0o.GEOG">Kanado</ref> en
      &gcirc;ia orienta parto; <ref tip="sin"
      cel="kebeki.K0o.GEOG">Kebekio</ref>:
      <ekz>
        jam tri fojojn la sendependisma partio venkis en balotoj kaj
        fari&gcirc;is registaro de la provinco <tld/>o, kaj tamen &gcirc;i ankora&ubreve;
        ne deklaris la sendependi&gcirc;on de <tld/>o
        <fnt>
          <aut>Y. Bellefeuille</aut>,
          <vrk>Re: Rememorante la forpasintan Pierre Trudeau</vrk>,
          <lok><url
          ref="https://groups.google.com/d/msg/soc.culture.esperanto/nvfl2fDITAo/qH78UQjOfo0J">soc.culture.esperanto</url>,
          2000-10-02</lok>
        </fnt>.
      </ekz>
    </dif> 
  </snc>
  <snc mrk="kebek.K0o.urbo">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      La <ref tip="lst" cel="urb.cxef0o"
      lst="voko:&ccirc;efurboj">&ccirc;efurbo</ref> de <ref tip="dif"
      cel="kebek.K0o.provinco"><tld/>o<sncref/></ref> (46&#730;49'N,
      71&#730;13'U):
    </dif>
    <trd lng="en">Quebec City</trd>
  </snc>
  <trd lng="be">&c_K;&c_v;&c_e;&c_b;&c_e;&c_k;</trd>
  <trd lng="bg">&c_K;&c_v;&c_je;&c_b;&c_je;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>Quebec</trd>,
    <trd>Quebek</trd>
  </trdgrp>
  <trd lng="de">Qu&eacute;bec</trd>
  <trd lng="en">Quebec</trd>
  <trd lng="es">Quebec</trd>
  <trd lng="fr">Qu&eacute;bec</trd>
  <trd lng="nl">Qu&eacute;bec</trd>
  <trd lng="pl">Quebec</trd>
  <trdgrp lng="sk">
    <trd>Quebec</trd>,
    <trd>Quebek</trd>
  </trdgrp>
</drv>

<drv mrk="kebek.K0urbo">
  <kap><tld/>urbo</kap>
  <snc>
    <ref tip="dif" cel="kebek.K0o.urbo"><tld/>o<sncref/></ref>
    <ekz>
      en <tld/>urbo certe rolas similaj konvinkoj
      <fnt>
        <aut>M. Fettes</aut>:
        <vrk>Diversaj historioj, diversaj futuroj &ndash; kaj la volo
        elekti</vrk>.
        <bib>LOdE</bib>, <lok>2001, n-ro 8-9</lok>
      </fnt>.
    </ekz>
  </snc>
  <trd lng="en">Quebec City</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>