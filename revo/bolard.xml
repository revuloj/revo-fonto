<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>bolard</rad>/o
  </kap>
  <drv mrk="bolard.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">MAR</uzo>
    <snc>
      <dif>
        Fera <ref tip="super" cel="palis.0o">paliso</ref> sur kajmuro, uzata por 
        alligi la &scirc;ipon.  
      </dif>
      <adm>
        Kial "sur kajmuro" kaj ne simple "sur kajo"? <aut>MB</aut>
      </adm>
    </snc>
    <ref tip="vid" cel="bit.0o.MAR">bito.</ref>
    <trd lng="be">&c_k;&c_n;&c_e;&c_h;&c_t; <klr tip="amb">(&c_p;&c_r;&c_y;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;)</klr></trd>
    <trdgrp lng="de">
      <trd>Poller</trd>,
      <trd>Vert&auml;ungspfosten</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>bollard</trd>,
      <trd>mooring-post</trd>
    </trdgrp>
    <trd lng="fr">bollard</trd>
    <trdgrp lng="hu">
      <trd>kik&ouml;t&odblac;bak</trd>,
      <trd>kik&ouml;t&odblac;c&ouml;l&ouml;p</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_n;&c_je;&c_h;&c_t;</trd>
  </drv>
</art>
