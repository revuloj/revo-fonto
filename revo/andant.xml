<art mrk="$Id: andant.xml,v 1.19 2023/10/12 18:05:09 revo Exp $">
<kap>
  <rad>andant</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="andant.0o">
  <kap><tld/>o</kap>
  <snc mrk="andant.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Muzik-verko a&ubreve; -movimento, kies movo estas meze rapida:
      <ekz>
        &scirc;i pruvas nesufi&ccirc;an guston ludante kun tia emfazo
        tiun tiel simplan <tld/>on
        <fnt>
          <aut>M. Proust, trad. P. Combot</aut>,
          <vrk>Pri la legado</vrk>,
          <lok>1993</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_d;&c_a;&c_n;&c_t;&c_e;</trd>
  <trd lng="cs">skladba ve st&#x159;edn&#xED;m tempu</trd>
  <trd lng="en">andante</trd>
  <trd lng="es">andante</trd>
  <trd lng="fr">andante<klr> (subst.)</klr></trd>
  <trd lng="hu">andante</trd>
  <trd lng="it">andante</trd>
  <trd lng="ja">&#x30A2;&#x30F3;&#x30C0;&#x30F3;&#x30C6;</trd>
  <trd lng="nl">andante</trd>
  <trd lng="ru">&c_a;&c_n;&c_d;&c_a;&c_n;&c_t;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>andante</trd>,
    <trd>pomal&#xE1; &#x10D;as&#x165; skladby</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x43D;&#x434;&#x430;&#x43D;&#x442;&#x435;&#xA0;(&#x456;&#x43C;.)</trd>
</drv>

<drv mrk="andant.0e">
  <kap><tld/>e</kap>
  <snc mrk="andant.0e.MUZ">
    <dif>
      Mezrapide.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_d;&c_a;&c_n;&c_t;&c_e;</trd>
  <trdgrp lng="cs">
    <trd>andante</trd>,
    <trd>st&#x159;edn&#xED;m tempem</trd>
  </trdgrp>
  <trd lng="en">andante</trd>
  <trd lng="es">andante</trd>
  <trd lng="fr">andante</trd>
  <trd lng="hu">andante</trd>
  <trd lng="it">andante</trd>
  <trd lng="ja">&#x6B69;&#x304F;&#x901F;&#x3055;&#x3067;  <pr>&#x3042;&#x308B;&#x304F;&#x306F;&#x3084;&#x3055;&#x3067;</pr></trd>
  <trd lng="nl">andante</trd>
  <trd lng="ru">&c_a;&c_n;&c_d;&c_a;&c_n;&c_t;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>andante</trd>,
    <trd>pomal&#xE1; &#x10D;as&#x165; skladby</trd>
  </trdgrp>
</drv>
</art>
