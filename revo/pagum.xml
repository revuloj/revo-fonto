<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>pagum</rad>/o
</kap>
<drv mrk="pagum.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Genro el familio <ref tip="super" cel="vivere.0oj">viveredoj</ref>
       (Paguma), kun unu specio (<trd lng="la">Paguma larvata</trd>) vivanta
       en orienta Tibeto, suda &Ccirc;inujo, Tajvano, Borneo, 60-centimetra
       brun-hara besto kun nigra-blanka kapo, noktema:
       <ekz>
         oni portu <klr>...</klr> pladon da salita <tld/>a&jcirc;o al Jado kaj
         Jaspa
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 75a, p. 577a</lok></fnt>
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_p;&c_a;&c_g;&c_u;&c_m;&c_a;</trd>
   <trd lng="en">masked palm civet</trd>
   <trd lng="fr">civette palmiste &agrave; masque</trd>
   <trd lng="hu">&aacute;lc&aacute;s p&aacute;lmasodr&oacute;</trd> 
   <trd lng="pt">civeta</trd>  
   <trd lng="ru">&c_p;&c_a;&c_g;&c_u;&c_m;&c_a;</trd>
</drv>
</art>

