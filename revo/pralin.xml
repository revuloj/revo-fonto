<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: pralin.xml,v 1.2 2023/10/19 15:25:14 afido Exp $">
    <kap>
      <rad>pralin</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="pralin.0o">
  <kap><tld/>o</kap>
  <snc mrk="pralin.0o.migdalo">
    <dif>
      Rostita, kandita <ref tip="super" cel="migdal.0o">migdalo</ref>:
      <ekz>
        la aktorino, surprizite, tamen akceptas la <tld/>ojn
        <fnt>
          <bib>KrB</bib>
          <lok>Muzika terminaro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">gebrannte <ind>Mandel</ind></trd>
    <trd lng="en">candied almond</trd>
    <trd lng="fr">praline <klr>(amande)</klr></trd>
    <trd lng="nl">gebrande <ind>amandel</ind></trd>
  </snc>
  <snc mrk="pralin.0o.cxokolada">
    <dif>
      Malgranda <ref tip="super" cel="dolcx.0ajxo">dol&ccirc;a&jcirc;o</ref> kun
      &ccirc;okolada tega&jcirc;o kaj bongusta karakteriza far&ccirc;a&jcirc;o:
      <ekz>
        meza polo por dol&ccirc;a&jcirc;oj enhavantaj kakaon elspezas &ccirc;irka&ubreve; 190 zlotojn jare, al la
        plej popularaj kalkuli&gcirc;as &ccirc;okoladkaheloj kaj <tld/>oj
        <fnt>
          <aut>Krystyna Grochocka</aut>
          <vrk><url
          ref="https://pola-retradio.org/2021/11/pollando-cokolada-potenculo-en-la-centr-orienta-europo/"
              >Pollando, &ccirc;okolada potenculo en la Centr-Orienta E&ubreve;ropo</url></vrk>
          <lok>Pola Retradio, elsendo 3a nov. 2021</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plej populara kaj tradicia kafejo <klr>[&#8230;]</klr> surmerkatigas sian propran
        markon de luksaj <tld/>oj en skatoloj
        <fnt>

          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/1998/monato199803.pdf#page=25">Etikedado kaj Vero Moda</url></vrk>
          <bib>Monato</bib>
          <lok>1998/03, p. 25</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Praline</trd>
    <trd lng="en">chocolate <ind>praline</ind></trd>
    <trd lng="fr">praline <klr>(chocolat)</klr></trd>
    <trd lng="nl">praline</trd>
  </snc>
</drv>

  </art>

