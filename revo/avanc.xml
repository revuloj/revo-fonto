<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: avanc.xml,v 1.11 2021/03/04 18:15:49 revo Exp $">
<kap>
  <rad>avanc</rad>/i
</kap>
<drv mrk="avanc.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="avanc.0i.MIL">
    <fnt><bib>PIV1</bib></fnt>
    <gra><vspec>ntr</vspec></gra>
    <dif>
      Anta&ubreve;i&gcirc;i, ekokupi landoparton:
      <ekz>
        la bataliono <tld/>is na&ubreve; mejlojn norden
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la <tld/>o de la trupoj de <nom>Franco</nom>, la familio transloki&gcirc;is al Barcelono
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="https://www.monato.be/2004/008467.html">Kampoj de Soria Antonio Machado, universala poeto</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u ni la kurbon de la vojo
        <fnt>
          <bib>Monato</bib>
          <aut>Geraldo Mattos</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006863.html">Kio estas Amo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sudorienta E&ubreve;ropo <tld/>as kaj komencas likvidi la malfrui&gcirc;on dum la 90aj jaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="https://www.monato.be/2003/008045.html">Balkanio likvidas ekonomian malfrui&gcirc;on</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">avan&ccedil;ar</trd>
    <trd lng="de">vordringen</trd>
    <trd lng="fr">avancer<klr> (en parlant d'une arm&eacute;e)</klr></trd>
    <trd lng="es">avanzar</trd>
    <trdgrp lng="hu">
      <trd>el&odblac;ret&ouml;r</trd>,
      <trd>el&odblac;renyomul</trd>
    </trdgrp>
    <trd lng="it">avanzare</trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="ind"> (&c_v;&c_o;&c_je;&c_n;.)</klr></trd>,
      <trd>&c_p;&c_r;&c_o;&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_v;&c_o;&c_je;&c_n;.)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="avanc.0i.POL">
    <fnt><bib>SPV</bib></fnt>
    <dif>
      <ref tip="dif" cel="promoc.0igxi">Promocii&gcirc;i</ref>, atingi pli altan rangon:
      <ekz>
        la oficiroj ricevos <tld/>on je du gradoj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>McCrae</nom> rapide <tld/>is kiel kuracisto, laboris pri a&ubreve;topsioj, pri patologio, pri
        konta&gcirc;aj malsanoj &ndash; instruis, verkis studlibrojn ktp
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="https://www.monato.be/2015/011504.php">Tre bonvena verko, sed ...</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">ascendir</trd>
    <trd lng="de"><ind>bef&ouml;rdert</ind> werden</trd>
    <trd lng="es">avanzar</trd>
    <trd lng="hu">el&odblac;l&eacute;p<klr tip="amb"> (rangl&eacute;tr&aacute;n)</klr></trd> 
    <trd lng="it">avanzare</trd>   
    <trd lng="ru">&c_p;&c_r;&c_o;&c_d;&c_v;&c_i;&c_g;&c_a;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_v; &c_k;&c_a;&c_r;&c_mol;&c_je;&c_r;&c_je;)</klr></trd>
  </snc>
  <snc mrk="avanc.0i.EKON">
    <fnt><bib>NeoGlo</bib></fnt>
    <gra><vspec>tr</vspec></gra>
    <dif>
      <ref tip="dif" cel="pag.antaux0i">Anta&ubreve;pagi</ref>:
      <ekz>
        la kaso <tld/>is kelkajn kunlaborantojn per la salajro jam por
        du-tri monatoj
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>bestreure</trd>,
      <trd>anticipar</trd>
    </trdgrp>
    <trd lng="de">vorausbezahlen</trd>
    <trd lng="es">anticipar</trd>
    <trd lng="hu">el&odblac;leget ad</trd>    
    <trd lng="it">anticipare</trd>
    <trdgrp lng="ru">
      <trd>&c_a;&c_v;&c_a;&c_n;&c_s;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_p;&c_l;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol; <ind>&c_a;&c_v;&c_a;&c_n;&c_s;</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="ca">avan&ccedil;ar</trd>
  <trdgrp lng="cs">
    <trd>avansovat</trd>,
    <trd>b&yacute;t pov&yacute;&scaron;en</trd>,
    <trd>dos&aacute;hnout vy&scaron;&scaron;&iacute; hodnosti</trd>,
    <trd>postupovat</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5B0;&#x5E7;&#x5B7;&#x5D3;&#x5B5;&#x5DD;</trd>
  <trdgrp lng="sk">
    <trd>by&tcaron; pov&yacute;&scaron;en&yacute;</trd>,
    <trd>post&uacute;pi&tcaron;</trd>
  </trdgrp>
</drv>
</art>

