<art mrk="$Id: makis.xml,v 1.16 2024/04/02 07:23:38 revo Exp $">
<kap>
  <rad>makis</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>

<drv mrk="makis.0o">
  <kap><tld/>o</kap>
  <snc mrk="makis.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Vasta, densa, seka <ref tip="super" cel="vepr.0ejo.GEOG">veprejo</ref>,
      kian oni trovas sur apudmediteraneaj montaroj:
      <ekz>
        mi kaj kelkaj aliaj kapristoj ser&ccirc;is lin preska&ubreve; du tagojn en
        la
        <tld/>o
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 23a, p.
        168a</lok></fnt>;
      </ekz>
      <ekz>
        li reiris la padon, repasis tra l' arbaro, la <tld/>o, la
        senkultivejo
        <fnt><aut>A. Manzoni, trad. B. Cadei</aut>, <vrk>La
        Gefian&ccirc;oj</vrk>, <lok>FEI, 2006</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_k;&c_v;&c_ib;&c_s;</trd>,
    <trd>&c_g;&c_u;&c_sh;&c_ch;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="ca">m&agrave;quia</trd>
  <trdgrp lng="cs">
    <trd>horsk&aacute; hou&scaron;tina</trd>,
    <trd>&uacute;kryt povstalc&uring;</trd>
  </trdgrp>
  <trd lng="de">Macchie</trd>
  <trd lng="fr">maquis</trd>
  <trd lng="hu">boz&oacute;t</trd>
  <trdgrp lng="ja">
    <trd>&#x6F45;&#26408;&#x5BC6;&#x751F;&#22320;&#x5E2F; 
    <pr>&#x304B;&#12435;&#x307C;&#x304F;&#x307F;&#12387;&#x305B;&#12356;&#12385;&#x305F;&#12356;</pr></trd>,
    <trd>&#x30DE;&#x30AD;</trd>,
    <trd>&#x6F5C;&#x4F0F;&#22580;&#25152;  <pr>&#x305B;&#12435;&#12407;&#x304F;&#12400;&#12375;&#12423;</pr></trd>
  </trdgrp>
  <trd lng="nl">maquis</trd>
  <trd lng="pl">makia</trd>
  <trdgrp lng="pt">
    <trd>maquis</trd>,
    <trd>m&aacute;quia</trd>,
    <trd>maquis</trd>
  </trdgrp>
  <trd lng="ro">machia</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_shch;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_r;&c_o;&c_s;&c_l;&c_i;</trd>,
    <trd>&c_m;&c_a;&c_k;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>div&eacute; kroviny</trd>,
    <trd>horsk&aacute; h&uacute;&scaron;tina</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_ib;&c_s;&#x43E;&c_v;&c_ib; &c_z;&c_a;&c_r;&#x43E;&c_s;&c_t;&c_ib;</trd>,
    <trd>&c_ch;&c_a;&c_g;&c_a;&c_r;&#x43D;&c_i;&#x43A;&c_i;</trd>,
    <trd>&c_h;&c_a;&c_shch;&c_ib;</trd>,
    <trd>&#x43D;&c_je;&c_t;&c_r;&c_ib;</trd>,
    <trd>&c_d;&c_je;&c_b;&c_r;&c_ib;</trd>,
    <trd>&#x43C;&c_a;&#x43A;&c_v;&c_ib;&c_s;</trd>
  </trdgrp>
</drv>
