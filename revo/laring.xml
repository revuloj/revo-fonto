<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: laring.xml,v 1.27 2023/09/01 19:10:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>laring</rad>/o
</kap>
<drv mrk="laring.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Supra parto de la <ref tip="malprt" cel="trahxe.0o.ANA">tra&hcirc;eo</ref>
      en kiu formi&gcirc;as la <ref cel="vocx.0o.SON">vo&ccirc;o</ref>:
      <ekz>
        la esploristoj <klr>[&#x2026;]</klr> prenis <tld/>on de mortinta elefanto kaj plenumis
        eksperimentojn pri &gcirc;i en la departemento pri kogna biologio de la
        universitato de Vieno
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2013/010876.php">Elefanta komunikado malkodita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        valorajn servojn faras pomoj &ccirc;e malvarmumo, tuso, <tld/>omalsanoj ktp
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2014/011362.php">Simbolo de juneco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_l;&c_a;&c_r;&c_i;&c_n;&c_k;&c_s;</trd>
  <trdgrp lng="de">
    <trd>Kehlkopf</trd>,
    <trd>Larynx [science]</trd>
  </trdgrp>
  <trd lng="be">&c_g;&c_a;&c_r;&c_t;&c_a;&c_n;&c_mol;</trd>
  <trd lng="br">garloc'henn</trd>
  <trd lng="ca">laringe</trd>
  <trd lng="el">&lambda;&alfa_ton;&rho;&rho;&ypsilon;&gamma;&gamma;&alfa;&sigma_fina;</trd>
  <trd lng="es">laringe</trd>
  <trd lng="en">larynx</trd>
  <trd lng="fr">larynx</trd>
  <trd lng="hu">g&eacute;gef&odblac;</trd>
  <trd lng="it">laringe</trd>
  <trd lng="ja">&#x5589;&#x982D;  [<ind>&#x3053;&#x3046;&#x3068;&#x3046;</ind>]</trd>
  <trd lng="nl">strottenhoofd</trd>
  <trd lng="pl">krta&nacute;</trd>
  <trd lng="pt">laringe</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_t;&c_a;&c_n;&c_mol;</trd>
  <trd lng="sv">struphuvud</trd>
  <trd lng="uk">&#x433;&#x43E;&#x440;&#x442;&#x430;&#x43D;&#x44C;</trd>
</drv>

<drv mrk="laring.0afrikativo">
  <kap>
    <tld/>a frikativo
  </kap>
  <uzo tip="fak">FON</uzo>
  <snc>
    <dif>
      Malaktuali&gcirc;inta, sumiga kaj erariga nomo por la glotaj, faringaj
      kaj
      faringecigitaj frikativoj.
      En Esperanto la sono H.
    </dif>
    <trd lng="ca">fricativa far&iacute;ngia</trd>
    <trdgrp lng="de">
      <trd>Laryngal</trd>,
      <trd>Kehlkopflaut</trd>
    </trdgrp>
    <trd lng="es">fricativa laringal</trd>
    <trd lng="fr">fricative laryngale</trd>
    <trd lng="hu">g&eacute;gehang</trd>
    <trd lng="it"><ind>fricativa</ind> laringale</trd>
    <trd lng="nl">laryngaal</trd>
    <trd lng="pl">sp&oacute;&lstroke;g&lstroke;oska szczelinowa</trd>
  </snc>
</drv>
</art>
