<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: dioksid.xml,v 1.8 2022/09/06 10:37:17 revo Exp $">
    <kap>
      <rad>dioksid</rad>/o
    </kap>

<drv mrk="dioksid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc mrk="dioksid.0o.KEM">
    <dif>
      <ref tip="dif" cel="oksid.du0o">Duoksido</ref>:
      <ekz>
        komence la tera atmosfero konsistis precipe el karbona
        <tld/>o, kaj natura selektado okazis en senoksigenaj kondi&ccirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.net/2008/009428.php">Lastaj opinioj pri la origino de
          vivo</url></vrk>
        </fnt>;
      </ekz><ekz>
        a&ubreve;toj emisias grandegan parton de karbon<tld/>o, kiu kulpas
        <klr>[&#8230;]</klr> pri la tutmonda varmi&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.net/2008/009637.php">Katastrofaj ciferoj</url></vrk>
        </fnt>;
      </ekz><ekz>
        titana <tld/>o <klr>[&#8230;]</klr>, TiO<sub>2</sub> <klr>[&#8230;]</klr>
        estas uzata en multaj terenoj tiel en farba&jcirc;oj, sunlaktoj kaj
        kiel nutra&jcirc;-farba&jcirc;o (E171)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=246902">Titana dioksido</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_d;&c_y;&c_ja;&c_k;&c_s;&c_y;&c_d;</trd>
    <trd lng="de">Dioxid</trd>
    <trd lng="en">dioxid</trd>
  </snc>
</drv>

<drv mrk="dioksid.karbon0o">
  <kap>
    karbon<tld/>o,
    <var><kap>karbona <tld/>o</kap></var>,
    <var><kap>karbona duoksido</kap></var>
  </kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      (<frm>CO<sub>2</sub></frm>)
      <ref tip="dif" cel="oksid.du0o">Duoksido</ref> de karbono, 
      &ccirc;e normala temperaturo <ref tip="super" cel="gas.0o.FIZ">gaso</ref>,
      i.a. enhavata en la <ref tip="malprt" cel="atmosf.0o.AST">atmosfero</ref>:
      <ekz>
        la homaro eligas tro multe da karbon<tld/>o en la atmosferon,
        bruligante precipe fosiliajn krudmaterialojn
        <fnt>
          <bib>Monato</bib>
          <aut>Werner Fu&szlig;</aut>
          <vrk><url ref="&Monato;/2011/010272.php">Nekontentiga progreso en
          klimatopolitiko</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz><ekz>
        ellasoj de karbona <tld/>o en tutmonda skalo ver&scirc;ajne iom
        malplii&gcirc;is en 2008 kiel rezulto de la tutmonda financa krizo
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2010/010115.php">Nekontentiga bilanco</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz><ekz>
        a&ubreve;toj emisias grandegan parton de karbon<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009637.php">Katastrofaj ciferoj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz><ekz>
        je maksimume 11,5 gradoj celsiaj la avera&gcirc;a temperaturo
        tutmondskale povos kreski &gcirc;is la fino de tiu &ccirc;i jarcento, se
        la karbon<tld/>o-procenta&jcirc;o en la atmosfero plu senlime
        kreskos dum la venonta tempo
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008735.php">&Ccirc;u la tero fari&gcirc;os sa&ubreve;no?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz><ekz>
          gasakvo kun pliigita koncentriteco de agresiva karbona duoksido
          <fnt>
            <aut>Jan Werner</aut>:
              <vrk><url
              ref="http://denizo.opia.dk/la.trezorejo/tekstoj/libroj.pdf/fakaj.libroj.pdf/scienco/Terminaro_de_betono.pdf">Terminaro
              de betono kaj betonistaj laboroj</url></vrk>.
            <lok>Konstru-Forumo 2016, 30.3.</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="acid.karbon0o">karbonacido</ref>,
      <ref cel="forc.0ejaefiko">forceja efiko</ref> 
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_y;&c_ja;&c_k;&c_s;&c_y;&c_d; &c_v;&c_u;&c_g;&c_l;&c_ja;&c_r;&c_o;&c_d;&c_u;</trd>,
    <trd>&c_d;&c_v;&c_u;&c_h;&c_v;&c_o;&c_k;&c_ib;&c_s; &c_v;&c_u;&c_g;&c_l;&c_ja;&c_r;&c_o;&c_d;&c_u;</trd>,
    <trd>&c_v;&c_u;&c_g;&c_l;&c_ja;&c_k;&c_ib;&c_s;&c_l;&c_y; &c_g;&c_a;&c_z;</trd>
  </trdgrp>
  <trd lng="de">Kohlenstoffdioxid</trd>
  <trd lng="en">carbon dioxid</trd>
  <trd lng="fr">dioxyde de carbone</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_v;&#x43E;&#x43E;&#x43A;&c_i;&c_s; &c_v;&c_u;&c_g;&#x43B;&c_je;&c_c;&#x44E;</trd>,
    <trd>&c_v;&c_u;&c_g;&#x43B;&c_je;&#x43A;&c_i;&c_s;&#x43B;&c_i;&c_j; &c_g;&c_a;&c_z;</trd>
  </trdgrp>
</drv>
</art>

