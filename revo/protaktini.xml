<art mrk="$Id: protaktini.xml,v 1.20 2024/02/17 07:52:37 revo Exp $">
<kap>
  <rad>protaktini</rad>/o
</kap>
<drv mrk="protaktini.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Pa</mlg>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="91">elemento</ref>
      kun
      atomnumero 91, atompezo 231,0359, kies &ccirc;iuj
      <ref tip="super" cel="izotop.0o">izotopoj</ref>
      estas radioaktivaj; aktinido:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estas rara tero <klr>[&#x2026;]</klr>, ar&gcirc;ent-kolora
        radioaktiva elemento, konita p<klr>[ro]</klr> &gcirc;ia superkonduktivo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=28842">Protaktinio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Lise Meitner</nom> <klr>[&#x2026;kaj]</klr> la kemiisto <nom>Otto Hahn</nom> <klr>[&#x2026;]</klr>
        esploris la
        fenomenon de la radioaktiveco kaj malkovris en 1918 la elementon 91, <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2657">Lise Meitner</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_t;&c_a;&c_k;&c_t;&c_y;&c_n;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_t;&c_a;&c_k;&c_t;&c_i;&c_n;&c_i;&c_j;</trd>
  <trd lng="br">protaktiniom</trd>
  <trd lng="cs">protaktinium</trd>
  <trd lng="de">Protactinium</trd>
  <trd lng="en">protactinium</trd>
  <trd lng="es">protactinio</trd>
  <trd lng="fa">&f_p;&a_r;&a_w;&a_t;&a_A;&f_k;&a_t;&f_y;&a_n;&f_y;&a_m;</trd>
  <trd lng="fr">protactinium</trd>
  <trd lng="hu">protakt&iacute;nium</trd>
  <trd lng="id">protaktinium</trd>
  <trd lng="it">protattinio</trd>
  <trd lng="la">Protactinium</trd>
  <trd lng="nl">protactinium</trd>
  <trd lng="pl">protaktyn</trd>
  <trd lng="pt">protact&iacute;nio</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_a;&c_k;&c_t;&c_i;&c_n;&c_i;&c_j;</trd>
  <trd lng="sk">protakt&iacute;nium</trd>
  <trd lng="sv">protaktinium</trd>
  <trd lng="uk">&#x43F;&c_r;&#x43E;&c_t;&c_a;&#x43A;&c_t;&c_i;&#x43D;&c_ib;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x93F7; <pr>p&#xFA;</pr></trd>,
    <trd>&#x9564; <pr>p&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>
