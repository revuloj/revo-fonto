<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: dublon.xml,v 1.8 2023/01/26 09:27:23 revo Exp $">
<kap>
  <rad>dublon</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>

<drv mrk="dublon.0o">
  <kap><tld/>o</kap>
  <snc mrk="dublon.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Antikva <ref tip="super" cel="mon.0ero">monero</ref> hispana kaj
      de hispanaj kolonioj, el oro, kun diversaj aspektoj kaj valoroj:
      <ekz>
        fripono &scirc;telinta de li cent <tld/>ojn
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 1a, p.
        398a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_u;&c_b;&c_l;&c_jo;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>doblon</trd>,
    <trd>dublon</trd>,
    <trd>pistole</trd>
  </trdgrp>
  <trd lng="de">Dublone<klr tip="amb"> (Goldm&uuml;nze)</klr></trd>
  <trd lng="es">dobl&oacute;n<klr> (moneda)</klr></trd>
  <trd lng="fr">doublon<klr> (monnaie)</klr></trd>
  <trd lng="pl">dublon</trd>
  <trdgrp lng="pt">
    <trd>dobra</trd>,
    <trd>dobr&atilde;o</trd>
  </trdgrp>
  <trd lng="sk">dubl&oacute;n</trd>
</drv>
</art>

