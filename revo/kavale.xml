<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kavaleri</rad>/o
</kap>
<drv mrk="kavale.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Parto de armeo konsistanta el <ref cel="soldat.rajdo0o">rajdosoldatoj</ref>.  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_v;&c_a;&c_l;&c_je;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kavallerie</trd>,
     <trd>Reiterei</trd>,
     <trd>berittene <ind>Truppe</ind></trd>
  </trdgrp>
  <trd lng="en">cavalry</trd>
  <trd lng="fr">cavalerie</trd>
  <trd lng="hu">lovass&aacute;g</trd>
  <trd lng="nl">cavalerie</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_v;&c_a;&c_l;&c_je;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kavale.0ano">
  <kap><tld/>ano</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Ano de <tld/>o.
    </dif>
    <ref tip="sin" cel="soldat.rajdo0o">rajdosoldato</ref>
  </snc>
  <trd lng="fr">cavalier<klr tip="ind"> (soldat de cavalerie)</klr></trd>
</drv>

</art>
