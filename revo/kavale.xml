<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kavale.xml,v 1.21 2022/11/17 12:58:53 revo Exp $">
<kap>
  <rad>kavaleri</rad>/o
</kap>
<drv mrk="kavale.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      Parto de <ref tip="malprt" cel="arme.0o.sxtata">armeo</ref>
      konsistanta el <ref cel="soldat.rajdo0o">rajdosoldatoj</ref>:
      <ekz>
        la nombro de la armeoj de la <tld/>o estis ducent milionoj
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 9:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Tutmozis</nom> ordonis jungi &ccirc;evalojn al dudek kvin &ccirc;aroj de la peza <tld/>o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o, vidante, ke unu homo forportas la ri&ccirc;a&jcirc;ojn de la tuta lando, ekkoleris
        kaj sendis sian <tld/>on kun ordono kapti la fortegulon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Sesope oni trairas la tutan mondon</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_v;&c_a;&c_l;&c_je;&c_r;&c_y;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_k;&c_a;&c_v;&c_a;&c_l;&c_je;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>jezdectvo</trd>,
    <trd>j&iacute;zda</trd>,
    <trd>kavalerie</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kavallerie</trd>,
     <trd>Reiterei</trd>,
     <trd>berittene <ind>Truppe</ind></trd>
  </trdgrp>
  <trd lng="en">cavalry</trd>
  <trd lng="fr">cavalerie</trd>
  <trd lng="hu">lovass&aacute;g</trd>
  <trd lng="it">cavalleria</trd>
  <trd lng="nl">cavalerie</trd>
  <trdgrp lng="pl">
    <trd>kawaleria</trd>,
    <trd>konnica</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_v;&c_a;&c_l;&c_je;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">jazdectvo</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_ib;&#x43D;&#x43D;&#x43E;&c_t;&c_a;</trd>,
    <trd>&#x43A;&c_a;&c_v;&c_a;&#x43B;&c_je;&c_r;&c_ib;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="kavale.0ano">
  <kap><tld/>ano</kap>
  <uzo tip="fak">MIL</uzo>
  <snc>
    <ref tip="dif" cel="soldat.rajdo0o">rajdosoldato</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_v;&c_a;&c_l;&c_je;&c_r;&c_y;&c_s;&c_t;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_a;&c_v;&c_a;&c_l;&c_je;&c_r;&c_i;&c_s;&c_t;</trd>
  <trd lng="cs">kavalerista</trd>
  <trdgrp lng="de">
    <trd>Kavallerist</trd>,
    <trd>Reiter</trd>,
    <trd>berittener <ind>Soldat</ind></trd>
  </trdgrp>
  <trd lng="en">cavalryman</trd>
  <trd lng="fr">cavalier<klr tip="ind"> (soldat de cavalerie)</klr>
  </trd>
  <trdgrp lng="it">
    <trd>cavaliere<klr> (soldato di cavalleria)</klr></trd>,
    <trd>cavalleggero</trd>
  </trdgrp>
  <trd lng="nl">cavalerist</trd>
  <trd lng="pl">kawalerzysta</trd>
  <trd lng="ru">&c_k;&c_a;&c_v;&c_a;&c_l;&c_je;&c_r;&c_i;&c_s;&c_t;
  </trd>
  <trd lng="sk">kavalerista</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_ib;&#x43D;&#x43D;&#x43E;&c_t;&#x43D;&c_i;&#x43A;</trd>,
    <trd>&#x43A;&c_a;&c_v;&c_a;&#x43B;&c_je;&c_r;&c_i;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

</art>
