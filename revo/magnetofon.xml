<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>magnetofon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="magnetofon.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sona <ref tip="super" cel="bend.sur0igilo">surbendigilo</ref>,
      ordinare uzebla kaj por registri kaj por a&ubreve;digi:
      <ekz>
        inter la novaj varoj <klr>...</klr> trovi&gcirc;is la unuaj <tld/>oj
        <fnt><bib>BonaLingvo</bib>, <lok>p. 51a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_g;&c_n;&c_ib;&c_t;&c_a;&c_f;&c_o;&c_n;</trd>
  <trdgrp lng="es">
    <trd>grabadora<klr> (magnet&oacute;fono)</klr></trd>,
    <trd>magnet&oacute;fono<klr> (en desuso)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>enregistreur<klr> (magn&eacute;tophone)</klr></trd>,
    <trd>magn&eacute;tophone</trd>
  </trdgrp>
  <trd lng="hu">magnetofon</trd>
  <trdgrp lng="pt">
    <trd>gravador <klr>(subst.,eletr&ocirc;n.)</klr></trd>,
    <trd>magnetofone</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_a;&c_g;&c_n;&c_i;&c_t;&c_o;&c_f;&c_o;&c_n;</trd>
</drv>
</art>

