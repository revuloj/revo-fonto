<kap>
  <rad>Andraprade&scirc;</rad>/o <fnt><bib>LanLin</bib></fnt>
</kap>
<drv mrk="andrapradesx.0o">
  <kap><tld/>o</kap>
  <snc mrk="andrapradesx.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Gubernio en la sudorienta parto de
      <ref tip="malprt" cel="hind.H0ujo.POL">Hindujo</ref>:
      <ekz>
        la registaro de <tld/>o preparis lernolibrojn 
        en la indi&gcirc;enaj lingvoj adivasi, orija, ban&gcirc;ara, 
        gondi, koja, kolami, konda, kuvi kaj savara
        <fnt>
          <aut>A. Giridhar Rao</aut>,
          <vrk><url ref="&Monato;/2005/008714.php">
          Gepatralingva edukado por indi&gcirc;enoj</url></vrk>,
          <bib>Monato</bib>,
          <lok>2005:9, p.21a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_A;&c_n;&c_d;&c_g;&c_r;&c_a;-&c_P;&c_r;&c_a;&c_d;&c_e;&c_sh;</trd>
  <trd lng="en">Andhra Pradesh</trd>
  <trd lng="fr">Andra Pradesh</trd>
  <trd lng="ja">&#x30A2;&#x30F3;&#x30C9;&#x30E9;&#x30D7;&#x30E9;&#x30C7;&#x30B7;&#x5DDE;  <pr>&#x30A2;&#x30F3;&#x30C9;&#x30E9;&#x30D7;&#x30E9;&#x30C7;&#x30B7;&#x3057;&#x3085;&#x3046;</pr></trd>
  <trd lng="uk">&c_A;&#x43D;&c_d;&c_h;&c_r;&c_a;-&#x41F;&c_r;&c_a;&c_d;&c_je;&c_sh;</trd>
</drv>
</art>

