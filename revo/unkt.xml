<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: unkt.xml,v 1.11 2025/04/08 15:21:55 revo Exp $">
<kap>
  <rad>unkt</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="unkt.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="unkt.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="ole.sankt0i.REL">Sanktolei</ref>:
      <ekz>
        la tu&scirc;' de via mano min <tld/>as kaj balzamas
        <fnt>
          <aut>E. Tarkony</aut>,
          <vrk><url ref="http://www.egalite.hu/tarkony/soifo/transtera%20sopiro.htm">
          Transtera sopiro</url></vrk>,
          <lok>[1964]</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi banis vian korpon belan, per bonodora oleo vin <tld/>is, kolon juvel&ccirc;ene
        dekoris
        <fnt>
          <aut>K&aacute;lm&aacute;n Kalocsay </aut>
          <vrk><url ref="http://verkoj.com/lauteme/klasika/kalman-kalocsay/tutmonda-sonoro/">Tutmonda
          sonoro</url></vrk>
          <lok>II. Tabulo: La Lukto inter Gilgame&scirc; kaj Enkidu</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>sanktolei</ke>, <ke>&#349;miri</ke> (per oleo).
  </rim>

  <!-- formon maloftan traduku per malofta&jcirc;o se tia ekzistas -->
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_p;&c_a;&c_m;&c_a;&c_z;&c_a;&c_n;&c_mol;&c_n;&c_je;</ind> <klr>(&c_c;&c_a;&c_r;&c_k;.)</klr></trd>
  <trd lng="cs">pomazat <klr>(na kr&aacute;le a pod.)</klr></trd>
  <trdgrp lng="de">
    <trd>&ouml;len <klr>(religi&ouml;s)</klr></trd>,
    <trd>salben</trd>
  </trdgrp>
  <trd lng="en">anoint</trd>
  <trd lng="fr">oindre</trd>
  <trd lng="pl">namaszcza&cacute;</trd>
  <trd lng="sk">pomaza&tcaron; olejom</trd>
  <trdgrp lng="uk">
    <trd>&c_jeu;&#1083;&c_je;&#1086;&c_s;&c_v;&#1103;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&#1086;&c_b;&#1086;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25273;&#27833; <pr>m&#466;y&#243;u</pr></trd>,
    <trd>&#33167;&#31435; <pr>g&#224;ol&#236;</pr></trd>
  </trdgrp>
</drv>
</art>

