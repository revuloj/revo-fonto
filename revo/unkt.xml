<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: unkt.xml,v 1.5 2020/07/14 00:43:52 revo Exp $">
<kap>
  <rad>unkt</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="unkt.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="unkt.0i.KOMUNE">
    <dif>
      <ref tip="dif" cel="ole.sankt0i.REL">Sanktolei</ref>:
      <ekz>
        la tu&scirc;' de via mano min <tld/>as kaj balzamas
        <fnt>
          <aut>E. Tarkony</aut>,
          <vrk><url ref="http://www.egalite.hu/tarkony/soifo/transtera%20sopiro.htm">
          Transtera sopiro</url></vrk>,
          <lok>[1964]</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi banis vian korpon belan, per bonodora oleo vin <tld/>is, kolon juvel&ccirc;ene
        dekoris
        <fnt>
          <aut>K&aacute;lm&aacute;n Kalocsay </aut>
          <vrk><url ref="http://verkoj.com/lauteme/klasika/kalman-kalocsay/tutmonda-sonoro/">Tutmonda sonoro</url></vrk>
          <lok>II. Tabulo: La Lukto inter Gilgame&scirc; kaj Enkidu</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <!-- forman maloftan traduku per malofta&jcirc;o se tia ekzistas -->
  <trdgrp lng="de">
    <trd>&ouml;len <klr>(religi&ouml;s)</klr></trd>,
    <trd>salben</trd>
  </trdgrp>
  <trd lng="en">anoint</trd>
  <trd lng="fr">oindre</trd>
  <trd lng="sk">pomaza&tcaron; olejom</trd>
  <trd lng="cs">pomazat <klr>(na kr&aacute;le a pod.)</klr></trd>
</drv>
</art>

