<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>unkt</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="unkt.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="unkt.0i.KOMUNE">
    <ref tip="dif" cel="ole.sankt0i.REL">sanktolei</ref>
    <ekz>
      la tu&scirc;' de via mano min <tld/>as kaj balzamas
      <fnt>
        <aut>E. Tarkony</aut>,
        <vrk><url
        ref="http://www.egalite.hu/tarkony/soifo/transtera%20sopiro.htm">
        Transtera sopiro</url></vrk>,
        <lok>[1964]</lok>
      </fnt>.
    </ekz>
  </snc>
  <!-- forman maloftan traduku per malofta&jcirc;o se tia ekzistas -->
  <trd lng="fr">oindre</trd>
  <trd lng="sk">pomazať olejom</trd>
  <trd lng="cs">pomazat <klr>(na krále a pod.)</klr></trd>
</drv>
</art>

