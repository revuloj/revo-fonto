<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: pivot.xml,v 1.15 2016/10/04 12:10:18 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>pivot</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="pivot.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">TEK</uzo>
    <dif>
      Ekstrema parto de akso, eni&gcirc;anta en truo, kie &gcirc;i turni&gcirc;as:
      <ekz>
        <tld/>o de rado en horlo&gcirc;o.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">pivot</trd>
  <trdgrp lng="de">
    <trd>Zapfen</trd>,
    <trd>Drehstift</trd>
  </trdgrp>
  <trd lng="en">pivot</trd>
  <trd lng="es">pivote</trd>
  <trd lng="fr">pivot</trd>
  <trdgrp lng="hu">
    <trd>tengelycsap</trd>,
    <trd>tengelyv&eacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tap</trd>,
    <trd>luns</trd>
  </trdgrp>
  <trd lng="pl">czop</trd>
  <trd lng="pt">piv&ocirc;<klr tip="amb">(eixo)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_sh;&c_je;&c_j;&c_k;&c_a; &c_v;&c_a;&c_l;&c_a;</trd>,
    <trd>&c_c;&c_a;&c_p;&c_f;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">pivot</trd>
</drv>

<drv mrk="pivot.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Turni&gcirc;i &ccirc;irka&ubreve; <tld/>o.
    </dif>
  </snc>
  <trd lng="cs">to&ccaron;it se na &ccaron;epu</trd>
  <trd lng="de">pivotieren</trd>
  <trdgrp lng="en">
    <trd>pivot</trd>,
    <trd>turn</trd>,
    <trd>rotate</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>pivotar</trd>,
    <trd>girar</trd>,
    <trd>rotar</trd>
  </trdgrp>
  <trd lng="fr">pivoter</trd>
  <trd lng="hu">forog</trd>
  <trd lng="nl">pivoteren</trd>
  <trd lng="pl">obraca&cacute; si&eogonek; na <ind>czopie</ind></trd>
  <trd lng="sk">kr&uacute;ti&tcaron; sa okolo &ccaron;apu</trd>
</drv>
</art>
