<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>heder</rad>/o
</kap>
<drv mrk="heder.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="arali.0acoj">araliacoj</ref>
      (<trd lng="la">Hedera</trd>), grimpanta vegeta&jcirc;o,      
      dedi&ccirc;ita de la antikvuloj al Bakuso. &Gcirc;i alteni&gcirc;as per
      kro&ccirc;radiketoj al arboj kaj muroj, kiujn &gcirc;i kovras per
      sia &ccirc;iamverda foliaro.
    </dif>
  </snc>
  <bld lok="../bld/heder.jpg">folioj de hedero
    (<trd lng="la">Hedera helix</trd>)</bld>
  <trd lng="de">Efeu</trd>
</drv>
