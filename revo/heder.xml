<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>heder</rad>/o
</kap>
<drv mrk="heder.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="arali.0oj">araliacoj</ref>
      (<trd lng="la">Hedera</trd>), grimpanta vegeta&jcirc;o,
      dedi&ccirc;ita de la antikvuloj al Bakuso. &Gcirc;i alteni&gcirc;as per
      kro&ccirc;radiketoj al arboj kaj muroj, kiujn &gcirc;i kovras per
      sia &ccirc;iamverda foliaro.
    </dif>
  </snc>
  <bld lok="../bld/heder.jpg">
    folioj de hedero <klr>(Hedera helix)</klr>
  </bld>
  <trd lng="be">&c_p;&c_l;&c_ju;&c_sh;&c_ch;</trd>    
  <trd lng="br">iliav(enn)</trd>
  <trd lng="de">Efeu</trd>
  <trd lng="en">ivy</trd>
  <trd lng="es">hiedra</trd>
  <trd lng="fr">lierre</trd>
  <trd lng="hu">borosty&aacute;n</trd>
  <trd lng="it">edera</trd>
  <trd lng="nl">klimop</trd>
  <trdgrp lng="pt">
    <trd>hera</trd>,
    <trd>hedra</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_l;&c_ju;&c_shch;</trd>
</drv>
</art>
