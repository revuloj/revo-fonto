<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vat.xml,v 1.21 2017/09/05 17:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vat.0o.HEJM">
    <dif>
      <ref tip="vid" cel="koton.0o">Kotono</ref> kombita kaj premita
      la&ubreve; molaj tavoloj (a&ubreve; substanco similaspekta):
      <ekz>
        <tld/>o estis produktita unue de la medicinisto Paul Victor von
        Bruns en 1865
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="https://eo.wikipedia.org/?curid=29575">
          Vato (kotono)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la embuskanta senbrueco en la domo subite &scirc;topis liajn
        orelojn kiel <tld/>obuletoj
        <fnt>
          <aut>M. Ende, trad. W. Diestel</aut>,
          <vrk>La Sen&ccirc;esa Rakonto</vrk>,
          <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">guata</trd>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_t;&c_a;</trd>
  <trd lng="ca">buata</trd>
  <trd lng="de">Watte</trd>
  <trdgrp lng="en">
    <trd>cotton <ind>wool</ind></trd>
  </trdgrp>
  <trd lng="fr">ouate</trd>
  <trd lng="hu">vatta</trd>
  <trd lng="nl">watte</trd>
  <trd lng="pl">wata</trd>
  <trdgrp lng="pt">
   <trd>algod&atilde;o em rama</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_a;&c_t;&c_a;</trd>
</drv>

<drv mrk="vat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vat.0i.HEJM">
    <dif>
      Garni per <tld/>o:
      <ekz>
        <tld/>i veston, banda&gcirc;on.
      </ekz>
    </dif>
    <trd lng="ca">embuatar</trd>
    <trdgrp lng="en">
      <trd>pad</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>enguatar</trd>,
      <trd>acolchar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vatt&aacute;val kit&ouml;m</trd>,
      <trd>kivatt&aacute;z</trd>
    </trdgrp>
    <trd lng="pl">watowa&cacute;</trd>
  </snc>
  <snc mrk="vat.0i.FIG">
    <dif>
      Similigi al <tld/>o.
    </dif>
    <subsnc mrk="vat.0i.stompi">
      <dif>
        Malprecizigi la&ubreve; la formo,
        <ref tip="vid" cel="stomp.0i.FIG">stompi</ref>:
        <ekz>
          la nebulo iom <tld/>as la konturojn.
        </ekz>
      </dif>
      <trdgrp lng="ca">
        <trd>difuminar</trd>,
        <trd>desdibuixar</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="vat.0i.komfortigi">
      <dif>
        <ref tip="vid" cel="mol.0igi.KOMUNE">Moligi</ref>,
        komfortigi, kiel per <tld/>o:
        <ekz>
          <tld/>i sin en komfortigaj revoj
          <fnt><aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>, 
          <lok>1978</lok></fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_v;&c_a;&c_t;&c_a;&c_j;</ind></trd>
  <trd lng="de">wattieren</trd>
  <trd lng="fr">ouater</trd>
  <trd lng="nl">watteren</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_v;&c_a;&c_t;&c_o;&c_j;</ind></trd>,
    <trd>&c_p;&c_o;&c_d;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_v;&c_a;&c_t;&c_o;&c_j;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="vat.suker0o">
  <kap>suker<tld/>o</kap>
  <snc mrk="vat.suker0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif"
      cel="sxpin.suker0ajxo.KUI">Suker&scirc;pina&jcirc;o</ref>:
      <ekz>
        suker<tld/>o estas farita el sukero kaj kolorilo
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="https://eo.wikipedia.org/?curid=208537">Suker&scirc;pina&jcirc;o</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ni haltis &ccirc;erande de vasta verda herbejo kie regatoj de la
        
        A&ubreve;tokrato svarmis <klr>...</klr> man&gcirc;ante
        suker<tld/>on
        <fnt>
          <aut>T. Jansson, trad. S. Johansson</aut>,
          <vrk><url
          ref="http://www.geocities.ws/muminvalo/memor/m5.html">
          Memoroj de Muminpatro</url></vrk>,
          <lok>[2011]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">algod&oacute;n de az&uacute;car</trd>
  <trd lng="fr">barbe-&agrave;-papa</trd>
  <trd lng="it"><ind>zucchero</ind> filato</trd>
</drv>
</art>
