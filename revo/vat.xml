<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vat.xml,v 1.34 2025/10/27 13:03:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vat</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="vat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vat.0o.HEJM">
    <dif>
      <ref tip="vid" cel="koton.0o">Kotono</ref> kombita kaj premita
      la&ubreve; molaj tavoloj (a&ubreve; substanco similaspekta):
      <ekz>
        &scirc;i alportis <tld/>on kaj sorbigis miajn larmojn
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;evaleto farita el pecoj da flava vakstolo, kunkudrita <klr>[&#8230;]</klr> kaj
        plenigita per <tld/>o
        <fnt>
          <bib>HMS</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estis produktita unue de la medicinisto <nom>Paul Victor von
        Bruns</nom> en 1865
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="https://eo.wikipedia.org/?curid=29575">
          Vato (kotono)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la embuskanta senbrueco en la domo subite &scirc;topis liajn
        orelojn kiel <tld/>obuletoj
        <fnt>
          <aut>M. Ende, trad. W. Diestel</aut>,
          <vrk>La Sen&ccirc;esa Rakonto</vrk>,
          <lok>1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        mola <tld/>a litkovrilo formovi&gcirc;is sur la altan bruston
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la dormo envolvas lin per <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="cxarpi.0o">&ccirc;arpio</ref>,
       <ref cel="lan.0o">lano</ref>
    </refgrp>
    <trd lng="es">guata</trd>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: (por eviti samforman sed malsamsignifan vorton) anka&ubreve; eblas diri
    <ke>kotonfloro</ke>.
  </rim>

  <ref tip="hom" cel="vat1.v0o">vato (mezurunuo)</ref>
  <trd lng="be">&c_v;&c_a;&c_t;&c_a;</trd>
  <trd lng="ca">buata</trd>
  <trd lng="de">Watte</trd>
  <trdgrp lng="en">
    <trd>cotton <ind>wool</ind></trd>
  </trdgrp>
  <trd lng="fr">ouate</trd>
  <trd lng="hu">vatta</trd>
  <trd lng="id">kapas</trd>
  <trd lng="ja">&#32191;  <pr>&#12431;&#12383;</pr></trd>
  <trd lng="nl">watte</trd>
  <trd lng="pl">wata</trd>
  <trdgrp lng="pt">
   <trd>algod&atilde;o em rama</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_a;&c_t;&c_a;</trd>
  <trd lng="uk">&c_v;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#32110; <pr>x&ugrave;</pr></trd>,
    <trd>&#26825;&#32110; <pr>mi&aacute;nx&ugrave;</pr></trd>,
    <trd>&#26825;&#33457; <pr>mi&aacute;nhua</pr></trd>,
    <trd>&#33067;&#33026;&#26825; <pr>tu&omacron;zh&imacron;mi&aacute;n</pr></trd>,
    <trd>&#33073;&#33026;&#26825; <pr>tu&omacron;zh&imacron;mi&aacute;n</pr></trd>,
    <trd>&#33647;&#26825; <pr>y&agrave;omi&aacute;n</pr></trd>,
    <trd>&#34277;&#26825; <pr>y&agrave;omi&aacute;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vat.0i.HEJM">
    <dif>
      <ref tip="super" cel="garn.0i.kompletigi">Garni</ref>, moligi,
      remburi per <tld/>o:
      <ekz>
        <tld/>i veston, banda&gcirc;on.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="bandag.0i">banda&gcirc;i</ref>,
       <ref cel="vind.0i">vindi</ref>
    </refgrp>
    <trd lng="ca">embuatar</trd>
    <trdgrp lng="en">
      <trd>pad</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>enguatar</trd>,
      <trd>acolchar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vatt&aacute;val kit&ouml;m</trd>,
      <trd>kivatt&aacute;z</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>isi</baz>mengisi dgn kapas</trd>,
      <trd><baz>lapis</baz>melapisi dgn kapas</trd>,
      <trd><baz>kapas</baz>mengapasi</trd>
    </trdgrp>
    <trd lng="pl">watowa&cacute;</trd>
  </snc>
  <snc mrk="vat.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Similigi al <tld/>o:
    </dif>
    <subsnc mrk="vat.0i.stompi">
      <dif>
        Malprecizigi la&ubreve; la formo,
        <ref tip="vid" cel="stomp.0i.FIG">stompi</ref>:
        <ekz>
          la nebulo iom <tld/>as la konturojn.
        </ekz>
      </dif>
      <trdgrp lng="ca">
        <trd>difuminar</trd>,
        <trd>desdibuixar</trd>
      </trdgrp>
    <trd lng="id"><baz>jadi</baz>menjadi kapas</trd>
    </subsnc>
    <subsnc mrk="vat.0i.komfortigi">
      <dif>
        <ref tip="vid" cel="mol.0igi.KOMUNE">Moligi</ref>,
        komfortigi, kiel per <tld/>o:
        <ekz>
          <tld/>i sin en komfortigaj revoj
          <fnt><aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok></fnt>.
        </ekz>
      </dif>
    <trdgrp lng="id">
      <trd><baz>lunak</baz>melunakkan</trd>,
      <trd><baz>nyaman</baz>menyamankan</trd>
    </trdgrp>
    </subsnc>
  <trd lng="id"><baz>kapas</baz>mengapas</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol;  <ind>&c_v;&c_a;&c_t;&c_a;&c_j;</ind></trd>,
    <trd>&c_v;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_l;&c_a;&c_c;&c_mol; <ind>&c_v;&c_a;&c_t;&c_a;&c_j;</ind></trd>
  </trdgrp>
  <trd lng="de">wattieren</trd>
  <trd lng="fr">ouater</trd>
  <trd lng="ja">&#32191;&#12434;&#20837;&#12428;&#12427;  <pr>&#12417;&#12435;&#12434;&#12356;&#12428;&#12427;</pr></trd>
  <trd lng="nl">watteren</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_v;&c_a;&c_t;&c_o;&c_j;</ind></trd>,
    <trd>&c_p;&c_o;&c_d;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_v;&c_a;&c_t;&c_o;&c_j;</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_i; &c_v;&c_a;&c_t;&c_o;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#29992;&#26825;&#33457;&#22635;&#20805; <pr>y&ograve;ngmi&aacute;nhuati&aacute;nch&omacron;ng</pr></trd>,
    <trd>&#32110;&#26825;&#33457; <pr>x&ugrave;mi&aacute;nhua</pr></trd>,
    <trd>&#32110;&#32110; <pr>x&ugrave;x&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="vat.0umi">
  <kap>
    <tld/>umi
  </kap>
  <snc mrk="vat.0umi.moligi">
    <dif>
      Moligi  per <tld/>o, <ref cel="vat.0i.HEJM">vati<sncref/></ref>:
      <ekz>
        en la silenta kaj peza atmosfero de tiu <tld/>umita &ccirc;ambro ne e&ccirc; sonis la e&hcirc;o de
        &scirc;ia vo&ccirc;o <klr>[&#8230;]</klr> la elasta teksa&jcirc;o repu&scirc;is &scirc;iajn batojn kaj redonis
        neniun sonon
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> revenis rapide sin &scirc;irmi, kiel malfeli&ccirc;a vundita birdino, en la
        koketan kaj <tld/>umitan neston
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="rembur.0i.KOMUNE">remburi</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol;  <ind>&c_v;&c_a;&c_t;&c_a;&c_j;</ind></trd>,
    <trd>&c_v;&c_y;&c_s;&c_mol;&c_c;&c_ib;&c_l;&c_a;&c_c;&c_mol; <ind>&c_v;&c_a;&c_t;&c_a;&c_j;</ind></trd>
  </trdgrp>
  <trd lng="de">auspolstern</trd>
  <trdgrp lng="id">
      <trd><baz>isi</baz>mengisi dgn kapas</trd>,
      <trd><baz>lapis</baz>melapisi dgn kapas</trd>,
      <trd><baz>kapas</baz>mengapasi</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wy&sacute;cie&lstroke;a&cacute;</trd>,
    <trd>wywatowa&cacute;</trd>
  </trdgrp>
</drv>

<drv mrk="vat.suker0o">
  <kap>suker<tld/>o</kap>
  <snc mrk="vat.suker0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="sxpin.suker0ajxo.KUI">Suker&scirc;pina&jcirc;o</ref>:
      <ekz>
        suker<tld/>o estas farita el sukero kaj kolorilo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="https://eo.wikipedia.org/?curid=208537">Suker&scirc;pina&jcirc;o</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ni haltis &ccirc;erande de vasta verda herbejo kie regatoj de la
        A&ubreve;tokrato svarmis <klr>[&#8230;]</klr> man&gcirc;ante
        suker<tld/>on
        <fnt>
          <aut>T. Jansson, trad. S. Johansson</aut>,
          <vrk><url ref="http://www.geocities.ws/muminvalo/memor/m5.html">
          Memoroj de Muminpatro</url></vrk>,
          <lok>[2011]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_u;&c_k;&c_r;&c_o;&c_v;&c_a;&c_ja; <ind>&c_v;&c_a;&c_t;&c_a;</ind></trd>
  <trd lng="es">algod&oacute;n de az&uacute;car</trd>
  <trd lng="fr">barbe-&agrave;-papa</trd>
  <trdgrp lng="id">
    <trd>permen kapas</trd>,
    <trd>permen awan</trd>
  </trdgrp>
  <trd lng="it"><ind>zucchero</ind> filato</trd>
  <trdgrp lng="ja">
    <trd>&#32191;&#12354;&#12417;  <pr>&#12431;&#12383;&#12354;&#12417;</pr></trd>,
    <trd>&#32191;&#33747;&#23376;  <pr>&#12431;&#12383;&#12364;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="pl">wata cukrowa</trd>
  <trd lng="uk">&c_c;&c_u;&c_k;&c_r;&c_o;&c_v;&c_a; &c_v;&c_a;&c_t;&c_a; (&c_s;&c_o;&c_l;&c_o;&c_d;&c_o;&c_shch;&c_ib;)</trd>
  <trd lng="zh">&#26825;&#33457;&#31958; <pr>mi&aacute;nhu&amacron;t&aacute;ng</pr></trd>
</drv>
</art>
