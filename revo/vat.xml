<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vat</rad>/o
</kap>
<drv mrk="vat.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Kotono kombita kaj premita la&ubreve; molaj tavoloj.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_t;&c_a;</trd>
  <trd lng="de">Watte</trd>
  <trdgrp lng="fr">
    <trd>ouate</trd>
  </trdgrp>
  <trd lng="nl">watte</trd>
  <trd lng="ru">&c_v;&c_a;&c_t;&c_a;</trd>
</drv>

<drv mrk="vat.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Garni per <tld/>o:
      <ekz>
        <tld/>i veston, banda&gcirc;on.
      </ekz>
    </dif>
  </snc>
  <snc mrk="vat.0i.FIG">
  <uzo tip="stl">FIG</uzo>
    <dif>
      Malprecizigi la formon de io:
      <ekz>
        la nebulo iom <tld/>as la konturojn.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="stomp.0i">stompi</ref>
  </refgrp>
  <trd lng="be">&c_p;&c_a;&c_d;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol; 
    <ind>&c_v;&c_a;&c_t;&c_a;&c_j;</ind></trd>
  <trd lng="de">wattieren</trd>
  <trdgrp lng="fr">
    <trd>ouater</trd>
  </trdgrp>
  <trd lng="nl">watteren</trd>
</drv>
</art>
