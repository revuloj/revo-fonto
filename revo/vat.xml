<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vat.xml,v 1.28 2024/04/06 02:28:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vat.0o.HEJM">
    <dif>
      <ref tip="vid" cel="koton.0o">Kotono</ref> kombita kaj premita
      la&ubreve; molaj tavoloj (a&ubreve; substanco similaspekta):
      <ekz>
        &scirc;i alportis <tld/>on kaj sorbigis miajn larmojn
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;evaleto farita el pecoj da flava vakstolo, kunkudrita <klr>[&#8230;]</klr> kaj
        plenigita per <tld/>o
        <fnt>
          <bib>HMS</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estis produktita unue de la medicinisto <nom>Paul Victor von
        Bruns</nom> en 1865
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="https://eo.wikipedia.org/?curid=29575">
          Vato (kotono)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la embuskanta senbrueco en la domo subite &scirc;topis liajn
        orelojn kiel <tld/>obuletoj
        <fnt>
          <aut>M. Ende, trad. W. Diestel</aut>,
          <vrk>La Sen&ccirc;esa Rakonto</vrk>,
          <lok>1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        mola <tld/>a litkovrilo formovi&gcirc;is sur la altan bruston
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la dormo envolvas lin per <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="cxarpi.0o">&ccirc;arpio</ref>,
       <ref cel="lan.0o">lano</ref>
    </refgrp>
    <trd lng="es">guata</trd>
  </snc>
  <ref tip="hom" cel="vat1.v0o">vato (mezurunuo)</ref>
  <trd lng="be">&c_v;&c_a;&c_t;&c_a;</trd>
  <trd lng="ca">buata</trd>
  <trd lng="de">Watte</trd>
  <trdgrp lng="en">
    <trd>cotton <ind>wool</ind></trd>
  </trdgrp>
  <trd lng="fr">ouate</trd>
  <trd lng="hu">vatta</trd>
  <trd lng="ja">&#x7DBF;  <pr>&#x308F;&#x305F;</pr></trd>
  <trd lng="nl">watte</trd>
  <trd lng="pl">wata</trd>
  <trdgrp lng="pt">
   <trd>algod&atilde;o em rama</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_a;&c_t;&c_a;</trd>
  <trd lng="uk">&c_v;&c_a;&c_t;&c_a;</trd>
</drv>

<drv mrk="vat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vat.0i.HEJM">
    <dif>
      <ref tip="super" cel="garn.0i.kompletigi">Garni</ref>, moligi,
      remburi per <tld/>o:
      <ekz>
        <tld/>i veston, banda&gcirc;on.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="bandag.0i">banda&gcirc;i</ref>,
       <ref cel="vind.0i">vindi</ref>
    </refgrp>
    <trd lng="ca">embuatar</trd>
    <trdgrp lng="en">
      <trd>pad</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>enguatar</trd>,
      <trd>acolchar</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vatt&aacute;val kit&ouml;m</trd>,
      <trd>kivatt&aacute;z</trd>
    </trdgrp>
    <trd lng="pl">watowa&cacute;</trd>
  </snc>
  <snc mrk="vat.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Similigi al <tld/>o:
    </dif>
    <subsnc mrk="vat.0i.stompi">
      <dif>
        Malprecizigi la&ubreve; la formo,
        <ref tip="vid" cel="stomp.0i.FIG">stompi</ref>:
        <ekz>
          la nebulo iom <tld/>as la konturojn.
        </ekz>
      </dif>
      <trdgrp lng="ca">
        <trd>difuminar</trd>,
        <trd>desdibuixar</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="vat.0i.komfortigi">
      <dif>
        <ref tip="vid" cel="mol.0igi.KOMUNE">Moligi</ref>,
        komfortigi, kiel per <tld/>o:
        <ekz>
          <tld/>i sin en komfortigaj revoj
          <fnt><aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok></fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_b;&c_ib;&c_v;&c_a;&c_c;&c_mol;
    <ind>&c_v;&c_a;&c_t;&c_a;&c_j;</ind></trd>
  <trd lng="de">wattieren</trd>
  <trd lng="fr">ouater</trd>
  <trd lng="ja">&#x7DBF;&#12434;&#20837;&#x308C;&#x308B;  <pr>&#12417;&#12435;&#12434;&#12356;&#x308C;&#x308B;</pr></trd>
  <trd lng="nl">watteren</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_v;&c_a;&c_t;&c_o;&c_j;</ind></trd>,
    <trd>&c_p;&c_o;&c_d;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol;
    <ind>&c_v;&c_a;&c_t;&c_o;&c_j;</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_ib;&c_d;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_i; &c_v;&c_a;&c_t;&#x43E;&#x44E;</trd>
  </trdgrp>
</drv>

<drv mrk="vat.0umi">
  <kap>
    <tld/>umi
  </kap>
  <snc mrk="vat.0umi.moligi">
    <dif>
      Moligi  per <tld/>o, <ref cel="vat.0i.HEJM">vati<sncref/></ref>:
      <ekz>
        en la silenta kaj peza atmosfero de tiu <tld/>umita &ccirc;ambro ne e&ccirc; sonis la e&hcirc;o de
        &scirc;ia vo&ccirc;o <klr>[&#8230;]</klr> la elasta teksa&jcirc;o repu&scirc;is &scirc;iajn batojn kaj redonis
        neniun sonon
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> revenis rapide sin &scirc;irmi, kiel malfeli&ccirc;a vundita birdino, en la
        koketan kaj <tld/>umitan neston
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="rembur.0i.KOMUNE">remburi</ref>
  </snc>
  <trd lng="de">auspolstern</trd>
</drv>

<drv mrk="vat.suker0o">
  <kap>suker<tld/>o</kap>
  <snc mrk="vat.suker0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="sxpin.suker0ajxo.KUI">Suker&scirc;pina&jcirc;o</ref>:
      <ekz>
        suker<tld/>o estas farita el sukero kaj kolorilo
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="https://eo.wikipedia.org/?curid=208537">Suker&scirc;pina&jcirc;o</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ni haltis &ccirc;erande de vasta verda herbejo kie regatoj de la
        A&ubreve;tokrato svarmis <klr>[&#8230;]</klr> man&gcirc;ante
        suker<tld/>on
        <fnt>
          <aut>T. Jansson, trad. S. Johansson</aut>,
          <vrk><url ref="http://www.geocities.ws/muminvalo/memor/m5.html">
          Memoroj de Muminpatro</url></vrk>,
          <lok>[2011]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">algod&oacute;n de az&uacute;car</trd>
  <trd lng="fr">barbe-&agrave;-papa</trd>
  <trd lng="it"><ind>zucchero</ind> filato</trd>
  <trdgrp lng="ja">
    <trd>&#x7DBF;&#12354;&#12417;  <pr>&#x308F;&#x305F;&#12354;&#12417;</pr></trd>,
    <trd>&#x7DBF;&#x83D3;&#x5B50;  <pr>&#x308F;&#x305F;&#x304C;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_c;&c_u;&#x43A;&c_r;&#x43E;&c_v;&c_a; &c_v;&c_a;&c_t;&c_a; (&c_s;&#x43E;&#x43B;&#x43E;&c_d;&#x43E;&c_shch;&c_ib;)</trd>
</drv>
</art>
