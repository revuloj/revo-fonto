    <trd>&c_ib;&c_n;&c_d;&c_u;&c_s;&c_t;&c_r;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_i;&c_n;&c_d;&c_u;&c_s;&c_t;&c_r;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_m;&c_i;&c_sh;&c_l;&c_je;&c_n;&c_o;&c_s;&c_t;</trd>
