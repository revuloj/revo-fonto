<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>industri</rad>/o
</kap>
<drv mrk="indust.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Arto, metio a&ubreve; plej &gcirc;enerale aparta agado per kiu oni 
      produktas ri&ccirc;a&jcirc;ojn a&ubreve; plivalora&jcirc;ojn per 
      transformado de krudaj materioj:
      <ekz>
        la <tld/>o de fero, minoj, libro, ledo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Tuto de la artoj kaj metioj per kiuj oni produktas ri&ccirc;a&jcirc;ojn per
      transformado de krudaj materioj, kontraste kun terkulturo
      kaj komerco:
      <ekz>
        la &scirc;tato devas interakordigi la bezonojn de terkulturo kaj <tld/>o;
      </ekz>
      <ekz>
        lando kun prospera, naski&gcirc;anta <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Industrie</trd>
  <trd lng="en">industry</trd>
  <trd lng="fr">industrie</trd>
  <trd lng="nl">industrie</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_m;&c_y;&c_sh;&c_l;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_i;&c_n;&c_d;&c_u;&c_s;&c_t;&c_r;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="indust.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a ekspozicio, societo, lernejo, laboristo;
      </ekz>
      <ekz>
        <tld/>aj artoj, produktoj.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Havanta fortan <tld/>on:
      <ekz>
        <tld/>a lando, urbo;
      </ekz>
      <ekz>
        <ind><tld/>igo</ind> de l' kolonioj.
        <trd lng="fr">industrialisation</trd>
      </ekz>
    </dif>
  </snc>
  <adm>
  La ekz-o kun ~igo havas nenion por fari cxi tie. Indus
  gxin senpolvigi. <aut>MB</aut>
  </adm>
  <trd lng="de">industriell</trd>
  <trdgrp lng="fr">
    <trd>industriel<klr tip="ind"> (adj.)</klr></trd>,
    <trd>industrialiser</trd></trdgrp> 
  <trd lng="nl">industrieel</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_m;&c_y;&c_sh;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_i;&c_n;&c_d;&c_u;&c_s;&c_t;&c_r;&c_i;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="indust.0isto0ulo">
  <kap><tld/>isto, <tld/>ulo</kap>
  <snc>
    <dif>
      Profesiulo, kiu mastras a&ubreve; direktas <tld/>an aferon.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Industrielle</trd>,
    <trd>Industrieller</trd>
  </trdgrp>
  <trd lng="fr">industriel<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="nl">industrieel</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_m;&c_y;&c_sh;&c_l;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
