<drv mrk="indust.0a">
  <kap><tld/>a</kap>
  <snc mrk="indust.0a.EKON">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a ekspozicio, societo, lernejo, laboristo;
      </ekz>
      <ekz>
        <tld/>aj artoj, produktoj.
      </ekz>
    </dif>
  </snc>
  <snc mrk="indust.0a.industrioza">
    <dif>
      Havanta fortan <tld/>on:
      <ekz>
        <tld/>a lando, urbo;
      </ekz>
      <ekz>
        <ind><tld/>igo</ind> de l' kolonioj.
        <trd lng="ca">industrialitzaci&oacute;</trd>
        <trd lng="es">industrializaci&oacute;n</trd>
        <trd lng="fr">industrialisation</trd>
        <trd lng="hu">iparos&iacute;t&aacute;s</trd>
        <trd lng="id">industrialisasi</trd>
      </ekz>
    </dif>
  </snc>
  <adm>
  La ekz-o kun ~igo havas nenion por fari &ccirc;i tie. Indus
  &gcirc;in senpolvigi. <aut>MB</aut>
  </adm>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_m;&c_y;&c_s;&c_l;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_ib;&c_n;&c_d;&c_u;&c_s;&c_t;&c_r;&c_y;&c_j;&c_n;&c_y;</trd>
  </trdgrp>
