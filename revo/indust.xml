<drv mrk="indust.0a">
  <kap><tld/>a</kap>
  <snc mrk="indust.0a.EKON">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a entrepreno
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a ekspozicio
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj lernejoj
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj progresoj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Oka</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="indust.0a.industrihava">
    <dif>
      Havanta fortan <tld/>on:
      <ekz>
        urbo <klr>[&#8230;]</klr> tre <tld/>a kaj komerca
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_m;&c_y;&c_s;&c_l;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_ib;&c_n;&c_d;&c_u;&c_s;&c_t;&c_r;&c_y;&c_j;&c_n;&c_y;</trd>
