<drv mrk="indust.0igo">
  <kap>
    <tld/>igo
  </kap>
  <snc>
    <dif>
      Transformo de produktado de malgrandkvanta metieja kaj manufaktura al
      grandkvanta fabrika:
      <ekz>
        &scirc;tat<tld/>igo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la ru&gcirc;armeaj veteranoj &scirc;ajnis nekapablaj helpi rapidan <tld/>igon de la lando
        <fnt>
          <bib>EeP</bib>
          <lok>Kvina Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la moderna amasa <tld/>igo de bestobredado ka&ubreve;zis, ke homoj man&gcirc;as &ccirc;iam pli
        multan viandon
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_u;&c_s;&c_t;&c_r;&c_y;&c_ja;&c_l;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">industrialitzaci&oacute;</trd>
  <trd lng="de">Industrialisierung</trd>
  <trd lng="en">industrialization</trd>
  <trd lng="es">industrializaci&oacute;n</trd>
  <trd lng="fr">industrialisation</trd>
  <trd lng="hu">iparos&iacute;t&aacute;s</trd>
  <trd lng="id">industrialisasi</trd>
</drv>

