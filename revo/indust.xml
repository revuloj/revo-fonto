<drv mrk="indust.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="indust.0o.EKON_tuto">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="ekonom1.0o.situacio">ekonomio</ref>
      produktanta grandkvante valora&jcirc;ojn el krudmaterialoj (kontraste kun terkulturo
      kaj komerco):
      <ekz>
        la komerco, la <tld/>o kaj tiel plu malprosperis kiel neniam anta&ubreve;e
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la pasintaj jaroj mirinde multe estis farita en fako de l&#8217; kulturo, <tld/>o
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;tato devas interakordigi la bezonojn de terkulturo kaj
        <tld/>o;
      </ekz>
      <ekz>
        lando kun prospera, naski&gcirc;anta <tld/>o.
      </ekz>
      <ekz>
        <klr>[jen]</klr> amasoj da &scirc;velformaj kaj beldesegnaj tapi&scirc;oj, <klr>[&#8230;]</klr>
        &ccirc;iu estis majstra&jcirc;o de <tld/>o kaj de bongusto
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="indust.0o.EKON_brancxo">
    <dif>
      Aparta <ref tip="super" cel="brancx.0o.FIG">bran&ccirc;o</ref> de <ref
      cel="indust.0o.EKON_tuto"><tld/>o<sncref/></ref>:
      <ekz>
        la <tld/>o de fero, minoj, libro, ledo;
      </ekz>
      <ekz>
        peza <tld/>o, malpeza <tld/>o
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>metal<tld/>o</ind>
        <klr>(vd <ref cel="metalu.0o">metalurgio</ref>)</klr>
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>&Ccirc;e Lev Tolstoj en Jasnaja Poljana</lok>
        </fnt>;
        <trd lng="de">Metallindustrie</trd>
      </ekz>
      <ekz>
        Mi sopiras nostalgie &#11005; Al &ccirc;i tiu nesto, kie &#11005; Kampa vivo kaj ferioj &#11005; Ne jam estas
        <tld/>oj
        <fnt>
          <bib>LSB</bib>
          <lok>Libertempo</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="af">bedryf</trd>
