<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gum</rad>/o
</kap>
<drv mrk="gum.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Glua suko el iaj arboj, kiun oni uzas por glui, apreturi, briligi,
      a&ubreve; kiel dol&ccirc;iga&jcirc;o en bombonoj, pasteloj, kc:
      <ekz>araba, elasta <tld/>o;</ekz>
      <ekz>lak<tld/>o;</ekz>
      <ekz>
        <ind>skrap<tld/>o</ind> <klr>(gumo preparita por forfroti krajonajn
        a&ubreve; inkajn strekojn)</klr>.
        <trd lng="fr">gomme<klr tip="ind"> (pour effacer)</klr></trd>
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="kauxcx.0o">ka&ubreve;&ccirc;uko</ref>,
      <ref cel="rezin.0o">rezino</ref>,
      <ref cel="sirop.0o">siropo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Gummi</trd>,
     <trd>Rohgummi</trd>,
     <trd>Gummiharz</trd></trdgrp>
  <trd lng="fr">gomme</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_z;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_m;&c_je;&c_d;&c_mol;</trd>,
    <trd>&c_g;&c_u;&c_m;&c_m;&c_i;</trd>
  </trdgrp>
</drv>
<drv mrk="gum.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri a&ubreve; densigi per <tld/>o:
      <ekz><tld/>i po&scirc;tmarkon, trinka&jcirc;on.</ekz>
    </dif>
  </snc>
  <trd lng="de">gummieren</trd>
  <trd lng="fr">gommer<klr tip="ind"> (enduire de gomme)</klr></trd>
</drv>
</art>
