<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: omar.xml,v 1.30 2019/07/08 20:10:15 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>omar</rad>/o
</kap>
<drv mrk="omar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> 
       (<trd lng="la">Homarus</trd>) de dekpiedaj
       <ref tip="lst" cel="krust.0ulo" lst="voko:krustuloj">krustuloj</ref>
       kies amba&ubreve; unuaj piedoj estas tre dikaj kaj pin&ccirc;ilformaj:
       <ekz>
         ru&gcirc;a kiel kuirita <tld/>o.
       </ekz>
     </dif>
     <ref tip="vid" cel="palinu.0o">palinuro</ref>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_a;&c_r;</trd>
  <trd lng="bg">&c_o;&c_m;&c_a;&c_r;</trd>
  <trd lng="br">legestr</trd>
  <trdgrp lng="cs">
    <trd>humr</trd>,
    <trd>mo&rcaron;sk&yacute; rak</trd>
  </trdgrp>
  <trd lng="de">Hummer</trd>
  <trd
   lng="el">&alfa;&sigma;&tau;&alfa;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">lobster</trd>
  <trd lng="es">bogavante</trd>
  <trd lng="fr">homard</trd>
  <trd lng="hu">hom&aacute;r</trd>
  <trd lng="id">lobster</trd>
  <trdgrp lng="nl">
     <trd>kreeft<klr tip="ind"> (zee-)</klr></trd>,
     <trd>zeekreeft</trd>
   </trdgrp>
  <trd lng="pl">homar</trd>
  <trd lng="pt">lagosta</trd>
  <trd lng="ru">&c_o;&c_m;&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>hom&aacute;r</trd>,
    <trd>rak <klr>(morsk&yacute;)</klr></trd>
  </trdgrp>
  <trd lng="sv">hummer</trd>
</drv>
</art>
