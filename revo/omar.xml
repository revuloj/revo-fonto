<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>omar</rad>/o
</kap>
<drv mrk="omar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="omar.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Homarus</trd>), kies amba&ubreve; unuaj piedoj
      estas tre dikaj kaj pin&ccirc;ilformaj:
      <ekz>
        ru&gcirc;a kiel kuirita <tld/>o.
      </ekz>
    </dif>
    <ref tip="vid" cel="palinu.0o">palinuro</ref>
  </snc>
  <trd lng="de">Hummer</trd>
  <trd lng="en">lobster</trd>
</drv>
<drv mrk="omar.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="krust.dekpiedaj0uloj">dekpiedaj
      krustuloj</ref> (<trd lng="la">Homaridae</trd>).
    </dif>
  </snc>
</drv>
</art>
