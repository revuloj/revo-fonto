<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>omar</rad>/o
</kap>
<drv mrk="omar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="omared.0oj"><tld/>edoj</ref>
      (<trd lng="la">Homarus</trd>), kies amba&ubreve; unuaj piedoj
      estas tre dikaj kaj pin&ccirc;ilformaj:
      <ekz>
        ru&gcirc;a kiel kuirita <tld/>o.
      </ekz>
    </dif>
    <ref tip="vid" cel="palinu.0o">palinuro</ref>
  </snc>
  <trd lng="de">Hummer</trd>
  <trd lng="el">&alfa;&sigma;&tau;&alfa;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">lobster</trd>
  <trd lng="hu">hom&aacute;r</trd>
  <trdgrp lng="nl">
    <trd>zeekreeft</trd>,
    <trd>kreeft <ind>(zee-)</ind></trd>
  </trdgrp>
  <trd lng="pt">lagosta</trd>
  <trd lng="ru">&c_o;&c_m;&c_a;&c_r;</trd>
  <trd lng="sv">hummer</trd>
</drv>
</art>
