<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: omar.xml,v 1.35 2024/01/30 16:09:26 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>omar</rad>/o
</kap>
<drv mrk="omar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> 
      (<trd lng="la">Homarus</trd>) de dekpiedaj
      <ref tip="lst" cel="krust.0ulo" lst="voko:krustuloj">krustuloj</ref>
      kies amba&ubreve; unuaj piedoj estas tre dikaj kaj pin&ccirc;ilformaj:
      <ekz>
        ru&gcirc;a kiel kuirita <tld/>o;
      </ekz>
      <ekz>
        boligita <tld/>o pli longe restas fre&scirc;a, ol kruda besto
        <fnt>
          <bib>KrM</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj estas rezervitaj por turistoj kaj eksportado
        <fnt>
          <bib>MD11</bib>
          <vrk>Sur la socialisma insulo, omaroj estas sole por turistoj </vrk>
        </fnt>.
      </ekz>
     </dif>
     <ref tip="vid" cel="palinu.0o">palinuro</ref>
    <bld lok="&WCU;/thumb/4/43/Humarus_gammarus_1.jpg/640px-Humarus_gammarus_1.jpg" prm="PD">E&ubreve;ropa <tld/>o (Homarus gammarus), Akvario en Prago, &Ccirc;e&hcirc;ujo
        <fnt>
          <aut>Karelj, propra verko</aut>
          <lok><url ref="&WCW;/File:Humarus_gammarus_1.jpg">&WCW;/File:Humarus_gammarus_1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="hom" cel="omar1.0o">Omaro (nomo)</ref>
  <trd lng="be">&c_a;&c_m;&c_a;&c_r;</trd>
  <trd lng="bg">&c_o;&c_m;&c_a;&c_r;</trd>
  <trd lng="br">legestr</trd>
  <trdgrp lng="cs">
    <trd>humr</trd>,
    <trd>mo&rcaron;sk&yacute; rak</trd>
  </trdgrp>
  <trd lng="de">Hummer</trd>
  <trd lng="el">&alfa;&sigma;&tau;&alfa;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">lobster</trd>
  <trd lng="es">bogavante</trd>
  <trd lng="fr">homard</trd>
  <trd lng="hu">hom&aacute;r</trd>
  <trd lng="id">lobster</trd>
  <trdgrp lng="ja">
    <trd>&#x30A6;&#x30DF;&#x30B6;&#x30EA;&#x30AC;&#x30CB;</trd>,
    <trd>&#x30AA;&#x30DE;&#x30FC;&#x30EB;&#x30A8;&#x30D3;</trd>,
    <trd>&#x30ED;&#x30D6;&#x30B9;&#x30BF;&#x30FC;</trd>
  </trdgrp>
  <trdgrp lng="nl">
     <trd>kreeft<klr tip="ind"> (zee-)</klr></trd>,
     <trd>zeekreeft</trd>
   </trdgrp>
  <trd lng="pl">homar</trd>
  <trd lng="pt">lagosta</trd>
  <trd lng="ru">&c_o;&c_m;&c_a;&c_r;</trd>
  <trdgrp lng="sk">
    <trd>hom&aacute;r</trd>,
    <trd>rak <klr>(morsk&yacute;)</klr></trd>
  </trdgrp>
  <trd lng="sv">hummer</trd>
  <trd lng="uk">&#x43E;&#x43C;&#x430;&#x440;</trd>
  <trdgrp lng="zh">
    <trd>&#x9F8D;&#x8766; <pr>l&#xF3;ngxi&#x101;</pr></trd>,
    <trd>&#x9F99;&#x867E; <pr>l&#xF3;ngxi&#x101;</pr></trd>
  </trdgrp>
</drv>
</art>
