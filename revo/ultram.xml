<art mrk="$Id: ultram.xml,v 1.24 2024/02/20 01:21:48 revo Exp $">
  <kap>
  <rad>ultramar</rad>/o
  <fnt>Z</fnt>
  </kap>
<drv mrk="ultram.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Lazurblua farbo:
      <ekz>
        la sablomonteto, sur kiu Bastjano nun staris estis <tld/>blua
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Goabo, la dezerto de la Koloroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="U"/>a paruo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16036">Blua paruo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/8/8a/Natural_ultramarine_pigment.jpg/320px-Natural_ultramarine_pigment.jpg"
    prm="PD">natura <tld/>a pigmento
        <fnt>
          <lok><url
          ref="&WCW;/File:Natural_ultramarine_pigment.jpg">&WCW;/File:Natural_ultramarine_pigment.jpg</url></lok>
        </fnt>
    </bld>
    <bld lok="&WCU;/thumb/6/67/Ultramarinepigment.jpg/320px-Ultramarinepigment.jpg"
    prm="PD">Sintezita <tld/>a pigmento
        <fnt>
          <lok><url
          ref="&WCW;/File:Ultramarinepigment.jpg">&WCW;/File:Ultramarinepigment.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_u;&c_l;&c_mol;&c_t;&c_r;&c_a;&c_m;&c_a;&c_r;&c_y;&c_n;</trd>
    <trd lng="ca">ultramar<klr> (pigment)</klr></trd>
    <trd lng="de">Ultramarinblau</trd>
    <trd lng="en">ultramarine</trd>
    <trd lng="es"><klr>(azul de) </klr>ultramar</trd>
    <trdgrp lng="fa">
      <trd>&a_A_madda;&a_b;&f_y; &a_A_madda;&a_s;&a_m;&a_A;&a_n;&f_y;</trd>,
      <trd>&a_l;&a_A;&a_j;&a_w;&a_r;&a_d;&f_y;</trd>
    </trdgrp>
    <trd lng="fi">ultramariini</trd>
    <trd lng="fr">outremer</trd>
    <trd lng="hu">ultramarin</trd>
    <trd lng="it"><klr>blu </klr>oltremare</trd>
    <trd lng="nl">ultramarijn</trd>
    <trd lng="pl">ultramaryna</trd>
    <trd lng="pt">ultramar</trd>
    <trd lng="ru">&c_u;&c_l;&c_mol;&c_t;&c_r;&c_a;&c_m;&c_a;&c_r;&c_i;&c_n;</trd>
  </snc>
  <trd lng="cs">ultramar&iacute;n <klr>(barva)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x7FA4;&#38738;  <pr>&#12368;&#12435;&#12376;&#12423;&#12358;</pr></trd>,
    <trd>&#x30A6;&#x30EB;&#x30C8;&#x30E9;&#x30DE;&#x30EA;&#x30F3;</trd>
  </trdgrp>
  <trd lng="sk">ultramar&iacute;n</trd>
  <trd lng="uk">&c_u;&#x43B;&#x44C;&c_t;&c_r;&c_a;&#x43C;&c_a;&c_r;&c_i;&#x43D;</trd>
</drv>
