<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="skit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      Ano de popolo vivinta en la stepo de <ref tip="vid"
      cel="skit.0ujo"><tld lit="S"/>ujo</ref>, proksimume inter la 10a kaj
      la 1a jarcentoj a. K., parte migre, parte fikshejme, parolinta
      hinde&ubreve;ropan lingvon:
      <ekz>
        la <tld lit="S"/>oj ne tro &scirc;atas paroladon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>oj, tiel kruelaj, kiel ha&ubreve;toblankaj
        <fnt><bib>DonKihxoto</bib> <lok>&ccirc;apitro 18a, p. 127a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_ib;&c_f;</trd>
  <trd lng="fr">Scythe<klr> (subst.)</klr></trd>
  <trd lng="de">Scythe</trd>
  <trd lng="hu">szk&iacute;ta</trd>
  <trd lng="ru">&c_s;&c_k;&c_i;&c_f;</trd>
</drv>

<drv mrk="skit.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_ib;&c_f;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">escita</trd>
  <trd lng="fr">scythe<klr> (adj.)</klr></trd>
  <trd lng="de">scythisch</trd>
  <trd lng="es">escita</trd>
  <trdgrp lng="hu">
    <trd>szk&iacute;ta</trd>,
    <trd>szittya</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_i;&c_f;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="skit.0ujo">
  <kap><tld lit="S"/>ujo</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      La lando lo&gcirc;ata siatempe de la <ref tip="vid"
      cel="skit.0o"><tld/>oj</ref>, norde de la Nigra Maro, precipe en
      nunaj Ukrainujo kaj okcidenta Rusujo.
    </dif>
    <ref tip="sin" cel="skiti.0o">Skitio</ref>
  </snc>
  <trd lng="be">&c_S;&c_k;&c_ib;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="fr">Scythie</trd>
  <trd lng="de">Scythien</trd>
  <trdgrp lng="hu">
    <trd>Szk&iacute;tia</trd>,
    <trd>Szittyaf&ouml;ld</trd>
  </trdgrp>
  <trd lng="ru">&c_S;&c_k;&c_i;&c_f;&c_i;&c_ja;</trd>
</drv>
</art>

