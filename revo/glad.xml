<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: glad.xml,v 1.27 2022/03/24 10:41:25 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>glad</rad>/i
</kap>

<drv mrk="glad.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Glatigi per varmega ilo, &gcirc;in prem&scirc;ovante tien k reen
      (sur tukoj, vestoj ktp):
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiel dirante, &scirc;i <tld/>is per la mano la silkan litkovrilon.
      </ekz>
    </dif>
    <ref tip="vid" cel="glat.0igi">glatigi.</ref>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_g;&c_l;&c_a;&c_d;&c_ja;<klr tip="amb"> (&c_s; &c_ju;&c_t;&c_i;&c_ja;)</klr></trd>
  <trd lng="ca">planxar</trd>
  <trd lng="cs">&zcaron;ehlit</trd>
  <trdgrp lng="de">
     <trd>pl&auml;tten</trd>,
     <trd>b&uuml;geln</trd></trdgrp>
  <trd lng="en">to iron</trd>
  <trd lng="es">planchar</trd>
  <trdgrp lng="fr">
    <trd>repasser<klr tip="ind"> (avec un fer)</klr></trd>,
    <trd>lisser</trd>
  </trdgrp>
  <trd lng="hu">vasal</trd>
  <trd lng="nl">strijken</trd>
  <trd lng="pl">prasowa&cacute;<klr> (&zdot;elazkiem)</klr></trd>
  <trdgrp lng="pt">
   <trd>passar a ferro</trd>, 
   <trd>alisar roupa<klr tip="amb"> (com ferro de passar)</klr></trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol;<klr tip="amb"> 
  (&c_u;&c_t;&c_ju;&c_g;&c_o;&c_m;)</klr></trd>
  <trdgrp lng="sk">
    <trd>hladi&tcaron;</trd>,
    <trd>&zcaron;ehli&tcaron;</trd>
  </trdgrp>
  <trd lng="tr">&uuml;t&uuml;lemek</trd>
  <trd lng="uk">&#x43F;&#x440;&#x430;&#x441;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="glad.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Fera ilo por <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;</trd>
  <trd lng="bg">&c_ju;&c_t;&c_i;&c_ja;</trd>
  <trd lng="ca">planxa</trd>
  <trd lng="cs">&zcaron;ehli&ccaron;ka</trd>
  <trdgrp lng="de">
     <trd>B&uuml;geleisen</trd>,
     <trd>Gl&auml;tteisen [svisgermana]</trd></trdgrp>
     <trd lng="en">iron</trd>
  <trd lng="es">plancha</trd>
  <trd lng="fr">fer &agrave; repasser</trd>
  <trd lng="hu">vasal&oacute;</trd>
  <trd lng="nl">strijkijzer</trd>
  <trdgrp lng="pl">
     <trd>&zdot;elazko</trd>,
     <trd>prasowalnica</trd>
  </trdgrp>
  <trd lng="pt">ferro de passar roupa</trd>
  <trd lng="ru">&c_u;&c_t;&c_ju;&c_g;</trd>
  <trd lng="sk">&zcaron;ehli&ccaron;ka</trd>
  <trd lng="tr">&uuml;t&uuml;</trd>
  <trd lng="uk">&#x43F;&#x440;&#x430;&#x441;&#x43A;&#x430;</trd>
</drv>

<drv mrk="glad.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Persono, kies metio estas <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_sh;&c_ch;&c_y;&c_k;</trd>
  <trd lng="ca">planxador</trd>
  <trdgrp lng="en"><trd>presser</trd>, <trd>ironer</trd></trdgrp>
  <trd lng="es">planchador</trd>
  <trd lng="fr">repasseur<klr tip="ind"> (de linge)</klr></trd>  
  <trd lng="hu">vasal&oacute;<klr tip="ind"> (munk&aacute;s)</klr></trd>
  <trd lng="nl">strijker<klr tip="ind"> (van kleren)</klr></trd>
  <trdgrp lng="pl">
     <trd>prasowacz</trd>,
     <trd>prasowaczka</trd>
  </trdgrp>
  <trd lng="pt">passador<klr tip="amb"> (de roupa)</klr></trd>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_d;&c_i;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>
</drv>

<drv mrk="glad.0istino">
  <kap><tld/>istino</kap>
  <snc>
    <dif>
      Virino, kies metio estas <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_s;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_sh;&c_ch;&c_y;&c_c;&c_a;</trd>
  <trd lng="ca">planxadora</trd>
  <trd lng="es">planchadora</trd>
  <trd lng="hu">vasal&oacute;n&odblac;</trd>  
  <trd lng="fr">repasseuse<klr tip="ind"> (de linge)</klr></trd>
  <trd lng="nl">strijkster<klr tip="ind"> (van kleren)</klr></trd>
  <trdgrp lng="pl">
     <trd>prasowaczka</trd>
  </trdgrp>
  <trd lng="pt">passadeira<klr tip="amb"> (de roupa)</klr></trd>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_d;&c_i;&c_l;&c_mol;&c_shch;&c_i;&c_c;&c_a;</trd>
  <trd lng="uk">&#x43F;&#x440;&#x430;&#x441;&#x443;&#x432;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x446;&#x44F;</trd>
</drv>

<drv mrk="glad.ne0enda">
  <kap>ne<tld/>enda</kap> 
  <snc>
      <ref tip="dif" cel="cxif.ne0ebla">ne&ccirc;ifebla</ref>
  </snc>
  <trd lng="ca">no planxable</trd>
  <trd lng="cs">ne&zcaron;ehliv&yacute;</trd>
  <trdgrp lng="en">
    <trd>wrinkle-free</trd>,
    <trd>no-iron</trd>
  </trdgrp>
  <trd lng="es">no planchable</trd>
  <trd lng="fr">infroissable</trd>
  <trd lng="pl">niemn&aogonek;cy</trd>
  <trd lng="sk">ne&zcaron;ehliv&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x44F;&#x43A;&#x438;&#x439; &#x43D;&#x435; &#x43C;&#x43D;&#x435;&#x442;&#x44C;&#x441;&#x44F;</trd>,
    <trd>&#x44F;&#x43A;&#x43E;&#x433;&#x43E; &#x43D;&#x435; &#x442;&#x440;&#x435;&#x431;&#x430; &#x43F;&#x440;&#x430;&#x441;&#x443;&#x432;&#x430;&#x442;&#x438; (&#x43F;&#x440;&#x43E; &#x442;&#x43A;&#x430;&#x43D;&#x438;&#x43D;&#x443;)</trd>
  </trdgrp>
</drv>

</art>
