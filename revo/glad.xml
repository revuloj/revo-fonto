<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>glad</rad>/i
</kap>
<drv mrk="glad.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Glatigi per varmega ilo, &gcirc;in prem&scirc;ovante tien k reen
      (sur tukoj, vestoj ktp):
      (f) <ekz>
        tiel dirante, &scirc;i <tld/>is per la mano la silkan litkovrilon.
       </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>pl&auml;tten</trd>,
     <trd>b&uuml;geln</trd></trdgrp>
  <trd lng="en">iron</trd>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_d;&c_i;&c_t;&c_mol; <klr>(&c_u;&c_t;&c_ju;&c_g;&c_o;&c_m;)</klr></trd>
</drv>
<drv mrk="glad.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Fera ilo por <tld/>i. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>B&uuml;geleisen</trd>,
     <trd>Gl&auml;tteisen [svisgermana]</trd></trdgrp>
  <trd lng="ru">&c_u;&c_t;&c_ju;&c_g;</trd>
</drv>
<drv mrk="glad.0istino">
  <kap><tld/>istino</kap>
  <snc>
    <dif>
      Virino, kies metio estas <tld/>i.  
    </dif>
  </snc>
</drv>
</art>
