<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: obez.xml,v 1.11 2025/04/04 16:57:06 revo Exp $">
<kap>
  <rad>obez</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="obez.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="obez.0a.MED">
    <dif>
      Tro <ref tip="dif" cel="dik.0a">dika</ref>:
      <ekz>
        unu el liaj samtempuloj skribis: &leftquot;mi ekvidis <klr>[&#8230;]</klr> homon 
        grandan, <tld/>an, kun alte levitaj &scirc;ultroj
        <fnt>
          <aut>V. Koti&hcirc;ov, trad. -</aut>,
          <vrk>En ombro...</vrk>,
          <lok><url ref="http://abonu.com/archive/kulturo.mozaiko/200211/29214220.html">
          Art-mozaiko, 2002-11-29, numero 7a</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="magr.0a.maldika">magra<sncref/></ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>trodika</ke>, <ke>dikega</ke>, <ke>superpeza</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_a;&c_t;&c_l;&c_u;&c_s;&c_c;&c_je;&c_l;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_l;&c_u;&c_s;&c_c;&c_je;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="de">fettleibig</trd>
  <trd lng="fr">ob&egrave;se</trd>
  <trd lng="pl">oty&lstroke;y</trd>
  <trd lng="ro">obez</trd>
  <trdgrp lng="uk">
    <trd>&#1086;&c_zh;&c_i;&c_r;&c_ib;&#1083;&c_i;&c_j;</trd>,
    <trd>&c_g;&#1083;&c_a;&c_d;&#1082;&c_i;&c_j;</trd>,
    <trd>&#1086;&#1087;&c_a;&c_s;&c_i;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&#1087;&#1086;&c_v;&#1085;&c_i;&c_j;</trd>,
    <trd>&#1086;&c_g;&c_r;&#1103;&c_d;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_t;&#1086;&c_v;&c_s;&c_t;&c_i;&c_j;</trd>,
    <trd>&#1103;&#1082;&c_i;&c_j; &c_s;&c_t;&c_r;&c_a;&c_zh;&c_d;&c_a;&c_jeu; &#1086;&c_zh;&c_i;&c_r;&c_ib;&#1085;&#1085;&#1103;&#1084;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#32933;&#32982; <pr>f&#233;ip&#224;ng</pr></trd>,
    <trd>&#20415;&#20415; <pr>pi&#225;npi&#225;n</pr></trd>,
    <trd>&#33219;&#32959; <pr>y&#333;ngzh&#466;ng</pr></trd>,
    <trd>&#33219;&#33131; <pr>y&#333;ngzh&#466;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="obez.0o">
  <kap><tld/>o, <var><kap><tld/>eco</kap></var></kap>
  <uzo tip="fak">MED</uzo>
  <snc mrk="obez.0o.MED">
    <dif>
      <ref tip="dif" cel="dik.tro0eco.MED">Trodikeco</ref>:
      <ekz>
        lastatempe lia &gcirc;ena kaj absurda <tld/>o, kiu jam ne permesis al 
        li la&ccirc;i al si la &scirc;uojn, kaj la trosatigo de &ccirc;iaj apetitoj, 
        komencis acidigi lian karakteron
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent 
        Jaroj da Soleco</vrk>, <lok>Fonto, 1992</lok></fnt>;
      </ekz>
      <ekz>
        kuracado de diabeto, <tld/>eco, metabolismo
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010553.php">Topinamburo &ccirc;u herba&ccirc;o?</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        la 22a de majo estas la e&ubreve;ropa tago de <tld/>eco
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2013/011013.php">&Ccirc;u kronika malsano?</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_t;&c_l;&c_u;&c_s;&c_mol;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_l;&c_u;&c_s;&c_mol;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obezita</trd>,
    <trd>otylost</trd>
  </trdgrp>
  <trd lng="de">Fettleibigkeit</trd>
  <trd lng="fr">ob&eacute;sit&eacute;</trd>
  <trd lng="pl">oty&lstroke;o&sacute;&cacute;</trd>
  <trd lng="ro">obezitate</trd>
  <trd lng="sk">obezita</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&#1083;&c_a;&c_d;&#1082;&c_ib;&c_s;&c_t;&#1100;</trd>,
    <trd>&#1086;&#1087;&c_a;&c_s;&c_i;&c_s;&c_t;&c_ib;&c_s;&c_t;&#1100;</trd>,
    <trd>&#1087;&#1086;&c_v;&#1085;&c_ib;&c_s;&c_t;&#1100;</trd>,
    <trd>&#1087;&#1086;&c_v;&#1085;&#1086;&c_t;&c_a;</trd>,
    <trd>&#1086;&c_g;&c_r;&#1103;&c_d;&#1085;&c_ib;&c_s;&c_t;&#1100;</trd>,
    <trd>&#1086;&c_zh;&c_i;&c_r;&c_ib;&#1085;&#1085;&#1103;</trd>
  </trdgrp>
  <trd lng="zh">&#32933;&#32982; <pr>f&#233;ip&#224;ng</pr></trd>
</drv>
</art>

