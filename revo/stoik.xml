<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>stoik</rad>/a
</kap>
<drv mrk="stoik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Apartenanta al la doktrino de la greka filozofo Zenono.
    </dif>
    <trd lng="de">stoisch</trd>
  </snc>
  <snc>
    <dif>
      Spertanta doloron kaj malfeli&ccirc;on kun trankvila senemociemo,
      neskuebla firmanimeco kaj serena sinregeco.
    </dif>
    <refgrp tip="sin">
      <ref cel="flegm">flegma</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>stoisch</trd>,
      <trd>gleichm&uuml;tig</trd>,
      <trd>gelassen</trd>,
      <trd>unersch&uuml;tterlich</trd>
    </trdgrp>
    <trd lng="nl">sto&iuml;cijns</trd>
  </snc>
  <trd lng="ru">&c_s;&c_t;&c_o;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>
