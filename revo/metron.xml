<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: metron.xml,v 1.18 2024/01/25 20:10:36 revo Exp $">
<kap>
   <ofc>2</ofc>
   <rad>metronom</rad>/o
</kap>
<drv mrk="metron.0o">
   <kap>
     <tld/>o
   </kap>
  <snc>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="instru2.0o">Instrumento</ref> kun pendoleto
      por indiki kaj reguligi la rapidecon en la ludo de muziko:
      <ekz>
        la inventa&jcirc;o de <nom>M&auml;lzel</nom> kiu plej impresis la komponiston estis la <tld/>o, kaj li
        verkis rekomendajn leterojn al eldonistoj kaj ekfaris notojn en la partituroj
        kun la <klr>[rapidoj]</klr> de la <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19677">Ludwig van Beethoven</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        por la boneco de cifereca piano pledas <klr>[&#x2026;]</klr> aldona&jcirc;oj kiel
        enkonstruita <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=350976">Elektronika piano</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
   <trd lng="be">&c_m;&c_e;&c_t;&c_r;&c_a;&c_n;&c_o;&c_m;</trd>
   <trd lng="ca">metr&ograve;nom</trd>
   <trd lng="de">Metronom</trd>
   <trd lng="en">metronome</trd>
   <trd lng="es">metr&oacute;nomo</trd>
   <trd lng="hu">metron&oacute;m</trd>
   <trd lng="ja">&#x30E1;&#x30C8;&#x30ED;&#x30CE;&#x30FC;&#x30E0;</trd>
  <trd lng="nl">metronoom</trd>
   <trd lng="fr">m&eacute;tronome</trd>
   <trd lng="pt">metr&ocirc;nomo</trd>
   <trd lng="ru">&c_m;&c_je;&c_t;&c_r;&c_o;&c_n;&c_o;&c_m;</trd>
  <trd lng="uk">&#x43C;&#x435;&#x442;&#x440;&#x43E;&#x43D;&#x43E;&#x43C;</trd>
  <trdgrp lng="zh">
    <trd>&#x7BC0;&#x62CD;&#x5668; <pr>ji&#xE9;p&#x101;iq&#xEC;</pr></trd>,
    <trd>&#x8282;&#x62CD;&#x5668; <pr>ji&#xE9;p&#x101;iq&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
