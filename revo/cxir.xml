<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>&ccirc;ir</rad>/o <fnt><bib>PIV1</bib></fnt>
    </kap>
    <drv mrk="cxir.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">BOT</uzo>
        <dif>
          Maldika spirale volvi&gcirc;anta
          <ref tip="sin" cel="krocx.0ilo.cxiro">kro&ccirc;ilo</ref>
          de grimpa planto, ekz-e de
          <ref cel="vin.0berarbo0berujo">vinberujo</ref>.
        </dif>
        <trd lng="be">&c_v;&c_u;&c_s;&c_ib;&c_k; <klr tip="amb">(&c_u; &c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_y;)</klr></trd>
        <trdgrp lng="en">
          <trd>tendril</trd>,
          <trd>runner</trd>        
        </trdgrp>
        <trd lng="hu">kacs</trd>
        <trd lng="ru">&c_u;&c_s;&c_i;&c_k;
<klr>(&c_b;&c_o;&c_t;.)</klr></trd>
      </snc>
    </drv>
  </art>
