<kap>
  <rad>&ccirc;ir</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="cxir.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Maldika spirale volvi&gcirc;anta
      <ref tip="sin" cel="krocx.0ilo.cxiro">kro&ccirc;ilo</ref>
      de grimpa planto, ekz-e de
      <ref cel="vin.0berarbo">vinberujo</ref>:
      <ekz>
        fabacoj <klr>[&#8230;]</klr>: kelkaj el ili havas <tld/>ojn, ekz. pizoj, vicioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19373">Fabacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fe/Bryonia-dioica_Ranke_0041a.jpg/320px-Bryonia-dioica_Ranke_0041a.jpg"
    prm="CC-BY-SA-3.0"><tld/>o de Ru&gcirc;a brionio (Bryonia dioica)
        <fnt>
          <aut>R&uuml;diger, propra foto</aut>
          <lok><url
          ref="&WCW;/File:Bryonia-dioica_Ranke_0041a.jpg">&WCW;/File:Bryonia-dioica_Ranke_0041a.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_s;&c_ib;&c_k; <klr tip="amb">(&c_u;
  &c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_y;)</klr></trd>
  <trdgrp lng="ca">
    <trd>circell</trd>,
    <trd>cirrus <klr tip="amb">(bot&agrave;nica)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
  <trd>Ranke</trd>,
  <trd>Cirrhus</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tendril</trd>,
    <trd>runner</trd>        
  </trdgrp>
  <trd lng="fr">vrille</trd>
  <trd lng="he">&qof;&nun;&vav;&qof;&nun;&tav;</trd>
  <trd lng="hu">kacs</trd>
  <trd lng="it">viticcio</trd>
  <trd lng="pl">w&aogonek;s<klr tip="amb"> czepny</klr></trd>
  <trd lng="ru">&c_u;&c_s;&c_i;&c_k; <klr>(&c_b;&c_o;&c_t;.)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_t;&c_ib;&c_g;</trd>,
    <trd>&c_v;&c_u;&c_s;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21367;&#39035;&#29366;&#20043;&#29289; <pr>ju&#462;nx&#363;zhu&agrave;ngzh&#299;w&ugrave;</pr></trd>,
    <trd>&#21367;&#39699; <pr>ju&#462;nb&igrave;n</pr></trd>,
    <trd>&#21367;&#20113; <pr>ju&#462;ny&uacute;n</pr></trd>,
    <trd>&#21367;&#39035; <pr>ju&#462;nx&#363;</pr></trd>,
    <trd>&#34067; <pr>m&agrave;n</pr></trd>,
    <trd>&#31824; <pr>t&eacute;ng</pr></trd>,
    <trd>&#34276; <pr>t&eacute;ng</pr></trd>,
    <trd>&#32399;&#32350; <pr>ch&aacute;nr&#462;o</pr></trd>
  </trdgrp>
</drv>
