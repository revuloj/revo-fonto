<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kuman.xml,v 1.8 2024/03/20 12:38:42 revo Exp $">
<kap>
  <rad>kuman</rad>/o
</kap>
<drv mrk="kuman.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc mrk="kuman.0o.HIS">
    <dif>
      Ano de la plej okcidentaj hordoj
      <ref tip="super" cel="kipcxak.0o">kip&ccirc;akaj</ref>,
      kiuj nomadis okcidente de Dono kaj poste, venkite de la
      mongoloj, migris en <ref cel="hungar.H0ujo">Hungarujon</ref>:
      <ekz>
        <klr>[en]</klr> <nom>Ajak</nom> <klr>[&#8230;]</klr> post la tatara invado en 1241 <tld/>oj
        setlis, la unua mencio devenis el 1270
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=157277"
              >Ajak</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>cum&agrave;</trd>,
      <trd>kiptxak</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>cumano <klr tip="amb">(hist.)</klr></trd>,
      <trd>kiptchak <klr tip="amb">(hist.)</klr></trd>,
      <trd>polovetsiano <klr tip="amb">(hist.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_m;&c_a;&c_n;&c_i;&c_n;</trd>,
      <trd>&c_p;&c_o;&c_l;&c_o;&c_v;&c_je;&c_c;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kuman.0o.posteulo">
    <dif>
      Posteulo de la <tld/>oj<sncref ref="kuman.0o.HIS"/>.
    </dif>
    <trd lng="ru">&c_k;&c_u;&c_m;&c_a;&c_n;&c_i;&c_n;</trd>
  </snc>
  <trd lng="be">&c_p;&c_o;&c_l;&c_a;&c_v;&c_je;&c_c;</trd>
  <trd lng="de">Kumane</trd>
  <trd lng="en">Cuman</trd>
  <trd lng="hu">kun</trd>
  <trd lng="pl">Kuman</trd>
  <trd lng="ro">Kuman</trd>
</drv>

</art>

