<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: asembl.xml,v 1.19 2023/10/13 17:08:38 revo Exp $">
   <kap>
     <rad>asembl</rad>/i
   </kap>
   <uzo tip="fak">KOMP</uzo>

<drv mrk="asembl.0i">
   <kap><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Traduki el <tld/>a programlingvo en
       <ref tip="vid" cel="lingv.masxin0o">ma&scirc;inan lingvon</ref>.
     </dif>
     <trd lng="en">to <ind>assemble</ind> <klr>(a
     program)</klr></trd>
     <trd lng="fa">&a_A;&a_s;&a_m;&a_b;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>
     <trd lng="fr">assembler</trd>
     <trd lng="hu">ford&iacute;t<klr tip="ind"> (g&eacute;pi k&oacute;dra)</klr></trd>
     <trd lng="it">assemblare<klr> (inf.)</klr></trd>
     <trdgrp lng="pl">
       <trd>asemblowa&cacute;</trd>,
       <trd><ind>kompilowa&cacute;</ind> kod asemblera</trd>
     </trdgrp>
     <trdgrp lng="ro">
       <trd>asambla</trd>,
       <trd>compila codul de asamblare</trd>
     </trdgrp>
     <trdgrp lng="ru">
       <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol; 
<klr>(&c_s; &c_ja;&c_z;&c_y;&c_k;&c_a; 
&c_a;&c_s;&c_s;&c_je;&c_m;&c_b;&c_l;&c_je;&c_r;&c_a;)</klr></trd>,
       <trd>&c_a;&c_s;&c_s;&c_je;&c_m;&c_b;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
     </trdgrp>
   </snc>
  <trd lng="cs">p&rcaron;elo&zcaron;it do strojov&eacute;ho jazyka</trd>
  <trd lng="de">assemblieren</trd>
  <trd lng="es">compilar<klr> (desde c&oacute;digo fuente a c&oacute;digo m&aacute;quina)</klr></trd>
  <trdgrp lng="he">
    <trd>&lamed;&alef;&samekh;&vav;&fpe;</trd>,
    <trd>&lamed;&kaf;&nun;&samekh;</trd>
  </trdgrp>
  <trd lng="sk">prelo&zcaron;i&tcaron; do strojov&eacute;ho jazyka</trd>
  <trd lng="uk">&c_ib;&#x43D;&c_f;&#x43E;&c_r;&#x43C;.&#xA0;&#x43F;&c_je;&c_r;&c_je;&#x43A;&#x43B;&c_a;&c_d;&c_a;&c_t;&c_i; &#x43D;&c_a;
  &#x43C;&c_a;&c_sh;&c_i;&#x43D;&#x43D;&c_u; &#x43C;&#x43E;&c_v;&c_u;</trd>
</drv>

 <drv mrk="asembl.0ilo">
   <kap><tld/>ilo</kap>
   <snc>
     <dif>Tradukilo el <tld/>a lingvo.
      <ekz>
         kelkaj kalkuloj povas ankora&ubreve; hodia&ubreve; esti transformitaj 
         en pli rapidan kodon per <tld/>ilo, kaj kelkaj
         malaltnivela&jcirc;oj estas pli facile fareblaj en <tld/>a lingvo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9143">Asembla lingvo</url></lok>
        </fnt>.
      </ekz>
     </dif>
     <trd lng="en">assembler</trd>
     <trdgrp lng="es">
       <trd>ensamblador<klr> (programa para traducir c&oacute;digo ensamblador)</klr></trd>,
       <trd>compilador</trd>
     </trdgrp>
     <trdgrp lng="fa">
       <trd>&a_A;&a_s;&a_m;&a_b;&a_l;&a_r;</trd>,
       <trd>&a_h;&a_m;&f_g;&a_dh;&a_A;&a_r;</trd>
     </trdgrp>
     <trd lng="fr">assembleur (traducteur)</trd>
     <trdgrp lng="hu">
       <trd>ford&iacute;t&oacute;<klr tip="ind"> (g&eacute;pi k&oacute;dra)</klr></trd>,
       <trd>assembler</trd>,
     </trdgrp>
     <trdgrp lng="it">
       <trd>assembler</trd>,
       <trd>assemblatore<klr> (inf.)</klr></trd>
      </trdgrp>
      <trd lng="pl">asembler</trd>
      <trd lng="ro">asamblator</trd>
     <trd lng="ru">&c_a;&c_s;&c_s;&c_je;&c_m;&c_b;&c_l;&c_je;&c_r;</trd>
     <trd lng="sk">asembler</trd>
   </snc>
  <trdgrp lng="cs">
    <trd>program na p&rcaron;eklad do stroj. jazyka</trd>,
    <trd>p&rcaron;eklada&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Assembler</trd>
   <trd lng="he">&he;&#x5B7;&resh;&kaf;&#x5BC;&#x5B8;&bet;&#x5B8;&he;</trd>
  <trd lng="ja">&#x30A2;&#x30BB;&#x30F3;&#x30D6;&#x30E9;</trd>
  <trd lng="sk">preklada&ccaron; do stroj. jazyka</trd>
  <trd lng="uk">&c_a;&c_s;&c_je;&#x43C;&c_b;&#x43B;&c_je;&c_r;</trd>
</drv>

<drv mrk="asembl.0a_lingvo">
  <kap><tld/>a lingvo,
    <var><kap><tld/>a kodo</kap></var>
  </kap>
  <snc>
    <dif>
      Simbola ma&scirc;inkodo, programlingvo kies nocioj kongruas kun la
      arkitekturo de komputilo, tamen en kiu, krom la perciferan
      indikon de la komandoj kaj de adresoj, oni rajtas uzi
      mnemonikajn simbolojn:
      <ekz>
        &ccirc;iu procesora arkitekturo havas sian propran ma&scirc;inan lingvon, kaj tial propran <tld/>an
        lingvon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9143">Asembla lingvo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        malkiel en altnivelaj lingvoj ebli&gcirc;as 1-al-1-traduko de <tld/>a
        kodo kaj ma&scirc;inkodo, do komputiloj povas traduki en amba&ubreve; direktoj sen perdi informojn.
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9143">Asembla lingvo</url></lok>
        </fnt>.
      </ekz>
    </dif>
     <trd lng="en">assembler language</trd>
     <trd lng="es">lenguaje ensamblador</trd>
     <trdgrp lng="fa">
       <trd>&a_z;&a_b;&a_A;&a_n; &a_A;&a_s;&a_m;&a_b;&a_l;&f_y;</trd>,
       <trd>&a_z;&a_b;&a_A;&a_n; &a_h;&a_m;&f_g;&a_dh;&a_A;&a_r;&f_y;</trd>
     </trdgrp>
     <trdgrp lng="fr">
       <trd>assembleur (langage)</trd>,
       <trd>langage d'<ind>assemblage</ind></trd></trdgrp>
     <trd lng="hu">asszembler nyelv</trd>
     <trd lng="it">assembly</trd>
     <trd lng="pl">j&eogonek;zyk <ind>asemblera</ind></trd>
     <trd lng="ro">limbaj de  <ind>asamblare</ind></trd>
     <trdgrp lng="ru">
       <trd>&c_ja;&c_z;&c_y;&c_k; 
         <ind>&c_a;&c_s;&c_s;&c_je;&c_m;&c_b;&c_l;&c_je;&c_r;&c_a;</ind></trd>,
       <trd>&c_a;&c_v;&c_t;&c_o;&c_k;&c_o;&c_d;</trd>
     </trdgrp>
     <trd lng="sk">asembler</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>asembler</trd>,
    <trd>jazyk symbolick&yacute;ch adres</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Assemblersprache</trd>,
    <trd>Assemblercode</trd>
  </trdgrp>
  <trd lng="he">&shin;&pe;&tav; &alef;&samekh;&mem;&bet;&lamed;&yod;</trd>
  <trd lng="ja">&#x30A2;&#x30BB;&#x30F3;&#x30D6;&#x30E9;&#x8A00;&#x8A9E;  <pr>&#x30A2;&#x30BB;&#x30F3;&#x30D6;&#x30EA;&#x3052;&#x3093;&#x3054;</pr></trd>
  <trd lng="sk">programovac&iacute; jazyk Assembler</trd>
</drv>
   </art>

