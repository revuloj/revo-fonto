<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap>
       <rad>asembl</rad>/i
     </kap>
     <uzo tip="fak">KOMP</uzo>
     <drv mrk="asembl.0i">
       <kap><tld/>i</kap>
       <gra><vspec>tr</vspec></gra>
       <snc>
         <dif>
           Traduki el <tld/>a programlingvo en
           <ref tip="vid" cel="lingv.0o.masxina">ma&scirc;inan lingvon</ref>.
         </dif>
         <trd lng="en">to <ind>assemble</ind> <klr>(a
         program)</klr></trd>
         <trd lng="fa">&a_A;&a_s;&a_m;&a_b;&a_l; &f_k;&a_r;&a_d;&a_n;</trd>
         <trd lng="fr">assembler</trd>
         <trd lng="hu">ford&iacute;t<klr tip="ind"> (g&eacute;pi k&oacute;dra)</klr></trd>
         <trd lng="it">assemblare<klr> (inf.)</klr></trd>
         <trdgrp lng="ru">
           <trd>&c_t;&c_r;&c_a;&c_n;&c_s;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol; 
<klr>(&c_s; &c_ja;&c_z;&c_y;&c_k;&c_a; 
&c_a;&c_s;&c_s;&c_je;&c_m;&c_b;&c_l;&c_je;&c_r;&c_a;)</klr></trd>,
           <trd>&c_a;&c_s;&c_s;&c_je;&c_m;&c_b;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
         </trdgrp>
       </snc>
  <trd lng="cs">přeložit do strojového jazyka</trd>
  <trd lng="sk">preložiť do strojového jazyka</trd>
</drv>
     <drv mrk="asembl.0ilo">
       <kap><tld/>ilo</kap>
       <snc>
         <dif>Tradukilo el <tld/>a lingvo.</dif>
         <trd lng="en">assembler</trd>
         <trdgrp lng="fa">
           <trd>&a_A;&a_s;&a_m;&a_b;&a_l;&a_r;</trd>,
           <trd>&a_h;&a_m;&f_g;&a_dh;&a_A;&a_r;</trd>
         </trdgrp>
         <trd lng="fr">assembleur (traducteur)</trd>
         <trdgrp lng="hu">
           <trd>ford&iacute;t&oacute;<klr tip="ind"> (g&eacute;pi k&oacute;dra)</klr></trd>,
           <trd>assembler</trd>,
         </trdgrp>
         <trdgrp lng="it">
           <trd>assembler</trd>,
           <trd>assemblatore<klr> (inf.)</klr></trd>
          </trdgrp>
         <trd lng="ru">&c_a;&c_s;&c_s;&c_je;&c_m;&c_b;&c_l;&c_je;&c_r;</trd>
         <trd lng="sk">asembler</trd>
       </snc>
  <trdgrp lng="cs">
    <trd>program na překlad do stroj. jazyka</trd>,
    <trd>překladač</trd>
  </trdgrp>
  <trd lng="sk">prekladač do stroj. jazyka</trd>
</drv>
     <drv mrk="asembl.0a.lingvo">
       <kap><tld/>a lingvo</kap>
       <snc>
         <dif>
           Simbola ma&scirc;inkodo, programlingvo kies nocioj kongruas kun la
           arkitekturo de komputilo, tamen en kiu, krom la perciferan
           indikon de la komandoj kaj de adresoj, oni rajtas uzi
           mnemonikajn simbolojn.
         </dif>
         <trd lng="en">assembler language</trd>
         <trdgrp lng="fa">
           <trd>&a_z;&a_b;&a_A;&a_n; &a_A;&a_s;&a_m;&a_b;&a_l;&f_y;</trd>,
           <trd>&a_z;&a_b;&a_A;&a_n; &a_h;&a_m;&f_g;&a_dh;&a_A;&a_r;&f_y;</trd>
         </trdgrp>
         <trdgrp lng="fr">
           <trd>assembleur (langage)</trd>,
           <trd>langage d'<ind>assemblage</ind></trd></trdgrp>
         <trd lng="hu">asszembler nyelv</trd>
         <trd lng="it">assembly</trd>
         <trdgrp lng="ru">
           <trd>&c_ja;&c_z;&c_y;&c_k; 
<ind>&c_a;&c_s;&c_s;&c_je;&c_m;&c_b;&c_l;&c_je;&c_r;&c_a;</ind></trd>,
           <trd>&c_a;&c_v;&c_t;&c_o;&c_k;&c_o;&c_d;</trd>
         </trdgrp>
         <trd lng="sk">asembler</trd>
       </snc>
  <trdgrp lng="cs">
    <trd>asembler</trd>,
    <trd>jazyk symbolických adres</trd>
  </trdgrp>
  <trd lng="sk">programovací jazyk Assembler</trd>
</drv>
   </art>

