<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>asembl</rad>/i
    </kap>
    <uzo tip="fak">KOMP</uzo>
    <drv mrk="asembl.0i">
      <kap><tld/>i</kap>
      <uzo tip="klr">(tr)</uzo>
      <snc>
        <dif>
          Traduki el <tld/>a programlingvo en
          <ref tip="vid" cel="lingv.0o.masxina">ma&scirc;inan lingvon</ref>.
        </dif>
        <trd lng="en">to <ind>assemble</ind> <klr>(a program)</klr></trd>
        <trdgrp lng="ru">
          <trd>&#x442;&#x440;&#x430;&#x43d;&#x441;&#x43b;&#x438;&#x440;&#x43e;&#x432;&#x430;&#x442;&#x44c; <klr>(&#x441; &#x44f;&#x437;&#x44b;&#x43a;&#x430; &#x430;&#x441;&#x441;&#x435;&#x43c;&#x431;&#x43b;&#x435;&#x440;&#x430;)</klr></trd>,
          <trd>&#x430;&#x441;&#x441;&#x435;&#x43c;&#x431;&#x43b;&#x438;&#x440;&#x43e;&#x432;&#x430;&#x442;&#x44c;</trd>
        </trdgrp>
      </snc>
    </drv>
    <drv mrk="asembl.0ilo">
      <kap><tld/>ilo</kap>
      <snc>
        <dif>Tradukilo el <tld/>a lingvo.</dif>
        <trd lng="en">assembler</trd>
        <trd lng="ru">&#x430;&#x441;&#x441;&#x435;&#x43c;&#x431;&#x43b;&#x435;&#x440;</trd>
      </snc>
    </drv>
    <drv mrk="asembl.0a.lingvo">
      <kap><tld/>a lingvo</kap>
      <snc>
        <dif>
          Simbola ma&scirc;inkodo, programlingvo kies nocioj kongruas kun la
          arkitekturo de komputilo, tamen en kiu, krom la perciferan
          indikon de la komandoj kaj de adresoj, oni rajtas uzi
          mnemonikajn simbolojn.
        </dif>
        <trd lng="en">assembler language</trd>
        <trdgrp lng="ru">
          <trd>&#x44f;&#x437;&#x44b;&#x43a; <ind>&#x430;&#x441;&#x441;&#x435;&#x43c;&#x431;&#x43b;&#x435;&#x440;&#x430;</ind></trd>,
          <trd>&#x430;&#x432;&#x442;&#x43e;&#x43a;&#x43e;&#x434;</trd>
        </trdgrp>
      </snc>
    </drv>
  </art>

