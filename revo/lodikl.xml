<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lodikl.xml,v 1.21 2024/03/10 21:17:55 revo Exp $">
<kap>
  <rad>lodikl</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="lodikl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      &Ccirc;e la <ref tip="malprt" cel="poac.0oj">poacoj</ref>,
      unu el ordinare du preska&ubreve; senkoloraj, skvamformaj
      <ref tip="super" cel="foli.0o.BOT">florfolietoj</ref>,
      kiuj malseki&gcirc;ante malfermas la floron:
      <ekz>
        <tld/>oj permesas fermon kaj malfermon de la glumetoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26106">Poacoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&ccirc;e]</klr> festuko <klr>[&#8230;]</klr> la fina floro estas
        sterila, la aliaj
        floroj estas duseksaj kun 3 stamenoj, 2 <tld/>oj kaj unu ovario
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=316847">Festuko</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="glum.0o">glumo</ref>,
      <ref cel="pale.0o">paleo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_ja;&c_d;&c_y;&c_k;&c_u;&c_l;&c_a;</trd>,
    <trd>&c_k;&c_v;&c_je;&c_t;&c_k;&c_a;&c_v;&c_a;&c_ja; <ind>&c_p;&c_l;&c_je;&c_w;&c_k;&c_a;</ind></trd>
  </trdgrp>
  <trd lng="ca">lod&iacute;cula</trd>
  <trdgrp lng="de">
    <trd>Schwellk&ouml;rper <klr>(Gr&auml;ser)</klr></trd>,
    <trd>Lodicula</trd>
  </trdgrp>
  <trd lng="en">lodicule</trd>
  <trd lng="hu">pikkelylev&eacute;l<klr tip="amb">
(kal&aacute;szon)</klr></trd>
  <trd lng="ja">&#40023;&#34987;  <pr>&#12358;&#12429;&#12371;&#12362;&#12362;</pr></trd>
  <trd lng="nl">bloemdekschub</trd>
  <trd lng="pl">plewka</trd>
  <trdgrp lng="pt">
    <trd>glom&eacute;lula</trd>,
    <trd>lod&iacute;cula</trd>
  </trdgrp>
  <trd lng="ro">glum&abreve;</trd>
  <trd lng="ru">&c_l;&c_o;&c_d;&c_i;&c_k;&c_u;&c_l;&c_a;</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_v;&c_ib;&c_t;&c_k;&c_o;&c_v;&c_a; &c_l;&c_u;&c_s;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_l;&c_ib;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
