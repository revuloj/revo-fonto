<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>lodikl</rad>/o
</kap>
<drv mrk="lodikl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      &Ccirc;e la <ref tip="malprt" cel="po1.0acoj">poacoj</ref>,
      unu el ordinare du preska&ubreve; senkoloraj, skvamformaj
      <ref tip="super" cel="flor.0folio">florfolietoj</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="glum.0o">glumo</ref>,
      <ref cel="pale.0o">paleo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Schwellk&ouml;rper</trd>
</drv>
</art>

