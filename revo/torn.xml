<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>torn</rad>/i
</kap>
<drv mrk="torn.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="torn.0i.trancxi">
    <dif>
      Rondforme prilabori per tran&ccirc;a ilo pecon el ligno, eburo,
      metalo
      fiksitan sur speciala turni&gcirc;anta aparato:
      <ekz>
        <tld/>i globon el ligno<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i belsonan frazon.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>, 
      <trd>&c_v;&c_y;&c_t;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
    <trd>drehen (Metall)</trd>,
    <trd>drechseln (Holz)</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>eszterg&aacute;l</trd>,
      <trd>eszterg&aacute;lyoz</trd>
    </trdgrp>
    <trd lng="pt">tornear</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_ch;&c_i;&c_t;&c_mol; <klr>(&c_n;&c_a;&c_p;&c_r;.
      &c_n;&c_a; &c_t;&c_o;&c_k;&c_a;&c_r;&c_n;&c_o;&c_m;
      &c_s;&c_t;&c_a;&c_n;&c_k;&c_je;)</klr></trd>,
      <trd>&c_v;&c_y;&c_t;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="torn.0i.argilon">
    <dif>
      Rondforme prilabori argilon fiksitan sur speciala turni&gcirc;anta
      plato
      por fari poton, vazon, kru&ccirc;on ks.
    </dif>
    <trd lng="be">&c_l;&c_ja;&c_p;&c_ib;&c_c;&c_mol; <klr
    tip="amb">(&c_n;&c_a; &c_g;&c_a;&c_n;&c_ch;&c_a;&c_r;&c_n;&c_y;&c_m;
    &c_k;&c_r;&c_u;&c_z;&c_je;)</klr></trd>
    <trd lng="de">t&ouml;pfern</trd>
    <trd lng="pl">toczy&cacute; <klr> (lepi&cacute;) garnki</klr></trd>
    <trd lng="ru">&c_l;&c_je;&c_p;&c_i;&c_t;&c_mol; <klr
    tip="amb">(&c_n;&c_a; &c_g;&c_o;&c_n;&c_ch;&c_a;&c_r;&c_n;&c_o;&c_m;
    &c_k;&c_r;&c_u;&c_g;&c_je;)</klr></trd>
  </snc>
  <trd lng="en">turn</trd>
  <trdgrp lng="hu">
    <trd>form&aacute;z <klr tip="amb">(korongon)</klr></trd>,
    <trd>korongol</trd>
  </trdgrp>
  <trd lng="nl">draaien<klr tip="ind"> (op draaibank of
  schijf)</klr></trd>
</drv>

<drv mrk="torn.0ilo">
  <kap>
    <tld/>ilo
    <fnt>
      <bib>PIV</bib>
    </fnt>
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc  mrk="torn.0ilo.trancxi">
    <dif>
      Ma&scirc;ino turni&gcirc;anta fiksitan pecon de iu materialo por
      &gcirc;in <tld/>i.
    </dif>
    <ref tip="vid" cel="frez.0ilo">frezilo</ref>
    <trd lng="en">lathe</trd>
    <trd lng="pl">tokarka</trd>
  </snc>
  <snc  mrk="torn.0ilo.argilo">
    <dif>
      Turni&gcirc;anta aparato konsistanta el plato por <tld/> argilon
      por fari poton, vazon, kru&ccirc;on ks.
    </dif>
    <trd lng="pl">ko&lstroke;o garncarskie</trd>
  </snc>
</drv>

<drv mrk="torn.0masxino">
  <kap><tld/>ma&scirc;ino</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <ref tip="dif" cel="torn.0ilo">tornilo</ref>
    <ref tip="vid" cel="frez.0masxino">frezma&scirc;ino</ref>
    <trd lng="en">lathe</trd>
    <trd lng="nl">draaibank</trd>
    <trd lng="pl">tokarka</trd>
  </snc>
</drv>

<drv mrk="torn.pot0ilo">
  <kap>pot<tld/>ilo</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <ref tip="dif" cel="torn.0ilo.argilo">tornilo</ref>
    <trd lng="pl">ko&lstroke;o garncarskie</trd>
  </snc>
</drv>

<drv mrk="torn.0isto">
  <kap>
    <tld/>isto
    <fnt>
      <bib>PIV</bib>
    </fnt>
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Homo kiu profesie <tld/>as.
    </dif>
    <trd lng="pl">tokarz</trd>
  </snc>
</drv>
</art>
