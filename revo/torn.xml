<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: torn.xml,v 1.26 2022/05/05 17:08:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>torn</rad>/i
</kap>
<drv mrk="torn.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="torn.0i.trancxi">
    <dif>
      Rondforme prilabori per tran&ccirc;a ilo pecon el ligno, eburo,
      metalo fiksitan sur speciala turni&gcirc;anta aparato:
      <ekz>
        &scirc;i rigardis la sennombrajn malgrandajn &scirc;tonojn sur la bordo, la akvo per
        konstanta <tld/>ado donis al ili &ccirc;iuj formon globan
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        senmove staris la cikonio-patro <klr>[&#x2026;]</klr>, oni povus pensi, ke li estas
        <tld/>ita el ligno
        <fnt>
          <bib>Fab1</bib>
          <lok>Cikonioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        el mahagona ligno <klr>[&#x2026;li]</klr> la turbon <klr>[&#x2026;]</klr> tornis, li posedas
        propran <tld/>ostablon
        <fnt>
          <bib>Fab2</bib>
          <lok>Gefian&ccirc;oj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i belsonan frazon.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>, 
      <trd>&c_v;&c_y;&c_t;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>drehen (Metall)</trd>,
      <trd>drechseln (Holz)</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>turn<klr> (on a lathe)</klr></trd>,
      <trd>lathe</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_t;&a_r;&a_A;&a_sh;&f_k;&a_A;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
      <trd>&a_kh;&a_r;&a_A;&a_T;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
    </trdgrp>
    <trd lng="fr">tourner</trd>
    <trdgrp lng="hu">
      <trd>eszterg&aacute;l</trd>,
      <trd>eszterg&aacute;lyoz</trd>
    </trdgrp>
    <trd lng="pl">toczy&cacute;</trd>
    <trd lng="pt">tornear</trd>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_ch;&c_i;&c_t;&c_mol; <klr>(&c_n;&c_a;&c_p;&c_r;.
      &c_n;&c_a; &c_t;&c_o;&c_k;&c_a;&c_r;&c_n;&c_o;&c_m; &c_s;&c_t;&c_a;&c_n;&c_k;&c_je;)</klr></trd>,
      <trd>&c_v;&c_y;&c_t;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="torn.0i.argilon">
    <dif>
      Rondforme prilabori argilon fiksitan sur speciala turni&gcirc;anta
      plato por fari poton, vazon, kru&ccirc;on ks.
    </dif>
    <trd lng="be">&c_l;&c_ja;&c_p;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_g;&c_a;&c_n;&c_ch;&c_a;&c_r;&c_n;&c_y;&c_m; &c_k;&c_r;&c_u;&c_z;&c_je;)</klr></trd>
    <trd lng="de">t&ouml;pfern</trd>
    <trd lng="en">turn<klr> (on a potter's wheel)</klr></trd>
    <trd lng="fa">&a_s;&a_f;&a_A;&a_l;&zwnj;&f_g;&a_r;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
    <trd lng="pl">toczy&cacute; <klr> (lepi&cacute;) garnki</klr></trd>
    <trd lng="ru">&c_l;&c_je;&c_p;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_g;&c_o;&c_n;&c_ch;&c_a;&c_r;&c_n;&c_o;&c_m; &c_k;&c_r;&c_u;&c_g;&c_je;)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>soustruhovat</trd>,
    <trd>soustru&zcaron;it</trd>,
    <trd>vyto&ccaron;it na hrn&ccaron;&iacute;&rcaron;sk&eacute;m kruhu</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>form&aacute;z <klr tip="amb">(korongon)</klr></trd>,
    <trd>korongol</trd>
  </trdgrp>
  <trd lng="nl">draaien<klr tip="ind"> (op draaibank of
  schijf)</klr></trd>
  <trdgrp lng="sk">
    <trd>2.opracova&tcaron; hlinu na hrn&ccaron;iarskom kruhu</trd>,
    <trd>s&uacute;stru&zcaron;i&tcaron;</trd>,
    <trd>to&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x442;&#x43E;&#x447;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x432;&#x438;&#x442;&#x43E;&#x447;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="torn.0ilo">
  <kap>
    <tld/>ilo,
    <var><kap><tld/>ma&scirc;ino</kap></var>
    <fnt>
      <bib>PIV1</bib>
    </fnt>
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc mrk="torn.0ilo.trancxi">
    <dif>
      Ma&scirc;ino turnanta fiksitan pecon de iu materialo por
      &gcirc;in <tld/>i:
      <ekz>
        li prenis la kapojn, metis ilin en la <tld/>ilon kaj rondigis
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fabelo pri iu, kiu migris por ekkoni timon</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="frez.0ilo">frezilo</ref>
    <trdgrp lng="de">
      <trd>Drehbank</trd>,
      <trd>Drechselbank</trd>
    </trdgrp>
    <trd lng="en">lathe</trd>
    <trd lng="fa">&a_d;&a_s;&a_t;&f_g;&a_A;&a_h; &a_t;&a_r;&a_A;&a_sh;</trd>
    <trd lng="fr">tour</trd>
    <trd lng="pl">tokarka</trd>
  </snc>
  <snc mrk="torn.0ilo.argilo">
    <dif>
      Turni&gcirc;anta aparato konsistanta el plato por (mane) <tld/>i argilon
      en formon de poto, vazo, kru&ccirc;o ks.
    </dif>
    <trd lng="de">T&ouml;pferscheibe</trd>
    <trd lng="en">potter's wheel</trd>
    <trd lng="fa">&f_ch;&a_r;&a_kh; &a_s;&a_f;&a_A;&a_l;&zwnj;&f_g;&a_r;&f_y;</trd>
    <trd lng="pl">ko&lstroke;o garncarskie</trd>
  </snc>

  <trd lng="uk">&#x442;&#x43E;&#x43A;&#x430;&#x440;&#x43D;&#x438;&#x439; &#x432;&#x435;&#x440;&#x441;&#x442;&#x430;&#x442;</trd>
</drv>


<drv mrk="torn.pot0ilo">
  <kap>pot<tld/>ilo</kap>
  <snc>
  <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="dif" cel="torn.0ilo.argilo">Tornilo<sncref/></ref>:
      <ekz>
        tieaj ceramika&jcirc;oj estis jam produktataj sur la pot<tld/>ilo
        <fnt>
          <vrk><url ref="&Viki;/%C4%8Cesk%C3%A9_Lhotice">&Ccaron;esk&eacute;
          Lhotice</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">T&ouml;pferscheibe</trd>
  <trd lng="en">potter's wheel</trd>
  <trd lng="fa">&f_ch;&a_r;&a_kh; &a_s;&a_f;&a_A;&a_l;&zwnj;&f_g;&a_r;&f_y;</trd>
  <trd lng="pl">ko&lstroke;o garncarskie</trd>
  <trd lng="cs">hrn&ccaron;&iacute;&rcaron;sk&yacute; kruh</trd>
  <trd lng="sk">hrn&ccaron;iarsky kruh</trd>
</drv>

<drv mrk="torn.0isto">
  <kap>
    <tld/>isto <fnt><bib>ElektFab</bib></fnt>
  </kap>
  <snc mrk="torn.0isto.metiisto">
    <dif>
      <ref tip="lst" lst="voko:metioj" cel="meti.0isto">Metiisto</ref>, kiu profesie <tld/>as:
      <ekz>
        la tria filo fari&gcirc;is lernanto de <tld/>isto kaj &ccirc;ar la metio estas malfacila, li
        lernis pli longe ol la fratoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Tablo , la orazeno kaj bastono el sako</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Dreher</trd>,
      <trd>Drechsler</trd>
    </trdgrp>
  </snc>
    <trdgrp lng="en">
      <trd>turner</trd>
    </trdgrp>
    <trd lng="fa">&a_t;&a_r;&a_A;&a_sh;&f_k;&a_A;&a_r;</trd>
    <trd lng="fr">tourneur</trd>
    <trd lng="pl">tokarz</trd>
  <trd lng="uk">&#x442;&#x43E;&#x43A;&#x430;&#x440;</trd>
</drv>
</art>
