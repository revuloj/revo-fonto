<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>torn</rad>/i
</kap>
<drv mrk="torn.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Rondforme prilabori per tran&ccirc;a ilo pecon el ligno, eburo, metalo
      fiksitan sur speciala turni&gcirc;anta aparato:
      <ekz>
        <tld/>i globon el ligno<fnt>B</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i belsonan frazon.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_t;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>, 
      <trd>&c_v;&c_y;&c_t;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">drehen</trd>
    <trdgrp lng="hu">
      <trd>eszterg&aacute;l</trd>,
      <trd>eszterg&aacute;lyoz</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_t;&c_o;&c_ch;&c_i;&c_t;&c_mol; <klr>(&c_n;&c_a;&c_p;&c_r;. &c_n;&c_a; &c_t;&c_o;&c_k;&c_a;&c_r;&c_n;&c_o;&c_m; &c_s;&c_t;&c_a;&c_n;&c_k;&c_je;)</klr></trd>,
      <trd>&c_v;&c_y;&c_t;&c_a;&c_ch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Rondforme prilabori argilon fiksitan sur speciala turni&gcirc;anta plato
      por fari poton, vazon, kru&ccirc;on ks.
    </dif>
    <trd lng="be">&c_l;&c_ja;&c_p;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_g;&c_a;&c_n;&c_ch;&c_a;&c_r;&c_n;&c_y;&c_m; &c_k;&c_r;&c_u;&c_z;&c_je;)</klr></trd>
    <trd lng="de">t&ouml;pfern</trd>
    <trd lng="ru">&c_l;&c_je;&c_p;&c_i;&c_t;&c_mol; <klr tip="amb">(&c_n;&c_a; &c_g;&c_o;&c_n;&c_ch;&c_a;&c_r;&c_n;&c_o;&c_m; &c_k;&c_r;&c_u;&c_g;&c_je;)</klr></trd>
  </snc>
  <trd lng="en">turn</trd>
  <trdgrp lng="hu">
    <trd>form&aacute;z <klr tip="amb">(korongon)</klr></trd>,
    <trd>korongol</trd>
  </trdgrp>
  <trd lng="nl">draaien<klr tip="ind"> (op draaibank of schijf)</klr></trd>
</drv>
</art>
