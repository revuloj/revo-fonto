<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>tegol</rad>/o
</kap>
<drv mrk="tegol.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Diversforma maldika plato el kuirita argilo a&ubreve; cemento, per
      aro da kiuj oni faras
      <ref tip="malprt" cel="tegmen.0o">tegmenton</ref>, fiksante ilin
      tiel, ke ili sin parte interkovras.
    </dif>
    <ref tip="sin" cel="sxind.0o">&scirc;indo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_h;&c_o;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_a;&c_p;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>  
  <trd lng="de">Ziegel</trd>
  <trd lng="fr">tuile</trd>
  <trd lng="hu">tet&odblac;cser&eacute;p</trd>
  <trd lng="nl">dakpan</trd>
  <trd lng="pl">dach&oacute;wka</trd>
  <trdgrp lng="pt">
    <trd>telha</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_je;&c_p;&c_i;&c_c;&c_a;</trd>
</drv>

<drv mrk="tegol.0i">
   <kap><tld/>i</kap>
   <snc mrk="tegol.0i.kovri">
     <uzo tip="fak">KON</uzo>
     <dif>
       Kovri <ref tip="malprt" cel="tegmen.0o">tegmenton</ref> per
       <tld/>oj.
     </dif>
     <trdgrp lng="pl">
       <trd>kry&cacute; <ind>dach&oacute;wk&aogonek;</ind></trd>,
       <trd>k&lstroke;a&sacute;&cacute; <ind>dach&oacute;wki</ind></trd>
     </trdgrp>
   </snc>
   <snc mrk="tegol.0i.arangxi">
     <dif>
       Aran&gcirc;i ion tiel ke la elementoj parte kovras unu la alian
       simile al tegoloj sur tegmento:
       <ekz>
       tegoli fenestrojn <klr>sur komputila ekrano</klr>
        <fnt>
          <vrk>
            <url ref="http://komputeko.net">Komputeko</url>
          </vrk>
        </fnt>.
       </ekz>
     </dif>
     <trdgrp lng="pl">
       <trd>ukada&cacute; na <ind>zak&lstroke;adk&eogonek;</ind></trd>,
       <trd>ukada&cacute; w <ind>kaskad&eogonek;</ind></trd>
     </trdgrp>
   </snc>
</drv>

<drv mrk="tegol.0isto">
   <kap><tld/>isto</kap>
   <snc>
     <uzo tip="fak">KON</uzo>
     <dif>
      Homo, kiu kovras <ref tip="malprt" cel="tegmen.0o">tegmenton</ref>
       per <tld/>oj.
     </dif>
      <ekz>
        Coupeau estas kura&gcirc;a kaj laborema tegolisto, kiu riparas
        tegmentojn
        <fnt>
          <aut>Manuel Halvelik</aut>,
          <vrk>
            <url
            ref="http://cindymckee.com/librejo/GAVARO.pdf">Gavaro</url>
          </vrk>,
          <lok>4.22 Vomkapeco</lok>
       </fnt>;
      </ekz>
     <trdgrp lng="pl">
       <trd>dekarz</trd>
     </trdgrp>
   </snc>
   <ref tip="sin" cel="tegmen.0isto">tegmentisto</ref>
</drv>
</art>
