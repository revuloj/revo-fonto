<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>tegol</rad>/o
</kap>
<drv mrk="tegol.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Diversforma maldika plato el kuirita argilo, per aro da kiuj oni
      faras tegmenton, fiksante ilin tiel, ke ili sin parte interkovras.
    </dif>
    <refgrp tip="sin">
      <ref cel="sxind.0o">&scirc;indo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_h;&c_o;&c_w;&c_k;&c_a;</trd>,
    <trd>&c_ch;&c_a;&c_r;&c_a;&c_p;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>  
  <trd lng="de">Ziegel</trd>
  <trd lng="fr">tuile</trd>
  <trd lng="hu">tet&odblac;cser&eacute;p</trd>
  <trd lng="nl">dakpan</trd>
  <trdgrp lng="pt">
    <trd>telha</trd>
  </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_je;&c_p;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
