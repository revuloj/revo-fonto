<art mrk="$Id: itali.xml,v 1.19 2023/11/19 10:22:14 revo Exp $">
<kap>
  <rad>Itali</rad>/o
</kap>

<drv mrk="itali.I0o">
  <kap><tld/>o</kap>
  <snc mrk="itali.I0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="ital.I0ujo">Italujo</ref>:
      <ekz>
        <tld lit="I"/>o estas lando situanta en suda E&ubreve;ropo  kaj 
        limanta norde al Alpoj
        <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Italio">Italio</url></lok></fnt>;
      </ekz>
      <ekz>
        en <tld/>o <klr>[&#x2026;]</klr> la interplekti&gcirc;o de politiko kaj mafio evidenti&gcirc;as
        preska&ubreve; &ccirc;iutage per nova skandalo
        <fnt>
          <aut>Stefan Maul</aut>
          
          <vrk><url ref="&Monato;/1993/A930802.php">Alarmsignalo el Italio</url></vrk>
          <bib>Monato</bib>
          <lok>1993/08, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        Seborgo havas sep konsulejojn en <tld/>o
        <fnt>
       <aut>Thomas Eccardt</aut>
          <vrk><url ref="&Monato;/2000/006859.php">Urbeto a&ubreve; sendependa lando?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas pretiganta elektro-energian ligon kun Albanio
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/007967.html">La Oka Koridoro</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ib;&c_t;&c_a;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_I;&c_t;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="ca">It&agrave;lia</trd>
  <trd lng="cs">It&aacute;lie</trd>
  <trd lng="da">Italien</trd>
  <trd lng="de">Italien</trd>
  <trd lng="en">Italy</trd>
  <trd lng="es">Italia</trd>
  <trdgrp lng="fr">
    <trd>Italie</trd>,
    <trd>R&eacute;publique italienne</trd>
  </trdgrp>
  <trd lng="fy">Itaalje</trd>
  <trdgrp lng="hu">
    <trd>Olaszorsz&aacute;g</trd>,
    <trd>It&aacute;lia</trd>
  </trdgrp>
  <trd lng="id">Italia</trd>
  <trd lng="io">Italia</trd>
  <trd lng="ja">&#x30A4;&#x30BF;&#x30EA;&#x30A2;  <pr>&#x3044;&#x305F;&#x308A;&#x3042;</pr></trd>
  <trd lng="lat">Italia</trd>
  <trd lng="nl">Itali&euml;</trd>
  <trd lng="no">Italia</trd>
  <trdgrp lng="pl">
    <trd>W&lstroke;ochy</trd>,
    <trd>Italia</trd>
  </trdgrp>
  <trd lng="pt">It&aacute;lia</trd>
  <trd lng="ru">&c_I;&c_t;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">Taliansko</trd>
  <trd lng="tok">ma Italija</trd>
  <trd lng="vo">Litaliy&auml;n</trd>
  <trdgrp lng="zh">
    <trd>&#x4E49;&#x5927;&#x5229; <pr>Y&#xEC;d&#xE0;l&#xEC;</pr></trd>,
    <trd>&#x610F;&#x5927;&#x5229; <pr>Y&#xEC;d&#xE0;l&#xEC;</pr></trd>,
    <trd>&#x4E49;&#x5927;&#x5229; <pr>y&#xEC;d&#xE0;l&#xEC;</pr></trd>
  </trdgrp>
</drv>
