<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: itali.xml,v 1.13 2019/10/12 11:10:21 revo Exp $">
<kap>
  <rad>Itali</rad>/o
</kap>

<drv mrk="itali.I0o">
  <kap><tld/>o</kap>
  <snc mrk="itali.I0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="ital.I0ujo">Italujo</ref>:
      <ekz>
        <tld lit="I"/>o estas lando situanta en suda E&ubreve;ropo  kaj 
        limanta norde al Alpoj
        <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Italio">Italio</url></lok></fnt>;
      </ekz>
      <ekz>
        en <tld/>o <klr>[&#8230;]</klr> la interplekti&gcirc;o de politiko kaj mafio evidenti&gcirc;as
        preska&ubreve; &ccirc;iutage per nova skandalo
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato1/monato-9308-alarmsignalo.txt"
              >El mia vidpunkto</url></vrk>
          <lok>93/08</lok>
        </fnt>;
      </ekz>
      <ekz>
        Seborgo havas sep konsulejojn en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006859.html"
              >Seborgo: Urbeto a&ubreve; sendependa lando?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas pretiganta elektro-energian ligon kun Albanio
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2003/007967.html"
              >La Oka Koridoro</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ib;&c_t;&c_a;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_I;&c_t;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="ca">It&agrave;lia</trd>
  <trd lng="cs">It&aacute;lie</trd>
  <trd lng="da">Italien</trd>
  <trd lng="de">Italien</trd>
  <trd lng="en">Italy</trd>
  <trd lng="es">Italia</trd>
  <trdgrp lng="fr">
    <trd>Italie</trd>,
    <trd>R&eacute;publique italienne</trd>
  </trdgrp>
  <trd lng="fy">Itaalje</trd>
  <trdgrp lng="hu">
    <trd>Olaszorsz&aacute;g</trd>,
    <trd>It&aacute;lia</trd>
  </trdgrp>
  <trd lng="id">Italia</trd>
  <trd lng="io">Italia</trd>
  <trd lng="lat">Italia</trd>
  <trd lng="nl">Itali&euml;</trd>
  <trd lng="no">Italia</trd>
  <trdgrp lng="pl">
    <trd>W&lstroke;ochy</trd>,
    <trd>Italia</trd>
  </trdgrp>
  <trd lng="pt">It&aacute;lia</trd>
  <trd lng="ru">&c_I;&c_t;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="sk">Taliansko</trd>
  <trd lng="tp">ma Italija</trd>
  <trd lng="vo">Litaliy&auml;n</trd>
</drv>
</art>

