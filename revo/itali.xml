<art mrk="$Id$">
<kap>
  <rad>Itali</rad>/o
</kap>

<drv mrk="itali.I0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ekz>
        <tld lit="I"/>o estas lando situanta en suda E&ubreve;ropo  kaj 
        limanta norde al Alpoj.
        <fnt><bib>Viki</bib>, <lok><url 
        ref="http://eo.wikipedia.org/wiki/Italio">Italio</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_I;&c_t;&c_a;&c_l;&c_i;&c_ja;</trd>
  <trd lng="de">Italien</trd>
  <trd lng="en">Italy</trd>
  <trdgrp lng="pl">
    <trd>W&lstroke;ochy</trd>,
    <trd>Italia</trd>
  </trdgrp>
</drv>
</art>

