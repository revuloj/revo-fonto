<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vanad.xml,v 1.19 2024/05/23 12:43:25 revo Exp $">
<kap>
  <rad>vanad</rad>/o
  <fnt><bib>SPV</bib></fnt>,
  <var>
    <kap><rad var="i">vanadi</rad>/o
        <fnt>
          <aut>Manfred Westermayer</aut>
          <vrk><url
          ref="http://www.eventoj.hu/steb/kemio/doug-portmann/nomoj-de-kemiaj-elementoj.htm"
              >International chemical dictionary</url></vrk>
        </fnt>
    </kap>
  </var>
</kap>
<drv mrk="vanad.0o">
  <kap><tld/>o, <var><kap><tld var="i"/>o</kap></var></kap>
  <mlg kod="IUPAC">V</mlg>
  <snc>
  <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="23">elemento</ref>,
      transir<ref tip="super" cel="metal.0o.KEM">metalo</ref> kun
      atomnumero 23, atompezo 50,942:
      <ekz>
        en Burundo trovi&gcirc;as gravaj minejoj de feldspato, kaolino, fosforo, <klr>[&#8230;]</klr>, <tld/>o,
        kalko&scirc;tono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=447">Burundo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        transirmetaloj estas: skandio, titano, <tld/>o, kromo <klr>[&#8230;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4279">Transirmetalo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Daniel Arnon</nom> <klr>[&#8230;]</klr> pruvis ke la <tld var="i"/>o kaj la molibdeno estis
        mikronutra&jcirc;oj absorbitaj de algoj kaj de plantoj, respektive, kaj
        ke ili intervenis en la kresko de tiuj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17885">Fotosintezo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;isnunaj redoks-flu-baterioj tamen enhavas <tld var="i"/>o-salojn kiuj estas multekostaj
        <fnt>
          <vrk><url
          ref="http://esperantaretradio.blogspot.com/2015/10/puraj-baterioj-por-sunenergio.html"
              >Puraj baterioj por sunenergio</url></vrk>
          <lok>Esperanta Retradio, 2015-10-12</lok>
        </fnt>;
      </ekz>
    </dif>
      </snc>
  <trd lng="be">&c_v;&c_a;&c_n;&c_a;&c_d;&c_y;&c_j;</trd>
  <trd lng="bg">&c_v;&c_a;&c_n;&c_a;&c_d;&c_i;&c_j;</trd>
  <trd lng="br">vanadiom</trd>
  <trd lng="ca">vanadi</trd>
  <trd lng="cs">vanad</trd>
  <trd lng="de">Vanadium</trd>
  <trd lng="en">vanadium</trd>
  <trd lng="es">vanadio</trd>
  <trd lng="fa">&a_w;&a_A;&a_n;&a_A;&a_d;&f_y;&a_m;</trd>
  <trd lng="fr">vanadium</trd>
  <trd lng="hu">van&aacute;dium</trd>
  <trd lng="id">vanadium</trd>
  <trd lng="it">vanadio</trd>
  <trd lng="ja">&#12496;&#12490;&#12472;&#12454;&#12512;</trd>
  <trd lng="la">Vanadium</trd>
  <trd lng="nl">vanadium</trd>
  <trd lng="pl">wanad</trd>
  <trd lng="pt">van&aacute;dio</trd>
  <trd lng="ru">&c_v;&c_a;&c_n;&c_a;&c_d;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>van&aacute;d V</trd>,
    <trd>van&aacute;dium</trd>
  </trdgrp>
  <trd lng="sv">vanadin</trd>
  <trd lng="uk">&c_v;&c_a;&#x43D;&c_a;&c_d;&c_ib;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#37353; <pr>f&aacute;n</pr></trd>,
    <trd>&#38034; <pr>f&aacute;n</pr></trd>,
    <trd>&#37895; <pr>f&aacute;n</pr></trd>,
    <trd>&#46349; <pr>f&aacute;n</pr></trd>
  </trdgrp>
</drv>
</art>

