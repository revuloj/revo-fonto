<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>serp</rad>/o
</kap>
<drv mrk="serp.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="falcx.0ileto">fal&ccirc;ileto</ref>
      <ekz>
        iuj ital- kaj hispan-lingvaj trupoj
        disfaldis ru&gcirc;ajn flagojn kun kvinpintaj steloj kaj simboloj de
        martelo kaj <tld/>o<fnt><aut>Ji&rcaron;i Patera</aut>: <vrk>Post la
        kunsido de financistoj en Prago</vrk>, <bib>Monato</bib><lok>jaro
        2001a, numero 1a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sichel</trd>
  <trdgrp lng="fr">
    <trd>faucille</trd>,
    <trd>serpe</trd>
  </trdgrp>
  <trd lng="nl">sikkel</trd>
  <trd lng="pl">sierp</trd>
  <trd lng="ru">&c_s;&c_je;&c_r;&c_p;</trd>
</drv>
</art>
