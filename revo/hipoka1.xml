<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hipoka1.xml,v 1.14 2023/07/04 06:37:07 revo Exp $">
<kap>
  <rad>hipoka&scirc;tanac</rad>/oj,
  <var><kap><rad>hipokastanac</rad>/oj</kap></var>
</kap>
<drv mrk="hipoka1.0oj">
  <kap><tld/>oj,
    <var><kap>hipokastanacoj</kap></var>
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref
      tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      arboj kaj arbedoj (<trd lng="la">Hippocastanaceae</trd>):
      <ekz>
        <ref tip="prt" cel="hipoka.0o">hipoka&scirc;tano</ref> apartenas al la familio <tld/>oj.
      </ekz>
    </dif>
    <rim>
      botanikistoj nuntempe konsideras la genrojn de <tld/>oj kiel subfamilio
      ene de la familio sapindacoj.
    </rim>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_s;&c_k;&c_a;&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;&c_a;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">hipocastan&agrave;cies</trd>
  <trd lng="de">Rosskastaniengew&auml;chse</trd>
  <trd lng="fr">hippocastanac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>vadgesztenyef&eacute;l&eacute;k</trd>,
    <trd>l&oacute;gesztenyef&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="it">ippocastanacee</trd>
  <trd lng="nl">paardekastanjefamilie</trd>
  <trd lng="pt">hipocastan&aacute;ceas</trd>
  <trd lng="ru">&c_k;&c_o;&c_n;&c_s;&c_k;&c_o;&c_k;&c_a;&c_sh;&c_t;&c_a;&c_n;&c_o;&c_v;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="uk">&c_g;&c_ib;&c_r;&#x43A;&#x43E;&#x43A;&c_a;&c_sh;&c_t;&c_a;&#x43D;&#x43E;&c_v;&c_ib;</trd>
</drv>
</art>
