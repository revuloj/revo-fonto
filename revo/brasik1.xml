    </dif>
    <ref tip="sin" cel="krucif.0oj">kruciferoj</ref>
  </snc>
  <trd lng="de">Kreuzbl&uuml;tler</trd>
  <trd lng="fr">crucif&egrave;res</trd>
  <trd lng="sv">korsblommiga v&auml;xter</trd>
