<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brasik1.xml,v 1.13 2022/03/13 11:53:49 revo Exp $">
<kap>
  <rad>brasikac</rad>/oj
</kap>
<drv mrk="brasik1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref> de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Brassicaceae</trd>), kun kvarpetalaj,
      krucformaj floroj kaj kies fruktoj estas <ref tip="prt" cel="silikv.0o">silikvoj</ref>:
      <ekz>
        la silikvo estas dufaka malfermfrukto de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20026">Gu&scirc;o</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <ref tip="prt" cel="vasabi.0o.BOT">vasabio</ref>
        (el <tld/>oj) estas genro de 4 specioj de herboj el orienta Azio
        <klr>[&#x2026;]</klr>, kies radiko estas uzata raspita kiel kondimento, i.a. kun
        sa&scirc;imo kaj bulsu&scirc;io
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=50816">Vasabio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="krucif.0oj">kruciferoj</ref>
    <refgrp tip="prt">
      <ref cel="brasik.0o">brasiko</ref>,
      <ref cel="rafan.0o">rafano</ref>,
      <ref cel="kres.0o">kreso</ref>,
      <ref cel="sinap.0o">sinapo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_y;&c_zh;&c_a;&c_k;&c_v;&c_je;&c_t;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>,
    <trd>&c_k;&c_r;&c_y;&c_zh;&c_y;&c_k;&c_a;&c_v;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>brassic&agrave;cies</trd>,
    <trd>cruc&iacute;feres</trd>
  </trdgrp>
  <trd lng="de">Kreuzbl&uuml;tler</trd>
  <trd lng="fa">&a_sh;&a_b;&zwnj;&a_b;&a_w;&f_y;&a_A;&a_n;</trd>
  <trd lng="fr">crucif&egrave;res</trd>
  <trdgrp lng="hu">
    <trd>keresztesek<klr tip="ind"> (k&aacute;posztaf&eacute;l&eacute;k)</klr></trd>,
    <trd>k&aacute;posztaf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kapustowate</trd>,
    <trd>krzy&zdot;owe</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_s;&c_t;&c_o;&c_c;&c_v;&c_je;&c_t;&c_n;&c_y;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_p;&c_u;&c_s;&c_t;&c_n;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sv">korsblommiga v&auml;xter</trd>
  <trdgrp lng="uk">
    <trd>&#x445;&#x440;&#x435;&#x441;&#x442;&#x43E;&#x446;&#x432;&#x456;&#x442;&#x456;</trd>,
    <trd>&#x43A;&#x430;&#x43F;&#x443;&#x441;&#x442;&#x44F;&#x43D;&#x456;</trd>
  </trdgrp>
</drv>
</art>
