<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: tegu.xml,v 1.5 2024/01/13 23:26:50 afido Exp $">
    <kap>
      <rad>Tegu</rad>/o
    </kap>

<drv mrk="tegu.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Granda <ref tip="lst" lst="voko:urboj" cel="urb.0o.ARKI">urbo</ref>
      en <ref tip="malprt" cel="kore.SudK0ujo">Sud-Koreujo</ref>,
      &ccirc;. 300&#x202F;km sud-sudoriente de <ref cel="seul.0o">Seulo</ref>:
      <ekz>
        <tld/>o la tria plej granda urbo en Sud-Koreio
        <fnt>
          <vrk><url ref="&Viki;/Taeguo">Teguo - Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi estas denaska Oomotano kaj <klr>[...]</klr>
        organizis Esperanto-karabanojn <klr>[...]</klr> al Busano,
        <tld/>o kaj Seulo
        <fnt>
          <aut>Nisinaga Atusi</aut>
          <vrk><url
          ref="https://www.academia.edu/39853054/Esperanto_kiel_pensilo_pri_kultura_diverseco">Novaj
          rimedoj forigos la kontinentan distancon</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <trd lng="be">&c_T;&c_e;&c_g;&c_u;</trd>
    <trd lng="de">Daegu</trd>
    <trd lng="en">Daegu</trd>
    <trd lng="fr">Daegu</trd>
    <trd lng="ko">&#xB300;&#xAD6C;</trd>
    <trd lng="pl">Daegu</trd>
    <trd lng="uk">&c_T;&c_je;&c_gu;&c_u;</trd>
</drv>
  </art>

