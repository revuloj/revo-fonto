  <rad>laringit</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="laring1.0o">
  <kap><tld/>o</kap>
  <snc mrk="laring1.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">Inflamo</ref>
      de la laringo:
      <ekz>
        <tld/>o akuta, kronika.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="inflam.gorgx0o">gor&gcirc;inflamo</ref>
  </refgrp>
  <trd lng="be">&c_l;&c_ja;&c_r;&c_y;&c_n;&c_g;&c_ib;&c_t;</trd>
  <trd lng="ca">laringitis</trd>
  <trdgrp lng="de">
    <trd>Kehlkopfentz&uuml;ndung</trd>,
    <trd>Laryngitis<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="el">&lambda;&alfa;&rho;&ypsilon;&gamma;&gamma;&jota_ton;&tau;&jota;&delta;&alfa;</trd>
  <trd lng="en">laryngitis</trd>
  <trd lng="es">laringitis</trd>
  <trdgrp lng="fr">
    <trd>laryngite</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>laryngitis</trd>,
    <trd>g&eacute;gegyullad&aacute;s</trd>
  </trdgrp>
  <trd lng="id">laringitis</trd>
  <trd lng="ja">&#x5589;&#x982D;&#x708E;  <pr>&#x3053;&#x3046;&#x3068;&#x3046;&#x3048;&#x3093;</pr></trd>
  <trd lng="nl">keelontsteking</trd>
  <trd lng="pl">zapalenie gard&lstroke;a</trd>
  <trd lng="pt">laringite</trd>
  <trdgrp lng="ro">
    <trd>gingivit&abreve;</trd>,
    <trd>faringit&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_a;&c_r;&c_i;&c_n;&c_g;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>laryngit&iacute;da</trd>,
    <trd>z&aacute;pal hrtanu</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>laryngitida</trd>,
    <trd>z&aacute;n&ecaron;t hrtanu</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x430;&#x440;&#x438;&#x43D;&#x433;&#x456;&#x442;</trd>,
    <trd>&#x437;&#x430;&#x43F;&#x430;&#x43B;&#x435;&#x43D;&#x43D;&#x44F; &#x433;&#x43E;&#x440;&#x442;&#x430;&#x43D;&#x456;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5589;&#x708E; <pr>h&#xF3;uy&#xE1;n</pr></trd>,
    <trd>&#x5589;&#x5934;&#x708E; <pr>h&#xF3;ut&#xF3;uy&#xE1;n</pr></trd>,
    <trd>&#x5589;&#x982D;&#x708E; <pr>h&#xF3;ut&#xF3;uy&#xE1;n</pr></trd>
  </trdgrp>
