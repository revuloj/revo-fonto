     <trd>entschlie&szlig;en</trd>,
     <trd>beschlie&szlig;en</trd>
  </trdgrp>
  <trd lng="en">decide</trd>
  <trd lng="es">decidir</trd>
  <trd lng="et">otsustada</trd>
  <trd lng="eu">erabakitzeko</trd>
  <trdgrp lng="fr">
    <trd>arr&ecirc;ter<klr tip="ind"> (une d&eacute;cision)</klr></trd>,
    <trd>d&eacute;cider<klr tip="ind"> (qc &agrave; faire)</klr></trd>,
