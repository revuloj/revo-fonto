  <trd lng="hu">hat&aacute;rozatlan</trd>
  <trd lng="ja">&#x512A;&#x67D4;&#x4E0D;&#x65AD;&#x306E;  <pr>&#12422;&#12358;&#12376;&#12421;&#12358;&#12405;&#12384;&#12435;&#x306E;</pr></trd>
  <trdgrp lng="pl">
