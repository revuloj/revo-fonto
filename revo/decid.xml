      <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; <ind>&c_r;&c_je;&c_sh;&c_i;&c_t;&c_mol;</ind></trd>,
      <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol; &c_p;&c_r;&c_i;&c_n;&c_ja;&c_t;&c_mol; <ind>&c_r;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je;</ind></trd>
    </trdgrp>
