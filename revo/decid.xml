<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>decid</rad>/i
</kap>
<drv mrk="decid.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Definitive fiksi post pripenso opinion, ju&gcirc;on a&ubreve; volon:
      <ekz>
        mi jam de longe tion <tld/>is<fnt>Z</fnt>
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Jesaja 37, 26</lok>)</fnt>;
      </ekz>
      <ekz>
        mi <tld/>is, ke vi <tld/>u<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li <tld/>is translo&gcirc;i&gcirc;i;
      </ekz>
      <ekz>
        oni <tld/>is organizi kurson;
      </ekz>
      <ekz>
        ni <tld/>is ne paroli plu pri iaj reformoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        oni <tld/>is por li la plej facilan rolon <klr>(f)</klr>;
      </ekz>
      <ekz>
        la loto <tld/>os inter vi;
      </ekz>
      <ekz>
        &ccirc;e tiu malsano, unu horo povas <tld/>i inter vivo k
        morto<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>ita loko de &scirc;ia
        sorto<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="solv.0i">solvi</ref>,
      <ref cel="difin.0i">difini</ref>,
      <ref cel="destin.0i">destini</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">rozhodnout</trd>
  <trd lng="de">entscheiden</trd>
  <trd lng="en">decide</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_sh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_n;&c_ja;&c_t;&c_mol; &c_r;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="decid.0igi">
  <kap><tld/>igi</kap>
  <snc num="1">
    <dif>
      Igi <tld/>anta:
      <ekz>
        mi <tld/>igis mian amikon veni en la teatron;
      </ekz>
      <ekz>
        nur monmanko <tld/>igis nin halti.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Igi <tld/>ata:
      <ekz>
        tio <tld/>igis la militon.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="decid.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc num="1">
    <dif>
      I&gcirc;i <tld/>anta; ek<tld/>i:
      <ekz>
        post longa &scirc;anceli&gcirc;o,
        li fine <tld/>i&gcirc;is.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <dif>
      i&gcirc;i <tld/>ata:
      <ekz>
        la entrepreno rapide <tld/>i&gcirc;is.
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
<drv mrk="decid.0a">
  <kap><tld/>(ig)a</kap>
  <snc>
    <dif>
      Estiganta <tld/>on: 
      <ekz>
        <tld/>a batalo, venko, vo&ccirc;o de la prezidanto, 
        pruvo, motivo, cirkonstanco.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">entscheidend</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_r;&c_je;&c_sh;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="decid.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld/>ado a&ubreve; <tld/>a&jcirc;o:
      <ekz>
        fari <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &scirc;an&gcirc;i tian <tld/>on;
      </ekz>
      <ekz>
        ju&gcirc;ista <tld/>o
        (<ref tip="sin" cel="verdik.0o">verdikto</ref>);
      </ekz>
      <ekz>
        &ccirc;iujn siajn argumentojn ili penadis konformigadi 
        al tiu anta&ubreve;farita <tld/>o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        por la <tld/>o de la demando estas kreita
        komitato<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu demando iom post iom per la
        uzado ricevis <tld/>on pli   difinitan<fnt>Z</fnt>; 
      </ekz>
      <ekz>
        mi ne povis <tld/>e (certe) diri, kian formon ni 
        devas uzi<fnt>Z</fnt>; 
      </ekz>
      <ekz>
        li restis kun klinita kapo, sen<tld/>e. 
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Entscheidung</trd>,
    <trd>Entschluss</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>
<drv mrk="decid.antaux0o">
  <kap>anta&ubreve;<tld/>o</kap>
  <snc>
    <dif>
      <tld/>o, kiun oni faris anta&ubreve; &ccirc;ia diskuto a&ubreve;
      pripenso
    </dif>
    <refgrp tip="sin">
      <ref cel="antaux.0jugxo">anta&ubreve;ju&gcirc;o</ref>.
    </refgrp>
  </snc>
</drv>
<drv mrk="decid.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Inklina <tld/>i, kiu facile <tld/>as, sen&scirc;anceli&gcirc;ema:
      <ekz>
        <tld/>emo estas &ccirc;efa kvalito de regnestro
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="energi.0a">energia</ref>,
      <ref cel="ordon.0ema">ordonema</ref>,
      <ref cel="auxtor1.0a">a&ubreve;toritata</ref>,
      <ref cel="dogm">dogmema</ref>.
    </refgrp>
  </snc>
  <trd lng="de">entscheidungsfreudig</trd>
  <trd lng="ru">&c_r;&c_je;&c_sh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
