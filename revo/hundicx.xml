<vortaro>
<art mrk="$Id: hundicx.xml,v 1.3 2020/03/21 09:10:18 revo Exp $">
<kap>
    <rad>hundi&ccirc;</rad>/o
</kap>
<drv mrk="hundicx.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="hundicx.0o.virhundo">
    <dif>
      <klr>(Vd <ref cel="icx.0">-i&ccirc;</ref>)</klr>
      <ref tip="dif" cel="hund.vir0o">Virhundo</ref>:
      <ekz>
        la <tld/>o fine estas eltrovita kaj malliberigita
        <fnt>
          <aut>Andy West</aut>
          <vrk>La Po&scirc;to Dismeo: Dua duono de 2001</vrk>
          <lok>
            <url ref="http://www.andywest.org/eo/numeroj/a01p2.html">25a
            julio 2001</url>
          </lok>
        </fnt>;
      </ekz>
      <ekz>
        tie lo&gcirc;as ilia <tld/>o Botelego kaj pluraj gekokoj
        <fnt>
          <aut>Luiz Portella</aut>
          <vrk>&Scirc;i estas mia amiko</vrk>
          <lok>
            Torixor&eacute;u Libroj, jaro 2005a, &ccirc;apitro 2a, p. 9a
            <url
            ref="https://eo.wikipedia.org/wiki/%C5%9Ci_estas_mia_amiko">La
            familio, la amikoj kaj la strato</url>
          </lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>

