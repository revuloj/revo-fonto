<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: braman.xml,v 1.21 2023/10/14 08:39:48 revo Exp $">
  <kap>
    <ofc>2</ofc>
    <rad>braman</rad>/o
  </kap>
  
  <drv mrk="braman.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Hinda <ref tip="super" cel="pastr.0o.gxenerale">pastro</ref> de Bramao:
      <ekz>
        la <tld/>o ne povis elporti la turmentojn, ricevatajn de sia edzino, kaj foriris
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <adm>
      PIV1 diras Bramo, ne Bramao. Referenco al la kasta sistemo estus
      eble interesa. <aut>MB</aut>
    </adm>
    <trd lng="be">&c_b;&c_r;&c_a;&c_m;&c_ib;&c_n;</trd>
    <trd lng="de">Brahmane</trd>
    <trdgrp lng="en">
      <trd>Brahmin</trd>,
      <trd>Brahman</trd>
    </trdgrp>
    <trd lng="fr">brahmane</trd>
    <trd lng="hu">brahman</trd>
    <trd lng="pl">braman</trd>
    <trd lng="ru">&c_b;&c_r;&c_a;&c_m;&c_i;&c_n;</trd>
    <trd lng="sk">brahman</trd>
  </snc>
  <trd lng="cs">br&aacute;hman</trd>
  <trd lng="es">brahm&aacute;n</trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5E8;&#x5B7;&#x5D4;&#x5DE;&#x5B4;&#x5D9;&#x5DF;</trd>
  <trd lng="ja">&#x30D0;&#x30E9;&#x30E2;&#x30F3;</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x440;&#x430;&#x445;&#x43C;&#x430;&#x43D;</trd>,
    <trd>&#x431;&#x440;&#x430;&#x43C;&#x430;&#x43D;</trd>,
    <trd>&#x431;&#x440;&#x430;&#x43C;&#x456;&#x43D;</trd>
  </trdgrp>
</drv>
  
<drv mrk="braman.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      <ref tip="lst" cel="religi.0o.REL" lst="voko:religioj">Religio</ref> 
      de Bramao.  
    </dif>
    <trd lng="be">&c_b;&c_r;&c_a;&c_m;&c_a;&c_n;&c_ib;&c_z;&c_m;</trd>
    <trd lng="de">Brahmanismus</trd>
    <trdgrp lng="en">
      <trd>Brahmanism</trd>,
      <trd>Brahminism</trd>
    </trdgrp>
    <trd lng="fr">brahmanisme</trd>
    <trd lng="hu">brahmanizmus</trd>
    <trd lng="pl">bramanizm</trd>
    <trd lng="ru">&c_b;&c_r;&c_a;&c_m;&c_a;&c_n;&c_i;&c_z;&c_m;</trd>
    <trd lng="sk">brahmanizmus</trd>
  </snc>
  <trd lng="cs">br&aacute;hmanstv&iacute;</trd>
  <trd lng="es">brahmanismo</trd>
  <trd lng="he">&#x5D1;&#x5E8;&#x5D4;&#x5DE;&#x5E0;&#x5D9;&#x5D6;&#x5DD;</trd>
  <trd lng="ja">&#x30D0;&#x30E9;&#x30E2;&#x30F3;&#x6559;  <pr>&#x30D0;&#x30E9;&#x30E2;&#x30F3;&#x304D;&#x3087;&#x3046;</pr></trd>
  <trd lng="sk">brahmanstvo</trd>
  <trd lng="uk">&#x431;&#x440;&#x430;&#x445;&#x43C;&#x430;&#x43D;&#x456;&#x437;&#x43C;</trd>
</drv>
</art>
