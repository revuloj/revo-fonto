<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: spic.xml,v 1.37 2023/05/10 08:31:03 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>spic</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="spic.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="spic.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Aroma, ordinare vegeta&jcirc;a, substanco destinita por bongustigi
      <ref tip="malprt" cel="mangx.0ajxo">man&gcirc;a&jcirc;ojn</ref>:
      <ekz>
        ili vendadis plej diversajn <tld/>ojn kiel ekzemple
        <ref tip="sub" cel="safran.0o">safranon</ref>,
        <ref tip="sub" cel="aniz.0o">anizon</ref>,
        <ref tip="sub" cel="zingib.0o">zingibron</ref> kaj precipe
        <ref tip="sub" cel="pipr.0o">pipron</ref>
        <fnt><bib>Fab3</bib>, <lok>Historio de unu patrino</lok></fnt>;
      </ekz>
      <ekz>
        aromo de <tld/>oj kaj floroj balda&ubreve; levi&gcirc;is al ili
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        tro da <tld/>o faras la man&gcirc;a&jcirc;on malbongusta
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        al farun' malbonspeca ne helpos la <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_e;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_r;&c_a;&c_s;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_mata;&t_na;&t_tsheg;&t_sa;&t_nata;&t_tsheg;</trd>
  <trd lng="ca">esp&egrave;cia</trd>
  <trd lng="cs">ko&rcaron;en&iacute;</trd>
  <trd lng="de">Gew&uuml;rz</trd>
  <trd lng="en">spice<klr tip="ind"> (noun)</klr></trd>
  <trd lng="es">especia</trd>
  <trd lng="fr">&eacute;pice</trd>
  <trd lng="hu">f&udblac;szer</trd>
  <trd lng="id">bumbu</trd>
  <trdgrp lng="ja">
    <trd>&#39321;&#36763;&#26009; [<ind>&#12371;&#12358;&#12375;&#12435;&#12426;&#12423;&#12358;</ind>]</trd>
    <trd>&#12473;&#12497;&#12452;&#12473; [<ind>&#12377;&#12401;&#12356;&#12377;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>specerij</trd>,
    <trd>kruid</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przypawa</trd>,
    <trd>korzenie <klr>(przyprawa)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_ja;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">korenie</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_p;&c_je;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_ja;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_s;&c_m;&c_a;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="spic.0a">
  <kap><tld/>a</kap>
  <snc mrk="spic.0a.KUI">
    <dif>
      Havanta fortan, precipe akran guston a&ubreve; odoron kiel <tld/>oj:
      <ekz>
        la sova&gcirc;aj kreska&jcirc;oj disvastigadis tiel <tld/>an
        odoron, ke mi devis terni kaj pensi pri kolbaso
        <fnt><bib>Fab3</bib>, <lok>Historio de unu patrino</lok></fnt>;
      </ekz>
      <ekz>
        boteleto da <tld/>a brando estis jam metita en la paka&jcirc;eton
        <fnt><bib>Fab3</bib>, <lok>Historio de unu patrino</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_o;&c_s;&c_t;&c_r;&c_y; <klr>(&c_s;&c_m;&c_a;&c_k;, &c_p;&c_r;&c_y;&c_p;&c_r;&c_a;&c_v;&c_a;)</klr></trd>,
      <trd>&c_p;&c_ib;&c_k;&c_a;&c_n;&c_t;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>especiat</trd>,
      <trd>assaonat</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>spiced</trd>
      <trd>seasoned</trd>
    </trdgrp>
    <trd lng="id">ber<ind>bumbu</ind></trd>
    <trd lng="ja">&#39321;&#36763;&#26009;&#12398;&#20837;&#12387;&#12383; [<ind>&#12371;&#12358;&#12375;&#12435;&#12426;&#12423;&#12358;&#12398;&#12399;&#12356;&#12387;&#12383;</ind>]</trd>
    <trdgrp lng="nl">
      <trd>gekruid</trd>,
      <trd>kruidig</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>wonny</trd>,
      <trd>korzenny</trd>,
      <trd>pikantny</trd>,
      <trd>przyprawny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_ja;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_s;&c_t;&c_r;&c_y;&c_j;<klr tip="amb"> (&c_o; &c_v;&c_k;&c_u;&c_s;&c_je;)</klr></trd>
    </trdgrp>
  </snc>
  <snc mrk="spic.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ekscita, <ref tip="vid" cel="pik.0a.FIG">pika<sncref/></ref>:
      <ekz>
        pri la freneza&jcirc;oj kiujn mi spertis nur kiel iama estrarano de 
        la Esperanto-Societo de Va&scirc;ingtono mi povus verki <tld/>an
        skandalromanon
        <fnt>
          <aut>R. Dumain</aut>,
          <vrk><url ref="http://esperanto.net/literaturo/lkk/mariagruprec.html">
          Maria kaj la Grupo [recenzo]</url></vrk>,
          <lok>La Kancerkliniko, 1992 (64)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gust.0o.karaktero">gusto<sncref/></ref>,
      <ref cel="suk.0a.FIG">suka<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_o;&c_s;&c_t;&c_r;&c_y; <klr>(&c_t;&c_o;&c_n;, &c_v;&c_y;&c_k;&c_a;&c_z;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>,
      <trd>&c_r;&c_e;&c_z;&c_k;&c_ib;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>picant</trd>,
      <trd>morda&ccedil;</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">ko&rcaron;en&ecaron;n&yacute;</trd>
  <trd lng="de">w&uuml;rzig</trd>
  <trdgrp lng="en">
    <trd>spicy<klr tip="ind"> (figuratively)</klr></trd>
    <trd>exciting</trd>
    <trd>tantalizing</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>condimentado</trd>,
    <trd>ali&ntilde;ado</trd>
  </trdgrp>
  <trd lng="fr">&eacute;pic&eacute;</trd>
  <trd lng="sk">korenen&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_ja;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_s;&c_t;&c_r;&c_i;&c_j; (&c_p;&c_r;&c_o; &c_p;&c_r;&c_i;&c_p;&c_r;&c_a;&c_v;&c_u;)</trd>
  </trdgrp>
</drv>

<drv mrk="spic.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="spic.0i.mangxon">
    <dif>
      Aldoni <tld/>ojn:
      <ekz>
        akva supo <tld/>ita per pipro kaj ranca oleo
        <fnt><bib>Fab1</bib>, <lok>Fajrilo</lok></fnt>;
      </ekz>
      <ekz>
        se e&ccirc; la salo sengusti&gcirc;is, per kio &gcirc;i estos <tld/>ata
        <fnt><bib>NT</bib>, <lok>&Luk; 14:34</lok></fnt>;
      </ekz>
      <ekz>
        <ind>ne <tld/>i&gcirc;as man&gcirc;o de mastrina beleco</ind>
        <trd lng="hu">a szak&aacute;csn&odblac; sz&eacute;ps&eacute;g&eacute;t&odblac;l m&eacute;g nem &iacute;zes az
        &eacute;tel</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">condimentar</trd>
    <trdgrp lng="en">
      <trd>spice<klr tip="ind"> (verb)</klr></trd>,
      <trd>season<klr tip="ind"> (verb)</klr></trd>
    </trdgrp>
    <trd lng="ja">&#39321;&#36763;&#26009;&#12434;&#21152;&#12360;&#12427; [<ind>&#12371;&#12358;&#12375;&#12435;&#12426;&#12423;&#12358;&#12434;&#12367;&#12431;&#12360;&#12427;</ind>]</trd>
    <trd lng="nl">kruiden<klr tip="ind"> (ww.)</klr></trd>
    <trdgrp lng="pl">
      <trd>przyprawia&cacute;</trd>,
      <trd>doprawia&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="spic.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fari pli interesa, pli alloga:
      <ekz>
        li tute ne provis, kiel ver&scirc;ajne farus multaj aliaj,
        <tld/>i siajn informojn per aldono de kromaj detaletoj
        <fnt>
          <aut>A. L&ouml;wenstein</aut>,
          <vrk>La &scirc;tona urbo</vrk>,
          <lok>1999</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>engrossir</trd>,
      <trd>refor&ccedil;ar</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>spice</trd>,
      <trd>liven up</trd>,
      <trd>make more exciting</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_p;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_r;&c_a;&c_w;&c_l;&c_ja;&c_c;&c_mol; <klr tip="amb">(&c_je;&c_zh;&c_u;)</klr></trd>
  </trdgrp>
  <trd lng="ca">especiar</trd>
  <trdgrp lng="cs">
    <trd>ko&rcaron;enit</trd>,
    <trd>oko&rcaron;enit <klr>(&ccaron;&iacute;m)</klr></trd>
  </trdgrp>
  <trd lng="de">w&uuml;rzen</trd>
  <trdgrp lng="es">
    <trd>ali&ntilde;ar</trd>,
    <trd>condimentar</trd>,
    <trd>especiar</trd>
  </trdgrp>
  <trd lng="fr">&eacute;picer</trd>
  <trd lng="hu">f&udblac;szerez</trd>
  <trd lng="id">mem<ind>bumbu</ind>i</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_a;&c_b;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">okoreni&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_s;&c_m;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_i; (&c_p;&c_r;&c_ja;&c_n;&c_o;&c_s;&c_t;&c_ja;&c_m;&c_i;)</trd>,
    <trd>&c_z;&c_a;&c_g;&c_o;&c_s;&c_t;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="spic.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <tezrad fak="KUI"/>
  <snc mrk="spic.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Io <tld/>a, miksa&jcirc;o kun <tld/>oj:
      <ekz>
        pastro donis al li oran pokalon, enhavantan vinon fortigitan per <tld/>a&jcirc;oj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kombino de forta <tld/>a&jcirc;o kun sukerita afero
        <fnt>
          <bib>ChC</bib>
          <lok>14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>     
        leteroj de legantoj kutime estas <tld/>a&jcirc;o en gazeto
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2006/009068.php"
              >Leteroj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp>
      <ref cel="mustar.0o">mustardo</ref>,
      <ref cel="kren.0o.mangxo">kreno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_e;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">condiment</trd>
  <trd lng="de">W&uuml;rze</trd>
  <trdgrp lng="en">
    <trd>seasoning</trd>
    <trd>condiment</trd>'
  </trdgrp>
  <trdgrp lng="es">
    <trd>ali&ntilde;o</trd>,
    <trd>condimento</trd>
  </trdgrp>
  <trd lng="fr">condiment</trd>
  <trd lng="ja">&#35519;&#21619;&#26009; [<ind>&#12385;&#12423;&#12358;&#12415;&#12426;&#12423;&#12358;</ind>]</trd>
  <trd lng="nl">kruidenmengsel</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
  <trd lng="uk">&c_p;&c_r;&c_i;&c_p;&c_r;&c_a;&c_v;&c_a;/&c_p;&c_r;&c_i;&c_s;&c_m;&c_a;&c_k;&c_a; &c_z;&c_ib; &c_s;&c_p;&c_je;&c_c;&c_ib;&c_ja;&c_m;&c_i;</trd>
</drv>
</art>
