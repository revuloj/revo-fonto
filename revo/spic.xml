<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>spic</rad>/o
</kap>
<drv mrk="spic.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Aroma vegeta&jcirc;a substanco servanta por bongustigi la man&gcirc;a&jcirc;ojn:
      <ekz>
        al farun' malbonspeca ne helpos la <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Gew&uuml;rz</trd>
  <trd lng="en">spice</trd>
  <trdgrp lng="nl">
    <trd>specerij</trd>,
    <trd>kruid</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_je;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_ja;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="spic.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Apetitige bongustigi per <tld/>o:
      <ekz>
        ne <tld/>i&gcirc;as man&gcirc;o de (per) mastrina beleco
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pliagrabligi:
      <ekz>
        tio <tld/>os la volupton;
      </ekz>
      <ekz>
        ni pasigis agrablajn horojn, <tld/>itajn de bonhumoro kaj gajeco;
      </ekz>
      <ekz>
        <tld/>a esprimo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">w&uuml;rzen</trd>
  <trd lng="nl">kruiden</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_d;&c_a;&c_b;&c_r;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_p;&c_r;&c_a;&c_v;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
