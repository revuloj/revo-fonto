<vortaro>
<art mrk="$Id: askari1.xml,v 1.10 2019/04/05 22:10:18 revo Exp $">
<kap>
  <rad>askarided</rad>/oj
</kap>
<drv mrk="askari1.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="verm.faden0oj">fadenvermoj</ref>
      (<trd lng="la">Ascarididae</trd>):
      <ekz>
        la unua mencio de <ref tip="prt" cel="askari.0o">askarido</ref>
        el <tld/>oj trovi&gcirc;as sur papiruso <nom>Ebers</nom>, skribita je 1540 a.K.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_k;&c_a;&c_r;&c_y;&c_d;&c_y; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">fam&iacute;lia Ascarididae</trd>
  <trd lng="en">roundworms</trd>
  <trd lng="de">Spulw&uuml;rmer</trd>
  <trd lng="hu">ors&oacute;f&eacute;regf&eacute;l&eacute;k</trd>
  <trdgrp lng="it">
    <trd>nematodi</trd>,
    <trd><ind>vermi</ind> cilindrici</trd>
  </trdgrp> 
  <trd lng="ru">&c_a;&c_s;&c_k;&c_a;&c_r;&c_i;&c_d;&c_y; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
</drv>
</art>

