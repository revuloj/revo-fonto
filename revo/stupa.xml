<vortaro>
<art mrk="$Id: stupa.xml,v 1.9 2021/05/22 16:02:26 revo Exp $">
<kap>
  <rad>stupa</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="stupa.0o">
  <kap><tld/>o</kap>
  <snc mrk="stupa.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">BUD</uzo>
    <dif>
      Budaisma kupola <ref tip="super"
      cel="pregx.0ejo">pre&gcirc;ejo</ref>
      ordinare ne enirebla, entenanta relikvojn a&ubreve; sanktajn
      tekstojn,
      anta&ubreve; kiu oni pre&gcirc;as, kaj kiun oni rite
      &ccirc;irka&ubreve;iras:
      <ekz>
        la montodeklivoj estis plene kovritaj de domoj, <tld/>oj kaj
        kavernoj
        <fnt><aut>&Ccirc;ina Radio Internacia</aut>, <vrk>Tie estis la
        antikva
        re&gcirc;lando Guge</vrk>, <lok>2005-03-30</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_u;&c_p;&c_a;</trd>
  <trd lng="bg">&c_s;&c_t;&c_u;&c_p;&c_a;</trd>
  <trd lng="ca">stupa</trd>
  <trdgrp lng="cs">
    <trd>st&uacute;pa</trd>,
    <trd>stupa</trd>
  </trdgrp>
  <trd lng="da">stupa</trd>
  <trd lng="de">Stupa</trd>
  <trd lng="en">stupa</trd>
  <trd lng="es">estupa</trd>
  <trd lng="fa">&a_A;&a_s;&a_t;&a_w;&f_p;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>st&ucirc;pa</trd>,
    <trd>stoupa</trd>
  </trdgrp>
  <trd lng="hi">&#2360;&#2381;&#2340;&#2370;&#2346; [<ind>st&umacron;pa</ind>]</trd>
  <trd lng="hr">stupa</trd>
  <trd lng="hu">szt&uacute;pa</trd>
  <trd lng="it">stupa</trd>
  <trd lng="ne">&#2360;&#2381;&#2340;&#2370;&#2346;</trd>
  <trd lng="nl">stoepa</trd>
  <trd lng="pl">stupa</trd>
  <trd lng="ru">&c_s;&c_t;&c_u;&c_p;&c_a;</trd>
  <trd lng="sk">stupa</trd>
  <trd lng="sr">&c_s;&c_t;&c_u;&c_p;&c_a;</trd>
  <trd lng="sv">stupa</trd>
  <trd lng="th">&#3626;&#3606;&#3641;&#3611;</trd>
</drv>
