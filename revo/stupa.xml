<vortaro>
<art mrk="$Id: stupa.xml,v 1.15 2024/03/25 07:28:37 revo Exp $">
<kap>
  <rad>stupa</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="stupa.0o">
  <kap><tld/>o</kap>
  <snc mrk="stupa.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">BUD</uzo>
    <dif>
      <ref tip="malprt" cel="buda.0ismo">Budaism</ref>a kupola <ref tip="super" cel="pregx.0ejo">pre&gcirc;ejo</ref>
      ordinare ne enirebla, entenanta relikvojn a&ubreve; sanktajn
      tekstojn, anta&ubreve; kiu oni pre&gcirc;as, kaj kiun oni rite
      &ccirc;irka&ubreve;iras:
      <ekz>
        la montodeklivoj estis plene kovritaj de domoj, <tld/>oj kaj
        kavernoj
        <fnt><aut>&Ccirc;ina Radio Internacia</aut>, <vrk>Tie estis la
        antikva re&gcirc;lando Guge</vrk>, <lok>2005-03-30</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        libroj estas niaj obeliskoj kaj piramidoj, niaj pagodoj kaj <tld/>oj
        <fnt>
          <aut>Spomenka &Scaron;timec</aut>
          <vrk>Ombro sur interna pejza&gcirc;o</vrk>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/3/35/Stupa_and_Fuji.jpg/640px-Stupa_and_Fuji.jpg" prm="CC BY 2.5"><tld/>o, fone monto Fuji en Gotemba, Japanujo
        <fnt>
          <lok><url ref="&WCW;/File:Stupa_and_Fuji.jpg">&WCW;/File:Stupa_and_Fuji.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_u;&c_p;&c_a;</trd>
  <trd lng="bg">&c_s;&c_t;&c_u;&c_p;&c_a;</trd>
  <trd lng="bo">&t_ma;&t_cha;&t_naro;&t_da;&t_tsheg;&t_ra;&t_tata;&t_drengbu;&t_na;</trd>
  <trd lng="ca">stupa</trd>
  <trdgrp lng="cs">
    <trd>st&uacute;pa</trd>,
    <trd>stupa</trd>
  </trdgrp>
  <trd lng="da">stupa</trd>
  <trd lng="de">Stupa</trd>
  <trd lng="en">stupa</trd>
  <trd lng="es">estupa</trd>
  <trd lng="fa">&a_A;&a_s;&a_t;&a_w;&f_p;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>st&ucirc;pa</trd>,
    <trd>stoupa</trd>
  </trdgrp>
  <trd lng="hi">&#x938;&#x94D;&#x924;&#x942;&#x92A; [<ind>st&umacron;pa</ind>]</trd>
  <trd lng="hr">stupa</trd>
  <trd lng="hu">szt&uacute;pa</trd>
  <trd lng="it">stupa</trd>
  <trdgrp lng="ja">
    <trd>&#x30B9;&#x30C8;&#x30A5;&#x30FC;&#x30D1;</trd>,
    <trd>&#x5352;&#x5854;&#x5A46;  <pr>&#x305D;&#x3068;&#x3070;</pr></trd>,
    <trd>&#x4ECF;&#x820E;&#x5229;&#x5854;  <pr>&#x3076;&#x3063;&#x3057;&#x3083;&#x308A;&#x3068;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="ne">&#x938;&#x94D;&#x924;&#x942;&#x92A;</trd>
  <trd lng="nl">stoepa</trd>
  <trd lng="pl">stupa</trd>
  <trd lng="ru">&c_s;&c_t;&c_u;&c_p;&c_a;</trd>
  <trd lng="sk">stupa</trd>
  <trd lng="sr">&c_s;&c_t;&c_u;&c_p;&c_a;</trd>
  <trd lng="sv">stupa</trd>
  <trd lng="th">&#xE2A;&#xE16;&#xE39;&#xE1B;</trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x442;&#x443;&#x43F;&#x430;</trd>,
    <trd>&#x434;&#x430;&#x433;&#x43E;&#x431;&#x430;</trd>
  </trdgrp>
  <trd lng="zh">&#x820D;&#x5229;&#x5854; <pr>sh&#xE8;l&#xEC;t&#x1CE;</pr></trd>
</drv>
