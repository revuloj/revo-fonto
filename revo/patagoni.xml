    <trd lng="en">Patagonia</trd>
    <trd lng="fr">Patagonie</trd>
  <trd lng="ru">&c_P;&c_a;&c_t;&c_a;&c_g;&c_o;&c_n;&c_i;&c_ja;</trd>
  </snc>
<trd lng="be">&c_P;&c_a;&c_t;&c_a;&c_g;&c_o;&c_n;&c_ib;&c_ja;</trd>
