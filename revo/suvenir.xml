<kap>
  <rad>suvenir</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="suvenir.0o">
  <kap><tld/>o</kap>
  <snc mrk="suvenir.0o.TUR">
    <dif>
      <ref tip="super" cel="memor.0ajxo.donaco">Memora&jcirc;o</ref>,
      objekto multokaze bagatela, kiun oni reportas el turisma loko:
      <ekz>
        eblos anka&ubreve; akiri amerikajn tipajn <tld/>ojn kaj arta&jcirc;etojn
        <fnt>
          <aut>-</aut>,
          <vrk>Nova&jcirc;letero de UEA-Komisiono por Ameriko</vrk>
          <lok><url ref="http://esperanto.ca/kea/rb/2000/rb0607.txt">Aktivameriko,
          numero 4a, 2000-06</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_v;&c_e;&c_n;&c_ib;&c_r;</trd>
  <trd lng="bg">&c_s;&c_u;&c_v;&c_je;&c_n;&c_i;&c_r;</trd>
  <trd lng="bo">&#xF51;&#xFB2;&#xF53;&#xF0B;&#xF62;&#xF9F;&#xF7A;&#xF53;&#xF0B;</trd>
  <trd lng="ca">record</trd>
  <trd lng="cs">suven&yacute;r</trd>
  <trd lng="de">Souvenir</trd>
  <trd lng="fr">souvenir<klr tip="amb"> (de voyage)</klr></trd>
  <trdgrp lng="hu">
    <trd>szuven&iacute;r</trd>
    <trd>eml&eacute;kt&aacute;rgy<ind/></trd>
  </trdgrp><trdgrp lng="it">
    <trd>souvenir</trd>, 
    <trd>ricordino</trd>
  </trdgrp>
  <trd lng="pl">pami&aogonek;tka</trd>
  <trd lng="pt">suvenir</trd>
  <trd lng="ro">suvenir</trd>
  <trd lng="sk">suven&iacute;r</trd>
</drv>
</art>
