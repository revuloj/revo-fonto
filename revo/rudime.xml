<art mrk="$Id$">
<kap>
  <rad>rudiment</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="rudime.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Tute elementa (pp scio):
      <ekz>
        sociologiaj esploroj en Esperantujo estas malmultaj, <tld/>aj kaj
        malofte pli-malpli profesiaj<fnt><aut>Boris Kolker</aut>: <vrk>Oni
        ne kulpigu la spegulon (Nikola Rasic, La rondo familia)</vrk>,
        <bib>Monato</bib><lok>jaro 1997a, numero 2a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_ch;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>,
      <trd>&c_e;&c_l;&c_je;&c_m;&c_e;&c_n;&c_t;&c_a;&c_r;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="de">rudiment&auml;r</trd>
    <trdgrp lng="en">
      <trd>elementary</trd>,
      <trd>rudimentary</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>alapvet&odblac;</trd>,
      <trd>elemi</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Apena&ubreve; elkreskinta (pp organo), atrofia.
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_u;&c_d;&c_y;&c_m;&c_e;&c_n;&c_t;&c_a;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_a;&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_t;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>rudiment&auml;r</trd>,
      <trd>unausgebildet</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>rudimentary</trd>,
      <trd>vestigial</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>cs&ouml;kev&eacute;nyes</trd>,
      <trd>fejletlen</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_je;&c_d;&c_o;&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_y;&c_j;</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>&eacute;l&eacute;mentaire</trd>,
    <trd>rudimentaire</trd>
  </trdgrp>
  <trd lng="nl">rudimentair</trd>
  <trd lng="ru">&c_r;&c_u;&c_d;&c_i;&c_m;&c_je;&c_n;&c_t;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>
