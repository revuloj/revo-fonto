<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rudime.xml,v 1.19 2023/09/03 07:53:12 revo Exp $">
<kap>
  <rad>rudiment</rad>/a <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rudime.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="rudime.0a.KOMUNE">
    <dif>
      Kompare kun pli plenaj formoj ne sufi&ccirc;e kreskinta.
    </dif>
    <subsnc mrk="rudime.0a.">
      <uzo tip="klr">(pri scio)</uzo>
      <dif>
        Tute elementa, baza:
        <ekz>
          sociologiaj esploroj en Esperantujo estas malmultaj, <tld/>aj
          kaj
          malofte pli-malpli profesiaj
          <fnt>
            <aut>B. Kolker</aut>,
            <vrk>Oni ne kulpigu la spegulon</vrk>,
            <bib>Monato</bib>,
            <lok>1997:2, p. 22a</lok>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_ch;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>,
        <trd>&c_e;&c_l;&c_je;&c_m;&c_e;&c_n;&c_t;&c_a;&c_r;&c_n;&c_y;</trd>
      </trdgrp>
      <trd lng="de">rudiment&auml;r</trd>
      <trdgrp lng="en">
        <trd>elementary</trd>,
        <trd>rudimentary</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>alapvet&odblac;</trd>,
        <trd>elemi</trd>
      </trdgrp>
      <trd lng="ru">&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </subsnc>
    <subsnc mrk="rudime.0a.BIO">
      <uzo tip="fak">BIO</uzo>
      <uzo tip="klr">(pri organo)</uzo>
      <dif>
        Apena&ubreve; elkreskinta,
        <ref tip="vid" cel="atrofi.0a">atrofia</ref>.
      </dif>
      <trdgrp lng="be">
        <trd>&c_r;&c_u;&c_d;&c_y;&c_m;&c_e;&c_n;&c_t;&c_a;&c_r;&c_n;&c_y;</trd>,
        <trd>&c_n;&c_je;&c_d;&c_a;&c_r;&c_a;&c_z;&c_mol;&c_v;&c_ib;&c_t;&c_y;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>rudiment&auml;r</trd>,
        <trd>unausgebildet</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>rudimentary</trd>,
        <trd>vestigial</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>cs&ouml;kev&eacute;nyes</trd>,
        <trd>fejletlen</trd>
      </trdgrp>
      <trd lng="ru">&c_n;&c_je;&c_d;&c_o;&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_y;&c_j;</trd>
    </subsnc>
  </snc>
  <trdgrp lng="cs">
    <trd>p&#x16F;vodn&#xED;</trd>,
    <trd>rudiment&#xE1;rn&#xED;</trd>,
    <trd>zakrn&#x11B;n&#xFD;</trd>,
    <trd>za&#x10D;&#xE1;te&#x10D;n&#xED;</trd>,
    <trd>z&#xE1;kladn&#xED;</trd>
  </trdgrp>
  <trd lng="fr">rudimentaire</trd>
  <trdgrp lng="ja">
    <trd>&#x521D;&#x6B69;&#x7684;&#x306A;  [<ind>&#x3057;&#x3087;&#x307B;&#x3066;&#x304D;&#x306A;</ind>]</trd>,
    <trd>&#x57FA;&#x672C;&#x306E;  [<ind>&#x304D;&#x307B;&#x3093;&#x306E;</ind>]</trd>,
    <trd>&#x672A;&#x767A;&#x9054;&#x306E;  [<ind>&#x307F;&#x306F;&#x3063;&#x305F;&#x3064;&#x306E;</ind>]</trd>,
    <trd>&#x75D5;&#x8DE1;&#x306E;  [<ind>&#x3053;&#x3093;&#x305B;&#x304D;&#x306E;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">rudimentair</trd>
  <trdgrp lng="pl">
    <trd>szcz&aogonek;tkowy</trd>,
    <trd>rudymentarny</trd>,
    <trd>pocz&aogonek;tkowy</trd>
  </trdgrp>
  <trd lng="pt">rudimentar</trd>
  <trd lng="ru">&c_r;&c_u;&c_d;&c_i;&c_m;&c_je;&c_n;&c_t;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>element&#xE1;rny</trd>,
    <trd>po&#x10D;iato&#x10D;n&#xFD;</trd>,
    <trd>primit&#xED;vny</trd>,
    <trd>p&#xF4;vodn&#xFD;</trd>,
    <trd>rudiment&#xE1;rny</trd>,
    <trd>zakrnel&#xFD;</trd>,
    <trd>z&#xE1;kladn&#xFD;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_u;&c_d;&c_i;&#x43C;&c_je;&#x43D;&c_t;&c_a;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43D;&c_je;&c_d;&#x43E;&c_r;&#x43E;&c_z;&c_v;&c_i;&#x43D;&c_je;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&#x43E;&c_ch;&c_a;&c_t;&#x43A;&#x43E;&c_v;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="rudime.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="rudime.0o.KOMUNE">
    <dif>
      Komenco, bazo, ne evoluinta formo
      de io ne elkreskinta, ne finfarita:
      <ekz>
        &ccirc;e la parazita kaj protektata <ctl>Proteolepas</ctl>,
        la tuta anta&ubreve;a parto de la kapo estas reduktita al
        nura <tld/>o
        <fnt><bib>OriginoDeSpecioj</bib>, <lok>&ccirc;apitro 5a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stump.0o">stumpo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_u;&c_d;&c_y;&c_m;&c_e;&c_n;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>rudiment</trd>,
    <trd>z&#xE1;klad znalost&#xED;</trd>
  </trdgrp>
  <trd lng="fr">rudiment</trd>
  <trdgrp lng="ja">
    <trd>&#x521D;&#x6B69;  [<ind>&#x3057;&#x3087;&#x307B;</ind>]</trd>,
    <trd>&#x5165;&#x9580;  [<ind>&#x306B;&#x3085;&#x3046;&#x3082;&#x3093;</ind>]</trd>,
    <trd>&#x57FA;&#x790E;  [<ind>&#x304D;&#x305D;</ind>]</trd>,
    <trd>&#x539F;&#x57FA;  [<ind>&#x306F;&#x3089;&#x3082;&#x3068;</ind>]</trd>,
    <trd>&#x75D5;&#x8DE1;&#x5668;&#x5B98;  [<ind>&#x3053;&#x3093;&#x305B;&#x304D;&#x304D;&#x304B;&#x3093;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>rudyment</trd>,
    <trd>narz&aogonek;d szcz&aogonek;tkowy</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rudiment</trd>,
    <trd>z&#xE1;klad znalost&#xED;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&#x43B;&c_je;&#x43C;&c_je;&#x43D;&c_t;&c_a;&c_r;&#x43D;&c_ib; &#x43F;&#x43E;&#x43D;&#x44F;&c_t;&c_t;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&c_ch;&c_a;&c_t;&#x43A;&#x43E;&c_v;&c_ib; &c_z;&#x43D;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_r;&c_u;&c_d;&c_i;&#x43C;&c_je;&#x43D;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_u;&c_d;&c_i;&#x43C;&c_je;&#x43D;&c_t;&c_a;&c_r;&#x43D;&c_i;&c_j; &#x43E;&c_r;&c_g;&c_a;&#x43D;</trd>
  </trdgrp>
</drv>
</art>
