<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rudiment</rad>/a
  <fnt>Z</fnt>
</kap>
<drv mrk="rudime.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Tute elementa (pp scio).
    </dif>
    <trd lng="de">rudiment&auml;r</trd>
    <trdgrp lng="en">
      <trd>elementary</trd>,
      <trd>rudimentary</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      Apena&ubreve; elkreskinta (pp organo), atrofia.
    </dif>
    <trdgrp lng="de">
      <trd>rudiment&auml;r</trd>,
      <trd>unausgebildet</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>rudimentary</trd>,
      <trd>vestigial</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_je;&c_d;&c_o;&c_r;&c_a;&c_z;&c_v;&c_i;&c_t;&c_y;&c_j;</trd>
  </snc>
  <trd lng="nl">rudimentair</trd>
  <trd lng="ru">&c_r;&c_u;&c_d;&c_i;&c_m;&c_je;&c_n;&c_t;&c_a;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
