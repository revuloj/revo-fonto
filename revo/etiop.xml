<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: etiop.xml,v 1.22 2023/11/07 20:34:12 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>etiop</rad>/o
    <fnt>
      <bib>FK</bib>
    </fnt>
    <fnt><bib>MT</bib></fnt>
</kap>

<drv mrk="etiop.0o">
  <kap><tld/>o</kap>
  <snc mrk="etiop.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="etiop.E0ujo.GEOG"><tld lit="E"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="etiop.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco:
      <ekz>
        <nom>Joab</nom> diris al <tld lit="E"/>o: Iru, diru al la re&gcirc;o,
        kion vi vidis, la <tld lit="E"/>o adorklini&gcirc;is anta&ubreve;
        <nom>Joab</nom> kaj ekkuris
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 18:21</lok>
        </fnt>;
      </ekz>
      <ekz>
        sennombra estis la popolo, kiu venis kun li el Egiptujo: la
        Luboj, Sukioj, kaj <tld lit="E"/>oj
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 12:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Ebed-Mele&hcirc;</nom>, <tld lit="E"/>o, e&ubreve;nuko, kiu estis en la
        domo de la re&gcirc;o, a&ubreve;dis, ke oni &jcirc;etis <nom>Jeremian</nom> en la
        kavon
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 38:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Kandake</nom>, re&gcirc;ino de la <tld lit="E"/>oj
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 8:27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_y;&c_jo;&c_p;</trd>
  <trd lng="ca">et&iacute;op <klr>(subst.)</klr></trd>
  <trdgrp lng="cs">
    <trd>Etiop</trd>,
    <trd>Etiopan</trd>
  </trdgrp>
  <trd lng="de">&Auml;thiopier</trd>
  <trd lng="en">Ethiopian</trd>
  <trd lng="fr">&Eacute;thiopie<klr> (subst.)</klr></trd>
  <trd lng="ja">&#x30A8;&#x30C1;&#x30AA;&#x30D4;&#x30A2;&#x4EBA;  <pr>&#x30A8;&#x30C1;&#x30AA;&#x30D4;&#x30A2;&#x3058;&#x3093;</pr></trd>
  <trd lng="nl">Ethiopi&euml;r</trd>
  <trd lng="sk">Eti&oacute;p&ccaron;an</trd>
  <trd lng="tok">jan pi ma Isijopija</trd>
  <trd lng="uk">&#x435;&#x444;&#x456;&#x43E;&#x43F;</trd>
  <trd lng="vo">L&auml;tiop&auml;nan</trd>
  <trd lng="zh">&#x57C3;&#x585E;&#x4FC4;&#x6BD4;&#x4E9A;&#x4EBA; <pr>&#x101;is&#x101;i&#xE9;b&#x1D0;y&#xE0;r&#xE9;n</pr></trd>
</drv>

<drv mrk="etiop.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato:
      <ekz>
        transe de la riveroj <tld lit="E"/>aj
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 18:1</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_y;&c_jo;&c_p;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">et&iacute;op <klr>(adj.)</klr></trd>
  <trd lng="de">&auml;thiopisch</trd>
  <trd lng="en">Ethiopian</trd>
  <trd lng="fr">&eacute;thiopien<klr> (adj.)</klr></trd>
  <trd lng="nl">Ethiopisch</trd>
  <trd lng="tok">pi ma Isijopija</trd>
  <trd lng="uk">&#x435;&#x444;&#x456;&#x43E;&#x43F;&#x441;&#x44C;&#x43A;&#x438;&#x439;</trd>
  <trd lng="vo">L&auml;tiop&auml;nik</trd>
</drv>

<drv mrk="etiop.E0ujo">
  <kap><tld lit="E"/>ujo <fnt><bib>MT</bib></fnt></kap>
  <mlg kod="ISO-3166">ET</mlg>
  <snc mrk="etiop.E0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" lst="voko:&scirc;tatoj" cel="sxtat.0o">&Scirc;tato</ref>
      en la nordorienta <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Adis-Abebo;
      teritorio de Federacia Popola Respubliko Etiopujo,
      limanta kun
      <ref tip="vid" cel="eritre.E0o.GEOG">Eritreo</ref>,
      <ref tip="vid" cel="gxibuti.GX0o.GEOG">&Gcirc;ibutio</ref>,
      <ref tip="vid" cel="kenj.K0o.GEOG">Kenjo</ref>,
      <ref tip="vid" cel="somal.S0ujo.GEOG">Somalujo</ref>
      kaj <ref tip="vid" cel="sudan.S0o.GEOG">Sudano</ref>:
      <ekz>
        <tld lit="E"/>ujo kaj la senfina Egiptujo estis &gcirc;ia forto, la
        Putidoj kaj Luboj donadis al vi helpon
        <fnt>
          <bib>MT</bib>
          <lok>Na&hcirc;um 3:9</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="etiopi.E0o">Etiopio</ref>
    <trd lng="be">&c_E;&c_t;&c_y;&c_jo;&c_p;&c_ib;&c_ja;</trd>
    <trd lng="bg">&c_Je;&c_t;&c_i;&c_o;&c_p;&c_i;&c_ja;</trd>
    <trdgrp lng="ca">
      <trd>Eti&ograve;pia</trd>,
      <trd>Rep&uacute;blica Federal Democr&agrave;tica d'Eti&ograve;pia</trd>
    </trdgrp>
    <trd lng="de">&Auml;thiopien</trd>
    <trd lng="en">Ethiopia</trd>
    <trdgrp lng="fa">
      <trd>&a_A;&a_t;&f_y;&a_w;&f_p;&f_y;</trd>,
      <trd>&a_H;&a_b;&a_sh;&a_h;</trd>
    </trdgrp>
    <trd lng="fr">&Eacute;thiopie</trd>
    <trd lng="fy">Etioopje</trd>
    <trd lng="id">Etiopia</trd>
    <trd lng="nl">Ethiopi&euml;</trd>
    <trdgrp lng="pl">
      <trd>Etiopia</trd>,
      <trd>Federalna Demokratyczna Republika <ind>Etiopii</ind></trd>
    </trdgrp>
    <trd lng="pt">Eti&oacute;pia</trd>
    <trd lng="tok">ma Isijopija</trd>
    <trd lng="vo">L&auml;tiop&auml;n</trd>
    <trd lng="zh">&#x57C3;&#x585E;&#x4FC4;&#x6BD4;&#x4E9A; <pr>&#x101;is&#xE0;i&#xE9;b&#x1D0;y&#xE0;</pr></trd>
</snc>
</drv>

</art>
