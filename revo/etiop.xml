<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>etiop</rad>/o
</kap>

<drv mrk="etiop.0ujo">
  <kap>
    <tld lit="E"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="etiop.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la nordorienta <ref tip="malprt"
      cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Adis-Abebo.
      Teritorio de Federacia Popola Respubliko Etiopujo.
      ISO-3166 kodo: ET.
      &Gcirc;i limas kun
      <ref tip="vid" cel="eritre.0o.GEOG">Eritreo</ref>,
      <ref tip="vid" cel="gxibuti.0o.GEOG">&Gcirc;ibutio</ref>,
      <ref tip="vid" cel="kenj.0o.GEOG">Kenjo</ref>,
      <ref tip="vid" cel="somal.0ujo.GEOG">Somalujo</ref>
      kaj <ref tip="vid" cel="sudan.0o.GEOG">Sudano</ref>.
    </dif>
    <trd lng="bg">&c_Je;&c_t;&c_i;&c_o;&c_p;&c_i;&c_ja;</trd>
    <trd lng="de">&Auml;thiopien</trd>
    <trd lng="en">Ethiopia</trd>
    <trdgrp lng="fa">
      <trd>&a_A;&a_t;&f_y;&a_w;&f_p;&f_y;</trd>,
      <trd>&a_H;&a_b;&a_sh;&a_h;</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>Etiopia</trd>,
      <trd>Federalna Demokratyczna Republika <ind>Etiopii</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

