<kap>
  <rad>renegat</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="renega.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo, kiu publike forlasis sian religion a&ubreve; partion:
      <ekz>
        la <tld/>oj kutimas, se ili intencas reveni al kristanaj landoj,
        porti kun si atestojn<fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro
        40a,
        p. 302a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="aposta.0o">apostato</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_n;&c_e;&c_g;&c_a;&c_t;</trd>,
    <trd>&c_a;&c_d;&c_s;&c_t;&c_u;&c_p;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">renegat</trd>
  <trdgrp lng="cs">
    <trd>odpadl&#xED;k</trd>,
    <trd>odrodilec</trd>,
    <trd>od&scaron;t&#x11B;penec</trd>,
    <trd>reneg&#xE1;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Renegat</trd>,
    <trd>Abtr&uuml;nniger</trd>,
    <trd>Glaubensverleugner</trd>
  </trdgrp>
  <trd lng="en">renegade</trd>
  <trdgrp lng="fr">
    <trd>apostat</trd>,
    <trd>ren&eacute;gat</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>reneg&aacute;t</trd>,
    <trd>hitehagyott</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x80CC;&#x6559;&#x8005;  <pr>&#x306F;&#x3044;&#x304D;&#x3087;&#x3046;&#x3057;&#x3083;</pr></trd>,
    <trd>&#x5909;&#x7BC0;&#x8005;  <pr>&#x3078;&#x3093;&#x305B;&#x3064;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trd lng="nl">afvallige</trd>
  <trdgrp lng="pl">
    <trd>apostata</trd>,
    <trd>odst&eogonek;pca</trd>,
    <trd>odszczepieniec</trd>,
    <trd>renegat</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>renegado</trd>,
    <trd>ap&oacute;stata</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>apostat</trd>,
    <trd>disiden&#x21B;&abreve;</trd>,
    <trd>renegat</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_n;&c_je;&c_g;&c_a;&c_t;</trd>,
    <trd>&c_o;&c_t;&c_s;&c_t;&c_u;&c_p;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odrodilec</trd>,
    <trd>reneg&#xE1;t</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&c_je;&#x43D;&c_je;&c_gu;&c_a;&c_t;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_s;&c_t;&c_u;&#x43F;&#x43D;&c_i;&#x43A;</trd>
