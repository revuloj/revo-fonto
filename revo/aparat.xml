<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aparat.xml,v 1.39 2022/03/12 10:47:30 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>aparat</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="aparat.0o">
  <kap><tld/>o</kap>

  <snc mrk="aparat.0o.TEK">
    <tezrad fak="TEK"/>	  
    <uzo tip="fak">TEK</uzo>
    <dif>
      Kuna&jcirc;o de objektoj a&ubreve; instrumentoj servanta por
      plenumi
      ion:
      <ekz>
        telegrafa <tld/>o;
      </ekz>
      <ekz>
         radio<tld/>o;
      </ekz>
      <ekz>
        elektraj <tld/>oj mankas en la kamparo
        <fnt>
          <bib>Monato</bib>
          <aut>MU Binghua</aut>
          <vrk><url ref="http://www.esperanto.be/fel/mon/007168.html">Elektraj
          aparatoj mankas en la kamparo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;tomata disdona <tld/>o;
      </ekz>
      <ekz>
        <tld/>o por distili vinon;
      </ekz>
      <ekz>
        kiam oni posedis familie nur unu telefonon, la <tld/>o trovi&gcirc;is
        en komuna &ccirc;ambro
        <fnt>
          <bib>Monato</bib>
          <aut>HORI Jasuo</aut>
          <vrk><url ref="http://www.esperanto.be/fel/mon/007187.html">PO&#xDE;TELEFONOJ:
          &Ccirc;u beno a&ubreve; malbeno?</url></vrk>
        </fnt>.
      </ekz>
    </dif>

    <subsnc mrk="aparat.0o.POL">
      <uzo tip="stl">FIG</uzo>
      <uzo tip="fak">POL</uzo>
      <dif>
        Aro da oficaj organizoj por plenumi socian a&ubreve; societan
        funkcion:
        <ekz>
          la &scirc;tataj administraj <tld/>oj;
        </ekz>
        <ekz>
          la polica <tld/>o
          <fnt>
            <bib>Monato</bib>
          </fnt>;
        </ekz>
        <ekz>
          devus formi&gcirc;i tiaspeca reguliga <tld/>o, kiu ebligus la
          ekonomian disvolvi&gcirc;on, sed samtempe limigus ties negativajn
          manifesti&gcirc;ojn
          <fnt>
            <bib>Monato</bib>
            <vrk><url ref="http://www.esperanto.be/fel/mon/007258.html">Mondaj
            finacoj: Post la kunsido de financistoj en Prago</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          mi anka&ubreve; opinias, ke estus tute maltrafe apliki la
          taktikon de politika partio en SAT ... nia organizo devas
          esti <tld/>o, kiun &ccirc;iuj samcelanoj uzu por la profito de
          sia partio a&ubreve; sindikato
          <fnt>
            <bib>Lanti</bib>,
            <vrk>Venis anarkisto...</vrk>
           </fnt>.
        </ekz>
      </dif>
      <trd lng="de">Apparat <klr>(polit.)</klr></trd>
      <trdgrp lng="hu">
        <trd>appar&aacute;tus</trd>,
        <trd>hivatali <ind>g&eacute;pezet</ind></trd>
       </trdgrp>
       <trdgrp lng="ca">
        <trd>aparell</trd>,
        <trd>aparat</trd>
       </trdgrp>
    </subsnc>
    <subsnc mrk="aparat.0o.BIO">
      <uzo tip="stl">FIG</uzo>
      <uzo tip="stl">EVI</uzo>
      <uzo tip="fak">ANA</uzo>
      <uzo tip="fak">BIO</uzo>
      <dif>
         <ref tip="dif" cel="organ.0aro.BIO">Organaro</ref>:
        <ekz>
          la digesta <tld/>o;
          <trd lng="hu">em&eacute;szt&odblac;rendszer</trd>
        </ekz>
        <ekz>
          pens- kaj sent<tld/>o <klr>(de bestoj kaj plantoj)</klr>
          <fnt>
            <bib>Lanti</bib>,
            <vrk>Kio estas animo?</vrk>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="organ.0aro">organaro</ref>,
        <ref cel="sistem.0o.FIZL">sistemo <sncref/></ref>
      </refgrp>
      <trd lng="en">system</trd>
      <trd lng="hu">szervrendszer</trd>
      <trdgrp lng="ca">
        <trd>aparell</trd>,
        <trd>sistema</trd>
      </trdgrp>
    </subsnc>

    <trdgrp lng="de">
        <trd>Ger&auml;t</trd>,
        <trd>Trakt</trd>,
        <trd>Apparat</trd>,
        <trd>System</trd>
      </trdgrp>
    <trdgrp lng="hu">
      <trd>k&eacute;sz&uuml;l&eacute;k</trd>,
      <trd>szerkezet</trd>,
      <trd>eszk&ouml;z</trd>
    </trdgrp>
  </snc>

  <snc mrk="aparat.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <uzo tip="fak">BIB</uzo>
    <dif>
      Kromteksto (notoj, bibliografio ktp) en scienca libro:
      <ekz>
        la kritika <tld/>o de Eneado<fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">appar&aacute;tus</trd>
    <trd lng="ca">aparat</trd>
    <trd lng="ms">alat</trd>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  <trd lng="bo">&#xF60;&#xF41;&#xFB2;&#xF74;&#xF63;&#xF0B;&#xF60;&#xF41;&#xF7C;&#xF62;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>apar&aacute;t</trd>,
    <trd>p&rcaron;&iacute;stroj</trd>,
    <trd>za&rcaron;&iacute;zen&iacute;</trd>
  </trdgrp>
  <trd lng="de">Apparat <klr>(bibliogr.)</klr></trd>
  <trdgrp lng="en">
    <trd>apparatus</trd>,<trd>machine</trd>
  </trdgrp>
  <trd lng="es">aparato</trd>
  <trd lng="fr">appareil</trd>
  <trdgrp lng="he">
    <trd>&#x5DE;&#x5EA;&#x5E7;&#x5DF;</trd>,
    <trd>&#x5DE;&#x5DB;&#x5E9;&#x5D9;&#x5E8;</trd>,
    <trd>&#x5DE;&#x5E0;&#x5D2;&#x5E0;&#x5D5;&#x5DF;</trd>
  </trdgrp>
  <trd lng="it">apparato</trd>
  <trd lng="oc">aparelh</trd>
  <trd lng="ru">&c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>pr&iacute;stroj</trd>,
    <trd>zariadenie</trd>
  </trdgrp>
  <trd lng="uk">&#x443; &#x440;&#x456;&#x437;&#x43D;. &#x437;&#x43D;&#x430;&#x447;.&#xA0;&#x430;&#x43F;&#x430;&#x440;&#x430;&#x442;</trd>
</drv>

<drv mrk="aparat.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="aparat.0aro.aro">
    <dif>
      Aro da aparatoj:
      <ekz>
        <tld/>o, telefona kaj komputila
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimlay Evans</aut>
          <vrk><url ref="http://www.esperanto.be/fel/mon/007132.html">Intereto:
          Interreto senpage</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        plej modernaj <tld/>oj kaj armilo
        <fnt>
          <bib>Monato</bib>
          <aut>Ionel Onet</aut>
          <vrk><url ref="http://www.esperanto.be/fel/mon/007036.html">Ekstremismo:
          Pri kaldronoj kaj potoj: &ccirc;u amba&ubreve; kotaj?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ekip.0ajxo">ekipa&jcirc;o</ref>
    <trd lng="it">equipaggiamento<klr> (insieme di apparati)</klr></trd>
    <trdgrp lng="ca">
      <trd>equip</trd>,
      <trd>equipament</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Ausr&uuml;stung</trd>,
      <trd>Technik</trd>
    </trdgrp>
  </snc>
  <snc mrk="aparat.0aro.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      La tuta materia ekipa&jcirc;o de komputilo, inklude la
      datumportilojn, ofte kontraste al
      <ref tip="ant" cel="progra.0aro">programaro</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Hardware</trd>,
      <trd>Ausr&uuml;stung</trd>
    </trdgrp>
    <trd lng="en">hardware</trd>
    <trd lng="hu">hardver</trd>
    <trd lng="it">hardware</trd>
    <trd lng="ca">maquinari</trd>
    <trd lng="tr">donan&inodot;m</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_p;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_a;&c_r;&c_a;&c_t;&c_n;&c_a;&c_je; &c_z;&c_a;&c_b;&c_je;&c_s;&c_mol;&c_p;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hardware</trd>,
    <trd>po&ccaron;&iacute;ta&ccaron;</trd>,
    <trd>v&yacute;po&ccaron;etn&iacute; za&rcaron;&iacute;zen&iacute;</trd>
  </trdgrp>
  <trd lng="es">equipo <klr>(conjunto de aparatos)</klr></trd>
  <trdgrp lng="fr">
    <trd>appareillage</trd>,
    <trd>mat&eacute;riel</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="sk">pr&iacute;strojov&eacute; vybavenie</trd>
</drv>

<drv mrk="aparat.flug0o">
  <kap>flug<tld/>o</kap>
  <snc mrk="aparat.flug0o.AVI">
  <tezrad fak="AVI"/>
  <uzo tip="fak">AVI</uzo>
    <dif>
      Aparato por flugi, voji en aero, ekzemple <ref tip="sub" cel="balon.0o">balono</ref>, <ref tip="sub" cel="parasx.0o">para&scirc;uto</ref>,
      <ref tip="sub" cel="avi.0o">avio</ref>:
      <ekz>
        en flug<tld/>oj li muntadis motorojn de rusaj motorboatoj
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Straznickas</aut>
          <vrk><url ref="http://www.esperanto.be/fel/mon/006877.html">Nekutimaj
          transportiloj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_a;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  <trd lng="ca">aeronau</trd>
  <trd lng="cs">letadlo</trd>
  <trdgrp lng="de">
    <trd>Flugapparat</trd>,
    <trd>Flugger&auml;t</trd>
  </trdgrp>
  <trd lng="en">flying machine</trd>
  <trd lng="es">aeronave</trd>
  <trd lng="fr">vaisseau a&eacute;rien</trd>
  <trd lng="hu">rep&uuml;l&odblac;eszk&ouml;z</trd>
  <trdgrp lng="it">
  <trd>aeromobile</trd>, <trd>macchina volante</trd>
  </trdgrp>
  <trd lng="sk">lietadlo</trd>
</drv>

<drv mrk="aparat.son0o">
  <kap>son<tld/>o</kap>
  <snc mrk="aparat.son0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      &Ccirc;ia <tld/>o por ricevo, a&ubreve;digo, registro, a&ubreve;
      prilaboro de sonoj:
      <ekz>
        la a&ubreve;da parto de la a&ubreve;dvida signalo
        <klr>...</klr> estas anka&ubreve; 
        uzebla de simpla amplifikilo, kiel tiu de via magnetofono
        a&ubreve; ia 
        ajn alia son<tld/>o
        <fnt>
          <aut>B. Masala</aut>,
          <vrk><url ref="http://osiek.org/aera/artik/satelit.html">
          Persatelita ricevado de radiostacioj en
          E&ubreve;ropo</url></vrk>,
          <lok>Osiek, 1996</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="aparat.son0o.RIM_SNC">
    Hejma Vortaro <fnt><bib>HejmVort</bib></fnt> proponas
    pli malvastan sencon:
    <ctl>aparato, plej ofte stereofonia, kiu estas
    samtempe radioricevilo, magnetofono kaj gramofono.</ctl>
  </rim>
  <trdgrp lng="de">
    <trd>Tonwiedergabeger&auml;t</trd>,
    <trd>Tonaufzeichnungsger&auml;t</trd>,
    <trd>Musikanalge</trd>,
    <trd>Hi-Fi-Ger&auml;te</trd>,
    <trd>Tontechnik</trd>,
  </trdgrp>
  <trd lng="es">equipo de sonido</trd>
  <trd lng="fr">appareil <ind>audio</ind></trd>
</drv>
</art>
