<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>aparat</rad>/o
</kap>
<drv mrk="aparat.0o">
  <kap><tld/>o</kap>
  <snc mrk="aparat.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Kuna&jcirc;o de objektoj a&ubreve; instrumentoj servanta por plenumi
      ion:
      <ekz>
        telegrafa <tld/>o;
      </ekz>
      <ekz>
        a&ubreve;tomata disdona <tld/>o;
      </ekz>
      <ekz>
        <tld/>o por distili vinon.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>k&eacute;sz&uuml;l&eacute;k</trd>,
      <trd>szerkezet</trd>,
      <trd>eszk&ouml;z</trd>
    </trdgrp>
  </snc>
  <snc mrk="aparat.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Aro da organoj rilatantaj al sama fiziologia funkcio:
      <ekz>
        la digesta <tld/>o;
        <trd lng="hu">em&eacute;szt&odblac;rendszer</trd>
      </ekz>
      <ekz>
        Anka&ubreve; bestoj, e&ccirc; plantoj
        sentas tiujn bezonojn; ili tamen ne pensas kaj sekve ne
        havas animon. Pli precize: ilia pens- kaj sent<tld/>o estas
        tiom elementa, ke &gcirc;ia funkciado ne povas produkti
        animon<fnt><aut>Lanti</aut>,
        <vrk>Kio estas animo?</vrk></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Trakt</trd>,
      <trd>System</trd>
    </trdgrp>
    <trd lng="en">system</trd>
    <trd lng="hu">szervrendszer</trd>
  </snc>
  <snc mrk="aparat.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Aro da oficaj organizoj por plenumi socian a&ubreve; societan
      funkcion:
      <ekz>
        la &scirc;tataj administraj <tld/>oj;
      </ekz>
      <ekz>
        mi anka&ubreve; opinias, ke estus tute maltrafe apliki la
        taktikon de politika partio en SAT ... nia organizo devas
        esti <tld/>o, kiun &ccirc;iuj samcelanoj uzu por la profito de
        sia partio a&ubreve; sindikato<fnt><aut>Lanti</aut>,
        <vrk>Venis anarkisto...</vrk></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>appar&aacute;tus</trd>,
      <trd>hivatali <ind>g&eacute;pezet</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="aparat.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <uzo tip="fak">BIB</uzo>
    <dif>
      Kromteksto (notoj, bibliografio kp) en scienca libro:
      <ekz>
        la kritika <tld/>o de Eneado<fnt>PIV</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">appar&aacute;tus</trd>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  <trd lng="de">Apparat</trd>
  <trdgrp lng="en">
    <trd>apparatus</trd>,<trd>machine</trd>
  </trdgrp>
  <trd lng="es">aparato</trd>
  <trd lng="fr">appareil</trd>
  <trd lng="ru">&c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
</drv>

<drv mrk="aparat.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="aparat.0aro.aro">
    <dif>
      Aro da aparatoj.
    </dif>
    <ref tip="sin" cel="ekip.0ajxo">ekipa&jcirc;o</ref>
  </snc>
  <snc mrk="aparat.0aro.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      La tuta fizika ekipa&jcirc;o de komputilo, inklude la
      datumportilojn, ofte kontraste al
      <ref tip="ant" cel="progra.0aro">programaro</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Hardware</trd>,
      <trd>Ausr&uuml;stung</trd>
    </trdgrp>
    <trd lng="en">hardware</trd>
    <trd lng="hu">hardver</trd>
    <trd lng="tr">donan&inodot;m</trd>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="fr">
    <trd>appareillage</trd>,
    <trd>mat&eacute;riel</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_p;&c_p;&c_a;&c_r;&c_a;&c_t;&c_u;&c_r;&c_a;</trd>
</drv>

<drv mrk="aparat.flug0o">
  <kap>flug<tld/>o</kap>
  <snc mrk="aparat.flug0o.AVI">
  <uzo tip="fak">AVI</uzo>
    <dif>
      Aparato por flugi, voji en aero, ekzemple balono, para&scirc;uto, avio.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_a;&c_p;&c_a;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Flugapparat</trd>,
    <trd>Flugger&auml;t</trd>
  </trdgrp>
  <trd lng="en">flying machine</trd>
  <trd lng="hu">rep&uuml;l&odblac;eszk&ouml;z</trd>
</drv>
</art>
