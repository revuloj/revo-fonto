<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: traban.xml,v 1.17 2024/05/10 10:43:46 revo Exp $">
<kap>
  <rad>trabant</rad>/o
</kap>
<drv mrk="traban.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">AST</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="dif" cel="sateli.0o">Satelito</ref>:
      <ekz>
        oni iniciatis la <esc>artefaritan</esc> <tld/>on <klr>[&#x2026;]</klr>,
        la stacio <klr>[&#x2026;]</klr> estas sukceso kaj modelo por
        plia internacia kunlaborado, e&ccirc; se la&ubreve; scienca vidpunkto
        ISS estas fiasko
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2009/009681.php">Politika
          sukceso, scienca fiasko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;&c_o;&c_zh;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_s;&c_a;&c_t;&c_e;&c_l;&c_ib;&c_t;</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>dru&zcaron;ice</trd>,
      <trd>souputn&iacute;k</trd>,
      <trd>trabant</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Trabant</trd>,
      <trd>Satellit</trd>
    </trdgrp>
    <trd lng="fr">satellite</trd>
    <trdgrp lng="hu">
      <trd>m&udblac;hold</trd>,
      <trd>cimbora</trd>,
      <trd>csatl&oacute;s</trd>
    </trdgrp>
    <trd lng="nl">satelliet</trd>
    <trdgrp lng="pl">
      <trd>trabant</trd>,
      <trd>&zdot;o&lstroke;nierz stra&zdot;y przybocznej</trd>,
      <trd>wsp&oacute;&lstroke;towarzysz</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_p;&c_u;&c_t;&c_n;&c_i;&c_k;</trd>
    <trdgrp lng="sk">
      <trd>satelit</trd>,
      <trd>str&aacute;&zcaron;ca</trd>,
      <trd>zbrojno&scaron;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="zh">
    <trd>&#x536B;&#x661F; <pr>w&#xE8;ix&#x12B;ng</pr></trd>,
    <trd>&#x885B;&#x661F; <pr>w&#xE8;ix&#x12B;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="traban.T0o">
  <kap><tld lit="T"/>o</kap>
  <snc mrk="traban.T0o.auxto">
    <uzo tip="fak">AUT</uzo>
    <dif>
      A&ubreve;tomarko, produktita de 1958 en <nom>Zwickau</nom>, GDR:
      <ekz>
        eskorto de 70 GDR-a&ubreve;toj &leftquot;<tld lit="T"/>o&#x201D;, parte en helaj
        koloroj,
        veturis brue hupante el Hungario trans la limo
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2009/009867.php">Forigo-festo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&#xAB;&c_T;&c_r;&c_a;&c_b;&c_a;&c_n;&c_t;&#xBB;</trd>
    <trd lng="de">Trabant <klr>(Auto)</klr></trd>
    <trd lng="fr">Trabant <klr>(Auto)</klr></trd>
    <trdgrp lng="zh">
    <trd>&#x62D6;&#x7B28;&#x8ECA; <pr>tu&#x14D;b&#xE8;nch&#x113;</pr></trd>,
    <trd>&#x62D6;&#x7B28;&#x8F66; <pr>tu&#x14D;b&#xE8;nch&#x113;</pr></trd>,
    <trd>&#x7279;&#x62C9;&#x6BD4; <pr>t&#xE8;l&#x101;b&#x1D0;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="uk">
    <trd>&#x434;&#x440;&#x430;&#x431;&#x430;&#x43D;&#x442;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x43D;. &#x43D;&#x435;&#x440;&#x43E;&#x437;&#x43B;&#x443;&#x447;&#x43D;&#x438;&#x439; &#x442;&#x43E;&#x432;&#x430;&#x440;&#x438;&#x448;</trd>,
    <trd>&#x441;&#x443;&#x43F;&#x443;&#x442;&#x43D;&#x438;&#x43A;</trd>
  </trdgrp>
</drv>
</art>
