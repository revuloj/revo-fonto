</kap>
<drv mrk="ili.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Persona pronomo de la 3-a persono en plurnombro,
      por &ccirc;iuj genroj.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_n;&c_y;</trd>
  <trd lng="bg">&c_t;&c_je;</trd>
  <trd lng="de">sie</trd>
  <trd lng="en">they</trd>
  <trd lng="es">ellos, ellas</trd>
  <trdgrp lng="fr">
    <trd>ils</trd>,
    <trd>elles</trd></trdgrp> 
  <trd lng="nl">zij</trd>
  <trd lng="ru">&c_o;&c_n;&c_i;</trd>
  <trd lng="sv">de</trd>
</drv>
</art>
