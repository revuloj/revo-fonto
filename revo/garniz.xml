<drv mrk="garniz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Militistaro gardanta fortika&jcirc;on, urbon a&ubreve; da&ubreve;re
      lo&gcirc;anta en &gcirc;i:
      <ekz>
        restigi<fnt>Z</fnt>, starigi<fnt>Z</fnt> <tld/>on;
      </ekz>
      <ekz>
        resti <tld/>e en la urboj
        <fnt>(<aut>Prus, trad. Kabe</aut>: <vrk>La Faraono, 2a vol.</vrk>,
        <lok>&ccirc;ap. 23</lok>)</fnt>;
      </ekz>
      <ekz>
        <tld/>a urbo;
      </ekz>
      <ekz>
        urboj, kie staras malgrandaj <tld/>oj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      <tld/>ejo.
    </dif>
  </snc>
</drv>
</art>

