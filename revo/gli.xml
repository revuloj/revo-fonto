<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gli.xml,v 1.6 2023/08/08 15:27:37 revo Exp $">
<kap>
  <rad>gli</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="gli.0o">
  <kap><tld/>o</kap>
  <snc mrk="gli.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Tuto de la
      <ref tip="prt" cel="cxel.gli0o.FIZL"><tld/>&ccirc;eloj</ref>,
      kiuj konsistigas pliparton de la nervosistemo:
      <ekz>
        la histologiaj teknikoj ekzistantaj ebligas tinkturi
        (por ebligi la mikroskopan observadon) la &ccirc;elajn korpojn
        de la ne&ubreve;ronoj, la aksonojn, la mjelinon kaj la <tld/>on
        <fnt>
          <aut>R. Feldman Gonzalez, trad. N. Kalusky k.a.</aut>,
          <vrk><url
          ref="http://domhnalluibriain.blogspot.com/2009/08/la-nova-paradigmo-en-psikologio-ruben_11.html">
          La nova paradigmo en psikologio</url></vrk>,
          <lok>SAT, 1985</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_l;&c_ib;&c_ja;</trd>,
    <trd>&c_n;&c_e;&c_w;&c_r;&c_a;&c_g;&c_l;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="ca">neur&ograve;glia</trd>
  <trd lng="cs">glee</trd>
  <trdgrp lng="fr">
    <trd>glie</trd>,
    <trd>neuroglie</trd>,
    <trd>n&eacute;vroglie</trd>
  </trdgrp>
  <trd lng="sk">glee</trd>
  <trdgrp lng="uk">
    <trd>&c_gu;&#x43B;&c_ib;</trd>,
    <trd>&c_v;&c_je;&c_s;&c_je;&#x43B;&c_a; &#x43F;&c_ib;&c_s;&#x43D;&#x44F; &#x43D;&c_a; &#x43A;&c_ib;&#x43B;&#x44C;&#x43A;&c_a;
    &c_g;&#x43E;&#x43B;&#x43E;&c_s;&c_ib;&c_v;&#xA0;(&c_v; &c_A;&#x43D;&c_g;&#x43B;&c_ib;&c_ji;)</trd>
  </trdgrp>
</drv>
</art>

