<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>niton</rad>/o
</kap>
<drv mrk="niton.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Nt. &Hcirc;emia elemento, gasforma, kiu emanas el la radiumo
      (malnova nomo de <ref tip="sin" cel="radon.0o">radono</ref>).
    </dif>
  </snc>
  <trd lng="de">Niton</trd>
  <trd lng="nl">radon</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_o;&c_n;</trd>
  </trdgrp>
</drv>
</art>
