<vortaro>
<art mrk="$Id$">
<kap>
  <rad>niton</rad>/o
</kap>

<drv mrk="niton.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Malnova nomo de la &hcirc;emia elemento <ref tip="sin" cel="radon.0o">radono</ref>, tiama simbolo Nt.
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_d;&c_o;&c_n;</trd>
  <trdgrp lng="bg">
    <trd>&c_n;&c_i;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="de">Niton</trd>
  <trd lng="hu">niton</trd>
  <trd lng="nl">radon</trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_i;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_r;&c_a;&c_d;&c_o;&c_n;</trd>
  </trdgrp>
</drv>
</art>
