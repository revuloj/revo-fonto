<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>koridor</rad>/o
</kap>
<drv mrk="korido.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Trairejo komunikiganta &ccirc;ambrojn de sama eta&gcirc;o, de sama apartamento:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Egiptujo estas <tld/>o inter du montoj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Gang</trd>,
      <trd>Flur</trd>,
      <trd>Korridor</trd>
    </trdgrp>
    <trd lng="nl">gang</trd>
    <trd lng="fr">corridor</trd>
    <trdgrp lng="en">
      <trd>corridor</trd>,
      <trd>hall</trd>,
      <trd>passageway</trd>
    </trdgrp>
    <trd lng="hu">folyos&oacute;</trd>    
  </snc>
  <snc>
    <dif>
      Galerio 1:
      <ekz>
        korto &ccirc;irka&ubreve;ita per <tld/>o apogita sur kolonoj<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>torn&aacute;c</trd>,
      <trd>kereng&odblac;</trd>,
      <trd>&aacute;rk&aacute;d</trd>
    </trdgrp>
    <trd lng="nl">galerij</trd>
  </snc>
  <snc>
    <dif>
      Galerio 4.
    </dif>
    <trdgrp lng="hu">
      <trd>t&aacute;rna</trd>,
      <trd>v&aacute;gat</trd>,
      <trd>j&aacute;rat</trd>
    </trdgrp>    
  </snc>
  <snc>
    <uzo tip="fak">EKOL</uzo>
    <dif>
      Strio de tero, liganta naturterenojn, kiun bezonas sova&gcirc;aj
      plantoj kaj bestoj por disvasti&gcirc;i.
    </dif>
    <trd lng="en">wildlife corridor</trd>
    <trd lng="hu">&eacute;l&odblac;folyos&oacute;</trd>    
    <trd lng="nl">corridor</trd>
  </snc>
  <snc>
    <uzo tip="fak">POL</uzo>
    <dif>
      Strio de teritorio, kiu trairas la teritorion de alia &scirc;tato,
      ordinare por ebligi trairon al la maro.
    </dif>
    <trd lng="en">corridor</trd>
    <trd lng="de">Korridor</trd>
    <trd lng="es">corredor</trd>
    <trd lng="fr">corridor</trd>
    <trdgrp lng="hu">
      <trd>folyos&oacute; <klr tip="ind">(politikai)</klr></trd>,
      <trd>korridor</trd>
    </trdgrp>
    <trd lng="nl">corridor</trd>
    <trd lng="pt">corredor</trd>
  </snc>
  <trd lng="cs">chodba</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_i;&c_d;&c_o;&c_r;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_h;&c_o;&c_d;</trd>
  </trdgrp>
</drv>
</art>
