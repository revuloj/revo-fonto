<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: stink.xml,v 1.12 2020/12/17 01:10:35 revo Exp $">
<kap>
  <rad>stink</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="stink.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="stink.0i.fetori">
    <dif>
      <ref tip="dif" cel="odor.malbon0i">Malbonodori</ref>:
      <ekz>
        eble anka&ubreve; mi <tld/>as
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 66a, p. 426a</lok></fnt>!
      </ekz>
      <ekz>
        &ccirc;ion lavis niaj junuloj, e&ccirc; per detergentoj, por ke nenio <tld/>u en la a&ubreve;tobuso
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2011/010394.php">&Ccirc;iam pli malalten</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        eta parto (2&#8239;%) de la e&ubreve;ropanoj havas akselojn, kiuj ne <tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2014/011217.php">Nekovrilaj malkovroj</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        fetora, <tld/>a ka&ccirc;o utila nek al besto nek al homo
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="https://www.monato.be/2012/010797.php">Ne, ne ... N!</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        multe da naciisma tordado <klr>[&#8230;]</klr> en la komenco de la 21a jarcento,
        delonge <tld/>as
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="https://www.monato.be/2006/008894.php">Atleto de naciismo</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="odor.malbon0i">odora&ccirc;i</ref>,
      <ref cel="fetor.0i">fetori</ref>
    </refgrp>
  </snc>
  <trd lng="cs">p&aacute;chnout</trd>
  <trdgrp lng="de">
    <trd>stinken</trd>,
    <trd>m&uuml;ffeln</trd>
  </trdgrp>
  <trd lng="fa">&a_b;&a_w;&f_g;&a_n;&a_d; &a_d;&a_A;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>puer</trd>
  </trdgrp>
  <trd lng="hu">b&udblac;zlik</trd>
  <trd lng="nl">stinken</trd>
  <trdgrp lng="pl">
    <trd>&sacute;mierdzie&cacute;</trd>,
    <trd>cuchn&aogonek;&cacute;</trd>,
    <trd>wonie&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_n;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_m;&c_je;&c_r;&c_d;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zap&aacute;cha&tcaron;</trd>
</drv>

<drv mrk="stink.0o">
  <kap><tld/>o</kap>
  <snc mrk="stink.0o.fetoro">
    <dif>
      <ref tip="dif" cel="odor.malbon0o">Malbonodoro</ref>:
      <ekz>
        al la aliaj &scirc;ajne ne gravis la <tld/>o kaj malpura&jcirc;o
        <fnt>
          <vrk><url ref="http://agrao.50webs.com/gandhi/eksperimentoj/03/13_denove_en_barato.htm"
              >La A&ubreve;tobiografio de Gandhi</url></vrk>
          <lok>Parto 3. 13. Denove en Barato</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="odor.malbon0o">odora&ccirc;o</ref>,
      <ref cel="fetor.0o">fetoro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_m;&c_ja;&c_r;&c_d;&c_z;&c_je;&c_c;&c_mol;</trd>
  <trd lng="be">&c_s;&c_m;&c_u;&c_r;&c_o;&c_d;</trd>
  <trd lng="de">Gestank</trd>
  <trd lng="fa">&a_b;&a_w;&f_g;&a_n;&a_d;</trd>
  <trdgrp lng="fr">
    <trd>puanteur</trd>,
    <trd>infection<klr tip="ind"> (puanteur)</klr></trd>,
    <trd>pestilence</trd>
  </trdgrp>
  <trd lng="hu">b&udblac;z</trd>
  <trd lng="nl">stank</trd>
  <trdgrp lng="pl">
    <trd>fetor</trd>,
    <trd>smr&oacute;d</trd>,
    <trd>od&oacute;r</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_n;&c_mol;</trd>,
    <trd>&c_s;&c_m;&c_r;&c_a;&c_d;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_v;&c_o;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>

