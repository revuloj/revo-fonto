  <rad>obe</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="obe.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <snc mrk="obe.0i.KOMUNE">
    <gra><vspec>x</vspec></gra>
    <dif>
      Konformi&gcirc;i je tio, kion alia persono ordonas a&ubreve; malpermesas;
      konformi&gcirc;i al regulo:
      <ekz>
        la &scirc;ipanoj devas <tld/>i la &scirc;ipestron
        <fnt>
          <bib>F</bib>
          <lok>&FE; 37</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi ne <tld/>is mian konsilon
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[civitano]</klr> devas <tld/>i &ccirc;iam nur klarajn, tute difinitajn
        fundamentajn le&gcirc;ojn
        <fnt>
          <bib>F</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jakob</nom> <tld/>is sian patron kaj sian patrinon kaj iris Mezopotamion
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 28:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi <tld/>is la <klr>(tentan)</klr> vo&ccirc;on de via edzino
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 3:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>e sekvis &scirc;iajn petojn, sugestojn kaj ideojn
        <fnt>
          <bib>Kon11</bib>
          <lok>Tiel la mondo...</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Jozef</nom> <klr>[&#8230;]</klr> ne volis <tld/>i &scirc;in kaj ku&scirc;i&gcirc;i kun &scirc;i kaj esti kun &scirc;i
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 39:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni ne <tld/>os al vi
        <fnt>
          <bib>MT</bib>
          <lok>Jeremia 44:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pastroj <tld/>is al la fido
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 6:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        se via interna sento ion diras kontra&ubreve; tio &ccirc;i, tiam <tld/>u al &gcirc;i
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;evalo <tld/>is lin <klr>[&#8230;]</klr> kaj iris la &gcirc;ustan vojon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li devis <tld/>i la sorton kaj pacience sekvi
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i unu el tiuj hontemaj kaj delikategaj sentoj, kiuj povas &gcirc;ermi en solaj
        superaj animoj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la lingvo internacia ni devas <tld/>i
        <klr>(<ref tip="super" cel="observ.0i.obei">observi<sncref/></ref>)</klr>
        sole nur la logikon
        <fnt>
          <bib>LR</bib>
          <lok>Dua Parto &ndash; Gramatiko</lok>
        </fnt>;
      </ekz>
      <ekz>
        respektu Dion kaj re&gcirc;on kaj <tld/>u la le&gcirc;on
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        feli&ccirc;o le&gcirc;on ne <tld/>as, subite naski&gcirc;as, subite pereas
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        deziro kaj inklino ordonon ne <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ced.0i">cedi</ref>,
      <ref cel="klin.0igxi.FIG_cedi">klini&gcirc;i<sncref/></ref>,
      <ref cel="plen.0umi.efektivigi">plenumi</ref>,
      <ref cel="sekv.0i.agi_konforme">sekvi<sncref/></ref>,
      <ref cel="met.sub0i">submeti&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="bg">&c_p;&c_o;&c_d;&c_ch;&c_i;&c_n;&c_ja;&c_v;&c_a;&c_t;</trd>
  <trd lng="br">senti&ntilde;</trd>
  <trdgrp lng="cs">
    <trd>db&aacute;t na&rcaron;&iacute;zen&iacute;</trd>,
    <trd>dodr&zcaron;ovat</trd>,
    <trd>poslechnout</trd>,
    <trd>poslouchat</trd>,
    <trd>uposlechnout</trd>,
    <trd>zachovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>gehorchen</trd>,
    <trd><klr>(einem Befehl) </klr>nachkommen</trd>,
    <trd>sich <ind>f&uuml;gen</ind></trd>
  </trdgrp>
  <trd lng="el">&ypsilon;&pi;&alfa;&kappa;&omikron;&ypsilon_ton;&omega;</trd>
  <trd lng="en">obey</trd>
  <trd lng="es">obedecer</trd>
  <trd lng="fr">ob&eacute;ir</trd>
  <trd lng="hu">engedelmeskedik</trd>
  <trdgrp lng="it">
    <trd>obbedire</trd>,
    <trd>ubbidire</trd>
  </trdgrp>
  <trd lng="kek">ab&#789;ink</trd>
  <trd lng="nl">gehoorzamen</trd>
  <trdgrp lng="pl">
    <trd>pos&lstroke;usznym by&cacute;</trd>,
    <trd>stosowa&cacute; si&eogonek;</trd>,
    <trd>ulega&cacute;</trd>
  </trdgrp>  
  <trd lng="pt">obedecer</trd>
  <trdgrp lng="ro">
    <trd>s&abreve; fii ascult&abreve;tor</trd>,
    <trd>s&abreve; se aplice</trd>,
    <trd>s&abreve; coboare</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_v;&c_i;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_ch;&c_i;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>podriadi&tcaron; sa</trd>,
    <trd>posl&uacute;cha&tcaron;</trd>
  </trdgrp>
  <trd lng="sv">lyda</trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_r;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_h;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_h;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_b;&c_u;&c_t;&c_i; &c_p;&c_o;&c_s;&c_l;&c_u;&c_sh;&c_n;&c_i;&c_m;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_k;&c_o;&c_r;&c_ja;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="obe.0ado">
  <kap><tld/>ado</kap>
  <snc mrk="obe.0ado.KOMUNE">
    <dif>
      Plenumo de ies volo:
      <ekz>
        <klr>[li]</klr> mokas la patron kaj mal&scirc;atas <tld/>adon al la patrino
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 30:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;uldata <tld/>ado
        <fnt>
          <bib>Hamlet</bib>
          <lok>Hamleto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &jcirc;uru al mi fidelecon kaj <tld/>adon &gcirc;is la morto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>!
      </ekz>
      <ekz>
        silenta <tld/>ado
        <fnt>
          <bib>Ifigenio</bib>
          <lok>Ifigenio en Ta&ubreve;rido</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj fleksemaj kaj kutimantaj al sklava <tld/>ado
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        li memoras la <tld/>adon de vi &ccirc;iuj, kiel kun timo kaj tremo vi lin akceptis
        <fnt>
          <bib>NT</bib>
          <lok>II. Korintanoj 7:15</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="discip.0o">disciplino</ref>,
      <ref cel="jug.0o">jugo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bg">&c_p;&c_o;&c_k;&c_o;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="br">sentidigezh</trd>
  <trd lng="cs">poslouch&aacute;n&iacute;</trd>
  <trd lng="de">Gehorsam</trd>
  <trd lng="el">&ypsilon;&pi;&alfa;&kappa;&omikron;&eta_ton;</trd>
  <trd lng="en">obedience</trd>
  <trd lng="fr">ob&eacute;issance</trd>
  <trd lng="hu">engedelmess&eacute;g</trd>
  <trd lng="nl">gehoorzaamheid</trd>
  <trd lng="pl">ci&aogonek;g&lstroke;e pos&lstroke;usze&nacute;stwo</trd>
  <trd lng="pt">obedi&ecirc;ncia</trd>
  <trd lng="ro">ascultarea continu&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_v;&c_i;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_ch;&c_i;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_l;&c_u;&c_sh;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">poslu&scaron;nos&tcaron; <klr>(&ccaron;innos&tcaron;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_k;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_l;&c_u;&c_h;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_h;&c_n;&c_ja;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>


<drv mrk="obe.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="obe.0ema.KOMUNE">
    <dif>
      Facile aganta la&ubreve; ordonoj:
      <ekz>
        se vi estos humilaj kaj <tld/>emaj, vi man&gcirc;os <klr>[bone]</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 1:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvankam vi estis la sklavoj de peko, vi tamen fari&gcirc;is kore <tld/>emaj al tiu
        tipo de instruado
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 6:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sin humiligis kaj fari&gcirc;is <tld/>ema &gcirc;is morto
        <fnt>
          <bib>NT</bib>
          <lok>Filipianoj 2:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i venkis sian maldiligentecon: estis laborema kaj <tld/>ema
        <fnt>
          <bib>ElektFab</bib>
          <lok>La maljunulino Holle</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estas via <tld/>ema servanto
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i <klr>[&#8230;]</klr> ka&ubreve;ris anta&ubreve; la knabino kiel 
        <tld/>ema hundido
        <fnt>
          <aut>E. R. Burroughs, trad. K. R. C. Sturmer</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0029/index.php">
          Princino de Marso</url></vrk>,
          <lok>1938</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_l;&c_u;&c_h;&c_m;&c_ja;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_o;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="cs">poslu&scaron;n&yacute;</trd>
  <trdgrp lng="de">
    <trd>gehorsam</trd>,
    <trd>folgsam</trd>,
    <trd>gef&uuml;gig</trd>
  </trdgrp>
  <trd lng="fr">ob&eacute;issant</trd>
  <trdgrp lng="pl">
    <trd>potulny</trd>,
    <trd>uleg&lstroke;y</trd>,
    <trd>pos&lstroke;uszny</trd>,
    <trd>karny</trd>
  </trdgrp>  
  <trdgrp lng="ro">
    <trd>docil</trd>,
    <trd>supus</trd>,
    <trd>ascult&abreve;</trd>,
    <trd>punitiv</trd>
  </trdgrp>  
  <trd lng="sk">poslu&scaron;n&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_l;&c_u;&c_h;&c_n;&c_ja;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_l;&c_u;&c_sh;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="obe.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="obe.0igi.KOMUNE">
    <dif>
      Igi <tld/>anta:
      <ekz>
        mi konas rimedojn por min <tld/>igi
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        li havis ekskuan tonon de supera viro, kiu scias komandi kaj sin <tld/>igi
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        amo ne estas ja la plej granda &gcirc;ojo kaj oni ne devas <tld/>igi al la amo &ccirc;iujn
        ceterajn sentojn
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni <tld/>igas tiun le&gcirc;on per armiloj
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Ok</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>igas al si la ceterajn kaj devigas ilin al &ccirc;iam pli dan&gcirc;eraj ludoj
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 2 (100)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dikt.0i">dikti</ref>,
      <ref cel="ordon.0i">ordoni</ref>,
      <ref cel="reg.0i">regi</ref>,
      <ref cel="met.sub0i">submeti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="br">
    <trd>plega&ntilde;<klr tip="ind"> (lakaat da senti&ntilde;)</klr></trd>,
    <trd>lakaat da <ind>senti&ntilde;</ind></trd>
  </trdgrp>
  <trd lng="cs">p&rcaron;inutit k poslu&scaron;nosti</trd>
  <trdgrp lng="de">
    <trd><ind>gef&uuml;gig</ind> machen</trd>,
    <trd>unterwerfen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>bring into <ind>allegiance</ind></trd>,
    <trd>bring to <ind>heel</ind></trd>,
    <trd>subdue</trd>,
    <trd>train</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>diriger<klr tip="amb"> (faire ob&eacute;ir)</klr></trd>,
    <trd>mener<klr tip="amb"> (faire ob&eacute;ir)</klr></trd>
  </trdgrp>
  <trd lng="hu">engedelmess&eacute;gre k&eacute;nyszer&iacute;t</trd>
  <trd lng="nl">doen <ind>gehoorzamen</ind></trd>
  <trdgrp lng="pl">
    <trd>podporz&aogonek;dkowa&cacute; sobie</trd>,
    <trd>ujarzmi&cacute;</trd>,
    <trd>poskromi&cacute;</trd>
  </trdgrp>  
  <trdgrp lng="ro">
    <trd>s&abreve; ne supunem</trd>,
    <trd>&icirc;mbl&acirc;nzi</trd>,
    <trd>&icirc;mpiedic&abreve;</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_d;&c_ch;&c_i;&c_n;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_mol;
    <ind>&c_s;&c_l;&c_u;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</ind></trd>
  </trdgrp>
  <trd lng="sk">prin&uacute;ti&tcaron; k&nbsp;poslu&scaron;nosti</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_m;&c_u;&c_s;&c_i;&c_t;&c_i; &c_s;&c_l;&c_u;&c_h;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_k;&c_o;&c_r;&c_i;&c_t;&c_i; &c_s;&c_o;&c_b;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="obe.mal0i">
  <kap>
    mal<tld/>i
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="star.kontraux0i">Kontra&ubreve;stari</ref> la ordonon de alia persono a&ubreve; regulon:
      <ekz>
        e&ccirc; ne pensu mal<tld/>i la cezaron
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj obstini&gcirc;is kaj mal<tld/>is
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 19:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>i la le&gcirc;ojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi mal<tld/>as al la universala sa&gcirc;o
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi mal<tld/>is la regularon, <klr>[&#8230;]</klr> mi konas nur miajn instrukciojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ne kura&gcirc;os mal<tld/>i sian patron
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi mal<tld/>as la ordonon de mia devo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi mal<tld/>is viajn karajn principojn mensogante
        <fnt>
          <bib>CKv</bib>
          <lok>12</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="obstin.0i">obstini</ref>,
      <ref cel="ofend.0i.ion">ofendi<sncref/></ref>,
      <ref cel="rezist.0i.KOMUNE">rezisti</ref>,
      <ref cel="spit.0i">spiti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>sich <ind>widersetzen</ind></trd>,
    <trd>trotzen</trd>
  </trdgrp>
</drv>









<drv mrk="obe.mal0ema">
  <kap>mal<tld/>ema</kap>
  <snc mrk="obe.mal0ema.KOMUNE">
    <dif>
      Kiu ne elmontras <tld/>on, ne facile submeti&gcirc;as,
      <ref tip="vid" cel="spit.0ema">spitema</ref>:
      <ekz>
        mal<tld/>ema tono
        <fnt><bib>DOz</bib>, <lok>&leftquot;la proceso kontra&ubreve; la katido
        He&ubreve;reka&rightquot;</lok></fnt>;
      </ekz>
      <ekz>
        mi konas vian mal<tld/>emecon kaj vian malmolan nukon
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 31:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;iaj mal<tld/>emaj haroj ne volis glate ku&scirc;i
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;aptiro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[li]</klr> pene provas tra&scirc;ovi mal<tld/>eman kudrilon tra dika ledo
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi estis mal<tld/>emaj kontra&ubreve; la Eternulo
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 9:7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_p;&c_a;&c_s;&c_l;&c_u;&c_h;&c_m;&c_ja;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_a;&c_k;&c_o;&c_r;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_n;&c_je;&c_p;&c_o;&c_k;&c_o;&c_r;&c_je;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>neposlu&scaron;n&yacute;</trd>,
    <trd>vzpurn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>widerspenstig</trd>,
    <trd>trotzig</trd>,
    <trd>eigensinnig</trd>
  </trdgrp>
  <trdgrp lng="en">
      <trd>defiant</trd>
      <trd>disobedient</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;sob&eacute;issant</trd>
  <trd lng="nl">ongehoorzaam</trd>
  <trdgrp lng="pl">
    <trd>krn&aogonek;brny</trd>,
    <trd>narowisty  <klr>(o koniu)</klr></trd>,
    <trd>niesforny</trd>,
    <trd>niepos&lstroke;uszny</trd>
  </trdgrp>  
  <trdgrp lng="ro">
    <trd>&icirc;nc&abreve;p&abreve;&#539;&acirc;nat</trd>,
    <trd>n&abreve;stase  <klr>(cu un cal)</klr></trd>,
    <trd>nesf&acirc;r&scedil;it&abreve;</trd>,
    <trd>neascult&abreve;tor</trd>
  </trdgrp>  
  <trd lng="sk">neposlu&scaron;n&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_p;&c_o;&c_s;&c_l;&c_u;&c_sh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_l;&c_u;&c_h;&c_n;&c_ja;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>


<drv mrk="obe.0eal">
  <kap><tld/>e al</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="obe.0eal.KOMUNE">
    <dif>
      <ref tip="super" cel="laux.0.io_obeata">La&ubreve;</ref>
      regulo de...; <ref cel="konfor.0eal.KOMUNE">konforme al</ref>:
      <ekz>
        <tld/>e al la dia ordono
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="https://www.monato.be/2012/010743.php"
              >Mezepoka nederlanda kavalirromano</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        la trupo <klr>[&#8230;]</klr> <tld/>e al la ordono de la &ccirc;efo, 
        envici&gcirc;as kaj ekmar&scirc;as la&ubreve; la herba deklivo
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="laux.0.io_obeata">la&ubreve;<sncref/></ref>,
    </refgrp>
  </snc>
  <trd lng="be">&c_u; <ind>&c_a;&c_d;&c_p;&c_a;&c_v;&c_je;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind> &c_z;</trd>
  <trdgrp lng="de">
    <trd>gem&auml;&szlig;</trd>,
    <trd>entsprechend</trd>,
    <trd>weisungsgem&auml;&szlig;</trd>
  </trdgrp>
  <trd lng="fr">conform&eacute;ment &agrave;</trd>
  <trd lng="pl">pos&lstroke;usznie</trd>
  <trd lng="ro">ascult&abreve;</trd>
</drv>

