<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Karon</rad>/o  <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="karon.0o">
  <kap><tld/>o</kap>
  <snc mrk="karon.0o.MIT">
    <uzo tip="fak">MIT</uzo>
    <uzo tip="fak">BELE</uzo>
    <ref tip="dif" cel="hxaron.0o.MIT">&Hcirc;arono</ref>
  </snc>
  <snc mrk="karon.0o.AST">
    <uzo tip="fak">AST</uzo>
    <ref tip="dif" cel="hxaron.0o.AST">&Hcirc;arono</ref>
    <ekz>
      &rightquot;New Horizons&#8221; sendis al Tero multajn detalan bildojn
      de Plutono kaj &gcirc;ia luno <tld/>o
        <fnt>
          <aut>A. Wandel</aut>,
          <vrk><url ref="http://sezonoj.ru/2018/01/wandel-3">
          Novaj horizontoj trans Plutono</url></vrk>,
          <lok>La Ondo de Esperanto, 2017:12</lok>
        </fnt>.
    </ekz>
  </snc>
  <trd lng="ca">Caront</trd>
  <trdgrp lng="cs">
    <trd>Charon</trd>,
    <trd>Char&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">Charon</trd>
  <trd lng="it">Caronte</trd>
  <trd lng="nl">Charon</trd>
  <trd lng="pl">Charon</trd>     
  <trd lng="ro">Charon</trd>
  <trd lng="sk">Ch&aacute;ron</trd>
</drv>
</art>

