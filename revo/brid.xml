<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brid.xml,v 1.41 2024/11/06 01:54:41 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>brid</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="brid.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="brid.0o.RAJ">
    <dif>
      Kap<ref tip="super" cel="rimen.0o">rimeno</ref> por konduki
      <ref cel="cxeval.0o.genro">&ccirc;evalo</ref>n a&ubreve; alian beston:
      <ekz>
        rajdas mi sen <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni al la &ccirc;evaloj enmetas la <tld/>ojn en la bu&scirc;ojn, por ke ili obeu al ni
        <fnt>
          <bib>NT</bib>
          <lok>Jakobo 3:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne estu <klr>[&#8230;]</klr> kiel senprudenta mulo, al kiuj oni devas kateni la bu&scirc;on
        per <tld/>o kaj bu&scirc;peco
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 32:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        vipo estas por &ccirc;evalo, <tld/>o por azeno
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 26:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> kondukis la &ccirc;evalon de l' princo per la <tld/>oj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li metos <tld/>on sur la absolutismon de l' pastroj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ri&ccirc;eco sen gvido kiel &ccirc;evalo sen <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="rimen.konduk0o">gvidrimeno</ref>,
      <ref cel="konduk.0ilo.rimeno">kondukilo<sncref/></ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#32366;&#32361; <pr>ji&amacron;ngsh&eacute;ng</pr></trd>,
    <trd>&#39340;&#37532; <pr>m&#462;xi&aacute;n</pr></trd>,
    <trd>&#38849;&#32361; <pr>ji&amacron;ngsheng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_b;&c_r;&c_o;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>brida</trd>,
    <trd>regna</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>brzda</trd>,
    <trd>tlumen&iacute;</trd>,
    <trd>uzda</trd>,
    <trd>z&aacute;brana</trd>
  </trdgrp>
  <trd lng="de">Z&uuml;gel</trd>
  <trd lng="en">bridle</trd>
  <trdgrp lng="es">
   <trd>brida</trd>,
   <trd>rienda</trd>
  </trdgrp>
  <trd lng="fr">bride</trd>
  <trd lng="he">&resh;&#1462;&samekh;&#1462;&fnun;</trd>
  <trd lng="hu">k&ouml;t&odblac;f&eacute;k</trd>
  <trd lng="ja">&#39340;&#21202;  <pr>&#12400;&#12429;&#12367;</pr></trd>
  <trd lng="nl">teugel</trd>
  <trdgrp lng="pl">
    <trd>cugle</trd>,
    <trd>uzda</trd>,
    <trd>lejce</trd>,
    <trd>wodze</trd>,
    <trd>smycz</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>freio <klr>(para cavalos)</klr></trd>,
    <trd>entrave</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_z;&c_d;&c_a;</trd>,
    <trd>&c_u;&c_z;&c_d;&c_je;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kant&aacute;r</trd>,
    <trd>uzda</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_u;&c_z;&c_d;&c_a;</trd>,
    <trd>&c_v;&c_u;&c_z;&c_d;&c_je;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_n;&c_u;&c_z;&c_d;&c_je;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="brid.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="brid.0i.RAJ">
    <dif>
      Surmeti <tld/>on al:
      <ekz>
        <klr>[ili]</klr> tuj salte levi&gcirc;is kaj komencis <tld/>i la &ccirc;evalojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni devis sendan&gcirc;erigi la simion kaj <tld/>i &gcirc;in per &scirc;nuro
        <fnt>
          <bib>QuV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="jung.0i.RAJ">jungi<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_b;&c_r;&c_y;&c_t;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_k;&c_ib;&c_l;&c_z;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ca">embridar</trd>
    <trdgrp lng="de">
      <trd>halftern</trd>,
      <trd>die <ind>Z&uuml;gel</ind> anlegen</trd>
    </trdgrp>
    <trd lng="en">to bridle</trd>
    <trd lng="es">poner riendas</trd>
    <trd lng="hu">k&ouml;t&odblac;f&eacute;ket tesz r&aacute;</trd>
    <trdgrp lng="pl">
      <trd>nak&lstroke;ada&cacute; <ind>uzd&eogonek;</ind></trd>
      <trd>zak&lstroke;ada&cacute; <ind>smycz</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_z;&c_n;&c_u;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="brid.0i.malrapidigi">
    <dif>
      Malrapidigi beston tirante la <tld/>on:
      <ekz>
        <nom>Hamul</nom> ek<tld/>is sian kamelon, kaj kun silenta admiro rigardis la okulfrapan
        vida&jcirc;on
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        nedecideme kaj nerve &scirc;i <tld/>is la &ccirc;evalon, kaj &ccirc;irka&ubreve;rigardis, ne sciante, kie
        &scirc;i sin trovas
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brems.0i.MAS">bremsi</ref>,
      <ref cel="halt.0igi.senmovigi">haltigi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_r;&c_y;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_p;&c_y;&c_n;&c_ja;&c_c;&c_mol; <klr>(&c_zh;&c_y;&c_v;&c_jo;&c_l;&c_u; &c_z;&c_a; &c_a;&c_b;&c_r;&c_o;&c_c;&c_mol;)</klr></trd>
    </trdgrp>
    <trd lng="de">z&uuml;geln</trd>
    <trdgrp lng="zh">
    <trd>&#32004;&#26463; <pr>yu&emacron;sh&ugrave;</pr></trd>,
    <trd>&#21202; <pr>l&egrave;</pr></trd>,
    <trd>&#20811;&#21046; <pr>k&egrave;zh&igrave;</pr></trd>,
    <trd>&#32560; <pr>ji&amacron;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="brid.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Moderigi, kvietigi, obeigi:
      <ekz>
        mi <tld/>os mian bu&scirc;on, kiam malpiulo staras kontra&ubreve; mi
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 39:1</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>ante sian langon
        <fnt>
          <bib>NT</bib>
          <lok>Jakobo 1:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        homo, kapabla <tld/>i anka&ubreve; la tutan korpon
        <fnt>
          <bib>NT</bib>
          <lok>Jakobo 3:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> sidis silente, koleron en sian koron <tld/>inte
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Elsa</nom> ne povis <tld/>i sian scivolon
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> maljuna Kristoforo <klr>[&#8230;]</klr> &scirc;atis moderan &gcirc;uon kaj
        <tld/>itan &gcirc;ojon
        <fnt>
          <bib>HsT</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brems.0i.FIG">bremsi<sncref/></ref>,
      <ref cel="buton.0umi">butonumi</ref> malvaste,
      <ref cel="katen.0i.FIG">kateni</ref>,
      <ref cel="reg.0i">regi</ref>,
      <ref cel="ten.0i">teni</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_u;&c_t;&c_a;&c_j;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_u;&c_c;&c_ib;&c_h;&c_a;&c_m;&c_ib;&c_r;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>contenir</trd>,
      <trd>moderar</trd>,
      <trd>reprimir</trd>
    </trdgrp>
    <trd lng="de">z&uuml;geln</trd>
    <trdgrp lng="en">
      <trd>to bridle</trd>,
      <trd>restrain</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>contener</trd>,
      <trd>moderar</trd>,
      <trd>reprimir</trd>
    </trdgrp>
    <trd lng="nl">beteugelen</trd>
    <trd lng="pl">kie&lstroke;zna&cacute;</trd>
    <trd lng="ru">&c_o;&c_b;&c_u;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>
    <trdgrp lng="zh">
    <trd>&#36335;&#36793; <pr>l&ugrave;bi&amacron;n</pr></trd>,
    <trd>&#38849;&#32361; <pr>ji&amacron;ngsheng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="cs">
    <trd>krotit</trd>,
    <trd>nasadit uzdu</trd>
  </trdgrp>
  <trd lng="fr">brider</trd>
  <trd lng="he">&lamed;&resh;&samekh;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>megf&eacute;kez</trd>,
    <trd>f&eacute;ken tart</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#39340;&#21202;&#12434;&#12388;&#12369;&#12427; <pr>&#12358;&#12414;&#12429;&#12367;&#12434;&#12434;&#12388;&#12369;&#12427;</pr></trd>,
    <trd>&#25163;&#32177;&#12434;&#32224;&#12417;&#12427; <pr>&#12383;&#12389;&#12394;&#12434;&#12375;&#12417;&#12427;</pr></trd>,
    <trd>&#25233;&#21046;&#12377;&#12427;  <pr>&#12424;&#12367;&#12379;&#12356;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>da&tcaron; na uzdu</trd>,
    <trd>skroti&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_g;&c_n;&c_u;&c_z;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_g;&c_n;&c_u;&c_z;&c_d;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="brid.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="brid.ne0ebla.KOMUNE">
    <dif>
      Tiel <ref tip="vid" cel="impet.0a">impeta</ref>,
      ke &gcirc;in oni ne povas <tld/>i, &ccirc;esigi, malfortigi:
      <ekz>
        ne<tld/>ebla furiozo
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>;
      </ekz>
      <ekz>
        perfidus lin lia ne<tld/>ebla ridado
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        elmetitaj al la krudaj, ne<tld/>eblaj naturaj fort-impulsoj
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        pli simpla solvo estus eviti la tutmondan plivarmi&gcirc;on, la ka&ubreve;zon de tiom da
        ne<tld/>eblaj komplika&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2007/009243.php">Epidemiaj sekvoj en
          E&ubreve;ropo</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="liber.0a.senbara">libera<sncref/></ref>,
      <ref cel="prem.nesub0ebla.KOMUNE">nesubpremebla</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#27178; <pr>h&egrave;ng</pr></trd>,
    <trd>&#34403; <pr>b&agrave;o</pr></trd>,
    <trd>&#24717; <pr>h&agrave;n</pr></trd>,
    <trd>&#30445; <pr>l&igrave;</pr></trd>,
    <trd>&#29378;&#28872; <pr>ku&aacute;ngli&egrave;</pr></trd>,
    <trd>&#29378;&#36481; <pr>ku&aacute;ngz&agrave;o</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_w;&c_t;&c_a;&c_j;&c_m;&c_o;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja;&c_s;&c_t;&c_r;&c_y;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>unb&auml;ndig</trd>,
    <trd>ungest&uuml;m</trd>
  </trdgrp>
  <trd lng="fr">irr&eacute;fr&eacute;nable</trd>
  <trdgrp lng="ja">
    <trd>&#39164;&#12356;&#39348;&#12425;&#12379;&#12394;&#12356;  <pr>&#12363;&#12356;&#12394;&#12425;&#12379;&#12394;&#12356;</pr></trd>,
    <trd>&#24481;&#12375;&#12364;&#12383;&#12356;  <pr>&#12366;&#12423;&#12375;&#12364;&#12383;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="pl">nieokie&lstroke;znany</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_b;&c_o;&c_r;&c_k;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_v;&c_t;&c_r;&c_i;&c_m;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_t;&c_r;&c_i;&c_m;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="brid.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="brid.sen0a.RAJ">
    <dif>
      Ne malrapidigita de <tld/>o:
      <ekz>
        dum la sen<tld/>a galopado de la &ccirc;evaloj &scirc;i montris eksplodan &gcirc;ojon
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_s;&c_k;&c_ib;&c_l;&c_z;&c_a;&c_n;&c_y;</trd>
    <trd lng="hu">k&ouml;t&odblac;f&eacute;k n&eacute;lk&uuml;li</trd>
    <trdgrp lng="pl">
      <trd>niesforny</trd>,
      <trd>&zdot;ywio&lstroke;owy</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#19978;&#33080; <pr>sh&agrave;ngli&#462;n</pr></trd>,
    <trd>&#20572;&#19981;&#19979;&#20358; <pr>t&iacute;ngb&ugrave;xi&agrave;l&aacute;i</pr></trd>,
    <trd>&#21049;&#19981;&#20303; <pr>ch&agrave;b&uacute;zh&ugrave;</pr></trd>,
    <trd>&#27794;&#26377;&#32004;&#26463; <pr>m&eacute;iy&#466;uyu&emacron;sh&ugrave;</pr></trd>,
    <trd>&#27794;&#26377;&#38480;&#21046; <pr>m&eacute;iy&#466;uxi&agrave;nzh&igrave;</pr></trd>,
    <trd>&#29378;&#28872; <pr>ku&aacute;ngli&egrave;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="brid.sen0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref cel="moder.mal0a">Malmodera</ref>, <ref cel="lim.sen0a">senlima</ref>,
      trolibera:
      <ekz>
        kor&#8217; sova&gcirc;a kaj sen<tld/>a
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        la seka tono <klr>[&#8230;]</klr> kvietigis la sen<tld/>an fantazion
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        ambicia, enviema, postulema kaj sen<tld/>a en siaj postuloj &scirc;i estis
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        infano mia, balbutis Maziero, <klr>[&#8230;]</klr> kortu&scirc;ita de tiu sen<tld/>a ammontro
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="impet.0a">impeta</ref>,
      <ref cel="frivol.0a.maldeca">frivola<sncref/></ref>,
      <ref cel="krud.0a">kruda</ref>,
      <ref cel="primit.0a">primitiva</ref>,
      <ref cel="gxen.sen0a.senhonta">sen&gcirc;ena<sncref/></ref>,
      <ref cel="sovagx.0a">sova&gcirc;a</ref>,
      <ref cel="sponta1.0a">spontanea</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_b;&c_e;&c_sh;&c_ch;&c_a;&c_n;&c_y;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_sh;&c_ch;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="de">z&uuml;gellos</trd>
    <trdgrp lng="es">
      <trd>sin riendas</trd>,
      <trd>sin moderaci&oacute;n</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>teugelloos</trd>,
      <trd>wild</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>rozpasany</trd>,
      <trd>wyuzdany</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#28010; <pr>l&agrave;ng</pr></trd>,
    <trd>&#36807;&#24230; <pr>gu&ograve;d&ugrave;</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="ca">
    <trd>desbocat</trd>,
    <trd>desenfrenat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bujn&yacute;</trd>,
    <trd>divok&yacute;</trd>,
    <trd>h&yacute;&rcaron;iv&yacute;</trd>,
    <trd>nev&aacute;zan&yacute;</trd>,
    <trd>nezkrotn&yacute;</trd>,
    <trd>prostop&aacute;&scaron;n&yacute;</trd>,
    <trd>rozpustil&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>ungez&uuml;gelt</trd>,
    <trd>ungest&uuml;m</trd>
  </trdgrp>
  <trd lng="en">unbridled</trd>
  <trd lng="fr">d&eacute;brid&eacute;</trd>
  <trd lng="hu">f&eacute;ktelen</trd>
