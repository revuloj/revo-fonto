<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brid.xml,v 1.29 2021/03/05 17:02:56 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>brid</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="brid.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="brid.0o.RAJ">
    <dif>
      Kaprimeno por konduki &ccirc;evalon a&ubreve; alian beston.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_r;&c_o;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>brida</trd>,
    <trd>regna</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>brzda</trd>,
    <trd>tlumen&iacute;</trd>,
    <trd>uzda</trd>,
    <trd>z&aacute;brana</trd>
  </trdgrp>
  <trd lng="de">Z&uuml;gel</trd>
  <trd lng="en">bridle</trd>
  <trdgrp lng="es">
   <trd>brida</trd>,
   <trd>rienda</trd>
  </trdgrp>
  <trd lng="fr">bride</trd>
  <trd lng="he">&#x5E8;&#x5B6;&#x5E1;&#x5B6;&#x5DF;</trd>
  <trd lng="hu">k&ouml;t&odblac;f&eacute;k</trd>
  <trd lng="nl">teugel</trd>
  <trdgrp lng="pl">
    <trd>cugle</trd>,
    <trd>uzda</trd>,
    <trd>lejce</trd>,
    <trd>wodze</trd>,
    <trd>smycz</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>freio <klr>(para cavalos)</klr></trd>,
    <trd>entrave</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_z;&c_d;&c_a;</trd>,
    <trd>&c_u;&c_z;&c_d;&c_je;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kant&aacute;r</trd>,
    <trd>uzda</trd>
  </trdgrp>
</drv>

<drv mrk="brid.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="brid.0i.RAJ">
    <dif>
      Surmeti <tld/>on al.
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_b;&c_r;&c_y;&c_t;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_k;&c_ib;&c_l;&c_z;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ca">embridar</trd>
    <trd lng="en">to bridle</trd>
    <trd lng="es">poner riendas</trd>
    <trd lng="hu">k&ouml;t&odblac;f&eacute;ket tesz r&aacute;</trd>
    <trdgrp lng="pl">
      <trd>nak&lstroke;ada&cacute; <ind>uzd&eogonek;</ind></trd>
      <trd>zak&lstroke;ada&cacute; <ind>smycz</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_z;&c_n;&c_u;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="brid.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Moderigi, kvietigi, obeigi:
      <ekz>
        devo kaj volo <tld/>as &ccirc;iujn pasiojn;
      </ekz>
      <ekz>
        <tld/>i sian bu&scirc;on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brems.0i.FIG">bremsi<sncref/></ref>,
      <ref cel="buton.0umi">butonumi</ref> malvaste,
      <ref cel="katen.0i.FIG">kateni</ref>,
      <ref cel="reg.0i">regi</ref>,
      <ref cel="ten.0i">teni</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_u;&c_t;&c_a;&c_j;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_u;&c_c;&c_ib;&c_h;&c_a;&c_m;&c_ib;&c_r;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>contenir</trd>,
      <trd>moderar</trd>,
      <trd>reprimir</trd>
    </trdgrp>
    <trd lng="de">z&uuml;geln</trd>
    <trdgrp lng="en">
      <trd>to bridle</trd>,
      <trd>restrain</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>contener</trd>,
      <trd>moderar</trd>,
      <trd>reprimir</trd>
    </trdgrp>
    <trd lng="nl">beteugelen</trd>
    <trdgrp lng="pl">
      <trd>kie&lstroke;zna&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_b;&c_u;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>krotit</trd>,
    <trd>nasadit uzdu</trd>
  </trdgrp>
  <trd lng="fr">brider</trd>
  <trd lng="he">&#x5DC;&#x5E8;&#x5E1;&#x5DF;</trd>
  <trdgrp lng="hu">
    <trd>megf&eacute;kez</trd>,
    <trd>f&eacute;ken tart</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>da&tcaron; na uzdu</trd>,
    <trd>skroti&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="brid.ne0ebla">
  <kap>ne<tld/>ebla</kap>
  <snc mrk="brid.ne0ebla.KOMUNE">
    <dif>
      Tiel <ref tip="vid" cel="impet.0a">impeta</ref>,
      ke &gcirc;in oni ne povas <tld/>i, &ccirc;esigi, malfortigi:
      <ekz>
        ne<tld/>ebla furiozo
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="prem.nesub0ebla.KOMUNE">nesubpremebla</ref>
    </refgrp>
  </snc>
  <trd lng="fr">irr&eacute;fr&eacute;nable</trd>
</drv>

<drv mrk="brid.sen0a">
  <kap>sen<tld/>a<fnt>Z</fnt></kap>
  <snc mrk="brid.sen0a.RAJ">
    <dif>
      Liberigita el <tld/>o.
    </dif>
    <trd lng="be">&c_r;&c_a;&c_s;&c_k;&c_ib;&c_l;&c_z;&c_a;&c_n;&c_y;</trd>
    <trd lng="hu">k&ouml;t&odblac;f&eacute;k n&eacute;lk&uuml;li</trd>
  </snc>
  <snc mrk="brid.sen0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senmodera, senlima, trolibera:
      <ekz>
        sen<tld/>a kolero, ambicio, fantazio, pasio.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_b;&c_e;&c_sh;&c_ch;&c_a;&c_n;&c_y;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_sh;&c_ch;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="de">z&uuml;gellos</trd>
    <trdgrp lng="es">
      <trd>sin riendas</trd>,
      <trd>sin moderaci&oacute;n</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>teugelloos</trd>,
      <trd>wild</trd>
    </trdgrp>
    <trd lng="pl">nieokie&lstroke;zany</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>desbocat</trd>,
    <trd>desenfrenat</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bujn&yacute;</trd>,
    <trd>divok&yacute;</trd>,
    <trd>h&yacute;&rcaron;iv&yacute;</trd>,
    <trd>nev&aacute;zan&yacute;</trd>,
    <trd>nezkrotn&yacute;</trd>,
    <trd>prostop&aacute;&scaron;n&yacute;</trd>,
    <trd>rozpustil&yacute;</trd>
  </trdgrp>
  <trd lng="en">unbridled</trd>
  <trd lng="fr">d&eacute;brid&eacute;</trd>
  <trd lng="hu">f&eacute;ktelen</trd>
  <trdgrp lng="pt">
    <trd>desenfreado</trd>,
    <trd>imoderado</trd>,
    <trd>impetuoso</trd>,
    <trd>&aacute;rdego</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_n;&c_u;&c_z;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_b;&c_u;&c_z;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bujn&yacute;</trd>,
    <trd>divok&yacute;</trd>,
    <trd>h&yacute;riv&yacute;</trd>,
    <trd>neviazan&yacute;</trd>,
    <trd>rozpustil&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="brid.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc mrk="brid.sen0igi.RAJ">
    <dif>
      Demeti <tld/>on de.
    </dif>
    <trd lng="be">&c_r;&c_a;&c_s;&c_k;&c_ib;&c_l;&c_z;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>desbridar</trd>,
      <trd>desembridar</trd>
    </trdgrp>
    <trd lng="hu">leveszi a
    <ind>k&ouml;t&odblac;f&eacute;ket</ind></trd>
    <trdgrp lng="pl">
      <trd>spuszcza&cacute; ze <ind>smyczy</ind></trd>,
      <trd>puszcza&cacute; <ind>luzem</ind></trd>,
      <trd>puszcza&cacute; <ind>wolno</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="brid.sen0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Lasi plenan liberecon:
      <ekz>
        la sen<tld/>igita popolo faris bu&ccirc;adon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_r;&c_a;&c_z;&c_b;&c_e;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>
    <trd lng="ca">donar plena llibertat</trd>
    <trd lng="es">dejar plena libertad</trd>
    <trdgrp lng="pl">
      <trd>spuszcza&cacute; ze <ind>smyczy</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">u&ccaron;init nezkrotn&yacute;m</trd>
  <trd lng="en">to unbridle</trd>
  <trd lng="fr">d&eacute;brider</trd>
  <trd lng="hu">szabadj&aacute;ra enged</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_n;&c_u;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>1.s&ncaron;a&tcaron; uzdu</trd>,
    <trd>2.ponecha&tcaron; &uacute;pln&uacute; vo&lcaron;nos&tcaron; <klr>(pren.)</klr></trd>
  </trdgrp>
</drv>
</art>
