<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>brid</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="brid.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="brid.0o.RAJ">
    <dif>
      Kaprimeno por konduki &ccirc;evalon a&ubreve; alian beston.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_r;&c_o;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>brida</trd>,
    <trd>regna</trd>
  </trdgrp>
  <trd lng="en">bridle</trd>
  <trdgrp lng="es">
   <trd>brida</trd>,
   <trd>rienda</trd>
  </trdgrp>
  <trd lng="fr">bride</trd>
  <trd lng="hu">k&ouml;t&odblac;f&eacute;k</trd>
  <trdgrp lng="pl">
    <trd>cugle</trd>,
    <trd>uzda</trd>,
    <trd>lejce</trd>,
    <trd>wodze</trd>,
    <trd>smycz</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>freio <klr>(para cavalos)</klr></trd>,
    <trd>entrave</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_z;&c_d;&c_a;</trd>,
    <trd>&c_u;&c_z;&c_d;&c_je;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">uzda</trd>
</drv>

<drv mrk="brid.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="brid.0i.RAJ">
    <dif>
      Surmeti <tld/>on al.
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_b;&c_r;&c_y;&c_t;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_a;&c_k;&c_ib;&c_l;&c_z;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="ca">embridar</trd>
    <trd lng="es">poner riendas</trd>
    <trd lng="hu">k&ouml;t&odblac;f&eacute;ket tesz r&aacute;</trd>
    <trdgrp lng="pl">
      <trd>nak&lstroke;ada&cacute; <ind>uzd&eogonek;</ind></trd>
      <trd>zak&lstroke;ada&cacute; <ind>smycz</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_z;&c_n;&c_u;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="brid.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Moderigi, kvietigi, obeigi:
      <ekz>
        devo kaj volo <tld/>as &ccirc;iujn pasiojn;
      </ekz>
      <ekz>
        <tld/>i sian bu&scirc;on<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="brems.0i.FIG">bremsi<sncref/></ref>,
      <ref cel="buton.0umimalvaste.FRAZ">butonumi malvaste</ref>,
      <ref cel="katen.0i.FIG">kateni</ref>,
      <ref cel="reg.0i">regi</ref>,
      <ref cel="ten.0i">teni</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_u;&c_t;&c_a;&c_j;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_u;&c_c;&c_ib;&c_h;&c_a;&c_m;&c_ib;&c_r;&c_y;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>contenir</trd>,
      <trd>moderar</trd>,
      <trd>reprimir</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>contener</trd>,
      <trd>moderar</trd>,
      <trd>reprimir</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>kie&lstroke;zna&cacute;</trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_b;&c_u;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="fr">brider</trd>
  <trdgrp lng="hu">
    <trd>megf&eacute;kez</trd>,
    <trd>f&eacute;ken tart</trd>
  </trdgrp>
</drv>

<drv mrk="brid.sen0a">
  <kap>sen<tld/>a<fnt>Z</fnt></kap>
  <snc mrk="brid.sen0a.RAJ">
    <dif>
      Liberigita el <tld/>o.
    </dif>
    <trd
    lng="be">&c_r;&c_a;&c_s;&c_k;&c_ib;&c_l;&c_z;&c_a;&c_n;&c_y;</trd>
    <trd lng="hu">k&ouml;t&odblac;f&eacute;k n&eacute;lk&uuml;li</trd>
  </snc>
  <snc mrk="brid.sen0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Senmodera, senlima, trolibera:
      <ekz>
        sen<tld/>a kolero, ambicio, fantazio, pasio.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_r;&c_a;&c_z;&c_b;&c_e;&c_sh;&c_ch;&c_a;&c_n;&c_y;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_p;&c_u;&c_sh;&c_ch;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>sin riendas</trd>,
      <trd>sin moderaci&oacute;n</trd>
    </trdgrp>
    <trd lng="pl">nieokie&lstroke;zany</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>desbocat</trd>,
    <trd>desenfrenat</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;brid&eacute;</trd>
  <trd lng="hu">f&eacute;ktelen</trd>
  <trdgrp lng="pt">
    <trd>desenfreado</trd>,
    <trd>imoderado</trd>,
    <trd>impetuoso</trd>,
    <trd>&aacute;rdego</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_n;&c_u;&c_z;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_o;&c_b;&c_u;&c_z;&c_d;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="brid.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc mrk="brid.sen0igi.RAJ">
    <dif>
      Demeti <tld/>on de.
    </dif>
    <trd
    lng="be">&c_r;&c_a;&c_s;&c_k;&c_ib;&c_l;&c_z;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="ca">
      <trd>desbridar</trd>,
      <trd>desembridar</trd>
    </trdgrp>
    <trd lng="hu">leveszi a
    <ind>k&ouml;t&odblac;f&eacute;ket</ind></trd>
    <trdgrp lng="pl">
      <trd>spuszcza&cacute; ze <ind>smyczy</ind></trd>,
      <trd>puszcza&cacute; <ind>luzem</ind></trd>,
      <trd>puszcza&cacute; <ind>wolno</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="brid.sen0igi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Lasi plenan liberecon:
      <ekz>
        la sen<tld/>igita popolo faris bu&ccirc;adon<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd
    lng="be">&c_r;&c_a;&c_z;&c_b;&c_e;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>
    <trd lng="ca">donar plena llibertat</trd>
    <trd lng="es">dejar plena libertad</trd>
    <trdgrp lng="pl">
      <trd>spuszcza&cacute; ze <ind>smyczy</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="fr">d&eacute;brider</trd>
  <trd lng="hu">szabadj&aacute;ra enged</trd>
  <trd
  lng="ru">&c_r;&c_a;&c_z;&c_n;&c_u;&c_z;&c_d;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
