<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kli&scirc;</rad>/i
</kap>
<drv mrk="klisx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>on de io.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>klischieren</trd>,
     <trd>ein <ind>Klischee anfertigen</ind></trd>
  </trdgrp>
  <trd lng="nl">clicheren</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_i;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_m;&c_n;&c_o;&c_zh;&c_a;&c_t;&c_mol; &c_k;&c_o;&c_p;&c_i;&c_i;</trd>
  </trdgrp>
</drv>
<drv mrk="klisx.0o">
  <kap><tld/>(a&jcirc;)o</kap>
  <snc num="1">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Reliefa reprodukto sur metala plato de la stampo de komposta&jcirc;o
      el moveblaj literoj.
    </dif>
    <trdgrp lng="de">
      <trd>Druckstock</trd>,
      <trd>Klischee</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="fak">FOT</uzo>
    <dif>
      Fotografia negativo fiksita sur vitro a&ubreve; membrano kaj servanta
      por fari pozitivajn kopiojn. 
    </dif>
    <trdgrp lng="de">
      <trd>Negativ</trd>
    </trdgrp>
    <trd lng="en">negative</trd>
  </snc>
  <snc num="3">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tro uzata senoriginala esprimo:
      <ekz>tiu frazo estas nur <tld/>a&jcirc;o</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gurd">gurdita</ref>.
    </refgrp>
    <refgrp tip="sin">
      <ref cel="banal.0a">banala</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>eingefahrene <ind>Vorstellung</ind></trd>,
      <trd>Klischee</trd>
    </trdgrp>
    <trd lng="en">cliche</trd>
    <trd lng="ru">&c_sh;&c_t;&c_a;&c_m;&c_p;</trd>
  </snc>
  <trd lng="nl">clich&eacute;</trd>
  <trd lng="ru">&c_k;&c_l;&c_i;&c_sh;&c_je;</trd>
</drv>
</art>
