<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>gimnastik</rad>/o
</kap>
<drv mrk="gimnas1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Arto pri ekzercado de la korpo per ta&ubreve;gaj movoj por &gcirc;in plifortigi
      kaj lertigi:
      <ekz>instrui <tld/>on;</ekz>
      <ekz><tld/>aj ludoj, ekzercoj.</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Gymnastik</trd>,
     <trd>Turnen</trd>,
     <trd>Leibes&uuml;bungen</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>gymnastique</trd>,
    <trd>culture physique</trd></trdgrp> 
  <trd lng="ru">&c_g;&c_i;&c_m;&c_n;&c_a;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sv">gymnastik</trd>
</drv>
<drv mrk="gimnas1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Fari <tld/>ajn ekzercojn. 
    </dif>
  </snc>
  <trd lng="fr">faire de la <ind>gymnastique</ind></trd>
  <trd lng="ru">&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_g;&c_i;&c_m;&c_n;&c_a;&c_s;&c_t;&c_i;&c_k;&c_o;&c_j;</trd>
  <trd lng="sv">gymnastisera</trd>
</drv>
<drv mrk="gimnas1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Profesiulo sperta pri <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="gimnas.0o">gimnasto</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="sport">sportisto</ref>,
      <ref cel="akroba.0o">akrobato</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">gymnaste</trd>
  <trd lng="ru">&c_g;&c_i;&c_m;&c_n;&c_a;&c_s;&c_t;</trd>
  <trd lng="sv">gymnast</trd>
</drv>
</art>
