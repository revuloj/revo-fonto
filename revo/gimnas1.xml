<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>gimnastik</rad>/o
</kap>
<drv mrk="gimnas1.0o">
  <kap><tld/>o</kap>
  <snc mrk="gimnas1.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Arto pri ekzercado de la korpo per ta&ubreve;gaj movoj por &gcirc;in plifortigi
      kaj lertigi:
      <ekz>
        instrui <tld/>on;
      </ekz>
      <ekz>
        <tld/>aj ludoj, ekzercoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_m;&c_n;&c_a;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Gymnastik</trd>,
    <trd>Turnen</trd>,
    <trd>Leibes&uuml;bungen</trd>
  </trdgrp>
  <trd lng="en">gymnastics</trd>
  <trd lng="es">gimnasia</trd>
  <trdgrp lng="fr">
    <trd>culture physique</trd>,
    <trd>gymnastique</trd>
  </trdgrp>
  <trd lng="he">&he;&tav;&ayin;&mem;&lamed;&vav;&tav;</trd>
  <trdgrp lng="hu">
    <trd>torna</trd>,
    <trd>testnevel&eacute;s</trd>,
    <trd>gimnasztika</trd>
  </trdgrp>
  <trd lng="nl">gymnastiek</trd>
  <trd lng="pt">gin&aacute;stica</trd>
  <trd lng="ru">&c_g;&c_i;&c_m;&c_n;&c_a;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sv">gymnastik</trd>
</drv>

<drv mrk="gimnas1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="gimnas1.0i.SPO">
    <dif>
      Fari <tld/>ajn ekzercojn:
      <ekz>
        kiam ili forte penadas a&ubreve; <tld/>as, tutkorpe ili &scirc;vitas
        <fnt><aut>Lukiano, trad. G. Berveling</aut>, <vrk><url 
        ref="http://www.esperanto.gr/lit/els/rakontoj.pdf">veraj 
        rakontoj</url></vrk>, <lok>2005</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_c;&c_a; <ind>&c_g;&c_ib;&c_m;&c_n;&c_a;&c_s;&c_t;&c_y;&c_k;&c_a;&c_j;</ind></trd>
  <trd lng="en">do <ind>gymnastics</ind></trd>
  <trd lng="es">hacer <ind>gimnasia</ind></trd>
  <trd lng="fr">faire de la <ind>gymnastique</ind></trd>
  <trd lng="he">&lamed;<ind>&he;&tav;&ayin;&mem;&lamed;</ind></trd>
  <trd lng="hu">torn&aacute;szik</trd>
  <trd lng="nl">turnen</trd>
  <trd lng="pt">fazer <ind>gin&aacute;stica</ind></trd>
  <trd lng="ru">&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja; &c_g;&c_i;&c_m;&c_n;&c_a;&c_s;&c_t;&c_i;&c_k;&c_o;&c_j;</trd>
  <trd lng="sv">gymnastisera</trd>
</drv>

<drv mrk="gimnas1.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="gimnas1.0ejo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="sport.0ejo.SPO">Sportejo</ref> por <tld/>aj 
      ekzercoj, ordinare halo kun tiucelaj ekipa&jcirc;oj:
      <ekz>
        &leftquot;kio &scirc;ajnas al vi plej mokinda?&rightquot; mi demandis. &leftquot;Sendube ke la 
        virinoj luktos nudaj kun la viroj <klr>...</klr> anka&ubreve; la 
        maljunulinoj, tute ne belaspektaj, same kiel la oldaj 
        falti&gcirc;intaj viroj en la <tld/>ejoj
        <fnt>
          <aut>Platono, trad. D. Broadribb</aut>,
          <vrk><url ref="http://esperantujo.org/eLibrejo/pdf/X26-4">
          La Respubliko</url></vrk>,
          <lok>2000</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">gymnase</trd>
</drv>

<drv mrk="gimnas1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="gimnas1.0isto.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Profesiulo sperta pri <tld/>o.
    </dif>
    <refgrp tip="vid">
      <ref cel="akroba.0o">akrobato</ref>,
      <ref cel="atlet.0o.SPO">atleto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_m;&c_n;&c_a;&c_s;&c_t;</trd>
  <trd lng="en">gymnast</trd>
  <trd lng="es">gimnasta</trd>
  <trd lng="fr">gymnaste</trd>
  <trd lng="he">&mem;&tav;&ayin;&mem;&lamed;</trd>
  <trd lng="hu">torn&aacute;sz</trd>
  <trdgrp lng="nl">
    <trd>gymnast</trd>,
    <trd>turner</trd>
  </trdgrp>
  <trd lng="pt">ginasta</trd>
  <trd lng="ru">&c_g;&c_i;&c_m;&c_n;&c_a;&c_s;&c_t;</trd>
  <trd lng="sv">gymnast</trd>
</drv>
</art>
