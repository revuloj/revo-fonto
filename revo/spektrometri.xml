<vortaro>
<art mrk="$Id: spektrometri.xml,v 1.13 2024/03/25 07:28:35 revo Exp $">
<kap>
  <rad>spektrometri</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="spektrometri.0o">
  <kap><tld/>o</kap>
  <snc mrk="spektrometri.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      <ref tip="vid" cel="spektr.0o.FIZ">Spektro</ref>mezuro,
      analizo distinganta la konsista&jcirc;ojn de miksa&jcirc;o
      per iliaj diversaj ondolongoj a&ubreve; masoj:
      <ekz>
        maso-<tld/>o <klr>...</klr> estas aplikebla <klr>...</klr>
        por distingi la&ubreve;kvante inter malsamaj izotopoj de la sama 
        kemia elemento
        <fnt><aut>E. Dudich</aut>, <vrk>&Ccirc;u vi Konas la Teron?</vrk>, 
        <lok>SEC, 1983</lok></fnt>;
      </ekz>
      <ekz>
        atoma <tld/>o kun senflama atomigo uzas por liofilizado 
        de prilaborata specimeno kuveton, varmi&gcirc;anta dum kelkaj 
        sekundoj &gcirc;is proks<klr>imume</klr> 3000&#x2DA;C
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Grafito">grafito</url>, 
          2010-03-27</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kromatografi.0o.KEM">kromatografio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_e;&c_k;&c_t;&c_r;&c_a;&c_m;&c_e;&c_t;&c_r;&c_y;&c_ja;</trd>
  <trd lng="ca">espectrometria</trd>
  <trd lng="cs">spektrometrie</trd>
  <trd lng="en">spectrometry</trd>
  <trd lng="fr">spectrom&eacute;trie</trd>
  <trd lng="ja">&#x5206;&#x5149;&#x6E2C;&#x5149;  <pr>&#x3076;&#x3093;&#x3072;&#x304B;&#x308A;&#x305D;&#x304F;&#x3072;&#x304B;&#x308A;</pr></trd>
  <trd lng="pl">spektrometria</trd>
  <trd lng="sk">spektrometria</trd>
  <trd lng="uk">&c_s;&#x43F;&c_je;&#x43A;&c_t;&c_r;&#x43E;&#x43C;&c_je;&c_t;&c_r;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x983B;&#x8B5C;&#x5206;&#x6790; <pr>p&#xED;np&#x1D4;f&#x113;nx&#x12B;</pr></trd>,
    <trd>&#x9891;&#x8C31;&#x5206;&#x6790; <pr>p&#xED;np&#x1D4;f&#x113;nx&#x12B;</pr></trd>
  </trdgrp>
</drv>
</art>

