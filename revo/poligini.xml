<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: poligini.xml,v 1.14 2025/05/04 16:00:21 revo Exp $">
<kap>
  <rad>poligini</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="poligini.0o">
  <kap><tld/>o</kap>
  <snc mrk="poligini.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="dif" cel="edz.plur0ineco.GEN">Pluredzineco</ref>:
      <ekz>
        la islama religio <klr>[&#8230;]</klr> ekskluzive allasas <tld/>on
        <fnt>
          <aut>P. Cadei</aut>,
          <vrk><url ref="http://www.radio-vatikana-esperanto.org/elsendo.php?d=2008-09-18">
          Poligamio la&ubreve; mormonismo</url></vrk>,
          <lok>Radio Vaticana, 2008-09-18</lok>
        </fnt>;
      </ekz>
      <ekz>
        samkiel poligamio, anka&ubreve; <tld/>o estas malpermesata en granda parto de la mondo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=281724">Poliginio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="poligini.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ordinara pari&gcirc;o de viro kun pluraj inoj:
      <ekz>
        laniedoj estas &gcirc;enerale monogamaj reproduktantoj, kvankam oni
        konstatis <tld/>on en kelkaj specioj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Laniedoj">
          laniedoj</url>, 2011-03-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_g;&c_ib;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="ca">polig&iacute;nia</trd>
  <trdgrp lng="cs">
    <trd>mnoho&zcaron;enstv&iacute;</trd>,
    <trd>polygynie</trd>
  </trdgrp>
  <trd lng="de">Polygynie</trd>
  <trd lng="en">polygamy</trd>
  <trd lng="es">poliginia</trd>
  <trd lng="fr">polygynie</trd>
  <trdgrp lng="ja">
    <trd>&#19968;&#22827;&#22810;&#22971;  <pr>&#12356;&#12387;&#12407;&#12383;&#12373;&#12356;</pr></trd>,
    <trd>&#19968;&#38596;&#22810;&#38604;  <pr>&#12363;&#12378;&#12362;&#12383;&#12417;&#12377;</pr></trd>
  </trdgrp>
  <trd lng="pl">poligynia</trd>
  <trd lng="ro">poliginie</trd>
  <trd lng="sk">mnoho&zcaron;enstvo</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_g;&c_a;&c_t;&c_o;&c_zh;&c_je;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_g;&c_ib;&c_n;&c_ib;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_g;&c_a;&c_m;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#19968;&#22827;&#22810;&#22971; <pr>y&imacron;f&umacron;du&omacron;q&imacron;</pr></trd>,
    <trd>&#22810;&#22971;&#20027;&#32681; <pr>du&omacron;q&imacron;zh&#468;y&igrave;</pr></trd>
  </trdgrp>
</drv>
</art>

