<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>alpar</rad>/o
</kap>
<drv mrk="alpar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Egaleco de valoro, ofte: kurza alparo, t.e. kiam akcia a&ubreve;
      obligacia kurzo atingas sian nominalan valoron. Kp. <ref tip="vid"
      cel="agxi.0o">a&gcirc;io</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Pariwert</trd>,
      <trd>Parit&auml;t</trd>
    </trdgrp>
    <trd lng="en">parity</trd>
    <trd lng="es">paridad</trd>
    <trd lng="ru">&c_a;&c_l;&c_mol;&c_p;&c_a;&c_r;&c_i;</trd>
  </snc>
  <snc>
    <dif>
      Nominala valoro de akcio a&ubreve; obligacio.
    </dif>
    <trdgrp lng="de">
      <trd>Nominalwert</trd>,
      <trd>Nennwert</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>face falue</trd>,
      <trd>nominal value</trd>
    </trdgrp>
    <trd lng="es">valor nominal</trd>
  </snc>
</drv>

<drv mrk="alpar.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Samvalore.
    </dif>
    <trd lng="en">at par</trd>
    <trd lng="de">pari</trd>
  </snc>
  <snc>
    <dif>
      Nominalvalore.
    </dif>
    <trd lng="en">at face value</trd>
    <trdgrp lng="de">
      <trd><klr>zum </klr>Nominalwert</trd>,
      <trd><klr>zum </klr>Nennwert</trd>
    </trdgrp>
  </snc>
</drv>
</art>
