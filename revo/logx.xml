<drv mrk="logx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="logx.0i.propre">
    <dif>
      Da&ubreve;re <ref tip="super" cel="rest.0adi">restadi</ref> en loko,
      kie oni kutime vivadas:
      <ekz>
        li <tld/>as ekster la urbo
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        la rusoj <tld/>as en Rusujo
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
      </ekz>
      <ekz>
        kiu <tld/>is en la domo kontra&ubreve; ni
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>?
      </ekz>
      <ekz>
        se la tero estus <tld/>ata <klr>[&#8230;]</klr> de sa&gcirc;uloj
        <fnt><bib>Lanti</bib>,
          <lok>publika letero al Romen Rolan</lok>
        </fnt>;
      </ekz>
      <ekz>
        insuloj <klr>[&#8230;]</klr> <tld/>ataj de sennombraj birdaj aroj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lando de via fremde<tld/>ado
        <fnt><bib>MT</bib>, <lok>&Gen; 28:4</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>loko
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 10:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en nia karno, en nia malbona naturo <tld/>as nenio bona
        <fnt>
          <bib>Fab3</bib>
          <lok>En la lasta tago</lok>
        </fnt>!
      </ekz>
    </dif>
    <trd lng="de">wohnen</trd>
    <trd lng="nl">wonen</trd>
    <trd lng="pl">mieszka&cacute;</trd>
    <trdgrp lng="pt">
      <trd>morar</trd>,
      <trd>habitar</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="zh">&#x5C45;&#x4F4F; <pr>j&#x16B;zh&ugrave;</pr></trd>
    <trd lng="ms">meninggal</trd>
  </snc>
  <snc mrk="logx.0i.gasti">
    <dif>
      <ref tip="dif" cel="gast.0i">Gasti</ref>:
      <ekz>
        <tld/>i &ccirc;e amiko, en hotelo;
      </ekz>
      <ekz>
        <tld/>i dum kelkaj tagoj &ccirc;e familio de kamparanoj
        <fnt>
       <aut>Maryvonne kaj Bruno Robineau</aut>
          <vrk><url ref="&Monato;/2000/006831.php">La&ubreve; ritmo de la rizejoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/02, p. 18</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>mieszka&cacute;</trd>,
      <trd>zamieszkiwa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">hospedar-se</trd>
    <trd lng="zh">&#x505A;&#x5BA2; <pr>zu&ograve;k&egrave;</pr></trd>
    <trd lng="ms">tetamu</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_zh;&c_i;&c_v;&c_je;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_i;&c_t;&c_a;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="cs">bydlet</trd>
  <trdgrp lng="de">
    <trd>wohnen</trd>,
    <trd>gastieren</trd>,
    <trd>&uuml;bernachten</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4F4F;&#12416;  <pr>&#12377;&#12416;</pr></trd>,
    <trd>&#x5C45;&#x4F4F;&#12377;&#x308B;  <pr>&#x304D;&#12423;&#12376;&#12421;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x6CCA;&#x307E;&#x308B;  <pr>&#12392;&#x307E;&#x308B;</pr></trd>,
    <trd>&#x6EDE;&#22312;&#12377;&#x308B;  <pr>&#x305F;&#12356;&#12374;&#12356;&#12377;&#x308B;</pr></trd>,
    <trd>&#x5BBF;&#x308B;  <pr>&#12420;&#12393;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">logeren</trd>
  <trdgrp lng="en">
    <trd>dwell</trd>,
    <trd>inhabit</trd>,
    <trd>live</trd>,
    <trd>reside</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>habitar</trd>,
    <trd>vivir</trd>,
    <trd>residir</trd>,
    <trd>morar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>habiter</trd>,
    <trd>loger</trd>
  </trdgrp>
  <trd lng="he">&lamed;&gimel;&vav;&resh;</trd>
  <trdgrp lng="hu">
    <trd>lakik</trd>,
    <trd>&eacute;l <klr tip="ind">(lakik)</klr></trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>diam</baz>berkediaman</trd>,
    <trd><baz>domisili</baz>berdomisili</trd>,
    <trd><baz>tempat</baz>bertempat tinggal</trd>,
    <trd>tinggal</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_zh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_i;&c_t;&c_i;</trd>,
    <trd>&#x43F;&c_r;&#x43E;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43C;&c_je;&c_sh;&#x43A;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="logx.0anto">
  <kap><ofc>*</ofc><tld/>anto</kap>
  <snc mrk="logx.0anto.homo">
    <dif>
      Tiu, kiu <tld/>as ie:
      <ekz>
        tiu &ccirc;i urbo havas milionon da <tld/>antoj
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>antoj de <nom>Sodom</nom> estis malbonaj kaj tre pekaj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 13:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>antoj de la lando
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 34:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        alia <tld/>anto de la strato
        <fnt><bib>Fab1</bib>, <lok>galo&scirc;oj de feli&ccirc;o, 5, metamorfozo
        de la skribisto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_y;&c_h;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Bewohner</trd>
  <trdgrp lng="en">
    <trd>dweller</trd>,
    <trd>inhabitant</trd>,
    <trd>resident</trd>
  </trdgrp>
  <trd lng="es">habitante</trd>
  <trdgrp lng="fr">
    <trd>habitant</trd>,
    <trd>occupant<klr> (d'une habitation)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lak&oacute;</trd>,
    <trd>lakos</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>duduk</baz>penduduk</trd>,
    <trd><baz>huni</baz>penghuni</trd>
