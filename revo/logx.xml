<drv mrk="logx.0antaro">
  <kap><tld/>antaro</kap>
  <snc mrk="logx.0antaro.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Tuto de la homoj <tld/>antaj en loko, urbo, provinco, lando...:
      <ekz>
        la <tld/>antaro de la urbo dividi&gcirc;is
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 14:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        bruis la <tld/>antaro de la granda urbo sur la vasta trotuaro
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la tuta <tld/>antaro de la tero
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 9:19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>
  <trd lng="de">Einwohner <klr>(Plur.)</klr></trd>
  <trd lng="en">population</trd>
  <trdgrp lng="fr">
    <trd>habitants</trd>,
    <trd>nombre d'habitants</trd>,
    <trd>population</trd>
  </trdgrp>
  <trd lng="id">populasi</trd>
  <trdgrp lng="ja">
    <trd>&#x4F4F;&#x6C11;  <pr>&#12376;&#12421;&#12358;&#x307F;&#12435;</pr></trd>,
    <trd>&#x4EBA;&#x53E3;  <pr>&#12376;&#12435;&#12371;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="pl">mieszka&nacute;cy</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="uk">&#x43D;&c_a;&c_s;&c_je;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  <trd lng="zh">&#x4EBA;&#x53E3; <pr>r&#xE9;nk&#x1D2;u</pr></trd>
</drv>

<drv mrk="logx.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="logx.0ebla.tauxga">
    <dif>
      Kie oni povas <tld/>i:
      <ekz>
        ili venis en landon <tld/>eblan
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 16:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        iu vendos <tld/>eblan domon en urbo, kiun &ccirc;irka&ubreve;as muro
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 25:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>eblaj lokoj de la tero
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 34:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la insulo restis plejparte ne<tld/>ebla pro venena gaso
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo/pg</aut>
          <vrk><url ref="&Monato;/2005/008590.php">Turismi
          kun gasmasko</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_l;&c_y;</trd>
  <trd lng="de">bewohnbar</trd>
  <trd lng="en">habitable</trd>
  <trd lng="es">habitable</trd>
  <trd lng="fr">habitable</trd>
  <trd lng="hu">lakhat&oacute;</trd>
  <trd lng="id"><ind>layak</ind> huni</trd>
  <trdgrp lng="ja">
    <trd>&#x5C45;&#x4F4F;&#12391;&#x304D;&#x308B;  <pr>&#x304D;&#12423;&#12376;&#12421;&#12358;&#12391;&#x304D;&#x308B;</pr></trd>,
    <trd>&#x5BBF;&#x6CCA;&#12391;&#x304D;&#x308B;
    <pr>&#12375;&#12421;&#x304F;&#x306F;&#x304F;&#12391;&#x304D;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">bewoonbaar</trd>
  <trd lng="pl">mieszkalny</trd>
  <trd lng="pt">habit&aacute;vel</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_l;&c_o;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_g;&c_o;&c_d;&c_n;&c_y;&c_j; &c_d;&c_l;&c_ja; &c_zh;&c_i;&c_l;&c_mol;&c_ja;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&c_r;&c_i;&c_d;&c_a;&c_t;&#x43D;&c_i;&c_j; &c_d;&#x43B;&#x44F; &c_zh;&c_i;&c_t;&#x43B;&c_a;</trd>
  <trd lng="zh">&#x5B9C;&#x5C45;&#30340; <pr>y&iacute; j&#x16B; de</pr></trd>
</drv>

<drv mrk="logx.0ejo">
  <kap><ofc>*</ofc><tld/>ejo</kap>
  <snc mrk="logx.0ejo.ARKI">
    <dif>
      Loko, domo a&ubreve; &ccirc;ambraro, kie oni <tld/>as:
      <ekz>
        &scirc;i prenis la plej belan ar&gcirc;entan vazon, kiu estis en la
        <tld/>ejo
        <fnt><bib>F</bib>, <lok>&FE; 19</lok></fnt>;
      </ekz>
      <ekz>
        mia <tld/>ejo estas mia re&gcirc;ejo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne bruligu fajron en &ccirc;iuj viaj <tld/>ejoj en la tago sabata
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 35:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estu izolita, ekster la tendaro estu lia <tld/>ejo
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:46</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Babel</nom> <klr>[&#8230;]</klr> fari&gcirc;is <tld/>ejo de demonoj
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 18:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta <tld/>ejo <klr>[de la muso]</klr> konsistis el malgranda
        kaverno
        sub la stoploj
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        somer<tld/>ejo
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandsinjora <tld/>ejo
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj
          filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ejo de la surtera eta&gcirc;o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi eklo&gcirc;is en la kel<tld/>ejo
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ejo de la taglaboristino
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> transloki&gcirc;as <klr>[&#8230;]</klr> el la malnova
        <tld/>ejo en la novan
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        post du tagoj estos la fikstempo, en kiu mi devas enporti la
        luan pagon por la
        <tld/>ejo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la domo de mia patro ekzistas multe da <tld/>ejoj, diris
        Kristo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        terura &scirc;losilo, kiu fermas post mi la malliberejon de la vivo
        kaj malfermas
        anta&ubreve; mi la <tld/>ejon de la eterna nokto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_y;&c_l;&c_mol;&c_l;&c_jo;</trd>,
    <trd>&c_zh;&c_y;&c_t;&c_l;&c_o;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_t;&c_e;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF42;&#xF53;&#xF66;&#xF0B;&#xF5A;&#xF44;&#xF0B;</trd>
  <trd lng="de">Wohnung</trd>
  <trdgrp lng="en">
    <trd>abode</trd>,
    <trd>dwelling</trd>,
    <trd>house</trd>,
    <trd>residence</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>appartement</trd>,
    <trd>habitation</trd>,
    <trd>logement</trd>
  </trdgrp>
  <trd lng="hu">lak&aacute;s</trd>
  <trdgrp lng="id">
    <trd><baz>diam</baz>kediaman</trd>,
    <trd>domisili</trd>,
    <trd><ind>tempat</ind> tinggal</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4F4F;&#x307F;&#x304B;  <pr>&#12377;&#x307F;&#x304B;</pr></trd>,
    <trd>&#x4F4F;&#x5C45;  <pr>&#12376;&#12421;&#12358;&#x304D;&#12423;</pr></trd>,
    <trd>&#x5BBF;&#x6CCA;&#25152;  <pr>&#12375;&#12421;&#x304F;&#x306F;&#x304F;&#12376;&#12423;</pr></trd>,
    <trd>&#x5BBF;&#x820E;  <pr>&#12375;&#12421;&#x304F;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trd lng="nl">woning</trd>
  <trdgrp lng="pl">
    <trd>mieszkanie</trd>,
    <trd>lokal</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_l;&c_mol;&c_jo;</trd>,
    <trd>&c_zh;&c_i;&c_l;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_r;&c_t;&c_i;&c_r;&c_a;</trd>
  </trdgrp>
