<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: logx.xml,v 1.35 2021/05/29 14:32:51 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>lo&gcirc;</rad>/i
</kap>
<drv mrk="logx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="logx.0i.propre">
    <dif>
      Da&ubreve;re <ref tip="super" cel="rest.0adi">restadi</ref> en loko,
      kie oni kutime vivadas:
      <ekz>
        li <tld/>as ekster la urbo
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>;
      </ekz>
      <ekz>
        la rusoj <tld/>as en Rusujo
        <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
      </ekz>
      <ekz>
        kiu <tld/>is en la domo kontra&ubreve; ni
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>?
      </ekz>
      <ekz>
        se la tero estus <tld/>ata <klr>[&#x2026;]</klr> de sa&gcirc;uloj
        <fnt><bib>Lanti</bib>,
          <lok>publika letero al Romen Rolan</lok>
        </fnt>;
      </ekz>
      <ekz>
        insuloj <klr>[&#x2026;]</klr> <tld/>ataj de sennombraj birdaj aroj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lando de via fremde<tld/>ado
        <fnt><bib>MT</bib>, <lok>&Gen; 28:4</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>loko
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 10:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en nia karno, en nia malbona naturo <tld/>as nenio bona
        <fnt>
          <bib>Fab3</bib>
          <lok>En la lasta tago</lok>
        </fnt>!
      </ekz>
    </dif>
    <trd lng="de">wohnen</trd>
    <trd lng="nl">wonen</trd>
    <trd lng="pl">mieszka&cacute;</trd>
    <trdgrp lng="pt">
      <trd>morar</trd>,
      <trd>habitar</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_i;&c_t;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>
    </trdgrp>
    <trd lng="zh">&#x5C45;&#x4F4F; [<ind>j&#x16B;zh&ugrave;</ind>]</trd>
    <trd lng="ms">meninggal</trd>
  </snc>
  <snc mrk="logx.0i.gasti">
    <dif>
      <ref tip="dif" cel="gast.0i">Gasti</ref>:
      <ekz>
        <tld/>i &ccirc;e amiko, en hotelo;
      </ekz>
      <ekz>
        <tld/>i dum kelkaj tagoj &ccirc;e familio de kamparanoj
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006831.html">La&ubreve; ritmo
          de la rizejoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>mieszka&cacute;</trd>,
      <trd>zamieszkiwa&cacute;</trd>
    </trdgrp>
    <trd lng="pt">hospedar-se</trd>
    <trd lng="zh">&#x505A;&#x5BA2; [<ind>zu&ograve;k&egrave;</ind>]</trd>
    <trd lng="ms">tetamu</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_zh;&c_i;&c_v;&c_je;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_i;&c_t;&c_a;&c_v;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="cs">bydlet</trd>
  <trdgrp lng="de">
    <trd>wohnen</trd>,
    <trd>gastieren</trd>,
    <trd>&uuml;bernachten</trd>
  </trdgrp>
  <trd lng="nl">logeren</trd>
  <trdgrp lng="en">
    <trd>dwell</trd>,
    <trd>inhabit</trd>,
    <trd>live</trd>,
    <trd>reside</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>habitar</trd>,
    <trd>vivir</trd>,
    <trd>residir</trd>,
    <trd>morar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>habiter</trd>,
    <trd>loger</trd>
  </trdgrp>
  <trd lng="he">&lamed;&gimel;&vav;&resh;</trd>
  <trdgrp lng="hu">
    <trd>lakik</trd>,
    <trd>&eacute;l <klr tip="ind">(lakik)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_zh;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="logx.0anto">
  <kap><ofc>*</ofc><tld/>anto</kap>
  <snc mrk="logx.0anto.homo">
    <dif>
      Tiu, kiu <tld/>as ie:
      <ekz>
        tiu &ccirc;i urbo havas milionon da <tld/>antoj
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>antoj de <nom>Sodom</nom> estis malbonaj kaj tre pekaj
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 13:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>antoj de la lando
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 34:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        alia <tld/>anto de la strato
        <fnt><bib>Fab1</bib>, <lok>galo&scirc;oj de feli&ccirc;o, 5, metamorfozo
        de la skribisto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_y;&c_h;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Bewohner</trd>
  <trdgrp lng="en">
    <trd>dweller</trd>,
    <trd>inhabitant</trd>,
    <trd>resident</trd>
  </trdgrp>
  <trd lng="es">habitante</trd>
  <trdgrp lng="fr">
    <trd>habitant</trd>,
    <trd>occupant<klr> (d'une habitation)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lak&oacute;</trd>,
    <trd>lakos</trd>
  </trdgrp>
  <trd lng="nl">bewoner</trd>
  <trd lng="pl">mieszkaniec</trd>
  <trdgrp lng="pt">
    <trd>morador</trd>,
    <trd>habitante</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_i;&c_t;&c_a;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="zh">&#x5C45;&#x6C11; [<ind>j&#x16B;m&iacute;n</ind>]</trd>
</drv>

<drv mrk="logx.0antaro">
  <kap><tld/>antaro</kap>
  <snc mrk="logx.0antaro.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Tuto de la homoj <tld/>antaj en loko, urbo, provinco, lando...:
      <ekz>
        la <tld/>antaro de la urbo dividi&gcirc;is
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 14:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        bruis la <tld/>antaro de la granda urbo sur la vasta trotuaro
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la tuta <tld/>antaro de la tero
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 9:19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_s;&c_je;&c_l;&c_mol;&c_n;&c_ib;&c_c;&c_t;&c_v;&c_a;</trd>
  <trd lng="de">Einwohner <klr>(Plur.)</klr></trd>
  <trd lng="en">population</trd>
  <trdgrp lng="fr">
    <trd>habitants</trd>,
    <trd>nombre d'habitants</trd>,
    <trd>population</trd>
  </trdgrp>
  <trd lng="pl">mieszka&nacute;cy</trd>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_je;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="logx.0ebla">
  <kap><tld/>ebla</kap>
  <snc mrk="logx.0ebla.tauxga">
    <dif>
      Kie oni povas <tld/>i:
      <ekz>
        ili venis en landon <tld/>eblan
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 16:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        iu vendos <tld/>eblan domon en urbo, kiun &ccirc;irka&ubreve;as muro
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 25:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/>eblaj lokoj de la tero
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 34:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la insulo restis plejparte ne<tld/>ebla pro venena gaso
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo/pg</aut>
          <vrk><url ref="https://www.monato.be/2005/008590.php">Turismi
          kun gasmasko</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_y;&c_l;&c_y;</trd>
  <trd lng="de">bewohnbar</trd>
  <trd lng="en">habitable</trd>
  <trd lng="es">habitable</trd>
  <trd lng="fr">habitable</trd>
  <trd lng="hu">lakhat&oacute;</trd>
  <trd lng="nl">bewoonbaar</trd>
  <trd lng="pl">mieszkalny</trd>
  <trd lng="pt">habit&aacute;vel</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_l;&c_o;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_g;&c_o;&c_d;&c_n;&c_y;&c_j; &c_d;&c_l;&c_ja; &c_zh;&c_i;&c_l;&c_mol;&c_ja;</trd>
  </trdgrp>
  <trd lng="zh">&#x5B9C;&#x5C45;&#x7684; [<ind>y&iacute; j&#x16B; de</ind>]</trd>
</drv>

<drv mrk="logx.0ejo">
  <kap><ofc>*</ofc><tld/>ejo</kap>
  <snc mrk="logx.0ejo.ARKI">
    <dif>
      Loko, domo a&ubreve; &ccirc;ambraro, kie oni <tld/>as:
      <ekz>
        &scirc;i prenis la plej belan ar&gcirc;entan vazon, kiu estis en la
        <tld/>ejo
        <fnt><bib>F</bib>, <lok>&FE; 19</lok></fnt>;
      </ekz>
      <ekz>
        mia <tld/>ejo estas mia re&gcirc;ejo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ne bruligu fajron en &ccirc;iuj viaj <tld/>ejoj en la tago sabata
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 35:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estu izolita, ekster la tendaro estu lia <tld/>ejo
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 13:46</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Babel</nom> <klr>[&#x2026;]</klr> fari&gcirc;is <tld/>ejo de demonoj
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 18:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta <tld/>ejo <klr>[de la muso]</klr> konsistis el malgranda
        kaverno
        sub la stoploj
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        somer<tld/>ejo
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        grandsinjora <tld/>ejo
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj
          filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ejo de la surtera eta&gcirc;o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi eklo&gcirc;is en la kel<tld/>ejo
        <fnt>
          <bib>Fab4</bib>
          <lok>Ne&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ejo de la taglaboristino
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> transloki&gcirc;as <klr>[&#x2026;]</klr> el la malnova
        <tld/>ejo en la novan
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        post du tagoj estos la fikstempo, en kiu mi devas enporti la
        luan pagon por la
        <tld/>ejo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        en la domo de mia patro ekzistas multe da <tld/>ejoj, diris
        Kristo
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        terura &scirc;losilo, kiu fermas post mi la malliberejon de la vivo
        kaj malfermas
        anta&ubreve; mi la <tld/>ejon de la eterna nokto
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_y;&c_l;&c_mol;&c_l;&c_jo;</trd>,
    <trd>&c_zh;&c_y;&c_t;&c_l;&c_o;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_t;&c_e;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF42;&#xF53;&#xF66;&#xF0B;&#xF5A;&#xF44;&#xF0B;</trd>
  <trd lng="de">Wohnung</trd>
  <trdgrp lng="en">
    <trd>abode</trd>,
    <trd>dwelling</trd>,
    <trd>house</trd>,
    <trd>residence</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>appartement</trd>,
    <trd>habitation</trd>,
    <trd>logement</trd>
  </trdgrp>
  <trd lng="hu">lak&aacute;s</trd>
  <trd lng="nl">woning</trd>
  <trdgrp lng="pl">
    <trd>mieszkanie</trd>,
    <trd>lokal</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_i;&c_l;&c_mol;&c_jo;</trd>,
    <trd>&c_zh;&c_i;&c_l;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_r;&c_t;&c_i;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x4F4F;&#x6240; [<ind>zh&ugrave;su&#x1D2;</ind>]</trd>
</drv>

<drv mrk="logx.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="logx.0igi.havigi">
    <dif>
      Havigi <tld/>ejon al iu:
      <ekz>
        <tld/>igi laboristojn en barako;
      </ekz>
      <ekz>
        en la plej bona loko de la lando <tld/>igu vian patron
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 47:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> ek<tld/>igis ilin en la urboj de Samario
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 17:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        Dio la Eternulo prenis la homon kaj en<tld/>igis lin en la
        &gcirc;ardeno Edena
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 2:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        li tuj sendos al vi <klr>[&#x2026;]</klr> tutan regimenton por
        en<tld/>igi
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la subtera eta&gcirc;o li <tld/>igis grekajn soldatojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        volonte mi <tld/>igos vin en mia palaco
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 18a, maltrankvilo en la
        re&gcirc;a palaco</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">beherbergen</trd>
  <trdgrp lng="en">
    <trd>accomodate</trd>,
    <trd>give a home to</trd>,
    <trd>put up</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>acoger</trd>,
    <trd>hospedar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>h&eacute;berger</trd>,
    <trd>loger<klr> (quelqu'un)</klr></trd>
  </trdgrp>
  <trd lng="hu">elsz&aacute;ll&aacute;sol</trd>
  <trd lng="nl">huisvesten</trd>
  <trdgrp lng="pl">
    <trd>da&cacute; <ind>mieszkanie</ind></trd>,
    <trd>zakwaterowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>alojar</trd>
    <trd>hospedar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="logx.amas0ejo">
  <kap>amas<tld/>ejo</kap>
  <snc mrk="logx.amas0ejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Loko aran&gcirc;ita por densa <tld/>ado
      de multaj homoj, kie privateco mankas:
      <ekz>
        <uzo tip="fak">ESP</uzo>
        kutime en UK-j amas<tld/>ejo
        estas ekzemple gimnastejo de lernejo
        <fnt>
          <aut>Nori</aut>,
          <vrk><url ref="http://groups.yahoo.com/group/E-istajVegetaranoj/message/610">
          Amaslo&gcirc;ejo dum la UK</url></vrk>,
          <lok>Esperantistaj Vegetaranoj, 2007-01-14</lok>
        </fnt>;
      </ekz>
      <ekz>
        longaj labortempoj, malsanigaj amas<tld/>ejoj, infanlaboro, 
        mallonga vivda&ubreve;ro, mankanta laborprotekto estis kelkaj el la 
        mankoj
        <fnt>
          <aut>T. Bergmann</aut>,
          <vrk><url ref="http://mas-eo.org/spip.php?article309">
          La socialisma disvolvo-strategio de la PR &Ccirc;inio</url></vrk>,
          <lok>Monda Asembleo Socia, 2008-08-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Massenquartier</trd>
  <trdgrp lng="en">
    <trd>dormitory (dorm)</trd>,
    <trd>barracks</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>grand ensemble</trd>,
    <trd>logement collectif</trd>
  </trdgrp>
  <trd lng="pt">alojamento coletivo</trd>
</drv>

<drv mrk="logx.ek0i">
  <kap>ek<tld/>i</kap>
  <snc mrk="logx.ek0i.arangxi">
    <dif>
      Aran&gcirc;i sian restadon en nova loko:
      <ekz>
        ne iru en Egiptujon, por tie ek<tld/>i
        <fnt><bib>MT</bib>, <lok>&Jer; 43:2</lok></fnt>;
      </ekz>
      <ekz>
        ili trovis valon en la lando <nom>&Scirc;inar</nom> kaj tie ek<tld/>is
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 11:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> ek<tld/>is sur la monto
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 19:30</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i ek<tld/>is en la alta mansardo
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni povis ek<tld/>i en &ccirc;i tiu dometo
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">beziehen <klr>(Wohnung)</klr></trd>
  <trdgrp lng="en">
    <trd>settle in</trd>,
    <trd>take up residence</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>installer</ind><klr> (pour habiter)</klr></trd>
  <trdgrp lng="pl">
    <trd>zamieszka&cacute;</trd>,
    <trd>urz&aogonek;dzi&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="logx.en0igxi">
  <kap>en<tld/>i&gcirc;i</kap>
  <snc mrk="logx.en0igxi.preni">
    <dif>
      Preni, ekokupi <tld/>ejon:
      <ekz>
        ili venis &gcirc;is <nom>&Hcirc;aran</nom> kaj en<tld/>i&gcirc;is tie
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 11:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <klr>[&#x2026;]</klr> en<tld/>i&gcirc;is en <nom>Jerusalem</nom> kun siaj
        fratoj
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 8:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        li en<tld/>i&gcirc;is en la hotelo
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; ol vi en<tld/>i&gcirc;as en mian &ccirc;ambron, mi devas al vi
        rimarki, <klr>[&#x2026;]</klr>
        ke oni akurate pagu la luan pagon
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">einziehen</trd>
  <trdgrp lng="en">
    <trd>settle in</trd>,
    <trd>take up residence</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>emm&eacute;nager</trd>,
    <trd>s'<ind>installer</ind><klr> (emm&eacute;nager)</klr></trd>
  </trdgrp>
  <trd lng="hu">bek&ouml;lt&ouml;zik</trd>
  <trd lng="nl">zijn <ind>intrek</ind> nemen</trd>
  <trdgrp lng="pl">
    <trd>zamieszka&cacute;</trd>,
    <trd>wprowadzi&cacute; si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_v;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="logx.kun0anto">
  <kap>kun<tld/>anto</kap>
  <snc mrk="logx.kun0anto.KOMUNE">
    <dif>
      <tld lit="L"/>anto de sama <tld/>ejo:
      <ekz>
        la produkta&jcirc;oj <klr>[&#x2026;]</klr> estu man&gcirc;a&jcirc;o por vi &ccirc;iuj: por vi,
        por via sklavo,
        <klr>[&#x2026;]</klr> kaj por via kun<tld/>anto, kiuj lo&gcirc;as &ccirc;e vi
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 25:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        li babilis kvaza&ubreve; ni ja estas ankora&ubreve; kun<tld/>antaj kiel
        geedzoj
        <fnt>
          <bib>Monato</bib>
          <aut>Betty Chatterjee</aut>
          <vrk><url ref="https://www.monato.be/2007/009304.php">Karulino
          kanibalino</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie &ccirc;i lo&gcirc;as homoj de diversaj naciecoj, kutimoj, religioj,
        rekonante la rajton
        al alieco de &ccirc;iu kun<tld/>anto
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007095.html">Pollando:
          Landlimoj sen limoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dom.0ano.ajna">domano</ref>
    </refgrp>
  </snc>
  <trd lng="bo">&#xF64;&#xF42;&#xF0B;&#xF62;&#xF7C;&#xF42;&#xF66;&#xF0B;</trd>
  <trd lng="de">Mitbewohner</trd>
  <trd lng="en">roommate</trd>
  <trdgrp lng="fr">
    <trd>commensal</trd>,
    <trd>colocataire</trd>
  </trdgrp>
  <trd lng="pl">wsp&oacute;&lstroke;lokator</trd>
</drv>

<drv mrk="logx.pas0anto">
  <kap>pas<tld/>anto</kap>
  <snc mrk="logx.pas0anto.portempa">
    <dif>
      Portempa restanto en loko, en lando:
      <ekz>
        fremdulo kaj pas<tld/>anto mi estas &ccirc;e vi
        <fnt><bib>MT</bib>, <lok>&Gen; 23:4</lok></fnt>;
      </ekz>
      <ekz>
        li fari&gcirc;is pas<tld/>anto en la lando de la promeso
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 11:9</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gast.0o">gasto</ref>,
      <ref cel="migr.0i">migri</ref>,
      <ref cel="nomad.0o">nomado</ref>
    </refgrp>
  </snc>
  <trd lng="de">Gast</trd>
  <trdgrp lng="en">
    <trd>sojourner</trd>,
    <trd>temporary dweller</trd>,
    <trd>temporary resident</trd>
  </trdgrp>
  <trd lng="fr">h&ocirc;te de passage</trd>
  <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j; &c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="logx.pra0anto">
  <kap>pra<tld/>anto</kap>
  <snc mrk="logx.pra0anto.iu">
    <dif>
      Iu el la plej antikve konataj <tld/>antoj de lando:
      <ekz>
        intermiksi&gcirc;as tradicioj de pra<tld/>antoj kaj hispanoj
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="https://www.monato.be/2005/008399.php">Unika
          fervojo en Andoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la pra<tld/>antoj de la amerika kontinento a
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="https://www.monato.be/2015/011355.php">Komunika&jcirc;o de
          Ikaro</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ajnas, ke parto de la pra<tld/>antoj de Finnlando rigardis sin
        kiel posteulojn
        de la alko
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2009/009756.php">Praa
          religia temo</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <nom>Akkadai</nom> (a&ubreve; montanoj), la pra<tld/>antoj en la
        orienta,
        monta parto de la Babela regno
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 3a, Lemuel da&ubreve;rigas sian
        esploron</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="indigx.0o">indi&gcirc;eno</ref>
    </refgrp>
  </snc>
  <trd lng="de">Ureinwohner</trd>
  <trd lng="en">aboriginal</trd>
  <trd lng="fr">indig&egrave;ne<klr> (subst.)</klr></trd>
  <trd lng="ru">&c_d;&c_r;&c_je;&c_v;&c_n;&c_je;&c_j;&c_sh;&c_i;&c_j; &c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
</drv>

<drv mrk="logx.pri0i">
  <kap>pri<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="logx.pri0i.logxi_en">
    <dif>
      Esti <tld/>anto, resti en:
      <ekz>
        kiam <nom>Jerusalem</nom> estis prilo&gcirc;ata kaj bonstata
        <fnt><bib>MT</bib>, <lok>&Zeh; 7:7</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="okup.0i">okupi</ref>
    </refgrp>
  </snc>
  <trd lng="de">bewohnen</trd>
  <trd lng="en">inhabit</trd>
  <trd lng="fr">occuper<klr> (un lieu, y r&eacute;sider)</klr></trd>
  <trdgrp lng="pl">
    <trd>zamieszkiwa&cacute;</trd>,
    <trd>zajmowa&cacute;</trd>,
    <trd>zasiedla&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;</trd>
</drv>

<drv mrk="logx.trans0igxi">
  <kap>trans<tld/>i&gcirc;i</kap>
  <snc mrk="logx.trans0igxi.sxangxi">
    <dif>
      &Scirc;an&gcirc;i sian <tld/>ejon, kaj okupi alian:
      <ekz>
        li trans<tld/>i&gcirc;is al la monto
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 12:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        el la belegaj &ccirc;ambroj, en kiuj li anta&ubreve;e lo&gcirc;is, li devis
        trans<tld/>i&gcirc;i
        <klr>[&#x2026;]</klr> en malgrandan frontonan &ccirc;ambreton tute sub la
        tegmento
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en &ccirc;iu trans<tld/>i&gcirc;o estas parto de ruini&gcirc;o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        komisario de trans<tld/>i&gcirc;oj
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        poste ni trans<tld/>i&gcirc;is Moskvon
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_s;&c_ja;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>
  <trd lng="de">umziehen</trd>
  <trdgrp lng="en">
    <trd>change one's dwelling</trd>,
    <trd>move</trd>,
    <trd>relocate</trd>
  </trdgrp>
  <trd lng="es">mudarse</trd>
  <trd lng="fr">d&eacute;m&eacute;nager</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;lt&ouml;zik</trd>,
    <trd>&aacute;tk&ouml;lt&ouml;zik</trd>
  </trdgrp>
  <trd lng="nl">verhuizen</trd>
  <trd lng="pl">przeprowadza&cacute; si&eogonek;</trd>
  <trd lng="pt">mudar-se<klr> (de endere&ccedil;o)</klr></trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_s;&c_je;&c_l;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
