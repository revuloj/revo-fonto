<vortaro>
<art mrk="$Id: ametis.xml,v 1.21 2023/08/30 18:23:33 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>ametist</rad>/o
</kap>
<drv mrk="ametis.0o">
  <kap><tld/>o</kap>
  <snc mrk="ametis.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Violkolora <ref tip="super" cel="sxton.juvel0o.MIN">juvel&scirc;tono</ref>:
      <ekz>
        &ccirc;iaspecaj multekostaj &scirc;tonoj vin kovris: rubeno, topazo, diamanto, sukceno,
        <klr>[&#x2026;]</klr>, smeraldo, <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 28:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        en Brazilo oni trovas grandegajn druzojn plenaj <klr>[de]</klr> <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=157331">Druzo (geologio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_m;&c_e;&c_t;&c_y;&c_s;&c_t;</trd>
  <trd lng="bg">&c_a;&c_m;&c_je;&c_t;&c_i;&c_s;&c_t;</trd>
  <trd lng="ca">ametista</trd>
  <trd lng="cs">ametyst</trd>
  <trd lng="da">ametyst</trd>
  <trd lng="de">Amethyst</trd>
  <trd lng="en">amethyst</trd>
  <trd lng="es">amatista</trd>
  <trd lng="fr">am&eacute;thyste</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5D7;&#x5DC;&#x5B8;&#x5DE;&#x5B8;&#x5D4;</trd>
  <trd lng="it">ametista</trd>
  <trd lng="ja">&#x7D2B;&#x6C34;&#x6676;  [<ind>&#x3080;&#x3089;&#x3055;&#x304D;&#x305A;&#x3044;&#x3057;&#x3087;&#x3046;</ind>]</trd>
  <trd lng="nl">amethist</trd>
  <trd lng="pl">ametyst</trd>
  <trd lng="ro">ametist</trd>
  <trd lng="ru">&c_a;&c_m;&c_je;&c_t;&c_i;&c_s;&c_t;</trd>
  <trd lng="sk">ametyst</trd>
  <trd lng="sv">ametist</trd>
  <trd lng="uk">&#x430;&#x43C;&#x435;&#x442;&#x438;&#x441;&#x442;</trd>
</drv>
