<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>norveg</rad>/o
</kap>

<drv mrk="norveg.0ujo">
  <kap>
    <tld lit="N"/>ujo
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="norveg.0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la norda E&ubreve;ropo sur la Skandinava duoninsulo kun
      &ccirc;efurbo 
      Oslo.
      Teritorio de Regno Norvegujo.
      ISO-3166 kodo: NO.
      &Gcirc;i limas kun
      <ref tip="vid" cel="finn.F0lando">Finnlando</ref>,
      <ref tip="vid" cel="rus.R0ujo">Rusujo</ref>,
      <ref tip="vid" cel="sved.S0ujo">Svedujo</ref>
      kaj <ref tip="vid" cel="atlant.0o.GEOG">Atlantiko</ref>.
    </dif>
    <ref tip="sin" cel="norvegi.N0o">Norvegio</ref>
    <trd lng="en">Norway</trd>
    <trdgrp lng="pl">
      <trd>Norwegia</trd>,
      <trd>Kr&oacute;lestwo <ind>Norwegii</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

