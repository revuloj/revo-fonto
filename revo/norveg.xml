<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>norveg</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="norveg.0o">
  <kap><tld/>o<fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/aktoj/aktoj2/8oa.html"> 8a
Oficiala Aldono</url></vrk>, <lok>II.C.1</lok></fnt>,
  <var><kap><tld/>ino</kap></var></kap>
  <snc mrk="norveg.0o.sxtatano">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref>, &scirc;tatinano de
      <ref tip="malprt" cel="norveg.N0ujo.GEOG"><tld lit="N"/>ujo</ref>.
    </dif>
  </snc>
  <snc mrk="norveg.0o.naciano">
    <dif>
      Homo kun <tld/>a nacieco.
    </dif>
  </snc>
  <trd lng="cs">Nor</trd>
  <trd lng="en">Norwegian<klr> (subst.)</klr></trd>
  <trdgrp lng="fr"> 
    <trd>Norv&eacute;gien</trd>
    <trd>Norv&eacute;gienne<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">Noor</trd>
  <trd lng="no">nordmann</trd>
  <trd lng="sk">N&oacute;r</trd>
</drv>

<drv mrk="norveg.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilatanta al <tld/>oj, ilia nacieco a&ubreve; &scirc;tato.
    </dif>
   <trd lng="en">Norwegian<klr> (adj.)</klr></trd>
   <trdgrp lng="fr"> 
    <trd>norv&eacute;gien</trd>
    <trd>norv&eacute;gienne<klr> (adj.)</klr></trd>
   </trdgrp>
  </snc>
  <snc>
    <dif>
    "la norvega" = la nacia <ref tip="super" cel="lingv.0o">lingvo</ref>
    de  Norvegujo
    <ekz><fnt><bib>Viki</bib>, <lok><url 
    ref="http://eo.wikipedia.org/wiki/Norvega lingvo">Norvega
    lingvo</url></lok></fnt></ekz>.
    </dif>
   <trd lng="en">Norwegian<klr> (language)</klr></trd>
   <trd lng="fr">norv&eacute;gien<klr> (langue)</klr></trd>
  </snc>  
  <trd lng="cs">norsk&yacute;</trd>
  <trd lng="nl">Noors</trd>
  <trd lng="no">norsk</trd>
  <trd lng="sk">n&oacute;rsky</trd>
</drv>


<drv mrk="norveg.N0ujo">
  <kap>
    <tld lit="N"/>ujo
   (<var><kap><tld lit="N"/>/i/o</kap></var>)
    <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url
ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>
    <fnt><bib>Viki</bib>, <lok><url 
    ref="http://eo.wikipedia.org/wiki/Norvegio">Norvegio</url></lok></fnt>
 </kap>
  <snc mrk="norveg.N0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o.POL">&Scirc;tato</ref> en la norda 
      E&ubreve;ropo sur la Skandinava duoninsulo kun &ccirc;efurbo Oslo;
      teritorio de Regno Norvegujo;
      &gcirc;i limas kun
      <ref tip="vid" cel="finn.F0lando">Finnlando</ref>,
      <ref tip="vid" cel="rus.R0ujo">Rusujo</ref>,
      <ref tip="vid" cel="sved.S0ujo">Svedujo</ref>
      kaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantiko</ref>
      (ISO-3166 kodo: NO).
    </dif>
    <ref tip="sin" cel="norvegi.N0o">Norvegio</ref>
  </snc>
   <rim mrk="norveg.N0io.RIM">
  "Gentobazaj landnomoj. Oni ordinare formas la nomon de la lando per la
  <em>Fundamenta sufikso UJ</em> a&ubreve; per la <em>neoficiala sufikso
  I</em>. Pri la karaktero de tiu I sufikso (uzata post popolvortoj
  anstata&ubreve; UJ) ekzistas diversaj opinioj. Iuj rigardas &gcirc;in kiel
  <em>efektivan sufikson</em> (kvankam neoficialan), dum aliaj preferas
  rigardi &gcirc;in kiel <em>eron de aparta landnoma radiko</em> paralela kun
  la responda popolnoma radiko. 
  Tiuj diversaj analizoj &ccirc;iuokaze ne faras praktikan diferencon en la
  uzado de tiaj landnomoj kiel Francio, Rusio, Japanio
  kaj &Ccirc;inio. <em>En tiu &ccirc;i dokumento la Akademio ne alprenas starpunkton
  pri tiu teoria demando</em>" <fnt><aut>Akademio de Esperanto</aut>, 
  <vrk><url
  ref="http://www.akademio-de-esperanto.org/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficiala
  Informo 12</url>, Listo de Rekomendataj Landnomoj</vrk></fnt>.
  </rim>
    <trd lng="en">Norway</trd>
    <trd lng="fa">&a_n;&a_r;&a_w;&f_zh;</trd>
    <trd lng="fr">Norv&egrave;ge</trd>
    <trd lng="id">Norwegia</trd>
    <trd lng="nl">Noorwegen</trd>
    <trd lng="no">Norge</trd>
    <trdgrp lng="pl">
      <trd>Norwegia</trd>,
      <trd>Kr&oacute;lestwo <ind>Norwegii</ind></trd>
    </trdgrp>
</drv>

</art>

