<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alianc.xml,v 1.30 2023/10/28 08:33:04 revo Exp $">
<kap>
  <rad>alianc</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="alianc.0o">
  <kap><tld/>o</kap>
  <snc mrk="alianc.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Regna a&ubreve; alia politika
      <ref tip="super" cel="lig.inter0o.POL">interligo</ref>:
      <ekz>
        vi povus fari <tld/>on kun ili
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni decidadis pri <klr>[&#x2026;]</klr> <tld/>oj, pri diversaj inter&scirc;tataj kontraktoj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> kanonas de la maro per la <tld/>a &scirc;iparo
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>3. La e&ubreve;rope edukita homo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="A"/>o (politika partiaro) subtenas la
        manifeston
        <fnt>
          <aut>D. E. Rogers</aut>,
          <vrk>Ekonomia miraklo a&ubreve; malsano?</vrk>,
          <bib>Monato</bib>,
          <lok>1997:2, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x534F;&#x7EA6; <pr>xi&#xE9;yu&#x113;</pr></trd>,
    <trd>&#x5354;&#x7D04; <pr>xi&#xE9;yu&#x113;</pr></trd>,
    <trd>&#x540C;&#x76DF; <pr>t&#xF3;ngm&#xE9;ng</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_s;</trd>,
    <trd>&c_s;&c_a;&c_ju;&c_z;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_z;</trd>,
    <trd>&c_h;&c_a;&c_w;&c_r;&c_u;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Allianz</trd>,
    <trd>B&uuml;ndnis</trd>
  </trdgrp>
  <trd lng="en">alliance</trd>
  <trd lng="es">alianza</trd>
  <trdgrp lng="fr">
    <trd>alliance<klr tip="amb"> (pol.)</klr></trd>,
    <trd>coalition</trd>
  </trdgrp>
  <trd lng="he">&bet;&#x5BC;&#x5B0;&resh;&#x5B4;&yod;&tav;</trd>
  <trd lng="hu">sz&ouml;vets&eacute;g</trd>
  <trd lng="ja">&#x540C;&#x76DF;  <pr>&#x3069;&#x3046;&#x3081;&#x3044;</pr></trd>
  <trdgrp lng="nl">
    <trd>alliantie</trd>,
    <trd>verbintenis</trd>,
    <trd>verbond</trd>
  </trdgrp>
  <trd lng="pt">alian&ccedil;a</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_s;</trd>,
    <trd>&c_s;&c_o;&c_ju;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>aliancia</trd>,
    <trd>dru&zcaron;ba</trd>,
    <trd>spojenectvo</trd>
  </trdgrp>
  <trd lng="sv">allians</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_s;</trd>,
    <trd>(&c_p;&c_o;&c_l;&c_ib;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;) &c_s;&c_o;&c_ju;&c_z;</trd>
  </trdgrp>
</drv>

<drv mrk="alianc.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      Trovi&gcirc;i en <tld/>o kun iu:
      <ekz>
        li <tld/>as kun tiu fripono
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>intoj malpermesis la ali&gcirc;on al Germanio
        <fnt>
       <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2000/006885.php">Kiu estas J&ouml;rg Haider?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_mol;&c_s;&c_je;</ind></trd>,
    <trd>&c_h;&c_a;&c_w;&c_r;&c_u;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de"><ind>verb&uuml;ndet</ind> sein</trd>
</drv>


<drv mrk="alianc.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="alianc.0ano.POL">
    <dif>
      Regno, partio, kontraktinta en <tld/>o:
      <ekz>
        oni transdonos en viajn manojn tiun ligan trezoron <klr>[&#x2026;]</klr> kiel <tld/>ano
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;APITRO VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la venko de la <tld/>anoj restarigis en granda nombro da landoj demokratajn
        re&gcirc;imojn
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Japanio senkondi&ccirc;e kapitulacis je la <tld/>anoj la 15an de
        a&ubreve;gusto 1945
        <fnt>
          <aut>Igarasi Takeo</aut>,
          <vrk>Hontinda vizito</vrk>,
          <bib>Monato</bib>,
          <lok>2001:12, p. 4a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x540C;&#x76DF;&#x8005; <pr>t&#xF3;ngm&#xE9;ngzh&#x11B;</pr></trd>
</snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_ju;&c_z;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_h;&c_a;&c_w;&c_r;&c_u;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">spojenec</trd>
  <trdgrp lng="de">
    <trd>Verb&uuml;ndeter</trd>,
    <trd>B&uuml;ndnispartner</trd>
  </trdgrp>
  <trd lng="en">ally</trd>
  <trdgrp lng="fr">
    <trd>alli&eacute;<klr> (membre d'une alliance)</klr></trd>,
    <trd>partenaire<klr> (d'une coalition)</klr></trd>
  </trdgrp>
  <trd lng="he">&bet;&resh;&yod;&tav;</trd>
  <trdgrp lng="ja">
    <trd>&#x540C;&#x76DF;&#x56FD;  <pr>&#x3069;&#x3046;&#x3081;&#x3044;&#x3053;&#x304F;</pr></trd>,
    <trd>&#x540C;&#x76DF;&#x8005;  <pr>&#x3069;&#x3046;&#x3081;&#x3044;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_ju;&c_z;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">spojenec</trd>
  <trd lng="uk">&c_s;&c_o;&c_ju;&c_z;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="alianc.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="alianc.0igxi.POL">
    <dif>
      Ali&gcirc;i, <ref tip="super" cel="kun.0igxi">kuni&gcirc;i</ref> en politika interligo:
      <ekz>
        vi timis, ke ili <tld/>i&gcirc;os kun la proksimi&gcirc;antaj naziaj trupoj
        <fnt>
          <bib>CKv</bib>
          <lok>6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la versoj de <nom>De Kock</nom> estas konstruitaj en densa vortsinsekvo, kie la formo
        <tld/>i&gcirc;as kun la enhavo
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        eble la lingvoj povus e&ccirc; <tld/>i&gcirc;i
        <fnt>
          <aut>E. Grimley Evans</aut>,
          <vrk>La kornvala</vrk>,
          <bib>Monato</bib>,
          <lok>1998:1, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_v;&c_a;&c_r;&c_y;&c_c;&c_mol; <ind>&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_s;</ind></trd>
  <trd lng="de">sich <ind>verb&uuml;nden</ind></trd>
  <trdgrp lng="en">
    <trd>ally</trd>,
    <trd>side with</trd>,
    <trd>form an alliance</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire <ind>alliance</ind></trd>,
    <trd>s'<ind>allier</ind></trd>
  </trdgrp>
  <trd lng="he">&lamed;&kaf;&resh;&vav;&tav; &bet;&resh;&yod;&tav;</trd>
  <trd lng="ja">&#x540C;&#x76DF;&#x3059;&#x308B;  <pr>&#x3069;&#x3046;&#x3081;&#x3044;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol; <ind>&c_s;&c_o;&c_ju;&c_z;</ind></trd>,
    <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol; &c_v; <ind>&c_s;&c_o;&c_ju;&c_z;</ind></trd>
  </trdgrp>
  <trd lng="uk">&c_u;&c_k;&c_l;&c_a;&c_s;&c_t;&c_i; &c_s;&c_o;&c_ju;&c_z;</trd>
</drv>
</art>
