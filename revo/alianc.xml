<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: alianc.xml,v 1.23 2021/02/28 15:34:26 revo Exp $">
<kap>
  <rad>alianc</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="alianc.0o">
  <kap><tld/>o</kap>
  <snc mrk="alianc.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Regna a&ubreve; alia politika
      <ref tip="super" cel="lig.inter0o.POL">interligo</ref>:
      <ekz>
        la <tld lit="A"/>o (politika partiaro) subtenas la
        manifeston
        <fnt>
        <aut>D. E. Rogers</aut>,
        <vrk>Ekonomia miraklo a&ubreve; malsano?</vrk>,
        <bib>Monato</bib>,
        <lok>1997:2, p. 14a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_s;</trd>,
    <trd>&c_s;&c_a;&c_ju;&c_z;</trd>,
    <trd>&c_z;&c_mol;&c_v;&c_ja;&c_z;</trd>,
    <trd>&c_h;&c_a;&c_w;&c_r;&c_u;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Allianz</trd>,
    <trd>B&uuml;ndnis</trd>
  </trdgrp>
  <trd lng="en">alliance</trd>
  <trd lng="es">alianza</trd>
  <trdgrp lng="fr">
    <trd>alliance<klr tip="amb"> (pol.)</klr></trd>,
    <trd>coalition</trd>
  </trdgrp>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B0;&#x5E8;&#x5B4;&#x5D9;&#x5EA;</trd>
  <trd lng="hu">sz&ouml;vets&eacute;g</trd>
  <trdgrp lng="nl">
    <trd>alliantie</trd>,
    <trd>verbintenis</trd>,
    <trd>verbond</trd>
  </trdgrp>
  <trd lng="pt">alian&ccedil;a</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_s;</trd>,
    <trd>&c_s;&c_o;&c_ju;&c_z;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>aliancia</trd>,
    <trd>dru&zcaron;ba</trd>,
    <trd>spojenectvo</trd>
  </trdgrp>
  <trd lng="sv">allians</trd>
</drv>

<drv mrk="alianc.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="alianc.0ano.POL">
    <dif>
      Regno, partio, kontraktinta en <tld/>o:
      <ekz>
        Japanio senkondi&ccirc;e kapitulacis je la <tld/>anoj la 15an de
        a&ubreve;gusto
        1945
        <fnt>
        <aut>Igarasi Takeo</aut>,
        <vrk>Hontinda vizito</vrk>,
        <bib>Monato</bib>,
        <lok>2001:12, p. 4a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_a;&c_ju;&c_z;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_h;&c_a;&c_w;&c_r;&c_u;&c_s;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">spojenec</trd>
  <trd lng="en">ally</trd>
  <trdgrp lng="fr">
    <trd>alli&eacute;<klr> (membre d'une alliance)</klr></trd>,
    <trd>partenaire<klr> (d'une coalition)</klr></trd>
  </trdgrp>
  <trd lng="he">&#x5D1;&#x5E8;&#x5D9;&#x5EA;</trd>
  <trd lng="ru">&c_s;&c_o;&c_ju;&c_z;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">spojenec</trd>
</drv>

<drv mrk="alianc.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="alianc.0igxi.POL">
    <dif>
      Ali&gcirc;i, kuni&gcirc;i en politika interligo:
      <ekz>
        eble la lingvoj povus e&ccirc; <tld/>i&gcirc;i
        <fnt>
        <aut>E. Grimley Evans</aut>,
        <vrk>La kornvala</vrk>,
        <bib>Monato</bib>,
        <lok>1998:1, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_v;&c_a;&c_r;&c_y;&c_c;&c_mol; <ind>&c_a;&c_l;&c_mol;&c_ja;&c_n;&c_s;</ind></trd>
  <trdgrp lng="en">
    <trd>ally</trd>,
    <trd>side with</trd>,
    <trd>form an alliance</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire <ind>alliance</ind></trd>,
    <trd>s'<ind>allier</ind></trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5DB;&#x5E8;&#x5D5;&#x5EA; &#x5D1;&#x5E8;&#x5D9;&#x5EA;</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_k;&c_l;&c_ju;&c_ch;&c_i;&c_t;&c_mol; <ind>&c_s;&c_o;&c_ju;&c_z;</ind></trd>,
    <trd>&c_v;&c_s;&c_t;&c_u;&c_p;&c_i;&c_t;&c_mol; &c_v; <ind>&c_s;&c_o;&c_ju;&c_z;</ind></trd>
  </trdgrp>
</drv>
</art>
