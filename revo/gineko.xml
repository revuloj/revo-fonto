    <trd>ginecolog&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_i;&c_n;&c_je;&c_k;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">&zcaron;ensk&yacute; lek&aacute;r</trd>
  <trd lng="sv">gynekolog</trd>
  <trd lng="tr">jinekolog</trd>
  <trd lng="uk">&#x433;&#x456;&#x43D;&#x435;&#x43A;&#x43E;&#x43B;&#x43E;&#x433;</trd>
  <trd lng="vo">vomasanan</trd>
</drv>
