<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fuksin.xml,v 1.14 2024/05/12 09:22:51 revo Exp $">
<kap>
  <rad>fuksin</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="fuksin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Anilina <ref tip="super" cel="rugx.0o">ru&gcirc;</ref>a&jcirc;o:
      <ekz>
        la germana kemiisto, Henriko Karo, <klr>[&#8230;]</klr> anka&ubreve; disvolvis plurajn aliajn
        sintezojn tiel kiel indulino, eozino, <tld/>o, a&ubreve;rino
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=537867">Perokso-unusulfata
          acido</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        hiponimoj <klr>[de]</klr> ru&gcirc;a estas <klr>[&#8230;]</klr> karmezina, skarlata, punca,
        karmina, <tld/>a, eble anka&ubreve; al fuksina, rufa
        <fnt>
          <bib>LAE</bib>
          <lok>5. Leksiko Kaj Semantiko</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_k;&c_s;&c_ib;&c_n;</trd>
  <trd lng="de">Fuchsin</trd>
  <trd lng="fr">fuchsine</trd>
  <trd lng="ja">&#x30D5;&#x30AF;&#x30B7;&#x30F3;</trd>
  <trdgrp lng="pl">
    <trd>fuksyna</trd>,
    <trd>rozanilina</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fucsina</trd>,
    <trd>magenta</trd>,
    <trd>rosanilina</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_u;&c_k;&c_s;&c_i;&c_n;</trd>
  <trd lng="uk">&c_f;&c_u;&#x43A;&c_s;&c_i;&#x43D;</trd>
</drv>
</art>
