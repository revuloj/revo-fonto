<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fuksin.xml,v 1.12 2022/03/20 13:07:55 revo Exp $">
<kap>
  <rad>fuksin</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="fuksin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Anilina <ref tip="super" cel="rugx.0o">ru&gcirc;</ref>a&jcirc;o:
      <ekz>
        la germana kemiisto, Henriko Karo, <klr>[&#x2026;]</klr> anka&ubreve; disvolvis plurajn aliajn
        sintezojn tiel kiel indulino, eozino, <tld/>o, a&ubreve;rino, <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=537867">Perokso-unusulfata acido</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_u;&c_k;&c_s;&c_ib;&c_n;</trd>
  <trd lng="de">Fuchsin</trd>
  <trd lng="fr">fuchsine</trd>
  <trdgrp lng="pl">
    <trd>fuksyna</trd>,
    <trd>rozanilina</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fucsina</trd>,
    <trd>magenta</trd>,
    <trd>rosanilina</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_u;&c_k;&c_s;&c_i;&c_n;</trd>
  <trd lng="uk">&#x444;&#x443;&#x43A;&#x441;&#x438;&#x43D;</trd>
</drv>
</art>
