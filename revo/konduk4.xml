<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konduktiv</rad>/a
</kap>
<drv mrk="konduk4.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Kapabla <ref cel="konduk.0i.FIZ_ELE">konduki</ref> elektron,
      varmon k.s.
    </dif>
  </snc>
  <trd lng="en">conductive</trd>
  <trd lng="cs">vodiv&yacute;</trd>
  <trd lng="hu">vezet&odblac;k&eacute;pes</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;</trd>
  <trd lng="sk">vodiv&yacute;</trd>
</drv>

<drv mrk="konduk4.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="konduk4.0o.eco">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Eco de <tld/>a materialo.
    </dif>
  </snc>
  <snc mrk="konduk4.0o.grando">
    <uzo tip="fak">ELE</uzo>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>
      inversa al la <ref cel="rezist2.0o">rezistivo</ref>.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>konduktivita</trd>,
    <trd>vodivost</trd>
  </trdgrp>
  <trd lng="en">conductivity</trd>
  <trd lng="hu">vezet&odblac;k&eacute;pess&eacute;g</trd>
  <trd lng="ru">&c_u;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">vodivos&tcaron;</trd>
</drv>

<drv mrk="konduk4.super0eco">
  <kap>super<tld/>eco</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="konduk.super0eco">superkondukeco</ref>
  </snc>
  <trd lng="en">superconductivity</trd>
  <trd lng="hu">szupravezet&odblac;-k&eacute;pess&eacute;g</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_h;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>

