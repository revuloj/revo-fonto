<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>konduktiv</rad>/a
</kap>
<drv mrk="konduk4.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Kapabla <ref cel="konduk.0i.FIZ">konduki</ref> elektron,
      varmon k.s.
    </dif>
  </snc>
</drv>

<drv mrk="konduk4.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="konduk4.0o.eco">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Eco de <tld/>a materialo.
    </dif>
  </snc>
  <snc mrk="konduk4.0o.grando">
    <uzo tip="fak">ELE</uzo>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>
      inversa al la <ref cel="rezist2.0o">rezistivo</ref>.
    </dif>
  </snc>
</drv>

<drv mrk="konduk4.super0eco">
  <kap>super<tld/>eco</kap>
  <uzo tip="fak">FIZ</uzo>
  <uzo tip="fak">ELE</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="konduk.super0eco">superkondukeco</ref>
  </snc>
</drv>
</art>

<!--
$Log$
-->
</vortaro>