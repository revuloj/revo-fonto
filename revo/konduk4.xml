<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konduk4.xml,v 1.15 2023/12/03 09:58:55 revo Exp $">
<kap>
  <rad>konduktiv</rad>/a
</kap>
<drv mrk="konduk4.0a">
  <kap><tld/>a
  <fnt><bib>PIV1</bib></fnt>
  </kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Kapabla <ref cel="konduk.0i.FIZ_ELE">konduki</ref> elektron,
      varmon k.s.:
      <ekz>
        aluminia drato de certa <tld/>eco devas esti pli dika ol 
        same <tld/>a kupra drato, sed kompense estas malpli peza
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=384">Aluminio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;&c_n;&c_y;</trd>
  <trd lng="de">leitf&auml;hig</trd>
  <trd lng="en">conductive</trd>
  <trd lng="cs">vodiv&yacute;</trd>
  <trd lng="hu">vezet&odblac;k&eacute;pes</trd>
  <trd lng="ja">&#x4F1D;&#x5C0E;&#x6027;&#x306E;  <pr>&#x3067;&#x3093;&#x3069;&#x3046;&#x305B;&#x3044;&#x306E;</pr></trd>
  <trd lng="pl">przewodz&aogonek;cy</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_ja;&c_shch;&c_i;&c_j;</trd>
  <trd lng="sk">vodiv&yacute;</trd>
</drv>

<drv mrk="konduk4.0o">
  <kap><tld/>o
  <fnt><bib>PIV1</bib></fnt>
  </kap>
  <snc mrk="konduk4.0o.eco">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Eco de <tld/>a materialo:
      <ekz>
        la varmo<tld/>o de materialo dependas de la temperaturo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=527711">Termika konduktivo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
     <trd lng="de">Leitf&auml;higkeit</trd>
    <trd lng="en">conductivity</trd>
     <trd lng="pl">przewodnictwo</trd>
    <trdgrp lng="zh">
    <trd>&#x5BFC;&#x7535;&#x6027; <pr>d&#x1CE;odi&#xE0;nx&#xEC;ng</pr></trd>,
    <trd>&#x70ED;&#x5BFC;&#x7387; <pr>r&#xE8;d&#x1CE;ol&#xF9;:</pr></trd>,
    <trd>&#x4F20;&#x5BFC;&#x5EA6; <pr>zhu&#xE0;nd&#x1CE;od&#xF9;</pr></trd>,
    <trd>&#x4F20;&#x5BFC;&#x7387; <pr>zhu&#xE0;nd&#x1CE;oshu&#xE0;i</pr></trd>,
    <trd>&#x5BFC;&#x7535;&#x80FD;&#x529B; <pr>d&#x1CE;odi&#xE0;nn&#xE9;ngl&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="konduk4.0o.grando">
    <uzo tip="fak">ELE</uzo>
    <dif>
      <ref tip="super" cel="grand.fizika0o">Fizika grando</ref>
      inversa al la <ref cel="rezist2.0o">rezistivo</ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol; &c_s;&c_p;&c_e;&c_c;&c_y;&c_f;&c_ib;&c_ch;&c_n;&c_a;&c_ja;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol; &c_a;&c_d;&c_n;&c_o;&c_s;&c_n;&c_a;&c_ja;</trd>
    </trdgrp>
    <trd lng="en">conductance</trd>
    <trdgrp lng="pl">
      <trd>przewodno&sacute;&cacute;</trd>,
      <trd>konduktywno&sacute;&cacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>konduktivita</trd>,
    <trd>vodivost</trd>
  </trdgrp>
  <trd lng="hu">vezet&odblac;k&eacute;pess&eacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x4F1D;&#x5C0E;&#x6027;  <pr>&#x3067;&#x3093;&#x3069;&#x3046;&#x305B;&#x3044;</pr></trd>,
    <trd>&#x4F1D;&#x5C0E;&#x7387;  <pr>&#x3067;&#x3093;&#x3069;&#x3046;&#x308A;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">vodivos&tcaron;</trd>
</drv>

<drv mrk="konduk4.super0eco">
  <kap>super<tld/>eco
  <fnt><bib>PIV1</bib></fnt></kap>
  <snc>
    <uzo tip="fak">FIZ</uzo>
    <uzo tip="fak">ELE</uzo>
    <dif>
      <ref tip="dif" cel="konduk.super0eco">Superkondukeco</ref>:
      <ekz>
        kunmemora &scirc;ildo pri <nom>Bardeen</nom> kaj la Teorio de Super<tld/>eco, &ccirc;e la Universitato
        de Ilinojso
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=42757">John Bardeen</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_y;&c_sh;&c_p;&c_r;&c_a;&c_v;&c_o;&c_d;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="de">Supraleitung</trd>
  <trd lng="en">superconductivity</trd>
  <trd lng="hu">szupravezet&odblac;-k&eacute;pess&eacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x8D85;&#x4F1D;&#x5C0E;  <pr>&#x3061;&#x3087;&#x3046;&#x3067;&#x3093;&#x3069;&#x3046;</pr></trd>,
    <trd>&#x8D85;&#x96FB;&#x5C0E;  <pr>&#x3061;&#x3087;&#x3046;&#x3067;&#x3093;&#x3069;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="pl">nadprzewodnictwo</trd>
  <trd lng="ru">&c_s;&c_v;&c_je;&c_r;&c_h;&c_p;&c_r;&c_o;&c_v;&c_o;&c_d;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="zh">&#x8D85;&#x5BFC; <pr>ch&#x101;od&#x1CE;o</pr></trd>
</drv>
</art>

