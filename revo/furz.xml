<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>furz</rad>/i
</kap>
<drv mrk="furz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ellasi intestajn gasojn, plej ofte kun bruo kaj malbonodoro.
    </dif>
  </snc>
  <trd lng="ca">pet</trd>
  <trdgrp lng="de">
   <trd>furzen</trd>,
   <trd>pfurzen [sudgerm]</trd></trdgrp>
  <trd lng="en">fart</trd>
  <trd lng="es">pedo</trd>
  <trd lng="fr">p&eacute;ter</trd>
  <trdgrp lng="hu">
    <trd>szellent</trd>,
    <trd>fingik</trd>
  </trdgrp>
  <trdgrp lng="lat"><trd>pedo, ere</trd>, <trd>crepo, are</trd></trdgrp>
  <trd lng="nl">een <ind>wind</ind> laten</trd>
  <trd lng="ru">&c_p;&c_u;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
</drv>
</art>
