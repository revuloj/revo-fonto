<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>9</ofc><rad>furz</rad>/i
</kap>

<drv mrk="furz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ellasi intestajn gasojn, plej ofte kun bruo kaj malbonodoro.
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_malmol;&c_r;&c_d;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>petar</trd>,
    <trd>fer pets</trd>
  </trdgrp>
  <trdgrp lng="de">
   <trd>furzen</trd>,
   <trd>pfurzen [sudgerm]</trd></trdgrp>
  <trd lng="en">fart</trd>
  <trd lng="es">pedo</trd>
  <trd lng="fr">p&eacute;ter</trd>
  <trdgrp lng="hu">
    <trd>szellent</trd>,
    <trd>fingik</trd>
  </trdgrp>
  <trdgrp lng="lat"><trd>pedo, ere</trd>, <trd>crepo, are</trd></trdgrp>
  <trd lng="nl">een <ind>wind</ind> laten</trd>
  <trdgrp lng="pt">
    <trd>peidar</trd>,
    <trd>soltar um <ind>pum</ind></trd>,
    <trd>soltar um <ind>traque</ind></trd>,
    <trd>bufar</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_u;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  <trdgrp lng="sv">
    <trd>fisa</trd>,
    <trd>prutta</trd>,
    <trd>sl&auml;ppa <ind>v&auml;der</ind></trd>
  </trdgrp>
</drv>

<drv mrk="furz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i.
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_r;&c_malmol;&c_d;&c_n;&c_ja;</trd>
  <trd lng="ca">pet</trd>
  <trd lng="de">Furz</trd>
  <trd lng="en">fart</trd>
</drv>

</art>
