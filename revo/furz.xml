<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: furz.xml,v 1.31 2024/09/11 09:31:22 revo Exp $">
<kap>
  <ofc>9</ofc><rad>furz</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="furz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Ellasi intestajn gasojn, plej ofte kun bruo kaj malbonodoro:
      <ekz>
        estu gaja, &ccirc;iujn vivozorgojn sendu <tld/>e al la Himalaja
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga
          Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni devus malpermesi al bestoj kaj homoj <tld/>adon, &ccirc;ar ties
        intestaj gasoj venenas la aeron
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009215.php">Malpermesoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mia edzino ronkas kiel <tld/>anta bovo
        <fnt>
          <bib>Monato</bib>
          <aut>Trevor Steele</aut>
          <vrk><url ref="&Monato;/2015/011474.php">Tridek
          jarojn poste</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_r;&c_d;&c_z;&c_je;&c_c;&c_mol;</trd>
  <trdgrp lng="ja">
    <trd>&#x304A;&#x306A;&#12425;&#12434;&#12377;&#x308B;</trd>,
    <trd>&#x5C41;&#12434;&#12402;&#x308B;  <pr>&#12408;&#12434;&#12402;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="lat">
    <trd>pedo, ere</trd>,
    <trd>crepo, are</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_malmol;&c_r;&c_d;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>petar</trd>,
    <trd>fer pets</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prd&ecaron;t</trd>,
    <trd>b&yacute;t n&ecaron;co n&ecaron;komu jedno</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>furzen</trd>,
    <trd>pfurzen [sudgerm]</trd>
  </trdgrp>
  <trd lng="en">fart</trd>
  <trd lng="es">peer</trd>
  <trd lng="fr">p&eacute;ter</trd>
  <trdgrp lng="hu">
    <trd>szellent</trd>,
    <trd>fingik</trd>
  </trdgrp>
  <trd lng="nl">een <ind>wind</ind> laten</trd>
  <trdgrp lng="pl">
    <trd>pierdzie&cacute;</trd>,
    <trd>puszcza&cacute; <ind>b&aogonek;ki</ind></trd>,
    <trd>puszcza&cacute; <ind>gazy</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>peidar</trd>,
    <trd>soltar um <ind>pum</ind></trd>,
    <trd>soltar um <ind>traque</ind></trd>,
    <trd>bufar</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_u;&c_k;&c_n;&c_u;&c_t;&c_mol;</trd>
  <trd lng="sk">prdn&uacute;&tcaron;</trd>
  <trdgrp lng="sv">
    <trd>fisa</trd>,
    <trd>prutta</trd>,
    <trd>sl&auml;ppa <ind>v&auml;der</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&#x43F;&c_u;&c_s;&#x43A;&c_a;&c_t;&c_i; &c_g;&c_a;&c_z;&c_i;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_d;&c_ib;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5C41; <pr>p&#xEC;</pr></trd>,
    <trd>&#x53D1;&#x5C41; <pr>f&amacron;p&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="furz.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        produkti konstantajn <tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2007/009300.php">Sukcesoplena,
          &scirc;ercoplena kaj tre malkutima</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>oj trovi&gcirc;as flami&gcirc;emaj gasoj, ekzemple hidrogeno kaj
        metano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=208903">Furzo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ja;&c_r;&c_d;&c_z;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="bg">&c_p;&c_r;&c_malmol;&c_d;&c_n;&c_ja;</trd>
  <trd lng="ca">pet</trd>
  <trd lng="cs">prd</trd>
  <trd lng="de">Furz</trd>
  <trd lng="en">fart</trd>
  <trd lng="es">pedo</trd>
  <trd lng="fr">pet</trd>
  <trdgrp lng="ja">
    <trd>&#x304A;&#x306A;&#12425;</trd>,
    <trd>&#x653E;&#x5C41;  <pr>&#12408;&#12402;&#x308A;</pr></trd>
  </trdgrp>
  <trd lng="sk">prd</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&#x43F;&c_u;&c_s;&#x43A;&c_a;&#x43D;&#x43D;&#x44F; &c_g;&c_a;&c_z;&c_ib;&c_v;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_d;&c_ib;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_g;&c_a;&c_z;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x5C41; <pr>p&#xEC;</pr></trd>
</drv>

</art>
