<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <rad>Komod</rad>/o <fnt><bib>Viki</bib></fnt>
</kap>
<drv mrk="komod1.0o">
  <kap><tld/>o</kap>
  <snc mrk="komod1.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="vid" cel="insul.0o">Insulo</ref> en <ref tip="vid"
      cel="indonezi.I0o">Indonezio</ref>
      apartenanta al Malgrandaj Sundaj Insuloj:
      <ekz> la insulo estas precipe konata pro la tie vivanta <tld/>a
      varano.
      </ekz>
    </dif>
   </snc>
   <ref tip="hom" cel="komod.0o">komodo (meblo)</ref>
  <trd lng="fr">Komodo</trd>
</drv>
</art>

