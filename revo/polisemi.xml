<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: polisemi.xml,v 1.8 2024/03/20 12:38:46 revo Exp $">
<kap>
  <rad>polisemi</rad>/o
</kap>
<drv mrk="polisemi.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="polisemi.0o.LIN">
    <dif>
      <ref tip="dif" cel="senc.plur0eco.LIN">Plursenceco</ref>:
      <ekz>
        al la bazaj faktoj de la lingvo apartenas la <tld/>o:
        &gcirc;i estas unu el la esprimoj de la lingva ekonomio
        <fnt>
          <aut>B. Wacha</aut>,
          <vrk><url ref="http://www.lingviko.net/db/36_Vaha.htm">
          Anta&ubreve;-sosurumo</url></vrk>,
          en: Festlibro oma&gcirc;e al la 60-jari&gcirc;o de Detlev Blanke,
          2001<lok/>
        </fnt>;
      </ekz><ekz>
        <tld/>a termino estas termino havanta du a&ubreve; pli da interdependaj signifoj
        <fnt>
          <aut>J. Werner</aut>,
          <vrk>Terminologia Kurso</vrk>,
          <lok>2009-03-29</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="homonimi.0o">homonimio</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_m;&c_a;&c_t;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_s;&c_e;&c_m;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="cs">mnohov&yacute;znamovost slova</trd>
  <trd lng="de">Polysemie</trd>
  <trd lng="pl">polisemia</trd>
  <trd lng="ro">polisemie</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_i;&c_s;&c_je;&c_m;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>mnohov&yacute;znamnos&tcaron; slova</trd>,
    <trd>polys&eacute;mia</trd>
  </trdgrp>
  <trd lng="uk">&c_p;&c_o;&c_l;&c_ib;&c_s;&c_je;&c_m;&c_ib;&c_ja;</trd>
</drv>
</art>

