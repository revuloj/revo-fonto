<art mrk="$Id: impres2.xml,v 1.16 2022/03/21 15:27:48 revo Exp $">
<kap>
  <rad>impresionism</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="impres2.0o">
  <kap><tld/>o</kap>
  <snc mrk="impres2.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="dif" cel="impres.0ismo.BELA">Impresismo</ref>:
      <ekz>
        <nom>E. Manet</nom> jam en 1860-aj jaroj montris la direkton de <tld/>o;
      </ekz>
      <ekz>
        la <tld/>aj pentristoj, kiel <nom>Renoir</nom> kaj <nom>Sisley</nom>, estis kontra&ubreve; la realecaj
        pentra&jcirc;oj kaj havis la saman opinion, ke pentristoj povus akiri pli liberan
        pentromanieron
        <fnt>
          <aut>Matsumoto M.</aut>
          <vrk>Kial naski&gcirc;is impresionismo?</vrk>
          <lok>Kontakto, 2011-2019</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="impres3.0o">impresionisto</ref>
  <trd lng="be">&c_ib;&c_m;&c_p;&c_r;&c_e;&c_s;&c_ib;&c_ja;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_i;&c_m;&c_p;&c_r;&c_je;&c_s;&c_i;&c_o;&c_n;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="de">Impressionismus</trd>
  <trd lng="en">impressionism</trd>
  <trd lng="es">impresionismo</trd>
  <trd lng="fr">impressionnisme</trd>
  <trd lng="hu">impresszionizmus</trd>
  <trd lng="nl">impressionisme</trd>
  <trd lng="pt">impressionismo</trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_r;&c_je;&c_s;&c_s;&c_i;&c_o;&c_n;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">impresionizmus</trd>
  <trd lng="cs">impresionizmus</trd>
  <trd lng="uk">&#x456;&#x43C;&#x43F;&#x440;&#x435;&#x441;&#x456;&#x43E;&#x43D;&#x456;&#x437;&#x43C;</trd>
