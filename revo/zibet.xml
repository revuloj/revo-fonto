<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: zibet.xml,v 1.12 2023/06/04 12:39:41 revo Exp $">
<kap>
  <rad>zibet</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="zibet.0o">
  <kap><tld/>o</kap>
  <snc mrk="zibet.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Malgranda rabobesto el Hindujo, Hindo&ccirc;inio, Suda &Ccirc;inujo, 
      Malajzio,
      specio de la genro <ref tip="super" cel="viver.0o">vivero</ref> 
      (<trd lng="la">Viverra zibetha</trd>), 80
      centimetrojn longa, sin nutranta per musoj, birdoj, serpentoj kaj
      aliaj bestetoj:
      <ekz>
        tridek kvin <tld/>aj feloj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 105a, volumo 3a, p.
        367a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="cibet.0kato">cibetkato</ref>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_v;&c_e;&c_r;&c_a; &c_a;&c_z;&c_ib;&c_ja;&c_c;&c_k;&c_a;&c_ja;</trd>
  <trd lng="bg">&c_c;&c_i;&c_v;&c_je;&c_t;&c_a;</trd>
  <trd lng="ca"><ind>civeta</ind> grossa de l'&Iacute;ndia</trd>
  <trd lng="de">Zibetkatze</trd>
  <trd lng="en">civet</trd>
  <trd lng="fr">grande <ind>civette</ind> d'Inde</trd>
  <trd lng="hu">cibetmacska</trd>
  <trd lng="nl">civetkat</trd>
  <trdgrp lng="pt">
    <trd>civeta</trd>
  </trdgrp>
  <trd lng="it">zibetto</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_v;&c_je;&c_r;&c_a; &c_a;&c_z;&c_ib;&c_a;&c_t;&c_s;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_c;&c_i;&c_b;&c_je;&c_t;&c_a;</trd>
  </trdgrp>
</drv>
</art>

