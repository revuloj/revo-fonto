<drv mrk="negoc.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="negoc.0o.entrepreno">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Profitcela komerca a&ubreve; industria okupo,
      <ref tip="sin" cel="entrep.0o.EKON">entrepreno</ref>:
      <ekz>
        en malgranda urbeto, &ccirc;e unu homo, kiu havis sian propran
        <tld/>on
        <fnt><bib>Fab3</bib>, <lok>Malnova tomba &scirc;tono</lok></fnt>;
      </ekz>
      <ekz>
        en multaj landoj televido estas <tld/>o kaj elsendoj nur
        reklamujoj
        <fnt>
           <aut>H. Alos i Font</aut>,
           <vrk>Televido: spektantoj spektataj</vrk>,
           <bib>Monato</bib>           
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o viglis dum tri jaroj
        <fnt><aut>S. Szathmari</aut>, <vrk>Pythagoras</vrk></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="firm1.0o">firm(a)o</ref>,
      <ref cel="komerc.0ejo">komercejo</ref>,
      <ref cel="kompan.0o.afero">kompanio<sncref/></ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_a; <klr>(&c_k;&c_a;&c_m;&c_e;&c_r;&c_c;&c_y;&c_j;&c_n;&c_a;&c_ja;)</klr></trd>,
      <trd>&c_b;&c_ib;&c_z;&c_n;&c_e;&c_s;</trd>,
      <trd>&c_z;&c_a;&c_n;&c_ja;&c_t;&c_a;&c_k; <klr>(&c_k;&c_a;&c_m;&c_e;&c_r;&c_c;&c_y;&c_j;&c_n;&c_y;)</klr></trd>,
      <trd>&c_k;&c_a;&c_m;&c_e;&c_r;&c_c;&c_y;&c_ja;</trd>
    </trdgrp>
    <trd lng="de">Handlung <klr>(Gesch&auml;ft)</klr></trd>
    <trd lng="en">business</trd>
    <trd lng="nl">handelszaak</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_l;&c_o;</trd>,
      <trd>&c_b;&c_i;&c_z;&c_n;&c_je;&c_s;</trd>
    </trdgrp>
  </snc>
  <snc mrk="negoc.0o.transakcio">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Aparta komerca afero de <ref cel="acxet.0o">a&ccirc;eto</ref> 
      a&ubreve; <ref cel="vend.0o">vendo</ref>:
      <ekz>
        nenio malhelpas a&ccirc;etanton iri rekte al la <klr>[&#x2026;]</klr>
        fabrikanto, tiel ke la retservo ricevas nenion el la <tld/>o
        <fnt>
          <aut>E. Grimley Evans</aut>, <vrk>Interreto
          senpage</vrk>,
          <bib>Monato</bib>, <lok>2000:11, p. 23a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="operac.0o.EKON">operacio<sncref/></ref>,
      <ref cel="transa.0o">transakcio</ref>
      </refgrp>
    <trdgrp lng="be">
      <trd>&c_z;&c_mol;&c_d;&c_z;&c_je;&c_l;&c_k;&c_a;</trd>,
      <trd>&c_u;&c_g;&c_o;&c_d;&c_a;</trd>,
      <trd>&c_a;&c_p;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ja; <klr>(&c_k;&c_a;&c_m;&c_e;&c_r;&c_c;&c_y;&c_j;&c_n;&c_a;&c_ja;)</klr></trd>,
      <trd>&c_t;&c_r;&c_a;&c_n;&c_z;&c_a;&c_k;&c_c;&c_y;&c_ja; <klr>(&c_k;&c_a;&c_m;&c_e;&c_r;&c_c;&c_y;&c_j;&c_n;&c_a;&c_ja;)</klr></trd>
    </trdgrp>
    <trd lng="de">Handel <klr>(Gesch&auml;ft)</klr></trd>
    <trdgrp lng="nl">
      <trd>handelstransactie</trd>,
      <trd>transactie<klr tip="ind"> (handel)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_d;&c_je;&c_l;&c_k;&c_a;</trd>
  </snc>
  <trdgrp lng="cs">
  <trd>obchod <klr>(podnik)</klr></trd>,
  <trd>obchodn&iacute; jedn&aacute;n&iacute;</trd>,
  <trd>obchodov&aacute;n&iacute;</trd>
