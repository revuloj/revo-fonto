<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>negoc</rad>/o
</kap>
<drv mrk="negoc.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Afero, entrepreno, perado, kies celo estas mona profito.
    </dif>
    <refgrp tip="sin">
      <ref cel="komerc.0o">komerco</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Gesch&auml;ft</trd>
  <trd lng="nl">handelszaak</trd>
  <trd lng="ru">&c_b;&c_i;&c_z;&c_n;&c_je;&c_s;</trd>
</drv>

<drv mrk="negoc.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o.
    </dif>
    <trd lng="de">gesch&auml;ftlich</trd>
  </snc>
  <snc>
    <dif>
      <tld lit="N"/>ema:
      <ekz>
        malkareco en nia <tld/>a tempo &ccirc;iuokaze meritas
        prikonsideron<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="prakti.0a">praktika</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">zakelijk</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_l;&c_o;&c_v;&c_o;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_m;&c_je;&c_r;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
