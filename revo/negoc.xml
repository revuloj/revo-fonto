  <trd>obchodn&iacute;</trd>
  </trdgrp>
  <trd lng="de">gesch&auml;ftlich</trd>
  <trdgrp lng="en">
      <trd>commercial</trd>,
      <trd>business</trd>
    </trdgrp>
