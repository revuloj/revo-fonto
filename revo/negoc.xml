<drv mrk="negoc.0i">
  <kap><tld/>i</kap>
  <snc mrk="negoc.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="komerc.0i">Komerci</ref>
      planante kaj diskutante a&ccirc;etojn kaj vendojn:
      <ekz>
        vi povos <tld/>i en la lando
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 42:34</lok>
        </fnt>;
      </ekz>
      <ekz>
        apud la blankaj muroj <klr>[&#x2026;]</klr> <tld/>is je krucetoj kaj helkolorigitaj
        sanktaj bildoj maljunulinoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        unu grupo de akciuloj <tld/>is kun <nom>KBC</nom> kaj atingis pli
        grandan, kvankam ne la esperatan kompenson
        <fnt>
          <aut>R. Rotsaert</aut>, <vrk>Belgaj bankoj
          regrupigitaj</vrk>, <bib>Monato</bib>,
          <lok>1998:11, p. 16a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="trakt.inter0i">intertrakti</ref>,
      <ref cel="kontra3.0i">kontrakti</ref>,
      <ref cel="marcxa.0i">mar&ccirc;andi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_j;&c_m;&c_a;&c_c;&c_c;&c_a; <ind>&c_b;&c_ib;&c_z;&c_n;&c_e;&c_s;&c_a;&c_m;</ind></trd>,
    <trd>&c_v;&c_je;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_s;&c_p;&c_r;&c_a;&c_v;&c_y;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
  <trd>obchodn&ecaron; jednat</trd>,
  <trd>obchodovat</trd>,
  <trd>zab&yacute;vat se obchodem</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>handeln</trd>,
    <trd><ind>Handel</ind> treiben</trd>,
    <trd><ind>Gesch&auml;ft</ind>e machen</trd>
  </trdgrp>
  <trdgrp lng="en">
      <trd>trade</trd>,
      <trd>do business</trd>
    </trdgrp>
  <trd lng="es">negociar</trd>
  <trdgrp lng="fr">
      <trd>faire du <ind>n&eacute;goce</ind></trd>,
      <trd>n&eacute;gocier<klr> (commercialement)</klr></trd>
    </trdgrp>
  <trd lng="hu">&uuml;zletel</trd>
  <trdgrp lng="ja">
    <trd>&#x53D6;&#x5F15;&#x3059;&#x308B;  <pr>&#x3068;&#x308A;&#x3072;&#x304D;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5546;&#x8AC7;&#x3059;&#x308B;  <pr>&#x3057;&#x3087;&#x3046;&#x3060;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x4EA4;&#x6E09;&#x3059;&#x308B;  <pr>&#x3053;&#x3046;&#x3057;&#x3087;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl"><ind>handel</ind> drijven</trd>
  <trdgrp lng="pl">
