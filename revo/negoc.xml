      <trd>businessman</trd>
    </trdgrp>
  <trd lng="es">negociante</trd>
  <trdgrp lng="fr">
      <trd>n&eacute;gociant</trd>
    </trdgrp>
  <trd lng="hu">&uuml;zletember</trd>
  <trdgrp lng="ja">
