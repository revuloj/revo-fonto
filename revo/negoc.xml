    </trdgrp>
  <trd lng="hu">&uuml;zleti</trd>
  <trdgrp lng="ja">
    <trd>&#x53D6;&#x5F15;&#x306E;  <pr>&#x3068;&#x308A;&#x3072;&#x304D;&#x306E;</pr></trd>,
    <trd>&#x5546;&#x8AC7;&#x306E;  <pr>&#x3057;&#x3087;&#x3046;&#x3060;&#x3093;&#x306E;</pr></trd>,
    <trd>&#x5B9F;&#x52D9;&#x7684;&#x306A;  <pr>&#x3058;&#x3064;&#x3080;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x30D3;&#x30B8;&#x30CD;&#x30B9;&#x30E9;&#x30A4;&#x30AF;&#x306A;  <pr>&#x3073;&#x3058;&#x306D;&#x3059;&#x3089;&#x3044;&#x304F;&#x306A;</pr></trd>
