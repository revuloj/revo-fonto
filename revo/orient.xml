      <ekz>
        nord<tld/>e la &ccirc;ielo komencis brileti en varmaj koloroj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili faris longan ekskurson al alta monto nord<tld/>e de Torento
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Lemuel</nom> subite alprenis flankan vojeton, nord<tld/>en, kondukantan unue tra
        senherba, dezerta regiono
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XII</lok>
        </fnt>.
      </ekz>
    </dif>
