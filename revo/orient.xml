<drv mrk="orient.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="orient.0i.direkti">
    <dif>
      <ref tip="super" cel="direkt.0i">Direkti</ref> ion la&ubreve; geografia
      &ccirc;efpunkto, aparte al <tld/>o:
      <ekz>
        <tld/>i kelkajn tombojn al Mekko
        <fnt>
          <bib>ChB</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        dekstre <klr>(politika dekstro)</klr> <tld/>itaj medioj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006882.html">Pollando: Kiam orkestro
          fari&gcirc;as orkesto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i <klr>(prefere direkti, celi)</klr> la pafon al la lar&gcirc;a fenestro
        <fnt>
          <bib>ChM</bib>
          <lok>5</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">durc'haat war-zu ar <ind>reter</ind></trd>
    <trd lng="de"><klr>(nach </klr>Osten) <ind>ausrichten</ind></trd>
    <trd lng="hu">keletel</trd>
    <trd lng="es">orientar</trd>
    <trd lng="nl">naar het <ind>oosten</ind> richten</trd>
    <trd lng="pl">orientowa&cacute;</trd>
  </snc>
  <snc mrk="orient.0i.determini">
    <dif>
      <ref tip="super" cel="determ.0i.KOMUNE">Determini</ref> la situacion de persono a&ubreve; objekto
      rilate al <tld/>o:
      <ekz>
        <tld/>i karton, planon;
      </ekz>
      <ekz>
        kial estis tiel malfacile <tld/>i la foton
        <fnt>
          <bib>ChB</bib>
          <lok>27</lok>
        </fnt>?
      </ekz>
      <ekz>
        bone <tld/>ita en &ccirc;iuj bran&ccirc;oj de la scienco
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi povus iom <tld/>i min pri ebloj de negocado &ccirc;i tie
        <fnt>
          <bib>KrM</bib>
          <lok>25. Multe Da Spirito Sed Neniom Da Profito.</lok>
        </fnt>?
      </ekz>
      <ekz>
        <klr>[la verko]</klr> montri&gcirc;is bonega maniero <tld/>i a&ubreve; manipuli la
        publikopinion
        <fnt>
          <bib>ChV</bib>
          <lok>14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>priprofesia <tld/>ado</ind>
        <klr>(metoda disdonado de lernantoj en diversajn metiojn la&ubreve;
        iliaj apartaj kapabloj kaj gustoj)</klr>.
        <trd lng="fr">orientation professionnelle</trd>
        <trd lng="id">karier</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>ausrichten</trd>,
      <trd>einrichten</trd>,
      <trd>orientieren</trd>,
      <trd>einnorden</trd>
    </trdgrp>
    <trd lng="hu">bet&aacute;jol</trd>
    <trdgrp lng="id">
      <trd>mem<ind>bimbing</ind></trd>,
      <trd>meng<ind>orientasi</ind>kan</trd>
    </trdgrp>
    <trd lng="nl">naar het <ind>oosten</ind> richten</trd>
    <trd lng="pl">orientowa&cacute;</trd>
  </snc>
  <snc mrk="orient.0i.spacon">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      Elekti <ref cel="baz.0o.vektoraspaco">bazon</ref>, rilate al kiu
      oni determinos la <ref cel="orient.0igxo.bazo"><tld/>i&gcirc;on</ref>
      de &ccirc;iuj aliaj bazoj.
    </dif>
    <trdgrp lng="br">
      <trd>reteri&ntilde;<klr tip="ind"> (matematik)</klr></trd>,
      <trd>orianti&ntilde;<klr tip="ind"> (matematik)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>orientieren</trd>,
      <trd><klr>eine </klr>Orientierung <ind>festlegen</ind></trd>
    </trdgrp>
    <trd lng="en">orientate</trd>
    <trd lng="hu">ir&aacute;ny&iacute;t</trd>
    <trd lng="nl">ori&euml;nteren</trd>
    <trd lng="pl">orientowa&cacute;</trd>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_y;&c_je;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>orientovat</trd>,
    <trd>sm&ecaron;&rcaron;ovat na v&yacute;chod</trd>
  </trdgrp>
  <trd lng="fr">orienter</trd>
  <trdgrp lng="ja">
    <trd>&#x65B9;&#x5411;&#x3065;&#x3051;&#x308B;  <pr>&#x307B;&#x3046;&#x3053;&#x3046;&#x3065;&#x3051;&#x308B;</pr></trd>,
    <trd>&#x5411;&#x304D;&#x3092;&#x6C7A;&#x3081;&#x308B;  <pr>&#x3080;&#x304D;&#x3092;&#x304D;&#x3081;&#x308B;</pr></trd>,
    <trd>&#x6771;&#x5411;&#x304D;&#x306B;&#x5EFA;&#x3066;&#x308B; 
    <pr>&#x3072;&#x304C;&#x3057;&#x3080;&#x304D;&#x306B;&#x305F;&#x3066;&#x308B;</pr></trd>,
    <trd>&#x9053;&#x6848;&#x5185;&#x3059;&#x308B;  <pr>&#x3069;&#x3046;&#x3042;&#x3093;&#x306A;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6307;&#x5C0E;&#x3059;&#x308B;  <pr>&#x3057;&#x3069;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pt">orientar</trd>
  <trd lng="ru">&c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>orientova&tcaron;</trd>,
    <trd>smerova&tcaron;</trd>,
    <trd>usmer&ncaron;ova&tcaron;</trd>,
    <trd>zamera&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_r;&c_ib;&c_jeu;&c_n;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trd lng="vo">lef&uuml;d&ouml;n</trd>
  <trdgrp lng="zh">
    <trd>&#x5BFC;&#x5411; <pr>d&#x1CE;oxi&#xE0;ng</pr></trd>,
    <trd>&#x5C0E;&#x5411; <pr>d&#x1CE;oxi&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="orient.0igxi">
  <kap><tld/>i&gcirc;i, <var><kap>sin <tld/>i</kap></var></kap>
  <snc mrk="orient.0igxi.determini">
    <dif>
      <ref tip="super" cel="determ.0i.KOMUNE">Determini</ref> la direkton, kiu devas esti sekvata:
      <ekz>
        la estro ne povis sin <tld/>i, de kie venas la vo&ccirc;o
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        centoj da vojoj, kie oni ne povas sin <tld/>i
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas jam profunda nokto, kaj mi <klr>[&#x2026;]</klr> tute ne povas min <tld/>i
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la mallumo li ne povis tie <tld/>i&gcirc;i
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi sukcesis <tld/>i min per unu-du konstrua&jcirc;oj, kiujn mi rekonis
        <fnt>
          <bib>StU</bib>
          <lok>La Nazaretanoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li eksidas kaj &scirc;ajnas <tld/>i sin, iom post iom li konscias, kie li estas
        <fnt>
          <bib>IdO</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="br">en em <ind>reteri&ntilde;</ind></trd>
    <trd lng="de"><klr>sich </klr>orientieren</trd>
    <trdgrp lng="hu">
      <trd>t&aacute;j&eacute;koz&oacute;dik</trd>,
      <trd>eligazodik</trd>
    </trdgrp>
    <trd lng="nl">zich <ind>ori&euml;nteren</ind></trd>
  </snc>
  <snc mrk="orient.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Klare kompreni la situacion, en kiu oni trovi&gcirc;as, kaj difini,
      kion oni devas fari:
      <ekz>
        se mi demandas ilin pri iliaj sociaj institucioj, ili e&ccirc; troe min <tld/>as pri
        &ccirc;io
        <fnt>
          <bib>VaK</bib>
          <lok>Kvara &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ne povas sin <tld/>i en la pensmaniero de <klr>[tiu popolo]</klr>
        <fnt>
          <bib>VaK</bib>
          <lok>Dektria &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        atakoj, kiuj ne donis al niaj soldatoj la povon dece <tld/>i&gcirc;i kaj
        interkomuniki&gcirc;i
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Kvara Kongreso Esperantista en Dresden en la 17a de a&ubreve;gusto
          1908</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i parolis &ccirc;ion, kion &gcirc;i volis <klr>[&#x2026;]</klr> neniu havis la tempon <tld/>i&gcirc;i
        pri <klr>[&#x2026;]</klr> &gcirc;ia parolado
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">entscheiden</trd>
    <trd lng="hu">eligazodik</trd>
    <trd lng="nl">beslissen</trd>
  </snc>
      <trd lng="be">&c_a;&c_r;&c_y;&c_je;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>orientovat</trd>,
    <trd>sm&ecaron;&rcaron;ovat na v&yacute;chod</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>orienter</ind></trd>
  <trd lng="id">ber<ind>orientasi</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x81EA;&#x5206;&#x306E;&#x5C45;&#x6240;&#x3092;&#x77E5;&#x308B; 
    <pr>&#x3058;&#x3076;&#x3093;&#x306E;&#x3044;&#x3069;&#x3053;&#x308D;&#x3092;&#x3057;&#x308B;</pr></trd>,
    <trd>&#x72B6;&#x6CC1;&#x3092;&#x898B;&#x304D;&#x308F;&#x3081;&#x308B; 
    <pr>&#x3058;&#x3087;&#x3046;&#x304D;&#x3087;&#x3046;&#x3092;&#x307F;&#x304D;&#x308F;&#x3081;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">orientowa&cacute; si&eogonek;</trd>
  <trd lng="pt">orientar-se</trd>
  <trd lng="ru">&c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>orientova&tcaron;</trd>,
    <trd>smerova&tcaron;</trd>,
    <trd>usmer&ncaron;ova&tcaron;</trd>,
    <trd>zamera&tcaron;</trd>
  </trdgrp>
   <adm>
     Laux PIV1 ~igxi havas nur la figuran sencon, sed mi dubas, cxu
     tio restas gxusta. <aut>MB</aut>
   </adm>
</drv>

<drv mrk="orient.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="orient.0igxo.KOMUNE">
    <dif>
      Ago <tld/>i&gcirc;i; stato de io a&ubreve; iu <tld/>ita;
      de iu, kiu <tld/>i&gcirc;as:
      <ekz>
        <tld/>i&gcirc;o rilate al la ventodirekto
        <fnt>
          <bib>ChC</bib>
          <lok>17</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i unu tagon tiel perdis la <tld/>i&gcirc;on, ke &scirc;i plu ne pensis pri la doninto
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        por orienti&gcirc;o <klr>[&#x2026;]</klr> oni donis alnomojn la&ubreve; merito
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VI.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">orientation</trd>
    <trd lng="hu">t&aacute;j&eacute;koz&oacute;d&aacute;s</trd>
    <trd lng="id">orientasi</trd>
    <trdgrp lng="pl">
      <trd>orientacja</trd>,
      <trd>zorientowanie</trd>
    </trdgrp>
  </snc>
  <snc mrk="orient.0igxo.bazo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(de <ref cel="baz.0o.vektoraspaco">bazo</ref>
      <frm am="bb B"><g>B</g></frm>
      rilate al bazo <frm am="bb B&minute;"><g>B</g>'</frm>)</klr>
      La <ref cel="sign.0o.MAT">signo</ref> de la
      <ref cel="determ.0anto.vektoroj">determinanto</ref> de &gcirc;iaj
      vektoroj rilate al <frm am="bb B&minute;"><g>B</g>'</frm>:
      <ekz>
        pozitiva a&ubreve; negativa <tld/>i&gcirc;o;
      </ekz>
      <ekz>
        aksa simetrio en ebeno modifas la <tld/>i&gcirc;on <klr>(la
        <tld/>i&gcirc;o
        de bildo de bazo <frm am="bb B"><g>B</g></frm> rilate al
        <frm am="bb B&minute;"><g>B</g></frm>
        estas negativa)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dekstr.0uma.bazo">dekstruma</ref>,
      <ref cel="dekstr.mal0uma.bazo">maldekstruma</ref>.
    </refgrp>
    <trdgrp lng="br">
      <trd>reteradur<klr tip="ind"> (matematik)</klr></trd>,
      <trd>oriantadur<klr tip="ind"> (matematik)</klr></trd>
    </trdgrp>
    <trd lng="en">orientation</trd>
    <trd lng="fr">orientation</trd>
    <trd lng="hu">ir&aacute;ny&iacute;totts&aacute;g</trd>
    <trd lng="pl">orientacja</trd>
    <trd lng="pt">orienta&ccedil;&atilde;o</trd>
    <trd lng="nl">ori&euml;ntatie</trd>
    <trd lng="ru">&c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  </snc>
    <trd lng="be">&c_a;&c_r;&c_y;&c_je;&c_n;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Orientierung</trd>,
    <trd>Richtung <klr>(Orientierung)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x65B9;&#x4F4D;&#x306E;&#x78BA;&#x5B9A; 
    <pr>&#x307B;&#x3046;&#x3044;&#x306E;&#x304B;&#x304F;&#x3066;&#x3044;</pr></trd>,
    <trd>&#x6307;&#x91DD;  <pr>&#x3057;&#x3057;&#x3093;</pr></trd>,
    <trd>&#x6307;&#x5411;  <pr>&#x3057;&#x3053;&#x3046;</pr></trd>
  </trdgrp>
