<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>orient</rad>/o</kap>
<drv mrk="orient.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Parto de la &ccirc;ielo, en kiu la suno &scirc;ajnas levi&gcirc;i.
      <ekz>
        lando limita &ccirc;e la <tld/>o per la maro.
      </ekz>
    </dif>
    <trd lng="de">Osten</trd>
  </snc>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Terparto ku&scirc;anta <tld/>e de E&ubreve;ropo:
      <ekz>
        tapi&scirc;oj el <tld lit="O"/>o.
      </ekz>
    </dif>
    <trd lng="de">Orient</trd>
  </snc>
  <snc>
    <dif>
      En framasonaro, parto de lo&gcirc;io, kie sidas la lo&gcirc;iestro.
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_h;&c_o;&c_d;</trd>
  <trd lng="el">&alfa;&ny;&alfa;&tau;&omikron;&lambda;&eta_ton;</trd>
  <trd lng="en">east</trd>
  <trdgrp lng="es">
    <trd>este</trd>,
    <trd>oriente</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>est</trd>,
    <trd>orient</trd></trdgrp> 
  <trd lng="nl">oosten</trd>
  <trd lng="pt">oriente</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_t;&c_o;&c_k;</trd>
  <trd lng="sv">&ouml;st</trd>
</drv>

<drv mrk="orient.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>aj popoloj, lingvoj, juveloj;
      </ekz>
      <ekz>
        <tld/>a klimato, stelo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_s;&c_h;&c_o;&c_d;&c_n;&c_ib;</trd>
  <trdgrp lng="de">
    <trd>&ouml;stlich</trd>,
    <trd>orientalisch</trd>
  </trdgrp>
  <trd lng="el">&alfa;&ny;&alfa;&tau;&omikron;&lambda;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="es">oriental</trd>
  <trdgrp lng="fr">
    <trd>oriental</trd>,
    <trd>d'<ind>est</ind></trd></trdgrp> 
  <trd lng="hu">keleti</trd>
  <trd lng="nl">oostelijk</trd>
  <trd lng="pt">oriental</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="orient.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      &Ccirc;e la <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a; &c_w;&c_s;&c_h;&c_o;&c_d;&c_z;&c_je;</trd>
  <trd lng="fr">&agrave; <ind>l'est</ind></trd>
  <trd lng="hu">keleten</trd>
  <trd lng="ru">&c_n;&c_a; &c_v;&c_o;&c_s;&c_t;&c_o;&c_k;&c_je;</trd>
</drv>

<drv mrk="orient.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Bazi konstruon, turnante &gcirc;ian frunton al <tld/>o:
      <ekz>
        la katolikaj
        eklezioj kaj mahometanaj moskeoj estas <tld/>itaj;
      </ekz>
      <ekz>
        malbone <tld/>ita domo.
      </ekz>
    </dif>
    <trd lng="de"><klr>(nach </klr>Osten) <ind>ausrichten</ind></trd>
    <trd lng="hu">bet&aacute;jol</trd>
    <trd lng="es">orientar</trd>
    <trd lng="nl">naar het <ind>oosten</ind> richten</trd>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_y;&c_je;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <snc>
    <dif>
      Determini la situacion de persono a&ubreve; objekto rilate al <tld/>o:
      <ekz>
        <tld/>i karton, planon;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>priprofesia <tld/>ado</ind>
        <klr>(metoda disdonado de lernantoj en diversajn metiojn la&ubreve; 
        iliaj apartaj kapabloj kaj gustoj)</klr>.
        <trd lng="fr">orientation professionnelle</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>ausrichten</trd>,
      <trd>einrichten</trd>,
      <trd>orientieren</trd>,
      <trd>einnorden</trd>
    </trdgrp>

    <trd lng="nl">naar het <ind>oosten</ind> richten</trd>
  </snc>
  <snc mrk="orient.0i.spacon">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      Elekti <ref cel="baz.0o.vektoraspaco">bazon</ref>, rilate al kiu oni 
      determinos la <ref cel="orient.0igxo.bazo"><tld/>i&gcirc;on</ref>
      de &ccirc;iuj aliaj bazoj.
    </dif>
  </snc>
  <trdgrp lng="de">
      <trd>orientieren</trd>,
      <trd><klr>eine </klr>Orientierung <ind>festlegen</ind></trd>
  </trdgrp>
  <trd lng="fr">orienter</trd>
  <trd lng="pt">orientar</trd>
  <trd lng="ru">&c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="orient.0igxi">
  <kap><tld/>i&gcirc;i, sin <tld/>i</kap>
  <snc>
    <dif>
      Determini la direkton, kiu devas esti sekvata:
      <ekz>
        sin <tld/>i en arbaro, en nekonata urbo;
      </ekz>
      <ekz>
        la aviadistoj ne povis plu sin <tld/>i pro la densa nebulo.
      </ekz>
    </dif>
    <trd lng="de"><klr>sich </klr>orientieren</trd>
    <trd lng="hu">t&aacute;j&eacute;koz&oacute;dik</trd>
    <trd lng="nl">ori&euml;nteren</trd>
    <trd lng="be">&c_a;&c_r;&c_y;&c_je;&c_n;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
    <trd lng="ru">&c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc mrk="orient.0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Klare kompreni la situacion, en kiu oni trovi&gcirc;as, kaj difini, 
      kion oni devas fari:
      <ekz>
        la prezidanto de kunsido devas sin rapide <tld/>i en la debatoj.
      </ekz>
    </dif>
    <trd lng="de">entscheiden</trd>
    <trd lng="hu">eligaz&iacute;t</trd>
    <trd lng="nl">beslissen</trd>
  </snc>
  <trd lng="fr">s'<ind>orienter</ind></trd>
  <adm>
    Laux PIV1 ~igxi havas nur la figuran sencon, sed mi dubas, cxu
    tio restas gxusta. <aut>MB</aut>
  </adm>
</drv>

<drv mrk="orient.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="orient.0igxo.KOMUNE">
    <dif>
      Stato de io a&ubreve; iu <tld/>ita; de iu, kiu <tld/>i&gcirc;as
      <sncref ref="orient.0igxi.FIG"/>:
      <ekz>
        &scirc;i unu tagon tiel perdis la <tld/>i&gcirc;on, ke &scirc;i ne
        plu pensis pri la doninto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="fr">orientation</trd>
  </snc>
  <snc mrk="orient.0igxo.bazo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(de <ref cel="baz.0o.vektoraspaco">bazo</ref> <frm><g>B</g></frm>
      rilate al bazo <frm><g>B</g>'</frm>)</klr>
      La <ref cel="sign.0o.MAT">signo</ref> de la 
      <ref cel="determ.0anto.vektoroj">determinanto</ref> de &gcirc;iaj 
      vektoroj rilate al <frm><g>B</g>'</frm>:
      <ekz>
        pozitiva a&ubreve; negativa <tld/>i&gcirc;o;
      </ekz>
      <ekz>
        aksa simetrio modifas la <tld/>i&gcirc;on <klr>(la <tld/>i&gcirc;o
        de bildo de bazo <frm><g>B</g></frm> rilate al <frm><g>B</g></frm>
        estas negativa)</klr>. 
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dekstr.0uma.bazo">dekstruma</ref>,
      <ref cel="dekstr.mal0uma.bazo">maldekstruma</ref>.
    </refgrp>
    <trd lng="de">Orientierung</trd>
    <trd lng="en">orientation</trd>
    <trd lng="fr">orientation</trd>
    <trd lng="pl">orientacja</trd>
    <trd lng="ru">&c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>
  </snc>
</drv>

<drv mrk="orient.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      &Ccirc;io, kio servas por sin <tld/>i.
    </dif>
    <refgrp tip="sin">
      <ref cel="kompas.0o">kompaso</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>point de rep&egrave;re</trd>,
    <trd>signe d'orientation</trd></trdgrp> 
  <trd lng="hu">t&aacute;jol&oacute;eszk&ouml;z</trd>
  <trd lng="nl">ori&euml;ntatiemiddel</trd>
  <trd lng="ru">&c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;</trd>
</drv>

<drv mrk="orient.ekstrem0o">
  <kap>ekstrem<tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      &Ccirc;inujo, Japanujo kaj &ccirc;irka&ubreve;a&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_l;&c_jo;&c_k;&c_ib; &c_w;&c_s;&c_h;&c_o;&c_d;</trd>
  <trd lng="fr">Extr&ecirc;me-Orient</trd>
  <trd lng="hu">T&aacute;vol-Kelet</trd>
  <trd lng="nl">verre <ind>Oosten</ind></trd>
  <trd lng="ru">&c_D;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; <ind>&c_V;&c_o;&c_s;&c_t;&c_o;&c_k;</ind></trd>
</drv>

<drv mrk="orient.grand0o">
  <kap>grand<tld/>o</kap>
  <snc>
    <dif>
      Centra lo&gcirc;io, formita en &ccirc;efurbo de reprezentantoj de
      &ccirc;iuj provincaj framasonaj lo&gcirc;ioj.
    </dif>
  </snc>
  <trd lng="nl">groot <ind>Oosten</ind></trd>
  <trd lng="fr">Grand Orient</trd>
  <trd lng="hu">Nagy <ind>Oriens</ind></trd>
  <trd lng="ru">&c_V;&c_je;&c_l;&c_i;&c_k;&c_i;&c_j; <ind>&c_V;&c_o;&c_s;&c_t;&c_o;&c_k;</ind></trd>
</drv>
</art>
