<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kristofor.xml,v 1.13 2018/09/26 22:10:22 revo Exp $">
<kap>
  <rad>Kristofor</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kristofor.0o">
  <kap><tld/>o</kap>
  <snc mrk="kristofor.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        apud la tablo, kovrita de geografiaj kartoj, sidas en profunda
        meditado la granda admiralo de Hispanujo, <tld/>o Kolumbo
        <fnt><bib>FK</bib>, <lok>4, rakontoj, nokto, (de Eube el
        Odeso)</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_K;&c_r;&c_i;&c_s;&c_t;&c_o;&c_f;</trd>,
    <trd>&c_K;&c_r;&c_i;&c_s;&c_t;&c_o;&c_f;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>Kristof</trd>,
    <trd>Kristol</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>C. Colombo</trd>,
    <trd>Kolumbus</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Christoph</trd>,
    <trd>Christopher</trd>
  </trdgrp>
  <trd lng="fr">Christophe</trd>
  <trd lng="hu">Krist&oacute;f</trd>
  <trd lng="it">Cristoforo</trd>
  <trdgrp lng="nl">
    <trd>Christoffel</trd>,
    <trd>Kristoffel</trd>
  </trdgrp>
  <trd lng="pl">Krzysztof</trd>
  <trd lng="pt">Crist&oacute;v&atilde;o</trd>
  <trd lng="ro">Cristofor</trd>
  <trdgrp lng="ru">
    <trd>&c_H;&c_r;&c_i;&c_s;&c_t;&c_o;&c_f;&c_o;&c_r;</trd>,
    <trd>&c_K;&c_r;&c_i;&c_s;&c_t;&c_o;&c_f;&c_o;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>C. Colombo</trd>,
    <trd>Kolumbus</trd>
  </trdgrp>
</drv>
</art>

