<drv mrk="svat.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kies profesio a&ubreve; okupo estas proponi aliulojn por
      edzi&gcirc;o:
      <ekz>
        la oficiala <ind><tld/>istino</ind> venis kun horoskopaj kartoj
        <fnt>
          <bib>RugxDom</bib>,
          <lok>&ccirc;apitro 72a, p. 530a</lok>
        </fnt>;
        <trdgrp lng="it">
          <trd>mezzana</trd>, 
          <trd>ruffiana</trd>
         </trdgrp>
      </ekz>
      <ekz>
        kiel &leftquot;eminenta svatistino&rightquot; estas nomumita <nom>Liu Jinying</nom> de la urbestro
        <fnt>
          
          <vrk><url ref="&Monato;/1993/A930902.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/09, p. 4</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_a;&c_h;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>dohazova&ccaron;</trd>,
    <trd>dru&zcaron;ba</trd>,
    <trd>zprost&rcaron;edkovatel man&zcaron;elstv&iacute;</trd>,
    <trd>zprost&rcaron;edkovatel s&ncaron;atk&uring;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Heiratsvermittler</trd>,
    <trd>Kuppler</trd>
  </trdgrp>
  <trd lng="fa">&a_d;&a_l;&a_A;&a_l;
  &a_A;&a_z;&a_d;&a_w;&a_A;&a_j;</trd>
  <trdgrp lng="fr">
    <trd>marieur</trd>,
    <trd>marieuse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&aacute;zass&aacute;gszerz&odblac;</trd>,
    <trd>h&aacute;zass&aacute;gk&ouml;zvet&iacute;t&odblac;</trd>,
    <trd>t&aacute;rskeres&odblac;</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>mezzano</trd>, 
    <trd>ruffiano</trd>
   </trdgrp>
  <trd lng="ja">&#x7D50;&#x5A5A;&#x4EF2;&#x4ECB;&#x696D;&#x8005;  [<ind>&#x3051;&#x3063;&#x3053;&#x3093;&#x3061;&#x3085;&#x3046;&#x304B;&#x3044;&#x304E;&#x3087;&#x3046;&#x3057;&#x3083;</ind>]</trd>
  <trdgrp lng="nl">
    <trd>huwelijksmakelaar</trd>,
    <trd>koppelaar</trd>
  </trdgrp>
  <trd lng="pl">swat</trd>
  <trd lng="ru">&c_s;&c_v;&c_a;&c_h;&c_a;</trd>
  <trd lng="sk">pyta&ccaron; <klr>(kto p&yacute;ta za &zcaron;enu pre druh&eacute;ho)</klr></trd>
</drv>

<drv mrk="svat.0igxi">
  <kap><tld/>i&gcirc;i, <var><kap>sin <tld/>i</kap></var></kap>
  <snc>
    <dif>
      <ref tip="dif" cel="svat.0i.mem"><tld lit="S"/>i<sncref/></ref>:
      <ekz>
        ne &ccirc;iu fian&ccirc;i&gcirc;as, kiu <tld/>i&gcirc;as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        multaj <tld/>i&gcirc;as, feli&ccirc;ulo edzi&gcirc;as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni ja &gcirc;ustatempe avertis lin, li povis ja rifuzi la <tld/>i&gcirc;adon
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        venis nova <tld/>i&gcirc;anto je la re&gcirc;idino
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_a;&c_t;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>dohazovat</trd>,
    <trd>zprost&rcaron;edkovat man&zcaron;elstv&iacute;</trd>,
    <trd>zprost&rcaron;edkov&aacute;vat svatbu</trd>
  </trdgrp>
  <trd lng="fa">&a_kh;&a_w;&a_A;&a_s;&a_t;&f_g;&a_A;&a_r;&f_y;
  &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">demander en <ind>mariage</ind></trd>
  <trd lng="hu">megk&eacute;r&odblac; a kez&eacute;t <klr tip="amb">(maga
  sz&aacute;m&aacute;ra)</klr></trd>
  <trd lng="it">proporsi<klr> (matrimonio)</klr></trd>
  <trd lng="ja">&#x6C42;&#x5A5A;&#x3059;&#x308B;  [<ind>&#x304D;&#x3085;&#x3046;&#x3053;&#x3093;&#x3059;&#x308B;</ind>]</trd>
  <trd lng="pl">o&sacute;wiadcza&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_s;&c_v;&c_a;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>dohadzova&tcaron;</trd>,
    <trd>nahov&aacute;ra&tcaron; <klr>(na man&zcaron;elstvo)</klr></trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_v;&c_a;&c_t;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
</drv>

<drv mrk="svat.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc>
    <dif>
      Formala sinpropono kiel edzo a&ubreve; edzino:
      <ekz>
        ekzistis centoj da re&gcirc;idinoj, kiuj akceptus lian <tld/>i&gcirc;on kun &gcirc;ojo
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Heiratsantrag</trd>
  <trd lng="fa">&a_kh;&a_w;&a_A;&a_s;&a_t;&f_g;&a_A;&a_r;&f_y;</trd>
  <trd lng="fr">demande en <ind>mariage</ind></trd>
  <trd lng="hu">le&aacute;nyk&eacute;r&eacute;s</trd>
  <trd lng="it">proposta<klr> di matrimonio</klr></trd>
  <trd lng="ja">&#x6C42;&#x5A5A;  [<ind>&#x304D;&#x3085;&#x3046;&#x3053;&#x3093;</ind>]</trd>
  <trd lng="pl">o&sacute;wiadczyny</trd>
</drv>
</art>
