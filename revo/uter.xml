<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: uter.xml,v 1.36 2024/06/11 23:25:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>uter</rad>/o
  <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="uter.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="uter.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      La <ref tip="super" cel="organ.0o.BIO">organo</ref> de hominoj kaj
      multaj mamulinoj, en kiu la <ref tip="vid" cel="fet.0o">feto</ref> evoluas
      dum la gravedotempo:
      <ekz>
        &ccirc;io, kio malfermas unue la <tld/>on, apartenas al Mi
        <fnt>
          <bib>MT</bib>
          <vrk>&Eli; 34:19</vrk>
        </fnt>;
      </ekz>
      <ekz>
        kial mi ne estis mortigita en la <tld/>o, ke mia patrino estu
        mia tombo kaj &scirc;ia ventro restu graveda por &ccirc;iam
        <fnt>
          <bib>MT</bib>
          <vrk>&Jer; 20:17</vrk>
        </fnt>?
      </ekz>
      <ekz>
        en &scirc;ia <tld/>o kreskas la semo plantita tie de Domicio en la sama tago, en kiu
        &scirc;ia frato estis deklarita imperiestro
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Tria</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_l;&c_o;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bg">&c_m;&c_a;&c_t;&c_k;&c_a;</trd>
  <trd lng="br">mammog</trd>
  <trd lng="cs">d&ecaron;loha</trd>
  <trdgrp lng="de">
    <trd>Geb&auml;rmutter</trd>,
    <trd>Uterus</trd>
  </trdgrp>
  <trd lng="el">&my;&eta_ton;&tau;&rho;&alfa;</trd>
  <trdgrp lng="en">
    <trd>uterus</trd>,
    <trd>womb</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>&uacute;tero</trd>,
    <trd>matriz</trd>
  </trdgrp>
  <trd lng="fi">kohtu</trd>
  <trd lng="fr">uterus</trd>
  <trdgrp lng="fy">
    <trd>liifmoer</trd>,
    <trd>limoer</trd>
  </trdgrp>
  <trd lng="hu">anyam&eacute;h</trd>
  <trd lng="io">utero</trd>
  <trd lng="it">utero</trd>
  <trd lng="ja">&#23376;&#23470;  <pr>&#12375;&#12365;&#12421;&#12358;</pr></trd>
  <trdgrp lng="nl">
    <trd>baarmoeder</trd>,
    <trd>uterus</trd>
  </trdgrp>
  <trd lng="pl">macica</trd>
  <trd lng="pt">&uacute;tero</trd>
  <trd lng="ru">&c_m;&c_a;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>maternica</trd>,
    <trd>uterus</trd>
  </trdgrp>
  <trd lng="sv">livmoder</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_t;&c_r;&c_o;&c_b;&c_a;</trd>,
    <trd>&c_l;&c_o;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="vo">v&uuml;m</trd>
  <trdgrp lng="zh">
    <trd>&#23376;&#23467; <pr>z&#464;g&omacron;ng</pr></trd>,
    <trd>&#23376;&#23470; <pr>z&#464;g&omacron;ng</pr></trd>,
    <trd>&#32990;&#23467; <pr>b&amacron;og&omacron;ng</pr></trd>,
    <trd>&#32990;&#23470; <pr>b&amacron;og&omacron;ng</pr></trd>,
    <trd>&#23064;&#32974; <pr>ni&aacute;ngt&amacron;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="uter.0a">
  <kap><tld/>a</kap>
  <snc mrk="uter.0a.ANA">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        la supera parto de la <tld/>a mukozo eliras tra la vagino, estas la menstruo
        <fnt>
          <bib>Kon11</bib>
          <lok>SANO</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_c;&c_ib;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_m;&c_a;&c_c;&c_ib;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">uter&iacute;</trd>
  <trd lng="el">&my;&eta;&tau;&rho;&jota;&kappa;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">uterine</trd>
  <trd lng="es">uterino</trd>
  <trd lng="fr">ut&eacute;rin</trd>
  <trd lng="gl">uterino</trd>
  <trd lng="hy">&#1377;&#1408;&#1379;&#1377;&#1398;&#1380;&#1377;&#1397;&#1387;&#1398;</trd>
  <trd lng="io">uterala</trd>
  <trd lng="it">uterino</trd>
  <trd lng="ja">&#23376;&#23470;&#12398;  <pr>&#12375;&#12365;&#12421;&#12358;&#12398;</pr></trd>
  <trd lng="nl">baarmoeder-</trd>
  <trd lng="pl">maciczny</trd>
  <trd lng="pt">uterino</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_t;&c_r;&c_o;&c_b;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_a;&c_t;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="uk">&c_m;&c_a;&c_t;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>
  <trd lng="vo">v&uuml;mik</trd>
</drv>

<drv mrk="uter.ekster0a">
  <kap>
    ekster<tld/>a
  </kap>
  <snc mrk="uter.ekster0a.ANA">
    <dif>
      Okazanta a&ubreve; lokata ekster la <tld/>o.
    </dif>
    <subsnc mrk="uter.ekster0a.MED">
      <uzo tip="fak">MED</uzo>
      <uzo tip="klr">(pri gravedeco)</uzo>
      <dif>
        Okazanta en ovario a&ubreve; en ovodukto anstata&ubreve; en la <tld/>o:
        <ekz>
          se &gcirc;i ne venos &gcirc;is la utero, esti&gcirc;as ekster<tld/>a gravedeco
          <fnt>
          <aut>-</aut>
          <vrk><url ref="&Viki;/Gravedeco">Gravedeco</url></vrk>
          <lok>Vikipedio, 2021-08-12</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="fr">extra-ut&eacute;rin</trd>
    </subsnc>
    <subsnc mrk="uter.ekster0a.TEK">
      <uzo tip="fak">MED</uzo>
      <uzo tip="fak">TEK</uzo>
      <uzo tip="klr">(pri fekundigo)</uzo>
      <dif>
        Okazigita per kunigo de ovo kaj spermo en arta, medicine kontrolata medio:
        <ekz>
          &ccirc;e la ekster<tld/>a fekundigo ovo&ccirc;elo fekundi&gcirc;as per spermo&ccirc;elo en laboratoria glaso
          <fnt>
          <aut>[A. Oberndorfer]</aut>
          <vrk><url
          ref="http://esperantaretradio.blogspot.com/2013/07/eksterutera-fekundigo-je-reduktitaj.html">Eksterutera
          fekundigo je reduktitaj kostoj</url></vrk>
          <lok>Esperanta Retradio, 2013-07-22</lok>
          </fnt>.
        </ekz>
      </dif>
      <trd lng="fr">in vitro</trd>
    </subsnc>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_a;&c_m;&c_a;&c_c;&c_ib;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ja">&#23376;&#23470;&#22806;&#12398;  <pr>&#12375;&#12365;&#12421;&#12358;&#12364;&#12356;&#12398;</pr></trd>
  <trd lng="pl">pozamaciczny</trd>
  <trd lng="uk">&c_p;&c_o;&c_z;&c_a;&c_m;&c_a;&c_t;&c_k;&c_o;&c_v;&c_i;&c_j;</trd>
</drv>

<drv mrk="uter.0kolo">
  <kap><tld/>kolo, <var><kap><tld/>a kolo</kap></var></kap>
  <snc mrk="uter.0kolo.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      La malsupra mallar&gcirc;a parto de la <ref tip="malprt" cel="uter.0o">utero</ref>, kie &gcirc;i ali&gcirc;as al
      la supra fina parto de la <ref tip="vid" cel="vagin.0o">vagino</ref>:
      <ekz>
        la <tld/>a kolo estas plenigata de tre densa muko, kiu funkcias kiel efika
        &scirc;topilo
        <fnt>
          <bib>Kon11</bib>
          <lok>Sano</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_j;&c_k;&c_a; &c_m;&c_a;&c_c;&c_ib;&c_c;&c_y;</trd>
  <trd lng="ar">&a_ayn;&a_n;&a_q; &a_A;&a_l;&a_r;&a_H;&a_m;</trd>
  <trd lng="ca">c&egrave;rvix</trd>
  <trd lng="cs">d&ecaron;lo&zcaron;n&iacute; &ccaron;&iacute;pek</trd>
  <trdgrp lng="de">
    <trd>Cervix uteri</trd>,
    <trd>Cervix</trd>,
    <trd>Zervix</trd>
  </trdgrp>
  <trd lng="el">&tau;&rho;&alfa_ton;&chi;&eta;&lambda;&omikron;&sigma_fina;</trd>
  <trd lng="en">cervix</trd>
  <trd lng="es">c&eacute;rvix</trd>
  <trd lng="fi">kohdunkaula</trd>
  <trd lng="fo">l&iacute;vm&oacute;&#240;urh&aacute;lsur</trd>
  <trd lng="fr">col de l'ut&eacute;rus</trd>
  <trd lng="fy">limoerhals</trd>
  <trd lng="ga">ceirbheacs</trd>
  <trd lng="hi">&#2327;&#2381;&#2352;&#2368;&#2357;&#2366; <pr>gr&imacron;v&amacron;</pr></trd>
  <trd lng="hu">m&eacute;hnyak</trd>
  <trd lng="hy">&#1377;&#1408;&#1379;&#1377;&#1398;&#1380;&#1387; &#1406;&#1382;&#1387;&#1391;</trd>
  <trdgrp lng="it">
    <trd>cervice</trd>,
    <trd>collo dell'utero</trd>
  </trdgrp>
  <trd lng="ja">&#23376;&#23470;&#38968;&#37096; <pr>&#12375;&#12365;&#12421;&#12358;&#12369;&#12356;&#12406;</pr></trd>
  <trd lng="lv">dzemdes kakls</trd>
  <trd lng="mi">waha k&omacron;p&umacron;</trd>
  <trd lng="mn">&c_u;&c_m;&c_a;&c_j;&c_n; &c_h;&#1199;&c_z;&#1199;&#1199;</trd>
  <trdgrp lng="mr">
    <trd>&#2351;&#2379;&#2344;&#2367;&#2350;&#2366;&#2352;&#2381;&#2327;</trd>,
    <trd>&#2327;&#2381;&#2352;&#2368;&#2357;&#2366;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>baarmoederhals</trd>,
    <trd>cervix</trd>
  </trdgrp>
  <trd lng="no">livmorhals</trd>
  <trd lng="pl">szyjka<ind>  macicy</ind></trd>
  <trdgrp lng="pt">
    <trd>c&eacute;rvice</trd>,
    <trd>c&eacute;rvix</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_je;&c_j;&c_k;&c_a; &c_m;&c_a;&c_t;&c_k;&c_i;</trd>
  <trd lng="sl">materni&ccaron;ni vrat</trd>
  <trd lng="sv">livmoderhals</trd>
  <trd lng="tl">liig-liigan</trd>
  <trd lng="tr">serviks</trd>
  <trdgrp lng="zh">
    <trd>&#23467;&#39048; <pr>g&omacron;ngj&#464;ng</pr></trd>,
    <trd>&#23470;&#38968; <pr>g&omacron;ngj&#464;ng</pr></trd>,
    <trd>&#23376;&#23467;&#39048; <pr>z&#464;g&omacron;ngj&#464;ng</pr></trd>,
    <trd>&#23376;&#23470;&#38968; <pr>z&#464;g&omacron;ngj&#464;ng</pr></trd>
  </trdgrp>
  <adm>
    Okaze &scirc;ovu al "kol/".
  </adm>
</drv>

<drv mrk="uter.0kola">
  <kap><tld/>kola</kap>
  <snc mrk="uter.0kola.ANA">
    <dif>
      Rilata al <tld/>kolo.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_e;&c_r;&c_v;&c_ib;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="en">cervical</trd>
  <trd lng="es">cervical</trd>
  <trd lng="fr">cervical</trd>
  <trd lng="ga">ceirbheacsach</trd>
  <trd lng="hu">m&eacute;hnyaki</trd>
  <trd lng="it">cervicale</trd>
  <trd lng="nl">baarmoederhals-</trd>
  <trd lng="pl">szyjki<ind>  macicy</ind></trd>
  <trd lng="pt">cervical</trd>
  <trd lng="ru">&c_c;&c_je;&c_r;&c_v;&c_i;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
