<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fint.xml,v 1.10 2025/08/01 05:58:29 revo Exp $">
<kap>
  <rad>fint</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fint.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="fint.0i.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="vid" cel="tromp.0i.KOMUNE">Trompe</ref>
      &scirc;ajnigi iun movon kaj fari alian:
      <ekz>
        s<klr>injor</klr>ino <nom>Vanderlieb</nom> <tld/>is lian avidan bu&scirc;on
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>,
        <lok>1982</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;io koncentri&gcirc;as je imago pri ajna neekzistanta iulo, kun kiu vi povas amindume
        <tld/>i, intime babilzumi kaj dol&ccirc;avo&ccirc;e flustri
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Konfeso de la Potifar-edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iutage aperas &ccirc;inaj &scirc;ipoj en la disputata maro, dum kontra&ubreve;e <tld/>as japanaj
        &scirc;ipoj
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2013/011128.php"
              >Varmaj vortoj, malvarmaj rilatoj</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="artifi.0i">artifiki</ref>,
      <ref cel="bluf.0i">blufi</ref>,
      <ref cel="mistifik.0i">mistifiki</ref>
    </refgrp>
  </snc>
  <rim mrk="fint.0i.RIM_GRA">
    La&ubreve; PIV la verbo estas netransiga.
  </rim>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_f;&c_ib;&c_n;&c_t;</ind></trd>
  <trdgrp lng="de">
    <trd>t&auml;uschen</trd>,
    <trd>tricksen</trd>
  </trdgrp>
  <trd lng="en">feint</trd>
  <trd lng="fr">feinter</trd>
  <trdgrp lng="ja">
    <trd>&#x727D;&#x5236;&#x3059;&#x308B;  <pr>&#x3051;&#x3093;&#x305B;&#x3044;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x30D5;&#x30A7;&#x30A4;&#x30F3;&#x30C8;&#x3092;&#x304B;&#x3051;&#x308B;
    <pr>&#x3075;&#x3047;&#x3044;&#x3093;&#x3068;&#x3092;&#x304B;&#x3051;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>stosowa&cacute; <ind>fint&eogonek;</ind></trd>,
    <trd>stosowa&cacute; <ind>wybieg</ind></trd>,
    <trd>wykona&cacute; <ind>ruch pozorowany</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>oklama&tcaron;</trd>,
    <trd>pou&zcaron;i&tcaron; fintu</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obelst&iacute;t</trd>,
    <trd>pou&zcaron;&iacute;t finty</trd>
  </trdgrp>
  <trd lng="uk">&c_f;&c_ib;&c_n;&c_t;&c_i;&c_t;&c_i;</trd>
</drv>

<drv mrk="fint.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Trompa <ref tip="super" cel="mov.0o">movo</ref>:
      <ekz>
        la ta&ubreve;ro <klr>[&#x2026;]</klr> krakege frapas kontra&ubreve; la ru&gcirc;linian traba&jcirc;on, incitistoj
        klopodas trompi &gcirc;in per tukmovaj <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Eduardo Novembro</aut>
          <vrk><url ref="&Monato;/2014/011318.php"
              >Ta&ubreve;ro??? Festo!!!</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="artifi.0o">artifiko</ref>,
      <ref cel="mistifik.0o">mistifiko</ref>,
      <ref cel="truk.0o">truko</ref>
    </refgrp>
  </snc>
  <trd lng="de">Finte</trd>
</drv>

</art>

