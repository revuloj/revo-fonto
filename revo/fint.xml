<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fint.xml,v 1.6 2023/08/31 13:11:29 revo Exp $">
<kap>
  <rad>fint</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="fint.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="fint.0i.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="vid" cel="tromp.0i.KOMUNE">Trompe</ref>
      &scirc;ajnigi iun movon kaj fari alian:
      <ekz>
        s<klr>injor</klr>ino Vanderlieb <tld/>is lian avidan bu&scirc;on
        <fnt><aut>J. Francis</aut>, <vrk>Misio sen Alveno</vrk>, 
        <lok>1982</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="fint.0i.RIM_GRA">
    La&ubreve; PIV la verbo estas netransiga.
  </rim>
  <trd lng="en">feint</trd>
  <trd lng="fr">feinter</trd>
  <trdgrp lng="ja">
    <trd>&#x727D;&#x5236;&#x3059;&#x308B;  [<ind>&#x3051;&#x3093;&#x305B;&#x3044;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x30D5;&#x30A7;&#x30A4;&#x30F3;&#x30C8;&#x3092;&#x304B;&#x3051;&#x308B;  [<ind>&#x3075;&#x3047;&#x3044;&#x3093;&#x3068;&#x3092;&#x304B;&#x3051;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>stosowa&cacute; <ind>fint&eogonek;</ind></trd>,
    <trd>stosowa&cacute; <ind>wybieg</ind></trd>,
    <trd>wykona&cacute; <ind>ruch pozorowany</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>oklama&tcaron;</trd>,
    <trd>pou&zcaron;i&tcaron; fintu</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>obelst&iacute;t</trd>,
    <trd>pou&zcaron;&iacute;t finty</trd>
  </trdgrp>
  <trd lng="uk">&#x444;&#x456;&#x43D;&#x442;&#x438;&#x442;&#x438;</trd>
</drv>
</art>

