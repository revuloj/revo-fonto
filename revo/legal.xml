<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>legal</rad>/a <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="legal.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="legal.0a.JUR">
  <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="dif" cel="legx.laux0a">La&ubreve;le&gcirc;a</ref>:
      <ekz>
        &gcirc;i estas tre respektinda kaj tute <tld/>a asocio, ne akcelanta
        renverson de la ekzistanta re&gcirc;imo
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
      <ekz>
        la ondoj de la <tld/>a kaj ne<tld/>a enmigrado <klr>[&#8230;]</klr> alportis novajn
        Esperantistojn
        <fnt>
          <vrk><url ref="https://www.esperanto.org.il/historio.html"
              >Historio de la Esperanto-movado en Israelo - Esperanto-Ligo ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- ne traduku evitindajn...
  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">legal</trd>
  <trd lng="en">legal</trd>
  <trd lng="nl">legaal<klr tip="ind"> (ontr)</klr></trd>
  <trd lng="pt">legal<klr tip="amb"> (jur.)</klr></trd>
-->
</drv>

<drv mrk="legal.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="legal.0eco.JUR">
  <uzo tip="fak">JUR</uzo>
    <dif>
      La&ubreve;le&gcirc;eco, le&gcirc;konformo:
      <ekz>
        <ref cel="legxitim.0a">le&gcirc;itim</ref>eco pli valoras ol <tld/>eco
        <fnt>
          <vrk><url ref="https://labekbotelo.blogspot.com/2011/05/kio-estas-catalanrevolution.html"
              >Survoje al Kataluna Respubliko: Kio estas #catalanrevolution?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- ne traduku evitindajn...
  <trdgrp lng="be">
    <trd>&c_l;&c_je;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Legalit&auml;t</trd>
  <trd lng="en">legality</trd>
  <trd lng="nl">legaliteit<klr tip="ind"> (ontr.)</klr></trd>
-->
</drv>

<drv mrk="legal.mal0a">
  <kap>mal<tld/>a</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="legal.mal0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="dif" cel="legx.kontraux0a">Kontra&ubreve;le&gcirc;a</ref>:
      <ekz>
        oni diras, ili mal<tld/>an literaturon disvastigas
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
  </snc>
<!-- ne traduku evitindajn...
  <trdgrp lng="be">
    <trd>&c_n;&c_je;&c_l;&c_je;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_a;&c_k;&c_o;&c_n;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">illegal</trd>
  <trd lng="en">illegal</trd>
  <trd lng="nl">illegaal<klr tip="ind"> (ontr.)</klr></trd>
-->
</drv>
</art>

