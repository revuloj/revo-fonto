<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>legal</rad>/a <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="legal.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="legal.0a.JUR">
  <uzo tip="fak">JUR</uzo>
    <ref tip="dif" cel="legx.laux0a">la&ubreve;le&gcirc;a</ref>
    <ekz>
      &gcirc;i estas tre respektinda kaj tute <tld/>a asocio, ne akcelanta
      renverson de la ekzistanta re&gcirc;imo
      <fnt><bib>Metrop</bib></fnt>!
    </ekz>
  </snc>
</drv>

<drv mrk="legal.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="legal.0eco.JUR">
    <dif>
      La&ubreve;le&gcirc;eco, le&gcirc;konformo.
    </dif>
  </snc>
</drv>

<drv mrk="legal.mal0a">
  <kap>mal<tld/>a</kap>
  <uzo tip="stl">EVI</uzo>
  <snc mrk="legal.mal0a.JUR">
    <ref tip="dif" cel="legx.kontraux0a">kontra&ubreve;le&gcirc;a</ref>
    <ekz>
      oni diras, ili mal<tld/>an literaturon disvastigas
      <fnt><bib>Metrop</bib></fnt>.
    </ekz>
  </snc>
</drv>
</art>

<!--
$Log$
