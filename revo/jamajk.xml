<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jamajk.xml,v 1.16 2024/10/25 09:30:52 revo Exp $">
<kap><ofc>10</ofc>
  <rad>Jamajk</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="jamajk.0o">
  <kap>
    <ofc>10</ofc><tld/>o
  </kap>
  <snc mrk="jamajk.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="insul.0o" lst="voko:insuloj">Insulo</ref>
      en Centra Ameriko en <ref tip="malprt" cel="mar.0o">Kariba
      Maro</ref> apartenanta al <ref tip="vid" cel="jamajk.0o.sxtato">&scirc;tato
      <tld/>o</ref>:
      <ekz>
        dum tiu eksterlanda okupado, tra la teritorio diversaj kanabokampoj estis
        kulturitaj, &ccirc;efe en <tld/>o, elektita kiel fibroproduktejo por la brita
        imperio
        <fnt>
          <bib>Kon11</bib>
          <lok>&Ccirc;efartikolo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>Jamajka</trd>
    </trdgrp>
  </snc>
  <snc mrk="jamajk.0o.sxtato">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg kod="ISO-3166">JM</mlg>
    <dif>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&Scirc;tato</ref>
      en la samnoma insulo kun &ccirc;efurbo Kingstono:
      <ekz>
        <tld/>o estas la trie plej lo&gcirc;ata angloparolanta lando en Ameriko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1988">Jamajko</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        koncerto por paci&gcirc;o inter la du politikaj partioj de <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=581">Bob Marley</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>Jamajka</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_Ja;&c_m;&c_a;&c_j;&c_k;&c_a;</trd>
  <trd lng="bg">&c_Ja;&c_m;&c_a;&c_j;&c_k;&c_a;</trd>
  <trd lng="ca">Jamaica</trd>
  <trd lng="de">Jamaika</trd>
  <trd lng="en">Jamaica</trd>
  <trd lng="fr">Jama&iuml;que</trd>
  <trd lng="id">Jamaika</trd>
  <trd lng="ja">&#12472;&#12515;&#12510;&#12452;&#12459;</trd>
  <trd lng="nl">Jamaica</trd>
  <trd lng="pt">Jamaica</trd>
  <trd lng="uk">&c_Ja;&c_m;&c_a;&c_j;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#29273;&#20080;&#21152; <pr>Y&aacute;m&#462;iji&amacron;</pr></trd>,
    <trd>&#29273;&#20080;&#21152; <pr>y&aacute;m&#462;iji&amacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="jamajk.0a">
  <kap>
    <ofc>10</ofc><tld lit="j"/>a
  </kap>
  <snc>
    <dif>
      Rilata al <tld/>o, la <tld lit="j"/>anoj ktp.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_m;&c_a;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">jamaikanisch</trd>
</drv>

<drv mrk="jamajk.0ano">
  <kap>
    <ofc>10</ofc><tld lit="j"/>ano
  </kap>
  <snc>
    <dif>
      Iu el <tld/>o:
      <ekz>
        la rekordo estis rompita en la Olimpikoj en Pekino, <klr>[&#8230;]</klr> la <tld
        lit="j"/>ano <nom>Usain St. Leo Bolt</nom> atingis tempon de 9,69 sekundoj
        <fnt>
          <bib>Kon11</bib>
          <lok>Olimpikoj: kolekti&gcirc;as pacaj batalantoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_m;&c_a;&c_je;&c_c;</trd>
  <trd lng="de">Jamaikaner</trd>
</drv>

</art>

