<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lien</rad>/o
</kap>
<drv mrk="lien.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Granda mola kaj spongeca glando, &ccirc;e maldekstra flanko de ventro, kie
      formi&gcirc;as kaj detrui&gcirc;as la ru&gcirc;aj globetoj de sango:
      <ekz>
        &ccirc;iu el ni posedas cerbon, koron, <tld/>on, pulmojn de la sama
        strukturo
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 16a, p. 86a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_je;&c_l;&c_ja;&c_z;&c_jo;&c_n;&c_k;&c_a;</trd>
  <trd lng="ca">melsa</trd>
  <trd lng="de">Milz</trd>
  <trd lng="el">&sigma;&pi;&lambda;&eta_ton;&ny;&alfa;</trd>
  <trdgrp lng="en">
    <trd>spleen</trd>,
    <trd>milt</trd>
  </trdgrp>
  <trd lng="es">bazo</trd>
  <trdgrp lng="fr">
    <trd>rate</trd>
  </trdgrp>
  <trd lng="hu">l&eacute;p</trd>
  <trd lng="nl">milt</trd>
  <trd lng="pt">ba&ccedil;o</trd>
  <trd lng="ru">&c_s;&c_je;&c_l;&c_je;&c_z;&c_jo;&c_n;&c_k;&c_a;</trd>
  <trd lng="sv">mj&auml;lte</trd>
</drv>
</art>
