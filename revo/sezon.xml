<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sezon</rad>/o
</kap>
<drv mrk="sezon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sezon.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="jar.0o.KAL">jaro</ref>
      karakterizata per sia vetero, naturaj kondi&ccirc;oj, homaj okupoj:
    </dif>
    <subsnc mrk="sezon.0o.jarkvarono">
      <dif>
        En mezvarmaj landoj &ccirc;iu el la kvar partoj de jaro kun tipa
        temperaturo, nome
        <ref tip="vid" cel="printe.0o.KAL">printempo</ref> (varmi&gcirc;o),
        <ref tip="vid" cel="somer.0o.KAL">somero</ref> (varmo),
        <ref tip="vid" cel="auxtun.0o.KAL">a&ubreve;tuno</ref> (malvarmi&gcirc;o),
        <ref tip="vid" cel="vintr.0o.KAL">vintro</ref> (malvarmo).
        <ekz>
          oni havis ekster<tld/>an someron en tiu &ccirc;i jaro.
        </ekz>
      </dif>
      <trd lng="be">&c_p;&c_a;&c_r;&c_a; &c_g;&c_o;&c_d;&c_u;</trd>
      <trd lng="ca">estaci&oacute;<klr> (de l'any)</klr></trd>
      <trd lng="de">Jahreszeit</trd>
      <trd lng="es">estaci&oacute;n<klr> (del a&ntilde;o)</klr></trd>
      <trd lng="hu">&eacute;vszak</trd>
      <trd lng="ru">&c_v;&c_r;&c_je;&c_m;&c_ja; &c_g;&c_o;&c_d;&c_a;</trd>
    </subsnc>
    <subsnc mrk="sezon.0o.okupo">
      <dif>
        Jarparto speciale ta&ubreve;ga por ia faro:
        <ekz>
          la fi&scirc;<tld/>o proksimi&gcirc;is
          <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>blindaj &ccirc;uk&ccirc;oj, (du skizoj), unua
          skizo:, Filip Onkudimov, 4</lok></fnt>;
        </ekz>
        <ekz>
          ban<tld/>o<fnt>B</fnt>;
        </ekz>
        <ekz>
          por &ccirc;io estas <tld/>o
          <klr>(ta&ubreve;ga tempo)</klr>
          <fnt><bib>MT</bib>, <lok>&Pre; 3:1</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="be">&c_s;&c_e;&c_z;&c_o;&c_n;</trd>
      <trd lng="de">Saison</trd>
      <trdgrp lng="hu">
        <trd>id&eacute;ny</trd>,
        <trd>szezon</trd>
      </trdgrp>
      <trd lng="ru">&c_s;&c_je;&c_z;&c_o;&c_n;</trd>
    </subsnc>
  </snc>
  <trd lng="cs">ro&ccaron;n&iacute; doba</trd>
  <trd lng="en">season</trd>
  <trd lng="fr">saison</trd>
  <trd lng="tr">mevsim</trd>
  <trdgrp lng="nl">
    <trd>seizoen</trd>,
    <trd>jaargetijde</trd>
  </trdgrp>
</drv>

<drv mrk="sezon.fru0o">
  <kap>fru<tld/>o, <var><kap>frua <tld/>o</kap></var></kap>
  <snc mrk="sezon.fru0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <ref tip="dif" cel="printe.0o.KAL">printempo</ref>
    <ekz>
      ni timu la Eternulon, nian Dion, kiu donas al ni pluvon fru<tld/>an
      <fnt><bib>MT</bib>, <lok>&Jer; 5:24</lok></fnt>;
    </ekz>
    <ekz>
      estis frua <tld/>o, kelkaj <klr>[birdoj]</klr> havis ovojn, aliaj
      havis jam idojn
      <fnt><bib>Fab4</bib>, <lok>la vento rakontas pri Valdemaro Doe kaj
      pri liaj filinoj</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_v;&c_ja;&c_s;&c_n;&c_a;</trd>
  <trd lng="cs">jaro</trd>
  <trdgrp lng="de">
    <trd>Fr&uuml;hling</trd>,
    <trd>Fr&uuml;hjahr</trd>,
    <trd>Lenz<klr> (poezie)</klr></trd>
  </trdgrp>
  <trd lng="en">spring</trd>
  <trd lng="hu">tavasz</trd>
  <trdgrp lng="nl">
    <trd>lente</trd>,
    <trd>voorjaar</trd>
  </trdgrp>
  <trd lng="pl">wiosna</trd>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_n;&c_a;</trd>
  <trd lng="sv">v&aring;r</trd>
  <trd lng="vo">flolatim</trd>
</drv>

<drv mrk="sezon.malfru0o">
  <kap>malfru<tld/>o, <var><kap>malfrua <tld/>o</kap></var></kap>
  <snc mrk="sezon.malfru0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <ref tip="dif" cel="auxtun.0o.KAL">a&ubreve;tuno</ref>
    <ekz>
      la tritiko kaj la spelto ne estis batdifektitaj, &ccirc;ar ili estis
      malfru<tld/>aj
      <fnt><bib>MT</bib>, <lok>&Eli; 9:32</lok></fnt>;
    </ekz>
    <ekz>
      estis malfrua <tld/>o, la ru&gcirc;koloraj vinberaj folioj velke pendis
      malsupren, ver&scirc;i&gcirc;is abunda pluvo
      <fnt><bib>Fab4</bib>, <lok>malnova pre&gcirc;eja sonorilo, verkita por la
      Schiller'a albumo</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_s;&c_je;&c_n;&c_mol;</trd>
  <trd lng="br">diskar-amzer</trd>
  <trd lng="cs">podzim</trd>
  <trdgrp lng="de">
    <trd>Herbst</trd>,
    <trd>Sp&auml;tjahr [sudgermane]</trd>
  </trdgrp>
  <trd lng="ca">tardor</trd>
  <trd lng="en">autumn</trd>
  <trd lng="es">oto&ntilde;o</trd>
  <trd lng="fr">automne</trd>
  <trd lng="he">&samekh;&tav;&yod;&vav;</trd>
  <trd lng="hu">&odblac;sz</trd>
  <trd lng="nl">herfst</trd>
  <trd lng="pl">jesie&nacute;</trd>
  <trd lng="pt">outono</trd>
  <trd lng="ru">&c_o;&c_s;&c_je;&c_n;&c_mol;</trd>
  <trd lng="sk">jese&ncaron;</trd>
  <trd lng="sv">h&ouml;st</trd>
  <trd lng="tr">sonbahar</trd>
  <trd lng="vo">flukatim</trd>
</drv>
</art>
