<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prepos.xml,v 1.14 2024/02/12 21:20:51 revo Exp $">
<kap>
  <rad>prepost</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="prepos.0o">
  <kap><tld/>o</kap>
  <snc mrk="prepos.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      &Ccirc;efa <ref tip="super" cel="pastr.0o">pastro</ref> de pre&gcirc;ejo,
      la&ubreve;range sub episkopo:
      <ekz>
        la patro estis promociita kaj fari&gcirc;is <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Peco da perlovico</lok>
        </fnt>;
      </ekz>
      <ekz>
        proksime de la urbo <nom>Viborg</nom> staris la nova eleganta domo de la <tld/>o el ru&gcirc;aj
        &scirc;tonoj kaj kun dentita frontono
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        kanonetojn havas pastr&#8217; en <ind><tld/>ejo</ind>
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VI.</lok>
        </fnt>.
        <trd lng="de">Propstei</trd>
      </ekz>
      <ekz>
        &scirc;ia edzo estas &ccirc;arma homo, li nun estas milita <tld/>o en Jalto
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 42a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="parohx">paro&hcirc;estro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_o;&c_b;&c_a;&c_sh;&c_ch;</trd>
  <trdgrp lng="de">
    <trd>Propst</trd>,
    <trd>Pr&auml;positus</trd>
  </trdgrp>
  <trd lng="en">provost <klr>(rel.)</klr></trd>
  <trd lng="fr">pr&eacute;v&ocirc;t <klr>(religion)</klr></trd>
  <trd lng="hu">pr&eacute;post</trd>
  <trd lng="ja">&#20027;&#20219;&#21496;&#31085;  <pr>&#12375;&#12421;&#12395;&#12435;&#12375;&#12373;&#12356;</pr></trd>
  <trd lng="nl">pastoor</trd>
  <trdgrp lng="pt">
    <trd>p&aacute;roco</trd>,
    <trd>vig&aacute;rio</trd>,
    <trd>padre-cura</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_h;&c_o;&c_d;&c_s;&c_k;&c_i;&c_j; &c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_a;&c_r;&c_o;&c_h;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_v;&c_o;&c_s;&c_t; (&c_p;&c_o;&c_s;&c_a;&c_d;&c_a;)</trd>,
    <trd>&c_s;&c_u;&c_d;&c_d;&c_ja;</trd>,
    <trd>&c_o;&c_f;&c_ib;&c_c;&c_je;&c_r; &c_p;&c_o;&c_l;&c_mol;&c_o;&c_v;&c_o;&c_ji; &c_zh;&c_a;&c_n;&c_d;&c_a;&c_r;&c_m;&c_je;&c_r;&c_ib;&c_ji;</trd>,
    <trd>&c_m;&c_je;&c_r; &c_m;&c_ib;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
</drv>
</art>
