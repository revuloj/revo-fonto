<art mrk="$Id: prepos.xml,v 1.11 2023/09/03 12:45:11 revo Exp $">
<kap>
  <rad>prepost</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="prepos.0o">
  <kap><tld/>o</kap>
  <snc mrk="prepos.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      &Ccirc;efa pastro de pre&gcirc;ejo:
      <ekz>
        &scirc;ia edzo estas &ccirc;arma homo! Li nun estas milita <tld/>o en Jalto 
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen titolo</vrk>, 
        <lok>&ccirc;apitro 42a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="parohx">paro&hcirc;estro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_o;&c_b;&c_a;&c_sh;&c_ch;</trd>
  <trd lng="fr">cur&eacute;</trd>
  <trd lng="hu">pr&eacute;post</trd>  
  <trd lng="ja">&#x4E3B;&#x4EFB;&#x53F8;&#x796D;  [<ind>&#x3057;&#x3085;&#x306B;&#x3093;&#x3057;&#x3055;&#x3044;</ind>]</trd>
  <trd lng="nl">pastoor</trd>
  <trdgrp lng="pt">
    <trd>p&aacute;roco</trd>,
    <trd>vig&aacute;rio</trd>,
    <trd>padre-cura</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_h;&c_o;&c_d;&c_s;&c_k;&c_i;&c_j; &c_s;&c_v;&c_ja;&c_shch;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x430;&#x440;&#x43E;&#x445;</trd>,
    <trd>&#x43F;&#x440;&#x435;&#x432;&#x43E;</trd>,
    <trd>&#x43F;&#x440;&#x435;&#x432;&#x43E;&#x441;&#x442; (&#x43F;&#x43E;&#x441;&#x430;&#x434;&#x430;)</trd>,
    <trd>&#x441;&#x443;&#x434;&#x434;&#x44F;</trd>,
    <trd>&#x43E;&#x444;&#x456;&#x446;&#x435;&#x440; &#x43F;&#x43E;&#x43B;&#x44C;&#x43E;&#x432;&#x43E;&#x457; &#x436;&#x430;&#x43D;&#x434;&#x430;&#x440;&#x43C;&#x435;&#x440;&#x456;&#x457;</trd>,
    <trd>&#x43C;&#x435;&#x440; &#x43C;&#x456;&#x441;&#x442;&#x430;</trd>
  </trdgrp>
</drv>
