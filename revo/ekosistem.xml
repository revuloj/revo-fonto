<kap>
  <rad>ekosistem</rad>/o <fnt><bib>GDFE</bib></fnt>
</kap>
<drv mrk="ekosistem.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekosistem.0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="malprt" cel="natur.0o">Natur</ref>a 
      viv<ref tip="super" cel="sistem.0o">sistemo</ref> a&ubreve; speco de vivsistemo konsistanta el
      tuto de viva&jcirc;oj kaj ilia <ref cel="medi.0o">medio</ref>:
      <ekz>
        <tld/>oj oceanaj
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;ekologio
        (scienco)&rightquot;, 2007-05-28</lok></fnt>;
      </ekz>
      <ekz>
        tiu bone funkcianta <tld/>o sur la ha&ubreve;to nun estas
        minacata de la teksa&jcirc;industrio
        <fnt>
          <aut>A. Oberndorfer</aut>,
          <vrk>
        <url ref="http://members.aon.at/aldone/duling/trohigieno.html">
          &Ccirc;u tro da higieno?</url></vrk>,
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="biotop.0o">biotopo</ref>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_a;&c_s;&c_y;&c_s;&c_t;&c_e;&c_m;&c_a;</trd>
  <trd lng="cs">ekosyst&eacute;m</trd>
  <trd lng="de">&Ouml;kosystem</trd>
  <trd lng="fr">&eacute;cosyst&egrave;me</trd>
  <trd lng="ja">&#x751F;&#x614B;&#x7CFB;  [<ind>&#x305B;&#x3044;&#x305F;&#x3044;&#x3051;&#x3044;</ind>]</trd>
  <trd lng="pl">ekosystem</trd>
  <trd lng="pt">ecossistema</trd>
  <trd lng="sk">ekosyst&eacute;m</trd>
  <trd lng="uk">&#x435;&#x43A;&#x43E;&#x441;&#x438;&#x441;&#x442;&#x435;&#x43C;&#x430;</trd>
</drv>
</art>

