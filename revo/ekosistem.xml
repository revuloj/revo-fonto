<art mrk="$Id: ekosistem.xml,v 1.13 2025/09/17 20:31:46 revo Exp $">
<kap>
  <rad>ekosistem</rad>/o <fnt><bib>GDFE</bib></fnt>
</kap>
<drv mrk="ekosistem.0o">
  <kap><tld/>o</kap>
  <snc mrk="ekosistem.0o.EKOL">
    <uzo tip="fak">EKOL</uzo>
    <dif>
      <ref tip="dif" cel="sistem.ekologia0o.EKOL">Ekologia sistemo</ref>:
      <ekz>
        <tld/>oj oceanaj
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;ekologio
        (scienco)&rightquot;, 2007-05-28</lok></fnt>;
      </ekz>
      <ekz>
        la <tld/>o konsistas precipe el malaltaj arboj kaj arbustoj, kiuj havas mekanismojn por
        superi la severan klimaton
        <fnt>
          <aut>S. C. Praxedes</aut>
          <vrk>Biomoj</vrk>
          <lok>Revuo Esperanto, 2012-2018</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="biotop.0o">biotopo</ref>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_a;&c_s;&c_y;&c_s;&c_t;&c_e;&c_m;&c_a;</trd>
  <trd lng="cs">ekosyst&eacute;m</trd>
  <trd lng="de">&Ouml;kosystem</trd>
  <trd lng="fr">&eacute;cosyst&egrave;me</trd>
  <trd lng="ja">&#x751F;&#x614B;&#x7CFB;  <pr>&#x305B;&#12356;&#x305F;&#12356;&#12369;&#12356;</pr></trd>
  <trd lng="pl">ekosystem</trd>
  <trd lng="pt">ecossistema</trd>
  <trd lng="sk">ekosyst&eacute;m</trd>
  <trd lng="uk">&c_je;&#x43A;&#x43E;&c_s;&c_i;&c_s;&c_t;&c_je;&#x43C;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x751F;&#24577;&#x7CFB;&#x7EDF; <pr>sh&emacron;ngt&#xE0;ix&#xEC;t&#x1D2;ng</pr></trd>,
    <trd>&#x81EA;&#28982;&#x751F;&#24577;&#x7CFB;&#x7EDF;
    <pr>z&#xEC;r&#xE1;nsh&emacron;ngt&#xE0;ix&#xEC;t&#x1D2;ng</pr></trd>
  </trdgrp>
</drv>
</art>

