<kap><ofc>*</ofc>
<rad>galanteri</rad>/o</kap>
<drv mrk="galant2.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref cel="akceso.0a">Akcesoraj</ref>
      objektoj de vesta&jcirc;aro kaj <ref tip="malprt" cel="tualet.0o">tualeto</ref>
      (rubandoj, kombiletoj, pudriloj, ru&gcirc;igiloj kc):
      <ekz>
        mansako, retikulo a&ubreve; burso estas ina <tld/>o, akcesora&jcirc;o, malgranda sako kun
        ansoj mane a&ubreve; &scirc;ultre portebla por kunhavi ekstere diversajn utila&jcirc;ojn, kiaj
        tukoj, okulvitroj, &scirc;minko, skribiloj, portebla telefono, kaj similaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=129475">Mansako</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_ja;&c_n;&c_t;&c_a;&c_r;&c_e;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>galanterie</trd>,
    <trd>ozdobn&eacute; zbo&zcaron;&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Galanteriewaren [ark]</trd>,
    <trd>Schmuck- und Ziergegenst&auml;nde</trd>
  </trdgrp>
  <trd lng="en">toilet accessories</trd>
  <trdgrp lng="fr">
    <trd>accessoires de toilette</trd>,
    <trd>colifichets</trd>
  </trdgrp>
  <trd lng="hu">d&iacute;szm&udblac;&aacute;ruk</trd>
  <trdgrp lng="ja">
    <trd>&#x5C0F;&#x9593;&#x7269;  <pr>&#x3053;&#x307E;&#x3082;&#x306E;</pr></trd>,
    <trd>&#x88C5;&#x8EAB;&#x5177;  <pr>&#x305D;&#x3046;&#x3057;&#x3093;&#x3050;</pr></trd>,
    <trd>&#x5316;&#x7CA7;&#x7528;&#x5177;&#x4E00;&#x5F0F;  <pr>&#x3051;&#x3057;&#x3087;&#x3046;&#x3088;&#x3046;&#x3050;&#x3044;&#x3063;&#x3057;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">toiletgerij</trd>
  <trd lng="pl">galanteria</trd>
  <trd lng="pt">acess&oacute;rios <klr tip="amb">(vest.)</klr></trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_a;&c_n;&c_t;&c_je;&c_r;&c_je;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>doplnkov&eacute; toaletn&eacute; predmety</trd>,
    <trd>galant&eacute;ria</trd>
  </trdgrp>
  <trd lng="uk">&#x491;&#x430;&#x43B;&#x430;&#x43D;&#x442;&#x435;&#x440;&#x435;&#x44F;</trd>
  <trd lng="zh">&#x7DB4;&#x98FE; <pr>zhu&#xEC;sh&#xEC;</pr></trd>
