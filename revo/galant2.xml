  <trdgrp lng="de">
    <trd>Galanteriewaren [ark]</trd>,
    <trd>Schmuck- und Ziergegenst&auml;nde</trd>
  </trdgrp>
  <trd lng="en">toilet accessories</trd>
  <trdgrp lng="fr">
    <trd>accessoires de toilette</trd>,
    <trd>colifichets</trd>
  </trdgrp>
  <trd lng="nl">toiletgerij</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_a;&c_n;&c_t;&c_je;&c_r;&c_je;&c_ja;</trd>
</drv>
</art>
