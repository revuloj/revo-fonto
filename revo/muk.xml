<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: muk.xml,v 1.33 2024/01/26 19:05:54 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>muk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="muk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="muk.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Viskeca elflua&jcirc;o eligata de kelkaj membranoj de la homa
      a&ubreve; besta korpo:
      <ekz>
        oni suspektas, ke troa konsumado de lakta&jcirc;oj gvidas al pli
        granda produktado de <tld/>o
        <fnt>
          <aut>K. Ungar</aut>,
          <vrk>Vivu vigle kaj vegetare!</vrk>,
          <bib>Monato</bib>,
          <lok>1995:7, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mukoz.0o">mukozo</ref>,
      <ref cel="sekrec.0o.FIZL">sekrecio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_mol;</trd>
  <trd lng="bo">&#xF66;&#xFA3;&#xF0B;&#xF46;&#xF74;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>hlen</trd>,
    <trd>mukus</trd>,
    <trd>sliz</trd>
  </trdgrp>
  <trd lng="bg">&c_s;&c_l;&c_u;&c_z;</trd>
  <trd lng="de">Schleim</trd>
  <trdgrp lng="en">
    <trd>mucus</trd>,
    <trd>slime</trd>
  </trdgrp>
  <trd lng="es">moco</trd>
  <trd lng="fa">&a_m;&a_kh;&a_A;&a_T;</trd>
  <trd lng="fr">mucus</trd>
  <trd lng="it">muco</trd>
  <trd lng="ja">&#x7C98;&#x6DB2;  <pr>&#x306D;&#x3093;&#x3048;&#x304D;</pr></trd>
  <trd lng="nl">slijm</trd>
  <trd lng="pl">&sacute;luz</trd>
  <trd lng="pt">muco</trd>
  <trd lng="ru">&c_s;&c_l;&c_i;&c_z;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hlien</trd>,
    <trd>sliz</trd>
  </trdgrp>
  <trd lng="sv">slem</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43B;&c_i;&c_z;</trd>,
    <trd>&c_s;&#x43B;&c_i;&c_z;&#x43E;&c_v;&c_ib; &c_v;&c_i;&c_d;&c_ib;&#x43B;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6F26; <pr>l&#xED;</pr></trd>,
    <trd>&#x9ECF;&#x6DB2; <pr>ni&#xE1;ny&#xE8;</pr></trd>,
    <trd>&#x9F3B;&#x6D95; <pr>b&#xED;t&#xEC;</pr></trd>,
    <trd>&#x6D1F; <pr>t&#xEC;</pr></trd>,
    <trd>&#x7C98;&#x6DB2; <pr>ni&#xE1;ny&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="muk.0a">
  <kap><tld/>a</kap>
  <snc mrk="muk.0a.FIZL">
    <dif>
      Elmetanta <tld/>on, rilata al <tld/>o:
      <ekz>
        <tld/>aj membranoj de la seksaj organoj
        <fnt>
          <aut>V. Lemelev</aut>,
          <vrk>Grava bazo por preventi aidoson</vrk>,
          <bib>Monato</bib>,
          <lok>2001:5, p. 15a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_ib;&c_s;&c_t;&c_y;</trd>
  <trd lng="bg">&c_s;&c_l;&c_u;&c_z;&c_je;&c_s;&c_t;</trd>
  <trd lng="cs">hlenov&yacute;</trd>
  <trd lng="de">schleimig</trd>
  <trd lng="en">mucous</trd>
  <trd lng="es">mucoso</trd>
  <trd lng="fa">&a_m;&a_kh;&a_A;&a_T;&f_y;</trd>
  <trd lng="fr">muqueux</trd>
  <trd lng="it">mucosa</trd>
  <trd lng="pl">&sacute;luzowy</trd>
  <trd lng="ru">&c_s;&c_l;&c_i;&c_z;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  <trd lng="sk">hlienov&yacute;</trd>
  <trd lng="sv">slemmig</trd>
  <trd lng="uk">&c_s;&#x43B;&c_i;&c_z;&#x43E;&c_v;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x6CE5;&#x6CDE; <pr>n&#xED;n&#xEC;ng</pr></trd>,
    <trd>&#x6CE5;&#x6FD8; <pr>n&#xED;n&#xEC;ng</pr></trd>,
    <trd>&#x7C98;&#x6DB2;&#x6027; <pr>ni&#xE1;ny&#xE8;x&#xEC;ng</pr></trd>,
    <trd>&#x7C98;&#x6DB2;&#x7684; <pr>zh&#x101;ny&#xE8;de</pr></trd>
  </trdgrp>
</drv>

<drv mrk="muk.naz0o">
  <kap>naz<tld/>o</kap>
  <snc mrk="muk.naz0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="muk.0o.FIZL">Muko</ref>
      elfluanta el la nazo:
      <ekz>
        mia filino <klr>...</klr> fojfoje tusas kaj elfluigas
        naz<tld/>on
        <fnt>
          <aut>Shiro Kurage</aut>,
          <vrk><url ref="http://d.hatena.ne.jp/shiro-kurage/">
          Hatena de Esperanto</url></vrk>,
          <lok>2009-02-25</lok>
        </fnt>.
      </ekz>
    </dif>   
  </snc>
  <trd lng="be">&c_s;&c_o;&c_p;&c_l;&c_ib;</trd>
  <trd lng="bg">&c_s;&c_o;&c_p;&c_o;&c_l;</trd>
  <trd lng="bo">&#xF66;&#xFA3;&#xF0B;&#xF46;&#xF74;&#xF0B;</trd>
  <trd lng="cs">nosn&iacute; hlen</trd>
  <trdgrp lng="en">
    <trd>snot</trd>,
    <trd>phlegm</trd>,
    <trd>mucus</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>moco</trd>,
    <trd>mocarro</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b; &a_b;&f_y;&a_n;&f_y;</trd>,
    <trd>&a_A_madda;&a_b; &a_d;&a_m;&a_A;&a_gh;</trd>
  </trdgrp>
  <trd lng="fr">morve</trd>
  <trd lng="it">moccolo<klr> (di naso)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x9F3B;&#x6C41;  <pr>&#x306F;&#x306A;&#x3058;&#x308B;</pr></trd>,
    <trd>&#x9F3B;&#x6C34;  <pr>&#x306F;&#x306A;&#x307F;&#x305A;</pr></trd>
  </trdgrp>
  <trd lng="nl">snot</trd>
  <trdgrp lng="pl">
    <trd>smark</trd>.
    <trd>gil<klr> (pot.)</klr></trd>,
    <trd>koza<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="sk">sope&lcaron;</trd>
  <trd lng="sv">snor</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&#x43E;&#x43F;&#x43B;&#x44F;</trd>,
    <trd>&c_sh;&#x43C;&c_a;&c_r;&#x43A;&#x43B;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9F3B;&#x5C4E; <pr>b&#xED;sh&#x1D0;</pr></trd>,
    <trd>&#x9F3B;&#x6D95; <pr>b&#xED;t&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="muk.naz0ulo">
  <kap>naz<tld/>ulo</kap>
  <uzo tip="stl">VULG</uzo>
  <snc mrk="muk.naz0ulo.infano">
    <dif>
      <ref tip="vid" cel="infan.0o.juna">Infan</ref>a&ccirc;o:
      <ekz>
        tiu &ccirc;i naz<tld/>ulo volis elrigardi la bulkojn el mi
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk>Sur Sanga Tero</vrk>,
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_p;&c_l;&c_ja;&c_k;</trd>
    <trd lng="es">mocoso<klr> (subst.)</klr></trd>
    <trd lng="fr">morveux<klr> (subst.)</klr></trd>
    <trd lng="zh">&#x5C0F;&#x7956;&#x5B97; <pr>xi&#x1CE;oz&#x1D4;z&#x14D;ng</pr></trd>
</snc>
  <trd lng="ja">&#x306F;&#x306A;&#x305F;&#x308C;&#x5C0F;&#x50E7;  <pr>&#x306F;&#x306A;&#x305F;&#x308C;&#x3053;&#x305E;&#x3046;</pr></trd>
</drv>
</art>
