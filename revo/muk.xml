<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>muk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="muk.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="muk.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Viskeca elflua&jcirc;o eligata de kelkaj membranoj de la homa
      a&ubreve; besta korpo:
      <ekz>
        oni suspektas, ke troa konsumado de lakta&jcirc;oj gvidas al pli
        granda produktado de <tld/>o
        <fnt>
          <aut>K. Ungar</aut>,
          <vrk>Vivu vigle kaj vegetare!</vrk>,
          <bib>Monato</bib>,
          <lok>1995:7, p. 18a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mukoz.0o">mukozo</ref>,
      <ref cel="sekrec.0o.FIZL">sekrecio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_mol;</trd>
  <trd lng="de">Schleim</trd>
  <trd lng="en">mucus</trd>
  <trd lng="fa">&a_m;&a_kh;&a_A;&a_T;</trd>
  <trd lng="fr">mucus</trd>
  <trd lng="it">muco</trd>
  <trd lng="nl">slijm</trd>
  <trd lng="pl">&sacute;luz</trd>
  <trd lng="pt">muco</trd>
  <trd lng="ru">&c_s;&c_l;&c_i;&c_z;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>hlien</trd>,
    <trd>sliz</trd>
  </trdgrp>
  <trd lng="sv">slem</trd>
</drv>

<drv mrk="muk.0a">
  <kap><tld/>a</kap>
  <snc mrk="muk.0a.FIZL">
    <dif>
      Elmetanta <tld/>on, rilata al <tld/>o:
      <ekz>
        <tld/>aj membranoj de la seksaj organoj
        <fnt>
          <aut>V. Lemelev</aut>,
          <vrk>Grava bazo por preventi aidoson</vrk>,
          <bib>Monato</bib>,
          <lok>2001:5, p. 15a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_l;&c_ib;&c_z;&c_ib;&c_s;&c_t;&c_y;</trd>
  <trd lng="de">schleimig</trd>
  <trd lng="en">mucous</trd>
  <trd lng="fa">&a_m;&a_kh;&a_A;&a_T;&f_y;</trd>
  <trd lng="fr">muqueux</trd>
  <trd lng="it">mucosa</trd>
  <trd lng="pl">&sacute;luzowy</trd>
  <trd lng="ru">&c_s;&c_l;&c_i;&c_z;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>
  <trd lng="sk">hlienový</trd>
  <trd lng="sv">slemmig</trd>
</drv>

<drv mrk="muk.naz0o">
  <kap>naz<tld/>o</kap>
  <snc mrk="muk.naz0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      <ref tip="super" cel="muk.0o.FIZL">Muko</ref>
      elfluanta el la nazo:
      <ekz>
        mia filino <klr>...</klr> fojfoje tusas kaj elfluigas
        naz<tld/>on
        <fnt>
          <aut>Shiro Kurage</aut>,
          <vrk><url ref="http://d.hatena.ne.jp/shiro-kurage/">
          Hatena de Esperanto</url></vrk>,
          <lok>2009-02-25</lok>
        </fnt>.
      </ekz>
    </dif>   
  </snc>
  <trdgrp lng="en">
    <trd>mucus</trd>,
    <trd>phlegm</trd>,
    <trd>snot</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_A_madda;&a_b; &a_b;&f_y;&a_n;&f_y;</trd>,
    <trd>&a_A_madda;&a_b; &a_d;&a_m;&a_A;&a_gh;</trd>
  </trdgrp>
  <trd lng="fr">morve</trd>
  <trd lng="it">moccolo<klr> (di naso)</klr></trd>
  <trdgrp lng="pl">
    <trd>smark</trd>.
    <trd>gil<klr> (pot.)</klr></trd>,
    <trd>koza<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="sk">sopeľ</trd>
  <trd lng="sv">snor</trd>
</drv>

<drv mrk="muk.naz0ulo">
  <kap>naz<tld/>ulo</kap>
  <uzo tip="stl">VULG</uzo>
  <snc mrk="muk.naz0ulo.infano">
    <dif>
      <ref tip="vid" cel="infan.0o.juna">Infan</ref>a&ccirc;o:
      <ekz>
        tiu &ccirc;i naz<tld/>ulo volis elrigardi la bulkojn el mi
        <fnt>
          <aut>J. Baghy</aut>,
          <vrk>Sur Sanga Tero</vrk>,
          <lok>1933</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">morveux<klr> (subst.)</klr></trd>
  </snc>
</drv>
</art>
