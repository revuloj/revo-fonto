<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lagun.xml,v 1.21 2024/01/15 16:14:08 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>lagun</rad>/o
</kap>
<drv mrk="lagun.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Apudborda, malmulte profunda lageto, formita de maro, el kiu &gcirc;i
      estas apartigita de sablaj amasoj:
      <ekz>
        la <tld/>oj de Dancigo, de Venecio.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_g;&c_u;&c_n;&c_a;</trd>   
  <trd lng="br">loc'h<klr tip="ind"> (lenn)</klr></trd>
  <trd lng="el">&lambda;&jota;&my;&ny;&omikron;&theta;&alfa_ton;&lambda;&alfa;&sigma;&sigma;&alfa;</trd>
  <trd lng="de">Lagune</trd>
  <trd lng="en">lagoon</trd>
  <trd lng="es">laguna</trd>
  <trdgrp lng="fr">   
    <trd>lagon</trd>,   
    <trd>lagune</trd>
  </trdgrp>
  <trd lng="hu">laguna</trd>
  <trdgrp lng="ja">
    <trd>&#x6F5F;  <pr>&#x304B;&#x305F;</pr></trd>,
    <trd>&#x6F5F;&#x6E56;  <pr>&#x305B;&#x304D;&#x3053;</pr></trd>,
    <trd>&#x7901;&#x6E56;  <pr>&#x3057;&#x3087;&#x3046;&#x3053;</pr></trd>
  </trdgrp>
  <trd lng="nl">lagune</trd>
  <trd lng="pl">laguna</trd>
  <trd lng="pt">laguna</trd>
  <trd lng="ro">lagun&abreve;</trd>
  <trd lng="ru">&c_l;&c_a;&c_g;&c_u;&c_n;&c_a;</trd>
  <trd lng="sv">lagun</trd>
  <trd lng="uk">&#x43B;&#x430;&#x491;&#x443;&#x43D;&#x430;</trd>
  <trdgrp lng="zh">
    <trd>&#x6F5F;&#x6E56; <pr>x&#xEC;h&#xFA;</pr></trd>,
    <trd>&#x6CFB;&#x6E56; <pr>xi&#xE8;h&#xFA;</pr></trd>,
    <trd>&#x7009;&#x6E56; <pr>xi&#xE8;h&#xFA;</pr></trd>,
    <trd>&#x7901;&#x6E56; <pr>ji&#x101;oh&#xFA;</pr></trd>
  </trdgrp>
</drv>
</art>
