<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>pingl</rad>/o
    </kap>
    <drv mrk="pingl.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="pingl.0o.tajlora">
        <dif>
          Metala stangeto, pinta &ccirc;e unu ekstremo, kiun oni uzas
          por pike fiksi teksa&jcirc;on a&ubreve; veston:
          <ekz>
            sendan&gcirc;era <tld/>o
            <klr>(f)</klr>;
          </ekz><ekz>
            sidi sur <tld/>oj<fnt>Z</fnt>
            <klr>(maltrankvilege atendi. 
              <ref tip="vid" cel="pland.0o">plando</ref>)</klr>;
          </ekz><ekz>
            e&ccirc; <tld/>o <klr>(bagatelo)</klr> povas koron
            trapiki<fnt>Z</fnt>.
          </ekz>
        </dif>
        <ref tip="super" cel="fiks.0ilo.TEK">fiksilo</ref>
        <refgrp tip="vid">
          <ref cel="fibul.0o">fibulo</ref>, 
          <ref cel="najl.0o">najlo</ref>, 
          <ref cel="agraf.0o">agrafo</ref>, 
          <ref cel="brocx.0o">bro&ccirc;o</ref>. 
        </refgrp>
        <trd lng="nl">speld</trd>
        <trd lng="ru">&c_b;&c_u;&c_l;&c_a;&c_v;&c_k;&c_a;</trd>
      </snc>
      <snc mrk="pingl.0o.harpinglo">
        <dif>
          <ref tip="dif" cel="pingl.har0o">Har<tld/>o</ref>.
        </dif>
      </snc>
      <snc mrk="pingl.0o.BOT">
        <uzo tip="fak">BOT</uzo>
        <dif>
          <tld lit="P"/>oforma folio de koniferaj arboj:
          <ekz>
            <tld/>oj de pino<fnt>B</fnt>.
          </ekz> 
        </dif>
        <trd lng="nl">naald</trd>
      </snc>
      <trd lng="de">Nadel</trd>
      <trd lng="ru">&c_h;&c_v;&c_o;&c_ja;</trd>
    </drv>
    <drv mrk="pingl.al0i">
      <kap>al<tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Fiksi per <tld/>o. 
        </dif>
      </snc>
      <trd lng="nl">vastspelden</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_i;&c_k;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>,
        <trd>&c_p;&c_r;&c_i;&c_sh;&c_p;&c_i;&c_l;&c_i;&c_t;&c_mol; <klr>(&c_b;&c_u;&c_l;&c_a;&c_v;&c_k;&c_o;&c_j;)</klr></trd></trdgrp>
    </drv>
    <drv mrk="pingl.de0i">
      <kap>de<tld/>i</kap>
      <snc>
        <dif>
          Demeti ion fiksitan per <tld/>o.  
        </dif>
      </snc>
      <trd lng="nl">losspelden</trd>
    </drv>
    <drv mrk="pingl.har0o">
      <kap>har<tld/>o</kap>
      <snc>
        <dif>
          Metala senpinta stangeto, fleksita la&ubreve; du paralelaj
          bran&ccirc;oj kaj uzata por fiksi la harojn. 
        </dif>
      </snc>
      <trd lng="nl">speld</trd>
      <trd lng="ru">&c_sh;&c_p;&c_i;&c_l;&c_mol;&c_k;&c_a; <klr>(&c_d;&c_l;&c_ja; &c_v;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
    </drv>
    <drv mrk="pingl.prem0o">
      <kap>prem<tld/>o</kap>
      <snc>
        <dif>
          Mallonga <tld/>o kun lar&gcirc;a plata kapo, kiun oni enpikas
          per fingropremo por alkro&ccirc;i desegnopaperon, mapon ktp.
        </dif>
      </snc>
      <trd lng="ru">&c_k;&c_n;&c_o;&c_p;&c_k;&c_a; <klr>(&c_k;&c_a;&c_n;&c_c;&c_je;&c_l;&c_ja;&c_r;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
    </drv>
    <drv mrk="pingl.sendangxera0o">
      <kap>sendan&gcirc;era <tld/>o</kap>
      <snc>
        <dif>
          <tld lit="P"/>o fleksita risorte kiel U kun o-forma cirklo en la
          mezo, kun ingo &ccirc;e unu ekstermo, en kiun ingon eblas fiksi la
          alian (pikan) ekstremon. Oni uzas tion por fiksi etenda&jcirc;ojn
          de vesto a&ubreve; banda&gcirc;o a&ubreve; dorse de insignoj kiel
          <ref tip="super" cel="fiks.0ilo.TEK">fiksilon</ref>.
        </dif>
        <ref tip="vid" cel="agraf.0o">agrafo</ref>
      </snc>
      <trd lng="ru">&c_a;&c_n;&c_g;&c_l;&c_i;&c_j;&c_s;&c_k;&c_a;&c_ja; <ind>&c_b;&c_u;&c_l;&c_a;&c_v;&c_k;&c_a;</ind></trd>
    </drv>
  </art>
