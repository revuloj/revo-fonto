<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pingl.xml,v 1.30 2021/05/29 14:33:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pingl</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pingl.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pingl.0o.tajlora">
    <dif>
      Metala stangeto, pinta &ccirc;e unu ekstremo, kiun oni uzas
      por pike fiksi teksa&jcirc;on a&ubreve; veston:
      <ekz>
        <tld/>ujo, plena de akrapintaj <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        brusta <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Flikilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        kurtenoj <klr>[&#x2026;]</klr> kunetenataj de <tld/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj malobeemaj haroj <klr>[&#x2026;]</klr> liberi&gcirc;is el la katenoj de la duoblaj
        <tld/>oj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kaptadis majskarabojn kaj sterkoskarabojn, metis &ccirc;iun sur <tld/>on
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis tiel lume, ke oni povus trovi <tld/>on
        <fnt><bib>ElektFab</bib>, <lok>pa&scirc;tistino de anseroj apud
        puto</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>sidi sur <tld/>oj</ind>
        <klr>(maltrankvilege atendi)</klr>
        <trdgrp lng="fr">
          <trd>&ecirc;tre assis sur des &eacute;pingles</trd>,
          <trd>ne pas tenir en place</trd>
        </trdgrp>
        <fnt><bib>PrV</bib></fnt>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        e&ccirc; <tld/>o <klr>(bagatelo)</klr> povas koron trapiki
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="fiks.0ilo.TEK">fiksilo</ref>
    <refgrp tip="vid">
      <ref cel="agraf.0o">agrafo</ref>,
      <ref cel="brocx.0o">bro&ccirc;o</ref>,
      <ref cel="fibul.0o">fibulo</ref>,
      <ref cel="najl.0o">najlo</ref>
    </refgrp>
    <trd lng="de">Stecknadel</trd>
    <trd lng="fa">&a_s;&a_n;&a_j;&a_A;&a_q;</trd>
    <trd lng="hu">t&udblac;</trd>
    <trd lng="nl">speld</trd>
    <trd lng="pt">alfinete</trd>
    <trd lng="es">alfiler</trd>
    <trd lng="ru">&c_b;&c_u;&c_l;&c_a;&c_v;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="pingl.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <tld lit="P"/>oforma <ref tip="super" cel="foli.0o.BOT">folio</ref>
      de <ref tip="malprt" cel="konifer.0o">konifero</ref>:
      <ekz>
        <tld/>okreska&jcirc;oj, kiuj dissendis en la salono balzaman odoron
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        bonodoraj arbetoj <tld/>aj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>arboj anstata&ubreve; la foliarboj
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="https://www.monato.be/2014/011198.php">Ursoj mortigataj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fa">&a_s;&a_w;&a_z;&a_n;<klr tip="amb"> (&a_b;&a_r;&f_g;
    &f_k;&a_A;&a_j;)</klr></trd>
    <trd lng="hu">t&udblac;lev&eacute;l</trd>
    <trd lng="nl">naald</trd>
    <trd lng="ru">&c_h;&c_v;&c_o;&c_ja;</trd>
  </snc>
  <trd lng="bo">&#xF41;&#xF56;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>bro&zcaron;</trd>,
    <trd>jehli&ccaron;&iacute;</trd>,
    <trd>spona</trd>,
    <trd>&scaron;pendl&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Nadel</trd>
  <trd lng="en">needle</trd>
  <trd lng="fr">&eacute;pingle</trd>
  <trdgrp lng="sk">
    <trd>bodec</trd>,
    <trd>ihlica</trd>,
    <trd>&scaron;pendl&iacute;k</trd>
  </trdgrp>
</drv>

<drv mrk="pingl.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pingl.al0i.KOMUNE">
    <dif>
      Fiksi per <tld/>o:
      <ekz>
        panjo, atendu, mi al<tld/>os malanta&ubreve;e la tuketon
        <fnt><bib>Revizoro</bib>, <lok>akto 1a, sceno 6a</lok></fnt>!
      </ekz>
      <ekz>
        la knabino al<tld/>is la karton al &scirc;ia uniformo kaj entrajni&gcirc;is
        <fnt>
          <vrk><url ref="https://eo.globalvoices.org/2018/11/3201/">Japanaj mezlernejaj knabinoj portas insignojn por averti ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">p&rcaron;i&scaron;pendlit</trd>
  <trdgrp lng="de">
    <trd>anheften</trd>,
    <trd>anpinnen</trd>,
    <trd>festpinnen</trd>
  </trdgrp>
  <trd lng="en">pin</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&a_n;&a_j;&a_A;&a_q; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_s;&a_n;&a_j;&a_A;&a_q; &a_z;&a_d;&a_n;<klr tip="amb">
    (&a_b;&a_h;)</klr></trd>
  </trdgrp>
  <trd lng="fr">&eacute;pingler</trd>
  <trdgrp lng="hu">
    <trd>megt&udblac;z</trd>,
    <trd>odat&udblac;z<klr> (t&udblac;vel)</klr></trd>,
    <trd>let&udblac;z</trd>,
  </trdgrp>
  <trd lng="nl">vastspelden</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_i;&c_k;&c_o;&c_l;&c_o;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_sh;&c_p;&c_i;&c_l;&c_i;&c_t;&c_mol;
    <klr>(&c_b;&c_u;&c_l;&c_a;&c_v;&c_k;&c_o;&c_j;)</klr></trd>
  </trdgrp>
  <trd lng="sk">pri&scaron;pendli&tcaron;</trd>
</drv>

<drv mrk="pingl.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pingl.de0i.KOMUNE">
    <dif>
      Demeti ion fiksitan per <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">d&eacute;s&eacute;pingler</trd>
  <trd lng="nl">losspelden</trd>
</drv>

<drv mrk="pingl.dent0o">
  <kap>dent<tld/>o</kap>
  <snc mrk="pingl.dent0o.HEJM">
    <dif>
      <ref tip="dif" cel="dent.0opikilo">Dentopikilo</ref>:
      <ekz>
        <nom>La&ubreve;-&Ccirc;eng</nom> penis eligi per dent<tld/>o sezaman grajnon 
        el inter siaj dentoj
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tuta korpo kunmetita el du forkoj, &scirc;topilo kaj dent<tld/>o havas sian
        masocentron tuj malsupre de la pinto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=493766">Masocentro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_u;&c_b;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_a;&c_l;&c_y;&c_p;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_l;&c_je;&c_ch;&c_k;&c_a; &c_z;&c_a;
  &c_z;&c_malmol;&c_b;&c_i;</trd>
  <trdgrp lng="ca">
    <trd>escuradents</trd>,
    <trd>furgadents</trd>
  </trdgrp>
  <trd lng="de">Zahnstocher</trd>
  <trd lng="en">toothpick</trd>
  <trd lng="es">mondadientes</trd>
  <trd lng="fr">cure-dent</trd>
  <trd lng="he">&qof;&yod;&samekh;&fmem;
    &shin;&yod;&nun;&yod;&yod;&fmem;</trd>
  <trdgrp lng="hu">
    <trd>fogv&aacute;j&oacute;</trd>,
    <trd>fogpiszk&aacute;l&oacute;</trd>
  </trdgrp>
  <trd lng="it">stuzzicadenti</trd>
  <trd lng="pl">wyka&lstroke;aczka</trd>
  <trd lng="ru">&c_z;&c_u;&c_b;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_k;&c_a;</trd>
</drv>

<drv mrk="pingl.friz0o">
  <kap>friz<tld/>o</kap>
  <snc mrk="pingl.friz0o.KOS">
    <dif>
      Cilindreto &ccirc;irka&ubreve; kiu oni rulas hartufon
      por &gcirc;in bukligi:
      <ekz>
        blondulino de iom alta kresko kun <klr>[&#x2026;]</klr>
        friz<tld/>oj en la hararo
        <fnt>
          <aut>S. Bubeni&ccaron;</aut>,
          <vrk>Sub Cirkotendo</vrk>,
          <lok>1969</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">nat&aacute;&ccaron;ka na vlasy</trd>
  <trd lng="de">Lockenwickler</trd>
  <trd lng="fr">bigoudi</trd>
  <trd lng="sk">nat&aacute;&ccaron;ka na vlasy</trd>
</drv>

<drv mrk="pingl.har0o">
  <kap>har<tld/>o</kap>
  <snc mrk="pingl.har0o.KOS">
    <dif>
      Metala senpinta stangeto, ofte fleksita la&ubreve; du paralelaj
      bran&ccirc;oj, uzata por fiksi la harojn:
      <ekz>
        viajn harligojn vi levus iom pli alte kaj ornamus ilin
        <klr>[&#x2026;]</klr> per oraj harpingloj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>jehlice do vlas&uring;</trd>,
    <trd>sponka na vlasy</trd>,
    <trd>vl&aacute;senka</trd>
  </trdgrp>
  <trd lng="de">Haarnadel</trd>
  <trd lng="es">alfiler de pelo</trd>
  <trd lng="fa">&a_s;&a_n;&a_j;&a_A;&a_q;&zwnj;&a_s;&a_r;</trd>
  <trd lng="fr">&eacute;pingle &agrave; cheveux</trd>
  <trd lng="hu">hajt&udblac;</trd>
  <trd lng="nl">haarspeld</trd>
  <trd lng="ru">&c_sh;&c_p;&c_i;&c_l;&c_mol;&c_k;&c_a;
  <klr>(&c_d;&c_l;&c_ja; &c_v;&c_o;&c_l;&c_o;&c_s;)</klr></trd>
  <trdgrp lng="sk">
    <trd>ihlica do vlasov</trd>,
    <trd>sponka do vlasov</trd>
  </trdgrp>
</drv>

<drv mrk="pingl.kravat0o">
  <kap>kravat<tld/>o</kap>
  <snc mrk="pingl.kravat0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="P"/>o per kiu oni alfiksas kravaton al &ccirc;emizo:
      <ekz>
        la detektivo rapide mensnotis 
        la orrandajn okulvitrojn, oran horlo&gcirc;eton, oran 
        kravat<tld/>on
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u li venis 
        trakosme?</vrk>, <lok>1980</lok></fnt>;
      </ekz>
      <ekz>
        li surhavis nigran pluvmantelon, led&scirc;uojn, <klr>[&#x2026;]</klr> nigran kravaton,
        nigrajn sunokulvitrojn kaj perlamotan kravat<tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=250099">D. B. Cooper</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">&scaron;pendl&iacute;k do kravaty</trd>
  <trd lng="de">Krawattennadel</trd>
  <trd lng="es">alfiler de corbata</trd>
  <trd lng="fr">&eacute;pingle &agrave; cravate</trd>
  <trd lng="sk">&scaron;pendl&iacute;k do kravaty</trd>
</drv>

<drv mrk="pingl.prem0o">
  <kap>prem<tld/>o</kap>
  <snc mrk="pingl.prem0o.KOMUNE">
    <dif>
      Mallonga <tld/>o kun lar&gcirc;a plata kapo, kiun oni enpikas
      per fingropremo por alkro&ccirc;i desegnopaperon, mapon ktp:
      <ekz>
        dum la proceso de &scirc;ufarado necesas provizore fiksi la partojn de la &scirc;uo per
        prem<tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=103660">&Scirc;uformilo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la trakta&jcirc;oj estas larditaj per prem<tld/>oj, nedigestebla ka&ccirc;o, kancelaria prozo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20184">Richard Wagner</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">p&rcaron;ip&iacute;n&aacute;&ccaron;ek</trd>
  <trd lng="de">Reiszwecke</trd>
  <trd lng="es">chincheta</trd>
  <trd lng="fa">&f_p;&a_w;&a_n;&a_z;</trd>
  <trd lng="fr">punaise<klr tip="amb"> (de bureau)</klr></trd>
  <trd lng="hu">rajzt&udblac;</trd>
  <trdgrp lng="nl">
    <trd>duimspijker</trd>,
    <trd>punaise</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_n;&c_o;&c_p;&c_k;&c_a;
  <klr>(&c_k;&c_a;&c_n;&c_c;&c_je;&c_l;&c_ja;&c_r;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
  <trd lng="sk">pripin&aacute;&ccaron;ik</trd>
</drv>

<drv mrk="pingl.sendangxera0o">
  <kap>sendan&gcirc;era <tld/>o</kap>
  <snc mrk="pingl.sendangxera0o.HEJM">
    <dif>
      <tld lit="P"/>o fleksita risorte kiel U kun o-forma cirklo en la
      mezo, kun ingo &ccirc;e unu ekstremo, en kiun ingon eblas fiksi la
      alian (pikan) ekstremon:
      <ekz>
        fibolo estas praa sendan&gcirc;era <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=213174">Fibolo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la sendan&gcirc;era <tld/>o ne mem malfermi&gcirc;as, necesas kunpremi &gcirc;in
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=31497">Pinglo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="agraf.0o">agrafo</ref>
  </snc>
  <trd lng="de">Sicherheitsnadel</trd>
  <trd lng="es">imperdible</trd>
  <trd lng="fa">&a_s;&a_n;&a_j;&a_A;&a_q;&zwnj;&a_q;&a_f;&a_l;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>&eacute;pingle &agrave; nourprempinglojrice</trd>,
    <trd>&eacute;pingle de suret&eacute;</trd>
  </trdgrp>
  <trd lng="hu">biztost&udblac;</trd>
  <trd lng="nl">veiligheidsspeld</trd>
  <trd lng="ru">&c_a;&c_n;&c_g;&c_l;&c_i;&c_j;&c_s;&c_k;&c_a;&c_ja;
  <ind>&c_b;&c_u;&c_l;&c_a;&c_v;&c_k;&c_a;</ind></trd>
</drv>
</art>
