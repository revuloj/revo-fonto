<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aliz.xml,v 1.24 2023/10/12 18:05:09 revo Exp $">
<kap>
  <rad>aliz</rad>/o
</kap>
<drv mrk="aliz.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="frukt.0o.BOT">Frukto</ref> de <tld/>arbo:
      <ekz>
        ekde a&ubreve;gusto montri&gcirc;as la flave ru&gcirc;aj &gcirc;is skarlatru&gcirc;aj <tld/>oj, ili estas ovoforme sferaj
        kaj similas pometoj 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=327512">Blanka sorbuso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/5/5a/Sorbus_aria-3420.jpg/640px-Sorbus_aria-3420.jpg" prm="CC BY-SA     3.0">bran&ccirc;o kun <tld/>oj, Parko Vivary, Taunton, Britujo
        <fnt>
          <aut>Julie Anne Workman, propra verko</aut>
          <lok><url ref="&WCW;/File:Sorbus_aria-3420.jpg">&WCW;/File:Sorbus_aria-3420.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_n;&c_a; <klr>(&c_ja;&c_g;&c_a;&c_d;&c_a; &c_r;&c_a;&c_b;&c_ib;&c_n;&c_y; &c_m;&c_u;&c_ch;&c_n;&c_ib;&c_s;&c_t;&c_a;&c_j;)</klr></trd>
  <trdgrp lng="cs">
    <trd>b&rcaron;ekyn&ecaron;</trd>,
    <trd>plod je&rcaron;&aacute;bu muku</trd>
  </trdgrp>
  <trd lng="de">Mehlbeere</trd>
  <trd lng="fr">alise</trd>
  <trd lng="he">&#x5D1;&#x5DF; &#x5D7;&#x5D5;&#x5D6;&#x5E8;&#x5E8;</trd>
  <trd lng="ja">&#x30CA;&#x30CA;&#x30AB;&#x30DE;&#x30C9;&#x306E;&#x5B9F;  <pr>&#x30CA;&#x30CA;&#x30AB;&#x30DE;&#x30C9;&#x306E;&#x307F;</pr></trd>
  <trd lng="nl">meelbes</trd>
  <trd lng="pl">jarz&eogonek;bina</trd>
  <trd lng="ro">macara</trd>
  <trdgrp lng="sk">
    <trd>breky&ncaron;a</trd>,
    <trd>muky&ncaron;a</trd>
  </trdgrp>
  <trd lng="uk">&c_g;&#x43E;&c_r;&#x43E;&c_b;&c_i;&#x43D;&c_a; (&#x44F;&c_g;&#x43E;&c_d;&c_i;)</trd>
</drv>

<drv mrk="aliz.0arbo">
  <kap><tld/>arbo, <var><kap><tld/>ujo</kap></var></kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:botanikaj_specioj" cel="speci.0o">Specio</ref>
      de <ref tip="super" cel="sorp.0o">sorpo</ref>
      (<trd lng="la">Sorbus aria</trd>), kun oran&gcirc;koloraj
      &gcirc;is skarlatru&gcirc;aj, interne flavaj &scirc;ajnfruktoj.
    </dif>
    <bld lok="&WCU;/thumb/8/8a/Sorbus_aria_flowers.JPG/320px-Sorbus_aria_flowers.JPG" prm="CC BY-SA     4.0">floraro de <tld/>ujo (Sorbus aria), Erschmatt, Kantono Valais, Svisujo
        <fnt>
          <aut>MurielBendel, propra verko</aut>
          <lok><url ref="&WCW;/File:Sorbus_aria_flowers.JPG">&WCW;/File:Sorbus_aria_flowers.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_n;&c_a; &c_m;&c_u;&c_ch;&c_n;&c_ib;&c_s;&c_t;&c_a;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>b&rcaron;ekyn&ecaron;</trd>,
    <trd>je&rcaron;&aacute;b muk</trd>
  </trdgrp>
  <trd lng="de">Mehlbeerbaum</trd>
  <trd lng="en">common whitebeam</trd>
  <trd lng="fr">alisier</trd>
  <trdgrp lng="hu">
    <trd>lisztes <ind>berkenye</ind></trd>, 
    <trd>s&uuml;v&ouml;lt&eacute;ny</trd>
  </trdgrp>
  <trd lng="ja">&#x30CA;&#x30CA;&#x30AB;&#x30DE;&#x30C9;&#x306E;&#x4E00;&#x7A2E;  <pr>&#x30CA;&#x30CA;&#x30AB;&#x30DE;&#x30C9;&#x306E;&#x3044;&#x3063;&#x3057;&#x3085;</pr></trd>
  <trd lng="pl">jarz&eogonek;bina</trd>
  <trd lng="ro">macara</trd>
  <trd lng="ru">&c_r;&c_ja;&c_b;&c_i;&c_n;&c_a; &c_a;&c_r;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>breky&ncaron;a</trd>,
    <trd>muky&ncaron;a</trd>
  </trdgrp>
  <trd lng="sv">vitoxel</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&#x43E;&c_r;&#x43E;&c_b;&c_i;&#x43D;&c_a; &c_a;&c_r;&c_ib;&#x44F;</trd>,
    <trd>&c_g;&#x43E;&c_r;&#x43E;&c_b;&c_i;&#x43D;&c_a; &#x43A;&c_r;&c_u;&c_g;&#x43B;&#x43E;&#x43B;&c_i;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
</drv>
</art>
