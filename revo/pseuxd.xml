<art mrk="$Id: pseuxd.xml,v 1.18 2023/04/23 15:25:45 revo Exp $">
<kap>
  <rad>pse&ubreve;d</rad>/a
</kap>
<drv mrk="pseuxd.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="pseuxd.0a.KOMUNE">
    <dif>
      Nevera, <ref tip="dif" cel="sxajn.0a">&scirc;ajna</ref>:
      <ekz>
        ho, kiel <tld/>aj estas liaj tri trezoroj
        <fnt>
          <aut>Hasegawa T.</aut>,
          <vrk><url ref="http://en.lernu.net/komunikado/forumo/temo.php?t=6071">
          En &Ccirc;inio Batalanta</url></vrk>,
          <lok>1954</lok>
        </fnt>;
      </ekz>
      <ekz>
        hodia&ubreve; <tld/>aj maldekstruloj kaj ekstremaj dekstruloj dominas 
        la hungaran politikon
        <fnt>
          <aut>Z. T&oacute;th</aut>,
          <vrk><url ref="http://www.satesperanto.org/La-socia-kaj-instruada-ekskomuniko.html">
          La socia kaj instruada ekskomuniko de la ciganaro en
          Hungario</url></vrk>,
          <lok>Sennaciulo, 2007-02</lok>
        </fnt>;
      </ekz>
      <ekz>
        defendantoj de <tld/>aj homaj rajtoj
        <fnt>
	<aut>Gilbert Ledon</aut>
	<vrk><url ref="&Monato;/2001/007283.php">Putraj demokratioj</url></vrk>
	<bib>Monato</bib>
	<lok>2001/01, p. 5</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="pseuxdo.0.KOMUNE">pse&ubreve;do-</ref>
    <trd lng="de">angeblich</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_ja;&c_w;&c_n;&c_y;</trd>,
    <trd>&c_f;&c_a;&c_l;&c_mol;&c_sh;&c_y;&c_v;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_a;&c_p;&c_r;&c_a;&c_w;&c_d;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_s;&c_e;&c_w;&c_d;&c_a;-</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pseudo</trd>,
    <trd>sham</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>fals</trd>,
    <trd>fingit</trd>
  </trdgrp>
  <trd lng="fr">factice</trd>
  <trdgrp lng="pl">
    <trd>fa&lstroke;szywy</trd>,
    <trd>rzekomy</trd>,
    <trd>pozorny</trd>
  </trdgrp>
  <trd lng="uk">&#x43D;&c_je;&c_s;&#x43F;&c_r;&c_a;&c_v;&c_zh;&#x43D;&c_ib;&c_j;</trd>
</drv>
