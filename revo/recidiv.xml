<vortaro>
<art mrk="$Id: recidiv.xml,v 1.9 2022/08/20 22:30:35 revo Exp $">
<kap>
  <rad>recidiv</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="recidiv.0i">
  <kap><tld/>i</kap>
  <snc mrk="recidiv.0i.MED">
    <dif>
      Ree suferi pro malsano &scirc;ajne &ccirc;esinta:
      <ekz>
        mia kronika ftizo <tld/>is
        <fnt>
          <aut>Kurisu Kei</aut>,
          <vrk>Nova vivo en maljunulejo</vrk>,
          <lok>en: Vivprotokoloj, 2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">op&#x11B;tovn&#x11B; p&#xE1;chat trestn&#xFD; &#x10D;in</trd>
  <trd lng="de">rezidivieren</trd>
  <trd lng="es">recidivar</trd>
  <trd lng="fr">rechuter<klr> (m&eacute;d.)</klr></trd>
  <trd lng="nl">recidiveren</trd>
  <trd lng="pl">pope&lstroke;ni&cacute;  <ind>recydyw&eogonek;</ind></trd>
  <trd lng="ro">comite o recidiv&abreve;</trd>
  <trd lng="sk">op&#xE4;tovne p&#xE1;cha&tcaron; trestn&#xFD; &#x10D;in</trd>
</drv>

<drv mrk="recidiv.0o">
  <kap><tld/>o</kap>
  <snc mrk="recidiv.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Reapero &ccirc;e sama ulo de dumtempe &ccirc;esinta malsano:
      <ekz>
        &scirc;i jam informi&gcirc;is pri la <tld/>o de Jado
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 100a, volumo 3a, p.
        301a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">recidiva</trd>
  <trdgrp lng="de">
    <trd>Rezidiv<klr> (med.)</klr></trd>,
    <trd>R&uuml;ckfall</trd>
  </trdgrp>
  <trd lng="es">recidiva</trd>
  <trdgrp lng="fr">
    <trd>rechute<klr tip="amb"> (m&eacute;d.)</klr></trd>,
    <trd>r&eacute;cidive<klr> (m&eacute;d.)</klr></trd>
  </trdgrp>
  <trd lng="nl">recidive</trd>
  <trdgrp lng="pl">
    <trd>recydywa</trd>,
    <trd>nawr&oacute;t choroby</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>recidiv&abreve;</trd>,
    <trd>reapari&#539;ia bolii</trd>
  </trdgrp>
  <trd lng="sk">recid&#xED;va</trd>
  <trd lng="uk">&c_r;&c_je;&c_c;&c_i;&c_d;&c_i;&c_v;</trd>
</drv>
</art>

