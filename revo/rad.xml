<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rad</rad>/o
</kap>

<drv mrk="rad.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rad.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ronda disko turni&gcirc;anta &ccirc;irka&ubreve; centra akso:
      <ekz>
        <tld/>o de veturilo, de horlo&gcirc;o, de muelilo;
      </ekz>
      <ekz>
        <tld/>o malbona knaras plej multe
        (<ref tip="vid" cel="barel.0o">barelo</ref>)
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &jcirc;eti bastonon en la <tld/>on <klr>(malhelpi)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        esti la 5a <tld/>o &ccirc;e veturado<fnt>Z</fnt>
        <klr>(esti tute seninflua, senutila)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <tld/>o <klr>(&scirc;an&gcirc;i&gcirc;oj)</klr> de la sorto;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ne utilas diskuti pri tio, kio okazus se la <tld/>o de la
        historio turni&gcirc;us en alian direkton.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_l;&c_a;</trd>
  <trd lng="bg">&c_k;&c_o;&c_l;&c_je;&c_l;&c_o;</trd>
  <trd lng="de">Rad</trd>
  <trd lng="en">wheel</trd>
  <trd lng="es">rueda</trd>
  <trd lng="fr">roue</trd>
  <trd lng="hu">ker&eacute;k</trd>
  <trd lng="id">roda</trd>
  <trdgrp lng="nl">
    <trd>wiel</trd>,
    <trd>rad</trd>
  </trdgrp>
  <trd lng="pt">roda</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_s;&c_o;</trd>
  <trd lng="sv">hjul</trd>
</drv>

<drv mrk="rad.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="rad.0aro.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Tuto de la <ref tip="prt" cel="rad.0o.TEK"><tld/>oj</ref>
      funkciigantaj aparaton.
    </dif>
  </snc>
  <trd lng="de">Fahrwerk</trd>
  <trd lng="fr">rouage</trd>
  <trdgrp lng="hu">
    <trd>hajt&oacute;m&udblac;</trd>,
    <trd>ker&eacute;khajt&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">raderwerk</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_s;&c_t;&c_je;&c_m;&c_a; <ind>&c_k;&c_o;&c_l;&c_jo;&c_s;</ind></trd>,
    <trd>&c_k;&c_o;&c_l;&c_jo;&c_s;&c_n;&c_y;&c_j; &c_m;&c_je;&c_h;&c_a;&c_n;&c_i;&c_z;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="rad.0radio">
  <kap><tld/>radio</kap>
  <snc mrk="rad.0radio.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="radi.0o.FIG">Radio</ref> de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_c;&c_a;</trd>
  <trd lng="de">Speiche</trd>
  <trd lng="fr">rayon<klr tip="ind"> (de roue)</klr></trd>
  <trd lng="hu">k&uuml;ll&odblac;</trd>
  <trd lng="id">jari-jari</trd>
  <trd lng="nl">spaak</trd>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_c;&c_a;</trd>
</drv>

<drv mrk="rad.0ringo">
  <kap><tld/>ringo</kap>
  <snc mrk="rad.0ringo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ringo el fero a&ubreve; elasta substanco, per kiu oni provizas la randon
      de
      veturila <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_n;&c_a; <klr tip="amb">(&c_k;&c_o;&c_l;&c_a;)</klr></trd>
  <trd lng="de">Radreifen</trd>
  <trd lng="hu">ker&eacute;kabroncs</trd>
  <trd lng="id">ban</trd>
  <trd lng="nl">band<klr tip="ind"> (v.e.wiel)</klr></trd>
  <trd lng="ru">&c_sh;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="rad.0rondo">
  <kap><tld/>rondo</kap>
  <snc mrk="rad.0rondo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Cirkla ekstera parto de <ref tip="super"
      cel="rad.0o.TEK"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_b;&c_a;&c_d;</trd>
  <trd lng="de">Felge</trd>
  <trdgrp lng="hu">
    <trd>ker&eacute;ktalp</trd>,
    <trd>ker&eacute;kp&aacute;nt</trd>,
    <trd>felni</trd>
  </trdgrp>
  <trd lng="id">pelek</trd>
  <trd lng="nl">velg</trd>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_d;</trd>
</drv>

<drv mrk="rad.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rad.0umi.KOMUNE">
    <dif>
      Torture <ref tip="super" cel="mort.0igi.malvivigi">mortigi</ref>
      iun, rompante al li membrojn kaj lasante lin ku&scirc;i sur rado.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_je;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">r&auml;dern</trd>
  <trdgrp lng="fr">
    <trd>rouer<klr tip="ind"> (supplicier)</klr></trd>,
    <trd>faire subir le supplice de la <ind>roue</ind></trd>
  </trdgrp>
  <trd lng="hu">ker&eacute;kbe t&ouml;r</trd>
  <trd lng="nl">radbraken</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_k;&c_a;&c_z;&c_n;&c_i;&c_t;&c_mol;)</klr></trd>
</drv>

<drv mrk="rad.dent0o">
  <kap>dent<tld/>o</kap>
  <snc mrk="rad.dent0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="rad.0o.TEK"><tld lit="R"/>o</ref>, kies
      rondo
      estas provizita per dentoj.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_u;&c_b;&c_ch;&c_a;&c_s;&c_t;&c_a;&c_je; &c_k;&c_o;&c_l;&c_a;</trd>
  <trd lng="de">Zahnrad</trd>
  <trd lng="es">rueda dentada</trd>
  <trd lng="fr">roue dent&eacute;e</trd>
  <trd lng="hu">fogasker&eacute;k</trd>
  <trd lng="id"><ind>roda</ind> gigi</trd>
  <trd lng="nl">tandrad</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_o;&c_je; &c_k;&c_o;&c_l;&c_je;&c_s;&c_o;</trd>,
    <trd>&c_sh;&c_je;&c_s;&c_t;&c_je;&c_r;&c_n;&c_ja;</trd>,
    <trd>&c_sh;&c_je;&c_s;&c_t;&c_je;&c_r;&c_jo;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sv">kugghjul</trd>
</drv>

<drv mrk="rad.inerto0o">
  <kap>inerto<tld/>o,
  <var><kap>inerci<tld/>o<fnt><bib>PIV1</bib></fnt></kap></var></kap>
  <snc mrk="rad.inerto0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Peza <tld/>o, kies <ref cel="inert.0eco.FIZ">inerteco</ref>
      ebligas kontra&ubreve;stari momentajn rezistojn, reguligi
      rapidon kaj povas e&ccirc; servi kiel &ccirc;efa energifonto.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_h;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trd lng="fr">volant d'inertie</trd>
  <trd lng="hu">lendker&eacute;k</trd>
  <trd lng="ru">&c_m;&c_a;&c_h;&c_o;&c_v;&c_i;&c_k;</trd>
</drv>

<drv mrk="rad.spekto0ego">
  <kap>spekto<tld/>ego</kap>
  <snc mrk="rad.spekto0ego.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      granda staranta <ref tip="super" cel="rad.0o.TEK"><tld/>o</ref> en
      amuzparko, kiu turnas se&gcirc;ojn a&ubreve; vagonetojn, vidigante al la
      sursidantoj la &ccirc;irka&ubreve;a&jcirc;on:
      <ekz>
        tie trovi&gcirc;is ru&gcirc;a malgranda spekto-<tld/>ego el a&ccirc;aj lignoj,
        bambuoj kaj &scirc;nuroj
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;man&gcirc;i rubman&gcirc;a&jcirc;on&rightquot;, p.
        16a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_v;&c_i;&c_je;&c_n;&c_s;&c_k;&c_o; &c_k;&c_o;&c_l;&c_je;&c_l;&c_o;</trd>
  <trd lng="de">Riesenrad</trd>
  <trd lng="fr">grande roue</trd>
  <trd lng="hu">&oacute;ri&aacute;sker&eacute;k</trd>
  <trdgrp lng="id">
    <trd><ind>kincir</ind> raksasa</trd>,
    <trd><ind>komidi</ind> putar</trd>
  </trdgrp>
  <trd lng="pt">roda-gigante</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_s;&c_o; &c_o;&c_b;&c_o;&c_z;&c_r;&c_je;&c_n;&c_i;&c_ja;</trd>
</drv>
</art>
