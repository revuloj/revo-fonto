    <trd>&c_r;&c_a;&c_d;&c_a;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="rad.0umi">
  <kap><tld/>umi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rad.0umi.KOMUNE">
    <dif>
      Torture <ref tip="super" cel="mort.0igi.malvivigi">mortigi</ref>
      iun, rompante al li membrojn kaj lasante lin ku&scirc;i sur <tld/>o:
      <ekz>
        li estis <tld/>umita sur la &ccirc;efa placo en septembro 1602
        <fnt>
          <vrk><url ref="&Viki;/Douarnenez">Douarnenez -
          Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la reprezentantoj de rezistmovado estis ekzekutitaj (unu senkapigita per
        lignotabulo kaj la alia <tld/>umita)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=37537">Venceslao la 1-a
          (Bohemio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_je;&c_s;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>l&aacute;mat kolem</trd>,
    <trd>mu&ccaron;it vpl&eacute;t&aacute;n&iacute;m do kola</trd>,
    <trd>umu&ccaron;it vpleten&iacute;m do kola</trd>
  </trdgrp>
  <trd lng="de">r&auml;dern</trd>
  <trdgrp lng="fr">
    <trd>rouer<klr tip="ind"> (supplicier)</klr></trd>,
    <trd>faire subir le supplice de la <ind>roue</ind></trd>
  </trdgrp>
  <trd lng="hu">ker&eacute;kbe t&ouml;r</trd>
  <trd lng="nl">radbraken</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_k;&c_a;&c_z;&c_n;&c_i;&c_t;&c_mol;)</klr></trd>
  <trd lng="sk">l&aacute;ma&tcaron; v kolese</trd>
  <trd lng="uk">&c_k;&c_o;&c_l;&c_je;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>


<drv mrk="rad.0radio">
  <kap><tld/>radio</kap>
  <snc mrk="rad.0radio.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="radi.0o.discelanta">Radio</ref> de <tld/>o;
      <ref tip="sin" cel="spok.0o">spoko</ref>:
      <ekz>
        kiel la aran&gcirc;o de <tld/>oj de veturilo: iliaj aksoj kaj 
        <ref tip="prt" cel="aks.0ingo">aksingo</ref>j kaj
        <tld/>radioj kaj <tld/>rondoj, &ccirc;io estis fandita
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial la diaro de vi <klr>[&#8230;]</klr> ne 
        rompas en pecetojn la radiojn de via <tld/>o
        <fnt>
          <bib>Hamlet</bib>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_c;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>louko&tcaron;</trd>,
    <trd>paprsek kola</trd>
  </trdgrp>
  <trd lng="de">Speiche</trd>
  <trd lng="fr">rayon<klr tip="ind"> (de roue)</klr></trd>
  <trd lng="hu">k&uuml;ll&odblac;</trd>
  <trd lng="id">jari-jari</trd>
  <trd lng="nl">spaak</trd>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">spica</trd>
</drv>

<drv mrk="rad.0ringo">
  <kap><tld/>ringo</kap>
  <snc mrk="rad.0ringo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="ring.0o.FORM">Ringo</ref> el fero a&ubreve; elasta substanco, per kiu oni
      provizas la randon
      de veturila <tld/>o:
      <ekz>
        per preniloj <klr>[ili]</klr> kaptas <tld/>ringon, kiu staris por varmigo en la
        fajro, kaj poste kun granda ur&gcirc;o komencas meti &gcirc;in sur radon
        <fnt>
          <bib>HsT</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam pasintece &ccirc;arfaristoj konstruis a&ubreve; riparis la lignajn radojn de &ccirc;aroj, ili
        tre varmigis ferajn <tld/>ringojn, kiujn ili poste aplikis
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011332.php">Dilati&gcirc;o, kuntiri&gcirc;o ... kaj
          eksperimento por infana amuzi&gcirc;o</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        statistiko do eksterordinara tamen diras malmulton pri poezio: la grava demando
        estas ne, &ccirc;u poetoj a&ubreve; libroj estas multnombraj, kvaza&ubreve; temus pri produktado
        de <tld/>ringoj, sed &ccirc;u la poemoj estas arte valoraj
        <fnt>
          <bib>LPl</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="pneux.0o">pne&ubreve;o</ref>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_n;&c_a; <klr tip="amb">(&c_k;&c_o;&c_l;&c_a;)</klr></trd>
  <trd lng="cs">obru&ccaron; kola</trd>
  <trd lng="de">Radreifen</trd>
  <trd lng="hu">ker&eacute;kabroncs</trd>
  <trd lng="id">ban</trd>
  <trd lng="nl">band<klr tip="ind"> (v.e.wiel)</klr></trd>
  <trd lng="ru">&c_sh;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">obru&ccaron; kolesa</trd>
  <trd lng="uk">&c_o;&c_b;&c_ib;&c_d; (&c_k;&c_o;&c_l;&c_je;&c_s;&c_a;)</trd>
</drv>

<drv mrk="rad.0rondo">
  <kap><tld/>rondo</kap>
  <snc mrk="rad.0rondo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Cirkla ekstera parto de <ref tip="super" cel="rad.0o.TEK"><tld/>o</ref>:
      <ekz>
        iliaj <tld/>rondoj estis altaj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 1:18</lok>
        </fnt>;
      </ekz>
      <ekz>
        cikloido estas la kurbo difinita per <klr>[&#8230;]</klr> punktoj sur la <tld/>rondo
        de cirklo ruli&gcirc;anta la&ubreve; rekto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=105750">Cikloido</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="nab.0o.TEK">nabo</ref>
  </snc>
  <trd lng="be">&c_v;&c_o;&c_b;&c_a;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>disk kola</trd>,
    <trd>obru&ccaron;</trd>,
    <trd>okru&zcaron;&iacute;</trd>,
    <trd>r&aacute;fek kola</trd>
  </trdgrp>
  <trd lng="de">Felge</trd>
  <trdgrp lng="hu">
    <trd>ker&eacute;ktalp</trd>,
    <trd>ker&eacute;kp&aacute;nt</trd>,
    <trd>felni</trd>
  </trdgrp>
  <trd lng="id">pelek</trd>
  <trd lng="nl">velg</trd>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>disk kolesa</trd>,
    <trd>obru&ccaron;</trd>,
    <trd>okru&zcaron;ie</trd>,
    <trd>r&aacute;fik kolesa</trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_b;&c_ib;&c_d; (&c_k;&c_o;&c_l;&c_je;&c_s;&c_a;)</trd>
</drv>

<drv mrk="rad.dent0o">
  <kap>dent<tld/>o</kap>
  <snc mrk="rad.dent0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="rad.0o.TEK"><tld lit="R"/>o</ref>, kies
      rondo estas provizita per dentoj:
      <ekz>
        al iu krutega pinto de la Alpoj <klr>[&#8230;]</klr> oni konstruis dent<tld/>an
        fervojon
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;ipo sube movi&gcirc;as sur dent<tld/>aj reloj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        por la supreni&gcirc;o oni uzas nek dent<tld/>ojn, nek dentrelojn, sed zigzagojn kaj
        rondojn
        <fnt>
          <bib>Monato</bib>
          <aut>Marta Sol&iacute;s</aut>
          <vrk><url ref="&Monato;/2005/008399.php">Unika fervojo en Andoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iu homo havas potencialon esti valora ano de la socio, tamen dume oni estas
        dent<tld/>oj en sistemo
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2012/010826.php">Piratoj hoj!</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_u;&c_b;&c_ch;&c_a;&c_s;&c_t;&c_a;&c_je; &c_k;&c_o;&c_l;&c_a;</trd>
  <trd lng="cs">ozuben&eacute; kolo</trd>
  <trd lng="de">Zahnrad</trd>
  <trd lng="es">rueda dentada</trd>
  <trd lng="fr">roue dent&eacute;e</trd>
  <trd lng="hu">fogasker&eacute;k</trd>
  <trd lng="id"><ind>roda</ind> gigi</trd>
  <trd lng="nl">tandrad</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_u;&c_b;&c_ch;&c_a;&c_t;&c_o;&c_je; &c_k;&c_o;&c_l;&c_je;&c_s;&c_o;</trd>,
    <trd>&c_sh;&c_je;&c_s;&c_t;&c_je;&c_r;&c_n;&c_ja;</trd>,
    <trd>&c_sh;&c_je;&c_s;&c_t;&c_je;&c_r;&c_jo;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">ozuben&eacute; koleso</trd>
  <trd lng="sv">kugghjul</trd>
</drv>

<drv mrk="rad.inerto0o">
  <kap>inerto<tld/>o,
  <var><kap>inerci<tld/>o<fnt><bib>PIV1</bib></fnt></kap></var></kap>
  <snc mrk="rad.inerto0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Peza <tld/>o, kies <ref cel="inert.0eco.FIZ">inerteco</ref>
      ebligas kontra&ubreve;stari momentajn rezistojn, reguligi
      rapidon kaj povas e&ccirc; servi kiel &ccirc;efa energifonto:
      <ekz>
        la <klr>[kur]</klr>tapi&scirc;on movas elektra motoro a&ubreve; &gcirc;i estas provizita per
        inerci<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=356378">Kurtapi&scirc;o</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la akompanado imitas la rad&scirc;pinilon per da&ubreve;re turni&gcirc;aj sesdekononotaj figuroj de
        la dekstra mano, en la maldekstra mano oni a&ubreve;das kvaza&ubreve;e la piedon, tretantan
        la pedalon por la inerci<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=410850">Gretchen am Spinnrade</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_h;&c_a;&c_v;&c_ib;&c_k;</trd>
  <trd lng="de">Schwungrad</trd>
  <trd lng="fr">volant d'inertie</trd>
  <trd lng="hu">lendker&eacute;k</trd>
  <trd lng="ru">&c_m;&c_a;&c_h;&c_o;&c_v;&c_i;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_a;&c_h;&c_o;&c_v;&c_je; &c_k;&c_o;&c_l;&c_je;&c_s;&c_o;</trd>,
    <trd>&c_m;&c_a;&c_h;&c_o;&c_v;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="rad.padel0o">
  <kap>padel<tld/>o</kap>
  <snc mrk="rad.padel0o.TEK">
  <uzo tip="fak">TEK</uzo>
    <dif>
      Cirkla aran&gcirc;o de <ref tip="prt" cel="padel.0o">padelo</ref>j 
      por transformi turni&gcirc;on al fluo a&ubreve; fluon al turni&gcirc;o:
      <ekz>
        simpla ma&scirc;ino, konsistanta de padel<tld/>o kaj kliko
        <fnt>
          <vrk><url ref="&Viki;/Brown-a_Kliko">Brown-a Kliko -
          Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        du trog&ccirc;enoj elmarigis la nafton, tamen eblus aliaj metodoj: pumpiloj,
        rulbendoj, padel<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Christian Bertin</aut>
          <vrk><url ref="&Monato;/2003/008085.html">&Ccirc;u revo iam fari&gcirc;os
          realo?</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="malprt">
        <ref cel="muel.0ilo">muelilo</ref>,
        <ref cel="turbin.0o">turbino</ref>,
        <ref cel="ventol.0ilo">ventolilo</ref>,
        <ref cel="pump.0ilo">pumpilo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>kolo na spodn&iacute; vodu</trd>,
    <trd>lopatkov&eacute; kolo</trd>
  </trdgrp>
  <trd lng="de">Fl&uuml;gelrad</trd>
  <trd lng="es">h&eacute;lice</trd>
  <trd lng="fr">turbine</trd>
  <trd lng="hu">lap&aacute;tker&eacute;k</trd>
  <trd lng="nl">schoepenrad</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_o;&c_p;&c_a;&c_s;&c_t;&c_n;&c_o;&c_je; 
&c_k;&c_o;&c_l;&c_je;&c_s;&c_o;</trd>,
    <trd>&c_k;&c_r;&c_y;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_je;&c_b;&c_n;&c_o;&c_je; 
&c_k;&c_o;&c_l;&c_je;&c_s;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">lopatkov&eacute; koleso</trd>
  <trdgrp lng="sv">
    <trd>skovelhjul</trd>,
    <trd>vattenhjul</trd>
  </trdgrp>
  <trd lng="uk">&c_l;&c_o;&c_p;&c_a;&c_t;&c_je;&c_v;&c_je; &c_k;&c_o;&c_l;&c_je;&c_s;&c_o;</trd>
</drv>

<drv mrk="rad.spekto0ego">
  <kap>spekto<tld/>ego</kap>
  <snc mrk="rad.spekto0ego.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      granda staranta <ref tip="super" cel="rad.0o.TEK"><tld/>o</ref> en
      amuzparko, kiu turnas se&gcirc;ojn a&ubreve; vagonetojn, vidigante al la
      sursidantoj la &ccirc;irka&ubreve;a&jcirc;on:
      <ekz>
        tie trovi&gcirc;is ru&gcirc;a malgranda spekto-<tld/>ego el a&ccirc;aj lignoj,
        bambuoj kaj &scirc;nuroj
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;man&gcirc;i rubman&gcirc;a&jcirc;on&rightquot;, p.
        16a</lok></fnt>;
      </ekz>
      <ekz>
        alloga&jcirc;oj estas krom spekto<tld/>ego kaj onda fervojo ofte teatroprezentoj
        <fnt>
          <vrk><url ref="&Viki;/Amuzparko">Amuzparko - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_v;&c_i;&c_je;&c_n;&c_s;&c_k;&c_o; &c_k;&c_o;&c_l;&c_je;&c_l;&c_o;</trd>
  <trd lng="de">Riesenrad</trd>
  <trd lng="fr">grande roue</trd>
  <trd lng="hu">&oacute;ri&aacute;sker&eacute;k</trd>
  <trdgrp lng="id">
    <trd><ind>kincir</ind> raksasa</trd>,
    <trd><ind>komidi</ind> putar</trd>
  </trdgrp>
  <trd lng="pt">roda-gigante</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_je;&c_s;&c_o; &c_o;&c_b;&c_o;&c_z;&c_r;&c_je;&c_n;&c_i;&c_ja;</trd>
</drv>


<drv mrk="rad.spindel0o">
  <kap>spindel<tld/>o</kap>
  <snc mrk="rad.spindel0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Parto de transmisiilo en formo de <ref cel="spinde.0o">spindelo</ref>, kun ka&gcirc;eto el du &gcirc;is
      multaj dentoj a&ubreve; stangetoj, turni&gcirc;anta orte al kron<tld/>o.
    </dif>
    <bld lok="&WCU;/thumb/6/69/Kronrad2.jpg/320px-Kronrad2.jpg" prm="CC-BY-SA-3.0">spindel<tld/>o
    (supre) kaj kron<tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Kronrad2.jpg">&WCW;/File:Kronrad2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Spindelrad</trd>
  <trd lng="en">fusee</trd>
  <trd lng="fr">fus&eacute;e<klr tip="ind"> (horlogerie)</klr></trd>
</drv>

<drv mrk="rad.sxpin0o">
  <kap>
    &scirc;pin<tld/>o
  </kap>
  <snc mrk="rad.sxpin0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      La <tld/>o de <ref tip="malprt" cel="sxpin.rad0ilo"><tld/>&scirc;pinilo</ref>, 
      metonimie anka&ubreve; la tuta aparato:
      <ekz>
        la maljunulino sidis en sia domo kaj turnis &scirc;pin<tld/>on
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        festofajroj en la formo de &scirc;pin<tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://tekstaro.com/t?nomo=artikoloj-el-monato-2012-2018">Finnlando &dash;
          Somermeza festo en Seurasaari</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kaverna fajrejo kaj &scirc;pin<tld/>o, &ccirc;e kiu klinita oldulino <klr>[&#8230;]</klr> sidis
        dorse al mi, silente &scirc;pinante
        <fnt>
          <aut>Sonja Lang</aut>
          <vrk>La Soleno</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Rad eines <ind>Spinnrade</ind>s</trd>
  <trd lng="uk">&c_p;&c_r;&c_ja;&c_d;&c_k;&c_a;</trd>
</drv>


