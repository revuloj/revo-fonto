<art mrk="$Id: barier.xml,v 1.24 2024/10/07 18:36:21 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>barier</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="barier.0o">
  <kap><tld/>o</kap>
  <snc mrk="barier.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Movebla barilo por fermi pasejon:
      <ekz>
        ili tamen traveturis la urbojn kaj vila&gcirc;ojn de A&ubreve;strio,
        trapasis la limon sub blua-blanka <tld/>o, kaj da&ubreve;rigis la
        voja&gcirc;on en Germanio
        <fnt>
          <aut>Hevesi L., trad. Vizi L.</aut>,
          <vrk><url ref="https://sites.google.com/site/laaventurojdeandreojelky">
          La aventuroj de Andreo Jelky</url></vrk>,
          <lok>[2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="bar.0ajxo">bara&jcirc;o</ref>,
    <ref cel="bar.0ilo">barilo</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_r;'&c_je;&c_r;</trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_g;&c_b;&c_a;&c_w;&c_m;</trd>
  </trdgrp>
  <trd lng="ca">barrera</trd>
  <trdgrp lng="de">
    <trd>Schranke</trd>,
    <trd>Schlagbaum</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>barrier</trd>,
    <trd><klr>(movable) </klr>barricade</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_r;&a_A;&a_h;&a_b;&a_n;&a_d;</trd>,
    <trd>&a_z;&a_n;&a_b;&f_y;&a_l;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">barri&egrave;re<klr tip="ind"> (mobile)</klr></trd>
  <trd lng="he">&#x5DE;&#x5B7;&#x5D7;&#x5E1;&#x5D5;&#x5B9;&#x5DD;</trd>
  <trd lng="hu">soromp&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#38272;&#x67F5;  <pr>&#12418;&#12435;&#12373;&#x304F;</pr></trd>,
    <trd>&#x67F5;  <pr>&#x304D;</pr></trd>,
    <trd>&#x906E;&#x65AD;&#x6A5F;  <pr>&#12375;&#12419;&#12384;&#12435;&#x304D;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>slagboom</trd>,
    <trd>sluitboom</trd>,
    <trd>bareel</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>szlaban</trd>,
    <trd>rogatka</trd>,
    <trd>bariera</trd>
  </trdgrp>
  <trd lng="ro">barier&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_r;&c_mol;&c_je;&c_r;</trd>,
    <trd>&c_sh;&c_l;&c_a;&c_g;&c_b;&c_a;&c_u;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">&#x17E;elezni&#x10D;n&#xE1; z&#xE1;vora</trd>
  <trdgrp lng="cs">
    <trd>bari&#xE9;ra</trd>,
    <trd>sport. p&rcaron;ek&#xE1;&#x17E;ka</trd>,
    <trd>z&#xE1;vora</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_r;&#8217;&c_jeu;&c_r;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_sh;&#x43A;&#x43E;&c_d;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_r;&#x43E;&c_g;&c_a;&c_t;&#x43A;&c_a;</trd>,
    <trd>&c_sh;&#x43B;&c_a;&c_gu;&c_b;&c_a;&c_u;&#x43C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x58C1;&#22418; <pr>b&#xEC;l&#x11B;i</pr></trd>,
    <trd>&#x62E6;&#x8DEF; <pr>l&#xE1;nl&#xF9;</pr></trd>
  </trdgrp>
</drv>
