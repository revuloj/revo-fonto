<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>barier</rad>/o
</kap>
<drv mrk="barier.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Movebla barilo por fermi pasejon:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>oj starigitaj de Usono kontra&ubreve; viando el Nov-Zelando kaj
        aliaj eksportantaj landoj<fnt><aut>D. E. Rogers</aut>: <vrk>Pri
        potoj kaj balotoj</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero
        8a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="bar.0ajxo">bara&jcirc;o</ref>,
    <ref cel="bar.0ilo">barilo</ref>.
  </refgrp>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_r;'&c_je;&c_r;</trd>,
    <trd>&c_sh;&c_l;&c_ja;&c_g;&c_b;&c_a;&c_w;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Schranke</trd>,
    <trd>Schlagbaum</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>barrier</trd>,
    <trd><klr>(movable) </klr>barricade</trd>
  </trdgrp>
  <trd lng="fr">barri&egrave;re<klr tip="ind"> (mobile)</klr></trd>
  <trd lng="hu">soromp&oacute;</trd>  
  <trdgrp lng="ru">
    <trd>&c_b;&c_a;&c_r;&c_mol;&c_je;&c_r;</trd>,
    <trd>&c_sh;&c_l;&c_a;&c_g;&c_b;&c_a;&c_u;&c_m;</trd>
  </trdgrp>
</drv>
</art>
