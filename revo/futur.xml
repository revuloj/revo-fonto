<vortaro>
<art mrk="$Id$">
<kap>
  <rad>futur</rad>/o
</kap>
<drv mrk="futur.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <dif>
      Verba tempo esprimanta estontan agon a&ubreve; staton. 
    </dif>
    <refgrp tip="sin">
      <ref cel="prezen">prezenco</ref>,
      <ref cel="preter1.0o">preterito</ref>,
      <ref cel="perfek.0o">perfekto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Futur</trd>,
     <trd>Zukunftsform</trd></trdgrp>
  <trd lng="es">futuro</trd>
  <trd lng="fr">futur</trd>
  <trd lng="ru">&c_b;&c_u;&c_d;&c_u;&c_shch;&c_je;&c_je; &c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
</drv>
</art>
