<vortaro>
<art mrk="$Id$">
<kap>
<rad>fagopir</rad>/o
</kap>
<drv mrk="fagopi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="poligo1.0oj">poligonacoj</ref>
      (<trd lng="la">Fagopyrum</trd>),
      kies grenon oni uzas por fari farunon.
    </dif>
  </snc>
  <trd lng="de">Buchweizen</trd>
  <trd lng="en">buchwheat</trd>
  <trd lng="fr">sarrasin</trd>
  <trd lng="nl">boekweit</trd>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_ch;&c_i;&c_h;&c_a;</trd>
</drv>
</art>
