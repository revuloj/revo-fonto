<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fagopir</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fagopi.0o">
  <kap><tld/>o</kap>
  <snc mrk="fagopi.0o.BOT">
    <uzo tip="fak">AGR</uzo>
    <uzo tip="fak">BOT</uzo>
    <dif>
      Kultivata vegeta&jcirc;o
      kies grenon oni uzas por fari farunon,
      genro el familio <ref tip="super"
      cel="poligo1.0oj">poligonacoj</ref>
      (<trd lng="la">Fagopyrum</trd>):
      <ekz>
        mi kuiros <tld/>ajn nudelojn
        <fnt>
          <aut>shiro kurage</aut>,
          <vrk>Hatena de Esperanto</vrk>,
          <lok><url
          ref="http://d.hatena.ne.jp/shiro-kurage/20091231/p1">
          2009-12-31</url></lok>
        </fnt>.        
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_e;&c_ch;&c_k;&c_a;</trd>
  <trd lng="de">Buchweizen</trd>
  <trd lng="en">buckwheat</trd>
  <trd lng="fr">sarrasin</trd>
  <trd lng="hu">pon&aacute;nka</trd>
  <trd lng="nl">boekweit</trd>
  <trd lng="pl">gryka</trd>
  <trd lng="pt">fag&oacute;piro</trd>
  <trd lng="ru">&c_g;&c_r;&c_je;&c_ch;&c_i;&c_h;&c_a;</trd>
  <trd lng="tr">kara bu&gbreve;day</trd>
</drv>
</art>
