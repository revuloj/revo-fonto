<drv mrk="agrono.0o">
  <kap><tld/>o</kap>
  <snc mrk="agrono.0o.AGR">
    <dif>
      <ref tip="dif" cel="agrono1.0isto">Agronomiisto</ref>:
      <ekz>
        mi malgranda <tld/>o ripetis mian lekcion
        pri la bur&gcirc;onoj, la floroj
        <fnt>
          <aut>W. F. Hermans, trad. M. Meeuwissen</aut>,
          <vrk><url ref="http://meeuw.org/wimshurst/">
          La elektrilo de Wimshurst</url></vrk>,
          <lok>2013-01-06</lok>
        </fnt>;
      </ekz>
      <ekz>
        memore al elstara esperantisto, <tld/>o, la unua mortpafito de la usta&scirc;a re&gcirc;imo
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2004/008066.html">Retrorigardo al intensa vivo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;inaj <tld/>oj sukcese kulturis novan specon de hibrida rizo
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2003/008031.html">Superrizo vivtenos pli da &ccirc;inoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#20892;&#23398;&#23478; <pr>n&#243;ngxu&#233;ji&amacron;</pr></trd>,
    <trd>&#36786;&#23416;&#23478; <pr>n&#243;ngxu&#233;ji&amacron;</pr></trd>
  </trdgrp>
</snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>terkultivosciencisto</ke>, <ke>agronomiisto</ke> .
  </rim>

  <trd lng="be">&c_a;&c_g;&c_r;&c_a;&c_n;&c_o;&c_m;</trd>
  <trd lng="bg">&c_a;&c_g;&c_r;&c_o;&c_n;&c_o;&c_m;</trd>
  <trd lng="cs">v&ecaron;dec agronom</trd>
  <trdgrp lng="de">
     <trd>Agronom</trd>,
     <trd>studierter <ind>Landwirt</ind></trd>
  </trdgrp>
  <trd lng="en">agronomist</trd>
  <trdgrp lng="es">
     <trd>agr&oacute;nomo</trd>,
     <trd>ingeniero <ind>agr&oacute;nomo</ind></trd>
  </trdgrp>
  <trd lng="fr">agronome</trd>
  <trd lng="he">&#1488;&#1463;&#1490;&#1512;&#1493;&#1465;&#1504;&#1493;&#1465;&#1501;</trd>
  <trdgrp lng="ja">
    <trd>&#36786;&#23398;&#32773;  <pr>&#12398;&#12358;&#12364;&#12367;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trd lng="nl">agronoom</trd>
  <trd lng="pl">agronom</trd>
  <trd lng="pt">agr&ocirc;nomo</trd>
  <trd lng="ru">&c_a;&c_g;&c_r;&c_o;&c_n;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>agron&oacute;m</trd>,
