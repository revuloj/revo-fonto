<art mrk="$Id: fjord.xml,v 1.18 2022/03/20 11:57:20 revo Exp $">
<kap>
<ofc>2</ofc>
<rad>fjord</rad>/o
</kap>
<drv mrk="fjord.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Mallar&gcirc;a, longa kaj tre profunda golfo, kies bordoj estas 
krutaj:
      <ekz><tld/>oj de Norvegujo.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_jo;&c_r;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>fjord</trd>,
    <trd>&uacute;zk&yacute; z&aacute;liv</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>F&ouml;rde</trd>,
    <trd>Fjord</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fjord</trd>,
    <trd>firth</trd>,
    <trd>inlet</trd>
  </trdgrp>
  <trd lng="es">fiordo</trd>
  <trdgrp lng="fr">
    <trd>fjord</trd>,
    <trd>fiord</trd></trdgrp>
  <trd lng="hu">fjord</trd>
  <trd lng="nl">fjord</trd>
  <trd lng="pl">fiord</trd>
  <trd lng="pt">fiorde</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_o;&c_r;&c_d;</trd>,
    <trd>&c_f;&c_mol;&c_o;&c_r;&c_d;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>fjord</trd>,
    <trd>&uacute;zky morsk&yacute; z&aacute;liv</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x444;&#x456;&#x43E;&#x440;&#x434;</trd>,
    <trd>&#x444;&#x439;&#x43E;&#x440;&#x434;</trd>
  </trdgrp>
</drv>
</art>
