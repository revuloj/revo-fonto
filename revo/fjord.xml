<vortaro>
<art mrk="$Id$">
<kap>
<ofc>2</ofc>
<rad>fjord</rad>/o
</kap>
<drv mrk="fjord.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Mallar&gcirc;a, longa kaj tre profunda golfo, kies bordoj estas 
krutaj:
      <ekz><tld/>oj de Norvegujo.</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_ib;&c_jo;&c_r;&c_d;</trd>
  <trdgrp lng="de">
    <trd>F&ouml;rde</trd>,
    <trd>Fjord</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fjord</trd>,
    <trd>fiord</trd></trdgrp>
  <trd lng="hu">fjord</trd>
  <trd lng="nl">fjord</trd>
  <trd lng="pl">fiord</trd>
  <trd lng="pt">fiorde</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_o;&c_r;&c_d;</trd>,
    <trd>&c_f;&c_mol;&c_o;&c_r;&c_d;</trd>
  </trdgrp>
</drv>
</art>
