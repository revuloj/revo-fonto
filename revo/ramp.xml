<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ramp</rad>/i
</kap>
<drv mrk="ramp.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="ramp.0i.ventre">
    <dif>
      Anta&ubreve;eniri, trenante sin sur la ventro:
      <ekz>
        <tld/>as serpento, limakoj vermo, ra&ubreve;po;
      </ekz>
      <ekz>
        ili devis <tld/>i sur la manoj kaj piedoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la tigro alproksimi&gcirc;is <tld/>ante.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="glit.0i">gliti</ref>,
      <ref cel="sxov">&scirc;ovi&gcirc;i</ref>.
    </refgrp>
    <trd lng="es">reptar</trd>
  </snc>
  <snc mrk="ramp.0i.antauen">
    <dif>
      Pene, malfacile kaj malrapide anta&ubreve;eniri:
      <ekz>
        li devis <tld/>i trans grandajn blokojn da &scirc;tono<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li grimpis sur la arbon kaj <tld/>is tra la truo
        malsupren<fnt>Z</fnt>;
      </ekz>
      <ekz>
        nea&ubreve;deble el<tld/>i (sin malrapide el&scirc;ovi) el la
        lito<fnt>Z</fnt>;
      </ekz>
      <ekz>
        glitveturilo kaj du homoj <tld/>is sur la ebena&jcirc;o;
      </ekz>
      <ekz>
        li perdas la vojon kaj tute ne povas el<tld/>i el la
        arbaro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        se &ccirc;iu volas krei novajn sistemojn, ni neniam el<tld/>os el la
        sfero de projektoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la tempo iafoje flugas kiel birdo, iafoje <tld/>as kiel
        vermo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="es">arrastrarse</trd>
  </snc>
  <snc mrk="ramp.0i.supren">
    <dif>
      Pene kaj malfacile supreniri:
      <ekz>
        &scirc;i <tld/>is sur la arbon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i sur krutan rokon, sur altan muron;
      </ekz>
      <ekz>
        la vojo <tld/>as sur la senarbaj suproj;
      </ekz>
      <ekz>
        li supren<tld/>is la veturilon<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="grimp.0i">grimpi</ref>.
    </refgrp>
    <trd lng="es">treparse</trd>
  </snc>
  <snc mrk="ramp.0i.sin_etendi">
    <dif>
      Sin etendi sur iun supra&jcirc;on:
      <ekz>
        sur la muro <tld/>is sova&gcirc;aj vinberujoj<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>aj kreska&jcirc;oj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la ru&gcirc;aj briloj de l' fajro <tld/>is sur la
plafono<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>regarse</trd>,
      <trd>extenderse</trd>
    </trdgrp>
  </snc>
  <snc mrk="ramp.0i.sin_humiligi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malnoble sin humiligi:
      <ekz>
        <tld/>i anta&ubreve; la potenculoj;
      </ekz>
      <ekz>
        ne &ccirc;iam &scirc;iaj pensoj <tld/>is tiel malalte<fnt>Z</fnt>;
      </ekz>
      <ekz>
        genuojn homoj fleksas nur tiam, se la <tld/>ado profiton
        donas<fnt>Z</fnt>
      </ekz>
    </dif>
    <trd
lng="ru">&c_p;&c_r;&c_je;&c_s;&c_m;&c_y;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
    <trd lng="es">arrastrarse<klr> (ante alguien)</klr></trd>
 </snc>
  <trdgrp lng="be">
   <trd>&c_p;&c_a;&c_w;&c_z;&c_mol;&c_c;&c_ib;</trd>,
   <trd>&c_p;&c_o;&c_w;&c_z;&c_a;&c_c;&c_mol;</trd>
 </trdgrp>
  <trdgrp lng="cs">
    <trd>l&eacute;zt</trd>,
    <trd>plazit se</trd>,
    <trd>přen. podlézat</trd>
  </trdgrp>
  <trd lng="de">kriechen</trd>
  <trd lng="en">crawl</trd>
  <trd lng="fr">ramper</trd>
  <trdgrp lng="hu">
    <trd>k&uacute;szik</trd>,
    <trd>m&aacute;szik</trd>
  </trdgrp>
  <trd lng="nl">kruipen</trd>
  <trdgrp lng="pl">
    <trd>czo&lstroke;ga&cacute; si&eogonek;</trd>,
    <trd>pe&lstroke;za&cacute;</trd>
  </trdgrp>
  <trd lng="pt">arrastar(-se)</trd>
  <trd lng="ro">a se t&acirc;r&icirc;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_l;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_z;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>liezť</trd>,
    <trd>plaziť sa</trd>
  </trdgrp>
</drv>

<drv mrk="ramp.0uloj">
  <kap><tld/>uloj</kap>
  <snc>
    <uzo tip="stl">EVI</uzo>
    <ref tip="dif" cel="reptil.0oj">reptilioj</ref>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_z;&c_u;&c_n;&c_y;</trd>
  <trd lng="de">Kriechtiere</trd>
  <trd lng="es">reptiles</trd>
  <trd lng="hu">h&uuml;ll&odblac;k</trd>
  <trd lng="nl">reptielen</trd>
  <trd lng="pl">p&lstroke;azy</trd>
  <trd lng="tr">s&uuml;r&uuml;ngenler</trd>
  <trdgrp lng="ru">
  
<trd>&c_p;&c_r;&c_je;&c_s;&c_m;&c_y;&c_k;&c_a;&c_ju;&c_shch;&c_i;&c_je;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_je;&c_p;&c_t;&c_i;&c_l;&c_i;&c_i;</trd>
  </trdgrp>
  <trd lng="tp">akesi</trd>
</drv>
</art>
