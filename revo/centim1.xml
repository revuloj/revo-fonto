<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: centim1.xml,v 1.30 2022/03/14 15:25:39 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>centimetr</rad>/o
</kap>
<drv mrk="centim1.0o">
  <kap><tld/>o</kap>
  <mlg>cm</mlg>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de longo,
      centono de <ref tip="malprt" cel="metr.0o.unuo">metro</ref>:
      <ekz>
        li elfosis grandegan &scirc;tonon, havantan la alton de &ccirc;irka&ubreve; 8&nbsp;centimetroj kaj la lar&gcirc;on de &ccirc;irka&ubreve; 5&nbsp;centimetroj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        malgrandaj insektoj, kiuj havas apena&ubreve; la longecon de duono da <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu pa&gcirc;o kvarcent ok kvadrat<tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2007/009302.php">La dika Krause? ... kredu min, sinjorino!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ventromezuro &ndash; viro: pli ol 90 <tld/>oj &ndash; virino: pli ol 85 <tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2009/009700.php">Dikuloj maldiki&gcirc;u!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="kilome.0o">kilometro</ref>,
      <ref cel="hektom.0o">hektometro</ref>,
      <ref cel="dekame.0o">dekametro</ref>,
      <ref cel="metr.0o">metro</ref>,
      <ref cel="decime.0o">decimetro</ref>,
    </refgrp>
    <refgrp tip="prt">
      <ref cel="milime.0o">milimetro</ref>
    </refgrp>
    <trd lng="be">&c_s;&c_a;&c_n;&c_t;&c_y;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
    <trd lng="ca">cent&iacute;metre</trd>
    <trd lng="de">Zentimeter</trd>
    <trd lng="el">&epsilon;&kappa;&alfa;&tau;&omikron;&sigma;&tau;&omikron_ton;&my;&epsilon;&tau;&rho;&omikron;</trd>
    <trdgrp lng="en">
      <trd>centimeter</trd>,
      <trd>centimetre</trd>
    </trdgrp>
    <trd lng="es">cent&iacute;metro</trd>
    <trd lng="fa">&a_s;&a_A;&a_n;&a_t;&f_y;&a_m;&a_t;&a_r;</trd>
    <trd lng="fr">centim&egrave;tre</trd>
    <trd lng="hu">centim&eacute;ter</trd>
    <trd lng="it">centimetro</trd>
    <trd lng="nl">centimeter</trd>
    <trd lng="no">sentimeter</trd>
    <trd lng="pl">centymetr</trd>
    <trd lng="pt">cent&iacute;metro</trd>
    <trd lng="ru">&c_s;&c_a;&c_n;&c_t;&c_i;&c_m;&c_je;&c_t;&c_r;</trd>
    <trd lng="sk">centimeter</trd>
    <trd lng="sv">centimeter</trd>
  </snc>
  <trd lng="cs">centimetr</trd>
  <trd lng="he">&#x5E1;&#x5B7;&#x5E0;&#x5D8;&#x5B4;&#x5D9;&#x5DE;&#x5B6;&#x5D8;&#x5B6;&#x5E8;</trd>
  <trd lng="uk">&#x441;&#x430;&#x43D;&#x442;&#x438;&#x43C;&#x435;&#x442;&#x440;</trd>
</drv>
</art>
