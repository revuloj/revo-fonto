<drv mrk="propr.0e">
  <kap><tld/>e</kap>
  <snc mrk="propr.0e.KOMUNE">
    <dif>
      La&ubreve; <tld/>a maniero:
      <ekz>
        aparteni <tld/>e al iu.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>apropiadamente</trd>,
      <trd>en propiedad</trd>,
      <trd>a su manera</trd>
    </trdgrp>
    <trd lng="fr">en propre</trd>
    <trd lng="hu">saj&aacute;tosan</trd>    
  </snc>
  <snc mrk="propr.0e.ARG">
    <dif>
      <ref tip="vid" cel="dir.gxusta0e.ARG">&Gcirc;ustadire</ref>,
      <tld/>asence:
      <ekz>
        la utilo estas tiel videbla por &ccirc;iu, ke pri tio &ccirc;i ni <tld/>e
        ne
        bezonas paroli<fnt>Z</fnt>;
      </ekz><ekz>
        &ccirc;u vi ekpensis iam pri tio, kio <tld/>e levis la homaron super
        &ccirc;iuj aliaj bestoj?<fnt>Z</fnt>
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>tulajdonk&eacute;ppen</trd>,
      <trd>val&oacute;j&aacute;ban</trd>,
      <trd>egy&aacute;ltal&aacute;n</trd>
    </trdgrp>
    <trd lng="es">propiamente</trd>
    <trd lng="fr">&agrave; proprement parler</trd>
    <trd lng="ru">&c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;</trd>
  </snc>
  <trd lng="be">&c_u;&c_l;&c_a;&c_s;&c_n;&c_a;</trd>
  <trd lng="cs">vlastn&ecaron;</trd>
  <trd lng="nl">eigenlijk</trd>
  <trd lng="sk">vlastne</trd>
  <trd lng="uk">&c_v;&c_l;&c_a;&c_s;&c_n;&c_je;</trd>
</drv>

<drv mrk="propr.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="propr.0ajxo.KOMUNE">
    <dif>
      Aparta karakteriza trajto:
      <ekz>
        racio estas <tld/>a&jcirc;o de la homo.
      </ekz>
    </dif>
    <trd lng="be">&c_u;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trdgrp lng="hu">
      <trd>jellemz&odblac; von&aacute;s</trd>,
      <trd>saj&aacute;toss&aacute;g</trd>
    </trdgrp>
    <trd lng="nl">eigenschap</trd>
    <trd lng="ru">&c_s;&c_v;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  </snc>
  <snc mrk="propr.0ajxo.JUR">
    <uzo tip="fak">JUR</uzo>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Io, kio apartenas jure al iu:
      <ekz>
        Esperanto estas nenies <tld/>a&jcirc;o;
      </ekz><ekz>
        privata, hereda <tld/>a&jcirc;o.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_l;&c_a;&c_s;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
      <trd>&c_m;&c_a;&c_jo;&c_m;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="fr">possession<klr> (chose poss&eacute;d&eacute;e)</klr></trd>
    <trd lng="de">Eigentum</trd>
    <trd lng="hu">tulajdon</trd>    
    <trd lng="nl">eigendom</trd>
    <trd lng="ru">&c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="en">property</trd>
  <trd lng="es">propiedad</trd>
  <trd lng="fr">propri&eacute;t&eacute;</trd>
  <trd lng="uk">&c_v;&c_l;&c_a;&c_s;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="propr.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="propr.0eco.KOMUNE">
    <dif>
      Kvalito <tld/>a de objekto:
      <ekz>
        la <tld/>ecoj de la metaloj, de la magneto.
      </ekz>
    </dif>
    <trd lng="be">&c_u;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trd lng="en">peculiarity</trd>
    <trd lng="fr">propri&eacute;t&eacute;</trd>
    <trdgrp lng="hu">
      <trd>tulajdons&aacute;g</trd>,
      <trd>k&uuml;l&ouml;nlegess&eacute;g</trd>,
      <trd>saj&aacute;toss&aacute;g</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_v;&c_o;&c_j;&c_s;&c_t;&c_v;&c_o;</trd>
  </snc>
  <snc mrk="propr.0eco.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Le&gcirc;a, plena kaj <tld/>a posedo de io:
      <ekz>
        la <tld/>eco estas la rajto &gcirc;ui kaj disponi la aferojn kiel
        eble
        plej absolute.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_a; <ind>&c_w;&c_l;&c_a;&c_s;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
    <trdgrp lng="en">
      <trd>property</trd>,
      <trd>ownership</trd>
    </trdgrp>
    <trd lng="fr">possession<klr> (jur.)</klr></trd>
    <trd lng="hu">tulajdonjog</trd>
    <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_o; <ind>&c_s;&c_o;&c_b;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_i;</ind></trd>
  </snc>
  <trd lng="cs">vlastnost</trd>
  
  <trd lng="es">propiedad</trd>
  <trd lng="nl">eigendom</trd>
  <trd lng="sk">vlastnos&tcaron;</trd>
  <trd lng="uk">&c_v;&c_l;&c_a;&c_s;&c_t;&c_i;&c_v;&c_ib;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="propr.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="propr.0igi.KOMUNE">
    <dif>
      <ref tip="vid" cel="hav.0igi">Havigi</ref>
      kiel <tld/>a&jcirc;on:
      <ekz>
        <tld/>igi al si fremdan poseda&jcirc;on, ies verkojn, manierojn.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akapar.0i">akapari</ref>,
      <ref cel="akir.0i">akiri</ref>,
      <ref cel="plagia.0i">plagiati</ref>,
       <ref cel="uzurp.0i">uzurpi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_y;&c_s;&c_v;&c_o;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_s;&c_v;&c_o;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="en">appropriate</trd>
  <trd lng="es"><ind>apropiar</ind>se</trd>
  <trd lng="fr">s'<ind>approprier</ind></trd>
  <trdgrp lng="hu">
    <trd>kisaj&aacute;t&iacute;t</trd>,
    <trd>elsaj&aacute;t&iacute;t</trd>
  </trdgrp>
  <trd lng="nl">toe-eigenen</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_s;&c_v;&c_o;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_v;&c_l;&c_a;&c_s;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_v;&c_o;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="propr.0ulo">
  <kap><tld/>ulo<fnt>Z</fnt>,
  <var><kap><tld/>a&jcirc;ulo</kap></var>,
  <var><kap><tld/>umulo</kap></var>,
  <var><kap><tld/>umanto</kap></var>
  </kap>
  <snc mrk="propr.0ulo.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Tiu, al kiu io apartenas pro rajto de <tld/>eco.
    </dif>
    <refgrp tip="sin">
      <ref cel="posed">posedanto</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="ten">tenanto</ref>
