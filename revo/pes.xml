<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pes.xml,v 1.46 2023/10/20 14:25:20 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pes</rad>/i
</kap>

<drv mrk="pes.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="pes.0i.pezon">
    <dif>
      <ref tip="super" cel="mezur.0i.taksi">Mezuri</ref> la pezon de objekto, komparante tiun
      pezon al alia pezo <klr>(<ref cel="pes.0o"><tld/>o</ref>)</klr>
      elektita kiel unuo:
      <ekz>
        <tld/>i leteron, paka&jcirc;on;
      </ekz>
      <ekz>
        dek mil kikarojn da ar&gcirc;ento mi <tld/>os en la manojn de la oficistoj, por
        enporti en la kason de la re&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Ester 3:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i tiu marmelado &scirc;ajnas al mi bona, <tld/>u al mi du uncojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne<tld/>ebla <klr>(senpeza)</klr> substanco.
      </ekz>
    </dif>
    <trd lng="ca">pesar</trd>
    <trdgrp lng="de">
      <trd>wiegen</trd>,
      <trd>abwiegen</trd>,
      <trd>w&auml;gen</trd>
    </trdgrp>
    <trd lng="fr">peser<klr tip="ind"> (un objet)</klr></trd>
    <trd lng="hu">m&eacute;r<klr tip="ind"> (s&uacute;lyt)</klr></trd>
    <trd lng="nl">wegen<klr tip="ind"> (gewicht bepalen)</klr></trd>
  </snc>
  <snc mrk="pes.0i.valoron">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="esplor.0i">Esplori</ref> la valoron de,
      <ref tip="super" cel="taks.0i.KOMUNE">taksi</ref>, ekzameni:
      <ekz>
        ni devas <tld/>i la &scirc;ancojn;
      </ekz>
      <ekz>
        li &ccirc;ion <tld/>is, esploris, kaj verkis multe da sentencoj
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 12:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        longe pripensadi kaj
        <tld/>adi <klr>(singarde eldiri)</klr> &ccirc;iun sian
        vorton
        <fnt><bib>LR</bib>, <lok>Respondo 3, La Revuo, 1906,
        Decembro</lok></fnt>;
      </ekz>
      <ekz>
        bone estas al la homo, kiu <klr>[&#8230;]</klr> <tld/>as siajn farojn per justeco
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 112:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> reciproke sin rigardas kaj <tld/>as
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>i la poron kaj la kontra&ubreve;on
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        zorge releginte sian leteron por bone <tld/>i &ccirc;iujn &gcirc;iajn terminojn, Viktoro &gcirc;in
        sendis al la patro
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas <tld/>ata sorto de la &scirc;tato
        <fnt><bib>Far3</bib>, <lok>&ccirc;apitro 11a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ca">sospesar</trd>
    <trd lng="de">abw&auml;gen</trd>
    <trd lng="fr">peser<klr tip="ind"> (examiner
    attentivement)</klr></trd>
    <trdgrp lng="hu">
      <trd>m&eacute;rlegel</trd>,
      <trd>fontolgat</trd>
    </trdgrp>
    <trd lng="nl">afwegen</trd>
  </snc>
  <trd lng="be">&c_u;&c_z;&c_v;&c_a;&c_zh;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bo">&t_-a;&t_ga;&t_yata;&t_naro;&t_ga;&t_sa;&t_tsheg;</trd>
  <trd lng="br">poueza&ntilde;<klr tip="ind"> (v.k.)</klr></trd>
  <trdgrp lng="cs">
    <trd>oce&ncaron;ovat</trd>,
    <trd>v&aacute;&zcaron;it</trd>,
    <trd>zji&scaron;&tcaron;ovat v&aacute;hu</trd>,
    <trd>zva&zcaron;ovat</trd>
  </trdgrp>
  <trd lng="en">weigh</trd>
  <trd lng="es">pesar</trd>
  <trd lng="id"><baz>timbang</baz>menimbang</trd>
  <trdgrp lng="ja">
    <trd>&#xFF5E;<ind>&#x306E;&#x91CD;&#12373;&#12434;&#x8A08;&#x308B; 
    </ind><pr>&#x306E;&#x304A;&#12418;&#12373;&#12434;&#x306F;&#x304B;&#x308B;</pr></trd>,
    <trd>&#x306F;&#x304B;&#x308A;&#x306B;&#x304B;&#12369;&#x308B;</trd>,
    <trd>&#x8A08;&#x91CF;&#12377;&#x308B;  <pr>&#12369;&#12356;&#x308A;&#12423;&#12358;&#12377;&#x308B;</pr></trd>,
    <trd>&#x614E;&#x91CD;&#x306B;&#32771;&#x616E;&#12377;&#x308B; 
    <pr>&#12375;&#12435;&#12385;&#12423;&#12358;&#x306B;&#12371;&#12358;&#x308A;&#12423;&#12377;&#x308B;</pr></trd>,
    <trd>&#x541F;&#21619;&#12377;&#x308B;  <pr>&#x304E;&#12435;&#x307F;&#12377;&#x308B;</pr></trd>,
    <trd>&#x6BD4;&#x8F03;&#x691C;&#x8A0E;&#12377;&#x308B; 
    <pr>&#12402;&#x304B;&#x304F;&#12369;&#12435;&#12392;&#12358;&#12377;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">wa&zdot;y&cacute;</trd>
  <trd lng="ro">c&acirc;nt&abreve;ri</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_z;&c_v;&c_je;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_z;&c_v;&c_je;&c_s;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odv&aacute;&zcaron;i&tcaron;</trd>,
    <trd>v&aacute;&zcaron;i&tcaron;</trd>
  </trdgrp>
  <trd lng="th">&#xE0A;&#xE31;&#xE48;&#xE07;&#xE19;&#xE49;&#xE33;&#xE2B;&#xE19;&#xE31;&#xE01;</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_a;&c_zh;&c_i;&c_t;&c_i; (&c_v;&c_i;&c_z;&c_n;&c_a;&c_ch;&c_a;&c_t;&c_i; &c_v;&c_a;&c_g;&c_u;)</trd>,
    <trd>&c_z;&c_v;&c_a;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="pes.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="pes.0o.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        ne faru maljusta&jcirc;on en la ju&gcirc;o, en la mezuro, en la <tld/>o, en la amplekso
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 19:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        per mezuro kaj <tld/>o akiri&gcirc;as sukceso
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="pes1.0o">peso (monunuo)</ref>
  <trd lng="be">&c_u;&c_z;&c_v;&c_a;&c_zh;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">W&auml;gen</trd>
  <trd lng="ja">&#x30DA;&#x30BD;  <pr>&#x307A;&#x305D;</pr></trd>
  <trdgrp lng="pl">
    <trd>wa&zdot;enie</trd>,
    <trd>waga</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>c&acirc;nt&abreve;rire</trd>,
    <trd>greutate</trd>
  </trdgrp>
</drv>

<drv mrk="pes.0ilo">
  <kap><ofc>*</ofc><tld/>ilo <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5156">pes'il'</url></lok></fnt></kap>
  <snc mrk="pes.0ilo.aparato">
    <dif>
      Diversforma <ref tip="super" cel="aparat.0o.TEK">aparato</ref>
      por precize taksi la pezon:
      <ekz>
        vekta, risorta <tld/>ilo;
      </ekz>
      <ekz>
        &gcirc;usta <tld/>ilo kaj &gcirc;ustaj <tld/>iltasoj
        estas de la Eternulo
        <fnt><bib>MT</bib><lok>&Sen; 16:11</lok></fnt>;
      </ekz>
      <ekz>
        la haroj de lia kapo pezis ducent siklojn la&ubreve; la re&gcirc;a <tld/>ilo
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 14:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        li ekvolis scii la pezon de la donacita kapo, metis &gcirc;in sur la teleron de
        <tld/>ilo, kaj la telero mallevi&gcirc;is &gcirc;is la tero
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[tiu]</klr> eta nuna momento iros sur la <tld/>ilon kontra&ubreve; la tuta
        pasinteco kaj superpezos &gcirc;in
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <ind>sablero, kiu klinigas la <tld/>ilon</ind>
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dekkvara</lok>
        </fnt>;
        <trd lng="de">Z&uuml;nglein an der Waage</trd>
      </ekz>
      <ekz>
        klini la teleron de l&#8217; <tld/>ilo al lia flanko
        <fnt>
          <bib>QuV</bib>
          <lok>Epilogo</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun la <ref tip="prt" cel="lang.0o.FIG">lango</ref>
        <klr>(<ref tip="prt" cel="montr.0ilo">montrilo</ref>)</klr> de la <tld/>ilo
        staris kun apena&ubreve;a movo inter ili
        <fnt>
          <bib>VRA</bib>
          <lok>Harald Beijer</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la koro de la mortinto, metita sur la <tld/>ilon de la justeco
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kruko konstatas sur <tld/>ilo, ke li perdis du funtojn
        <fnt>
          <aut>Louis Beaucaire</aut>
          <vrk>Kruko kaj Baniko en Bervalo</vrk>
          <lok>A new approach</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="tas.pesil0o">pesiltaso</ref>,
      <ref cel="pez.0ilo">pezilo</ref>,
      <ref cel="vekt.0o.pesila">vekto<sncref/></ref>
    </refgrp>
    <ref tip="super" cel="mezur.0ilo">mezurilo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_g;&c_ib;</trd>,
    <trd>&c_sh;&c_a;&c_l;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_a;&c_n;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="br">
    <trd>bala&ntilde;s</trd>,
    <trd>mentel</trd>,
    <trd>pouezerez</trd>
  </trdgrp>
  <trd lng="ca">balan&ccedil;a</trd>
  <trdgrp lng="cs">
    <trd>v&aacute;ha</trd>,
    <trd>v&aacute;hy</trd>
  </trdgrp>
  <trd lng="de">Waage</trd>
  <trdgrp lng="en">
    <trd>balance</trd>,
    <trd>scales</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>balanza</trd>,
    <trd>b&aacute;scula</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>balance</trd>,
    <trd>peson</trd>
  </trdgrp>
  <trd lng="hu">m&eacute;rleg</trd>
  <trd lng="id"><ind>timbang</ind>an</trd>
  <trd lng="nl">weegschaal</trd>
  <trd lng="pl">waga</trd>
  <trd lng="ro">c&acirc;ntar</trd>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_y;</trd>
  <trd lng="sk">v&aacute;ha <klr>(na v&aacute;&zcaron;enie)</klr></trd>
  <trd lng="th">&#xE15;&#xE32;&#xE0A;&#xE31;&#xE48;&#xE07;</trd>
</drv>

<drv mrk="pes.P0ilo">
  <kap><tld lit="P"/>ilo</kap>
  <uzo tip="fak">AST</uzo>
  <mlg>Lib</mlg>
  <mlg>&#x264E;&#xFE0E;</mlg>
  <snc mrk="pes.P0ilo.stelfiguro">
    <dif>
      <ref tip="lst" cel="stel.0figuro" lst="voko:zodiakaj_signoj" val="7">Stelfiguro</ref>
      kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Libra</trd>):
      <ekz>
        la suno proksimi&gcirc;as al la stelaro de <tld lit="P"/>ilo
        <fnt><bib>Far1</bib>, <lok>Anta&ubreve;parolo</lok></fnt>;
      </ekz>
      <ekz>
        vi naski&gcirc;is sub la zodiaka signo de <nom>Libra</nom> (<tld/>ilo)
        <fnt>
          <bib>KrM</bib>
          <lok>39. Ni Forton Pli Fre&scirc;an Akiras.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Sh;&c_a;&c_l;&c_ib;</trd>
  <trd lng="br">Bala&ntilde;s<klr tip="ind"> (steredeg)</klr></trd>
  <trdgrp lng="ca">
    <trd>Libra</trd>,
    <trd>balan&ccedil;a</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>v&aacute;ha</trd>,
    <trd>v&aacute;hy</trd>
  </trdgrp>
  <trd lng="de">Waage</trd>
  <trd lng="en">Libra</trd>
  <trdgrp lng="es">
    <trd>Libra</trd>
    <trd>Balanza</trd>
  </trdgrp>
  <trd lng="fr">Balance<klr tip="ind"> (constellation du
zodiaque)</klr></trd>
  <trd lng="hu">M&eacute;rleg</trd>
  <trdgrp lng="id">
    <trd>Libra</trd>,
    <trd>Mizan</trd>,
    <trd>Tula</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x306F;&#x304B;&#x308A;</trd>,
    <trd>&#12390;&#12435;&#12403;&#12435;</trd>,
    <trd>&#x8A08;&#x91CF;&#22120;  <pr>&#12369;&#12356;&#x308A;&#12423;&#12358;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">weegschaal</trd>
  <trd lng="pl">Waga  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Balan&#x21B;&abreve;  <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="ru">&c_V;&c_je;&c_s;&c_y;</trd>
  <trd lng="sk">v&aacute;ha <klr>(na v&aacute;&zcaron;enie)</klr></trd>
  <trd lng="uk">&c_v;&c_a;&c_g;&c_a; (&c_p;&c_r;&c_i;&c_l;&c_a;&c_d;)</trd>
</drv>
</art>
