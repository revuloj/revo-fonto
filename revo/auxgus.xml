<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>a&ubreve;gust</rad>/o,
  <var><kap><ofc>*</ofc><rad>A&ubreve;gust</rad>/o</kap></var>
</kap>
<drv mrk="auxgus.0o">
  <kap><tld/>o,
    <var><kap><ofc>*</ofc> <tld lit="A"/>o</kap></var>
  </kap>
  <snc mrk="auxgus.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Oka <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj"
      val="8">monato</ref> de la <ref tip="malprt"
      cel="jar.0o.KAL">jaro</ref> la&ubreve; la Gregoria kalendaro, inter julio
      kaj septembro:
      <ekz>
        komencante de <tld lit="A"/>o 1888 mi eldonados &ccirc;iumonate
        nomarojn de &ccirc;iuj verkoj pri la lingvo internacia
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        en <tld lit="A"/>o 1905 en la franca urbo
        <nom>Boulogne-sur-Mer</nom> havis lokon la unua tutmonda
        kongreso de esperantistoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Homaranismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o &ccirc;iam falas multe da steloj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        en januaro oni havas 10 gradojn super la nulo, en <tld/>o 27
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">januaro, Rim.</ref>
  <trd lng="af">Augustus</trd>
  <trd lng="be">&c_zh;&c_n;&c_ib;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_a;&c_v;&c_g;&c_u;&c_s;&c_t;</trd>
  <trd lng="br">Eost<klr tip="ind"> (miz)</klr></trd>
  <trd lng="ca">agost</trd>
  <trd lng="cs">srpen</trd>
  <trd lng="de">August</trd>
  <trd lng="en">August</trd>
  <trd lng="es">agosto</trd>
  <trd lng="fr">ao&ucirc;t</trd>
  <trd lng="fy">augustus</trd>
  <trd lng="he">&alef;&vav;&gimel;&vav;&samekh;&tet;</trd>
  <trd lng="hu">augusztus</trd>
  <trd lng="id">Agustus</trd>
  <trd lng="io">agosto</trd>
  <trd lng="it">agosto</trd>
  <trd lng="ja">&#20843;&#26376; [<ind>&#12399;&#12385;&#12364;&#12388;</ind>]</trd>
  <trdgrp lng="jbo">
    <trd>bivma'i</trd>,
    <trd>cinfyma'i</trd>
  </trdgrp>
  <trdgrp lng="lat">
    <trd>Sextilis</trd>,
    <trd><ind>Augustus</ind> mensis</trd>
  </trdgrp>
  <trd lng="nl">augustus</trd>
  <trd lng="oc">agost</trd>
  <trd lng="pl">sierpie&nacute;</trd>
  <trd lng="ro">august</trd>
  <trd lng="ru">&c_a;&c_v;&c_g;&c_u;&c_s;&c_t;</trd>
  <trd lng="sk">august</trd>
  <trd lng="sv">augusti</trd>
  <trd lng="tp">tenpo mun nanpa luka tu wan</trd>
  <trd lng="tr">a&gbreve;ustos</trd>
  <trd lng="vo">gustul</trd>
</drv>

<drv mrk="auxgus.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o:
      <ekz>
        la <tld/>a &ccirc;ielo estis plena de steloj
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        komenci&gcirc;is la tradicia <tld/>a festo de la urbo
        <fnt>
          <bib>Monato</bib>
          <aut>Tatjana Loskutova</aut>
          <vrk><url ref="https://www.monato.be/2007/009164.php">Voja&gcirc;o
          en Iberio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a-septembra numero de Monato
        <fnt>
          <bib>Monato</bib>
          <aut>Josef &Scirc;emer</aut>
          <vrk><url ref="https://www.monato.be/2008/009590.php">Israelo
          60-jara (3)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="io">agostala</trd>
  <trd lng="ja">&#20843;&#26376;&#12398; [<ind>&#12399;&#12385;&#12364;&#12388;&#12398;</ind>]</trd>
  <trd lng="vo">gustulik</trd>
</drv>

</art>
