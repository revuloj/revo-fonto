<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>haleluja</rad><fnt>Z</fnt>
</kap>
<drv mrk="halelu.0">
  <kap>
    <tld/>
  </kap>
  <uzo tip="fak">REL</uzo>
  <snc>
    <dif>
      Hebrea vorto, signifanta &leftquot;la&ubreve;du Dion&rightquot;,
      uzata interjekcie &ccirc;e la fino de kelkaj religiaj kantoj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_ib;&c_l;&c_u;&c_ja;</trd>
  <trd lng="de">halleluja</trd>
  <trd lng="el">
  &alfa;&lambda;&lambda;&eta;&lambda;&omikron;&ypsilon_ton;&jota;&alfa;
  </trd>
  <trdgrp lng="en">
    <trd>alleluia</trd>,
    <trd>hallelujah</trd></trdgrp>
  <trd lng="es">aleluya</trd>
  <trd lng="fr">all&eacute;luia<klr tip="ind"> (interj.)</klr></trd>
  <trd lng="nl">halleluja</trd>
  <trdgrp lng="hu">
    <trd>alleluja</trd>,
    <trd>halleluja</trd>
  </trdgrp>
  <trd lng="pl">aleluja</trd>
  <trd lng="pt">aleluia</trd>
  <trd lng="ru">&c_a;&c_l;&c_i;&c_l;&c_u;&c_j;&c_ja;</trd>
</drv>
</art>
