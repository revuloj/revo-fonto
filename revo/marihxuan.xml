<vortaro>
  <art mrk="$Id: marihxuan.xml,v 1.1 2023/05/28 07:20:39 afido Exp $">
    <kap>
      <rad>mari&hcirc;uan</rad>/o
    </kap>

<drv mrk="marihxuan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="drog.0o">Drogo</ref> farita el la floroj kaj   
      apudaj folietoj de <ref tip="super" cel="kanab.0o">kanabo</ref>:
      <ekz>
        la serba polico <klr>[&#8230;]</klr> konfiskis 1,4 tunojn da <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Dimitrije Jani&ccaron;i&cacute;</aut>
          <vrk><url ref="https://www.monato.be/2009/009807.php"
              >Heroino-herooj</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        por la e&ubreve;ropa merkato Maroko fari&gcirc;is lastatempe granda <tld/>o-rikoltejo
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2015/011459.php"
              >Armeaj pilotoj arestitaj</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>      
    </dif>
    <refgrp tip="vid">
      <ref cel="hasxisx.0o">ha&scirc;i&scirc;o</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_a;&c_r;&c_y;&c_h;&c_u;&c_a;&c_n;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Marihuana</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>marijuana</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>marihuana</trd>
    </trdgrp>
  </snc>
</drv>

  </art>

