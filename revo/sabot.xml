    <trd>&#22952;&#23475;&#12377;&#12427;  <pr>&#12412;&#12358;&#12364;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#12382;&#12435;&#12374;&#12356;&#12395;&#34892;&#12358;  <pr>&#12382;&#12435;&#12374;&#12356;&#12395;&#12362;&#12371;&#12394;&#12358;</pr></trd>,
    <trd>&#25163;&#25244;&#12365;&#12377;&#12427;  <pr>&#12390;&#12396;&#12365;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">saboteren</trd>
  <trd lng="pl">sabotowa&cacute;</trd>
  <trdgrp lng="pt">
