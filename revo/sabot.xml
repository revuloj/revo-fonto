  </snc>
  <trd lng="be">&c_s;&c_a;&c_b;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_s;&c_a;&c_b;&c_o;&c_t;&c_i;&c_r;&c_a;&c_m;</trd>
