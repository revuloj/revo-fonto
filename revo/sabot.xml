<kap>
  <rad>sabot</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sabot.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sabot.0i.MIL">
    <dif>
      Konscie fu&scirc;i, difekti, malutile al iu.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_b;&c_a;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_s;&c_a;&c_b;&c_o;&c_t;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="ca">sabotatge</trd>
  <trdgrp lng="cs">
    <trd>ma&#x159;it pr&#xE1;ci</trd>,
    <trd>sabotovat</trd>
  </trdgrp>
  <trd lng="de">sabotieren</trd>
  <trd lng="en">sabotage</trd>
  <trd lng="es">sabotear</trd>
  <trd lng="fr">saboter</trd>
  <trd lng="he">&lamed;<ind>&het;&bet;&lamed;</ind></trd>
  <trd lng="hu">szabot&aacute;l</trd>
  <trd lng="it">sabotare</trd>
  <trd lng="nl">saboteren</trd>
  <trd lng="pl">sabotowa&cacute;</trd>
  <trdgrp lng="pt">
   <trd>sabotar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_b;&c_o;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja; <ind>&c_v;&c_r;&c_je;&c_d;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mari&#x165; pr&#xE1;cu</trd>,
    <trd>sabotova&#x165;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x448;&#x43A;&#x43E;&#x434;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x430;&#x431;&#x43E;&#x442;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x439;&#x43C;&#x430;&#x442;&#x438;&#x441;&#x44F; &#x441;&#x430;&#x431;&#x43E;&#x442;&#x430;&#x436;&#x435;&#x43C;</trd>,
    <trd>&#x448;&#x43A;&#x456;&#x434;&#x43D;&#x438;&#x446;&#x442;&#x432;&#x43E;&#x43C;</trd>
  </trdgrp>
</drv>

<drv mrk="sabot.0o">
  <kap><tld/>o</kap>
  <snc mrk="sabot.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Volata difektado:
      <ekz>
        li aldonis ke Kubo suferadis <klr>...</klr> <tld/>ojn kaj 
        atakon de armitaj bandoj
        <fnt>
          <aut>M. G. Gonz&aacute;lez</aut>,
          <vrk><url ref="http://www.radiohc.cu/eo/noticias/nacionales/10230-konfirmas-kubo-sian-kompromison-al-la-popolo">
          [elsendo de 2014-02-01]</url></vrk>,
          <lok>Radio Havano Kubo, 2014-01-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">sabotage</trd>
</drv>

<drv mrk="sabot.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="sabot.0isto.MIL">
    <dif>
      Iu, kiu difektas por embarasi malamikon:
      <ekz>
        li trafus unu el la patroloj kaj oni erare taksus 
        lin <tld/>isto a&ubreve; spiono
        <fnt>
          <aut>Chun-Chan Yeh, trad. W. Auld</aut>,
          <vrk>Montara Vila&gcirc;o</vrk>,
          <lok>1984</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>diverzant</trd>,
    <trd>z&#xE1;&#x161;kodn&#xED;k</trd>
  </trdgrp>
  <trd lng="fr">saboteur</trd>
  <trdgrp lng="sk">
    <trd>diverzant</trd>,
    <trd>z&#xE1;&#x161;kodn&#xED;k</trd>
  </trdgrp>
</drv>
</art>
