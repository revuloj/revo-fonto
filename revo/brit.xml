<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brit.xml,v 1.37 2023/10/29 19:11:03 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>brit</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="brit.0o">
  <kap><ofc>9</ofc><tld/>o <fnt><aut>Akademio de Esperanto</aut>, 
<vrk><url ref="&AdE;/decidoj/9oa.html">
9a Oficiala Aldono</url></vrk>, <lok>brit/o</lok></fnt></kap> 
  <snc mrk="brit.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Ano de <ref tip="super" cel="kelt.0o">kelta</ref> popolo, kiu
      lo&gcirc;is en <ref cel="briti.B0o.insulo">Britio<sncref/></ref> anta&ubreve;
      kaj dum la Romia konkero, kaj donis sian nomon al la insulo kaj la
      lando (<ref cel="brit.B0ujo"><tld lit="B"/>ujo</ref>):
      <ekz>
        la <tld/>oj parolis lingvon parencan al la nuna kimra, kaj ilia
        teritorio inkluzivis, krom Anglio kaj Kimrio, anka&ubreve; kelkajn
        partojn de suda kaj centra Skotlando
        <fnt>
          <vrk>Lingvoj en Skotlando</vrk>,
          <lok><url ref="http://fp.borderline.f9.co.uk/eas/En%20Esperanto/lingvoj_en_skotlando.htm">Esperanto
          Asocio de Skotlando</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_r;&c_y;&c_t;</trd>
    <trd lng="en">Briton</trd>
    <trd lng="es">brit&aacute;nico</trd>
    <trd lng="hu">brit</trd>
    <trd lng="pl">bryt</trd>
    <trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_t;</trd>
  </snc>
  <snc mrk="brit.0o.britujano">
    <dif>
      <tld lit="B"/>ujano, <ref tip="super" cel="sxtat.0ano">&scirc;tatano</ref> de <ref tip="malprt" cel="brit.B0ujo"><tld lit="B"/>ujo</ref>:
      <ekz>
        la 6an de junio celebros <tld/>oj &#x2010; kune kun francoj, usonanoj,
        k.a. &#x2010; la 50an datrevenon de la alianca invado en Francion, kiu
        liberigis E&ubreve;ropon de sub nazia jugo, <tld/>oj jam celebris la
        25an, la 40an, la 45an datrevenojn, &ccirc;ar al <tld/>oj pla&ccirc;as
        eventoj, kiuj memorigas pri militista sukceso
        <fnt>
          <aut>P. Gubbins</aut>,
          <vrk>Revivi&gcirc;as la fantomoj &#x2010; de 1944</vrk>,
          <bib>Monato</bib>, <lok>1994:05</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_r;&c_y;&c_t;&c_a;&c_n;&c_je;&c_c;</trd>
    <trdgrp lng="en">
      <trd>Britisher</trd>,
      <trd>Brit</trd>
    </trdgrp>
    <trd lng="hu">nagy-britanniai</trd>
    <trd lng="pl">Brytyjczyk</trd>
    <trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_je;&c_c;</trd>
  </snc>
  <trd lng="bg">&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="de">Brite</trd>
  <trd lng="fr"><ind>Breton</ind> insulaire</trd>
  <trd lng="fy">Brit</trd>
  <trd lng="he">&#x5D1;&#x5E8;&#x5D9;&#x5D8;&#x5D9;</trd>
  <trdgrp lng="ja">
    <trd>&#x30A4;&#x30AE;&#x30EA;&#x30B9;&#x4EBA;  <pr>&#x30A4;&#x30AE;&#x30EA;&#x30B9;&#x3058;&#x3093;</pr></trd>,
    <trd>&#x30A4;&#x30AE;&#x30EA;&#x30B9;&#x56FD;&#x6C11;  <pr>&#x3044;&#x304E;&#x308A;&#x3059;&#x3053;&#x304F;&#x307F;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">Brit</trd>
  <trd lng="pt">bret&atilde;o</trd>
  <trd lng="sk">Brit</trd>
  <trd lng="uk">&c_b;&c_r;&c_i;&c_t;&c_a;&#x43D;&c_je;&c_c;&#x44C;</trd>
</drv>

<drv mrk="brit.0a">
  <kap><tld/>a</kap>
  <snc mrk="brit.0a.HIS">
    <dif>
      Rilata al <tld/>oj<sncref ref="brit.0o.HIS"/>:
      <ekz>
        la <tld/>a <ref tip="lst" cel="lingv.0o.homa" lst="voko:lingvoj">lingvo</ref> 
        estas unu el la <ref tip="super" cel="kelt.0a_lingvo">keltaj lingvoj</ref>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_r;&c_y;&c_c;&c_k;&c_ib;</trd>
    <trd lng="en">Brittonic</trd>
    <trd lng="hu">brit</trd>
    <trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="brit.0a.landa">
    <dif>
      Rilata al <tld/>oj<sncref ref="brit.0o.britujano"/> a&ubreve;
      ilia &scirc;tato:
      <ekz>
        mi havas nun la okazon saluti la <tld/>an popolon en &gcirc;ia granda
        &ccirc;efurbo
        <fnt><bib>Paroloj</bib>, <vrk>Parolado en la Guildhall (Londono)
        en la 21a de a&ubreve;gusto 1907</vrk></fnt>;
      </ekz>
      <ekz>
        neniu el ni forgesis kiel amike ni estis akceptitaj en la
        <tld/>aj urboj Folkestone kaj Dover
        <fnt><bib>Paroloj</bib>, <vrk>Parolado en la Guildhall (Londono)
        en la 21a de a&ubreve;gusto 1907</vrk></fnt>;
      </ekz>
      <ekz>
        <tld/>aj kaj usonaj aviadiloj neniige bombardis Dresdenon
        <fnt><aut>S. Maul</aut>, <vrk>El mia vidpunkto</vrk>,
        <bib>Monato</bib>, <lok>1995:03</lok></fnt>;
      </ekz>
      <ekz>
        la dato markis la finon ne nur de <tld/>a, sed anka&ubreve; de e&ubreve;ropa
        influo
        <fnt>
          <aut>P. Gubbins</aut>:
          <vrk>Revivi&gcirc;as la fantomoj &#x2010; de 1944</vrk>,
          <bib>Monato</bib>, <lok>1994:05</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_r;&c_y;&c_t;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
    <trd lng="en">British</trd>
    <trd lng="hu">nagy-britanniai</trd>
    <trd lng="pl">brytyjski</trd>
    <trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc mrk="brit.0a.insula">
    <dif>
      Rilata al la <ref cel="brit.B0aInsularo"><tld lit="B"/>a
      Insularo</ref>:
      <ekz>
        la <tld/>aj insuloj.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_r;&c_y;&c_t;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
    <trd lng="en">British</trd>
    <trd lng="es">brit&aacute;nico</trd>
    <trd lng="pl">Wielkiej Brytanii<klr> {A}</klr></trd>
    <trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <trd lng="de">britisch</trd>
  <trd lng="fr">britannique<klr> (adj.)</klr></trd>
  <trd lng="fy">Britsk</trd>
  <trd lng="he">&#x5D1;&#x5E8;&#x5D9;&#x5D8;&#x5D9;</trd>
  <trd lng="ja">&#x30A4;&#x30AE;&#x30EA;&#x30B9;&#x56FD;&#x6C11;&#x306E;  <pr>&#x3044;&#x304E;&#x308A;&#x3059;&#x3053;&#x304F;&#x307F;&#x3093;&#x306E;</pr></trd>
  <trd lng="nl">Brits</trd>
  <trdgrp lng="pt">
    <trd>bret&atilde;o</trd>,
    <trd>ingl&ecirc;s</trd>
  </trdgrp>
  <trd lng="sk">britsk&yacute;</trd>
  <trd lng="uk">&c_b;&c_r;&c_i;&c_t;&c_a;&#x43D;&c_s;&#x44C;&#x43A;&c_i;&c_j;</trd>
</drv>

<drv mrk="brit.B0ujo">
  <kap><ofc>9</ofc><tld lit="B"/>ujo <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="brit.B0ujo.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Insula <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&scirc;tato</ref>
      situanta en okcidenta <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>, norde de Francujo kaj
      oriente de
      Irlando, &ccirc;irka&ubreve;ita de <ref tip="vid" cel="atlant.A0o">Atlantiko</ref> kaj <ref tip="vid" cel="mar.NordaM0o">Norda Maro</ref>:
      <ekz>
        li revenis kun ri&ccirc;a militakira&jcirc;o de la Galla bordo, kie la
        popolo same kiel en <tld lit="B"/>ujo kun timego kantadis
        <ctl>Liberigu nin Dio de l' sova&gcirc;aj Normandoj</ctl>
        <fnt><bib>Fab4</bib>, <lok>Lasta son&gcirc;o de maljuna
        kverko</lok></fnt>;
      </ekz>
      <ekz>
        ni kongresas nun en glora universitata urbo de Granda <tld lit="B"/>ujo
        <fnt><bib>Paroloj</bib>, <vrk>Parolado anta&ubreve; la Tria Kongreso
        Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x82F1; <pr>y&#x12B;ng</pr></trd>,
    <trd>&#x5927;&#x4E0D;&#x5217;&#x985A; <pr>d&#xE0;b&#xF9;li&#xE8;di&#x101;n</pr></trd>,
    <trd>&#x4E0D;&#x5217;&#x985B; <pr>b&#xFA;li&#xE8;di&#x101;n</pr></trd>
  </trdgrp>
</snc>
  <ref tip="sin" cel="briti.B0o">Britio</ref>
  <trdgrp lng="be">
    <trd>&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_b;&c_r;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_ja;</trd>,
    <trd>&c_V;&c_ja;&c_l;&c_ib;&c_k;&c_a;&c_ja;
    <ind>&c_B;&c_r;&c_y;&c_t;&c_a;&c_n;&c_ib;&c_ja;</ind></trd>
  </trdgrp>
  <trd lng="da">Storbritannien</trd>
  <trd lng="de">Gro&szlig;britannien</trd>
  <trd lng="en">Great Britain</trd>
  <trd lng="es">Gran Breta&ntilde;a</trd>
  <trd lng="fr">Grande Bretagne</trd>
  <trd lng="fy">Grut-Brittanje</trd>
  <trd lng="hu">Nagy-Britannia</trd>
  <trd lng="nl">Groot-Brittanni&euml;</trd>
  <trd lng="pl">Wielka Brytania</trd>
  <trdgrp lng="pt">
    <trd>Reino Unido</trd>,
    <trd>Gr&atilde; Bretanha</trd>
  </trdgrp>
  <trd lng="ru">&c_V;&c_je;&c_l;&c_i;&c_k;&c_o;&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">Ve&lcaron;k&aacute; <ind>Brit&aacute;nia</ind></trd>
</drv>

<drv mrk="brit.0ujano">
  <kap><tld/>ujano</kap>
  <snc mrk="brit.0ujano.ETN">
    <dif>
      Lo&gcirc;anto de <tld lit="B"/>ujo, i.a. anglo, skoto, kimro.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_y;&c_t;&c_a;&c_n;&c_je;&c_c;</trd>
  <trdgrp lng="en">
    <trd>Brit</trd>,
    <trd>Briton</trd>,
    <trd>Britisher</trd>
  </trdgrp>
  <trd lng="fr">Britannique<klr> (subst.)</klr></trd>
  <trd lng="fy">Brit</trd>
  <trd lng="hu">nagy-britanniai</trd>
  <trd lng="nl">Brit</trd>
  <trd lng="pl">Brytyjczyk</trd>
  <trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="sk">Brit</trd>
</drv>

<drv mrk="brit.B0aInsularo">
  <kap><tld lit="B"/>a Insularo</kap>
  <snc mrk="brit.B0aInsularo.GEOG">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      Insularo sidanta sur la nordokcidenta e&ubreve;ropa submara
      kontinentrando kaj konsistanta el la du precipaj insuloj <ref tip="prt" cel="briti.B0o.insulo">Britio</ref> kaj <ref tip="prt" cel="irland.I0o.insulo">Irlando</ref>,
      kaj la Manikinsuloj apud
      Francio, Manksinsulo kaj pliaj malgrandaj.
    </dif>
    <trd lng="zh">&#x4E0D;&#x5217;&#x985B;&#x7FA4;&#x5CF6; <pr>b&#xF9;li&#xE8;di&#x101;nq&#xFA;nd&#x1CE;o</pr></trd>
</snc>
  <trd lng="be">&c_B;&c_r;&c_y;&c_t;&c_a;&c_n;&c_s;&c_k;&c_ib;&c_ja; &c_a;&c_s;&c_t;&c_r;&c_a;&c_v;&c_y;</trd>
  <trd lng="de">Britische Inseln</trd>
  <trd lng="en">British Islands</trd>
  <trd lng="es">Islas Brit&aacute;nicas</trd>
  <trd lng="fr">Iles britanniques</trd>
  <trd lng="hu">Brit-szigetek</trd>
  <trd lng="pl">Wyspy Brytyjskie</trd>
  <trd lng="ru">&c_B;&c_r;&c_i;&c_t;&c_a;&c_n;&c_s;&c_k;&c_i;&c_je; &c_o;&c_s;&c_t;&c_r;&c_o;&c_v;&c_a;</trd>
</drv>
</art>
