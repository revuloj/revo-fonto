<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
    <rad>brit</rad>/o
    </kap>

    <drv mrk="brit.0o">
      <kap><tld/>o</kap>
      <snc mrk="brit.0o.HIS">
	<uzo tip="fak">HIS</uzo>
    	<dif>
          Ano de <ref tip="super" cel="kelt.0o">kelta</ref> popolo, kiu
          lo&gcirc;is en <ref cel="briti.B0o">Britio</ref> anta&ubreve;
          kaj dum la Romia konkero, kaj donis sian nomon al la insulo
          kaj la lando (<ref cel="brit.B0ujo"><tld lit="B"/>ujo</ref>):
          <ekz>
            la <tld/>oj parolis lingvon parencan al la nuna        
            kimra, kaj ilia teritorio inkluzivis, krom Anglio
            kaj Kimrio, anka&ubreve; kelkajn partojn de suda kaj
            centra Skotlando.
    	    <fnt>
    	    <vrk>Lingvoj en Skotlando</vrk>,
    	    <lok><url
 ref="http://fp.borderline.f9.co.uk/eas/En%20Esperanto/lingvoj_en_skotlando.htm"
                  >Esperanto Asocio de Skotlando</url></lok>
    	    </fnt>.
    	  </ekz>
    	</dif>
	<trd lng="en">Briton</trd>
        <trd lng="es">brit&aacute;nico</trd>
	<trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_t;</trd>
      </snc>
      <snc mrk="brit.0o.britujano">
    	<dif>
          <tld lit="B"/>ujano,
    	  <ref tip="super" cel="sxtat.0ano">&scirc;tatano</ref> de
    	  <ref tip="malprt" cel="brit.B0ujo"><tld lit="B"/>ujo</ref>:
    	  <ekz>
    	    la 6an de junio celebros <tld/>oj &dash; kune kun francoj, usonanoj,
    	    k.a. &dash; la 50an datrevenon de la alianca invado en Francion,
    	    kiu liberigis E&ubreve;ropon de sub nazia jugo, <tld/>oj jam celebris
    	    la 25an, la 40an, la 45an datrevenojn, &ccirc;ar al <tld/>oj pla&ccirc;as
    	    eventoj, kiuj memorigas pri militista sukceso
    	    <fnt>
    	    <aut>Pa&ubreve;lo GUBBINS</aut>:
    	    <vrk>Revivi&gcirc;as la fantomoj &dash; de 1944</vrk>,
    	    <bib>Monato</bib>, <lok>n-ro 05/1994</lok>
    	    </fnt>.
    	  </ekz>
    	</dif>
	<trd lng="en">Britisher</trd>
	<trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_je;&c_c;</trd>
      </snc>
      <trd lng="de">Brite</trd>
      <trd lng="pt">bret&atilde;o</trd>
      <trd lng="sk">Brit</trd>
    </drv>

    <drv mrk="brit.0a">
      <kap><tld/>a</kap>
      <snc mrk="brit.0a.HIS">
    	<dif>
          Rilantanta al <tld/>oj<sncref ref="brit.0o.HIS"/>:
          <ekz>
            la <tld/>a lingvo estas unu el la
            <ref tip="super" cel="kelt.0a.lingvo">keltaj lingvoj</ref>.
    	  </ekz>
    	</dif>
	<trd lng="en">Brittonic</trd>
	<trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
      </snc>
      <snc mrk="brit.0a.landa">
    	<dif>
    	  Rilantanta al <tld/>oj a&ubreve; ilia &scirc;tato:
    	  <ekz>
    	    mi havas nun la okazon saluti la <tld/>an popolon en &gcirc;ia granda
    	    &ccirc;efurbo
    	    <fnt><bib>Paroloj</bib>, <vrk>Parolado en la Guildhall (Londono)
    	    en la 21a de a&ubreve;gusto 1907</vrk></fnt>;
    	  </ekz>
    	  <ekz>
    	    neniu el ni forgesis kiel amike ni estis akceptitaj en la <tld/>aj
    	    urboj Folkestone kaj Dover
    	    <fnt><bib>Paroloj</bib>, <vrk>Parolado en la Guildhall (Londono)
    	    en la 21a de a&ubreve;gusto 1907</vrk></fnt>;
    	  </ekz>
    	  <ekz>
    	    <tld/>aj kaj usonaj aviadiloj neniige bombardis Dresdenon
    	    <fnt><aut>Stefan MAUL</aut>: <vrk>El mia vidpunkto</vrk>,
    	    <bib>Monato</bib>, <lok>n-ro 03/1995</lok></fnt>;
    	  </ekz>
    	  <ekz>
    	    la dato markis la finon ne nur de <tld/>a, sed anka&ubreve; de
    	    e&ubreve;ropa influo
    	    <fnt>
    	    <aut>Pa&ubreve;lo GUBBINS</aut>:
    	    <vrk>Revivi&gcirc;as la fantomoj &dash; de 1944</vrk>,
    	    <bib>Monato</bib>, <lok>n-ro 05/1994</lok>
    	    </fnt>.
    	  </ekz>
    	</dif>
	<trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
      </snc>
      <snc mrk="brit.0a.insula">
    	<dif>
    	  Rilata al la <ref cel="brit.B0aInsularo"><tld lit="B"/>a Insularo</ref>:
    	  <ekz>
    	    la <tld/>aj insuloj.
    	  </ekz>
    	</dif>
        <trd lng="es">brit&aacute;nico</trd>
	<trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
      </snc>
      <trd lng="de">britisch</trd>
      <trdgrp lng="pt">
    	<trd>bret&atilde;o</trd>,
    	<trd>ingl&ecirc;s</trd>
      </trdgrp>
      <trd lng="sk">britsk&yacute;</trd>
    </drv>

    <drv mrk="brit.B0ujo">
      <kap><tld lit="B"/>ujo</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
    	<dif>
    	  Insula <ref tip="super" cel="sxtat.0o">&scirc;tato</ref> situanta
    	  en okcidenta <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>,
    	  norde de Francujo kaj oriente de Irlando,
    	  &ccirc;irka&ubreve;ita de <ref tip="vid" cel="atlant.0o">Atlantiko</ref>
    	  kaj <ref tip="vid" cel="nord.N0aMaro">Norda Maro</ref>:
    	  <ekz>
    	    li revenis kun ri&ccirc;a militakira&jcirc;o de la Galla bordo, kie
    	    la popolo same kiel en <tld lit="B"/>ujo kun timego kantadis
    	    <ctl>Liberigu nin Dio de l' sova&gcirc;aj Normandoj</ctl>
    	    <fnt><bib>Fab4</bib>, <lok>Lasta son&gcirc;o de maljuna
    	    kverko</lok></fnt>;
    	  </ekz>
    	  <ekz>
    	    ni kongresas nun en glora universitata urbo
    	    de Granda <tld lit="B"/>ujo
    	    <fnt><bib>Paroloj</bib>, <vrk>Parolado anta&ubreve; la Tria Kongreso
    	    Esperantista en Cambridge en la 12a de a&ubreve;gusto 1907</vrk>
    	    </fnt>.
    	  </ekz>
    	</dif>
      </snc>
      <ref tip="sin" cel="briti.B0o.sxtato">Britio</ref>
      <trd lng="de">Gro&szlig;britannien</trd>
      <trd lng="en">Gran Breta&ntilde;a</trd>
      <trd lng="pt">Gr&atilde; Bretanha</trd>
      <trd lng="ru">&c_V;&c_je;&c_l;&c_i;&c_k;&c_o;&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_i;&c_ja;</trd>
      <trd lng="sk">Ve&lcaron;k&aacute; <ind>Brit&aacute;nia</ind></trd>
    </drv>

    <drv mrk="brit.0ujano">
      <kap><tld/>ujano</kap>
      <snc>
    	<dif>
          Lo&gcirc;anto de <tld lit="B"/>ujo,
          i.a. anglo, skoto, kimro.
    	</dif>
	<trd lng="en">Britisher</trd>
	<trd lng="ru">&c_b;&c_r;&c_i;&c_t;&c_a;&c_n;&c_je;&c_c;</trd>
      </snc>
      <trd lng="sk">Brit</trd>
    </drv>

    <drv mrk="brit.B0aInsularo">
      <kap><tld lit="B"/>a Insularo</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
    	<dif>
    	  Insularo sidanta sur la nordokcidenta e&ubreve;ropa
    	  submara kontinentrando kaj konsistanta el la du precipaj
    	  insuloj <ref tip="prt" cel="briti.B0o.insulo">Britio</ref>
    	  kaj <ref tip="prt" cel="irland.I0o.insulo">Irlando</ref>, kaj la
    	  la Manikinsuloj apud Francio, Manksinsulo kaj pliaj malgrandaj.
    	</dif>
      </snc>
      <trd lng="de">Britische Inseln</trd>
      <trd lng="es">Islas Brit&aacute;nicas</trd>
      <trd lng="ru">&c_B;&c_r;&c_i;&c_t;&c_a;&c_n;&c_s;&c_k;&c_i;&c_je; &c_o;&c_s;&c_t;&c_r;&c_o;&c_v;&c_a;</trd>
    </drv>
  </art>
