<drv mrk="lobeli.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="kampan3.0oj"><tld/>acoj</ref>
      (<trd lng="la">Lobelia</trd>), kies suko estas lakteca, akra kaj
      venena; pluraj specioj estas &gcirc;ardene kulturataj.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_a;&c_b;&c_e;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="de">Lobelie</trd>
  <trd lng="en">lobelia</trd>
  <trd lng="es">lobelia</trd>
  <trd lng="hu">lob&eacute;lia</trd>
  <trd lng="nl">lobelia</trd>
  <trd lng="pt">lob&eacute;lia</trd>
  <trd lng="ru">&c_l;&c_o;&c_b;&c_je;&c_l;&c_i;&c_ja;</trd>
</drv>
</art>
