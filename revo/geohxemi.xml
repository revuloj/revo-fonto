<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: geohxemi.xml,v 1.7 2022/04/30 08:42:40 revo Exp $">
<kap>
  <rad>geo&hcirc;emi</rad>/o <fnt><vrk><url ref="http://www.majstro.com/vortaroj/esperanto-germana/geo%C4%A5emio">majstro</url></vrk></fnt>,
  <var><kap><rad var="k">geokemi</rad>/o <fnt><bib>NePiv</bib></fnt></kap></var>
</kap>
<drv mrk="geohxemi.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <snc mrk="geohxemi.0o.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Apliko de <ref tip="super" cel="hxemi.0o.SCI">&hcirc;emio</ref>,
      al mineraloj, &scirc;tonoj, roka&jcirc;oj:    
      <ekz>
        por <tld var="k"/>o, la distribui&gcirc;o <frm><k>L:M</k></frm>, <frm><k>Sial:Sima</k></frm> k.s.
        estas unuaranga 
        indico de la substanca nekontinueco en la terkrusto
        <fnt>
          <aut>J. Kavka</aut>,
          <vrk><url ref="&STEB;geologio/magmo-rokajxoj/magmo-rokajxoj.htm">
          Perspektivoj por sistemigo de la magmoroka&jcirc;oj</url></vrk>,
          <lok>Scienca Revuo, 1978:1 (129), p. 9a-20a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_a;&c_h;&c_ib;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="de">Geochemie</trd>
  <trd lng="en">geochemistry</trd>
  <trd lng="fr">g&eacute;ochimie</trd>
  <trd lng="uk">&#x433;&#x435;&#x43E;&#x445;&#x456;&#x43C;&#x456;&#x44F;</trd>
</drv>

<drv mrk="geohxemi.0isto">
  <kap><tld/>isto, <var><kap><tld var="k"/>isto</kap></var></kap>
  <snc mrk="geohxemi.0isto.GEOL">
    <uzo tip="fak">GEOL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Specialisto pri <ref tip="vid" cel="geohxemi.0o.GEOL"><tld/>o</ref>:
      <ekz>
        <nom>Harmon Craig</nom> <klr> [&#x2026;]</klr> estis usona <tld/>isto
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Harmon_Craig">
          Harmon Craig</url>, 2012-08-13</lok>
        </fnt>;
      </ekz>    
      <ekz>
        <tld var="k"/>isto ofte bezonas ne nur kemiajn kaj geologiajn,
        sed anka&ubreve; matematikajn konojn
        <fnt><aut>E. Dudich</aut>, <vrk>&Ccirc;u vi Konas la Teron?</vrk>, 
        <lok>1983</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_a;&c_h;&c_ib;&c_m;&c_ib;&c_k;</trd>
  <trd lng="de">Geochemiker</trd>
  <trd lng="en">geochemist</trd>
  <trd lng="fr">g&eacute;ochimiste</trd>
</drv>
</art>

