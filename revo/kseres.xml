<vortaro>
<art mrk="$Id: kseres.xml,v 1.8 2019/10/13 18:10:19 revo Exp $">
<kap>
  <rad>kseres</rad>/o
</kap>

<drv mrk="kseres.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="kseres.0o.VIN">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="dif" cel="sxere.0o.VIN">&Scirc;ereo</ref>:
      <ekz>
        la preska&ubreve; nigra <tld/>o <klr>[&#8230;]</klr> odoris kupron
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 20a, p.
        122a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Sherry</trd>
  <trd lng="fr">x&eacute;r&egrave;s</trd>
  <trdgrp lng="nl">
    <trd>sherry<klr tip="ind"> (arch.)</klr></trd>,
    <trd>xeres<klr tip="ind"> (arch.)</klr></trd>
  </trdgrp>
  <trd lng="pl">xeres <klr>(wino)</klr></trd>
  <trd lng="pt">xerez</trd>
  <trd lng="ro">xeres <klr>(vin)</klr></trd>
</drv>
</art>

