<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gam</rad>/o
</kap>
<drv mrk="gam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gam.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Orda, sinsekva a&ubreve;digo de &ccirc;iuj sonoj de tonalo,
      ordinare la&ubreve; tuta okta amplekso.
    </dif>
    <trdgrp lng="nl">
      <trd>toonladder</trd>
    </trdgrp>
  </snc>
  <snc mrk="gam.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <ref tip="dif" cel="seri.0o.aroghen">serio</ref>
    <ekz>
      aliflanke de la <tld/>o triumfis anka&ubreve; la socialisma maldekstra
      partio)<fnt><aut>Baard Hekland</aut>: <vrk>Triumfas tedulo</vrk>,
      <bib>Monato</bib><lok>jaro 2001a, numero 11a, p. 6a</lok></fnt>.
    </ekz>
    <trdgrp lng="nl">
      <trd>naar sterkte, intensiteit enz. geordende <ind>reeks</ind></trd>,
      <trd>muziekgamma</trd>
    </trdgrp>
  </snc>
  <trd lng="de">Tonleiter</trd>
  <trd lng="fr">gamme</trd>
  <trd lng="hu">sk&aacute;la</trd>
  <trdgrp lng="nl">
    <trd>gamma</trd>
  </trdgrp>
  <trd lng="pt">gama</trd>
  <trd lng="ru">&c_g;&c_a;&c_m;&c_m;&c_a;</trd>
</drv>
</art>
