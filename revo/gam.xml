<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>gam</rad>/o</kap>
<drv mrk="gam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Serio de la sep muzikaj notoj vicigitaj la&ubreve; sia natura
      ordo en la intertempo de unu oktavo.
    </dif>
  </snc>
  <trd lng="de">Tonleiter</trd>
  <trd lng="fr">gamme</trd>
  <trdgrp lng="nl">
    <trd>toonladder</trd>,
    <trd>gamma</trd>,
    <trd>muziekgamma</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_a;&c_m;&c_m;&c_a;</trd>
</drv>
</art>
