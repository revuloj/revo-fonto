<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kompat</rad>/i
</kap>
<drv mrk="kompat.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Senti kortu&scirc;on pro ies malfeli&ccirc;o kaj deziron &gcirc;in malakrigi:
      <ekz><tld/>i iun<fnt>Z</fnt></ekz>
      <ekz>al iu, ies doloron;</ekz>
      <ekz>
        pli feli&ccirc;a estas martelo insultata ol amboso <tld/>ata<fnt>Z</fnt>;
      </ekz>
      <ekz>
        virtulo <tld/>as (indulgas, &scirc;paras) la vivon de sia bruto<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="kondol.0i">kondolenci</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>pity</trd>,
    <trd>have <ind>compassion</ind> for</trd>,
    <trd>feel <ind>compassion</ind> for</trd>,
    <trd>have <ind>pity</ind> for</trd>,
    <trd>have <ind>mercy</ind> on</trd>,
    <trd>be <ind>sorry</ind> for</trd>,  
    <trd>commiserate with</trd>
  </trdgrp>
  <trd lng="nl">medelijden hebben met</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_l;&c_je;&c_t;&c_mol; <klr>(&c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="kompat.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sento de <tld/>anto:
      <ekz>
        mi ne volas baton, mi ne volas <tld/>on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        rideto de <tld/>o<fnt>Z</fnt>
        <klr>(mal&scirc;ata rido)</klr>;
      </ekz>
      <ekz>eksenti <tld/>on al iu<fnt>Z</fnt>;</ekz>
      <ekz>granda <tld/>o moligis ilian koron</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kor.0o">koro 2</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>compassion</trd>,
    <trd>mercy</trd>,
    <trd>pity</trd>,
    <trd>sympathy</trd>
  </trdgrp>
  <trd lng="nl">medelijden</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="kompat.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Inklina al <tld/>o 
    </dif>
    <refgrp tip="sin">
      <ref cel="kor.bon0a">bonkora</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>compassionate</trd>,
    <trd>sympathetic</trd>,
    <trd>tenderhearted</trd>
  </trdgrp>
  <trd lng="nl">barmhartig</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="kompat.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari, ke iu <tld/>u al vi:
      <ekz>pli bone estas enviigi ol <tld/>igi<fnt>Z</fnt>.</ekz> 
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>move to <ind>compassion</ind></trd>,
    <trd>incite to <ind>pity</ind></trd>
  </trdgrp>
  <trd lng="nl">medelijden opwekken</trd>
</drv>
<drv mrk="kompat.0inda">
  <kap><tld/>inda<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Meritanta <tld/>on; malfeli&ccirc;a:
      <ekz>&gcirc;i estas vera <tld/>inda&jcirc;o<fnt>Z</fnt>.</ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>deplorable</trd>,
    <trd>miserable</trd>,
    <trd>piteable</trd>,
    <trd>poor</trd>
  </trdgrp>
  <trd lng="nl">beklagenswaardig</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_l;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_d;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j; &c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
<drv mrk="kompat.mal0ema">
  <kap>sen<tld/>a<fnt>Z</fnt>, mal<tld/>ema</kap>
  <snc>
    <dif>
      Ne inklina al <tld/>o pro fiereco kaj egoismo:
      <ekz>la sen<tld/>a morto diris sian kruelan vorton<fnt>Z</fnt></ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kor.sen0a">senkora</ref>,
      <ref cel="kruel.0a">kruela</ref>,
      <ref cel="sever.0a">severa</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>hard-hearted</trd>,
    <trd>hard</trd>,
    <trd>harsh</trd>,
    <trd>implacable</trd>,
    <trd>merciless</trd>,
    <trd>pitiless</trd>,
    <trd>ruthless</trd>,
    <trd>uncompassionate</trd>, 
    <trd>unfeeling</trd>
  </trdgrp>
  <trd lng="nl">onbarmhartig</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>
</art>
