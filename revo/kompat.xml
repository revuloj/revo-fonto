  <trdgrp lng="sk">
    <trd>chud&aacute;k</trd>,
    <trd>&uacute;bo&zcaron;iak</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_ib;&c_d;&c_o;&c_l;&c_a;&c_h;&c_a;</trd>,
    <trd>&c_b;&c_ib;&c_d;&c_n;&c_ja;&c_k;</trd>,
    <trd>&c_b;&c_ib;&c_d;&c_n;&c_ja;&c_g;&c_a;</trd>,
    <trd>&c_b;&c_ib;&c_d;&c_a;&c_ch;&c_i;&c_s;&c_mol;&c_k;&c_o;</trd>,
    <trd>&c_n;&c_je;&c_b;&c_o;&c_g;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kompat.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kompat.ek0i.PSI">
    <dif>
      Eksenti <tld/>on:
      <ekz>
        la Eternulo ek<tld/>os Jakobon, kaj denove elektos Izraelon,
        kaj relo&gcirc;igos ilin en ilia lando
        <fnt><bib>MT</bib><lok>&Jes; 14:1</lok></fnt>;
      </ekz>
      <ekz>
        seki&gcirc;u la mano de tiu, kiu kurus helpi lin, ek<tld/>inte
        lian mizeron
        <fnt><bib>Far1</bib><lok>&ccirc;ap. 25</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_sh;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_l;&c_ib;&c_t;&c_a;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>apitoyer</ind> (de, sur)</trd>,
    <trd>s'<ind>attendrir</ind> (sur)</trd>,
    <trd>prendre en <ind>piti&eacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megsajn&aacute;l</trd>,
    <trd>megsz&aacute;n</trd>,
    <trd>megk&ouml;ny&ouml;r&uuml;l</trd>
  </trdgrp>
  <trd lng="nl"><ind>medelijden</ind> krijgen met</trd>
  <trd lng="pl">rozczula&cacute;</trd>
  <trd lng="ro">agita&tcomma;ie</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_t;&c_mol;<klr tip="amb">
  (&c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>
</drv>

<drv mrk="kompat.mal0a">
  <kap>mal<tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kompat.mal0a.PSI">
    <dif>
      <ref tip="dif" cel="kompat.sen0a">Sen<tld/>a</ref>:
      <ekz>
        morta dan&gcirc;ero minacas lin de malkompataj malamikoj
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 14</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_z;&c_mol;&c_l;&c_ib;&c_t;&c_a;&c_s;&c_n;&c_y;</trd>
  <trd lng="cs">nel&iacute;tostn&yacute;</trd>
  <trdgrp lng="fr">
    <trd>implacable</trd>,
    <trd>intraitable<klr tip="ind"> (impitoyable)</klr></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>irgalmatlan</trd>,
    <trd>kegyetlen</trd>
  </trdgrp>
  <trd lng="nl">onbarmhartig</trd>
  <trdgrp lng="pl">
    <trd>bezlitosny</trd>,
    <trd>okrutny</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>nemilos</trd>,
    <trd>crud</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>bezcitn&yacute;</trd>,
    <trd>ne&lcaron;&uacute;tostn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E25;&#x9177; <pr>y&#xE1;nk&#xF9;</pr></trd>,
    <trd>&#x6740;&#x4EBA;&#x4E0D;&#x7728;&#x773C; <pr>sh&#x101;r&#xE9;nb&#xF9;zh&#x1CE;y&#x1CE;n</pr></trd>,
    <trd>&#x65E0;&#x60C5; <pr>w&#xFA;q&#xED;ng</pr></trd>,
    <trd>&#x72E0;&#x7D55; <pr>h&#x11B;nju&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kompat.mem0o">
  <kap>mem<tld/>o</kap>
  <snc mrk="kompat.mem0o.PSI">
    <dif>
      <tld lit="K"/>o al si mem:
      <ekz>
        li estis plena je mem<tld/>o kaj alte taksis la okazon malkovri
        &ccirc;ion al a&ubreve;skultanto
        <fnt><bib>&Scirc;talrato</bib>, <lok>&ccirc;ap. 2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_m;&c_a;&c_sh;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="cs">sebel&iacute;tost</trd>
  <trd lng="de">Selbstmitleid</trd>
  <trd lng="fr">apitoiement sur soi</trd>
  <trd lng="hu">&ouml;nsajn&aacute;lat</trd>
  <trd lng="nl">zelfbeklag</trd>
  <trdgrp lng="pl">
    <trd>rozczulenie</trd>,
    <trd>u&zdot;alanie si&eogonek; nad sob&aogonek;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>autocomp&abreve;timire</trd>,
    <trd>sensibilitate</trd>
  </trdgrp>
  <trd lng="ru">&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_mol; &c_k; &c_s;&c_je;&c_b;&c_je;</trd>
  <trd lng="sk">seba&lcaron;&uacute;tos&tcaron;</trd>
  <trd lng="zh">&#x81EA;&#x601C; <pr>z&#xEC;li&#xE1;n</pr></trd>
</drv>

<drv mrk="kompat.sen0a">
  <kap>sen<tld/>a,
  <var><kap>mal<tld/>ema</kap></var></kap>
  <snc mrk="kompat.sen0a.PSI">
    <dif>
      Ne inklina al <tld/>o,
      <ref tip="vid" cel="kor.sen0a">senkora</ref>,
      <ref tip="vid" cel="kruel.0a">kruela</ref>,
      <ref tip="vid" cel="sever.0a">severa</ref>;
      <ref tip="sin" cel="kompat.mal0a">mal<tld/>a</ref>:
      <ekz>
        &ccirc;iam la infanaj bu&scirc;oj parolis sen<tld/>ajn vortojn pri &scirc;i
        <fnt><bib>Fab4</bib>, <lok>Knabino kiu pa&scirc;is sur
        panon</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lian kapon, sur&scirc;utitan per la cindro, <ind>sen<tld/>e</ind>
        bruligis
        la suno
        <fnt><bib>Far2</bib>, <lok>&ccirc;ap. 21</lok></fnt>;
        <trdgrp lng="fr">
          <trd>impitoyablement</trd>,
          <trd>implacablement</trd>,
          <trd>sans <ind>piti&eacute;</ind></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>k&ouml;ny&ouml;rtelen&uuml;l</trd>,
          <trd>k&iacute;m&eacute;letlen&uuml;l</trd>
        </trdgrp>
        <trd lng="nl">onbarmhartig<klr tip="ind"> (bw.)</klr></trd>
      </ekz>
      <ekz>
        sen iu azilo, en kiu mi povintus forsavi min de miaj
        mal<tld/>emaj persekutantoj
        <fnt><bib>Revadoj</bib>, <lok>Tria promenado</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_z;&c_mol;&c_l;&c_ib;&c_t;&c_a;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_ib;&c_b;&c_je;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bezcitn&yacute;</trd>,
    <trd>krut&yacute;</trd>,
    <trd>nel&iacute;tostn&yacute;</trd>,
    <trd>nemilosrdn&yacute;</trd>,
    <trd>ne&uacute;prosn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hard-hearted</trd>,
    <trd>hard</trd>,
    <trd>harsh</trd>,
    <trd>implacable</trd>,
    <trd>merciless</trd>,
    <trd>pitiless</trd>,
    <trd>ruthless</trd>,
    <trd>uncompassionate</trd>,
    <trd>unfeeling</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>impitoyable</trd>,
    <trd>qui ne se laisse pas <ind>apitoyer</ind></trd>,
    <trd>inflexible<klr tip="ind"> (impitoyable)</klr></trd>,
    <trd>intransigeant<klr tip="ind"> (impitoyable)</klr></trd>,
    <trd>sans <ind>piti&eacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;ny&ouml;rtelen</trd>,
    <trd>k&iacute;m&eacute;letlen</trd>,
    <trd>sz&iacute;vtelen</trd>,
    <trd>irgalmatlan</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x7121;&#x6148;&#x60B2;&#x306A;  <pr>&#x3080;&#x3058;&#x3072;&#x306A;</pr></trd>,
    <trd>&#x8584;&#x60C5;&#x306A;  <pr>&#x306F;&#x304F;&#x3058;&#x3087;&#x3046;&#x306A;</pr></trd>,
    <trd>&#x60C5;&#x3051;&#x5BB9;&#x8D66;&#x306A;&#x3044;  <pr>&#x306A;&#x3055;&#x3051;&#x3088;&#x3046;&#x3057;&#x3083;&#x306A;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">onbarmhartig</trd>
  <trd lng="pl">bezlitosny</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>bezcitn&yacute;</trd>,
    <trd>nemilosrdn&yacute;</trd>,
    <trd>ne&lcaron;&uacute;tostn&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_je;&c_z;&c_zh;&c_a;&c_l;&c_ib;&c_s;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_s;&c_je;&c_r;&c_d;&c_je;&c_ch;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_o;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x65E0;&#x60C5; <pr>w&#xFA;q&#xED;ng</pr></trd>,
    <trd>&#x4E25;&#x9177; <pr>y&#xE1;nk&#xF9;</pr></trd>,
    <trd>&#x6740;&#x4EBA;&#x4E0D;&#x7728;&#x773C; <pr>sh&#x101;r&#xE9;nb&#xF9;zh&#x1CE;y&#x1CE;n</pr></trd>,
    <trd>&#x72E0;&#x7D55; <pr>h&#x11B;nju&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kompat.sen0eco">
  <kap>sen<tld/>eco,
  <var><kap>mal<tld/>emo</kap></var></kap>
  <snc mrk="kompat.sen0eco.PSI">
    <dif>
      Eco de iu sen<tld/>a:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la rigida kolumo de la &ccirc;emizo kun ordinara sen<tld/>eco
        sin apogis je razita mentono
        <fnt>
        <aut>I. Turgenjev, trad. K. Bein</aut>
        <vrk>Patroj kaj filoj</vrk>
        <lok>&ccirc;ap. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <klr>[la diskutoj]</klr> oni notas la vivkarecon,
        senlaborecon,
        mal<tld/>emon de la entreprenistoj
        <fnt><bib>Metrop</bib>, <lok>&ccirc;ap. 3</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_z;&c_mol;&c_l;&c_ib;&c_t;&c_a;&c_s;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_zh;&c_a;&c_l;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>absence de <ind>piti&eacute;</ind></trd>,
    <trd>absence de <ind>compassion</ind></trd>,
    <trd>inflexibilit&eacute;<klr tip="ind"> (absence de piti&eacute;)</klr></trd>,
    <trd>implacabilit&eacute;<klr tip="ind"> (absence de piti&eacute;)</klr></trd>,
    <trd>intransigeance<klr tip="ind"> (absence de piti&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;ny&ouml;rtelens&eacute;g</trd>,
    <trd>irgalmatlans&aacute;g</trd>,
    <trd>k&iacute;m&eacute;letlens&eacute;g</trd>,
    <trd>sz&iacute;vtelens&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">onbarmhartigheid</trd>
  <trdgrp lng="pl">
    <trd>bezlitosno&sacute;&cacute;</trd>,
    <trd>okrutno&sacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd> inflexibilitate</trd>,
    <trd>cruzime</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
