<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kompat</rad>/i
</kap>
<drv mrk="kompat.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      Senti kortu&scirc;on pro ies malfeli&ccirc;o kaj deziron &gcirc;in malakrigi:
      <ekz>
        <tld/>i iun<fnt>Z</fnt>,
      </ekz>
      <ekz>
        al iu,
      </ekz>
      <ekz>
        ies doloron;
      </ekz>
      <ekz>
        <ind>pli feli&ccirc;a estas martelo insultata, ol amboso
        <tld/>ata</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">jobb sz&aacute;z irigy, mint egy
        <ind>sz&aacute;n&oacute;</ind></trd>
      </ekz>
      <ekz>
        virtulo <tld/>as <klr>(indulgas, &scirc;paras)</klr> la vivon de sia
        bruto
        <fnt><bib>MT</bib><lok>&Sen; 12:10</lok></fnt>;
      </ekz>
      <ekz>
        Mi etendas Mian manon sur vin kaj pereigas vin
        <klr>[, &ccirc;ar]</klr> Mi laci&gcirc;is de
        <ind><tld/>ado</ind>
        <fnt><bib>MT</bib><lok>&Jer; 15:6</lok></fnt>;
        <trdgrp lng="fr">
          <trd>apitoiement</trd>,
          <trd>attendrissement<klr tip="ind"> (apitoiement)</klr></trd>,
          <trd>commis&eacute;ration</trd>
        </trdgrp>
        <trd lng="hu">sz&aacute;nakoz&aacute;s</trd>
        <trdgrp lng="nl">
          <trd>medelijden</trd>,
          <trd>compassie</trd>
        </trdgrp>
      </ekz>
      <ekz>
        ne trovi&gcirc;u <tld/>anto por liaj orfoj
        <fnt><bib>MT</bib><lok>&Psa; 109:12</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kondol.0i">kondolenci</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_g;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bemitleiden</trd>,
    <trd>bedauern</trd>,
    <trd>Mitleid haben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pity</trd>,
    <trd>have <ind>compassion</ind> for</trd>,
    <trd>feel <ind>compassion</ind> for</trd>,
    <trd>have <ind>pity</ind> for</trd>,
    <trd>have <ind>mercy</ind> on</trd>,
    <trd>be <ind>sorry</ind> for</trd>,
    <trd>commiserate with</trd>
  </trdgrp>
  <trd lng="es">compadecer</trd>
  <trdgrp lng="fr">
    <trd>plaindre<klr tip="ind"> (avoir de la compassion)</klr></trd>,
    <trd>s'<ind>apitoyer</ind> (de, sur)</trd>,
    <trd>s'<ind>attendrir</ind> (sur)</trd>,
    <trd>avoir de la <ind>compassion</ind> (pour)</trd>,
    <trd>compatir (&agrave;)</trd>,
    <trd>avoir <ind>piti&eacute;</ind> (de)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megsz&aacute;n</trd>,
    <trd>sajn&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>medelijden</ind> hebben met</trd>,
    <trd><ind>compassie</ind> hebben met</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>compadecer-se</trd>,
    <trd>lastimar</trd>,
    <trd>ter <ind>compaix&atilde;o</ind></trd>,
    <trd>apiedar-se de</trd>,
    <trd>condoer-se de</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_l;&c_je;&c_t;&c_mol; <klr>(&c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="kompat.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sento de <tld/>anto:
      <ekz>
        mi ne volas baton, mi ne volas <tld/>on
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>eksenti <tld/>on al</ind> iu<fnt>Z</fnt>;
        <trdgrp lng="fr">
          <trd>s'<ind>apitoyer</ind> (de, sur)</trd>,
          <trd>s'<ind>attendrir</ind> (sur)</trd>,
          <trd>prendre <ind>piti&eacute;</ind> (de)</trd>
        </trdgrp>
        <trd lng="hu">megsz&aacute;n</trd>
        <trd lng="nl"><ind>medelijden</ind> voelen</trd>
      </ekz>
      <ekz>
        granda <tld/>o moligis ilian koron;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>rideto de <tld/>o</ind><fnt>Z</fnt>
        <klr>(afable malestima)</klr>;
        <trd lng="fr">sourire <ind>condescendant</ind></trd>
        <trd lng="hu">sz&aacute;nakoz&oacute; mosoly</trd>
        <trd lng="nl"><ind>minachten</ind>de glimlach</trd>
      </ekz>
      <ekz>
        <ind><tld/>plena</ind> Dio! donu Vi al &scirc;i la tomban ripozon,
        kiun mi ne povis havigi al &scirc;i!
        <fnt><bib>Fab3</bib><lok>Historio</lok></fnt>;
        <trdgrp lng="fr">
          <trd>compatissant</trd>,
          <trd>bon<klr tip="ind"> (enclin &agrave; la compassion)</klr></trd>,
          <trd>bienveillant</trd>,
          <trd>humain<klr tip="ind"> (enclin &agrave; la
          compassion)</klr></trd>,
          <trd>mis&eacute;ricordieux</trd>,
          <trd>pitoyable<klr tip="ind"> (enclin &agrave; la
          piti&eacute;)</klr></trd>,
          <trd>secourable</trd>,
          <trd>sensible<klr tip="ind"> (enclin &agrave; la
          compassion)</klr></trd>
        </trdgrp>
        <trd lng="hu">k&ouml;ny&ouml;r&uuml;letes</trd>
        <trdgrp lng="nl">
          <trd>genadig</trd>,
          <trd>goedertieren</trd>
        </trdgrp>
      </ekz>
      <ekz>
        li do aliris, unu post alia, la personojn, pri kies sinteno
        li dubis, proponante al ili siajn fruktojn, la&ubreve; la sama
        plenda, <ind><tld/>veka</ind> maniero
        <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 14</lok></fnt>.
        <trdgrp lng="fr">
          <trd>plaintif</trd>,
          <trd>attendrissant<klr tip="ind"> (qui inspire
          compassion)</klr></trd>,
          <trd>geignard<klr tip="ind"> (cherchant &agrave; inspirer
          compassion)</klr></trd>,
          <trd>touchant<klr tip="ind"> (qui inspire
          compassion)</klr></trd>
        </trdgrp>
        <trd lng="hu">sz&aacute;nalomgerjeszt&odblac;</trd>
        <trdgrp lng="nl">
          <trd>aandoenlijk</trd>,
          <trd>roerend</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kor.0o.FIG">koro <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_g;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mitleid</trd>,
    <trd>Erbarmen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>compassion</trd>,
    <trd>mercy</trd>,
    <trd>pity</trd>,
    <trd>sympathy</trd>
  </trdgrp>
  <trd lng="es">compasi&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>compassion</trd>,
    <trd>apitoiement</trd>,
    <trd>attendrissement<klr tip="ind"> (apitoiement)</klr></trd>,
    <trd>commis&eacute;ration</trd>,
    <trd>piti&eacute;</trd>,
    <trd>sympathie<klr tip="ind"> (compassion)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;nalom</trd>,
    <trd>k&ouml;ny&ouml;r&uuml;let</trd>,
    <trd>sajn&aacute;lat</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>medelijden</trd>,
    <trd>compassie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="kompat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Montranta <tld/>on, rilata al <tld/>o:
      <ekz>
        mi a&ubreve;das <tld/>ajn vortojn de virino, kiun mi tiel
        longe kaj tiel pasie amas
        <fnt><bib>FK</bib><lok>Arturo</lok></fnt>;
      </ekz>
      <ekz>
        vidante tiujn eluzitajn mantelojn kaj tiujn viza&gcirc;ojn, mizerajn
        de malsato, denove mi spertis kvaza&ubreve; <tld/>an senton
        <fnt>
        <aut>H. Sienkiewicz trad. L. Zamenhof</aut>
        <vrk>Quo vadis?</vrk>
        <lok>&ccirc;ap. 28</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne toleras, se knabo <tld/>e min rigardas
        <fnt>
        <aut>trad. K. Bein</aut>
        <vrk>Internacia Krestomatio</vrk>
        <lok>Patro</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>compatissant</trd>,
    <trd>bon<klr tip="ind"> (montrant la compassion)</klr></trd>,
    <trd>bienveillant</trd>,
    <trd>de <ind>compassion</ind></trd>,
    <trd>humain<klr tip="ind"> (montrant la compassion)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;nakoz&oacute;</trd>,
    <trd>sajn&aacute;lkoz&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>meewarig</trd>,
    <trd>deelnemend</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="kompat.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="stl">ARK</uzo>
  <snc>
    <ref tip="dif" cel="kompat.0emo"><tld/>emo</ref>
    <ekz>
      Dio, estante ri&ccirc;a en <tld/>eco, <klr>[...]</klr>
      nin vivigis en Kristo
      <fnt><bib>NT</bib><lok>&Efe; 2:4</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="hu">
    <trd>k&ouml;ny&ouml;r&uuml;letess&eacute;g</trd>,
    <trd>irgalmass&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>compassie<klr tip="ind"> (arch.)</klr></trd>,
    <trd>medelijden<klr tip="ind"> (arch.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="kompat.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Inklina al <tld/>o:
      <ekz>
        kiam la faraonoj jam unu fojon montris sian potencon,
        ili povas esti <tld/>emaj
        <fnt><bib>Far2</bib><lok>&ccirc;ap. 24</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &scirc;i iris al la homo, kies <tld/>ema mano jam donis al
        &scirc;i unu fojon laboron, la duan fojon almozon
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kor.bon0a">bonkora</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_l;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_g;&c_a;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>mitleidig</trd>,
    <trd>barmherzig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>compassionate</trd>,
    <trd>sympathetic</trd>,
    <trd>tenderhearted</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>compatissant</trd>,
    <trd>bon<klr tip="ind"> (enclin &agrave; la compassion)</klr></trd>,
    <trd>bienveillant</trd>,
    <trd>humain<klr tip="ind"> (enclin &agrave; la compassion)</klr></trd>,
    <trd>mis&eacute;ricordieux</trd>,
    <trd>pitoyable<klr tip="ind"> (enclin &agrave; la piti&eacute;)</klr></trd>,
    <trd>secourable</trd>,
    <trd>sensible<klr tip="ind"> (enclin &agrave; la compassion)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;ny&ouml;r&uuml;letes</trd>,
    <trd>irgalmas</trd>,
    <trd>sz&aacute;nakoz&oacute;</trd>
  </trdgrp>
  <trd lng="nl">barmhartig</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_je;&c_r;&c_d;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="kompat.0emo">
  <kap><tld/>emo</kap>
  <snc>
    <dif>
      Eco de iu <tld/>ema:
      <ekz>
        la Sinjoro, nia Dio, havas <tld/>emon kaj pardonemon,
        kvankam ni defalis de Li
        <fnt><bib>MT</bib><lok>&Dan; 9:9</lok></fnt>;
      </ekz>
      <ekz>
        li komencis konsideri, &ccirc;u efektive tiu boneco kaj <tld/>emo
        de la kristanoj ne pruvas malta&ubreve;gecon de iliaj animoj
        <fnt>
        <aut>H. Sienkiewicz trad. L. Zamenhof</aut>
        <vrk>Quo vadis?</vrk>
        <lok>&ccirc;ap. 29</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>mis&eacute;ricorde</trd>,
    <trd>compassion</trd>,
    <trd>commis&eacute;ration</trd>,
    <trd>piti&eacute;</trd>,
    <trd>sympathie<klr tip="ind"> (compassion)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;ny&ouml;r&uuml;letess&eacute;g</trd>,
    <trd>irgalmass&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">barmhartigheid</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_je;&c_r;&c_d;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="kompat.0emeco">
  <kap><tld/>emeco</kap>
  <snc>
    <ref tip="dif" cel="kompat.0emo"><tld/>emo</ref>
    <ekz>
      la&ubreve; Via granda <tld/>emeco elstreku miajn pekojn
      <fnt><bib>MT</bib><lok>&Psa; 51:1</lok></fnt>;
    </ekz>
    <ekz>
      lia ago koncerne tiun virinon ne povis esti rigardata kiel malbona
      atesto pri la kvanto da boneco kaj <tld/>emeco trovi&gcirc;anta en lia
      koro
      <fnt><bib>Marta</bib></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>mis&eacute;ricorde</trd>,
    <trd>compassion</trd>,
    <trd>commis&eacute;ration</trd>,
    <trd>piti&eacute;</trd>,
    <trd>sympathie<klr tip="ind"> (compassion)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;ny&ouml;r&uuml;letess&eacute;g</trd>,
    <trd>irgalmass&aacute;g</trd>
  </trdgrp>
  <trd lng="nl">barmhartigheid</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>,
    <trd>&c_m;&c_i;&c_l;&c_o;&c_s;&c_je;&c_r;&c_d;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="kompat.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Fari, ke iu <tld/>u al vi:
      <ekz>
        <ind>pli bone estas enviigi ol <tld/>igi</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">mieux vaut l'envie que la <ind>piti&eacute;</ind></trd>
        <trd lng="hu">ink&aacute;bb irigyeljenek, mint sajn&aacute;ljanak</trd>
      </ekz>
    </dif>
    <ref tip="vid" cel="plend.0i">plendi</ref>
  </snc>
  <adm>
    Kontroli la ekz-on en PrV. Laux mia elektronika versio
    temas pri "enviigxi" k "kompatigxi".
    <aut>MB</aut>
  </adm>
  <trdgrp lng="en">
    <trd>move to <ind>compassion</ind></trd>,
    <trd>incite to <ind>pity</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>faire <ind>piti&eacute;</ind> (&agrave;)</trd>,
    <trd>apitoyer</trd>,
    <trd>attendrir<klr tip="ind"> (apitoyer)</klr></trd>,
    <trd>&eacute;mouvoir<klr tip="ind"> (apitoyer)</klr></trd>,
    <trd>toucher<klr tip="ind"> (apitoyer)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_zh;&c_a;&c_l;&c_o;&c_b;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_o;&c_n;&c_u;&c_t;&c_mol;<klr tip="ind">
    (&c_r;&c_a;&c_z;&c_zh;&c_a;&c_l;&c_o;&c_b;&c_i;&c_t;&c_mol;)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>medelijden</ind> opwekken</trd>
    <trd>vertederen</trd>,
    <trd>vermurwen</trd>
  </trdgrp>
</drv>

<drv mrk="kompat.0inda">
  <kap><tld/>inda</kap>
  <snc>
    <dif>
      Inda je <tld/>o:
      <ekz>
        miaj <tld/>indaj floroj tute forvelkis!
        <fnt><bib>Fab1</bib><lok>Floroj de la malgranda Ida</lok></fnt>;
      </ekz>
      <ekz>
        kiu vidis la viza&gcirc;on de Afrodito, ne estas <tld/>inda
        <fnt>
        <aut>H. Sienkiewicz trad. L. Zamenhof</aut>
        <vrk>Quo vadis?</vrk>
        <lok>&ccirc;ap. 50</lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas vera <tld/>inda&jcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kion do &scirc;i volas ankora&ubreve; de tiu <ind><tld/>indulo</ind>,
        li ja plu ne vivas!
        <fnt><bib>Marta</bib></fnt>;
        <trdgrp lng="fr">
          <trd>pauvre<klr tip="ind"> (subst., personne
          pitoyable)</klr></trd>,
          <trd>pauvre <ind>h&egrave;re</ind></trd>,
          <trd>malheureux<klr tip="ind"> (subst., personne
          pitoyable)</klr></trd>,
          <trd>mis&eacute;rable<klr tip="ind"> (subst., personne
          pitoyable)</klr></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>szerencs&eacute;tlen ember</trd>,
          <trd>sajn&aacute;latra m&eacute;lt&oacute; ember</trd>
        </trdgrp>
        <trd lng="nl">stakker</trd>
      </ekz>
      <ekz>
        li e&ccirc; ne divenas, <tld/>indulo, ke li estas jam
        pli ombro ol homo
        <fnt>
        <aut>H. Sienkiewicz trad. L. Zamenhof</aut>
        <vrk>Quo vadis?</vrk><lok>&ccirc;ap. 15</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="felicx.mal0a">malfeli&ccirc;a</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_l;&c_a;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_t;&c_y; <ind>&c_zh;&c_a;&c_l;&c_ju;</ind></trd>,
    <trd>&c_b;&c_je;&c_d;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>deplorable</trd>,
    <trd>miserable</trd>,
    <trd>piteable</trd>,
    <trd>poor</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>pauvre<klr tip="ind"> (pitoyable)</klr></trd>,
    <trd>affligeant</trd>,
    <trd>infortun&eacute;</trd>,
    <trd>lamentable</trd>,
    <trd>malheureux<klr tip="ind"> (pitoyable)</klr></trd>,
    <trd>mis&eacute;rable<klr tip="ind"> (pitoyable)</klr></trd>,
    <trd>piteux</trd>,
    <trd>qui inspire <ind>piti&eacute;</ind></trd>,
    <trd>pitoyable<klr tip="ind"> (qui inspire piti&eacute;)</klr></trd>,
    <trd>&agrave; <ind>plaindre</ind></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sajn&aacute;latra m&eacute;lt&oacute;</trd>,
    <trd>sz&aacute;nand&oacute;</trd>
    <trd>szerencs&eacute;tlen</trd>,
    <trd>szeg&eacute;ny <klr tip="ind">(sz&aacute;nand&oacute;)</klr></trd>
  </trdgrp>
  <trd lng="nl">beklagenswaardig</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_l;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_je;&c_d;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j;
    &c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_ja;)</klr></trd>,
    <trd>&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j; <ind>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_ja;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="kompat.ek0i">
  <kap>ek<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Eksenti <tld/>on:
      <ekz>
        la Eternulo ek<tld/>os Jakobon, kaj denove elektos Izraelon,
        kaj relo&gcirc;igos ilin en ilia lando
        <fnt><bib>MT</bib><lok>&Jes; 14:1</lok></fnt>;
      </ekz>
      <ekz>
        seki&gcirc;u la mano de tiu, kiu kurus helpi lin, ek<tld/>inte
        lian mizeron
        <fnt><bib>Far1</bib><lok>&ccirc;ap. 25</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>s'<ind>apitoyer</ind> (de, sur)</trd>,
    <trd>s'<ind>attendrir</ind> (sur)</trd>,
    <trd>prendre <ind>piti&eacute;</ind> (de)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megsajn&aacute;l</trd>,
    <trd>megsz&aacute;n</trd>,
    <trd>megk&ouml;ny&ouml;r&uuml;l</trd>
  </trdgrp>
  <trd lng="nl"><ind>medelijden</ind> krijgen met</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_t;&c_mol;<klr tip="amb">
  (&c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>
</drv>

<drv mrk="kompat.mal0a">
  <kap>mal<tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="kompat.sen0a">sen<tld/>a</ref>
    <ekz>
      morta dan&gcirc;ero minacas lin de malkompataj malamikoj
      <fnt><bib>I&scirc;tar</bib><lok>&ccirc;ap. 14</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="hu">
    <trd>irgalmatlan</trd>,
    <trd>kegyetlen</trd>
  </trdgrp>
  <trd lng="nl">onbarmhartig</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="kompat.mal0ema">
  <kap>mal<tld/>ema</kap>
  <snc>
    <ref tip="dif" cel="kompat.sen0a">sen<tld/>a</ref>
    <ekz>
      sen iu azilo, en kiu mi povintus forsavi min de miaj
      mal<tld/>emaj persekutantoj
      <fnt><bib>Revadoj</bib><lok>Tria promenado</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_z;&c_mol;&c_l;&c_ib;&c_t;&c_a;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_ib;&c_b;&c_je;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hard-hearted</trd>,
    <trd>hard</trd>,
    <trd>harsh</trd>,
    <trd>implacable</trd>,
    <trd>merciless</trd>,
    <trd>pitiless</trd>,
    <trd>ruthless</trd>,
    <trd>uncompassionate</trd>,
    <trd>unfeeling</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>impitoyable</trd>,
    <trd>qui ne se laisse pas <ind>apitoyer</ind></trd>,
    <trd>inflexible<klr tip="ind"> (impitoyable)</klr></trd>,
    <trd>implacable</trd>,
    <trd>intraitable<klr tip="ind"> (impitoyable)</klr></trd>,
    <trd>intransigeant<klr tip="ind"> (impitoyable)</klr></trd>,
    <trd>sans <ind>piti&eacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;ny&ouml;rtelen</trd>,
    <trd>k&iacute;m&eacute;letlen</trd>
  </trdgrp>
  <trd lng="nl">onbarmhartig</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="kompat.mal0emo">
  <kap>mal<tld/>emo</kap>
  <snc>
    <ref tip="dif" cel="kompat.sen0eco">sen<tld/>eco</ref>
    <ekz>
      en <klr>[la diskutoj]</klr> oni notas la vivkarecon, senlaborecon,
      mal<tld/>emon de la entreprenistoj, sed en ili nenio estas
      mallegala
      <fnt><bib>Metrop</bib><lok>&ccirc;ap. 3</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="fr">
    <trd>absence de <ind>piti&eacute;</ind></trd>,
    <trd>absence de <ind>compassion</ind></trd>,
    <trd>inflexibilit&eacute;<klr tip="ind"> (absence de piti&eacute;)</klr></trd>,
    <trd>implacabilit&eacute;<klr tip="ind"> (absence de piti&eacute;)</klr></trd>,
    <trd>intransigeance<klr tip="ind"> (absence de piti&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;ny&ouml;rtelens&eacute;g</trd>,
    <trd>k&iacute;m&eacute;letlens&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">onbarmhartigheid</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="kompat.mem0o">
  <kap>mem<tld/>o</kap>
  <snc>
    <dif>
      <tld lit="K"/>o al si mem:
      <ekz>
        li estis plena je mem<tld/>o kaj alte taksis la okazon malkovri
        &ccirc;ion al a&ubreve;skultanto
        <fnt><bib>&Scirc;talrato</bib><lok>&ccirc;ap. 2</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Selbstmitleid</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>apitoiement sur soi</trd>
  </trdgrp>
  <trd lng="hu">&ouml;nsajn&aacute;lat</trd> 
  <trd lng="nl">zelfbeklag</trd> 
  <trd lng="ru">&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_mol; &c_k; &c_s;&c_je;&c_b;&c_je;</trd>
</drv>

<drv mrk="kompat.sen0a">
  <kap>sen<tld/>a</kap>
  <snc>
    <dif>
      Ne inklina al <tld/>o pro fiereco kaj egoismo:
      <ekz>
        Mi transdonos la Egiptojn en la manon de kruela sinjoro,
        kaj sen<tld/>a re&gcirc;o regos super ili
        <fnt><bib>MT</bib><lok>&Jes; 19:4</lok></fnt>;
      </ekz>
      <ekz>
        la sen<tld/>a morto diris sian kruelan vorton<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;iam la infanaj bu&scirc;oj parolis sen<tld/>ajn vortojn pri &scirc;i
        <fnt><bib>Fab4</bib><lok>Knabino kiu pa&scirc;is sur
        panon</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lian kapon, sur&scirc;utitan per la cindro, <ind>sen<tld/>e</ind>
        bruligis
        la suno
        <fnt><bib>Far2</bib><lok>&ccirc;ap. 21</lok></fnt>.
        <trdgrp lng="fr">
          <trd>impitoyablement</trd>,
          <trd>implacablement</trd>,
          <trd>sans <ind>piti&eacute;</ind></trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>k&ouml;ny&ouml;rtelen&uuml;l</trd>,
          <trd>k&iacute;m&eacute;letlen&uuml;l</trd>
        </trdgrp>
        <trd lng="nl">onbarmhartig<klr tip="ind"> (bw.)</klr></trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="kompat.mal0a">mal<tld/>a</ref>
    <refgrp tip="vid">
      <ref cel="kor.sen0a">senkora</ref>,
      <ref cel="kruel.0a">kruela</ref>,
      <ref cel="sever.0a">severa</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_z;&c_mol;&c_l;&c_ib;&c_t;&c_a;&c_s;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_ib;&c_b;&c_je;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hard-hearted</trd>,
    <trd>hard</trd>,
    <trd>harsh</trd>,
    <trd>implacable</trd>,
    <trd>merciless</trd>,
    <trd>pitiless</trd>,
    <trd>ruthless</trd>,
    <trd>uncompassionate</trd>,
    <trd>unfeeling</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>impitoyable</trd>,
    <trd>qui ne se laisse pas <ind>apitoyer</ind></trd>,
    <trd>inflexible<klr tip="ind"> (impitoyable)</klr></trd>,
    <trd>implacable</trd>,
    <trd>intraitable<klr tip="ind"> (impitoyable)</klr></trd>,
    <trd>intransigeant<klr tip="ind"> (impitoyable)</klr></trd>,
    <trd>sans <ind>piti&eacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;ny&ouml;rtelen</trd>,
    <trd>k&iacute;m&eacute;letlen</trd>,
    <trd>sz&iacute;vtelen</trd>,
    <trd>irgalmatlan</trd>
  </trdgrp>
  <trd lng="nl">onbarmhartig</trd>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="kompat.sen0eco">
  <kap>sen<tld/>eco</kap>
  <snc>
    <dif>
      Eco de iu sen<tld/>a:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la rigida kolumo de la &ccirc;emizo kun ordinara sen<tld/>eco
        sin apogis je razita mentono
        <fnt>
        <aut>I. Turgenjev trad. K. Bein</aut>
        <vrk>Patroj kaj filoj</vrk>
        <lok>&ccirc;ap. 5</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kompat.mal0emo">mal<tld/>emo</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>absence de <ind>piti&eacute;</ind></trd>,
    <trd>absence de <ind>compassion</ind></trd>,
    <trd>inflexibilit&eacute;<klr tip="ind"> (absence de piti&eacute;)</klr></trd>,
    <trd>implacabilit&eacute;<klr tip="ind"> (absence de piti&eacute;)</klr></trd>,
    <trd>intransigeance<klr tip="ind"> (absence de piti&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>k&ouml;ny&ouml;rtelens&eacute;g</trd>,
    <trd>irgalmatlans&aacute;g</trd>,
    <trd>k&iacute;m&eacute;letlens&eacute;g</trd>,
    <trd>sz&iacute;vtelens&eacute;g</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_je;&c_z;&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
