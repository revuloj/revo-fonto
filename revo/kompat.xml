<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kompat.xml,v 1.38 2023/12/03 09:58:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kompat</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="kompat.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="kompat.0i.PSI">
    <dif>
      Senti kortu&scirc;on pro ies malfeli&ccirc;o kaj deziron &gcirc;in malakrigi:
      <ekz>
         kompati iun, ies doloron;
      </ekz>
      <ekz>
        &gcirc;i estis knabeto, kiu ploris, kaj &scirc;i <tld/>is lin
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 2:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> vian mizeron ek<tld/>is
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        virtulo <tld/>as <klr>(indulgas, &scirc;paras)</klr> la vivon de sia
        bruto
        <fnt><bib>MT</bib><lok>&Sen; 12:10</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;u vi anka&ubreve; ne devis <tld/>i vian kunserviston, kiel mi <tld/>is
        vin
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 18:33</lok>
        </fnt>?
      </ekz>
      <ekz>
        ne trovi&gcirc;u <tld/>anto por liaj orfoj
        <fnt><bib>MT</bib><lok>&Psa; 109:12</lok></fnt>;
      </ekz>
      <ekz>
        Dio, <tld/>u nin, pekulojn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto unua</lok>
        </fnt>!
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        je la noktomezo la dormo finfine <tld/>is la hegemonon
        <fnt>
          <bib>MkM</bib>
          <lok>26</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>u, afablaj sinjoroj, <tld/>u! ili &gcirc;emadis kaj montradis siajn malsanajn
        membrojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        se fama kuracisto ne <tld/>us min, mi mortus
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        pastredzino <tld/>is &scirc;in kaj prenis &scirc;in kiel servistinon
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        Dio batas, Dio <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="bedaux.0i.kunsenti">beda&ubreve;ri<sncref/></ref>,
      <ref cel="domagx.0i.kompati">doma&gcirc;i<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="indulg.0i">indulgi</ref>,
      <ref cel="kondol.0i">kondolenci</ref>,
      <ref cel="sent.kun0i">kunsenti</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_k;&c_a;&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_g;&c_a;&c_d;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>m&iacute;t soucit s n&ecaron;k&yacute;m</trd>,
    <trd>m&iacute;t soustrast s n&ecaron;k&yacute;m</trd>,
    <trd>politovat n&ecaron;koho</trd>,
    <trd>slitovat se nad n&ecaron;k&yacute;m</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>bemitleiden</trd>,
    <trd>bedauern</trd>,
    <trd><ofc>*</ofc>Mitleid haben</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pity</trd>,
    <trd>have <ind>compassion</ind> for</trd>,
    <trd>feel <ind>compassion</ind> for</trd>,
    <trd>have <ind>pity</ind> for</trd>,
    <trd>have <ind>mercy</ind> on</trd>,
    <trd>be <ind>sorry</ind> for</trd>,
    <trd>commiserate with</trd>
  </trdgrp>
  <trd lng="es">compadecer</trd>
  <trdgrp lng="fr">
    <trd>plaindre<klr tip="ind"> (avoir de la compassion)</klr></trd>,
    <trd>s'<ind>apitoyer</ind> (de, sur)</trd>,
    <trd>s'<ind>attendrir</ind> (sur)</trd>,
    <trd>avoir de la <ind>compassion</ind> (pour)</trd>,
    <trd>compatir (&agrave;)</trd>,
    <trd>avoir <ind>piti&eacute;</ind> (de)</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megsz&aacute;n</trd>,
    <trd>sajn&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x54C0;&#x308C;&#x3080;  <pr>&#x3042;&#x308F;&#x308C;&#x3080;</pr></trd>,
    <trd>&#x540C;&#x60C5;&#x3059;&#x308B;  <pr>&#x3069;&#x3046;&#x3058;&#x3087;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6C17;&#x306E;&#x6BD2;&#x306B;&#x601D;&#x3046;  <pr>&#x304D;&#x306E;&#x3069;&#x304F;&#x306B;&#x304A;&#x3082;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd><ind>medelijden</ind> hebben met</trd>,
    <trd><ind>compassie</ind> hebben met</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>litowa&cacute; si&eogonek;</trd>,
    <trd>wsp&oacute;&lstroke;czu&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>compadecer-se</trd>,
    <trd>lastimar</trd>,
    <trd>ter <ind>compaix&atilde;o</ind></trd>,
    <trd>apiedar-se de</trd>,
    <trd>condoer-se de</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_l;&c_je;&c_t;&c_mol; <klr>(&c_k;&c_o;&c_g;&c_o;-&c_l;.)</klr></trd>,
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>ma&tcaron; s&uacute;cit</trd>,
    <trd>zmilova&tcaron; sa</trd>,
    <trd>z&lcaron;utova&tcaron; sa</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_m;&c_i;&c_l;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_p;&c_ib;&c_v;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x601C;&#x60AF; <pr>li&#xE1;nm&#x1D0;n</pr></trd>,
    <trd>&#x60DC; <pr>x&#x12B;</pr></trd>,
    <trd>&#x601C; <pr>li&#xE1;n</pr></trd>,
    <trd>&#x601C;&#x6064; <pr>li&#xE1;nx&#xF9;</pr></trd>,
    <trd>&#x54C0;&#x77DC; <pr>&#x101;ij&#x12B;n</pr></trd>,
    <trd>&#x60AF; <pr>m&#x1D0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kompat.0a">
  <kap><tld/>a</kap>
  <snc mrk="kompat.0a.PSI">
    <dif>
      Montranta <tld/>on, rilata al <tld/>o:
      <ekz>
        vidante <klr>[&#x2026;]</klr> tiujn viza&gcirc;ojn, mizerajn
        de malsato, denove mi spertis kvaza&ubreve; <tld/>an senton
        <fnt>
        <aut>H. Sienkiewicz trad. L. Zamenhof</aut>
        <vrk>Quo vadis?</vrk>
        <lok>&ccirc;ap. 28</lok>
        </fnt>;
      </ekz>
      <ekz>
        prenu tion &ccirc;i kiel instruo, por en estonteco rigardi la aferojn kun pli <tld/>a
        kaj pli sa&gcirc;a okulo
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ne toleras, se knabo <tld/>e min rigardas
        <fnt>
        <aut>trad. K. Bein</aut>,
        <vrk>Internacia Krestomatio</vrk>,
        <lok>Patro</lok></fnt>;
      </ekz>
      <ekz>
        mi a&ubreve;das <tld/>ajn vortojn de virino, kiun mi tiel longe kaj tiel pasie amas
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>!
      </ekz>
      <ekz>
        <ind>pli feli&ccirc;a estas martelo insultata, ol amboso
        <tld/>ata</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="hu">jobb sz&aacute;z irigy, mint egy <ind>sz&aacute;n&oacute;</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_a;&c_ch;&c_u;&c_t;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_g;&c_a;&c_d;&c_l;&c_ib;&c_v;&c_y;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>vyjad&rcaron;uj&iacute;c&iacute; l&iacute;tost</trd>,
    <trd>vyjad&rcaron;uj&iacute;c&iacute; politov&aacute;n&iacute;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>humain<klr tip="ind"> (montrant la compassion)</klr></trd>
    <trd>pitoyable<klr tip="ind"> (enclin &agrave; la
    piti&eacute;)</klr></trd>,
    <trd>secourable</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;nakoz&oacute;</trd>,
    <trd>sajn&aacute;lkoz&oacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6148;&#x60B2;&#x306E;  <pr>&#x3058;&#x3072;&#x306E;</pr></trd>,
    <trd>&#x54C0;&#x308C;&#x307F;&#x306E;  <pr>&#x3042;&#x308F;&#x308C;&#x307F;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>deelnemend</trd>,
    <trd>meewarig</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wsp&oacute;&lstroke;czuj&aogonek;cy</trd>,
    <trd>lituj&aogonek;cy si&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">vyjadruj&uacute;ci &lcaron;&uacute;tos&tcaron;</trd>
  <trd lng="uk">&c_s;&c_p;&c_ib;&c_v;&c_ch;&c_u;&c_t;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>
  <trd lng="zh">&#x60B2;&#x51C4; <pr>b&#x113;iq&#x12B;</pr></trd>
</drv>

<drv mrk="kompat.0o">
  <kap><tld/>o</kap>
  <snc mrk="kompat.0o.PSI">
    <dif>
      Sento de <tld/>anto:
      <ekz>
        &scirc;i rigardis la malgrandan knabinon kaj eksentis <tld/>on al &scirc;i
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
        <!--
        <trdgrp lng="fr">
          <trd>s'<ind>apitoyer</ind> (de, sur)</trd>,
          <trd>s'<ind>attendrir</ind> (sur)</trd>,
          <trd>prendre <ind>piti&eacute;</ind> (de)</trd>
        </trdgrp>
        <trd lng="hu">megsz&aacute;n</trd>
        <trd lng="nl"><ind>medelijden</ind> voelen</trd>
        -->
      </ekz>
      <ekz>
        Li eksentis <tld/>on pro la suferado de <nom>Izrael</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 10:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        se &scirc;i kelkfoje uzis sian influon je la juna regnestro, &scirc;i faris &gcirc;in nur por
        elpeti <tld/>on por iu
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>rideto de <tld/>o</ind>
        <klr>(afable malestima)</klr>
        <fnt>
          <bib>EE</bib>
        </fnt>;
        <trd lng="de">mitleidiges L&auml;cheln</trd>
        <trd lng="fr">sourire <ind>condescendant</ind></trd>
        <trd lng="hu">sz&aacute;nakoz&oacute; mosoly</trd>
        <trd lng="nl"><ind>minachten</ind>de glimlach</trd>
      </ekz>
      <ekz>
        li do aliris <klr>[&#x2026;]</klr> proponante al ili siajn fruktojn,
        la&ubreve; la sama plenda, <ind><tld/>veka</ind> maniero
        <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;ap. 14</lok></fnt>;
        <trdgrp lng="fr">
          <trd>plaintif</trd>,
          <trd>attendrissant<klr tip="ind"> (qui inspire
          compassion)</klr></trd>,
          <trd>geignard<klr tip="ind"> (cherchant &agrave; inspirer
          compassion)</klr></trd>,
          <trd>touchant<klr tip="ind"> (qui inspire
          compassion)</klr></trd>
        </trdgrp>
        <trd lng="hu">sz&aacute;nalomgerjeszt&odblac;</trd>
        <trdgrp lng="nl">
          <trd>aandoenlijk</trd>,
          <trd>roerend</trd>
        </trdgrp>
      </ekz>
      <ekz>
        Mi etendas Mian manon sur vin kaj pereigas vin
        <klr>[, &ccirc;ar]</klr> Mi laci&gcirc;is de <tld/>ado
        <fnt><bib>MT</bib>, <lok>&Jer; 15:6</lok></fnt>.
      </ekz>
      <ekz>
        mi ne volas baton, mi ne volas <tld/>on
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kor.0o.FIG">koro<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_a;&c_l;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_g;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_ch;&c_u;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bo">&t_sa;&t_nyata;&t_gigu;&t_nga;&t_tsheg;&t_ra;&t_jata;&t_drengbu;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>l&iacute;tost</trd>,
    <trd>slitov&aacute;n&iacute;</trd>,
    <trd>smilov&aacute;n&iacute;</trd>,
    <trd>soucit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mitleid</trd>,
    <trd>Erbarmen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>compassion</trd>,
    <trd>mercy</trd>,
    <trd>pity</trd>,
    <trd>sympathy</trd>
  </trdgrp>
  <trd lng="es">compasi&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>compassion</trd>,
    <trd>apitoiement</trd>,
    <trd>attendrissement<klr tip="ind"> (apitoiement)</klr></trd>,
    <trd>commis&eacute;ration</trd>,
    <trd>piti&eacute;</trd>,
    <trd>sympathie<klr tip="ind"> (compassion)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&aacute;nalom</trd>,
    <trd>k&ouml;ny&ouml;r&uuml;let</trd>,
    <trd>sajn&aacute;lat</trd>
    <trd>sz&aacute;nakoz&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x54C0;&#x308C;&#x307F;  <pr>&#x3042;&#x308F;&#x308C;&#x307F;</pr></trd>,
    <trd>&#x6148;&#x60B2;  <pr>&#x3058;&#x3072;</pr></trd>,
    <trd>&#x60C5;&#x3051;  <pr>&#x306A;&#x3055;&#x3051;</pr></trd>,
    <trd>&#x601D;&#x3044;&#x3084;&#x308A;  <pr>&#x304A;&#x3082;&#x3044;&#x3084;&#x308A;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>medelijden</trd>,
    <trd>compassie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lito&sacute;&cacute;</trd>,
    <trd>wsp&oacute;&lstroke;czucie</trd>
  </trdgrp>
  <trd lng="ro">mil&abreve;</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_o;&c_zh;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_s;&c_t;&c_r;&c_a;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_o;&c_ch;&c_u;&c_v;&c_s;&c_t;&c_v;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>s&uacute;cit</trd>,
    <trd>&lcaron;&uacute;tos&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_i;&c_l;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_ib;&c_v;&c_ch;&c_u;&c_t;&c_t;&c_ja;</trd>,
    <trd>&c_zh;&c_a;&c_l;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x601C;&#x60AF; <pr>li&#xE1;nm&#x1D0;n</pr></trd>,
    <trd>&#x6148;&#x60B2; <pr>c&#xED;b&#x113;i</pr></trd>,
    <trd>&#x540C;&#x60C5; <pr>t&#xF3;ngq&#xED;ng</pr></trd>,
    <trd>&#x607B;&#x9690;&#x4E4B;&#x5FC3; <pr>c&#xE8;y&#x1D0;nzh&#x12B;x&#x12B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kompat.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="stl">ARK</uzo>
  <snc mrk="kompat.0eco.PSI">
    <dif>
      <ref tip="dif" cel="kompat.0emo.PSI"><tld lit="K"/>emo</ref>:
      <ekz>
        Dio, estante ri&ccirc;a en <tld/>eco, <klr>[...]</klr>
        nin vivigis en Kristo
        <fnt><bib>NT</bib><lok>&Efe; 2:4</lok></fnt>;
      </ekz>
      <ekz>
        rigardu, Dio neniam laci&gcirc;as en la <tld/>eco, kaj ni, mizeraj vermoj, iras dormi
        kun nia kolero
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
