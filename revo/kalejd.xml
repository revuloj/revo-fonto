<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>kalejdoskop</rad>/o
</kap>
<drv mrk="kalejd.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Optika aparato, enhavanta tri spegulojn kaj multajn diverskolorajn
      vitropecetojn, donanta simetriajn bildojn, kiuj &scirc;an&gcirc;i&gcirc;as
      je &ccirc;iu moveto de la aparato.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_je;&c_j;&c_d;&c_a;&c_s;&c_k;&c_o;&c_p;</trd>
  <trd lng="en">kaleidoscope</trd>
  <trd lng="es">caleidoscopio</trd>
  <trd lng="fr">kal&eacute;idoscope</trd>
  <trd lng="nl">kaleidoscoop</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_je;&c_j;&c_d;&c_o;&c_s;&c_k;&c_o;&c_p;</trd>
</drv>

<drv mrk="kalejd.0i">
  <kap><tld/>i</kap>
  <uzo tip="stl">FIG</uzo>
  <snc mrk="kalejd.0i.FIG">
    <dif>
      <ref tip="dif" cel="karuse.0i">karuseli</ref>
      <ekz>
        sed kian? <tld lit="K"/>is proponoj
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 62a, p. 338a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="bunt.0a">bunta</ref>,
    <ref cel="karuse.0i">karuseli</ref>
  </refgrp>
  <trdgrp lng="fr">
    <trd>se <ind>bousculer</ind><klr> (fig.)</klr></trd>
  </trdgrp>
</drv>
</art>
