<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gxanga.xml,v 1.26 2023/11/11 11:36:37 revo Exp $">
<kap>
  <rad>&gcirc;angal</rad>/o
</kap>
<drv mrk="gxanga.0o">
  <kap><tld/>o</kap>
  <snc mrk="gxanga.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Densa <ref tip="super" cel="arb.0aro">arbaro</ref> de varma,
      malseka
      klimato, precipe de musonaj landoj.
    </dif>
    <trdgrp lng="ca">
      <trd>jungla</trd>,
      <trd>selva</trd>
    </trdgrp>
  <trd lng="en"><klr>(tropical) </klr>rain <ind>forest</ind></trd>
  </snc>
  <snc mrk="gxanga.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pro multaj diversaj eroj implika loko a&ubreve; medio:
      <ekz>
la enhavo de la libro tute ne estas <tld/>o de lingvistikaj terminoj
      <fnt>
          <aut>Sergej Derevjanny&hcirc;</aut>
          <vrk><url ref="&Monato;/2001/pdf/monato200111.pdf#page=30">Atrakcio por
          lingvo&scirc;atantoj?</url></vrk>
          <bib>Monato</bib>
          <lok>2001/11, p. 30</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>embolic</trd>,
      <trd>garbuix</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_d;&c_zh;&c_u;&c_n;&c_g;&c_l;&c_ib;</trd>
  <trd lng="de">Dschungel</trd>
  <trd lng="es">jungla</trd>
  <trd lng="en">jungle</trd>
  <trd lng="fa">&a_j;&a_n;&f_g;&a_l;</trd>
  <trd lng="fr">jungle</trd>
  <trdgrp lng="he">
    <trd>&yod;&ayin;&resh; &ayin;&dalet;</trd>,
    <trd>&gimel;'&vav;&nun;&gimel;&lamed;</trd>,
    <trd>&samekh;&bet;&fkaf;</trd>
  </trdgrp>
  <trd lng="hu">dzsungel</trd>
  <trdgrp lng="ja">
    <trd>&#x30B8;&#x30E3;&#x30F3;&#x30B0;&#x30EB;</trd>,
    <trd>&#x5BC6;&#x6797;  <pr>&#x307F;&#x3064;&#x308A;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_zh;&c_u;&c_n;&c_g;&c_l;&c_i;</trd>
  <trd lng="nl">wildernis</trd>
  <trd lng="pl">d&zdot;ungla</trd>
  <trdgrp lng="pt">
   <trd>j&acirc;ngal</trd>, 
   <trd>floresta</trd>, 
   <trd>selva</trd> 
  </trdgrp>
  <trd lng="sv">djungel</trd>
<trdgrp lng="it">
   <trd>giungla</trd>, 
   <trd>jungla</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_d;&c_zh;&c_u;&#x43D;&c_gu;&#x43B;&c_ib;</trd>,
    <trd>&c_h;&c_a;&c_shch;&c_a;</trd>,
    <trd>&c_z;&c_a;&c_r;&#x43E;&c_s;&c_t;&c_ib;</trd>,
    <trd>&#x43D;&c_je;&c_t;&c_r;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E1B;&#x6797; <pr>c&#xF3;ngl&#xED;n</pr></trd>,
    <trd>&#x7349; <pr>zh&#x113;n</pr></trd>
  </trdgrp>
</drv>
</art>
