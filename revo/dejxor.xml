<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: dejxor.xml,v 1.26 2023/11/04 13:16:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>de&jcirc;or</rad>/i
</kap>
<drv mrk="dejxor.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="serv.0i.labori">Servi</ref> la&ubreve; anta&ubreve;e
      difinita tempo:
      <ekz>
        <klr>[ili]</klr> estis pordegistoj, kaj 
        <tld/>is en la provizejoj &ccirc;e la pordegoj
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 12:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        li alvokis du el siaj domservantoj <klr>[&#x2026;]</klr> el tiuj, kiuj &ccirc;iam <tld/>is
        apud li
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 10:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> <tld/>as &ccirc;e la altaro
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 9:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        por la nokto oni starigis la kofrojn kontra&ubreve; la pordo, unu el la voja&gcirc;antoj
        gardo<tld/>is, dum la aliaj dormis
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>anta oficiro klarigis, ke kolekti&gcirc;is la re&gcirc;aj laboristoj kaj postulas
        la nepagitan salajron
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la trotuaro <tld/>is policano
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>is en la <nom>S&egrave;vres&#x2019;</nom>a stacidomo dum tempo 
        tro longa por ne koni tiun sinjoron
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        ni kune en la ambulanco <tld/>is
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        post definitiva forlaso de la <ind><tld/>ejo</ind>, fari&gcirc;is iom malkomforte 
        frekventi eksterdoman aparaton
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2011/010495.php">Rekte interrete</url></vrk>
          <lok>2011</lok>
        </fnt>.
        <trd lng="de">Dienststelle</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ofic.0i">ofici</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_zh;&c_u;&c_r;&c_y;&c_c;&c_mol;</trd>
  <trdgrp lng="ca">
    <trd>servir</trd>,
    <trd>estar de servei</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>konat</trd>,
    <trd>m&iacute;t slu&zcaron;bu</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Dienst tun</trd>,
     <trd>dienen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>be on duty</trd>,
    <trd>serve</trd>,
    <trd>be on shift</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&ecirc;tre de <ind>garde</ind></trd>,
    <trd>&ecirc;tre de <ind>service</ind></trd>,
    <trd>&ecirc;tre de <ind>permanence</ind></trd></trdgrp>
  <trdgrp lng="es">
    <trd>estar de <ind>turno</ind></trd>,
    <trd>hacer <ind>turno</ind></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&lamed;&shin;&resh;&tav;</trd>,
    <trd>&lamed;&he;&yod;&vav;&tav; &bet;&tav;&vav;&resh;&nun;&vav;&tav;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&uuml;gyel</trd>,
    <trd>&uuml;gyeletet tart</trd>,
    <trd>&uuml;gyeletben van</trd>,
    <trd>szolg&aacute;latban van</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>lavorare<klr> (relativ. turno o servizio)</klr></trd>,
    <trd>essere di <ind>turno</ind></trd>,
    <trd>essere di <ind>servizio</ind></trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5F53;&#x76F4;&#x3059;&#x308B;  <pr>&#x3068;&#x3046;&#x3061;&#x3087;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x52E4;&#x52D9;&#x4E2D;&#x3067;&#x3042;&#x308B;  <pr>&#x304D;&#x3093;&#x3080;&#x3061;&#x3085;&#x3046;&#x3067;&#x3042;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>in <ind>dienst</ind> zijn</trd>,
    <trd>wacht hebben</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dy&zdot;urowa&cacute;</trd>,
    <trd>by&cacute; na <ind>s&lstroke;u&zdot;bie</ind></trd>,
    <trd>mie&cacute; <ind>s&lstroke;u&zdot;b&eogonek;</ind></trd>
    <trd>by&cacute; na <ind>wachcie</ind></trd>,
    <trd>mie&cacute; <ind>wacht&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>estar de <ind>servi&ccedil;o</ind></trd>,
    <trd>estar de <ind>plant&atilde;o</ind></trd>,
    <trd>montar <ind>guarda</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_zh;&c_u;&c_r;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_je;&c_r;&c_g;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_t;&c_i; &c_s;&c_l;&c_u;&c_zh;&c_b;&c_u; &c_z;&c_a; &c_g;&c_r;&c_a;&c_f;&c_ib;&c_k;&c_o;&c_m;</trd>
  </trdgrp>
  <trd lng="zh">&#x503C;&#x65E5; <pr>zh&#xED;r&#xEC;</pr></trd>
</drv>

<drv mrk="dejxor.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="serv.0o.laboro">Servo</ref> plenumata 
      de iu en anta&ubreve;difinita tempo:
      <ekz>
        <klr>[ili]</klr> estis gardistoj de la pordegoj <klr>[&#x2026;]</klr> la&ubreve; <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 9:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el la junaj oficistoj <klr>[&#x2026;]</klr> devis &gcirc;uste en tiu vespero havi sian
        <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; lin kolerigi per ripro&ccirc;oj, kiujn meritis la nereguleco de lia <tld/>o,
        li opiniis, ke estas pli prudente agi kun li tiel kiel kun malsanulo
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o en tiu &ccirc;i malproksima, malanta&ubreve;fronta loko estis facila, kaj la
        konservo de la ordo en tiuj lokoj ne signifis grandan taskon
        <fnt>
          <bib>LGA</bib>
        </fnt>;
      </ekz>
      <ekz>
        dum &ccirc;iuj horoj ne prenitaj de sia <tld/>o, la komandanto restis anta&ubreve; sia
        spionfenestreto rigardadante
        <fnt>
          <bib>KPr</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dev">devo</ref>,
      <ref cel="funkci.0o">funkcio</ref>,
      <ref cel="ofic.0o">ofico</ref>,
      <ref cel="skip.0o">skipo</ref>,
      <ref cel="task.0o">tasko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_z;&c_ja;&c_zh;&c_u;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_h;&c_t;&c_a;</trd>,
    <trd>&c_z;&c_mol;&c_m;&c_je;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">servei</trd>
  <trdgrp lng="de">
     <trd>Dienstzeit</trd>,
     <trd>Dienst</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>duty</trd>,
    <trd>service</trd>,
    <trd>shift</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>permanence</trd>,
    <trd>garde<klr tip="ind"> (permanence)</klr></trd>,
    <trd>service<klr tip="ind"> (permanence)</klr></trd></trdgrp>
  <trd lng="he">&mem;&#x5B4;&shin;&#x5C1;&mem;&#x5B6;&resh;&#x5B6;&tav;</trd>
  <trdgrp lng="hu">
    <trd>&uuml;gyelet</trd>,
    <trd>szolg&aacute;lat<klr tip="ind"> (&uuml;gyelet)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>orario<klr> (di lavoro)</klr></trd>,
    <trd>turno<klr> (di lavoro)</klr></trd>,
    <trd>servizio<klr> (orario di lavoro)</klr></trd>
  </trdgrp>
  <trd lng="es">turno</trd>
  <trd lng="ja">&#x5F53;&#x76F4;  <pr>&#x3068;&#x3046;&#x3061;&#x3087;&#x304F;</pr></trd>
  <trdgrp lng="nl">
    <trd>dienst</trd>,
    <trd>wacht</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>dy&zdot;ur</trd>,
    <trd>s&lstroke;u&zdot;ba</trd>,
    <trd>wachta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_zh;&c_u;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_v;&c_a;&c_h;&c_t;&c_a;</trd>,
    <trd>&c_s;&c_m;&c_je;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_je;&c_r;&c_g;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_b;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja; &c_n;&c_a; &c_s;&c_l;&c_u;&c_zh;&c_b;&c_ib;</trd>,
    <trd>&c_r;&c_o;&c_b;&c_o;&c_ch;&c_i;&c_j; &c_d;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_z;&c_m;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_h;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x670D;&#x52A1; <pr>f&#xFA;w&#xF9;</pr></trd>,
    <trd>&#x529E;&#x516C; <pr>b&#xE0;ng&#x14D;ng</pr></trd>
  </trdgrp>
</drv>
</art>
