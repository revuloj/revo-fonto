<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <ofc>*</ofc>
   <rad>pen</rad>/i
</kap>
<drv mrk="pen.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>ntr</vspec></gra>
   <snc>
     <dif>
       Energie kaj lacige stre&ccirc;i siajn fortojn por atingi rezulton:
       <ekz>
         en korespondado <klr>...</klr> estas bone &ccirc;iam <tld/>i uzadi nur
         vortojn el la <ctl>Fundamento</ctl>
         <fnt><bib>F</bib>, <lok>anta&ubreve;parolo</lok></fnt>.
       </ekz>
       <ekz>
         la maljuna &ccirc;evalo <klr>...</klr> vane <tld/>is kuri pli rapide
         <fnt><bib>FK</bib>, <lok>4, rakontoj, Arturo, (rakonto de V.
         Devjatnin), 4</lok></fnt>;
       </ekz>
       <ekz>
         <tld/>i porti, anstata&ubreve;igi<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <tld/>i pri la efektivigo de siaj planoj;
       </ekz>
       <ekz>
         mi <tld/>is rekte pensi en la lingvo ne&ubreve;trala<fnt>Z</fnt>;
       </ekz>
       <ekz>
         mi <tld/>os veni al la kongreso<fnt>Z</fnt>;
       </ekz>
       <ekz>
         oni <tld/>is allogi min per tre granda sumo da mono<fnt>Z</fnt>;
       </ekz>
       <ekz>
         mi estas preta, de mia flanko mi volonte <tld/>os
         <fnt><bib>Revizoro</bib>, <lok>akto 5a, sceno 7a</lok></fnt>;
       </ekz>
       <ekz>
         kion vi pri<tld/>as, tio al vi venas
         <fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="klopod.0i">klopodi</ref>,
       <ref cel="labor.0i">labori</ref>,
       <ref cel="prov.0i">provi</ref>,
       <ref cel="streb.0i">strebi</ref>,
       <ref cel="strecx.0i">stre&ccirc;i</ref>,
       <ref cel="sxvit.0i">&scirc;viti</ref>
     </refgrp>
   </snc>
   <rim>
     <em>Peni</em> pli atentigas pri stre&ccirc;o kaj laci&gcirc;o,
     <em>labori</em> pri celo kaj rezulto.
   </rim>
   <trdgrp lng="be">
     <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_n;&c_a;&c_m;&c_a;&c_g;&c_a;&c_c;&c_c;&c_a;</trd>,
     <trd>&c_p;&c_r;&c_y;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol; &c_v;&c_y;&c_s;&c_ib;&c_l;&c_k;&c_ib;</trd>,
     <trd>&c_n;&c_a;&c_k;&c_ib;&c_r;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; &c_v;&c_y;&c_s;&c_ib;&c_l;&c_k;&c_ib;</trd>
   </trdgrp>
   <trd lng="cs">sna&zcaron;it se</trd>
   <trdgrp lng="de">
     <trd>sich <ind>m&uuml;hen</ind></trd>,
     <trd>sich <ind>abm&uuml;hen</ind></trd>,
     <trd>sich <ind>anstrengen</ind></trd>
   </trdgrp>
   <trd lng="en">try</trd>
   <trdgrp lng="es">
     <trd>intentar</trd>,
     <trd>tratar</trd>
   </trdgrp>
   <trd lng="fr">s'<ind>efforcer</ind> de</trd>
   <trd lng="hu">f&aacute;radozik</trd>
   <trdgrp lng="nl">
     <trd>trachten</trd>,
     <trd>moeite doen</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>&c_p;&c_y;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
     <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol; &c_u;&c_s;&c_i;&c_l;&c_i;&c_ja;</trd>
   </trdgrp>
</drv>

<drv mrk="pen.0o">
   <kap><tld/>o</kap>
   <snc>
     <dif>
       Energia kaj laciga stre&ccirc;o de &ccirc;iuj fortoj al iu celo:
       <ekz>
         mi vin dankas pro viaj <tld/>oj;
       </ekz>
       <ekz>
         mi faros &ccirc;iujn eblajn <tld/>ojn por kontentigi vin;
       </ekz>
       <ekz>
         ho, ne faru al vi <tld/>ojn, diris Fanny
         <fnt><bib>FK</bib>, <lok>4, rakontoj, la porcio da glacia&jcirc;o,
         (rakonto de L. Dilling, tradukita de Eduard Hall el Joensuu
         (Finnlando))</lok></fnt>;
       </ekz>
       <ekz>
         bona verkisto <tld/>as eviti <tld/>on al la legantoj;
       </ekz>
       <ekz>
         venas &ccirc;agreno sen granda <tld/>o
         <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         via <tld/>ado ne estos vana;
       </ekz>
       <ekz>
         por sukcesi vi devas ankora&ubreve; plifortigi viajn <tld/>ojn;
       </ekz>
       <ekz>
         ne &scirc;pari monon nek <tld/>on
         (<ref tip="vid" cel="hauxt.0o">ha&ubreve;to</ref>)
         <fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_n;&c_a;&c_m;&c_a;&c_g;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
     <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
     <trd>&c_v;&c_y;&c_s;&c_ib;&c_l;&c_a;&c_k;</trd>
   </trdgrp>
   <trd lng="cs">n&aacute;maha</trd>
   <trdgrp lng="de">
     <trd>M&uuml;he</trd>,
     <trd>Anstrengung</trd>
   </trdgrp>
   <trd lng="es">intento</trd>
   <trdgrp lng="fr">
     <trd>efforts</trd>,
     <trd>peine<klr tip="amb"> (efforts)</klr></trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>f&aacute;rads&aacute;g</trd>,
     <trd>f&aacute;radoz&aacute;s</trd>,
     <trd>er&odblac;fesz&iacute;t&eacute;s</trd>
   </trdgrp>
   <trd lng="nl">moeite</trd>
   <trdgrp lng="ru">
     <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_i;&c_je;</trd>,
     <trd>&c_u;&c_s;&c_i;&c_l;&c_i;&c_je;</trd>
   </trdgrp>
</drv>

<drv mrk="pen.0egi">
   <kap><tld/>egi</kap>
   <snc>
     <dif>
       Tre <tld/>i, uzi sian tutan kapablon por io:
       <ekz>
         vi kaj viaj kunkrimuloj <tld/>egis detrui min
         <fnt><bib>I&scirc;tar</bib>, <lok>&ccirc;apitro 21a, Zalmuna batalas kontra&ubreve;
         la &ccirc;efpastroj</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="fr">
     <trd>s'escrimer &agrave;</trd>,
     <trd>s'&eacute;vertuer &agrave;</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_b;&c_i;&c_t;&c_mol;&c_s;&c_ja;<klr tip="amb"> (&c_n;&c_a;&c_d; &c_ch;&c_je;&c_m;-&c_l;.)</klr></trd>,
     <trd>&c_p;&c_r;&c_i;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol; &c_v;&c_s;&c_je; &c_u;&c_s;&c_i;&c_l;&c_i;&c_ja;<klr tip="amb"> (&c_k; &c_ch;&c_je;&c_m;&c_u;-&c_l;.)</klr></trd>
   </trdgrp>
</drv>

<drv mrk="pen.0ego">
   <kap><tld/>ego</kap>
   <snc>
     <dif>
       Tre granda sinstre&ccirc;o por iu celo.
     </dif>
   </snc>
   <trd lng="fr">efforts &eacute;normes</trd>
   <trd lng="ru">&c_o;&c_g;&c_r;&c_o;&c_m;&c_n;&c_o;&c_je; <ind>&c_u;&c_s;&c_i;&c_l;&c_i;&c_je;</ind></trd>
</drv>

<drv mrk="pen.0iga">
   <kap><tld/>iga</kap>
   <snc>
     <dif>
       Trudanta a&ubreve; postulanta <tld/>on:
       <ekz>
         <tld/>iga laboro;
       </ekz>
       <ekz>
         &ccirc;iu nokto i&gcirc;as &ccirc;iam pli nigra ol la anta&ubreve;a, pli senkonsola, kaj la
         tuta vivo pli malhela kaj <tld/>iga
         <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>blindaj &ccirc;uk&ccirc;oj, (du skizoj), unua
         skizo:, Filip Onkudimov, 1</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_c;&c_ja;&c_zh;&c_k;&c_ib;</trd>,
     <trd>&c_s;&c_t;&c_o;&c_m;&c_n;&c_y;</trd>
   </trdgrp>
   <trd lng="fr">p&eacute;nible</trd>
   <trd lng="hu">f&aacute;rads&aacute;gos</trd>
   <trd lng="nl">moeitevol</trd>
   <trdgrp lng="ru">
     <trd>&c_u;&c_t;&c_o;&c_m;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
     <trd>&c_t;&c_ja;&c_zh;&c_jo;&c_l;&c_y;&c_j;</trd>,
     <trd>&c_t;&c_r;&c_u;&c_d;&c_n;&c_y;&c_j;</trd>
   </trdgrp>
</drv>

<drv mrk="pen.0inda">
   <kap><tld/>inda</kap>
   <snc>
     <dif>
       Sufi&ccirc;e <ref tip="super" cel="grav.0a">grava</ref>, ke oni <tld/>u
       por &gcirc;i:
       <ekz>
         estus <tld/>inde a&ubreve;skulti mian historion
         <fnt><bib>Fab3</bib>, <lok>kolo de botelo</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_v;&c_a;&c_r;&c_t;&c_y; <ind>&c_v;&c_y;&c_s;&c_ib;&c_l;&c_k;&c_a;&c_w;</ind></trd>
   <trd lng="es">que vale la <ind>pena</ind></trd>
   <trdgrp lng="hu">
     <trd>f&aacute;radoz&aacute;sra m&eacute;lt&oacute;</trd>,
     <trd>f&aacute;radoz&aacute;sra &eacute;rdemes</trd>
   </trdgrp>
   <trd lng="nl">de <ind>moeite</ind> waard</trd>
   <trd lng="ru">&c_d;&c_o;&c_s;&c_t;&c_o;&c_j;&c_n;&c_y;&c_j; <ind>&c_u;&c_s;&c_i;&c_l;&c_i;&c_j;</ind></trd>
</drv>

<drv mrk="pen.0indi">
   <kap><tld/>indi</kap>
   <snc>
     <dif>
       Esti afero valora, dezirinda, e&ccirc; koste de <tld/>o:
       <ekz>
         li opiniis, ke tute ne <tld/>indas konfidi tion al la pordisto
         <fnt><bib>Fab1</bib>, <lok>galo&scirc;oj de feli&ccirc;o, 4, &ccirc;efa momento,
         peco ta&ubreve;ganta por deklamado, tute eksterordinara
         voja&gcirc;o</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="fr">valoir la peine</trd>
   <trd lng="ru">&c_s;&c_t;&c_o;&c_i;&c_t;&c_mol; &c_u;&c_s;&c_i;&c_l;&c_i;&c_j;</trd>
</drv>

<drv mrk="pen.sen0e">
   <kap>sen<tld/>e</kap>
   <snc>
     <dif>
       <ref tip="super" cel="facil.0e">Facile</ref>, sen fortostre&ccirc;o a&ubreve;
       laci&gcirc;o:
       <ekz>
         ah, bona afero estas la rajdado, <klr>...</klr> oni sidas kvaza&ubreve;
         sur se&gcirc;o, ne faletas sur &scirc;tono, indulgas la &scirc;uojn kaj sen<tld/>e
         voja&gcirc;as
         <fnt><bib>ElektFab</bib>, <lok>Feli&ccirc;a Jo&ccirc;jo</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="be">
     <trd>&c_l;&c_jo;&c_g;&c_k;&c_a;</trd>,
     <trd>&c_b;&c_ja;&c_z; <ind>&c_v;&c_y;&c_s;&c_ib;&c_l;&c_k;&c_a;&c_w;</ind></trd>
   </trdgrp>
   <trd lng="de">m&uuml;helos</trd>
   <trdgrp lng="fr">
     <trd>ais&eacute;ment</trd>,
     <trd>sans peine</trd>
   </trdgrp>
   <trdgrp lng="hu">
     <trd>k&ouml;nnyed&eacute;n</trd>,
     <trd>minden k&uuml;l&ouml;n&ouml;sebb n&eacute;lk&uuml;l</trd>
   </trdgrp>
   <trdgrp lng="nl">
     <trd>moeiteloos</trd>,
     <trd>gemakkelijk</trd>
   </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_l;&c_je;&c_g;&c_k;&c_o;</trd>,
     <trd>&c_b;&c_je;&c_z; &c_u;&c_s;&c_i;&c_l;&c_i;&c_j;</trd>
   </trdgrp>
</drv>

<drv mrk="pen.valorila0on">
   <kap>valori la <tld/>on</kap>
   <snc>
     <ref tip="dif" cel="pen.0indi"><tld/>indi</ref>
     <ekz>
       la rezultato ne valoras la <tld/>on
       <klr>(= ne estas inda)</klr><fnt>Z</fnt>;
     </ekz>
     <ekz>
       <ind>ne valoras la <tld/>on</ind> ekzameni la aferon
       <trd lng="hu">nem &eacute;ri meg a <ind>f&aacute;rads&aacute;got</ind></trd>.
     </ekz>
   </snc>
   <trd lng="fr">valoir la peine</trd>
   <trd lng="ru">&c_s;&c_t;&c_o;&c_i;&c_t;&c_mol; &c_u;&c_s;&c_i;&c_l;&c_i;&c_j;</trd>
</drv>
</art>
