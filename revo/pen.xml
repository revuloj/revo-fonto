    <trd>fatyga</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>efort</trd>,
    <trd>durere</trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&c_n;&c_i;&c_je;</trd>,
     <trd>&c_u;&c_s;&c_i;&c_l;&c_i;&c_je;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>dravos&tcaron;</trd>,
    <trd>drav&aacute; t&uacute;&zcaron;ba</trd>,
    <trd>n&aacute;por</trd>,
    <trd>&uacute;silie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&#x43D;&c_a;&#x43C;&c_a;&c_g;&c_a;&#x43D;&#x43D;&#x44F;</trd>,
    <trd>&c_z;&c_u;&c_s;&c_i;&#x43B;&#x43B;&#x44F;</trd>
