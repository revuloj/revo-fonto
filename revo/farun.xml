<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>farun</rad>/o
</kap>

<drv mrk="farun.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Muela&jcirc;o el grenoj, post apartigo de brano:
      <ekz>tritika, hordea, sinapa <tld/>o;</ekz>
      <ekz><tld/>o el linsemo..</ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_k;&c_a;</trd>
  <trd lng="bg">&c_b;&c_r;&c_a;&c_sh;&c_n;&c_o;</trd>
  <trd lng="cs">mouka</trd>
  <trd lng="de">Mehl</trd>
  <trd lng="es">harina</trd>
  <trd lng="fr">farine</trd>
  <trd lng="hu">liszt</trd>
  <trd lng="nl">meel</trd>
  <trd lng="pt">farinha</trd>
  <trd lng="ru">&c_m;&c_u;&c_k;&c_a;</trd>
</drv>
<drv mrk="farun.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <tld lit="F"/>a man&gcirc;a&jcirc;o.  
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_ch;&c_n;&c_y; &c_v;&c_y;&c_r;&c_a;&c_b;</trd>
  <trd lng="de">Mehlspeise</trd>
  <trd lng="fr">bouillie<klr tip="ind"> (de farine)</klr></trd>
  <trd lng="hu">t&eacute;szta<klr tip="ind"> (lisztes &eacute;tel)</klr></trd>
  <trd lng="nl">meelspijs</trd>
  <trd lng="ru">&c_m;&c_u;&c_ch;&c_n;&c_o;&c_je; <klr>(&c_k;&c_u;&c_sh;&c_a;&c_n;&c_mol;&c_je;)</klr></trd>
</drv>
</art>
