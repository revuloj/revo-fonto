<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: farun.xml,v 1.30 2023/11/09 19:13:16 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>farun</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="farun.0o">
  <kap>
    <ofc>*</ofc>
    <tld/>o</kap>
  <snc mrk="farun.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Muela&jcirc;o el grenoj, post apartigo de brano:
      <ekz>
        delikata tritika <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 29:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        sojo<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2013/010973.php">Bombo-bankroto</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        fratino kuiris ka&ccirc;on el akvo kaj sekal<tld/>o kaj ni man&gcirc;is
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        se vi alportos <tld/>oferon el io, kio estas bakita en forno, &gcirc;i estu macokukoj
        el plej bona <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 2:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la maro la kuiristo devas povoscii helpi al si <klr>[je provizo]</klr> de peklita
        viando, kaj de &scirc;ima <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Supo el kolbasaj bastonetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        faboj, pizoj, lentoj, kiel anka&ubreve; la <tld/>o el ili
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        montanino scias kuiri franda&jcirc;ojn, uzante <klr>[&#x2026;]</klr> abrikotojn, safranon,
        sova&gcirc;an zingibron kaj avenan <tld/>on
        <fnt>
          <bib>IKr</bib>
          <lok>La Miraklo De Purun Bhagat</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni ne plu zorgis pri &ccirc;iutaga pano, amara sorgo<tld/>o ja estis historia&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Wei Yubin</aut>
          <vrk><url ref="https://www.monato.be/2012/010573.php">La biciklo de mia patro</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        ordinara familio &ccirc;iutage man&gcirc;as tortiljon bakitan el maiz-<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2007/009177.php">Bone nutrataj a&ubreve;toj</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        kazea bulo estas miksa&jcirc;o el <tld/>o kaj kazeo
        <fnt>
          <bib>VaK</bib>
          <lok>Dekkvara &Ccirc;apitro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> uzis kiel aldonan proteinan nutra&jcirc;on al bestoj nesufi&ccirc;e
        desinfektitajn ostodevenan <tld/>on kaj viandajn resta&jcirc;ojn, fabrikitajn el
        kadavroj de &scirc;afoj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="https://www.monato.be/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        muso sati&gcirc;is, <tld/>o malboni&gcirc;is
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_k;&c_a;</trd>
  <trd lng="bg">&c_b;&c_r;&c_a;&c_sh;&c_n;&c_o;</trd>
  <trd lng="bo">&t_ga;&t_rata;&t_naro;&t_tsheg;&t_zha;&t_gigu;&t_ba;&t_tsheg;</trd>
  <trd lng="cs">mouka</trd>
  <trd lng="de">Mehl</trd>
  <trdgrp lng="en">
    <trd>flour</trd>,
    <trd>meal</trd>
  </trdgrp>
  <trd lng="es">harina</trd>
  <trd lng="fa">&a_A_madda;&a_r;&a_d;</trd>
  <trd lng="fr">farine</trd>
  <trd lng="hu">liszt</trd>
  <trd lng="id">tepung</trd>
  <trdgrp lng="ja">
    <trd>&#x7C89;  <pr>&#x3053;&#x306A;</pr></trd>,
    <trd>&#x5C0F;&#x9EA6;&#x7C89;  <pr>&#x3053;&#x3080;&#x304E;&#x3053;</pr></trd>
  </trdgrp>
  <trd lng="nl">meel</trd>
  <trd lng="pl">m&aogonek;ka</trd>
  <trd lng="pt">farinha</trd>
  <trd lng="ru">&c_m;&c_u;&c_k;&c_a;</trd>
  <trd lng="sv">mj&ouml;l</trd>
  <trd lng="tr">un</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_o;&c_r;&c_o;&c_sh;&c_n;&c_o;</trd>,
    <trd>&c_m;&c_u;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9762;&#x513F; <pr>mi&#xE0;nr</pr></trd>,
    <trd>&#x9762;&#x7C89; <pr>mi&#xE0;nf&#x11B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="farun.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="farun.0ajxo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="F"/>a man&gcirc;a&jcirc;o:
      <ekz>
        baka&jcirc;o kaj <tld/>a&jcirc;o de diversaj specoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kazea bulo ne estas flava &scirc;tonero, sed <tld/>a&jcirc;o, kiu estas man&gcirc;ebla
        <fnt>
          <bib>VaK</bib>
          <lok>DEKKVARA &Ccirc;APITRO</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni preparas man&gcirc;a&jcirc;ojn: kukurban supon, terpoman gula&scirc;on, <tld/>a&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2013/011170.php">Urbo kukurba</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_ch;&c_n;&c_y;
  &c_v;&c_y;&c_r;&c_a;&c_b;</trd>
  <trd lng="de">Mehlspeise</trd>
  <trdgrp lng="en">
    <trd>baked good</trd>,
    <trd>pastry</trd>
  </trdgrp>
  <trd lng="fa">&a_f;&a_r;&a_n;&f_y;</trd>
  <trd lng="hu">t&eacute;szta<klr tip="ind"> (lisztes
  &eacute;tel)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x7CA5;  <pr>&#x304B;&#x3086;</pr></trd>,
    <trd>&#x7A40;&#x7C89;&#x3067;&#x4F5C;&#x3063;&#x305F;&#x98DF;&#x7269;  <pr>&#x3053;&#x304F;&#x3075;&#x3093;&#x3067;&#x3064;&#x304F;&#x3063;&#x305F;&#x3057;&#x3087;&#x304F;&#x3082;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="nl">meelspijs</trd>
  <trd lng="pl">potrawa <ind>m&aogonek;czna</ind></trd>
  <trd lng="ru">&c_m;&c_u;&c_ch;&c_n;&c_o;&c_je;
  <klr>(&c_k;&c_u;&c_sh;&c_a;&c_n;&c_mol;&c_je;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_o;&c_r;&c_o;&c_sh;&c_n;&c_ja;&c_n;&c_a;/&c_m;&c_u;&c_ch;&c_n;&c_a; &c_s;&c_t;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_v;&c_a; &c_z; &c_t;&c_ib;&c_s;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="zh">&#x9762;&#x98DF; <pr>mi&#xE0;nsh&#xED;</pr></trd>
</drv>

<drv mrk="farun.0i">
  <kap><tld/>i</kap>
  <snc mrk="farun.0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Kovri ion (ekz. rosta&jcirc;on) per <tld/>o.
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_c;&c_ja;&c_r;&c_u;&c_sh;&c_v;&c_a;&c_c;&c_mol; <ind>&c_m;&c_u;&c_k;&c_o;&c_j;</ind></trd>,
      <trd>&c_a;&c_s;&c_y;&c_p;&c_a;&c_c;&c_mol; <ind>&c_m;&c_u;&c_k;&c_o;&c_j;</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>to flour</trd>,
      <trd>dredge <klr>(cooking)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>opr&oacute;szy&cacute; <ind>m&aogonek;k&aogonek;</ind></trd>,
      <trd>wytarza&cacute; w <ind>m&aogonek;ce</ind></trd>,
      <trd>obtoczy&cacute; w <ind>m&aogonek;ce</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>
