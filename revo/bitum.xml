<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bitum.xml,v 1.21 2023/08/31 08:48:51 revo Exp $">
<kap>
  <rad>bitum</rad>/o
  <ofc>1</ofc>
</kap>

<drv mrk="bitum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Nigre bruna terpe&ccirc;o, uzata por tega&jcirc;o de <ref tip="malprt" cel="strat.0o">stratoj</ref>
      kaj <ref tip="malprt" cel="tegmen.0o">tegmentoj</ref>:
      <ekz>
        la brikoj fari&gcirc;is por ili &scirc;tonoj, kaj la <tld/>o fari&gcirc;is por ili kalko
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 11:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        en <nom>Selenica</nom> trovi&gcirc;as dumiljara minejo de tre bonkvalita <tld/>o, kiu estis uzata
        i.a. por pavimi la bulvardojn de e&ubreve;ropaj &ccirc;efurboj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2011/010474.php">Ri&ccirc;a tero, malri&ccirc;a popolo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la alunskisto estas skisto penetrita per karbo kaj <tld/>aj substancoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=36381">Skisto</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asfalt.0o">asfalto</ref>,
      <ref cel="gudr.0o">gudro</ref>,
      <ref cel="pecx.0o">pe&ccirc;o</ref>,
      <ref cel="pecx.ter0o">terpe&ccirc;o</ref>
    </refgrp>
    <trd lng="be">&c_b;&c_ib;&c_t;&c_u;&c_m;</trd>
    <trd lng="bg">&c_b;&c_i;&c_t;&c_u;&c_m;</trd>
    <trd lng="de">Bitumen</trd>
    <trd lng="en">bitumen</trd>
    <trd lng="fr">bitume</trd>
    <trd lng="hu">bitumen</trd>
    <trd lng="nl">bitumen</trd>
    <trd lng="pl">bitum</trd>
    <trd lng="ru">&c_b;&c_i;&c_t;&c_u;&c_m;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>bitumen</trd>,
    <trd>zemn&iacute; vosk</trd>,
    <trd>&zcaron;ivice</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&#x5D1;&#x5BC;&#x5B4;&#x5D9;&#x5D8;&#x5D5;&#x5BC;&#x5DE;&#x5B5;&#x5DF;</trd>,
    <trd>&#x5D6;&#x5E4;&#x5EA;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x701D;&#x9752;  [<ind>&#x308C;&#x304D;&#x305B;&#x3044;</ind>]</trd>,
    <trd>&#x30D3;&#x30C1;&#x30E5;&#x30FC;&#x30E1;&#x30F3;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>bit&uacute;men</trd>,
    <trd>&zcaron;ivica</trd>
  </trdgrp>
  <trd lng="uk">&#x431;&#x456;&#x442;&#x443;&#x43C;</trd>
</drv>
<drv mrk="bitum.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="bitum.0i.kovri">
    <dif>
      Kovri a&ubreve; &scirc;miri per <tld/>o:
      <ekz>
        antikve oni <tld/>is la kadavrojn por konservi ilin
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=17611">Mumio</url></lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;
  <ind>&c_b;&c_ib;&c_t;&c_u;&c_m;&c_a;&c_m;</ind></trd>
  <trdgrp lng="fr">
    <trd>bitumer</trd>,
    <trd>bituminer</trd></trdgrp> 
  <trd lng="he">&#x5DC;&#x5D6;&#x5E4;&#x5EA;</trd>
  <trdgrp lng="hu">
    <trd>bitumenez</trd>,
    <trd>aszfaltoz</trd>
  </trdgrp>
  <trd lng="ja">&#x701D;&#x9752;&#x3067;&#x8217;&#x88C5;&#x3059;&#x308B;  [<ind>&#x701D;&#x9752;&#x3067;&#x307B;&#x305D;&#x3046;&#x3059;&#x308B;</ind>]</trd>
  <trd lng="nl">bitumineren</trd>
  <trdgrp lng="pl">
    <trd>bitumowa&cacute;</trd>,
    <trd>pokrywa&cacute; <ind>bitumem</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;
  &c_b;&c_i;&c_t;&c_u;&c_m;&c_o;&c_m;</trd>
</drv>
</art>
