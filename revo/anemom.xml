<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anemom.xml,v 1.24 2024/11/10 21:22:59 revo Exp $">
<kap>
  <rad>anemometr</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="anemom.0o">
  <kap><tld/>o</kap>
  <snc mrk="anemom.0o.MET">
    <uzo tip="fak">MET</uzo>
    <dif>
      <ref tip="dif" cel="mezur.vento0ilo.MET">Ventomezurilo</ref>:
      <ekz>
        ventosakoj estas striitaj ru&gcirc;e kaj blanke, tiel bone rekoneblaj, pli precizaj
        aparatoj por mezurado de la vento estas la ventodirekta mezurilo kaj la
        <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=260519">Ventosako</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        lia pranepo <nom>Antoine Breguet</nom> (1851-1882) reguligis elektran <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=510021">Br&eacute;guet - Sabin (pariza
          metrostacio)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_e;&c_m;&c_o;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="cs">
    <trd>anemometr</trd>,
    <trd>v&ecaron;trom&ecaron;r</trd>
  </trdgrp>
  <trdgrp lng="de">
      <trd>Windgeschwindigkeitsmesser</trd>,
      <trd>Windst&auml;rkemesser</trd>,
      <trd>Anemometer</trd>
  </trdgrp>
  <trd lng="en">anemometer</trd>
  <trd lng="es">anem&oacute;metro</trd>
  <trd lng="fa">&a_b;&a_A;&a_d;&a_s;&a_n;&a_j;</trd>
  <trd lng="fr">an&eacute;mom&egrave;tre</trd>
  <trd lng="he">&#x5DE;&#x5B7;&#x5D3; &#x5E8;&#x5D5;&#x5BC;&#x5D7;&#x5B7;</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;l(sebess&eacute;g)m&eacute;r&odblac;</trd>,
    <trd>anemom&eacute;ter</trd>
  </trdgrp>
  <trd lng="ja">&#x98A8;&#x901F;&#x8A08;  <pr>&#12405;&#12358;&#x305D;&#x304F;&#12369;&#12356;</pr></trd>
  <trd lng="nl">windkrachtmeter</trd>
  <trdgrp lng="pl">
    <trd>anemometr</trd>,
    <trd>wiatromierz</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_je;&c_m;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>,
    <trd>&c_v;&c_je;&c_t;&c_r;&c_o;&c_m;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>anemometer</trd>,
    <trd>vetromer</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&#x43D;&c_je;&#x43C;&#x43E;&#x43C;&c_je;&c_t;&c_r;</trd>,
    <trd>&c_v;&c_ib;&c_t;&c_r;&#x43E;&#x43C;&c_ib;&c_r;</trd>
  </trdgrp>
</drv>
</art>
