    <trd>&c_v;&c_je;&c_t;&c_r;&c_o;&c_m;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>anemometer</trd>,
    <trd>vetromer</trd>
  </trdgrp>
</drv>
</art>
