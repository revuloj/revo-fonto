<art mrk="$Id$">
<kap>
  <rad>infarkt</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="infarkt.0o">
  <kap><tld/>o</kap>
  <snc mrk="infarkt.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Loka, amasa morti&gcirc;o de &ccirc;eloj pro
      nesufi&ccirc;a alveno de sango:
      <ekz>
        la loka malutila&jcirc;o konsistas en ia alia malsano de la okulo, 
        <klr>...</klr> kiel ekzemple <klr>...</klr> <tld/>oj en
        la glandetoj Meibomaj
        <fnt>
          <bib>FK</bib>
          <lok>Kronika Katara Konjunktivito</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="es">infarto</trd>
  <trd lng="fr">infarctus</trd>
</drv>
</art>

