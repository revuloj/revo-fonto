<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>gast</rad>/o</kap>
<drv mrk="gast.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gast.0o.senpaga">
    <dif>
      Vizitanto, amike kaj senpage akceptata de dommastro &ccirc;e la hejmo
      a&ubreve; &ccirc;e la tablo:
      <ekz><tld/>' en tempo mal&gcirc;usta estas &scirc;tono sur brusto<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        <ind>se oni amas la <tld/>on, oni zorgas la paston</ind><fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">ha kedves a <ind>vend&eacute;g</ind>, megter&uuml;l az asztal</trd>
      </ekz>
      <ekz>
        <ind><tld/>o kiel fi&scirc;o balda&ubreve; fari&gcirc;as malfre&scirc;a</ind><fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">harmadnapra mind a <ind>vend&eacute;g</ind>, mind a hal b&uuml;d&ouml;s</trd>
      </ekz>
      <ekz>
        <ind>por <tld/>o ne petita mankas kulero</ind><fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">h&iacute;vatlan <ind>vend&eacute;gnek</ind> sarokban a helye</trd>
      </ekz>
      <ekz>
        estu bona <klr>(bonvenanta)</klr> <tld/>o;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lignaj vila&gcirc;oj, kie la fajro estas ofte terura <tld/>o.
      </ekz>
    </dif>
    <adm>
      Kion signifas &leftquot;bonvenanta&rightquot; en ekz &leftquot;estu bona ~o&rightquot;? Kion
      signifas la ekzemplo entute (eble &leftquot;estu bonvena&rightquot;)? <aut>MB</aut>
    </adm>
  </snc>
  <snc mrk="gast.0o.paganta">
    <dif>
      Vizitanto, pagante akceptata en hotelo a&ubreve; restoracio.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">host</trd>
  <trd lng="de">Gast</trd>
  <trd lng="es">hu&eacute;sped</trd>
  <trd lng="en">guest</trd>
  <trdgrp lng="fr">
    <trd>h&ocirc;te<klr tip="ind"> (invit&eacute; ou client)</klr></trd>,
    <trd>client<klr tip="ind"> (d'un h&ocirc;tel ou d'un restaurant)</klr></trd>,
    <trd>convive</trd>,
    <trd>invit&eacute;</trd>
  </trdgrp>
  <trd lng="hu">vend&eacute;g</trd>  
  <trd lng="nl">gast</trd>
  <trd lng="pt">h&oacute;spede<klr tip="amb"> (recebido)</klr></trd>
  <trd lng="os">&c_u;&c_a;&c_z;&aelig;&c_g;</trd>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sv">g&auml;st</trd>
</drv>

<drv mrk="gast.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>o:
      <ekz>
        &ccirc;e la pastro ni <tld/>as<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_g;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>habiter<klr tip="ind"> (chez qn)</klr></trd>,
    <trd>&ecirc;tre l'<ind>h&ocirc;te</ind><klr tip="ind"> (de qn)</klr></trd>,
    <trd>&ecirc;tre <ind>invit&eacute;</ind></trd>,
    <trd>&ecirc;tre <ind>re&ccedil;u</ind></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vend&eacute;geskedik</trd>,
    <trd>vend&eacute;g <klr>(vhol)</klr></trd>
  </trdgrp>
  <trd lng="nl">te <ind>gast</ind> zijn</trd>
  <trd lng="pt">hospedar-se</trd>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">g&auml;sta</trd>
</drv>

<drv mrk="gast.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Akcepti kiel <tld/>on:
      <ekz>
        ricevi <ind><tld/>igo</ind>n &ccirc;e iu;
        <trd lng="fr">h&eacute;bergement</trd>
        <trd lng="hu">vend&eacute;gl&aacute;t&aacute;s</trd>
      </ekz>
      <ekz>
        li <ind>en<tld/>i&gcirc;i</ind>s en la plej bela hotelo
        <fnt><bib>Fab1</bib>, <lok>Fajrilo</lok></fnt>.
        <trdgrp lng="fr">
          <trd>descendre<klr tip="ind"> (dans un h&ocirc;tel, chez
          qn)</klr></trd>,
          <trd>s'<ind>installer</ind><klr tip="ind"> (dans un h&ocirc;tel, chez
          qn)</klr></trd>
        </trdgrp>
        <trd lng="hu">megsz&aacute;ll <klr>(vhol)</klr></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol;
  &c_g;&c_a;&c_s;&c_mol;&c_c;&c_je;&c_j;</trd>
  <trd lng="es">albergar</trd>
  <trdgrp lng="fr">
    <trd>h&eacute;berger</trd>,
    <trd>accueillir<klr tip="ind"> (un h&ocirc;te)</klr></trd>,
    <trd>recevoir<klr tip="ind"> (un h&ocirc;te)</klr></trd>
  </trdgrp>
  <trd lng="hu">vend&eacute;g&uuml;l l&aacute;t</trd>  
  <trdgrp lng="nl">
    <trd>ontvangen<klr tip="ind"> (v.gast)</klr></trd>,
    <trd>onderdak geven</trd>
  </trdgrp>
  <trd lng="pt">hospedar</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol; &c_g;&c_o;&c_s;&c_t;&c_je;&c_j;</trd>
</drv>

<drv mrk="gast.0iganto">
  <kap><tld/>iganto</kap>
  <snc>
    <dif>
      Tiu, kiu akceptas hejmen <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;</trd>
  <trd lng="de">Gastgeber</trd>
  <trdgrp lng="fr">
    <trd>h&ocirc;te<klr tip="ind"> (qui re&ccedil;oit)</klr></trd>,
    <trd>h&eacute;bergeur<klr tip="ind"> (h&ocirc;te)</klr></trd>
  </trdgrp>
  <trd lng="hu">vend&eacute;gl&aacute;t&oacute;</trd>  
  <trd lng="nl">gastheer</trd>
  <trd lng="os">&c_f;&c_y;&c_s;&c_y;&c_m;</trd>
  <trd lng="pt">h&oacute;spede<klr tip="amb"> (que recebe)</klr></trd>
  <trd lng="ru">&c_h;&c_o;&c_z;&c_ja;&c_i;&c_n; <klr>(&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_ju;&c_shch;&c_i;&c_j; &c_g;&c_o;&c_s;&c_t;&c_je;&c_j;)</klr></trd>
  <trd lng="sv">v&auml;rd</trd>
</drv>

<drv mrk="gast.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Malgranda, malluksa hotelo a&ubreve; restoracio.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="de">Gastst&auml;tte</trd>
  <trd lng="es">albergue</trd>
  <trd lng="fr">auberge</trd>
  <trd lng="hu">vend&eacute;gl&odblac;</trd>  
  <trdgrp lng="nl">
    <trd>herberg</trd>,
    <trd>(klein) <ind>restaurant</ind></trd>,
    <trd>eethuis</trd>
  </trdgrp>
  <trd lng="pl">gospoda</trd>
  <trd lng="pt">hospedaria</trd>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_t;&c_i;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sv">v&auml;rdshus</trd>
</drv>

<drv mrk="gast.0ama">
  <kap><tld/>ama, <tld/>(ig)ema</kap>
  <snc>
    <dif>
      &Scirc;atanta akcepti <tld/>ojn kaj afable trakti ilin:
      <ekz>
        <tld/>ama homo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>ne<tld/>ama</ind> kaj dezerta lando<fnt>Z</fnt>;
        <trd lng="fr">inhospitalier</trd>
        <trd lng="hu">bar&aacute;ts&aacute;gtalan <klr tip="ind">(vend&eacute;ggel)</klr></trd>
        <trd lng="nl">onherbergzaam</trd>
      </ekz>
      <ekz>
        &gcirc;ui la <ind><tld/>ameco</ind>n <klr>(<tld/>igon)</klr>
        de iu<fnt>Z</fnt>.
        <trd lng="fr">hospitalit&eacute;</trd>
        <trd lng="hu">vend&eacute;gszeretet</trd>
        <trd lng="nl">gastvrijheid</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_h;&c_l;&c_je;&c_b;&c_a;&c_s;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">gastfreundlich</trd>
  <trd lng="es">hospitalario</trd>
  <trdgrp lng="fr">
    <trd>accueillant</trd>,
    <trd>hospitalier</trd>
  </trdgrp>
  <trd lng="hu">vend&eacute;gszeret&odblac;</trd>  
  <trd lng="nl">gastvrij</trd>
  <trd lng="pt">hospitaleiro<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_t;&c_je;&c_p;&c_r;&c_i;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">g&auml;stv&auml;nlig</trd>
</drv>
</art>
