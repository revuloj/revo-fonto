<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gast</rad>/o
</kap>
<drv mrk="gast.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Vizitanto amike kaj senpage akceptata de dommastro &ccirc;e la hejmo
      a&ubreve; &ccirc;e la tablo:
      <ekz>
        <tld/>' en tempo mal&gcirc;usta estas &scirc;tono sur brusto
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        se oni amas la <tld/>on, oni zorgas la paston
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        <tld/>o kiel fi&scirc;o balda&ubreve; fari&gcirc;as malfre&scirc;a
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        por <tld/>o ne petita mankas kulero
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>;
      </ekz>
      <ekz>
        estu bona <klr>(bonvenanta)</klr> <tld/>o;
      </ekz>
      <ekz>
        <klr>(f)</klr> lignaj vila&gcirc;oj, kie la fajro estas
        ofte terura <tld/>o.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Vizitanto pagante akceptata en hotelo a&ubreve; restoracio.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">host</trd>
  <trd lng="de">Gast</trd>
  <trd lng="en">guest</trd>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sv">g&auml;st</trd>
</drv>
<drv mrk="gast.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Esti <tld/>o:
      <ekz>
        &ccirc;e la pastro ni <tld/>as<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol;</trd>, 
    <trd>&c_g;&c_a;&c_s;&c_mol;&c_c;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd> 
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sv">g&auml;sta</trd>
</drv>
<drv mrk="gast.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Akcepti kiel <tld/>on:
      <ekz>
        ricevi <tld/>igon &ccirc;e iu;
      </ekz>
      <ekz>
        li en<tld/>i&gcirc;is en la plej bela hotelo
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Fajrilo</vrk>)</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_m;&c_a;&c_c;&c_mol; &c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol; &c_g;&c_o;&c_s;&c_t;&c_je;&c_j;</trd>
</drv>
<drv mrk="gast.0iganto">
  <kap><tld/>iganto</kap>
  <snc>
    <dif>
      Tiu, kiu akceptas hejmen <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_s;&c_p;&c_a;&c_d;&c_a;&c_r;</trd>
  <trd lng="de">Gastgeber</trd>
  <trd lng="ru">&c_h;&c_o;&c_z;&c_ja;&c_i;&c_n; <klr>(&c_p;&c_r;&c_i;&c_n;&c_i;&c_m;&c_a;&c_ju;&c_shch;&c_i;&c_j; &c_g;&c_o;&c_s;&c_t;&c_je;&c_j;)</klr></trd>
  <trd lng="sv">v&auml;rd</trd>
</drv>
<drv mrk="gast.0ejo">
  <kap><tld/>ejo</kap>
  <snc>
    <dif>
      Malgranda, malluksa hotelo a&ubreve; restoracio.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_ib;&c_c;&c_a;</trd>
  <trd lng="de">Gastst&auml;tte</trd>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_t;&c_i;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="sv">v&auml;rdshus</trd>
</drv>
<drv>
  <kap><tld/>ama, <tld/>(ig)ema</kap>
  <snc>
    <dif>
      &Scirc;atanta akcepti <tld/>ojn kaj
      afable trakti ilin:
      <ekz>
        <tld/>ama homo <klr>(f)</klr>;
      </ekz>
      <ekz>
        ne<tld/>ama kaj dezerta lando<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &gcirc;ui la <tld/>amecon <klr>(<tld/>igon)</klr> de iu<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_n;&c_n;&c_y;</trd>, 
    <trd>&c_h;&c_l;&c_je;&c_b;&c_a;&c_s;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd> 
  </trdgrp>
  <trd lng="de">gastfreundlich</trd>
  <trd lng="ru">&c_g;&c_o;&c_s;&c_t;&c_je;&c_p;&c_r;&c_i;&c_i;&c_m;&c_n;&c_y;&c_j;</trd>
  <trd lng="sv">g&auml;stv&auml;nlig</trd>
</drv>
</art>
