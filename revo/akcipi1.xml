  </snc>
  <trd lng="be">&c_ja;&c_s;&c_t;&c_r;&c_a;&c_b;&c_ib;&c_n;&c_y;&c_ja; <klr
  tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="de">Habichtartige</trd>
  <trd lng="en">Accipitridae family</trd>
  <trd lng="fr">accipitrid&eacute;s</trd>
  <trd lng="hu">v&aacute;g&oacute;madarak</trd>
  <trd lng="pt">acipitr&iacute;deos</trd>
  <trd lng="ru">&c_ja;&c_s;&c_t;&c_r;&c_je;&c_b;&c_i;&c_n;&c_y;&c_je; <klr
tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">h&ouml;kartade rovf&aring;glar</trd>
</drv>
</art>

