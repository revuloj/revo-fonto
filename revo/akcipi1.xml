<art mrk="$Id: akcipi1.xml,v 1.14 2023/10/28 08:33:03 revo Exp $">
<kap>
  <rad>akcipitred</rad>/oj <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akcipi1.0oj">
  <kap><tld/>oj</kap>
  <snc mrk="akcipi1.0oj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Tagaj rabobirdoj troveblaj en plejmultaj mondregionoj
      kaj medioj, familio el la ordo <ref tip="super" cel="bird.rabo0oj">rabobirdoj</ref>
      (<trd lng="la">Accipitridae</trd>):
      <ekz>
        la taksonomio de <tld/>oj estas iom konfuza
        <fnt>
          <bib>Viki</bib>,
          <lok>artikolo &leftquot; akcipitredoj&rightquot;, 2009-06-16</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x9280;&#x8033;&#x76F8;&#x601D;&#x9CE5; <pr>y&#xED;n&#x11B;rxi&#x101;ngs&#x12B;ni&#x1CE;o</pr></trd>,
    <trd>&#x94F6;&#x8033;&#x76F8;&#x601D;&#x9E1F; <pr>y&#xED;n&#x11B;rxi&#x101;ngs&#x12B;ni&#x1CE;o</pr></trd>,
    <trd>&#x9DF9;&#x79D1; <pr>y&#x12B;ngk&#x113;</pr></trd>,
    <trd>&#x9E70;&#x79D1; <pr>y&#x12B;ngk&#x113;</pr></trd>
  </trdgrp>
</snc>
  <refgrp tip="sub">
    <ref cel="agl.0o">aglo</ref>,
    <ref cel="akcipi.0o">akcipitro</ref>,
    <ref cel="bute.0o">buteo</ref>,
    <ref cel="milv.0o">milvo</ref>
  </refgrp>
  <trd lng="be">&c_ja;&c_s;&c_t;&c_r;&c_a;&c_b;&c_ib;&c_n;&c_y;&c_ja; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">accip&iacute;trids</trd>
  <trd lng="de">Habichtartige</trd>
  <trd lng="en">Accipitridae family</trd>
  <trd lng="es">accip&iacute;tridos</trd> 
  <trd lng="fr">accipitrid&eacute;s</trd>
  <trd lng="hu">v&aacute;g&oacute;madarak</trd>
  <trd lng="pt">acipitr&iacute;deos</trd>
  <trd lng="ru">&c_ja;&c_s;&c_t;&c_r;&c_je;&c_b;&c_i;&c_n;&c_y;&c_je; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trd lng="sv">h&ouml;kartade rovf&aring;glar</trd>
  <trd lng="sk">jastrabovit&#xED;</trd>
  <trd lng="cs">jest&#x159;&#xE1;bovit&#xED;</trd>
  <trdgrp lng="uk">
    <trd>&#x44F;&#x441;&#x442;&#x440;&#x443;&#x431;&#x43E;&#x432;&#x456;</trd>,
    <trd>&#x43E;&#x440;&#x43B;&#x43E;&#x432;&#x456;</trd>
  </trdgrp>
</drv>
</art>

