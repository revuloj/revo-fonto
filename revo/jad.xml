<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jad.xml,v 1.25 2023/12/03 07:09:06 revo Exp $">
<kap>
  <rad>jad</rad>/o
</kap>
<drv mrk="jad.0o">
  <kap><tld/>o</kap>
  <snc mrk="jad.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Malmola kaj polurebla <ref tip="super" cel="minera.0o">mineral</ref>a
      silikato el alumino kaj kalko, verda a&ubreve; verdeta:
      <ekz>
        la olmekoj uzis <tld/>on kaj aliajn fajnajn &scirc;tonojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16187">Olmekoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        oni elfosis pli ol 1700 objektojn el bronzo, <tld/>o, lako, argilo a&ubreve; oro, 80 el
        eburo, pli ol 4600 monerojn kaj grandan kvanton de konka&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2009/009901.php">Enigmoj de Sanxingdui</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        arkeologoj trovis ruinon de antikva urbo kun areo de 12 kvadrataj kilometroj kaj
        elfosis grandan kvanton da bronzaj, <tld/>aj kaj oraj objektoj
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2009/009901.php">Enigmoj de Sanxingdui</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        li elfosis pli ol 400 delikatajn <tld/>a&jcirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="&Monato;/2009/009901.php">Enigmoj de Sanxingdui</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_e;&c_f;&c_r;&c_y;&c_t;</trd>
  <trd lng="ca">jade</trd>
  <trd lng="cs">ledvinec</trd>
  <trd lng="de">Jade</trd>
  <trd lng="el">&ny;&epsilon;&phi;&rho;&jota_ton;&tau;&eta;&sigma_fina;
  </trd>
  <trd lng="en">jade</trd>
  <trd lng="es">jade</trd>
  <trd lng="fr">jade</trd>
  <trd lng="hu">jade</trd>
  <trd lng="it">giada</trd>
  <trdgrp lng="ja">
    <trd>&#x7FE1;&#x7FE0;  <pr>&#x304B;&#x308F;&#x305B;&#x307F;</pr></trd>,
    <trd>&#x7389;  <pr>&#x304E;&#x3087;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">jade</trd>
  <trd lng="pl">jadeit</trd>
  <trd lng="pt">jade</trd>
  <trd lng="ru">&c_n;&c_je;&c_f;&c_r;&c_i;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>nefrit</trd>,
    <trd>&lcaron;advinec</trd>
  </trdgrp>
  <trd lng="sv">jade</trd>
  <trd lng="uk">&#x43D;&#x435;&#x444;&#x440;&#x438;&#x442;</trd>
  <trdgrp lng="zh">
    <trd>&#x7389; <pr>y&#xF9;</pr></trd>,
    <trd>&#x73C3; <pr>r&#x1CE;n</pr></trd>,
    <trd>&#x73D4; <pr>ji&#xE0;n</pr></trd>,
    <trd>&#x73D6; <pr>gu&#x101;ng</pr></trd>,
    <trd>&#x7407; <pr>xi&#xF9;</pr></trd>,
    <trd>&#x7428; <pr>k&#x16B;n</pr></trd>,
    <trd>&#x742D; <pr>l&#xF9;</pr></trd>,
    <trd>&#x7441; <pr>m&#xE0;o</pr></trd>,
    <trd>&#x744F; <pr>chu&#x101;n</pr></trd>,
    <trd>&#x746D; <pr>t&#xE1;ng</pr></trd>
  </trdgrp>
</drv>
</art>
