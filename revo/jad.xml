  <trd lng="el">&ny;&epsilon;&phi;&rho;&jota_ton;&tau;&eta;&sigma_fina;
  </trd>
  <trd lng="en">jade</trd>
  <trd lng="es">jade</trd>
  <trd lng="fr">jade</trd>
  <trd lng="hu">jade</trd>
  <trd lng="nl">jade</trd>
  <trd lng="pl">jadeit</trd>
  <trd lng="pt">jade</trd>
  <trd lng="ru">&c_n;&c_je;&c_f;&c_r;&c_i;&c_t;</trd>
  <trd lng="sk">nefrit</trd>
  <trd lng="sv">jade</trd>
  <trd lng="it">giada</trd>
</drv>
</art>
