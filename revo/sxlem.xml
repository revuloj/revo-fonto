<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxlem.xml,v 1.6 2023/12/01 01:43:15 revo Exp $">
<kap>
  <rad>&scirc;lem</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sxlem.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxlem.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="venk.0o.KOMUNE">Venko</ref> en &ccirc;iuj
      a&ubreve; preska&ubreve; &ccirc;iuj ma&ccirc;oj de serio:
      <ekz>
        Tiger Woods gajnis sian kvaran grandan <tld/>on en golfludo
        <fnt>
          <aut>H. Gravatt</aut>,
          <vrk>Adia&ubreve; Jaro 2001...</vrk>,
          <lok><url ref="http://www.esperanto.org.nz/af/ag-filiko-13.html">
          Ar&gcirc;enta Filiko, 2002-03</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la nacio kiu gajnas &ccirc;iujn siajn mat&ccirc;ojn kontra&ubreve;
        la aliaj nacioj gajnas anka&ubreve; la Grandan <tld lit="&Scirc;"/>on
        <fnt>
          <aut>-</aut>,
          <vrk>Granda &Scirc;lemo por Kimrio</vrk>,
          <lok><url ref="http://www.eventeo.net/web/">eventeo</url>,
          2008-03-16</lok>
        </fnt>.
     </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_l;&c_je;&c_m; <klr tip="amb">(&c_u; &c_g;&c_u;&c_l;&c_mol;&c_n;&c_ib;)</klr></trd>
  <trd lng="de">Slam</trd>
  <trd lng="fr">chelem</trd>
  <trd lng="it">slam</trd>
  <trd lng="pl">szlem</trd>
  <trd lng="uk">&#x43A;&c_a;&c_r;&c_t;. &c_sh;&#x43B;&c_je;&#x43C;</trd>
</drv>
</art>

