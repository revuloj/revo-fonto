<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krik.xml,v 1.20 2023/12/03 09:59:01 revo Exp $">
<kap>
  <rad>krik</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krik.0o">
  <kap><tld/>o</kap>
  <snc mrk="krik.0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="lev.0ilo.aparato">Levilo</ref>,
      havanta <ref tip="prt" cel="dent.0ostango">dentostango</ref>n 
      suprenlevatan de dentradeto, movata per
      <ref tip="prt" cel="krank.0o">kranko</ref> kaj uzata por levi pezajn &scirc;ar&gcirc;ojn:
      <ekz>
        <tld/>o de pumpilo;
      </ekz>
      <ekz>
        <ind>veturil<tld/>o</ind>;
        <trd lng="de">Wagenheber</trd>
        <trd lng="hu">kocsiemel&odblac;</trd>
      </ekz>
      <ekz>
        mia pa&ccirc;jo havas du a&ubreve;tojn, kaj <klr>[&#x2026;]</klr> por esti sekura, mia pa&ccirc;jo havas
        anka&ubreve; du <tld/>ojn
        <fnt>
          <bib>KrB</bib>
          <lok>Elingita glavo</lok>
        </fnt>.
      </ekz>   
    </dif>
    <refgrp tip="vid">
      <ref cel="argan.0o">argano</ref>,
      <ref cel="kapsta.0o">kapstano</ref>,
      <ref cel="vincx.0o">vin&ccirc;o</ref>,
      <ref cel="vindas.0o">vindaso</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/c/c9/Cric-L&eacute;onard.jpg/420px-Cric-L&eacute;onard.jpg" prm="PD">desegno de <tld/>o, Leonardo da Vinci
        <fnt>
          <aut>L&eacute;onard de Vinci, Codex Atlanticus f.259r</aut>
          <lok><url ref="&WCW;/File:Cric-L%C3%A9onard.jpg">&WCW;/File:Cric-L&eacute;onard.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_m;&c_k;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>hever</trd>,
    <trd>zved&aacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Winde</trd>,
    <trd>Zahnstangenwinde</trd>
  </trdgrp>
  <trd lng="en">jack</trd>
  <trd lng="fr">cric</trd>
  <trd lng="hu">emel&odblac;</trd>
  <trd lng="ja">&#x30B8;&#x30E3;&#x30C3;&#x30AD;</trd>
  <trd lng="nl">krik</trd>
  <trd lng="pl">lewar</trd>
  <trd lng="pt">macaco <klr tip="amb">(de autom&oacute;vel)</klr></trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_k;&c_r;&c_a;&c_t;</trd>
  <trd lng="sk">hever <klr>(s ozub. kolesom)</klr></trd>
  <trd lng="uk">&#x434;&#x43E;&#x43C;&#x43A;&#x440;&#x430;&#x442;</trd>
  <trd lng="zh">&#x63D2;&#x5B54; <pr>ch&#x101;k&#x1D2;ng</pr></trd>
</drv>
</art>
