<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>krik</rad>/o
</kap>
<drv mrk="krik.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Speco de levilo, havanta dentostangon suprenlevatan de dentradeto,
      movata per kranko kaj uzata por levi pezajn &scirc;ar&gcirc;ojn:
      <ekz>
        <tld/>o de pumpilo;
      </ekz>
      <ekz>
        <ind>veturil<tld/>o</ind>;
        <trd lng="hu">kocsiemel&odblac;</trd>
      </ekz>
      <ekz>
        nitinga <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="argan.0o">argano</ref>,
      <ref cel="kapsta.0o">kapstano</ref>,
      <ref cel="vindas.0o">vindaso</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="dent.0ostango">dentostango</ref>,
      <ref cel="krank.0o">kranko</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_m;&c_k;&c_r;&c_a;&c_t;</trd>
  <trdgrp lng="de">
     <trd>Winde</trd>,
     <trd>Zahnstangenwinde</trd>,
     <trd>Wagenheber</trd>
  </trdgrp>
  <trd lng="en">jack</trd>
  <trd lng="hu">emel&odblac;</trd>
  <trd lng="pl">lewar</trd>
  <trd lng="ru">&c_d;&c_o;&c_m;&c_k;&c_r;&c_a;&c_t;</trd>
</drv>
</art>
