<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>tuj</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="tuj1.0o">
   <kap><tld/>o</kap>
   <snc mrk="tuj1.0o.BOT">
     <uzo tip="fak">BOT</uzo>
     <dif>
       &Ccirc;iamverda arbeto, tre ofta en
       &gcirc;ardenoj kaj kies ligno estas valora;
       genro el familio <ref tip="super"
       cel="cipres1.0oj">cipresacoj</ref>
       (<trd lng="la">Thuja</trd>):
       <ekz>
         Martin sentis la odoron de <tld/>oj
         <fnt><aut>I. Nemere</aut>, <vrk>Sur Kampo Granita</vrk>, 
         <lok>1983</lok></fnt>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_t;&c_u;&c_ja;</trd>
   <trd lng="de">Lebensbaum</trd>
   <trd lng="fr">thuya</trd>
   <trdgrp lng="hu">
     <trd>tuja</trd>,
     <trd>&eacute;letfa</trd>
   </trdgrp>
   <trd lng="nl">thuja</trd>
   <trdgrp lng="pl">
     <trd>tuja</trd>,
     <trd>&zdot;ywotnik</trd>
   </trdgrp>
   <trd lng="pt">tuia</trd>
   <trd lng="ru">&c_t;&c_u;&c_ja;</trd>
   <trd lng="uk">&c_t;&c_u;&c_ja;</trd>
</drv>
</art>
