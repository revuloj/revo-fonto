      <drv mrk="kvar.0a">
        <kap><ofc>*</ofc><tld/>a</kap>
        <snc mrk="kvar.0a.MAT">
          <dif>
            Vicmontra numeralo responda al <tld/>;
            kiu (la&ubreve; iu ordo) venas
            post tri anta&ubreve;aj:
            <ekz>
              aprilo estas la <tld/>a monato de la jaro.
            </ekz>
          </dif>
        </snc>
  <trd lng="be">&c_ch;&c_a;&c_c;&c_mol;&c_v;&c_je;&c_r;&c_t;&c_y;</trd>
  <trdgrp lng="br">
          <trd>pevare<klr tip="ind"> (ag.)</klr></trd>,
          <trd>pevarvet<klr> [vira]</klr></trd>,
          <trd>pedervet<klr> [ina]</klr></trd>
        </trdgrp>
  <trd lng="cs">&ccaron;tvrt&yacute;</trd>
  <trd lng="de">vierte(r, -s)</trd>
  <trd lng="en">fourth</trd>
  <trd lng="es">cuarto</trd>
  <trd lng="fi">nelj&auml;s</trd>
  <trd lng="fr">quatri&egrave;me</trd>
  <trd lng="hu">negyedik</trd>
  <trd lng="id">ke<ind>empat</ind></trd>
  <trd lng="la">quartus</trd>
  <trd lng="nl">vierde</trd>
  <trd lng="pl">czwarty</trd>
  <trdgrp lng="ro">
          <trd>a patra</trd>,
          <trd>al patrulea</trd>,
        </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_t;&c_v;&c_jo;&c_r;&c_t;&c_y;&c_j;</trd>
  <trd lng="sk">&scaron;tvrt&yacute;</trd>
  <trd lng="sv">fj&auml;rde</trd>
</drv>

      <drv mrk="kvar.0e">
        <kap><tld/>e</kap>
        <snc mrk="kvar.0e.MAT">
          <dif>
            En la <tld/>a loko, post tri anta&ubreve;aj.
          </dif>
        </snc>
  <trd lng="br">da <ind>bevare</ind></trd>
  <trd lng="cs">za &ccaron;tvrt&eacute;</trd>
  <trdgrp lng="de">
          <trd>als Vierter</trd>,
          <trd>zu viert</trd>
        </trdgrp>
  <trd lng="en">fourthly</trd>
  <trd lng="fi">nelj&auml;nneksi</trd>
  <trd lng="fr">quatri&egrave;mement</trd>
  <trd lng="hu">negyedszer <klr tip="ind">(felsorol&aacute;sn&aacute;l)</klr></trd>
  <trd lng="nl">ten <ind>vierde</ind></trd>
  <trd lng="pl">po czwarte</trd>
  <trd lng="ro">pentru a patra oar&abreve;</trd>
  <trd lng="ru">&c_v;-&c_ch;&c_je;&c_t;&c_v;&c_jo;&c_r;&c_t;&c_y;&c_h;</trd>
  <trd lng="sk">za &scaron;tvrt&eacute;</trd>
</drv>

      <drv mrk="kvar.0angula">
        <kap><tld/>angula</kap>
        <snc>
          <dif>
            Havanta kvar angulojn.
          </dif>
        </snc>
  <trdgrp lng="be">
          <trd>&c_ch;&c_a;&c_t;&c_y;&c_r;&c_o;&c_h;&c_v;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>,
          <trd>&c_ch;&c_a;&c_t;&c_y;&c_r;&c_o;&c_h;&c_k;&c_u;&c_t;&c_n;&c_y;</trd>
        </trdgrp>
  <trd lng="br">pevarc'hornek</trd>
  <trd lng="cs">&ccaron;ty&rcaron;&uacute;heln&iacute;kov&yacute;</trd>
  <trd lng="de">viereckig</trd>
  <trd lng="en">quadrangular</trd>
  <trd lng="es">cuadrangular</trd>
  <trd lng="fi">nelikulmainen</trd>
  <trd lng="fr">quadrangulaire</trd>
  <trd lng="he">&mem;&resh;&vav;&bet;&ayin;</trd>
  <trd lng="hu">n&eacute;gysz&ouml;g&udblac;</trd>
  <trd lng="nl">vierhoekig</trd>
  <trd lng="pl">czterok&aogonek;tny</trd>
  <trd lng="ru">&c_ch;&c_je;&c_t;&c_y;&c_r;&c_jo;&c_h;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">&scaron;tvoruho&lcaron;n&iacute;kov&yacute;</trd>
</drv>

      <drv mrk="kvar.0ono">
        <kap><ofc>*</ofc><tld/>ono
        <fnt><bib>F</bib>, <lok>Universala Vortaro,
        -on'</lok></fnt></kap>
        <snc mrk="kvar.0ono.MAT">
          <dif>
            &Ccirc;iu el la <tld/> egalaj partoj de unu tuto.
          </dif>
        </snc>
  <trd lng="be">&c_ch;&c_v;&c_e;&c_r;&c_c;&c_mol;</trd>
  <trd lng="bo">&#xF56;&#xF5E;&#xF72;&#xF0B;&#xF46;&#xF0B;&#xF42;&#xF45;&#xF72;&#xF42;&#xF0B;</trd>
  <trdgrp lng="br">
          <trd>kard</trd>,
          <trd>pevarvedenn</trd>,
          <trd>pevarvenn</trd>,
          <trd>palefarzh</trd>
        </trdgrp>
  <trdgrp lng="cs">
    <trd>&ccaron;tvrt</trd>,
    <trd>&ccaron;tvrtina</trd>,
    <trd>&ccaron;tvrtka</trd>
  </trdgrp>
  <trd lng="de">Viertel</trd>
  <trd lng="en">quarter</trd>
  <trd lng="es">un <ind>cuarto</ind></trd>
  <trdgrp lng="fi">
          <trd>nelj&auml;nnes</trd>,
          <trd>nelj&auml;sosa</trd>,
        </trdgrp>
  <trd lng="fr">quart</trd>
  <trd lng="he">&resh;&bet;&ayin;</trd>
  <trd lng="hu">negyed</trd>
  <trd lng="id">per<ind>empat</ind></trd>
  <trd lng="nl">vierde<klr tip="ind"> (deel)</klr></trd>
  <trdgrp lng="pl">
          <trd>&cacute;wier&cacute;</trd>,
          <trd>jedna czwarta</trd>,
        </trdgrp>
  <trdgrp lng="ro">
          <trd>p&abreve;trime</trd>,
          <trd>un sfert</trd>,
        </trdgrp>
  <trd lng="ru">&c_ch;&c_je;&c_t;&c_v;&c_je;&c_r;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>&scaron;tvrtina</trd>,
    <trd>&scaron;tvrtka</trd>
  </trdgrp>
</drv>

      <drv mrk="kvar.0onjaro">
        <kap><tld/>onjaro</kap>
        <snc mrk="kvar.0onjaro.ajna">
          <dif>
            <tld lit="K"/>ono da <ref tip="malprt" cel="jar.0o.AST">jaro</ref>,
            duono da <ref tip="malprt" cel="jar.duon0o.AST">duonjaro</ref>:
            <ekz>
              unue mi havos bonan rosta&jcirc;on, poste multe da graso, kiu
              gutos kaj sufi&ccirc;os por pano dum <tld/>onjaro
              <fnt>
                <aut>Fratoj Grimm, tr. Kabe</aut>: <vrk>Feli&ccirc;a
Jo&ccirc;jo</vrk>
              </fnt>;
            </ekz><ekz>
              dum la unua <tld/>onjaro post la baloto (novembro 1999),
              la nova registaro tre popularis
              <fnt>
                <aut>Donald ROGERS</aut>:
                <vrk>Nov-Zelando: &Scirc;an&gcirc;i&gcirc;as la direkto</vrk>. 
                <bib>Monato</bib>
              </fnt>;
            </ekz><ekz>
              &leftquot;<tld/>onjaro&rightquot; (= mezuro) estas parto da jaro, sed
              &leftquot;printempo&rightquot; (= speciale montrita parto) estas parto de
              jaro
	    <fnt><bib>LR</bib>,
	      <lok>Respondo 16a, La Revuo, 1907, junio</lok></fnt>.
            </ekz>
          </dif>
          <trd lng="fi">nelj&auml;nnesvuosi</trd>
          <trd lng="ru">&c_ch;&c_je;&c_t;&c_v;&c_je;&c_r;&c_t;&c_mol; &c_g;&c_o;&c_d;&c_a;</trd>
        </snc>
        <snc mrk="kvar.0onjaro.numerita">
          <uzo tip="fak">KAL</uzo>
          <ref tip="dif" cel="kvar.jar0ono.KAL">jar<tld/>ono</ref>
        </snc>
  <trdgrp lng="cs">
    <trd>kvart&aacute;l</trd>,
    <trd>&ccaron;tvrtlet&iacute;</trd>
  </trdgrp>
  <trd lng="id">triwulan</trd>
  <trdgrp lng="nl">
          <trd>kwartaal</trd>,
          <trd>trimester</trd>
        </trdgrp>
  <trdgrp lng="sk">
    <trd>kvart&aacute;l</trd>,
    <trd>&scaron;tvr&tcaron;rok</trd>
  </trdgrp>
</drv>

      <drv mrk="kvar.jar0ono">
        <kap>jar<tld/>ono</kap>
        <snc mrk="kvar.jar0ono.KAL">
          <uzo tip="fak">KAL</uzo>
          <dif>
            &Ccirc;iu el la <tld/> fiksitaj kaj numeritaj <tld/>onoj de la
            <ref tip="malprt" cel="jar.0o.KAL">jaro</ref>, la periodoj
            po tri sinsekvaj monatoj; la unua jar<tld/>ono
            komenci&gcirc;as je la 1a de januaro:
            <ekz>
              la unua kaj la dua jar<tld/>onoj formas la unuan
              <ref tip="malprt" cel="du.jar0ono">jarduonon</ref>,
              la tria kaj la <tld/>a jar<tld/>onoj formas la duan
              jarduonon;
            </ekz>
            <ekz>
              la leter-fako de la Eksterlanda Programo de Pola Radio
&jcirc;us
              publikigis informojn pri la statistiko de la
koresponda&jcirc;oj
              alvenintaj en la unua jar<tld/>ono de 1999 <klr>...</klr>
              <fnt>
                <aut>Andrzej Pettyn</aut>: <vrk>Pola Radio</vrk>.
                <bib>LOdE</bib>, <lok>1999. N-ro 7 (57)</lok>
              </fnt>;
            </ekz><ekz>
              pozitiva jar<tld/>ono por firmao FIAT
              <fnt>
                <aut>Radio Itala Internacia</aut>,
                <lok><url ref="http://radioarkivo.org/vidu.php/2312">2005-07-30</url></lok>
              </fnt>
            </ekz>
          </dif>
        </snc>
  <trdgrp lng="cs">
    <trd>kvart&aacute;l</trd>,
    <trd>&ccaron;tvrtlet&iacute;</trd>
  </trdgrp>
  <trd lng="de">Quartal</trd>
  <trdgrp lng="en">
          <trd>quarter<klr> (of a year)</klr></trd>,
          <trd>trimester</trd>
        </trdgrp>
  <trd lng="fi">vuosinelj&auml;nnes</trd>
  <trd lng="fr">trimestre</trd>
  <trdgrp lng="nl">
          <trd>kwartaal</trd>,
          <trd>trimester</trd>
        </trdgrp>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l;<klr> &c_g;&c_o;&c_d;&c_a;</klr></trd>
  <trdgrp lng="sk">
    <trd>kvart&aacute;l</trd>,
    <trd>&scaron;tvr&tcaron;rok</trd>
  </trdgrp>
</drv>

      <drv mrk="kvar.0onjara">
        <kap><tld/>onjara</kap>
        <snc mrk="kvar.0onjara.iu">
          <dif>
            Rilata al <tld/>onjaro a&ubreve; jar<tld/>ono:
            <ekz>
              <tld/>onjara plano, enspezo, pago.
            </ekz>
          </dif>
          <trd lng="fi">nelj&auml;nnesvuosi-</trd>
          <trd lng="ru">&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
        </snc>
        <snc mrk="kvar.0onjara.KAL">
          <dif>
            Okazanta a&ubreve; aperanta &ccirc;iun
            <tld/>onjaron, po jar<tld/>one:
            <ekz>
              <tld/>onjara revuo.
            </ekz>
          </dif>
          <trd lng="fi">nelj&auml;nnesvuosittainen</trd>
          <trd lng="ru">&c_je;&c_zh;&c_je;&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
        </snc>
  <trdgrp lng="be">
          <trd>&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
          <trd>&c_sh;&c_t;&c_o;&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
        </trdgrp>
  <trd lng="br">trimiziek</trd>
  <trd lng="cs">&ccaron;tvrtlet&yacute;</trd>
  <trd lng="de">viertelj&auml;hrlich</trd>
  <trd lng="en">quarterly</trd>
  <trd lng="es">trimestre</trd>
  <trd lng="fr">trimestriel</trd>
  <trd lng="he">&resh;&bet;&ayin;&vav;&nun;&yod;</trd>
  <trd lng="hu">negyed&eacute;vi</trd>
  <trdgrp lng="nl">
          <trd>trimestrieel</trd>,
          <trd>driemaandelijks</trd>
        </trdgrp>
  <trd lng="pl">kwartalny</trd>
  <trd lng="ro">trimestrul</trd>
  <trd lng="sk">&scaron;tvr&tcaron;ro&ccaron;n&yacute;</trd>
</drv>

