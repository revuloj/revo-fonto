<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>kvar</rad></kap>
<drv mrk="kvar.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Dufoje du. Matematika simbolo 4.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_t;&c_y;&c_r;&c_y;</trd>
  <trd lng="bg">&c_ch;&c_je;&c_t;&c_i;&c_r;&c_i;</trd>
  <trd lng="cs">&ccaron;ty&rcaron;i</trd>
  <trd lng="de">vier</trd>
  <trd lng="en">four</trd>
  <trd lng="es">cuatro</trd>
  <trd lng="fr">quatre</trd>
  <trd lng="he">&alef;&resh;&bet;&ayin;</trd>
  <trd lng="nl">vier</trd>
  <trd lng="pl">cztery</trd>
  <trd lng="ru">&c_ch;&c_je;&c_t;&c_y;&c_r;&c_je;</trd>
  <trd lng="sv">fyra</trd>
  <trd lng="tr">d&ouml;rt</trd>
  <trd lng="vo">fol</trd>
</drv>

<drv mrk="kvar.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="K"/>aranga, venanta post tri anta&ubreve;aj:
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_c;&c_mol;&c_v;&c_je;&c_r;&c_t;&c_y;</trd>
  <trd lng="nl">vierde</trd>
</drv>

<drv mrk="kvar.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld lit="K"/>arange, post tri anta&ubreve;aj.
    </dif>
  </snc>
  <trd lng="nl">ten <ind>vierde</ind></trd>
</drv>

<drv mrk="kvar.0angula">
  <kap><tld/>angula</kap>
  <snc>
    <dif>
      Havanta kvar angulojn.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_t;&c_y;&c_r;&c_o;&c_h;&c_v;&c_u;&c_g;&c_o;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_ch;&c_a;&c_t;&c_y;&c_r;&c_o;&c_h;&c_k;&c_u;&c_t;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">viereckig</trd>
  <trd lng="en">quadrangular</trd>
  <trd lng="he">&mem;&resh;&vav;&bet;&ayin;</trd>
  <trd lng="nl">vierhoekig</trd>
</drv>

<drv mrk="kvar.0ono">
  <kap><tld/>ono</kap>
  <snc>
    <dif>
      Kvara parto.
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_v;&c_e;&c_r;&c_c;&c_mol;</trd>
  <trd lng="de">Viertel</trd>
  <trd lng="he">&resh;&bet;&ayin;</trd>
  <trd lng="nl">vierde<klr tip="ind"> (deel)</klr></trd>
</drv>

<drv mrk="kvar.0onjara">
  <kap><tld/>onjara</kap>
  <snc>
    <dif>
      &Ccirc;iun kvaronan jaron, po &ccirc;iu kvarona jaro.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_sh;&c_t;&c_o;&c_k;&c_v;&c_a;&c_r;&c_t;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">viertelj&auml;rlich</trd>
  <trd lng="he">&resh;&bet;&ayin;&vav;&nun;&yod;</trd>
  <trd lng="nl">trimestrieel</trd>
</drv>
</art>
