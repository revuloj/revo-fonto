<drv mrk="kvar.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="kvar.0a.MAT">
    <dif>
      Kiu (la&ubreve; iu ordo) venas post tri anta&ubreve;aj:
      <ekz>
        Januaro estas la unua monato de la jaro, Aprilo estas la <tld/>a
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis mateno, la <tld/>a tago
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 1:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        la dek-<tld/>a jaro
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 14:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        kupeo de <tld/>a klaso
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>Du blinduloj</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estas en la <tld/>a monato de naskonteco
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a eta&gcirc;o
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;is la kvara horo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la <tld/>a generacio ili revenos
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 15:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fama <nom>Panini</nom> <klr>[&#8230;]</klr> vivis en la <tld/>a jarcento anta&ubreve; nia erao
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        tri <tld/>onoj de la &scirc;ipanaro forkuris kaj ke la <tld/>a sekvis la ekzemplon de
        sia estro
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>El Odeso &gcirc;is Konstantinopolo</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_c;&c_mol;&c_v;&c_jo;&c_r;&c_t;&c_y;</trd>
  <trdgrp lng="br">
    <trd>pevare<klr tip="ind"> (ag.)</klr></trd>,
    <trd>pevarvet<klr> [vira]</klr></trd>,
    <trd>pedervet<klr> [ina]</klr></trd>
  </trdgrp>
  <trd lng="cs">&ccaron;tvrt&yacute;</trd>
  <trd lng="de">vierte<klr>(r, -s)</klr></trd>
  <trd lng="en">fourth</trd>
  <trd lng="es">cuarto</trd>
  <trd lng="fi">nelj&auml;s</trd>
  <trd lng="fr">quatri&egrave;me</trd>
  <trd lng="hu">negyedik</trd>
  <trd lng="id"><baz>empat</baz>keempat</trd>
  <trdgrp lng="ja">
    <trd>&#65300;&#30058;&#30446;&#12398; <pr>&#12424;&#12387;&#12400;&#12435;&#12417;&#12398;</pr></trd>,
    <trd>&#31532;&#22235;&#12398; <pr>&#12384;&#12356;&#12424;&#12387;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="la">quartus</trd>
  <trd lng="nl">vierde</trd>
  <trd lng="pl">czwarty</trd>
  <trdgrp lng="ro">
