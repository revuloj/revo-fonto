<drv mrk="kvar.0">
  <kap><ofc>*</ofc><tld/></kap>
  <mlg>4</mlg>
  <snc mrk="kvar.0.MAT">
    <dif>
      Du kaj du:
      <ekz>
        por miaj <tld/> infanoj mi a&ccirc;etis dek du pomojn
        <fnt><bib>F</bib>, <lok>&FE;27.</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj <tld/> ekiris kune
        <fnt>
          <bib>ElektFab</bib>
          <lok>La bremenaj muzikistoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        ok estas <tld/> kvinonoj de dek
        <fnt>
          <bib>F</bib>
          <lok>&FE; 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi finos la tutan libron en <tld/> tagoj
        <fnt>
          <bib>F</bib>
          <lok>&FE; 14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> vivis tridek <tld/> jarojn
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 11:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lar&gcirc;o de &ccirc;iu tapi&scirc;o estu <tld/> ulnoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 26:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/> evangeliistoj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/> bremenaj muzikistoj
        <fnt>
          <bib>ElektFab</bib>
          <lok>La bremenaj muzikistoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        sed vi donis al mi <tld/>
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>cent spesmiloj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ian ludadon a&ubreve;dis nur la <tld/> muroj de &scirc;ia bela saloneto
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        unu, du, tri, <tld/>, kaj finita la far'
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        certe, kiel duoble du <tld/>
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; <tld/> piedoj de &ccirc;evalo &gcirc;in ne savas de falo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
