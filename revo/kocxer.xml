<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kocxer.xml,v 1.24 2025/11/15 12:53:39 revo Exp $">
<kap>
  <rad>ko&ccirc;er</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kocxer.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="vetur.0igisto">Veturigisto</ref> de
      kale&scirc;o:
      <ekz>
        <tld/>o de funebra veturilo
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili elkuregis, kaptis la &ccirc;evalojn je la kondukiloj, mortigis <klr>[&#x2026;]</klr> la
        <tld/>on kaj la servistojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de Nikolao <nom>Petrovi&ccirc;</nom> elkondukis la &ccirc;evalojn
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 2a, p.
        10a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>kale&scirc;isto</ke>, <ke>luveturigisto</ke>.
  </rim>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_z;&c_a;&c_k;</trd>,
    <trd>&c_r;&c_a;&c_m;&c_ib;&c_z;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>cotxer</trd>,
    <trd>majoral</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Kutscher</trd>,
    <trd>Fuhrmann</trd>
  </trdgrp>
  <trd lng="en">coachman</trd>
  <trdgrp lng="fr">
    <trd>cocher</trd>
  </trdgrp>
  <trd lng="nl">koetsier</trd>
  <trdgrp lng="pl">
    <trd>stangret</trd>,
    <trd>wo&zacute;nica</trd>,
    <trd>furman</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>vizitiu</trd>,
    <trd>c&abreve;r&abreve;u&scomma;</trd>
  </trdgrp>

  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_ch;&c_je;&c_r;</trd>,
    <trd>&c_i;&c_z;&c_v;&c_o;&c_z;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_ja;&c_m;&c_shch;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_o;&c_z;&c_n;&c_i;&c_ch;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8ECA;&#x592B; <pr>ch&emacron;f&umacron;</pr></trd>,
    <trd>&#x99AC;&#x8ECA;&#x4F15; <pr>m&#x1CE;ch&emacron;fu</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kocxer.K0o">
  <kap><tld lit="K"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
  </kap>
  <mlg>Aur</mlg>
  <snc>
    <dif>
      Nord-hemisfera stelfiguro (<trd lng="la">Auriga</trd>):
      <ekz>
        NGC 1883 estas malfermita stelamaso en konstelacio <tld lit="K"/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=111721"
              >NGC 1883</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="cxar1.CX0isto">&Ccirc;aristo</ref>
  </snc>
  <trd lng="be">&c_V;&c_o;&c_z;&c_mol;&c_n;&c_ib;&c_k; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;&#x2019;&c_je;)</klr></trd>
  <trd lng="ca">el Cotxer</trd>
  <trd lng="de">Fuhrmann</trd>
  <trd lng="en">Auriga</trd>
  <trd lng="es">Cochero</trd>
  <trd lng="fr">Cocher<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="ja">&#x5FA1;&#x8005;  <pr>&#x304E;&#x3087;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">Voerman<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trd lng="pl">Wo&zacute;nica  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Vizitiul <klr>(constela&tcomma;ie)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_k;&c_u;&c_ch;&c_je;&c_r;</trd>,
    <trd>&c_v;&c_ib;&c_z;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trd lng="zh">&#x5FA1;&#x592B;&#x5EA7; <pr>Y&ugrave;f&umacron;zu&ograve;</pr></trd>
</drv>

</art>
