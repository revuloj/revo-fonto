<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sof</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="sof.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sof.0o.MEBL">
    <dif>
      <ref tip="vid" cel="kanap.0o.MEBL">Kanapo</ref>
      kun dikaj kusenoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="divan.0o">divano</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_a;&c_f;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_k;&c_a;&c_n;&c_a;&c_p;&c_je;</trd>,
    <trd>&c_d;&c_i;&c_v;&c_a;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">sof&agrave;</trd>
  <trd lng="de">Sofa</trd>
  <trd lng="en">sofa</trd>
  <trd lng="es">sof&aacute;</trd>
  <trd lng="fr">sofa</trd>
  <trdgrp lng="hu">
    <trd>sz&oacute;fa</trd>,
    <trd>hever&odblac;</trd>
  </trdgrp>
  <trd lng="nl">sofa</trd>
  <trd lng="pl">sofa</trd>
  <trd lng="ru">&c_s;&c_o;&c_f;&c_a;</trd>
  <trd lng="sk">pohovka</trd>
  <trd lng="sv">soffa</trd>
</drv>
</art>
