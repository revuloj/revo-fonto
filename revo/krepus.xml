<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krepusk</rad>/o
</kap>
<drv mrk="krepus.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Nehela lumo, kiun estigas sunaj radioj resenditaj de la tera
      atmosfero post subiro de la suno kaj anta&ubreve; &gcirc;ia 
levi&gcirc;o.
    </dif>
    <trdgrp lng="en">
      <trd>twilight</trd>,
      <trd>dawn <klr>(matenru&gcirc;o)</klr></trd>,
      <trd>dusk <klr>(vesperru&gcirc;o)</klr></trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ekkaduki&gcirc;o, ekfini&gcirc;o, ekmalforti&gcirc;o:
      <ekz>
        mia cerbo na&gcirc;as kvaza&ubreve; en <tld/>o
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="agoni.0o">agonio</ref>,
      <ref cel="dekade.0o">dekadenco</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_r;&c_o;&c_k;</trd>,
    <trd>&c_z;&c_m;&c_r;&c_o;&c_k;&c_ib;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_c;&c_je;&c_m;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="de">D&auml;mmerung</trd>
  <trdgrp lng="pl">
    <trd>zmierzch</trd>,
    <trd>zmrok</trd>,
  </trdgrp>
  <trd lng="pt">crep&uacute;sculo</trd>
  <trd lng="ro">crepuscul</trd>
  <trd lng="ru">&c_s;&c_u;&c_m;&c_je;&c_r;&c_k;&c_i;</trd>
</drv>

<drv mrk="krepus.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        <tld/>a lumo.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malluma, malvigla, malgaja:
      <ekz>
        la <tld/>a periodo de la vivo<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_r;&c_o;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_c;&c_je;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">d&auml;mmerig</trd>
  <trd lng="pl">mroczny</trd>
  <trd lng="ru">&c_s;&c_u;&c_m;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="krepus.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <snc>
    <dif>
      Proksimi&gcirc;i, veni, pp tago.
    </dif>
    <trd 
lng="be">&c_z;&c_mol;&c_m;&c_ja;&c_r;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>
    <trd lng="de">d&auml;mmern</trd>
    <trd lng="pl">zmierzcha&cacute;</trd>
    <trd 
lng="ru">&c_s;&c_m;&c_je;&c_r;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc>
    <dif>
      Ekkaduki&gcirc;i:
      <ekz>
        lia spirito <tld/>i&gcirc;as
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="klin.0igxi">klini&gcirc;i</ref>.
    </refgrp>
    <trd lng="de">in D&auml;mmerung fallen</trd>
    <trd lng="ru">&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_k; 
&c_z;&c_a;&c_k;&c_a;&c_t;&c_u;</trd>
  </snc>
</drv>

<drv mrk="krepus.el0igxi">
  <kap>
    el<tld/>i&gcirc;i
  </kap>
  <snc>
    <dif>
      Proksimi&gcirc;i, veni, pp tago.
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_y;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; 
<klr>(&c_p;&c_r;&c_a; &c_d;&c_z;&c_je;&c_n;&c_mol;)</klr></trd>,
      <trd>&c_v;&c_ib;&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Klari&gcirc;i, naski&gcirc;i:
      <ekz>
        grandaj ideoj el<tld/>i&gcirc;as en mia animo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      
<trd>&c_p;&c_r;&c_a;&c_ja;&c_s;&c_mol;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_ib;&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">licht werden</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_ja;&c_s;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
