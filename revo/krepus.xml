<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: krepus.xml,v 1.21 2021/05/28 12:15:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>krepusk</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="krepus.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="krepus.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <uzo tip="fak">MET</uzo>
    <dif>
      Nehela lumo, kiun estigas sunaj radioj resendataj
      de la tera atmosfero post subiro de la suno a&ubreve;
      anta&ubreve; &gcirc;ia levi&gcirc;o.
    </dif>
    <trdgrp lng="en">
      <trd>twilight</trd>,
      <trd>dawn <klr>(matenru&gcirc;o)</klr></trd>,
      <trd>dusk <klr>(vesperru&gcirc;o)</klr></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>ochtendrood</trd>,
      <trd>avondgloren</trd>
    </trdgrp>
  </snc>
  <snc mrk="krepus.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kadukeco, malforteco:
      <ekz>
        mia cerbo na&gcirc;as kvaza&ubreve; en <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agoni.0o">agonio</ref>,
      <ref cel="dekade.0o">dekadenco</ref>.
    </refgrp>
    <trd lng="nl">schemering</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_r;&c_o;&c_k;</trd>,
    <trd>&c_z;&c_m;&c_r;&c_o;&c_k;&c_ib;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_c;&c_je;&c_m;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="bo">&#xF53;&#xF58;&#xF0B;&#xF63;&#xF44;&#xF66;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>ztr&#xE1;ta &#x17E;ivotn&#xED;ch sil</trd>,
    <trd>zubo&#x17E;en&#xED;</trd>
  </trdgrp>
  <trd lng="de">D&auml;mmerung</trd>
  <trd lng="es">crep&uacute;sculo</trd>
  <trd lng="fr">cr&eacute;puscule</trd>
  <trdgrp lng="pl">
    <trd>zmierzch</trd>,
    <trd>zmrok</trd>,
  </trdgrp>
  <trd lng="pt">crep&uacute;sculo</trd>
  <trd lng="ro">crepuscul</trd>
  <trd lng="ru">&c_s;&c_u;&c_m;&c_je;&c_r;&c_k;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>brie&#x17E;denie</trd>,
    <trd>s&#xFA;mrak</trd>,
    <trd>&#x161;ero</trd>
  </trdgrp>
</drv>

<drv mrk="krepus.0a">
  <kap><tld/>a</kap>
  <snc mrk="krepus.0a.KAL">
    <dif>
      Karakterizata per <tld/>o:
      <ekz>
        <tld/>a lumo.
      </ekz>
    </dif>
  </snc>
  <snc mrk="krepus.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malluma, malvigla, malgaja:
      <ekz>
        la <tld/>a periodo de la vivo<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_m;&c_r;&c_o;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_p;&c_r;&c_y;&c_c;&c_je;&c_m;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>za&#x161;e&#x159;en&#xFD;</trd>,
    <trd>&#x161;er&#xFD;</trd>
  </trdgrp>
  <trd lng="de">d&auml;mmerig</trd>
  <trd lng="es">crepuscular</trd>
  <trdgrp lng="fr">
    <trd>cr&eacute;pusculaire</trd>,
    <trd>cr&eacute;pusculin</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>schemerend</trd>,
    <trd>schemerachtig</trd>
  </trdgrp>
  <trd lng="pl">mroczny</trd>
  <trd lng="ru">&c_s;&c_u;&c_m;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_y;&c_j;</trd>
  <trd lng="sk">&#x161;er&#xFD;</trd>
</drv>

<drv mrk="krepus.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="krepus.0igxi.KAL">
    <dif>
      &Scirc;an&gcirc;i&gcirc;i en <tld/>on, i&gcirc;i duonhela.
      <ekz>
        la tago <tld/>i&gcirc;is kaj la <tld/>o densi&gcirc;is en nokton 
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_mol;&c_m;&c_ja;&c_r;&c_k;&c_a;&c_c;&c_c;&c_a;</trd>
    <trd lng="de">d&auml;mmern</trd>
    <trd lng="nl">beginnen te <ind>schemeren</ind></trd>
    <trd lng="pl">zmierzcha&cacute;</trd>
    <trd lng="ru">&c_s;&c_m;&c_je;&c_r;&c_k;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc mrk="krepus.0igxi.FIG">
    <dif>
      Ekkaduki&gcirc;i:
      <ekz>
        lia spirito <tld/>i&gcirc;as.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klin.0igxi">klini&gcirc;i</ref>.
    </refgrp>
    <trd lng="de">in D&auml;mmerung fallen</trd>
    <trdgrp lng="nl">
      <trd>verzwakken</trd>,
      <trd>tanen</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_l;&c_o;&c_n;&c_i;&c_t;&c_mol;&c_s;&c_ja; &c_k; &c_z;&c_a;&c_k;&c_a;&c_t;&c_u;</trd>
  </snc>
</drv>

<drv mrk="krepus.0igxo">
  <kap><tld/>i&gcirc;o</kap>
  <snc mrk="krepus.0igxo.KAL">
    <dif>
      Tempo, kiam esti&gcirc;as <tld/>o:
      <ekz>
        li <klr>...</klr> ripozis sibarite sur sia lito &gcirc;is la 
        <tld/>i&gcirc;o
        <fnt>
          <aut>N. Gogol, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="http://www.esperanto-ondo.ru/Libroj/V_bibl07.pdf">
          Palto</url></vrk>,
          <lok>2011</lok>
        </fnt>..
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">cr&eacute;puscule</trd>
</drv>

<drv mrk="krepus.el0igxi">
  <kap>el<tld/>i&gcirc;i</kap>
  <snc mrk="krepus.el0igxi.KAL">
    <dif>
      Proksimi&gcirc;i, veni, pp tago.
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_y;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol; <klr>(&c_p;&c_r;&c_a; &c_d;&c_z;&c_je;&c_n;&c_mol;)</klr></trd>,
      <trd>&c_v;&c_ib;&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>dagen</trd>,
      <trd><ind>dag</ind> worden</trd>
    </trdgrp>
  </snc>
  <snc mrk="krepus.el0igxi.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Klari&gcirc;i, naski&gcirc;i:
      <ekz>
        grandaj ideoj el<tld/>i&gcirc;as en mia animo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_ja;&c_s;&c_mol;&c_n;&c_ja;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_ib;&c_d;&c_n;&c_je;&c_c;&c_mol;</trd>
    </trdgrp>
      <trd lng="nl">ontspruiten</trd>
  </snc>
  <trd lng="de">licht werden</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_ja;&c_s;&c_n;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_s;&c_v;&c_je;&c_t;&c_l;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
