<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>miasm</rad>/o
</kap>
<drv mrk="miasm.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Malsanigaj haladzoj eli&gcirc;antaj el nevideblaj putri&gcirc;antaj eroj de
      organikaj substancoj.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_ib;&c_ja;&c_z;&c_m;&c_y;</trd>
  <trdgrp lng="de">
    <trd>Miasma</trd>,
    <trd>verseuchte <ind>Ausd&uuml;nstung</ind></trd>,
    <trd>Pesthauch</trd>
  </trdgrp>
  <trd lng="hu">miazma</trd>
  <trdgrp lng="nl">
    <trd>miasma</trd>,
    <trd>moerasdamp</trd>
  </trdgrp>
  <trd lng="pt">miasma</trd>
  <trd lng="ru">&c_m;&c_i;&c_a;&c_z;&c_m;&c_y;</trd>
  <trd lng="fr">miasmes</trd>
</drv>
</art>
