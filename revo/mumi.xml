        &scirc;telistoj rabadis la trezoron kaj <tld/>ojn el
        piramidoj<fnt><bib>IntArkeol</bib>, <lok>p. 22a</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_u;&c_m;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_m;&c_u;&c_m;&c_i;&c_ja;</trd>
  <trd lng="cs">mumie</trd>
  <trd lng="de">Mumie</trd>
  <trd lng="en">mummy</trd>
  <trd lng="es">momia</trd>
  <trd lng="fa"><klr>(&a_j;&a_s;&a_d;)
  </klr>&a_m;&a_w;&a_m;&f_y;&a_A;&f_y;&f_y;</trd>
  <trd lng="fr">momie</trd>
  <trd lng="hu">m&uacute;mia</trd>
  <trd lng="ie">mumie</trd>
  <trd lng="nl">mummie</trd>
  <trd lng="pl">mumia</trd>
  <trd lng="pt">m&uacute;mia</trd>
  <trd lng="ru">&c_m;&c_u;&c_m;&c_i;&c_ja;</trd>
  <trd lng="sk">m&uacute;mia</trd>
  <trd lng="sv">mumie</trd>
  <trd lng="uk">&#x43C;&c_u;&#x43C;&c_ib;&#x44F;</trd>
</drv>
