<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hum.xml,v 1.25 2023/11/15 17:08:02 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>hum</rad>/o
</kap>
<drv mrk="hum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Nigre bruna <ref tip="super" cel="ter.0o.substanco">tero</ref>, 
      konsistanta el malkomponi&gcirc;intaj
      vegeta&jcirc;oj, uzata kiel <ref tip="super" cel="sterk.0o">sterko</ref>
      &ccirc;e &gcirc;ardenkulturo:
      <ekz>
        la rokoj estas kovritaj de <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        li propagandis la ideon de la <tld/>o-necesejo por ne malpurigi la valoran akvon
        <fnt><aut>Walter Klag</aut>,
        <vrk><url ref="&Monato;/2000/006938.php">Friedensreich Hundertwasser 1928 -
        2000</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/07, p. 12</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="komposxt.0o">kompo&scirc;to</ref>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_m;&c_u;&c_s;</trd>
  <trdgrp lng="br">
    <trd>humus</trd>,
    <trd>rotel</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>humus</trd>,
    <trd>prs&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Humus</trd>,
    <trd>Humusboden</trd>
  </trdgrp>
  <trd lng="el">&phi;&ypsilon;&tau;&omikron;&gamma;&eta_ton;</trd>
  <trd lng="en">humus</trd>
  <trd lng="fa">&f_g;&f_y;&a_A;&a_kh;&a_A;&f_k;</trd>
  <trdgrp lng="fr">
    <trd>humus</trd>,
    <trd>terreau</trd></trdgrp>
  <trd lng="hu">humusz</trd>
  <trd lng="it">humus</trd>
  <trdgrp lng="ja">
    <trd>&#x8150;&#x690D;&#x571F;  <pr>&#x3075;&#x3057;&#x3087;&#x304F;&#x3069;</pr></trd>,
    <trd>&#x5EAD;&#x571F;  <pr>&#x306B;&#x308F;&#x3069;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>humus</trd>,
    <trd>bladaarde</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>humo</trd>,
    <trd>h&uacute;mus</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_n;&c_o;&c_j;</trd>,
    <trd>&c_g;&c_u;&c_m;&c_u;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>humus</trd>,
    <trd>prs&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&c_je;&c_g;&#x43D;&c_ib;&c_j;</trd>,
    <trd>&c_g;&c_u;&#x43C;&c_u;&c_s;</trd>
  </trdgrp>
  <trd lng="zh">&#x8150;&#x6B96;&#x8CEA; <pr>f&#x1D4;zh&#xED;zh&#xEC;</pr></trd>
</drv>
</art>
