<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>hum</rad>/o
</kap>
<drv mrk="hum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AGR</uzo>
  <snc>
    <dif>
      Nigrebruna tero, konsistanta el malkomponi&gcirc;intaj
      vegeta&jcirc;oj, uzata kiel sterko &ccirc;e &gcirc;ardenkulturo.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_u;&c_m;&c_u;&c_s;</trd>  
  <trdgrp lng="br">
    <trd>humus</trd>,
    <trd>rotel</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Humus</trd>,
    <trd>Humusboden</trd>
  </trdgrp>
  <trd lng="el">&phi;&ypsilon;&tau;&omikron;&gamma;&eta_ton;</trd>
  <trdgrp lng="fr">
    <trd>humus</trd>,
    <trd>terreau</trd></trdgrp>
  <trd lng="hu">humusz</trd>
  <trdgrp lng="nl">
    <trd>humus</trd>,
    <trd>bladaarde</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>humo</trd>,
    <trd>h&uacute;mus</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_g;&c_n;&c_o;&c_j;</trd>,
    <trd>&c_g;&c_u;&c_m;&c_u;&c_s;</trd>
  </trdgrp>
</drv>
</art>
