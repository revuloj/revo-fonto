<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>togo</rad>/o
</kap>

<drv mrk="togo.T0olando">
  <kap>
    <tld lit="T"/>lando
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="togo.T0olando.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la okcidenta Afriko kun &ccirc;efurbo Lomeo.
      Teritorio de Respubliko Togolando.
      ISO-3166 kodo: TG.
      &Gcirc;i limas kun
      <ref tip="vid" cel="benin.B0o.GEOG">Benino</ref>,
      <ref tip="vid" cel="burkin.B0o.GEOG">Burkino</ref>,
      <ref tip="vid" cel="gana.G0o.GEOG">Ganao</ref>
      kaj <ref tip="vid" cel="atlant.A0o.GEOG">Atlantiko</ref> (Gvinea 
      Golfo).
    </dif>
    <trdgrp lng="pl">
      <trd>Togo</trd>,
      <trd>Republika <ind>Togo</ind></trd>,
      <trd>Republika <ind>Togijska</ind></trd>
    </trdgrp>
  </snc>
</drv>

</art>

