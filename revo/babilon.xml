<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: babilon.xml,v 1.20 2023/08/31 08:48:48 revo Exp $">
<kap><rad>Babilon</rad>/o</kap>
<drv mrk="babilon.B0o">
  <kap><tld/>o<fnt>Z</fnt></kap>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      La plej granda <ref tip="lst" lst="voko:urboj" cel="urb.0o">urbo</ref> de la
      antikva
      <ref tip="malprt" cel="mezopotami.M0o">Mezopotamio</ref>,
      &ccirc;e <ref cel="euxfrat.E0o">E&ubreve;frato</ref>, proks. 300 km
      anta&ubreve; ties kunflui&gcirc;o kun Tigriso (44&#x2DA;25&minute;E,
      32&#x2DA;35&minute;N) kaj la &ccirc;efurbo de
      <ref tip="sin" cel="babiloni.B0o">Babilonio</ref>:
      <ekz>
	kiu ne renkontis ofte en sia vivo homojn, kiuj ploras
	super la riveroj de <tld/>o, &ccirc;irka&ubreve;fluantaj la ruinojn
	de ilia perdita bonstato?
	<fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
	&Ccirc;e la river' de <tld/>o
	Ni sidis kaj amare ploris,
	Kiam ni tie pri Ciono
	Kun afliktita kor' memoris
	<fnt>
	<aut>tr. K. Kalocsay</aut>: <vrk>Ps. 137.</vrk>
	<lok>Tutmonda Sonoro, I:55.</lok></fnt>;
      </ekz>
      <ekz>
	la <ind><tld lit="b"/>a Talmudo</ind>.
	<trd lng="be">&c_B;&c_a;&c_b;&c_ib;&c_l;&c_jo;&c_n;&c_s;&c_k;&c_ib;
	&c_T;&c_a;&c_l;&c_m;&c_u;&c_d;</trd>
	<trd lng="bg">&c_V;&c_a;&c_v;&c_i;&c_l;&c_o;&c_n;&c_s;&c_k;&c_i;
	&c_T;&c_a;&c_l;&c_m;&c_u;&c_d;)</trd>
        <trd lng="ca">Talmud de Babil&ograve;nia</trd>
	<trd lng="de">Babylonischer Talmud</trd>
	<trd lng="ru">&c_v;&c_a;&c_v;&c_i;&c_l;&c_o;&c_n;&c_s;&c_k;&c_i;&c_j;
	&c_T;&c_a;&c_l;&c_m;&c_u;&c_d;</trd>
        <trd lng="sv">Babyloniska Talmud</trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="babel.B0o">Babelo</ref>
  </snc>
  <trd lng="be">&c_B;&c_a;&c_b;&c_ib;&c_l;&c_jo;&c_n;</trd>
  <trd lng="bg">&c_V;&c_a;&c_v;&c_i;&c_l;&c_o;&c_n;</trd>
  <trd lng="ca">Babil&ograve;nia</trd>
  <trd lng="de">Babylon</trd>
  <trd lng="en">Babylon</trd>
  <trd lng="fa">&a_b;&a_A;&a_b;&a_kasra;&a_l;<klr tip="ind"> (&a_sh;&a_h;&a_r;)</klr></trd>
  <trd lng="fr">Babylone</trd>
  <trd lng="he">&#x5D1;&#x5BC;&#x5B8;&#x5D1;&#x5B6;&#x5DC;</trd>
  <trd lng="hu">Babilon</trd>
  <trdgrp lng="it">
    <trd>Babele</trd>,
    <trd>Babilonia<klr> (citt&agrave;)</klr></trd>
  </trdgrp>
  <trd lng="ja">&#x30D0;&#x30D3;&#x30ED;&#x30F3;</trd>
  <trd lng="nl">Babylon</trd>
  <trd lng="pl">Babilon</trd>
  <trd lng="ru">&c_V;&c_a;&c_v;&c_i;&c_l;&c_o;&c_n;</trd>
  <trd lng="sk">Babylon</trd>
  <trd lng="sv">Babylon</trd>
  <trd lng="uk">&#x412;&#x430;&#x432;&#x438;&#x43B;&#x43E;&#x43D;</trd>
</drv>
</art>
