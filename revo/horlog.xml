<drv mrk="horlog.akvo0o">
  <kap>akvo<tld/>o</kap>
  <snc mrk="horlog.akvo0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="super" cel="horlog.0o.KAL"><tld lit="H"/>o</ref>
      montranta
      tempopason la&ubreve; konstanta fluo de akvo en ujon:
      <ekz>
        la antikvaj civilizoj konis jam diversajn speciojn de <tld/>oj,
        Vitruvio parolis pri akvo<tld/>o a&ubreve; klepsidro, pri aliaj
        el aero (vento), el suno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4154">Tempo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        akvo<tld/>o de <nom>Luterbach</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=279655">Luterbach</url></lok>
        </fnt>.
      </ekz>
   </dif>
    <ref tip="sin" cel="klepsidr.0o">klepsidro</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_y; <ind>&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_mol;&c_n;&c_ib;&c_k;</ind></trd>,
    <trd>&c_k;&c_l;&c_je;&c_p;&c_s;&c_y;&c_d;&c_r;&c_a;</trd>
  </trdgrp>
  <trd lng="br">eurier-dour</trd>
  <trd lng="de">Wasseruhr</trd>
  <trdgrp lng="en">
    <trd>clepsydra</trd>,
    <trd><klr>water </klr>clock</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>clepsydre</trd>,
    <trd>horloge &agrave; eau</trd>
  </trdgrp>
  <trd lng="hu">v&iacute;z&oacute;ra</trd>
  <trd lng="id"><ind>jam</ind> air</trd>
  <trd lng="it"><ind>orologio</ind> ad acqua</trd>
  <trd lng="ja">&#x6C34;&#26178;&#x8A08;  <pr>&#x307F;&#x305A;&#12393;&#12369;&#12356;</pr></trd>
  <trd lng="nl">wateruurwerk</trd>
