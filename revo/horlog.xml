<drv mrk="horlog.atom0o">
  <kap>atom<tld/>o, <var><kap>atoma <tld/>o</kap></var></kap>
  <snc mrk="horlog.atom0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="super" cel="horlog.0o.KAL"><tld lit="H"/>o</ref>
      kalkulanta tempon el atomreakcioj:
      <ekz>
        la&ubreve; la atoma <tld/>o la rotacio de Tero fari&gcirc;as iom
        post iom malrapida
        <fnt>
        <aut>Vastalto</aut>,
        <vrk><url ref="http://www.vastalto.com/blog/">Akvosfero</url></vrk>,
        <lok>Supersekundo, 2005-11-14</lok>
        </fnt>;
      </ekz>
      <ekz>
        atom<tld/>oj trovi&gcirc;as anka&ubreve; en satelitoj
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2001/007024.php">TEMPO:
          Pli ekzakte ne eblas: Radi-regata horlo&gcirc;o</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_t;&c_a;&c_m;&c_n;&c_y; <ind>&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_mol;&c_n;&c_ib;&c_k;</ind></trd>
  <trd lng="de">Atomuhr</trd>
  <trd lng="en"><klr tip="amb">atomic </klr>clock</trd>
  <trd lng="fr">horloge atomique</trd>
  <trd lng="id"><ind>jam</ind> atom</trd>
  <trd lng="it"><ind>orologio</ind> atomico</trd>
  <trd lng="nl">atoomklok</trd>
  <trd lng="pl">zegar atomowy</trd>
  <trd lng="ro">ceas atomic</trd>
  <trdgrp lng="zh">
    <trd>&#x539F;&#x5B50;&#37912; <pr>yu&#xE1;nz&#x1D0;zh&#x14D;ng</pr></trd>,
    <trd>&#x539F;&#x5B50;&#37912; <pr>yu&#xE1;nz&#x1D0;zh&#x14D;ng</pr></trd>
  </trdgrp>
</drv>

