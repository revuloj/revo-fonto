<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>horlo&gcirc;</rad>/o
</kap>
<drv mrk="horlog.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="horlog.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Aparato por mezuri la tempon kaj montri la horojn:
      <ekz>
        li havis la senton, kvaza&ubreve; la mondo estas unu granda
        <ind><tld/>ejo</ind>, kie &ccirc;iuj <tld/>oj batas <ctl>tik,
tak</ctl>,
        &ccirc;iuj tur<tld/>oj sonas <ctl>bam, bam</ctl>
        <fnt><bib>Fab3</bib>, <lok>&Scirc;tono de la
sa&gcirc;uloj</lok></fnt>;
        <trd lng="br">horolajerezh<klr tip="ind"> (stal)</klr></trd>
        <trd lng="fr">horlogerie<klr tip="ind"> (magasin)</klr></trd>
      </ekz>
      <ekz>
        suna <tld/>o <fnt><bib>PV</bib></fnt>
        (vidu &ccirc;e <ref tip="vid" cel="horlog.sun0o">sun<tld/>o</ref>);
      </ekz>
      <ekz>
        mia <tld/>o montras la dekan;
      </ekz>
      <ekz>
        stre&ccirc;i <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>eurier</trd>,
    <trd>horolaj</trd>
  </trdgrp>
  <trd lng="de">Uhr</trd>
  <trd lng="el">&rho;&omikron;&lambda;&omikron_ton;&jota;</trd>
  <trd lng="en">clock</trd>
  <trd lng="es">reloj</trd>
  <trdgrp lng="fr">
    <trd>horloge</trd>,
    <trd>pendule<klr tip="ind"> (horloge)</klr></trd>
  </trdgrp>
  <trd lng="hu">&oacute;ra</trd>
  <trd lng="nl">horloge</trd>
  <trd lng="pl">zegar</trd>
  <trd lng="ro">ceas</trd>
  <trd lng="pt">rel&oacute;gio</trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_y;</trd>
  <trdgrp lng="sv">
    <trd>klocka</trd>,
    <trd>ur</trd>
  </trdgrp>
  <trd lng="sk">hodiny</trd>
  <trdgrp lng="zh">
    <trd>&#26102;&#38047;</trd>,
    <trd><klr>tr. </klr>&#26178;&#37912;<klr>
[sh&iacute;zh&#333;ng]</klr></trd>
  </trdgrp>
</drv>

<drv mrk="horlog.H0o">
  <kap><tld lit="H"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <mlg>Hor</mlg>
  <snc>
    <dif>
      Nord-hemisfera <ref tip="super"
cel="stel.0figuro">stelfiguro</ref> (<trd lng="la">Horologium</trd>).
    </dif>
  </snc>
  <trd lng="de">Pendeluhr</trd>
  <trd lng="en">Horologium</trd>
  <trd lng="es">Reloj</trd>
  <trd lng="pl">Zegar</trd>
</drv>

<drv mrk="horlog.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="horlog.0isto.KAL">
    <dif>
      Homo, kiu fabrikas a&ubreve; zorgas la <tld/>ojn.
    </dif>
  </snc>
  <trd lng="br">horolajer</trd>
  <trd lng="de">Uhrmacher</trd>
  <trd
lng="el">&rho;&omikron;&lambda;&omikron;&gamma;&alfa_ton;&sigma_fina;</trd>
  <trd lng="es">relojero</trd>
  <trd lng="fr">horloger</trd>
  <trd lng="hu">&oacute;r&aacute;s</trd>
  <trd lng="nl">horlogemaker</trd>
  <trd lng="pl">zegarmistrz</trd>
  <trd lng="pt">relojoeiro</trd>
  <trd lng="ro">ceasornicar</trd>
  <trd lng="ru">&c_ch;&c_a;&c_s;&c_o;&c_v;&c_shch;&c_i;&c_k;</trd>
  <trd lng="sv">urmakare</trd>
  <trd lng="sk">hodin&aacute;r</trd>
</drv>

<drv mrk="horlog.akvo0o">
  <kap>akvo<tld/>o</kap>
  <snc mrk="horlog.akvo0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="super" cel="horlog.0o.KAL"><tld lit="H"/>o</ref> montranta
      tempopason la&ubreve; konstanta fluo de akvo en ujon.
    </dif>
    <ref tip="sin" cel="klepsidr.0o">klepsidro</ref>
  </snc>
  <trd lng="br">eurier-dour</trd>
  <trdgrp lng="fr">
    <trd>clepsydre</trd>,
    <trd>horloge &agrave; eau</trd>
  </trdgrp>
  <trd lng="hu">v&iacute;z&oacute;ra</trd>
  <trd lng="nl">wateruurwerk</trd>
  <trd lng="pl">zegar wodny</trd>
  <trd lng="pt">rel&oacute;gio de &aacute;gua</trd>
  <trd lng="ro">ceas de ap&#x103;</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_l;&c_je;&c_p;&c_s;&c_i;&c_d;&c_r;&c_a;</trd>,
    <trd>&c_v;&c_o;&c_d;&c_ja;&c_n;&c_y;&c_je;
<ind>&c_ch;&c_a;&c_s;&c_y;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="horlog.atom0o">
  <kap>atom<tld/>o, <var><kap>atoma <tld/>o</kap></var></kap>
  <snc mrk="horlog.atom0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="super" cel="horlog.0o.KAL"><tld lit="H"/>o</ref>
      kalkulanta tempon el atomreakcioj:
      <ekz>
        la&ubreve; la atoma <tld/>o la rotacio de Tero fari&gcirc;as iom
post iom
        malrapida
        <fnt>
        <aut>Vastalto</aut>,
        <vrk><url
        ref="http://www.vastalto.com/blog/">Akvosfero</url></vrk>,
        <lok>Supersekundo, 2005-11-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">horloge atomique</trd>
  <trd lng="pl">zegar atomowy</trd>
</drv>

<drv mrk="horlog.bracelet0o">
  <kap>bracelet<tld/>o</kap>
  <snc mrk="horlog.bracelet0o.brakhorlogxo">
    <uzo tip="fak">KAL</uzo>
    <ref tip="dif" cel="horlog.brak0o">brak<tld/>o</ref>
    <ekz>
      La ora bracelet<tld/>o ku&scirc;is anta&ubreve; &scirc;i apud la
skatoleto
      <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 20a, p. 199a</lok></fnt>.
    </ekz>
  </snc>
  <trdgrp lng="br">
    <trd>eurier-brec'h</trd>,
    <trd>montr arzorn</trd>
  </trdgrp>
  <trd lng="fr">montre-bracelet</trd>
  <trd lng="hu">kar&oacute;ra</trd>
  <trd lng="nl">polshorloge</trd>
  <trd lng="pl">zegarek r&eogonek;czny <klr>(damski)</klr></trd>
  <trd lng="pt">rel&oacute;gio de bra&ccedil;o</trd>
  <trd lng="ro">ceas cu br&#x103;&#x163;ar&#x103;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_ch;&c_n;&c_y;&c_je;
<ind>&c_ch;&c_a;&c_s;&c_y;</ind></trd>,
    <trd>&c_n;&c_a;&c_r;&c_u;&c_ch;&c_n;&c_y;&c_je;
<ind>&c_ch;&c_a;&c_s;&c_y;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="horlog.brak0o">
  <kap>brak<tld/>o</kap>
  <snc mrk="horlog.brak0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Eta me&hcirc;anika <ref tip="super" cel="horlog.0o.KAL"><tld/>o</ref>
      portata
      &ccirc;e la pojno danke al &ccirc;irka&ubreve;tena rimeno:
      <ekz>
        Mi iris por riparigi mian brak<tld/>on
        <fnt><aut>Marjorie Boulton</aut>, <vrk>Dinomanio</vrk>,
        <bib>Monato</bib>, <lok>jaro 1994a, numero 2a, p. 16a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="horlog.bracelet0o">bracelet<tld/>o</ref>
  </snc>
  <trdgrp lng="br">
    <trd>eurier-brec'h</trd>,
    <trd>montr arzorn</trd>
  </trdgrp>
  <trd lng="fr">montre-bracelet</trd>
  <trd lng="hu">kar&oacute;ra</trd>
  <trd lng="nl">polshorloge</trd>
  <trd lng="pl">zegarek r&eogonek;czny</trd>
  <trd lng="pt">rel&oacute;gio de bra&ccedil;o</trd>
  <trd lng="ro">ceas de m&acirc;n&#x103;</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_ch;&c_n;&c_y;&c_je;
<ind>&c_ch;&c_a;&c_s;&c_y;</ind></trd>,
    <trd>&c_n;&c_a;&c_r;&c_u;&c_ch;&c_n;&c_y;&c_je;
<ind>&c_ch;&c_a;&c_s;&c_y;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="horlog.mur0o">
  <kap>mur<tld/>o</kap>
  <snc mrk="horlog.mur0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="super" cel="horlog.0o.KAL"><tld lit="H"/>o</ref> penda sur
      muro a&ubreve; vando.
    </dif>
  </snc>
  <trd lng="fr">horloge murale</trd>
  <trd lng="hu">fali&oacute;ra</trd>
  <trd lng="nl">wandklok</trd>
  <trd lng="pl">zegar &sacute;cienny</trd>
  <trd lng="pt">rel&oacute;gio de parede</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_n;&c_n;&c_y;&c_je;
<ind>&c_ch;&c_a;&c_s;&c_y;</ind></trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_je;&c_n;&c_n;&c_y;&c_je;
<ind>&c_ch;&c_a;&c_s;&c_y;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="horlog.posx0o">
  <kap>po&scirc;<tld/>o</kap>
  <snc mrk="horlog.posx0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Eta me&hcirc;anika <ref tip="super" cel="horlog.0o.KAL"><tld/>o</ref>
      portebla
      en po&scirc;o, ofte fiksita per &ccirc;eneto al vesto:
      <ekz>
        la nekonato <klr>...</klr> eligis grandan oran po&scirc;<tld/>on kun
        brilianta triangulo sur la kovrilo
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 7a, p. 65a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">eurier-godell</trd>
  <trd lng="fr">montre<klr tip="ind"> (de gousset)</klr></trd>
  <trd lng="hu">zseb&oacute;ra</trd>
  <trd lng="nl">zakhorloge</trd>
  <trd lng="pl">zegarek kieszonkowy</trd>
  <trd lng="pt">rel&oacute;gio de bolso</trd>
  <trd lng="ro">ceas de buzunar</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_m;&c_a;&c_n;&c_n;&c_y;&c_je;
<ind>&c_ch;&c_a;&c_s;&c_y;</ind></trd>
</drv>

<drv mrk="horlog.sablo0o">
  <kap>sablo<tld/>o, <var><kap>sabla <tld/>o</kap></var></kap>
  <snc mrk="horlog.sablo0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="super" cel="horlog.0o.KAL"><tld lit="H"/>o</ref> montranta
      tempopason la&ubreve; la konstanta &scirc;uti&gcirc;o de sablo tra
funelo:
      <ekz>
        oni povas ankora&ubreve; vidi sur <klr>[la tomba &scirc;tono]</klr>
        sablan <tld/>on kaj pecon de an&gcirc;elo
        <fnt><bib>Fab3</bib>, <lok>malnova tomba &scirc;tono</lok></fnt>;
      </ekz>
      <ekz>
        en la tendo ekregis tia silento, ke oni povis a&ubreve;di la erojn,
        falantajn en la sablo<tld/>o
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="br">eurier-traezh</trd>
  <trd lng="fr">sablier</trd>
  <trd lng="hu">homok&oacute;ra</trd>
  <trd lng="nl">zandloper</trd>
  <trd lng="pl">zegar piaskowy</trd>
  <trd lng="pt">ampulheta</trd>
  <trd lng="ru">&c_p;&c_je;&c_s;&c_o;&c_ch;&c_n;&c_y;&c_je;
<ind>&c_ch;&c_a;&c_s;&c_y;</ind></trd>
</drv>

<drv mrk="horlog.sun0o">
  <kap>sun<tld/>o</kap>
  <snc mrk="horlog.sun0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="super" cel="horlog.0o.KAL"><tld lit="H"/>o</ref> montranta
      tempopason la&ubreve; la pozicio de sunluma ombro de stango sur plato.
    </dif>
  </snc>
  <trdgrp lng="br">
    <trd>eurier-heol</trd>,
    <trd>horolaj-heol</trd>
  </trdgrp>
  <trd lng="fr">cadran solaire</trd>
  <trd lng="hu">nap&oacute;ra</trd>
  <trd lng="nl">zonnewijzer</trd>
  <trd lng="pl">zegar s&lstroke;oneczny</trd>
  <trd lng="pt">rel&oacute;gio de sol</trd>
  <trd lng="ro">ceas solar</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_n;&c_je;&c_ch;&c_n;&c_y;&c_je;
<ind>&c_ch;&c_a;&c_s;&c_y;</ind></trd>
</drv>

<drv mrk="horlog.vek0o">
  <kap>vek<tld/>o</kap>
  <snc mrk="horlog.vek0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      <ref tip="super" cel="horlog.0o.KAL"><tld lit="H"/>o</ref> kun
      sonorilo
      por veki:
      <ekz>
        jen grincas la vek<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_u;&c_d;&c_z;&c_ib;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  <trd lng="br">dihuner</trd>
  <trd lng="de">Wecker</trd>
  <trdgrp lng="fr">
    <trd>r&eacute;veille-matin</trd>,
    <trd>r&eacute;veil<klr tip="ind"> (r&eacute;veille-matin)</klr></trd>
  </trdgrp>
  <trd lng="hu">&eacute;breszt&odblac;&oacute;ra</trd>
  <trd lng="nl">wekker</trd>
  <trd lng="pl">budzik</trd>
  <trd lng="pt">despertador</trd>
  <trd lng="ro">ceas de&#x15F;tept&#x103;tor</trd>
  <trd lng="ru">&c_b;&c_u;&c_d;&c_i;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
