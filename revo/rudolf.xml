<art mrk="$Id: rudolf.xml,v 1.15 2024/04/14 06:28:51 revo Exp $">
<kap>
  <rad>Rudolf</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rudolf.0o">
  <kap><tld/>o</kap>
  <snc mrk="rudolf.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        la atendantoj denove atakis lin, forpu&scirc;ante <tld/>on
        <fnt><bib>Metrop</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_R;&c_u;&c_d;&c_o;&c_l;&c_mol;&c_f;</trd>
    <trd lng="de">Rudolf</trd>
    <trd lng="en">Rudolph</trd>
    <trd lng="es">Rodolfo</trd>
    <trd lng="fr">Rodolphe</trd>
    <trd lng="hu">Rudolf</trd>
    <trd lng="it">Rodolfo</trd>
    <trd lng="nl">Rudolf</trd>
    <trd lng="pl">Rudolf</trd>
    <trd lng="pt">Rodolfo</trd>
    <trd lng="ru">&c_R;&c_u;&c_d;&c_o;&c_l;&c_mol;&c_f;</trd>
    <trdgrp lng="zh">
    <trd>&#x9B6F;&#x9053;&#x592B; <pr>l&#x1D4;d&#xE0;of&#x16B;</pr></trd>,
    <trd>&#x9C81;&#x9053;&#x592B; <pr>l&#x1D4;d&#xE0;of&#x16B;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="ja">&#x30EB;&#x30C9;&#x30EB;&#x30D5;</trd>
  <trd lng="uk">&c_R;&c_u;&c_d;&#x43E;&#x43B;&#x44C;&c_f;</trd>
</drv>
