<art mrk="$Id: kantat.xml,v 1.21 2024/12/16 08:26:30 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kantat</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="kantat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="kantat.0o.MUZ">
    <dif>
      <ref tip="super" cel="kompon.0ajxo">Kompona&jcirc;o</ref>
      kun lirika kanta&jcirc;o por solkantistoj, &hcirc;oro
      kaj banda a&ubreve; orkestra akompano:
      <ekz>
        la <tld/>oj de <nom>Johann Sebastian Bach</nom> akiris en &ccirc;i tiu
        &gcirc;enro tian famon, ke
        enkutimi&gcirc;is la termino <nom>Bach</nom>-<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=329528">Kantato de Bach</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        anka&ubreve; la esperantistoj ser&ccirc;as en la movado misiistojn, mesiojn kaj profetojn
        <klr>[&#8230;]</klr> &ndash; nu, via <tld/>o sonas dekaloge
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 3</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="ari.0o">ario</ref>,
      <ref cel="hxor.0ajxo">&hcirc;ora&jcirc;o</ref>,
      <ref cel="recita.0o">recitativo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_t;&c_a;&c_t;&c_a;</trd>
  <trd lng="ca">cantata</trd>
  <trd lng="cs">kant&aacute;ta</trd>
  <trd lng="de">Kantate</trd>
  <trd lng="en">cantata</trd>
  <trd lng="es">cantata</trd>
  <trdgrp lng="fr">
    <trd>cantate</trd>
  </trdgrp>
  <trd lng="hu">kant&aacute;ta</trd>
  <trd lng="ja">&#12459;&#12531;&#12479;&#12540;&#12479;</trd>
  <trd lng="nl">cantate</trd>
  <trd lng="pl">kantata</trd>
  <trd lng="ro">cantat&abreve;</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_t;&c_a;&c_t;&c_a;</trd>
  <trd lng="sk">kant&aacute;ta</trd>
  <trd lng="uk">&c_k;&c_a;&c_n;&c_t;&c_a;&c_t;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#22823;&#21512;&#21809; <pr>d&agrave;h&eacute;ch&agrave;ng</pr></trd>,
    <trd>&#22307;&#27468;&#35875;&#26354; <pr>sh&egrave;ngg&emacron;y&aacute;oq&#468;</pr></trd>,
    <trd>&#24247;&#22612;&#22612; <pr>k&amacron;ngt&#462;t&#462;</pr></trd>,
    <trd>&#28152;&#21809;&#21127; <pr>q&imacron;ngch&agrave;ngj&ugrave;</pr></trd>
  </trdgrp>
</drv>
</art>
