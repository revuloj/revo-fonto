<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: merin.xml,v 1.19 2025/01/16 22:25:17 revo Exp $">
<kap>
  <rad>merin</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="merin.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Speco de <ref tip="super" cel="sxaf.0o.ZOO">&scirc;afo</ref> el hispana raso, kies longhara lano estas tre
      delikata.
    </dif>
  </snc>
  <rim>
      En <fnt><bib>JED</bib></fnt> aperas la formo &leftquot;merinoso&rightquot;.
    </rim>
  <trd lng="be">&c_m;&c_e;&c_r;&c_y;&c_n;&c_o;&c_s;</trd>
  <trd lng="ca">ovella merina</trd>
  <trd lng="de">Merinoschaf</trd>
  <trd lng="en">merino</trd>
  <trd lng="hu">merin&oacute;</trd>
  <trd lng="ja">&#12513;&#12522;&#12494;&#32650;  <pr>&#12513;&#12522;&#12494;&#12402;&#12388;&#12376;</pr></trd>
  <trd lng="nl">merinosschaap</trd>
  <trd lng="pl">merynos <klr>(rasa owiec)</klr></trd>
  <trd lng="pt">merino</trd>
  <trd lng="ro">merinos</trd>
  <trd lng="ru">&c_m;&c_je;&c_r;&c_i;&c_n;&c_o;&c_s; <klr>(&c_p;&c_o;&c_r;&c_o;&c_d;&c_a; &c_t;&c_o;&c_n;&c_k;&c_o;&c_r;&c_u;&c_n;&c_n;&c_y;&c_h; &c_o;&c_v;&c_je;&c_c;)</klr></trd>
  <trd lng="uk">&c_m;&c_je;&c_r;&c_i;&c_n;&c_o;&c_s; (&c_t;&c_o;&c_n;&c_k;&c_o;&c_r;&c_u;&c_n;&c_n;&c_a;
  &c_v;&c_ib;&c_v;&c_c;&c_ja;)</trd>
</drv>

<drv mrk="merin.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Rilatanta al <tld/>o.
    </dif>
  </snc>
  <snc>
    <dif>
      De <tld/>o:
      <ekz>
        <tld/>a lano.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_r;&c_y;&c_n;&c_o;&c_s;&c_a;&c_v;&c_y;</trd>
  <trd lng="ca">mer&iacute;</trd>
  <trd lng="en">merino</trd>
  <trd lng="hu">merin&oacute;i</trd>
  <trd lng="ja">&#12513;&#12522;&#12494;&#32650;&#12398;  <pr>&#12513;&#12522;&#12494;&#12402;&#12388;&#12376;&#12398;</pr></trd>
  <trd lng="pl">merynosowy</trd>
  <trd lng="pt">merino</trd>
  <trd lng="ro">merinos</trd>
</drv>

<drv mrk="merin.0olano">
  <kap>
    <tld/>olano
  </kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Lano de <tld/>o:
      <ekz>
        kruda lano povas malagrable grati la ha&ubreve;ton, <klr>[sed ne la]</klr> altkvalita
        <tld/>olano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22227"
              >Lano</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_r;&c_y;&c_n;&c_o;&c_s;&c_a;&c_v;&c_a;&c_ja; &c_v;&c_o;&c_w;&c_n;&c_a;</trd>
  <trd lng="ca">llana merina</trd>
  <trd lng="de">Merinowolle</trd>
  <trd lng="en">merino wool</trd>
  <trd lng="hu">merin&oacute;i <ind>gyapj&uacute;</ind></trd>
  <trd lng="ja">&#12513;&#12522;&#12494;&#32650;&#27611;  <pr>&#12513;&#12522;&#12494;&#12424;&#12358;&#12418;&#12358;</pr></trd>
  <trd lng="nl">merinoswol</trd>
  <trd lng="pl"> we&lstroke;na z <ind>merynos&oacute;w</ind></trd>
  <trd lng="pt">merino</trd>
  <trd lng="ro">l&acirc;n&abreve; merinos</trd>
  <trd lng="ru">&c_m;&c_je;&c_r;&c_i;&c_n;&c_o;&c_s;&c_o;&c_v;&c_a;&c_ja; &c_sh;&c_je;&c_r;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="merin.0osxtofo">
  <kap>
    <tld/>o&scirc;tofo
  </kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      &Scirc;tofo farita el <tld/>olano:
      <ekz>
        <tld/>olana &scirc;alo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_r;&c_y;&c_n;&c_o;&c_s;&c_a;&c_v;&c_a;&c_ja; &c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="ca">tela merina</trd>
  <trd lng="de">Merino</trd>
  <trd lng="hu">merin&oacute;<klr tip="amb"> (sz&ouml;vet)</klr></trd>
  <trd lng="ja">&#12513;&#12522;&#12494;&#27611;&#32340;&#29289;  <pr>&#12513;&#12522;&#12494;&#12369;&#12362;&#12426;&#12418;&#12398;</pr></trd>
  <trd lng="nl">merinosstof</trd>
  <trd lng="pl">tkanina z we&lstroke;ny <ind>merynos&oacute;w</ind></trd>
  <trdgrp lng="pt">
   <trd>merino</trd>,
   <trd>merin&oacute;</trd>
  </trdgrp>
  <trd lng="ro">&tcomma;es&abreve;tur&abreve; din l&acirc;n&abreve; merino</trd>
</drv>
</art>
