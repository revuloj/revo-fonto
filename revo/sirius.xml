<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sirius.xml,v 1.12 2024/03/24 20:21:57 revo Exp $">
<kap>
<rad>Sirius</rad>/o
</kap>
<drv mrk="sirius.0o">
  <kap><tld/>o</kap>
  <snc mrk="sirius.0o.mit">
  <uzo tip="fak">MIT</uzo>
    <dif>
      Fidela <ref tip="super" cel="hund.0o.dombesto">hundo</ref>
      de <ref tip="vid" cel="orion.0o.mit">Oriono</ref>.
    </dif>
  </snc>
  <snc mrk="sirius.0o.ast">
  <uzo tip="fak">AST</uzo>
    <dif>
      Tre brila <ref tip="super" cel="stel.0o.AST">stelo</ref>
      de la stelaro de la
      <ref tip="malprt" cel="hund.Cxas0oj.AST">&Ccirc;ashundoj</ref>, 
      apud la konstelacio de 
      <ref cel="orion.0o.AST">Oriono</ref>:
      <ekz>
        super la kapo de l' voja&gcirc;anto brulis Oriono, kaj super la nigraj pilonoj <tld/>o
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fa">&a_sh;&a_b;&a_A;&a_h;&a_n;&f_g;<klr tip="ind"> (&a_s;&a_t;&a_A;&a_r;&a_h;)</klr></trd>
  </snc>
  <trd lng="be">&c_S;&c_y;&c_r;&c_y;&c_ju;&c_s;</trd>
  <trd lng="bg">&c_S;&c_i;&c_r;&c_i;&c_u;&c_s;</trd>
  <trd lng="cs">S&iacute;rius</trd>
  <trd lng="de">Sirius</trd>
  <trd lng="en">Sirius</trd>
  <trd lng="fr">Sirius</trd>
  <trd lng="it">Sirio</trd>
  <trd lng="ja">&#x30B7;&#x30EA;&#x30A6;&#x30B9;</trd>
  <trd lng="la">Sirius</trd>
  <trd lng="nl">Sirius</trd>
  <trd lng="pl">Syriusz</trd>
  <trd lng="sk">S&iacute;rius</trd>
  <trd lng="uk">&c_S;&c_ib;&c_r;&c_ib;&c_u;&c_s;</trd>
  <trd lng="zh">&#x5929;&#x72FC;&#x661F; <pr>Ti&#x101;nl&#xE1;ngx&#x12B;ng</pr></trd>
</drv>
</art>

