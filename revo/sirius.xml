<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Sirius</rad>/o
</kap>
<drv mrk="sirius.0o">
  <kap><tld/>o</kap>
  <snc mrk="sirius.0o.mit">
  <uzo tip="fak">MIT</uzo>
    <dif>
Fidela hundo de 
<ref tip="vid" cel="orion.0o.mit">Oriono</ref>.
    </dif>
  </snc>

  <snc mrk="sirius.0o.ast">
  <uzo tip="fak">AST</uzo>
    <dif>
Tre brila stelo de la stelaro de la
<ref tip="vid" cel="hund.Cxas0oj.AST">&Ccirc;ashundoj</ref>, 
apud la konstelacio de 
<ref tip="vid" cel="orion.0o.AST">Oriono</ref>.
    </dif>
  </snc>

<trd lng="it">Sirio</trd>
<trd lng="en">Sirius</trd>
<trd lng="la">Sirius</trd>

</drv>
</art>

