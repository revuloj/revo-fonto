<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: juni.xml,v 1.38 2023/12/03 07:09:19 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>Juni</rad>/o,
  <var><kap><rad var="j">juni</rad>/o</kap></var>
</kap>
<drv mrk="juni.0o">
  <kap><ofc>*</ofc> <tld/>o,
    <var><kap><tld var="j"/>o</kap></var>
  </kap>
  <snc mrk="juni.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Sesa <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj" val="6">monato</ref> de la <ref tip="malprt" cel="jar.0o.KAL">jaro</ref> la&ubreve; la Gregoria kalendaro, inter <ref cel="maj.0o">Majo</ref>
      kaj <ref cel="juli.0o">Julio</ref>:
      <ekz>
        la dek du monatoj de l' kristana jaro estas: Januaro, Februaro,
        Marto, Aprilo, Majo, <tld/>o, Julio, <klr>[&#x2026;]</klr>
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kontrakto fini&gcirc;is la 30-an de <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        de la komenco de <tld var="j"/>o &gcirc;is la fino de septembro, Nilo levi&gcirc;as
        kaj superver&scirc;as preska&ubreve; la tutan Egipton
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">Januaro, Rim.</ref>
  <trd lng="af">Junie</trd>
  <trd lng="be">&c_ch;&c_e;&c_r;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_ju;&c_n;&c_i;</trd>
  <trd lng="bo">&#xF5F;&#xFB3;&#xF0B;&#xF56;&#xF0B;&#xF51;&#xFB2;&#xF74;&#xF42;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>Mezheven</trd>,
    <trd>Even</trd>
  </trdgrp>
  <trd lng="cs">&ccaron;erven</trd>
  <trd lng="de">Juni</trd>
  <trd lng="el">&Jota;&omikron;&ypsilon_ton;&ny;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="en">June</trd>
  <trd lng="es">junio</trd>
  <trd lng="fa">&f_zh;&a_w;&a_y_hamza;&a_n;</trd>
  <trd lng="fr">juin</trd>
  <trd lng="fy">juny</trd>
  <trd lng="he"><klr tip="ind">&het;&vav;&dalet;&shin; </klr>&yod;&vav;&nun;&yod;</trd>
  <trd lng="hu">j&uacute;nius</trd>
  <trd lng="id">Juni</trd>
  <trd lng="io">junio</trd>
  <trd lng="it">giugno</trd>
  <trd lng="ja">&#x516D;&#x6708; <pr>&#x308D;&#x304F;&#x304C;&#x3064;</pr></trd>
  <trdgrp lng="jbo">
    <trd>xavma'i</trd>,
    <trd>matsi'uma'i</trd>
  </trdgrp>
  <trd lng="nl">juni</trd>
  <trd lng="pl">czerwiec</trd>
  <trd lng="pt">junho</trd>
  <trd lng="ro">iunie</trd>
  <trd lng="ru">&c_i;&c_ju;&c_n;&c_mol;</trd>
  <trd lng="sk">j&uacute;n</trd>
  <trd lng="sv">juni</trd>
  <trd lng="tok">tenpo mun nanpa luka wan</trd>
  <trd lng="tr">haziran</trd>
  <trd lng="uk">&#x447;&#x435;&#x440;&#x432;&#x435;&#x43D;&#x44C;</trd>
  <trd lng="vo">yunul</trd>
  <trdgrp lng="zh">
    <trd>&#x516D;&#x6708; <pr>Li&#xF9;yu&#xE8;</pr></trd>,
    <trd>&#x516D;&#x6708;&#x4EFD; <pr>li&#xF9;yu&#xE8;f&#xE8;n</pr></trd>,
    <trd>&#x516D;&#x6708; <pr>li&#xF9;yu&#xE8;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="juni.0a">
  <kap><tld lit="j"/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o:
      <ekz>
        subita <tld lit="j"/>a pluvo
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2003/007854.html">&Ccirc;u &gcirc;oja
          plorpluvo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        hoteloj en <tld lit="j"/>o malaltigas siajn prezojn kaj varbas per la
        romantika nocio &leftquot;<tld lit="j"/>a <esc>novedzino</esc>&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2003/007854.html">&Ccirc;u &gcirc;oja
          plorpluvo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="j"/>a referenduma malvenko
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2006/008871.php">&Scirc;tato,
          eklezio kaj ideologia laikeco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="j"/>a numero, kiu atingis min je la fino de la eldonmonato,
        estas tre interesa
        <fnt>
          <bib>Monato</bib>
          <aut>Katoo Ivao</aut>
          <vrk><url ref="&Monato;/2003/008176.html">La junia
          numero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_r;&c_v;&c_je;&c_n;&c_mol;&c_s;&c_k;&c_ib;</trd>
  <trd lng="io">juniala</trd>
  <trd lng="ja">&#x516D;&#x6708;&#x306E; <pr>&#x308D;&#x304F;&#x304C;&#x3064;&#x306E;</pr></trd>
  <trd lng="vo">yunulik</trd>
</drv>
</art>
