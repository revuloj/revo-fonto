<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>juni</rad>/o
</kap>
<drv mrk="juni.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">KAL</uzo>
    <dif>
      Sesa <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj"
      val="6">monato</ref> de la <ref tip="malprt"
      cel="jar.0o.KAL">jaro</ref> la&ubreve; la Gregoria kalendaro, inter majo
      kaj julio:
      <ekz>
        la dek du monatoj de l' kristana jaro estas: Januaro, Februaro,
        Marto, Aprilo, Majo, Junio, Julio, <klr>[&#8230;]</klr>
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kontrakto fini&gcirc;is la 30-an de <tld lit="J"/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        de la komenco de <tld/>o &gcirc;is la fino de septembro, Nilo levi&gcirc;as
        kaj superver&scirc;as preska&ubreve; la tutan Egipton
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">januaro, Rim.</ref>
  <trd lng="af">Junie</trd>
  <trd lng="be">&c_ch;&c_e;&c_r;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_ju;&c_n;&c_i;</trd>
  <trdgrp lng="br">
    <trd>Mezheven</trd>,
    <trd>Even</trd>
  </trdgrp>
  <trd lng="cs">&ccaron;erven</trd>
  <trd lng="de">Juni</trd>
  <trd lng="el">&Jota;&omikron;&ypsilon_ton;&ny;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="en">June</trd>
  <trd lng="es">junio</trd>
  <trd lng="fa">&f_zh;&a_w;&a_y_hamza;&a_n;</trd>
  <trd lng="fr">juin</trd>
  <trd lng="fy">juny</trd>
  <trd lng="he"><klr tip="ind">&het;&vav;&dalet;&shin; </klr>&yod;&vav;&nun;&yod;</trd>
  <trd lng="hu">j&uacute;nius</trd>
  <trd lng="id">Juni</trd>
  <trd lng="io">junio</trd>
  <trd lng="it">giugno</trd>
  <trd lng="ja">&#20845;&#26376; [<ind>&#12429;&#12367;&#12364;&#12388;</ind>]</trd>
  <trdgrp lng="jbo">
    <trd>xavma'i</trd>,
    <trd>matsi'uma'i</trd>
  </trdgrp>
  <trd lng="nl">juni</trd>
  <trd lng="pl">czerwiec</trd>
  <trd lng="pt">junho</trd>
  <trd lng="ro">iunie</trd>
  <trd lng="ru">&c_i;&c_ju;&c_n;&c_mol;</trd>
  <trd lng="sk">j&uacute;n</trd>
  <trd lng="sv">juni</trd>
  <trd lng="tp">tenpo mun nanpa luka wan</trd>
  <trd lng="tr">haziran</trd>
  <trd lng="vo">yunul</trd>
</drv>

<drv mrk="juni.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o:
      <ekz>
        subita <tld/>a pluvo
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url
          ref="http://www.esperanto.be/fel/2003/007854.html">&Ccirc;u &gcirc;oja
          plorpluvo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        hoteloj en <tld/>o malaltigas siajn prezojn kaj varbas per la
        romantika nocio &leftquot;<tld/>a <esc>novedzino</esc>&#8221;
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url
          ref="http://www.esperanto.be/fel/2003/007854.html">&Ccirc;u &gcirc;oja
          plorpluvo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a referenduma malvenko
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="http://www.monato.net/2006/008871.php">&Scirc;tato,
          eklezio kaj ideologia laikeco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a numero, kiu atingis min je la fino de la eldonmonato,
        estas tre interesa
        <fnt>
          <bib>Monato</bib>
          <aut>Katoo Ivao</aut>
          <vrk><url
          ref="http://www.esperanto.be/fel/2003/008176.html">La junia
          numero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="io">juniala</trd>
  <trd lng="ja">&#20845;&#26376;&#12398; [<ind>&#12429;&#12367;&#12364;&#12388;&#12398;</ind>]</trd>
  <trd lng="vo">yunulik</trd>
</drv>
</art>
