<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>juni</rad>/o
</kap>
<drv mrk="juni.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sesa monato de la jaro.
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o">januaro, Rim.</ref>
  <trd lng="be">&c_ch;&c_e;&c_r;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trdgrp lng="br">
    <trd>Mezheven</trd>,
    <trd>Even</trd>
  </trdgrp>
  <trd lng="cs">&ccaron;erven</trd>
  <trd lng="de">Juni</trd>
  <trd lng="el">&Jota;&omikron;&ypsilon_ton;&ny;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="en">June</trd>
  <trd lng="es">junio</trd>
  <trd lng="fr">juin</trd>
  <trd lng="he"><klr tip="ind">&het;&vav;&dalet;&shin; </klr>&yod;&vav;&nun;&yod;</trd>
  <trd lng="hu">j&uacute;nius</trd>
  <trd lng="it">giugno</trd>
  <trd lng="nl">juni</trd>
  <trd lng="pl">czerwiec</trd>
  <trd lng="pt">junho</trd>
  <trd lng="ru">&c_i;&c_ju;&c_n;&c_mol;</trd>
  <trd lng="sv">juni</trd>
  <trd lng="tr">haziran</trd>
  <trd lng="vo">yunul</trd>
</drv>
</art>
