<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: juni.xml,v 1.42 2025/07/20 10:00:24 revo Exp $">
<kap>
  <ofc>*</ofc> <rad>Juni</rad>/o <fnt><bib>UV</bib></fnt>,
  <var><kap><rad var="j">juni</rad>/o</kap></var>
</kap>
<drv mrk="juni.0o">
  <kap><ofc>*</ofc> <tld/>o,
    <var><kap><tld var="j"/>o</kap></var>
  </kap>
  <snc mrk="juni.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Sesa <ref tip="lst" cel="monat.0o.KAL" lst="voko:monatoj" val="6">monato</ref> de la <ref
      tip="malprt" cel="jar.0o.KAL">jaro</ref> la&ubreve; la Gregoria kalendaro, inter <ref
      cel="maj.0o">Majo</ref>
      kaj <ref cel="juli.0o">Julio</ref>:
      <ekz>
        la dek du monatoj de l' kristana jaro estas: Januaro, Februaro,
        Marto, Aprilo, Majo, <tld/>o, Julio, <klr>[&#8230;]</klr>
        <fnt>
          <bib>DL</bib>
        </fnt>;
      </ekz>
      <ekz>
        la kontrakto fini&gcirc;is la 30-an de <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        de la komenco de <tld var="j"/>o &gcirc;is la fino de septembro, Nilo levi&gcirc;as
        kaj superver&scirc;as preska&ubreve; la tutan Egipton
        <fnt>
          <bib>Far1</bib>
          <lok>Anta&ubreve;parolo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="januar.0o.RIM">Januaro, Rim.</ref>
  <trd lng="af">Junie</trd>
  <trd lng="be">&c_ch;&c_e;&c_r;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trd lng="bg">&c_ju;&c_n;&c_i;</trd>
  <trd
  lng="bo">&#3935;&#4019;&#3851;&#3926;&#3851;&#3921;&#4018;&#3956;&#3906;&#3851;&#3924;&#3851;</trd>
  <trdgrp lng="br">
    <trd>Mezheven</trd>,
    <trd>Even</trd>
  </trdgrp>
  <trd lng="ca">juny</trd>
  <trd lng="cs">&ccaron;erven</trd>
  <trd lng="cv">&#1195;&#1239;&c_r;&c_t;&c_m;&c_je;</trd>
  <trd lng="de">Juni</trd>
  <trd lng="el">&Jota;&omikron;&ypsilon_ton;&ny;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="en">June</trd>
  <trd lng="es">junio</trd>
  <trd lng="fa">&f_zh;&a_w;&a_y_hamza;&a_n;</trd>
  <trd lng="fr">juin</trd>
  <trd lng="fy">juny</trd>
  <trd lng="he"><klr tip="ind">&het;&vav;&dalet;&shin; </klr>&yod;&vav;&nun;&yod;</trd>
  <trd lng="hu">j&uacute;nius</trd>
  <trd lng="id">Juni</trd>
  <trd lng="io">junio</trd>
  <trd lng="it">giugno</trd>
  <trd lng="ja">&#20845;&#26376; <pr>&#12429;&#12367;&#12364;&#12388;</pr></trd>
  <trdgrp lng="jbo">
    <trd>xavma'i</trd>,
    <trd>matsi'uma'i</trd>
  </trdgrp>
  <trd lng="nl">juni</trd>
  <trd lng="pl">czerwiec</trd>
  <trd lng="pt">junho</trd>
  <trd lng="ro">iunie</trd>
  <trd lng="ru">&c_i;&c_ju;&c_n;&c_mol;</trd>
  <trd lng="sk">j&uacute;n</trd>
  <trd lng="sv">juni</trd>
  <trd lng="tok">tenpo mun nanpa luka wan</trd>
  <trd lng="tr">haziran</trd>
  <trd lng="uk">&c_ch;&c_je;&c_r;&c_v;&c_je;&c_n;&c_mol;</trd>
  <trd lng="vo">yunul</trd>
  <trdgrp lng="zh">
    <trd>&#20845;&#26376; <pr>Li&ugrave;yu&egrave;</pr></trd>,
    <trd>&#20845;&#26376;&#20221; <pr>li&ugrave;yu&egrave;f&egrave;n</pr></trd>,
    <trd>&#20845;&#26376; <pr>li&ugrave;yu&egrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="juni.0a">
  <kap><tld lit="j"/>a</kap>
  <snc>
    <dif>
      Okazanta en <tld/>o, rilata al <tld/>o:
      <ekz>
        subita <tld lit="j"/>a pluvo
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2003/007854.html">&Ccirc;u &gcirc;oja
          plorpluvo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        hoteloj en <tld lit="j"/>o malaltigas siajn prezojn kaj varbas per la
        romantika nocio &leftquot;<tld lit="j"/>a <esc>novedzino</esc>&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="&Monato;/2003/007854.html">&Ccirc;u &gcirc;oja
          plorpluvo?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="j"/>a referenduma malvenko
        <fnt>
          <bib>Monato</bib>
          <aut>Armando Zecchin</aut>
          <vrk><url ref="&Monato;/2006/008871.php">&Scirc;tato,
          eklezio kaj ideologia laikeco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="j"/>a numero, kiu atingis min je la fino de la eldonmonato,
        estas tre interesa
        <fnt>
          <bib>Monato</bib>
          <aut>Katoo Ivao</aut>
          <vrk><url ref="&Monato;/2003/008176.html">La junia numero</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_e;&c_r;&c_v;&c_je;&c_n;&c_mol;&c_s;&c_k;&c_ib;</trd>
  <trd lng="io">juniala</trd>
  <trd lng="ja">&#20845;&#26376;&#12398; <pr>&#12429;&#12367;&#12364;&#12388;&#12398;</pr></trd>
  <trd lng="pl">czerwcowy</trd>
  <trd lng="vo">yunulik</trd>
</drv>
</art>
