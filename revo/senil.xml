<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>senil</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="senil.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="kaduk.0ega">kadukega</ref>
  </snc>
  <trd lng="fr">s&eacute;nile</trd>
  <trdgrp lng="hu">
    <trd>szenilis</trd>,
    <trd>elaggott</trd>
  </trdgrp>
  <trd lng="nl">seniel</trd>
  <trd lng="pl">starczy</trd>
  <trd lng="ru">&c_s;&c_je;&c_n;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="senil.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <ref tip="dif" cel="kaduk.0egeco">kadukegeco</ref>
    <ekz>
      kromedzinoj kaj &ccirc;ambristinoj <klr>...</klr> abomenis lian patron
      pro ties <tld/>eco kaj obstino
      <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 69a, p. 471a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="fr">s&eacute;nilit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>szenilit&aacute;s</trd>,
    <trd>aggkori elbutul&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">seniliteit</trd>
  <trd lng="pl">uwi&aogonek;d starczy</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; 
&c_m;&c_a;&c_r;&c_a;&c_z;&c_m;</trd>
</drv>
</art>
