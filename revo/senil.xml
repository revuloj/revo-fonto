<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: senil.xml,v 1.15 2024/03/24 20:00:01 revo Exp $">
<kap>
  <rad>senil</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="senil.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="senil.0a.kadukega">
    <dif>
      <klr>(pri homo)</klr> <ref tip="dif" cel="kaduk.0ega">Kadukega</ref>:
      <ekz>
        kaduki&gcirc;antaj, suferantaj preska&ubreve; pro <tld/>a demenco
        <fnt>
          <bib>Monato</bib>
          <aut>Angela Tellier</aut>
          <vrk><url ref="&Monato;/2004/008306.html">La&ubreve;proverbaj
          fabeloj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> fari&gcirc;is maljuna kaj <tld/>a meze de siaj
        dibo&ccirc;adoj
        <fnt>
          <aut>Clark Ashton Smith</aut>
          <vrk><url ref="http://www.eldritchdark.com/writings/translations/esperanto/2">La Sor&ccirc;ado de Ulua (The Witchcraft Of Ulua)</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        mi estas maljunul' <tld/>a, sen mangestoj viglaj, kaj la dors'
        pli &gcirc;iba ol anta&ubreve;e
        <fnt>
          <vrk><url ref="http://www.elerno.cn/penseo/PEN329.pdf">Penseo N-ro 329</url></vrk>
          <lok>elerno.cn</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_n;&c_ib;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_e;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>seniln&iacute;</trd>,
    <trd>sta&rcaron;eck&yacute;</trd>
  </trdgrp>
  <trd lng="de">senil</trd>
  <trdgrp lng="en">
    <trd>decrepit</trd>,
    <trd>senile <klr>(not necessarily demented)</klr></trd>
  </trdgrp>
  <trd lng="fr">s&eacute;nile</trd>
  <trdgrp lng="hu">
    <trd>szenilis</trd>,
    <trd>elaggott</trd>
  </trdgrp>
  <trd lng="it">senile</trd>
  <trd lng="nl">seniel</trd>
  <trd lng="pl">starczy</trd>
  <trd lng="pt">senil</trd>
  <trd lng="ru">&c_s;&c_je;&c_n;&c_i;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>seniln&yacute;</trd>,
    <trd>stareck&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_s;&c_t;&c_a;&c_r;&c_je;&c_ch;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x8001;&#x6001; <pr>l&#x1CE;ot&#xE0;i</pr></trd>,
    <trd>&#x8001;&#x614B; <pr>l&#x1CE;ot&#xE0;i</pr></trd>,
    <trd>&#x9F8D;&#x937E; <pr>l&#xF3;ngzh&#x14D;ng</pr></trd>,
    <trd>&#x9F99;&#x949F; <pr>l&#xF3;ngzh&#x14D;ng</pr></trd>,
    <trd>&#x9839;&#x8001; <pr>tu&#xED;l&#x1CE;o</pr></trd>,
    <trd>&#x9893;&#x8001; <pr>tu&#xED;l&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="senil.0eco">
  <kap><tld/>eco</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="senil.0eco.kadukegeco">
    <dif>
      Homa <ref tip="dif" cel="kaduk.0egeco">kadukegeco</ref>:
      <ekz>
        kromedzinoj kaj &ccirc;ambristinoj <klr>[&#x2026;]</klr> abomenis lian patron
        pro ties <tld/>eco kaj obstino
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 69a, p. 471a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_n;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_e;&c_ch;&c_a;&c_ja; &c_s;&c_l;&c_a;&c_b;&c_a;&c_s;&c_mol;&c_c;&c_mol; &c_r;&c_o;&c_z;&c_u;&c_m;&c_u;</trd>,
    <trd>&c_d;&c_z;&c_ja;&c_c;&c_ib;&c_n;&c_je;&c_c; <klr>(&c_r;&c_a;&c_z;&c_m;.)</klr></trd>
  </trdgrp>
  <trd lng="de">Senilit&auml;t</trd>
  <trdgrp lng="en">
    <trd>decrepitude</trd>,
    <trd>senility</trd>
  </trdgrp>
  <trd lng="fr">s&eacute;nilit&eacute;</trd>
  <trdgrp lng="hu">
    <trd>szenilit&aacute;s</trd>,
    <trd>aggkori elbutul&aacute;s</trd>
  </trdgrp>
  <trd lng="it">senilit&agrave;</trd>
  <trd lng="nl">seniliteit</trd>
  <trd lng="pl">uwi&aogonek;d starczy</trd>
  <trd lng="pt">senilidade</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_m;&c_a;&c_r;&c_a;&c_z;&c_m;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_o;&c_shch;&c_ib;</trd>
  </trdgrp>
</drv>
</art>
