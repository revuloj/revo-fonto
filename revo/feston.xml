<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>feston</rad>/o
</kap>
<drv mrk="feston.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Ornama arkoforma girlando el kunplektitaj floroj, folioj,
bran&ccirc;etoj.
    </dif>
    <trdgrp lng="de">
     <trd>Girlande</trd>,
     <trd>Blumengebinde</trd>,
     <trd>Laubgebinde</trd>,
     <trd>Blumengeh&auml;nge</trd></trdgrp>
     <trdgrp lng="hu">
       <trd>vir&aacute;gf&uuml;z&eacute;r</trd>,
       <trd>lombf&uuml;z&eacute;r</trd></trdgrp>
  </snc>
  <snc num="2">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Ornama&jcirc;o imitanta arkoformajn girlandojn.  
    </dif>
    <trdgrp lng="de">
     <trd>Feston</trd>,
     <trd>Ornamentgeh&auml;nge</trd></trdgrp>
    <trdgrp lng="hu">
      <trd>f&uuml;z&eacute;rd&iacute;sz</trd>,
      <trd>feszton</trd></trdgrp>
  </snc>
  <snc num="3">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ornama&jcirc;o de rando de &scirc;tofo formita per regulaj arkoformaj
      dentetoj, ordinare broditaj:
      <ekz>elegante <tld/>ita &ccirc;emizo.</ekz>
    </dif>
    <trdgrp lng="de">
     <trd>Feston</trd>,
     <trd>Zierb&ouml;gen</trd></trdgrp>
    <trd lng="hu">slingel&eacute;s</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_e;&c_s;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_g;&c_ib;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>feston</trd>,
    <trd>guirlande</trd></trdgrp>
  <trdgrp lng="pt">
    <trd>fest&#227;o</trd>,
    <trd>grinalda</trd>,
    <trd>bambolim</trd>,
    <trd>recorte</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_f;&c_je;&c_s;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_g;&c_i;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_a;</trd>
  </trdgrp>
</drv>
