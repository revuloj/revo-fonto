<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: feston.xml,v 1.20 2022/09/12 08:47:38 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>feston</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="feston.0o">
  <kap><tld/>o</kap>
  <snc mrk="feston.0o.KOMUNE">
    <dif>
      Ornama borderanta
      <ref tip="vid" cel="girlan.0o.KOMUNE">girlando</ref>
      el kunplektitaj floroj, folioj, bran&ccirc;etoj:
      <ekz>
        la murojn de la klaso&ccirc;ambro dekoras grandaj koloraj <tld/>oj, dum de la plafono
        pendas paperaj drakoj
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010678.php">Bratislavo, urbo kun (&ccirc;ina) klaso</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
     <trd>Girlande</trd>,
     <trd>Blumengebinde</trd>,
     <trd>Laubgebinde</trd>,
     <trd>Blumengeh&auml;nge</trd></trdgrp>
     <trdgrp lng="hu">
       <trd>vir&aacute;gf&uuml;z&eacute;r</trd>,
       <trd>lombf&uuml;z&eacute;r</trd>
     </trdgrp>
  </snc>
  <snc mrk="feston.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="super" cel="ornam.0ajxo">Ornama&jcirc;o</ref> imitanta arkoformajn girlandojn:
      <ekz>
        sur la listeloj estis tiel supre, kaj sube de la leonoj kaj bovoj estis
        malsuprenpendantaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        flordesegnaj tapi&scirc;oj pendis de oraj <tld/>oj sur la muroj
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>.
      </ekz>
    </dif>
    <bld
    lok="&WCU;/thumb/5/56/Wuppertal%2C_Handelstr._27%2C_Feston-Relief.jpg/320px-Wuppertal%2C_Handelstr._27%2C_Feston-Relief.jpg"
    prm="CC BY-SA 4.0"><tld/>a reliefo, Wuppertal, Handelstr. 27
        <fnt>
          <aut>Im Fokus, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Wuppertal,_Handelstr._27,_Feston-Relief.jpg">&WCW;/File:Wuppertal,_Handelstr._27,_Feston-Relief.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
     <trd>Feston</trd>,
     <trd>Ornamentgeh&auml;nge</trd></trdgrp>
    <trdgrp lng="hu">
      <trd>f&uuml;z&eacute;rd&iacute;sz</trd>,
      <trd>feszton</trd>
    </trdgrp>
  </snc>
  <snc mrk="feston.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Ornama&jcirc;o de <ref tip="malprt" cel="vest.0o">vesto</ref> imitanta florojn a&ubreve; foliojn:
      <ekz>
        &scirc;ia tre dekoltita vesto <klr>[&#8230;]</klr> estis unu el tiuj nepriskribeblaj,
        majstraj tualetoj el Parizo, kie puntoj kaj bantrozetoj, faldoj kaj <tld/>oj
        sin kuni&gcirc;is en mirinda bonordita senordeco
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frangx.0o">fran&gcirc;o</ref>,
      <ref cel="kvast.0o">kvasto</ref>,
      <ref cel="punt.0o">punto</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/3/38/Feston.jpg/280px-Feston.jpg" prm="PD">mezepoka robo kun <tld/>oj
        <fnt>
          <aut>Lvova Anastasiya, propra foto</aut>
          <lok><url ref="&WCW;/File:Feston.jpg">&WCW;/File:Feston.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
     <trd>Feston</trd>,
     <trd>Zierb&ouml;gen</trd></trdgrp>
    <trd lng="hu">slingel&eacute;s</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_e;&c_s;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_g;&c_ib;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>girlanda</trd>,
    <trd>v&ecaron;ncov&iacute;</trd>
  </trdgrp>
  <trd lng="en">festoon</trd>
  <trd lng="fr">feston</trd>
  <trdgrp lng="pt">
    <trd>fest&atilde;o</trd>,
    <trd>grinalda</trd>,
    <trd>bambolim</trd>,
    <trd>recorte</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_f;&c_je;&c_s;&c_t;&c_o;&c_n;</trd>,
    <trd>&c_g;&c_i;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>girlanda</trd>,
    <trd>kvetinov&yacute; z&aacute;ves</trd>
  </trdgrp>
  <trd lng="sv">festong</trd>
  <trdgrp lng="uk">
    <trd>&c_f;&c_je;&c_s;&c_t;&#x43E;&#x43D;</trd>,
    <trd>&c_gu;&c_ib;&c_r;&#x43B;&#x44F;&#x43D;&c_d;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="feston.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="feston.0i.TEKS">
    <dif>
      Ornami per <tld/>o,
      randi kiel <tld/>o;
      <ref tip="vid" cel="border.0i">borderi</ref>:
      <ekz>
        dank' al tiu, kies arkon floroj <tld/>as
        <klr>(la dio Amoro)</klr>
        <fnt>
          <aut>Kalidasa, trad. K. Kalocsay</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr66/amaj/hindujo/printempo.html">
          La printempo</url></vrk>,
          <lok>la nica literatura revuo, 1961-07 &gcirc;is 08 
          (36), p. 204a-207a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">blumenbeh&auml;ngt</trd>
  </snc>
  <trd lng="be">&c_a;&c_z;&c_d;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol; <ind>&c_g;&c_ib;&c_r;&c_l;&c_ja;&c_n;&c_d;&c_a;&c_m;&c_ib;</ind></trd>
  <trd lng="cs">v&ecaron;n&ccaron;it</trd>
  <trd lng="fr">festonner</trd>
  <trd lng="sk">ven&ccaron;i&tcaron;</trd>
  <trd lng="uk">&#x43F;&c_r;&c_i;&#x43A;&c_r;&c_a;&c_sh;&c_a;&c_t;&c_i; &c_gu;&c_ib;&c_r;&#x43B;&#x44F;&#x43D;&c_d;&c_a;&#x43C;&c_i;</trd>
</drv>
</art>
