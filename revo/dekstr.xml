<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>dekstr</rad>/a
</kap>
<drv mrk="dekstr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Trovi&gcirc;anta &ccirc;e la flanko kontra&ubreve;a al tiu de la koro:
      <ekz>
        <tld/>a mano
        <fnt>(<aut>Zamenhof</aut>: <vrk>Malnova testamento</vrk>,
        <lok>Eliro 15, 6</lok>)</fnt>;
      </ekz>
      <ekz>
        <ind>mal<tld/>a</ind>.
        <trd lng="nl">linker</trd>
        <trd lng="ru">&c_l;&c_je;&c_v;&c_y;&c_j;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">prav&yacute;</trd>
  <trd lng="en">right</trd>
  <trd lng="nl">rechter</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_y;&c_j; <klr>(&c_o; &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_je;)</klr></trd>
  <trd lng="tr">sa&gbreve;</trd>
</drv>
<drv mrk="dekstr.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      &Ccirc;e la <tld/>a flanko:
      <ekz>
        <tld/>e staras statuo;
      </ekz>
      <ekz>
        turni&gcirc;u <ind><tld/>en</ind>;
        <trd lng="nl">draai <ind>naar rechts</ind></trd>
        <trd lng="ru"><ind>&c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;</ind></trd>
      </ekz>
      <ekz>
        <ind>mal<tld/>e</ind>.
        <trd lng="nl">links</trd>
        <trd lng="en">left</trd>
        <trd lng="ru">&c_s;&c_l;&c_je;&c_v;&c_a;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">rechts</trd>
  <trd lng="nl">rechts</trd>
  <trd lng="ru">&c_s;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
</drv>
<drv mrk="dekstr.0ano0ulo">
  <kap><tld/>ano, <tld/>ulo</kap>
  <snc>
    <dif>
      Persono apartenanta al la konservativaj a&ubreve; reakciaj partioj, kiuj
      sidas en Parlamentejo <tld/>e de la prezidanto.
    </dif>
  </snc>
  <trd lng="de">Rechter</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_y;&c_j; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
  <trd lng="tr">sa&gbreve;c&inodot;</trd>
</drv>
<drv mrk="dekstr.mal0ulo">
  <kap>mal<tld/>ulo</kap>
  <snc num="1">
    <dif>
      Kiu uzas kutime mal<tld/>an manon anstata&ubreve; <tld/>a. 
    </dif>
    <trd lng="de">Linksh&auml;nder</trd>
    <trd lng="nl">linkshandige</trd>
    <trd lng="ru">&c_l;&c_je;&c_v;&c_sh;&c_a;</trd>
  </snc>
  <snc num="2">
    <dif>
      Apartenanta al la anta&ubreve;enemaj partioj, kiuj sidas mal<tld/>e de
      la prezidanto.
    </dif>
    <trd lng="de">Linker</trd>
    <trd lng="ru">&c_l;&c_je;&c_v;&c_y;&c_j; <klr>(&c_s;&c_u;&c_shch;.)</klr></trd>
    <trd lng="tr">solcu</trd>
  </snc>
</drv>
<drv mrk="dekstr.0uma">
  <kap><tld/>uma</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Turni&gcirc;anta a&ubreve; volvi&gcirc;anta la&ubreve; la direkto de
      horlo&gcirc;a montrilo.
    </dif>
  </snc>
  <trd lng="de">rechtsdrehend</trd>
  <trd lng="nl">rechtsdraaiend</trd>
  <trd lng="ru">&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_p;&c_o; &c_ch;&c_a;&c_s;&c_o;&c_v;&c_o;&c_j; &c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_je;</trd>
</drv>
</art>
