<drv mrk="dekstr.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="dekstr.0ulo.MOV">
    <dif>
      Tiu, kiu uzas kutime la <tld/>an manon por skribi, labori ks.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_w;&c_sh;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_w;&c_sh;&c_u;&c_n;</trd>
    </trdgrp>
    <trd lng="de">Rechtsh&auml;nder</trd>
    <trdgrp lng="en">
      <trd>right-handed person</trd>,
      <trd>right-hander</trd>
    </trdgrp>
    <trd lng="es">diestro</trd>
    <trd lng="fr">droitier<klr tip="ind"> (subst.)</klr></trd>
    <trd lng="hu">jobbkezes</trd>
    <trd lng="pl">prawor&eogonek;czny</trd>
    <trd lng="sk">prav&aacute;k</trd>
    <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_sh;&c_a;</trd>
  <trd lng="th">&#3588;&#3609;&#3606;&#3609;&#3633;&#3604;&#3586;&#3623;&#3634;</trd>
  </snc>
  <snc mrk="dekstr.0ulo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Persono apartenanta al la konservemaj a&ubreve; reakciaj partioj,
      kiuj tradicie sidas en Parlamentejo <tld/>e de la prezidanto.
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_y; <klr tip="amb">
    (&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_k;)</klr></trd>
    <trd lng="es">derechista</trd>
    <trd lng="de">Rechter</trd>
    <trd lng="en">right wing conservative or reactionary</trd>
    <trd lng="fr">droitier<klr tip="ind"> (politique, subst.)</klr>
    </trd>
    <trd lng="hu">jobboldali</trd>
    <trd lng="nl">rechtse</trd>
    <trd lng="pl">prawicowiec</trd>
    <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_y;&c_j;<klr tip="amb">
(&c_p;&c_o;&c_l;., &c_s;&c_u;&c_shch;.)</klr></trd>
    <trd lng="sk">pravi&ccaron;iar</trd>
    <trd lng="th">&#3613;&#3656;&#3634;&#3618;&#3586;&#3623;&#3634;</trd>
    <trd lng="tr">sa&gbreve;c&inodot;</trd>
  </snc>
  <trd lng="ca">dret&agrave;</trd>
  <trd lng="cs">pravi&ccaron;&aacute;k</trd>
  <trd lng="he">&alef;&yod;&shin; &yod;&mem;&yod;&fnun;</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_s;&c_o;&c_b;&c_a;</trd>,
    <trd>&c_shch;&c_o; &c_p;&c_r;&c_a;&c_c;&c_ju;&c_jeu;</trd>,
    <trd>&c_p;&c_i;&c_sh;&c_je; &c_p;&c_r;&c_a;&c_v;&c_o;&c_ju; &c_r;&c_u;&c_k;&c_o;&c_ju;</trd>
  </trdgrp>
