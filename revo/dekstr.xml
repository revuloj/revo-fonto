<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dekstr</rad>/a
</kap>

<drv mrk="dekstr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Trovi&gcirc;anta &ccirc;e la flanko kontra&ubreve;a al tiu de la
      koro:
      <ekz>
        Via <tld/>a mano
        <fnt><bib>MT</bib>, <lok>&Eli; 15:6</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>dret</trd>,
    <trd>destre</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>prav&yacute;</trd>,
    <trd>pravicový</trd>
  </trdgrp>
  <trd lng="de">rechter</trd>
  <trd lng="en">right</trd>
  <trd lng="es">derecho</trd>
  <trd lng="fr">droit<klr tip="ind"> (du c&ocirc;t&eacute;
  droit)</klr></trd>
  <trdgrp lng="hu">
    <trd>jobb</trd>,
    <trd>jobboldali</trd>
  </trdgrp>
  <trd lng="ja">&#21491; [<ind>&#12415;&#12366;</ind>]</trd>
  <trd lng="nl">rechter</trd>
  <trd lng="oc">drech</trd>
  <trd lng="pl">prawy</trd>
  <trdgrp lng="pt">
    <trd>direito <klr tip="amb">(lado)</klr></trd>,
    <trd>destro</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_y;&c_j; <klr>(&c_o;
  &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>prav&yacute;</trd>,
    <trd>pravicový</trd>
  </trdgrp>
  <trd lng="tr">sa&gbreve;</trd>
  <trd lng="vo">detik</trd>
  <trd lng="zh">&#21491;<klr> [y&ograve;u]</klr></trd>
</drv>

<drv mrk="dekstr.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc>
    <dif>
      &Ccirc;e la <tld/>a flanko:
      <ekz>
        <tld/>e staras statuo;
      </ekz>
      <ekz>
        turni&gcirc;u <ind><tld/>en</ind>.
        <trdgrp lng="be">
          <trd>&c_u;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>,
          <trd>&c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
        </trdgrp>
        <trd lng="ca">cap a la dreta</trd>
        <trd lng="fr">&agrave; <ind>droite</ind></trd>
        <trdgrp lng="hu">
          <trd>jobbra</trd>,
          <trd>jobbfel&eacute;</trd>
        </trdgrp>
        <trd lng="oc">a <ind>drecha</ind></trd>
        <trd lng="nl">naar <ind>rechts</ind></trd>
        <trdgrp lng="pl">
          <trd>po <ind>prawej</ind></trd>,
          <trd>z <ind>prawej</ind></trd>,
          <trd>na <ind>prawo</ind></trd>
        </trdgrp>
        <trd lng="ru"><ind>&c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;</ind>
        </trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_a;&c_r;&c_u;&c_ch;</trd>
  </trdgrp>
  <trd lng="ca">a la dreta</trd>
  <trdgrp lng="cs">
    <trd>napravo <klr>(kde)</klr></trd>,
    <trd>vpravo</trd>
  </trdgrp>
  <trd lng="de">rechts</trd>
  <trd lng="fr">&agrave; <ind>droite</ind></trd>
  <trdgrp lng="hu">
    <trd>jobb</trd>,
    <trd>jobboldalt</trd>
  </trdgrp>
  <trd lng="nl">rechts</trd>
  <trd lng="oc">a <ind>drecha</ind></trd>
  <trd lng="ru">&c_s;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>napravo</trd>,
    <trd>vpravo</trd>
  </trdgrp>
  <trd lng="vo">detiko</trd>
</drv>

<drv mrk="dekstr.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="apud.0">Apud</ref>, &ccirc;e la <tld/>a
      flanko de:
      <ekz>
        la Eternulo diris al mia sinjoro: Sidu <tld/>e de Mi
        <fnt><bib>MT</bib>, <lok>&Psa; 110:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_a; &c_a;&c_d;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_a;&c_r;&c_u;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>al costat <klr>[dret]</klr> de</trd>,
    <trd>a la dreta de</trd>
  </trdgrp>
  <trd lng="cs">napravo od</trd>
  <trd lng="de">rechts von</trd>
  <trd lng="fr">&agrave; droite de</trd>
  <trdgrp lng="pl">
    <trd>po <ind>prawej</ind></trd>,
    <trd>z <ind>prawej</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>napravo od</trd>,
    <trd>vpravo</trd>
  </trdgrp>
</drv>

<drv mrk="dekstr.0ano">
  <kap><tld/>ano<fnt>PV</fnt></kap>
  <snc>
    <uzo tip="fak">POL</uzo>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="dekstr.0ulo.POL"><tld/>ulo <sncref
    ref="dekstr.0ulo.POL"/></ref>
    <trd lng="ca">dret&agrave;</trd>
  </snc>
  <trd lng="cs">pravičák</trd>
  <trd lng="sk">pravičiar</trd>
</drv>

<drv mrk="dekstr.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Tiu, kiu uzas kutime la <tld/>an manon por skribi, labori ks.
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_r;&c_a;&c_w;&c_sh;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_w;&c_sh;&c_u;&c_n;</trd>
    </trdgrp>
    <trd lng="de">Rechtsh&auml;nder</trd>
    <trd lng="es">diestro</trd>
    <trd lng="fr">droitier<klr tip="ind"> (subst.)</klr></trd>
    <trd lng="hu">jobbkezes</trd>
    <trd lng="pl">prawor&eogonek;czny</trd>
    <trd lng="sk">prav&aacute;k</trd>
    <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_sh;&c_a;</trd>
  </snc>
  <snc mrk="dekstr.0ulo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Persono apartenanta al la konservemaj a&ubreve; reakciaj partioj,
      kiuj tradicie sidas en Parlamentejo <tld/>e de la prezidanto.
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_y; <klr tip="amb">
    (&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_k;)</klr></trd>
    <trd lng="es">derechista</trd>
    <trd lng="de">Rechter</trd>
    <trd lng="fr">droitier<klr tip="ind"> (politique, subst.)</klr>
    </trd>
    <trd lng="hu">jobboldali</trd>
    <trd lng="nl">rechtse</trd>
    <trd lng="pl">prawicowiec</trd>
    <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_y;&c_j;<klr tip="amb">
(&c_p;&c_o;&c_l;., &c_s;&c_u;&c_shch;.)</klr></trd>
    <trd lng="sk">pravi&ccaron;iar</trd>
    <trd lng="tr">sa&gbreve;c&inodot;</trd>
  </snc>
  <trd lng="ca">dret&agrave;</trd>
  <trd lng="cs">pravičák</trd>
</drv>

<drv mrk="dekstr.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Trovi&gcirc;anta &ccirc;e la flanko de la koro.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_je;&c_v;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>esquerre</trd>,
    <trd>sinistre</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>levý</trd>,
    <trd>na levé straně těla</trd>
  </trdgrp>
  <trd lng="fr">gauche<klr tip="ind"> (du c&ocirc;t&eacute;
  gauche)</klr></trd>
  <trd lng="ja">&#24038; [<ind>&#12402;&#12384;&#12426;</ind>]</trd>
  <trd lng="nl">linker</trd>
  <trd lng="pl">lewy</trd>
  <trd lng="ru">&c_l;&c_je;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">&lcaron;av&yacute;</trd>
  <trd lng="vo">nedetik</trd>
  <trd lng="zh">&#24038;<klr> [zu&#466;]</klr></trd>
</drv>

<drv mrk="dekstr.mal0e">
  <kap><ofc>*</ofc>mal<tld/>e</kap>
  <snc>
    <dif>
      &Ccirc;e la mal<tld/>a flanko:
      <ekz>
        ni disi&gcirc;is kaj iris en diversajn flankojn: mi iris
        <tld/>en, kaj li iris mal<tld/>en
        <fnt><bib>F</bib>, <lok>&FE; 28</lok></fnt>;
      </ekz>
      <ekz>
        staru mal<tld/>e;
      </ekz>
      <ekz>
        turni&gcirc;u <ind>mal<tld/>en</ind>.
        <trdgrp lng="be">
          <trd>&c_n;&c_a;&c_l;&c_je;&c_v;&c_a;</trd>,
          <trd>&c_u;&c_l;&c_je;&c_v;&c_a;</trd>
        </trdgrp>
        <trd lng="ca">cap a l'esquerra</trd>
        <trd lng="fr">&agrave; <ind>gauche</ind></trd>
        <trdgrp lng="hu">
          <trd>balra</trd>,
          <trd>balfel&eacute;</trd>
        </trdgrp>
        <trd lng="ru">&c_n;&c_a;&c_l;&c_je;&c_v;&c_o;</trd>
        <trd lng="vo">nedetiko</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_l;&c_je;&c_v;&c_a;</trd>,
    <trd>&c_l;&c_je;&c_v;&c_a;&c_r;&c_u;&c_ch;</trd>
  </trdgrp>
  <trd lng="ca">a l'esquerra</trd>
  <trd lng="cs">vlevo</trd>
  <trd lng="en">left</trd>
  <trd lng="fr">&agrave; <ind>gauche</ind></trd>
  <trdgrp lng="hu">
    <trd>balra</trd>,
    <trd>baloldalt</trd>
  </trdgrp>
  <trd lng="nl">links</trd>
  <trdgrp lng="pl">
    <trd>po <ind>lewej</ind></trd>,
    <trd>z <ind>lewej</ind></trd>,
    <trd>na <ind>lewo</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_je;&c_v;&c_a;</trd>
  <trd lng="sk">vľavo</trd>
</drv>

<drv mrk="dekstr.mal0ede">
  <kap>mal<tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="apud.0">Apud</ref>, &ccirc;e la mal<tld/>a
      flanko de.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_l;&c_je;&c_v;&c_a; &c_a;&c_d;</trd>,
    <trd>&c_l;&c_je;&c_v;&c_a;&c_r;&c_u;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>al costat <klr>[esquerre]</klr> de</trd>,
    <trd>a l'esquerra de</trd>
  </trdgrp>
  <trd lng="de">links von</trd>
  <trd lng="fr">&agrave; gauche de</trd>
  <trdgrp lng="pl">
    <trd>po <ind>lewej</ind></trd>,
    <trd>z <ind>lewej</ind></trd>
  </trdgrp>
</drv>

<drv mrk="dekstr.mal0ulo">
  <kap>mal<tld/>ulo</kap>
  <snc>
    <dif>
      Tiu, kiu uzas kutime la mal<tld/>an manon por skribi, labori ks.
    </dif>
    <trdgrp lng="be">
      <trd>&c_l;&c_ja;&c_w;&c_sh;&c_a;</trd>,
      <trd>&c_l;&c_ja;&c_w;&c_sh;&c_u;&c_n;</trd>
    </trdgrp>
    <trd lng="de">Linksh&auml;nder</trd>
    <trd lng="es">zurdo</trd>
    <trd lng="fr">gaucher</trd>
    <trd lng="hu">balkezes</trd>
    <trd lng="nl">linkshandige</trd>
    <trdgrp lng="pl">
      <trd>lewor&eogonek;czny</trd>,
      <trd>ma&nacute;kut<klr> (pot.)</klr></trd>,
      <trd>szmaja<klr> (wulg.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_je;&c_v;&c_sh;&c_a;</trd>
    <trd lng="sk">&lcaron;av&aacute;k</trd>
  </snc>
  <snc mrk="dekstr.mal0ulo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Persono apartenanta al la anta&ubreve;enemaj partioj,
      kiuj tradicie sidas en Parlamentej mal<tld/>e de la prezidanto.
    </dif>
    <trd lng="be">&c_l;&c_je;&c_v;&c_y; <klr tip="amb">(&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_k;)</klr></trd>
    <trd lng="de">Linker</trd>
    <trd lng="es">izquierdista</trd>
    <trd lng="fr">gauchisant<klr tip="ind"> (subst.)</klr></trd>
    <trd lng="hu">baloldali</trd>
    <trd lng="nl">linkse</trd>
    <trd lng="pl">lewicowiec</trd>
    <trd lng="ru">&c_l;&c_je;&c_v;&c_y;&c_j;<klr tip="amb"> (&c_p;&c_o;&c_l;.,
    &c_s;&c_u;&c_shch;.)</klr></trd>
    <trd lng="sk">&lcaron;avi&ccaron;iar</trd>
    <trd lng="tr">solcu</trd>
  </snc>
  <trd lng="ca">esquerr&agrave;</trd>
  <trd lng="cs">levičák</trd>
</drv>

<drv mrk="dekstr.0uma">
  <kap><tld/>uma</kap>
  <snc>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Turni&gcirc;anta a&ubreve; volvi&gcirc;anta la&ubreve; la direkto
      de horlo&gcirc;a montrilo.
    </dif>
    <trd lng="ca">dextrogir</trd>
    <trd lng="de">rechtsdrehend</trd>
    <trdgrp lng="fr">
      <trd>dextrorsum</trd>,
      <trd>dextrogyre</trd>,
      <trd>direct<klr tip="ind"> (en p. du sens de
      rotation)</klr></trd>,
      <trd>dans le <ind>sens</ind> des aiguilles d'une montre</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>jobbos</trd>,
      <trd>jobb ir&aacute;ny&uacute;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>dextr&oacute;giro</trd>,
      <trd>horario</trd>
    </trdgrp>
    <trd lng="nl">rechtsdraaiend</trd>
    <trd lng="pl">prawoskr&eogonek;tny</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_p;&c_o; &c_ch;&c_a;&c_s;&c_o;&c_v;&c_o;&c_j;
      &c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_v;&c_i;&c_n;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dekstr.0uma.bazo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="baz.0o.vektoraspaco">bazo</ref> de
      <ref cel="orient.0i.spacon">orientita</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
      Havanta pozitivan <ref cel="orient.0igxo.bazo">orienti&gcirc;on
      </ref>.
    </dif>
    <trd lng="ca">amb orientaci&oacute; positiva</trd>
    <trdgrp lng="de">
      <trd>Rechts-<klr tip="amb">(system)</klr></trd>,
      <trd>positiv orientiert<klr tip="ind"> (-e Basis)</klr></trd>
    </trdgrp>
    <trd lng="en">positively oriented<klr tip="ind"> (basis)</klr></trd>
    <trd lng="fr"><klr tip="amb">(base) </klr>directe</trd>
    <trd lng="pl"><klr tip="amb">(baza) </klr>prawoskr&eogonek;tna</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_a;&c_v;&c_y;&c_j; &c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;<klr tip="ind">
      (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>,
      <trd>&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o; &c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;<klr
      tip="ind"> (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">pravotočivý <klr>(šroub| vrtule| schodiště)</klr></trd>
  <trd lng="sk">pravoto&ccaron;iv&yacute;</trd>
</drv>

<drv mrk="dekstr.mal0uma">
  <kap>mal<tld/>uma</kap>
  <snc>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Turni&gcirc;anta a&ubreve; volvi&gcirc;anta kontra&ubreve; la
      direkto de horlo&gcirc;a montrilo.
    </dif>
    <trd lng="ca">levogir</trd>
    <trdgrp lng="fr">
      <trd>indirect<klr tip="ind"> (en p. du sens de rotation)</klr>
      </trd>,
      <trd>l&eacute;vogyre</trd>,
      <trd>r&eacute;trograde<klr tip="ind"> (en p. du sens de
      rotation)</klr></trd>,
      <trd>dans le <ind>sens</ind> contraire des aiguilles d'une
      montre</trd>,
      <trd>sinistrogyre</trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_p;&c_r;&c_o;&c_t;&c_i;&c_v; &c_ch;&c_a;&c_s;&c_o;&c_v;&c_o;&c_j;
      &c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_i;</trd>,
      <trd>&c_l;&c_je;&c_v;&c_o;&c_v;&c_i;&c_n;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>lev&oacute;giro</trd>,
      <trd>antihorario</trd>
    </trdgrp>
    <trd lng="pl">lewoskr&eogonek;tny</trd>
  </snc>
  <snc mrk="dekstr.mal0uma.bazo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="baz.0o.vektoraspaco">bazo</ref> de
      <ref cel="orient.0i.spacon">orientita</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
      Havanta negativan <ref cel="orient.0igxo.bazo">orienti&gcirc;on
      </ref>.
    </dif>
    <trd lng="ca">amb orientaci&oacute; negativa</trd>
    <trdgrp lng="de">
