<drv mrk="dekstr.0uma">
  <kap><tld/>uma</kap>
  <snc mrk="dekstr.0uma.KOMUNE">
    <dif>
      en direkto <tld/>a; <tld/>amana; <tld/>apartia:
      <ekz>
        &ccirc;i tiu loka versio de la silabaro, havanta kelkajn aparta&jcirc;ojn, estis ofte uzata
        por la skribado de tekstoj <tld/>umaj kaj estas karakterizata de aparta
        kronologio
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2014/011357.php">Kio estas la kipra
          silabaro?</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_a;&c_g;&c_a; &c_k;&c_ib;&c_r;&c_u;&c_n;&c_k;&c_u;</trd>,
    <trd>&c_z;&c_mol;&c_l;&c_je;&c_v;&c_a; &c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="en">righthand</trd>
  <trdgrp lng="pl">
    <trd>prawoskr&eogonek;tny</trd>,
    <trd>prawostronny</trd>
  </trdgrp>
  </snc>
  <snc mrk="dekstr.0uma.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Turni&gcirc;anta a&ubreve; volvi&gcirc;anta la&ubreve; la direkto
      de horlo&gcirc;a montrilo.
    </dif>
    <trd lng="be">&c_p;&c_a; &c_h;&c_o;&c_d;&c_z;&c_je; &c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_mol;&c_n;&c_ib;&c_k;&c_a;&c_v;&c_a;&c_j; <ind>&c_s;&c_t;&c_r;&c_e;&c_l;&c_k;&c_ib;</ind></trd>
    <trd lng="ca">dextrogir</trd>
    <trd lng="de">rechtsdrehend</trd>
    <trd lng="en">clockwise</trd>
    <trdgrp lng="fr">
      <trd>dextrorsum</trd>,
      <trd>dextrogyre</trd>,
      <trd>direct<klr tip="ind"> (en p. du sens de
      rotation)</klr></trd>,
      <trd>dans le <ind>sens</ind> des aiguilles d'une montre</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>jobbos</trd>,
      <trd>jobb ir&aacute;ny&uacute;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>dextr&oacute;giro</trd>,
      <trd>horario</trd>
    </trdgrp>
    <trd lng="id"><baz>arah</baz>searah jarum jam</trd>
    <trd lng="nl">rechtsdraaiend</trd>
    <trd lng="pl">zgodny z ruchem <ind>zegara</ind></trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_p;&c_o; &c_ch;&c_a;&c_s;&c_o;&c_v;&c_o;&c_j;
      &c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_v;&c_i;&c_n;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>
    </trdgrp>
    <trd lng="zh">&#21491;&#26059; <pr>y&ograve;uxu&aacute;n</pr></trd>
  </snc>
  <snc mrk="dekstr.0uma.bazo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="baz.0o.vektoraspaco">bazo</ref> de
      <ref cel="orient.0i.spacon">orientita</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
      Havanta pozitivan <ref cel="orient.0igxo.bazo">orienti&gcirc;on
      </ref>.
    </dif>
    <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_y; &c_w;&c_p;&c_a;&c_r;&c_a;&c_d;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y; <klr>(&c_b;&c_a;&c_z;&c_y;&c_s;)</klr></trd>
    <trd lng="ca">amb orientaci&oacute; positiva</trd>
    <trdgrp lng="de">
      <trd>Rechts-<klr tip="amb">(system)</klr></trd>,
      <trd>positiv orientiert<klr tip="ind"> (-e Basis)</klr></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>positively <ind>oriented</ind><klr tip="ind"> (basis)</klr></trd>,
      <trd><ind>left</ind>-handed <klr>(coordinate system)</klr></trd>
    </trdgrp>
    <trd lng="fr"><klr tip="amb">(base) </klr>directe</trd>
    <trd lng="pl"><klr tip="amb">(baza) </klr>prawoskr&eogonek;tna</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_a;&c_v;&c_y;&c_j; &c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;<klr tip="ind">
      (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>,
      <trd>&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o; &c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">pravoto&ccaron;iv&yacute; <klr>(&scaron;roub| vrtule| schodi&scaron;t&ecaron;)</klr></trd>
  <trd lng="he">&yod;&mem;&nun;&yod;</trd>
  <trdgrp lng="ja">
    <trd>&#21491;&#24059;&#12365;&#12398;  <pr>&#12415;&#12366;&#12414;&#12365;&#12398;</pr></trd>,
    <trd>&#21491;&#22238;&#12426;&#12398;
    <pr>&#12415;&#12366;&#12414;&#12431;&#12426;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="sk">pravoto&ccaron;iv&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_k;&c_i;&c_j; &c_r;&c_u;&c_h;&c_a;&c_jeu;&c_t;&c_mol;&c_s;&c_ja; &c_z;&c_l;&c_ib;&c_v;&c_a; &c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;</trd>,
    <trd>&c_z;&c_a; &c_g;&c_o;&c_d;&c_i;&c_n;&c_n;&c_i;&c_k;&c_o;&c_v;&c_o;&c_ju; &c_s;&c_t;&c_r;&c_ib;&c_l;&c_k;&c_o;&c_ju;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_ja;&c_k;&c_i;&c_j; &c_v;&#8217;&c_jeu;&c_t;&c_mol;&c_s;&c_ja; &c_p;&c_r;&c_a;&c_v;&c_o;&c_r;&c_u;&c_ch;</trd>
  </trdgrp>
</drv>

<drv mrk="dekstr.mal0uma">
