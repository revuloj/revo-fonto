<drv mrk="dekstr.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="dekstr.0o.flanko">
    <dif>
      La <tld/>a <ref tip="super" cel="flank.0o.propre">flanko</ref>:
      <ekz>
        trovi&gcirc;is je la <tld/>o difinita vojeto, kiun <nom>Elvisa</nom> kaj mi &ccirc;iam haste preteriris
        <fnt>
          <bib>StU</bib>
          <lok>Britujo</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">right <klr>(side)</klr></trd>
  <trd lng="id">kanan</trd>
  </snc>
  <snc mrk="dekstr.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      La tuto de la politike <tld/>aj grupi&gcirc;oj de iu lando:
      <ekz>
        senbrida naciismo de la hispana <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="https://www.monato.be/2006/008812.php">Survoje al pli granda
          memregado</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="en">right <klr>(wing)</klr></trd>
  <trd lng="id">sayap kanan</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_v;&c_y; &c_b;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_y; &c_f;&c_l;&c_ja;&c_n;&c_g;</trd>
  </trdgrp>
  <trd lng="de">die <ind>Rechte</ind></trd>
  <trdgrp lng="pl">
    <trd>prawa strona</trd>,
    <trd>prawica</trd>
  </trdgrp>
</drv>

