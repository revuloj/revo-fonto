<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>dekstr</rad>/a
</kap>
<drv mrk="dekstr.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Trovi&gcirc;anta &ccirc;e la flanko kontra&ubreve;a al tiu de la koro:
      <ekz>
        Via <tld/>a mano
        <fnt><bib>MT</bib>, <lok>&Eli; 15:6</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">prav&yacute;</trd>
  <trd lng="en">right</trd>
  <trd lng="es">derecho</trd>
  <trd lng="fr">droit<klr tip="ind"> (du c&ocirc;t&eacute;
  droit)</klr></trd>
  <trdgrp lng="hu">
    <trd>jobb</trd>,
    <trd>jobboldali</trd>
  </trdgrp>
  <trd lng="nl">rechter</trd>
  <trd lng="de">rechter</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_y;&c_j; <klr>(&c_o;
  &c_s;&c_t;&c_o;&c_r;&c_o;&c_n;&c_je;)</klr></trd>
  <trd lng="sk">prav&yacute;</trd>
  <trd lng="tr">sa&gbreve;</trd>
  <trd lng="vo">detik</trd>
  <trd lng="zh">&#21491;<klr> [y&ograve;u]</klr></trd>
</drv>

<drv mrk="dekstr.0e">
  <kap><ofc>*</ofc><tld/>e</kap>
  <snc>
    <dif>
      &Ccirc;e la <tld/>a flanko:
      <ekz>
        <tld/>e staras statuo;
      </ekz>
      <ekz>
        turni&gcirc;u <ind><tld/>en</ind>.
        <trd lng="fr">&agrave; <ind>droite</ind></trd>
        <trdgrp lng="hu">
          <trd>jobbra</trd>,
          <trd>jobbfel&eacute;</trd>
        </trdgrp>
        <trd lng="nl">naar <ind>rechts</ind></trd>
        <trd lng="ru"><ind>&c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">rechts</trd>
  <trd lng="fr">&agrave; <ind>droite</ind></trd>
  <trdgrp lng="hu">
    <trd>jobb</trd>,
    <trd>jobboldalt</trd>
  </trdgrp>
  <trd lng="nl">rechts</trd>
  <trd lng="ru">&c_s;&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
  <trd lng="sk">vpravo</trd>
  <trd lng="vo">detiko</trd>
</drv>

<drv mrk="dekstr.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="apud.0">Apud</ref>, &ccirc;e la <tld/>a flanko de:
      <ekz>
        la Eternulo diris al mia sinjoro: Sidu <tld/>e de Mi
        <fnt><bib>MT</bib>, <lok>&Psa; 110:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&agrave; droite de</trd>
</drv>

<drv mrk="dekstr.0ano">
  <kap><tld/>ano<fnt>PV</fnt></kap>
  <snc>
    <uzo tip="fak">POL</uzo>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="dekstr.0ulo.POL"><tld/>ulo <sncref
    ref="dekstr.0ulo.POL"/></ref>
  </snc>
</drv>

<drv mrk="dekstr.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Tiu, kiu uzas kutime la <tld/>an manon por skribi, labori ks.
    </dif>
    <trd lng="es">diestro</trd>
    <trd lng="fr">droitier<klr tip="ind"> (subst.)</klr></trd>
    <trd lng="hu">jobbkezes</trd>
    <trd lng="sk">prav&aacute;k</trd>
    <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_sh;&c_a;</trd>
  </snc>
  <snc mrk="dekstr.0ulo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Persono apartenanta al la konservemaj a&ubreve; reakciaj partioj,
      kiuj tradicie sidas en Parlamentejo <tld/>e de la prezidanto.
    </dif>
    <trd lng="es">derechista</trd>
    <trd lng="de">Rechter</trd>
    <trd lng="fr">droitier<klr tip="ind"> (politique, subst.)</klr></trd>
    <trd lng="hu">jobboldali</trd>
    <trd lng="nl">rechtse</trd>
    <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_y;&c_j;<klr tip="amb"> (&c_p;&c_o;&c_l;., &c_s;&c_u;&c_shch;.)</klr></trd>
    <trd lng="sk">pravi&ccaron;iar</trd>
    <trd lng="tr">sa&gbreve;c&inodot;</trd>
  </snc>
</drv>

<drv mrk="dekstr.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Trovi&gcirc;anta &ccirc;e la flanko de la koro.
    </dif>
  </snc>
  <trd lng="fr">gauche<klr tip="ind"> (du c&ocirc;t&eacute;
  gauche)</klr></trd>
  <trd lng="nl">linker</trd>
  <trd lng="ru">&c_l;&c_je;&c_v;&c_y;&c_j;</trd>
  <trd lng="sk">&lcaron;av&yacute;</trd>
  <trd lng="vo">nedetik</trd>
  <trd lng="zh">&#24038;<klr> [zu&#466;]</klr></trd>
</drv>

<drv mrk="dekstr.mal0e">
  <kap><ofc>*</ofc>mal<tld/>e</kap>
  <snc>
    <dif>
      &Ccirc;e la mal<tld/>a flanko:
      <ekz>
        ni disi&gcirc;is kaj iris en diversajn flankojn: mi iris <tld/>en, kaj
        li iris mal<tld/>en
        <fnt><bib>F</bib>, <lok>&FE; 28</lok></fnt>;
      </ekz>
      <ekz>
        staru mal<tld/>e;
      </ekz>
      <ekz>
        turni&gcirc;u <ind>mal<tld/>en</ind>.
        <trd lng="fr">&agrave; <ind>gauche</ind></trd>
        <trdgrp lng="hu">
          <trd>balra</trd>,
          <trd>balfel&eacute;</trd>
        </trdgrp>
        <trd lng="ru">&c_n;&c_a;&c_l;&c_je;&c_v;&c_o;</trd>
        <trd lng="vo">nedetiko</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="en">left</trd>
  <trd lng="fr">&agrave; <ind>gauche</ind></trd>
  <trdgrp lng="hu">
    <trd>balra</trd>,
    <trd>baloldalt</trd>
  </trdgrp>
  <trd lng="nl">links</trd>
  <trd lng="ru">&c_s;&c_l;&c_je;&c_v;&c_a;</trd>
</drv>

<drv mrk="dekstr.mal0ede">
  <kap>mal<tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="apud.0">Apud</ref>, &ccirc;e la mal<tld/>a flanko de.
    </dif>
  </snc>
  <trd lng="fr">&agrave; gauche de</trd>
</drv>

<drv mrk="dekstr.mal0ulo">
  <kap>mal<tld/>ulo</kap>
  <snc>
    <dif>
      Tiu, kiu uzas kutime la mal<tld/>an manon por skribi, labori ks.
    </dif>
    <trd lng="de">Linksh&auml;nder</trd>
    <trd lng="es">zurdo</trd>
    <trd lng="fr">gaucher</trd>
    <trd lng="hu">balkezes</trd>
    <trd lng="nl">linkshandige</trd>
    <trd lng="ru">&c_l;&c_je;&c_v;&c_sh;&c_a;</trd>
    <trd lng="sk">&lcaron;av&aacute;k</trd>
  </snc>
  <snc mrk="dekstr.mal0ulo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Persono apartenanta al la anta&ubreve;enemaj partioj,
      kiuj tradicie sidas en Parlamentej mal<tld/>e de la prezidanto.
    </dif>
    <trd lng="de">Linker</trd>
    <trd lng="es">izquierdista</trd>
    <trd lng="fr">gauchisant<klr tip="ind"> (subst.)</klr></trd>
    <trd lng="hu">baloldali</trd>
    <trd lng="nl">linkse</trd>
    <trd lng="ru">&c_l;&c_je;&c_v;&c_y;&c_j;<klr tip="amb"> (&c_p;&c_o;&c_l;., &c_s;&c_u;&c_shch;.)</klr></trd>
    <trd lng="sk">&lcaron;avi&ccaron;iar</trd>
    <trd lng="tr">solcu</trd>
  </snc>
</drv>

<drv mrk="dekstr.0uma">
  <kap><tld/>uma</kap>
  <snc>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Turni&gcirc;anta a&ubreve; volvi&gcirc;anta la&ubreve; la direkto de
      horlo&gcirc;a montrilo.
    </dif>
    <trd lng="de">rechtsdrehend</trd>
    <trdgrp lng="fr">
      <trd>dextrorsum</trd>,
      <trd>dextrogyre</trd>,
      <trd>direct<klr tip="ind"> (en p. du sens de rotation)</klr></trd>,
      <trd>dans le <ind>sens</ind> des aiguilles d'une montre</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>jobbos</trd>,
      <trd>jobb ir&aacute;ny&uacute;</trd>
    </trdgrp>
    <trd lng="nl">rechtsdraaiend</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_p;&c_o; &c_ch;&c_a;&c_s;&c_o;&c_v;&c_o;&c_j; &c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_v;&c_i;&c_n;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dekstr.0uma.bazo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="baz.0o.vektoraspaco">bazo</ref> de
      <ref cel="orient.0i.spacon">orientita</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
      Havanta pozitivan <ref cel="orient.0igxo.bazo">orienti&gcirc;on</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Rechts-<klr tip="amb">(system)</klr></trd>,
      <trd>positiv orientiert<klr tip="ind"> (-e Basis)</klr></trd>
    </trdgrp>
    <trd lng="en">positively oriented<klr tip="ind"> (basis)</klr></trd>
    <trd lng="fr"><klr tip="amb">(base) </klr>directe</trd>
    <trd lng="pl"><klr tip="amb">(baza) </klr>prawoskr&eogonek;tna</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_a;&c_v;&c_y;&c_j; &c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>,
      <trd>&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o; &c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;<klr tip="ind">
      (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="sk">pravoto&ccaron;iv&yacute;</trd>
</drv>

<drv mrk="dekstr.mal0uma">
  <kap>mal<tld/>uma</kap>
  <snc>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Turni&gcirc;anta a&ubreve; volvi&gcirc;anta kontra&ubreve; la direkto de
      horlo&gcirc;a montrilo.
    </dif>
    <trdgrp lng="fr">
      <trd>indirect<klr tip="ind"> (en p. du sens de rotation)</klr></trd>,
      <trd>l&eacute;vogyre</trd>,
      <trd>r&eacute;trograde<klr tip="ind"> (en p. du sens de
      rotation)</klr></trd>,
      <trd>dans le <ind>sens</ind> contraire des aiguilles d'une
      montre</trd>,
      <trd>sinistrogyre</trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_p;&c_r;&c_o;&c_t;&c_i;&c_v; &c_ch;&c_a;&c_s;&c_o;&c_v;&c_o;&c_j; &c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_i;</trd>,
      <trd>&c_l;&c_je;&c_v;&c_o;&c_v;&c_i;&c_n;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dekstr.mal0uma.bazo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="baz.0o.vektoraspaco">bazo</ref> de
      <ref cel="orient.0i.spacon">orientita</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
      Havanta negativan <ref cel="orient.0igxo.bazo">orienti&gcirc;on</ref>.
    </dif>
    <trdgrp lng="de">
      <trd>Links-<klr tip="amb">(system)</klr></trd>,
      <trd>negativ orientiert<klr tip="ind"> (-e Basis)</klr></trd>
    </trdgrp>
    <trd lng="en">negatively oriented<klr tip="ind"> (basis)</klr></trd>
    <trd lng="fr"><klr tip="ind">(base) </klr>r&eacute;trograde</trd>
    <trd lng="pl"><klr tip="amb">(baza) </klr>lewoskr&eogonek;tna</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_je;&c_v;&c_y;&c_j; &c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>,
      <trd>&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o; &c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;<klr tip="ind">
      (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="sk">&lcaron;avoto&ccaron;iv&yacute;</trd>
</drv>
</art>
