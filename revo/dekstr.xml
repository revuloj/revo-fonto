<drv mrk="dekstr.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="dekstr.0ede.KOMUNE">
    <dif>
      <ref tip="super" cel="apud.0">Apud</ref>, &ccirc;e la <tld/>a
      flanko de:
      <ekz>
        la Eternulo diris al mia sinjoro: Sidu <tld/>e de Mi
        <fnt><bib>MT</bib>, <lok>&Psa; 110:1</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_a; &c_a;&c_d;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_a;&c_r;&c_u;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>al costat <klr>[dret]</klr> de</trd>,
    <trd>a la dreta de</trd>
  </trdgrp>
  <trd lng="cs">napravo od</trd>
  <trd lng="de">rechts von</trd>
  <trd lng="en">to the right of</trd>
  <trd lng="fr">&agrave; droite de</trd>
  <trdgrp lng="pl">
    <trd>po <ind>prawej</ind></trd>,
    <trd>z <ind>prawej</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>napravo od</trd>,
    <trd>vpravo</trd>
  </trdgrp>
</drv>

