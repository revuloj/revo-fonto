      <trd>ma&nacute;kut<klr> (pot.)</klr></trd>,
      <trd>szmaja<klr> (wulg.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_l;&c_je;&c_v;&c_sh;&c_a;</trd>
    <trd lng="sk">&lcaron;av&aacute;k</trd>
  </snc>
  <snc mrk="dekstr.mal0ulo.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Persono apartenanta al la progresemaj partioj,
      kiuj tradicie sidas en Parlamentejo mal<tld/>e de la prezidanto.
    </dif>
    <trd lng="be">&c_l;&c_je;&c_v;&c_y; <klr tip="amb">(&c_p;&c_a;&c_l;&c_ib;&c_t;&c_y;&c_k;)</klr></trd>
    <trd lng="de">Linker</trd>
    <trd lng="en">left wing liberal or radical</trd>
    <trd lng="es">izquierdista</trd>
    <trd lng="fr">gauchisant<klr tip="ind"> (subst.)</klr></trd>
    <trd lng="hu">baloldali</trd>
    <trd lng="nl">linkse</trd>
    <trd lng="pl">lewicowiec</trd>
    <trd lng="ru">&c_l;&c_je;&c_v;&c_y;&c_j;<klr tip="amb"> (&c_p;&c_o;&c_l;.,
    &c_s;&c_u;&c_shch;.)</klr></trd>
    <trd lng="sk">&lcaron;avi&ccaron;iar</trd>
    <trd lng="tr">solcu</trd>
