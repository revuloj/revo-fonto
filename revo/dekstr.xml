<drv mrk="dekstr.0uma">
  <kap><tld/>uma</kap>
  <snc mrk="dekstr.0uma.KOMUNE">
    <dif>
      en direkto <tld/>a; <tld/>amana; <tld/>apartia:
      <ekz>
        &ccirc;i tiu loka versio de la silabaro, havanta kelkajn aparta&jcirc;ojn, estis ofte uzata
        por la skribado de tekstoj <tld/>umaj kaj estas karakterizata de aparta
        kronologio
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2014/011357.php">Kio estas la kipra
          silabaro?</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="dekstr.0uma.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Turni&gcirc;anta a&ubreve; volvi&gcirc;anta la&ubreve; la direkto
      de horlo&gcirc;a montrilo.
    </dif>
    <trd lng="ca">dextrogir</trd>
    <trd lng="de">rechtsdrehend</trd>
    <trd lng="en">clockwise</trd>
    <trdgrp lng="fr">
      <trd>dextrorsum</trd>,
      <trd>dextrogyre</trd>,
      <trd>direct<klr tip="ind"> (en p. du sens de
      rotation)</klr></trd>,
      <trd>dans le <ind>sens</ind> des aiguilles d'une montre</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>jobbos</trd>,
      <trd>jobb ir&aacute;ny&uacute;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>dextr&oacute;giro</trd>,
      <trd>horario</trd>
    </trdgrp>
    <trd lng="id">searah jarum <ind>jam</ind></trd>
    <trd lng="nl">rechtsdraaiend</trd>
    <trd lng="pl">prawoskr&eogonek;tny</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_p;&c_o; &c_ch;&c_a;&c_s;&c_o;&c_v;&c_o;&c_j;
      &c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_v;&c_i;&c_n;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>
    </trdgrp>
    <trd lng="zh">&#x53F3;&#x65CB; <pr>y&#xF2;uxu&#xE1;n</pr></trd>
</snc>
  <snc mrk="dekstr.0uma.bazo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="baz.0o.vektoraspaco">bazo</ref> de
      <ref cel="orient.0i.spacon">orientita</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
      Havanta pozitivan <ref cel="orient.0igxo.bazo">orienti&gcirc;on
      </ref>.
    </dif>
    <trd lng="ca">amb orientaci&oacute; positiva</trd>
    <trdgrp lng="de">
      <trd>Rechts-<klr tip="amb">(system)</klr></trd>,
      <trd>positiv orientiert<klr tip="ind"> (-e Basis)</klr></trd>
    </trdgrp>
    <trd lng="en">positively oriented<klr tip="ind"> (basis)</klr></trd>
    <trd lng="fr"><klr tip="amb">(base) </klr>directe</trd>
    <trd lng="pl"><klr tip="amb">(baza) </klr>prawoskr&eogonek;tna</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_a;&c_v;&c_y;&c_j; &c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;<klr tip="ind">
      (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>,
      <trd>&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o; &c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">pravoto&ccaron;iv&yacute; <klr>(&scaron;roub| vrtule| schodi&scaron;t&ecaron;)</klr></trd>
  <trd lng="he">&yod;&mem;&nun;&yod;</trd>
  <trdgrp lng="ja">
    <trd>&#x53F3;&#x5DFB;&#x304D;&#x306E;  <pr>&#x307F;&#x304E;&#x307E;&#x304D;&#x306E;</pr></trd>,
    <trd>&#x53F3;&#x56DE;&#x308A;&#x306E;
    <pr>&#x307F;&#x304E;&#x307E;&#x308F;&#x308A;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="sk">pravoto&ccaron;iv&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_k;&c_i;&c_j; &c_r;&c_u;&c_h;&c_a;&c_jeu;&c_t;&c_mol;&c_s;&c_ja; &c_z;&c_l;&c_ib;&c_v;&c_a; &c_n;&c_a;&c_p;&c_r;&c_a;&c_v;&c_o;</trd>,
    <trd>&c_z;&c_a; &c_g;&c_o;&c_d;&c_i;&c_n;&c_n;&c_i;&c_k;&c_o;&c_v;&c_o;&c_ju; &c_s;&c_t;&c_r;&c_ib;&c_l;&c_k;&c_o;&c_ju;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_ja;&c_k;&c_i;&c_j; &c_v;&#8217;&c_jeu;&c_t;&c_mol;&c_s;&c_ja; &c_p;&c_r;&c_a;&c_v;&c_o;&c_r;&c_u;&c_ch;</trd>
  </trdgrp>
</drv>

<drv mrk="dekstr.mal0uma">
  <kap>mal<tld/>uma</kap>
  <snc mrk="dekstr.mal0uma.KOMUNE">
    <dif>
      en direkto mal<tld/>a; mal<tld/>amana; mal<tld/>apartia::
      <ekz>
        oni rimarkas du malsamajn surskribojn, en kipra silabaro, amba&ubreve; mal<tld/>umaj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="https://www.monato.be/2014/011357.php">Kio estas la kipra
          silabaro?</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi skribis tre politikan, sufi&ccirc;e unu-flankan mal<tld/>uman tekston
        <fnt>
          <bib>Kon11</bib>
          <lok>Reagoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="dekstr.mal0uma.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Turni&gcirc;anta a&ubreve; volvi&gcirc;anta kontra&ubreve; la
      direkto de horlo&gcirc;a montrilo.
    </dif>
    <trd lng="ca">levogir</trd>
    <trd lng="en">counterclockwise</trd>
    <trdgrp lng="fr">
      <trd>indirect<klr tip="ind"> (en p. du sens de rotation)</klr>
      </trd>,
      <trd>l&eacute;vogyre</trd>,
      <trd>r&eacute;trograde<klr tip="ind"> (en p. du sens de
      rotation)</klr></trd>,
      <trd>dans le <ind>sens</ind> contraire des aiguilles d'une
      montre</trd>,
      <trd>sinistrogyre</trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_p;&c_r;&c_o;&c_t;&c_i;&c_v; &c_ch;&c_a;&c_s;&c_o;&c_v;&c_o;&c_j;
      &c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_i;</trd>,
      <trd>&c_l;&c_je;&c_v;&c_o;&c_v;&c_i;&c_n;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>lev&oacute;giro</trd>,
      <trd>antihorario</trd>
    </trdgrp>
    <trd lng="id">berlawanan jarum <ind>jam</ind></trd>
    <trd lng="pl">lewoskr&eogonek;tny</trd>
  </snc>
  <snc mrk="dekstr.mal0uma.bazo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="baz.0o.vektoraspaco">bazo</ref> de
      <ref cel="orient.0i.spacon">orientita</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
      Havanta negativan <ref cel="orient.0igxo.bazo">orienti&gcirc;on
      </ref>.
    </dif>
    <trd lng="ca">amb orientaci&oacute; negativa</trd>
    <trdgrp lng="de">
      <trd>Links-<klr tip="amb">(system)</klr></trd>,
      <trd>negativ orientiert<klr tip="ind"> (-e Basis)</klr></trd>
    </trdgrp>
    <trd lng="en">negatively oriented<klr tip="ind"> (basis)</klr></trd>
    <trd lng="fr"><klr tip="ind">(base) </klr>r&eacute;trograde</trd>
    <trd lng="pl"><klr tip="amb">(baza) </klr>lewoskr&eogonek;tna</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_je;&c_v;&c_y;&c_j; &c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j; <klr tip="ind">
      (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>,
      <trd>&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o; &c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">levoto&ccaron;iv&yacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x5DE6;&#x5DFB;&#x304D;&#x306E;  <pr>&#12402;&#12384;&#x308A;&#x307E;&#x304D;&#x306E;</pr></trd>,
    <trd>&#x5DE6;&#x56DE;&#x308A;&#x306E;  <pr>&#12402;&#12384;&#x308A;&#x307E;&#x308F;&#x308A;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="sk">&lcaron;avoto&ccaron;iv&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_k;&c_i;&c_j; &c_r;&c_u;&c_h;&c_a;&c_jeu;&c_t;&c_mol;&c_s;&c_ja; &c_s;&c_p;&c_r;&c_a;&c_v;&c_a; &c_n;&c_a;&c_l;&c_ib;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i; &c_g;&c_o;&c_d;&c_i;&c_n;&c_n;&c_i;&c_k;&c_o;&c_v;&c_o;&c_ji; &c_s;&c_t;&c_r;&c_ib;&c_l;&c_k;&c_i;</trd>,
    <trd>&c_l;&c_ib;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_ja;&c_k;&c_i;&c_j; &c_v;&#8217;&c_jeu;&c_t;&c_mol;&c_s;&c_ja; &c_s;&c_p;&c_r;&c_a;&c_v;&c_a; &c_n;&c_a;&c_l;&c_ib;&c_v;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_t;&c_i; &c_g;&c_o;&c_d;&c_i;&c_n;&c_n;&c_i;&c_k;&c_o;&c_v;&c_o;&c_ji; &c_s;&c_t;&c_r;&c_ib;&c_l;&c_k;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="dekstr.0ede">
  <kap><tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="dekstr.0ede.KOMUNE">
    <dif>
      <ref tip="super" cel="apud.0">Apud</ref>, &ccirc;e la <tld/>a
      flanko de:
      <ekz>
        la Eternulo diris al mia sinjoro: sidu <tld/>e de Mi
        <fnt><bib>MT</bib>, <lok>&Psa; 110:1</lok></fnt>;
      </ekz>
      <ekz>
        Ido kaj Anno-Doroteo iris <tld/>e kaj mal<tld/>e de li
        <fnt>
          <bib>Fab4</bib>
          <lok>La vento rakontas pri Valdemaro Doe kaj pri liaj filinoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e de la &scirc;oseo kaj pli profunde aperis la akvovasto de Lupolago
        <fnt>
          <bib>ViV</bib>
          <lok>III.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_a;&c_v;&c_a; &c_a;&c_d;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_a;&c_r;&c_u;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>al costat <klr>[dret]</klr> de</trd>,
    <trd>a la dreta de</trd>
  </trdgrp>
  <trd lng="cs">napravo od</trd>
  <trd lng="de"><ind>rechts</ind> von</trd>
  <trd lng="en">to the right of</trd>
  <trd lng="fr">&agrave; droite de</trd>
  <trd lng="id">di sebelah <ind>kanan</ind></trd>
  <trdgrp lng="pl">
    <trd>po <ind>prawej</ind></trd>,
    <trd>z <ind>prawej</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>napravo od</trd>,
    <trd>vpravo</trd>
  </trdgrp>
</drv>

<drv mrk="dekstr.mal0ede">
  <kap>mal<tld/>e de</kap>
  <gra><vspec>prepozicia&jcirc;o</vspec></gra>
  <snc mrk="dekstr.mal0ede.KOMUNE">
    <dif>
      <ref tip="super" cel="apud.0">Apud</ref>, &ccirc;e la mal<tld/>a
      flanko de:
      <ekz>
        je unu metro mal<tld/>e de tie <klr>[&#8230;]</klr> riveli&gcirc;is bela, identigebla
        &scirc;u-spuro
        <fnt>
          <bib>CKv</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_mol;&c_l;&c_je;&c_v;&c_a; &c_a;&c_d;</trd>,
    <trd>&c_l;&c_je;&c_v;&c_a;&c_r;&c_u;&c_ch;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>al costat <klr>[esquerre]</klr> de</trd>,
    <trd>a l'esquerra de</trd>
  </trdgrp>
  <trd lng="de">links von</trd>
  <trd lng="en">to the left of</trd>
  <trd lng="fr">&agrave; gauche de</trd>
  <trd lng="id">di sebelah <ind>kiri</ind></trd>
  <trdgrp lng="pl">
    <trd>po <ind>lewej</ind></trd>,
    <trd>z <ind>lewej</ind></trd>
  </trdgrp>
</drv>

