<drv mrk="dekstr.0uma">
  <kap><tld/>uma</kap>
  <snc mrk="dekstr.0uma.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Turni&gcirc;anta a&ubreve; volvi&gcirc;anta la&ubreve; la direkto
      de horlo&gcirc;a montrilo.
    </dif>
    <trd lng="ca">dextrogir</trd>
    <trd lng="de">rechtsdrehend</trd>
    <trd lng="en">clockwise</trd>
    <trdgrp lng="fr">
      <trd>dextrorsum</trd>,
      <trd>dextrogyre</trd>,
      <trd>direct<klr tip="ind"> (en p. du sens de
      rotation)</klr></trd>,
      <trd>dans le <ind>sens</ind> des aiguilles d'une montre</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>jobbos</trd>,
      <trd>jobb ir&aacute;ny&uacute;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>dextr&oacute;giro</trd>,
      <trd>horario</trd>
    </trdgrp>
    <trd lng="nl">rechtsdraaiend</trd>
    <trd lng="pl">prawoskr&eogonek;tny</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_p;&c_o; &c_ch;&c_a;&c_s;&c_o;&c_v;&c_o;&c_j;
      &c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_v;&c_i;&c_n;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="dekstr.0uma.bazo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="baz.0o.vektoraspaco">bazo</ref> de
      <ref cel="orient.0i.spacon">orientita</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
      Havanta pozitivan <ref cel="orient.0igxo.bazo">orienti&gcirc;on
      </ref>.
    </dif>
    <trd lng="ca">amb orientaci&oacute; positiva</trd>
    <trdgrp lng="de">
      <trd>Rechts-<klr tip="amb">(system)</klr></trd>,
      <trd>positiv orientiert<klr tip="ind"> (-e Basis)</klr></trd>
    </trdgrp>
    <trd lng="en">positively oriented<klr tip="ind"> (basis)</klr></trd>
    <trd lng="fr"><klr tip="amb">(base) </klr>directe</trd>
    <trd lng="pl"><klr tip="amb">(baza) </klr>prawoskr&eogonek;tna</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_a;&c_v;&c_y;&c_j; &c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j;<klr tip="ind">
      (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>,
      <trd>&c_p;&c_o;&c_l;&c_o;&c_zh;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o; &c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">pravoto&ccaron;iv&yacute; <klr>(&scaron;roub| vrtule| schodi&scaron;t&ecaron;)</klr></trd>
  <trd lng="he">&#x5D9;&#x5DE;&#x5E0;&#x5D9;</trd>
  <trd lng="sk">pravoto&ccaron;iv&yacute;</trd>
</drv>

<drv mrk="dekstr.mal0uma">
  <kap>mal<tld/>uma</kap>
  <snc mrk="dekstr.mal0uma.SCI">
    <uzo tip="fak">SCI</uzo>
    <dif>
      Turni&gcirc;anta a&ubreve; volvi&gcirc;anta kontra&ubreve; la
      direkto de horlo&gcirc;a montrilo.
    </dif>
    <trd lng="ca">levogir</trd>
    <trd lng="en">counterclockwise</trd>
    <trdgrp lng="fr">
      <trd>indirect<klr tip="ind"> (en p. du sens de rotation)</klr>
      </trd>,
      <trd>l&eacute;vogyre</trd>,
      <trd>r&eacute;trograde<klr tip="ind"> (en p. du sens de
      rotation)</klr></trd>,
      <trd>dans le <ind>sens</ind> contraire des aiguilles d'une
      montre</trd>,
      <trd>sinistrogyre</trd>,
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_d;&c_v;&c_i;&c_zh;&c_u;&c_shch;&c_i;&c_j;&c_s;&c_ja; &c_p;&c_r;&c_o;&c_t;&c_i;&c_v; &c_ch;&c_a;&c_s;&c_o;&c_v;&c_o;&c_j;
      &c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_i;</trd>,
      <trd>&c_l;&c_je;&c_v;&c_o;&c_v;&c_i;&c_n;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>lev&oacute;giro</trd>,
      <trd>antihorario</trd>
    </trdgrp>
    <trd lng="pl">lewoskr&eogonek;tny</trd>
  </snc>
  <snc mrk="dekstr.mal0uma.bazo">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatVort</bib></fnt>
    <dif>
      <klr>(p.p. <ref cel="baz.0o.vektoraspaco">bazo</ref> de
      <ref cel="orient.0i.spacon">orientita</ref>
      <ref cel="spac.vektora0o">vektora spaco</ref>)</klr>
      Havanta negativan <ref cel="orient.0igxo.bazo">orienti&gcirc;on
      </ref>.
    </dif>
    <trd lng="ca">amb orientaci&oacute; negativa</trd>
    <trdgrp lng="de">
      <trd>Links-<klr tip="amb">(system)</klr></trd>,
      <trd>negativ orientiert<klr tip="ind"> (-e Basis)</klr></trd>
    </trdgrp>
    <trd lng="en">negatively oriented<klr tip="ind"> (basis)</klr></trd>
    <trd lng="fr"><klr tip="ind">(base) </klr>r&eacute;trograde</trd>
    <trd lng="pl"><klr tip="amb">(baza) </klr>lewoskr&eogonek;tna</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_je;&c_v;&c_y;&c_j; &c_u;&c_p;&c_o;&c_r;&c_ja;&c_d;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_y;&c_j; <klr tip="ind">
      (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>,
      <trd>&c_o;&c_t;&c_r;&c_i;&c_c;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o; &c_o;&c_r;&c_i;&c_je;&c_n;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_n;&c_y;&c_j;<klr tip="ind"> (&c_b;&c_a;&c_z;&c_i;&c_s;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">levoto&ccaron;iv&yacute;</trd>
  <trd lng="sk">&lcaron;avoto&ccaron;iv&yacute;</trd>
</drv>
