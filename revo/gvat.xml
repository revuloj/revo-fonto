<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gvat.xml,v 1.26 2023/11/11 11:36:37 revo Exp $">
<kap>
  <rad>gvat</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="gvat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gvat.0i.sxtele">
    <dif>
      Ka&scirc;e, &scirc;telume <ref tip="super" cel="observ.0i.rigardi">observi</ref>,
      atendi:
      <ekz>
        trans la kurteno de la kabaredo li <tld/>is
        la eliradon de la kudristinoj;
      </ekz>
      <ekz>
        rabobirdoj elektas la altan konstrua&jcirc;on kiel <tld/>ejon
        <fnt>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2000/007052.php">Verda
          nature, science, socie</url></vrk>
          <bib>Monato</bib>
          <lok>2000/08, p. 8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> emas <tld/>i detalojn de &scirc;ia interesa vivo <klr>[&gcirc;uante
        la]</klr> a&ubreve;tobiografian romanon
        <fnt>
          <bib>Monato</bib>
          <aut>Reinaldo Marcelo Ferreira</aut>
          <vrk><url ref="&Monato;/2005/008629.php">Ne nur intimaj temoj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni instalis &ccirc;ie en grandaj urboj <tld/>-kameraojn
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2007/009127.php">Sukcesa terorismo</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ie <tld/>is timo kaj prema senespero
        <fnt>
          <aut>Julian Modest</aut>
            <vrk><url ref="&Monato;/2001/007140.html">Vetiha</url></vrk>
            <bib>Monato</bib>
            <lok>2001/02, p. 26</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="embusk.0i">embuski</ref>,
       <ref cel="esplor.0i">esplori</ref>,
       <ref cel="sond.0i">sondi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_s;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_a;&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>espiar</trd>,
      <trd>ullar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>heimlich <ind>beobachten</ind></trd>,
      <trd>lauern</trd>,
      <trd>belauern</trd>,
      <trd>spannen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>acechar</trd>,
      <trd>espiar</trd>
    </trdgrp> 
    <trdgrp lng="en">
      <trd>spy on</trd>,
      <trd>stalk</trd>,,
      <trd>surveil</trd>
    </trdgrp>    
    <trd lng="hu">les</trd>
    <trd lng="nl">bespieden</trd>
    <trd lng="pt">espiar</trd>
    <trdgrp lng="fr">
      <trd>guetter</trd>,
      <trd>&eacute;pier</trd>
    </trdgrp>
  </snc>
  <snc mrk="gvat.0i.dejxore">
    <dif>
      De&jcirc;ore observi:
      <ekz>
        fu&scirc;ita polica <tld/>ado ekster la apartamento de la murdito
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2005/008763.php">Faktoj senfaktaj kaj murdo en la
          metroo</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>sekcio de la brita polico
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009525.php">Fratego fiaskas</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la registaro <klr>[&#x2026;]</klr> <tld/>as dum pli ol unu jaro la akuzitojn kaj jam
        posedas detalojn pri la komploto
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2006/009061.php">Faktoj kaj famoj</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="gard.0i.protekti">gardi</ref>,
       <ref cel="spion.0i">spioni</ref>,
       <ref cel="enket.0i">enketi</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>sotjar</trd>,
      <trd>aguaitar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd><ind>Wache</ind> schieben</trd>,
      <trd>wachen</trd>,
      <trd>bewachen</trd>,
      <trd>observieren</trd>,
      <trd>aufkl&auml;ren<klr> (observieren)</klr></trd>
    </trdgrp>
    <trd lng="es">vigilar</trd> 
    <trdgrp lng="en">
      <trd>keep watch</trd>,    
      <trd>guard</trd>,
       <trd>sentinel</trd>
    </trdgrp>
    <trd lng="fr">faire le <ind>guet</ind></trd>
    <trdgrp lng="hu">
      <trd>figyel<klr tip="ind"> (k&eacute;mlel)</klr></trd>,
      <trd>k&eacute;mlel</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bewaken</trd>,
      <trd>uitkijken</trd>
    </trdgrp>
    <trdgrp lng="pt">
     <trd>espionar</trd>, 
     <trd>tocaiar</trd> 
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>tajn&ecaron; pozorovat</trd>,
    <trd>&ccaron;&iacute;hat</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x69D8;&#x5B50;&#x3092;&#x3046;&#x304B;&#x304C;&#x3046;  <pr>&#x3088;&#x3046;&#x3059;&#x3092;&#x3046;&#x304B;&#x304C;&#x3046;</pr></trd>,
    <trd>&#x898B;&#x5F35;&#x308B;  <pr>&#x307F;&#x306F;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_l;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>striehnu&tcaron;</trd>,
    <trd>&scaron;pehova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_s;&c_t;&c_je;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_s;&c_t;&c_je;&c_r;&c_ib;&c_g;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ch;&c_a;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="gvat.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="gvat.0isto.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Tiu, kiu de&jcirc;ore <tld/>as kaj alarmas se aperas dan&gcirc;ero:
      <ekz>
        homoj en cirklo tenas enmane grandegan felan tapi&scirc;on kaj 
        saltigas tre alte <tld/>iston, kiu observas la venon de balenoj 
        &ccirc;e la horizonto
        <fnt>
          <aut>F. Cuny, trad. G. Martin</aut>,
          <vrk><url ref="http://tirkesto.skyrock.com/1612230846-Ne-o-kaj-glacio-tria-parto.html">
          Ne&gcirc;o kaj glacio</url></vrk>,
          <lok>2008-05-31</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; la &ccirc;ef<tld/>isto brita mem konfesas, ke tia elspionado estas kompleta fiasko
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2008/009525.php">Fratego fiaskas</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="skolt.0o.MIL">skolto<sncref/></ref>,
       <ref cel="gard.0isto">gardisto</ref>,
       <ref cel="gvardi.0o.MIL_gardistaro">gvardio<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_g;&c_l;&c_ja;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_n;&c_a;&c_z;&c_o;&c_r;&c_c;&c_a;</trd>,
    <trd>&c_v;&c_a;&c_r;&c_t;&c_a;&c_w;&c_n;&c_ib;&c_k;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_zh;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Beobachter</trd>,
    <trd>Beschatter</trd>,
    <trd>Observierender</trd>,
    <trd>Observant</trd>,
    <trd>W&auml;chter</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>lookout</trd>,
    <trd>sentinel</trd>,
    <trd>guard</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>homme de guet</trd>,
    <trd>vigie</trd>
  </trdgrp>
  <trd lng="ja">&#x76E3;&#x8996;&#x5175;  <pr>&#x304B;&#x3093;&#x3057;&#x3078;&#x3044;</pr></trd>
  <trdgrp lng="zh">
    <trd>&#x5B88;&#x62A4;&#x8005; <pr>sh&#x1D2;uh&#xF9;zh&#x11B;</pr></trd>,
    <trd>&#x76D1; <pr>ji&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
