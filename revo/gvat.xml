<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gvat</rad>/i
</kap>
<drv mrk="gvat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc num="1">
    <dif>
      Ka&scirc;e, &scirc;telume observi, atendi:
      <ekz>
        trans la kurteno de la kabareto li <tld/>is
        la eliradon de la kudristinoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
     <trd>heimlich beobachten</trd>,
     <trd>lauern</trd>,
     <trd>belauern</trd>,
     <trd>spannen</trd></trdgrp>
     <trd lng="hu">les</trd>
     <trd lng="nl">bespieden</trd>
    <trdgrp lng="fr">
      <trd>guetter</trd>,
      <trd>&eacute;pier</trd></trdgrp> 
  </snc>
  <snc num="2">
    <dif>
      De&jcirc;ore observi.
    </dif>
<trdgrp lng="de">
     <trd>Wache schieben</trd>,
     <trd>wachen</trd>,
     <trd>bewachen</trd></trdgrp>
  <trd lng="fr">faire le <ind>guet</ind></trd>
  <trdgrp lng="hu">
    <trd>figyel<klr tip="ind"> (k&eacute;mlel)</klr></trd>,
    <trd>k&eacute;mlel</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>bewaken</trd>,
    <trd>uitkijken</trd>
  </trdgrp>
  </snc>
    <trd lng="ru">&c_s;&c_l;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>

