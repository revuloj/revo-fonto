<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gvat</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="gvat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gvat.0i.sxtele">
    <dif>
      Ka&scirc;e, &scirc;telume observi, atendi:
      <ekz>
        trans la kurteno de la kabareto li <tld/>is
        la eliradon de la kudristinoj.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_s;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_d;&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_d;&c_a;&c_p;&c_ib;&c_l;&c_mol;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>espiar</trd>,
      <trd>ullar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>heimlich beobachten</trd>,
      <trd>lauern</trd>,
      <trd>belauern</trd>,
      <trd>spannen</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>acechar</trd>,
      <trd>espiar</trd>
    </trdgrp> 
    <trdgrp lng="en">
      <trd>spy on</trd>,
      <trd>stalk</trd>,
    </trdgrp>    
    <trd lng="hu">les</trd>
    <trd lng="nl">bespieden</trd>
    <trd lng="pt">espiar</trd>
    <trdgrp lng="fr">
      <trd>guetter</trd>,
      <trd>&eacute;pier</trd>
    </trdgrp>
  </snc>
  <snc mrk="gvat.0i.dejxore">
    <dif>
      De&jcirc;ore observi.
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_a;&c_ch;&c_y;&c_c;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="ca">
      <trd>sotjar</trd>,
      <trd>aguaitar</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Wache schieben</trd>,
      <trd>wachen</trd>,
      <trd>bewachen</trd>
    </trdgrp>
    <trd lng="es">vigilar</trd> 
    <trdgrp lng="en">
      <trd>keep watch</trd>,    
      <trd>guard</trd>,
       <trd>sentinel</trd>
    </trdgrp>
    <trd lng="fr">faire le <ind>guet</ind></trd>
    <trdgrp lng="hu">
      <trd>figyel<klr tip="ind"> (k&eacute;mlel)</klr></trd>,
      <trd>k&eacute;mlel</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bewaken</trd>,
      <trd>uitkijken</trd>
    </trdgrp>
    <trdgrp lng="pt">
     <trd>espionar</trd>, 
     <trd>tocaiar</trd> 
    </trdgrp>
  </snc>
  <trd lng="ru">&c_s;&c_l;&c_je;&c_d;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>striehnuť</trd>,
    <trd>špehovať</trd>
  </trdgrp>
</drv>
</art>
