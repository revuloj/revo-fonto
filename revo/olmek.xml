<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>olmek</rad>/o
</kap>
<drv mrk="olmek.0o">
  <kap><tld/>o</kap>
  <snc mrk="olmek.0o.ETN">
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="super" cel="indian.0o.ETN">Indiano</ref> el nacio, kiu
      vivis &ccirc;irka&ubreve; la meksika golfo inter 1200 a.K. kaj 400 a.K. proksimume:
      <ekz>
        la <tld/>oj komencis la kultivadon de la kakaujo, mil jarojn anta&ubreve;
        nia erao
        <fnt>
        <bib>Viki</bib>,
        <lok>&ccirc;okolado, 2005-08-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">Olm&egrave;que<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="pt">olmeca</trd>
</drv>

<drv mrk="olmek.0a">
  <kap><tld/>a</kap>
  <snc mrk="olmek.0a.ETN">
    <dif>
      Rilata al la <tld/>oj.
    </dif>
  </snc>
  <trd lng="fr">olm&egrave;que<klr tip="amb"> (adj.)</klr></trd>
</drv>
</art>

