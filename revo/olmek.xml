<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: olmek.xml,v 1.9 2024/01/30 16:09:26 revo Exp $">
<kap>
  <rad>olmek</rad>/o
</kap>
<drv mrk="olmek.0o">
  <kap><tld/>o</kap>
  <snc mrk="olmek.0o.ETN">
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="super" cel="indian.0o.ETN">Indiano</ref> el nacio, kiu
      vivis &ccirc;irka&ubreve; la meksika golfo inter 1200 a.K. kaj 400 a.K.
      proksimume:
      <ekz>
        la <tld/>oj komencis la kultivadon de la kakaujo, mil jarojn
        anta&ubreve;
        nia erao
        <fnt>
        <bib>Viki</bib>,
        <lok>&ccirc;okolado, 2005-08-12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_m;&c_e;&c_k;</trd>
  <trd lng="de">Olmeke</trd>
  <trd lng="en">Olmec</trd>
  <trd lng="fr">Olm&egrave;que<klr tip="amb"> (subst.)</klr></trd>
  <trd lng="hu">olm&eacute;k</trd>
  <trd lng="nl">Olmeek</trd>
  <trd lng="pl">Olmek</trd>
  <trd lng="pt">olmeca</trd>
  <trdgrp lng="zh">
    <trd>&#x5965;&#x5C14;&#x6885;&#x514B;&#x6587;&#x660E; <pr>&#xE0;o&#x11B;rm&#xE9;ik&#xE8;w&#xE9;nm&#xED;ng</pr></trd>,
    <trd>&#x5967;&#x723E;&#x6885;&#x514B;&#x6587;&#x660E; <pr>&#xE0;o&#x11B;rm&#xE9;ik&#xE8;w&#xE9;nm&#xED;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="olmek.0a">
  <kap><tld/>a</kap>
  <snc mrk="olmek.0a.ETN">
    <dif>
      Rilata al la <tld/>oj.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_l;&c_mol;&c_m;&c_e;&c_c;&c_k;&c_ib;</trd>
  <trd lng="de">olmekisch</trd>
  <trd lng="en">Olmec</trd>
  <trd lng="fr">olm&egrave;que<klr tip="amb"> (adj.)</klr></trd>
  <trd lng="hu">olm&eacute;k</trd>
  <trd lng="nl">Olmeeks</trd>
  <trd lng="pl">olmecki</trd>
</drv>
</art>

