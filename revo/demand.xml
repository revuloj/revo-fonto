  <trd lng="ru">&c_v;&c_o;&c_p;&c_r;&c_o;&c_s;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_z;&c_n;&c_a;&c_k;</trd>
  <trd lng="sk">ot&aacute;znik</trd>
</drv>
