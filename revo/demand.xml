  <trd lng="fr">point d'interrogation</trd>
  <trd lng="he">&samekh;&yod;&mem;&fnun; &shin;&alef;&lamed;&he;</trd>
  <trd lng="hu">k&eacute;rd&odblac;jel</trd>
  <trd lng="nl">vraagteken</trd>
  <trdgrp lng="pl">
