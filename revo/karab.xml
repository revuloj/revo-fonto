<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karab.xml,v 1.14 2022/03/21 17:51:57 revo Exp $">
<kap>
  <rad>karabed</rad>/oj
</kap>
<drv mrk="karab.0oj">
  <kap>
    <tld/>oj
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:zoologiaj_familioj">Familio</ref> el la ordo
      <ref tip="malprt" cel="koleop.0oj">koleopteroj</ref>
      (<trd lng="la">Carabidae</trd>), rapidkuraj:
      <ekz>
        la ora kur-skarabo estas specio de genro karabo el la familio de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=46662">Ora kur-skarabo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_u;&c_zh;&c_a;&c_l;&c_ib; <klr tip="amb">(&c_s;&c_ja;&c_m;'&c_ja;)</klr></trd>
  <trd lng="ca">car&agrave;bids</trd>
  <trd lng="de">Laufk&auml;fer<klr tip="ind"> (Familie)</klr></trd>
  <trd lng="fr">carabid&eacute;s</trd>
  <trd lng="hu">futrinkaf&eacute;l&eacute;k</trd>
  <trd lng="nl">loopkevers</trd>
  <trd lng="pl">biegaczowate</trd>     
  <trd lng="ro">carabidae</trd>
  <trd lng="ru">&c_zh;&c_u;&c_zh;&c_je;&c_l;&c_i;&c_c;&c_y; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x442;&#x443;&#x440;&#x443;&#x43D;&#x43E;&#x432;&#x456;</trd>,
    <trd>&#x436;&#x443;&#x436;&#x435;&#x43B;&#x438;&#x446;&#x435;&#x432;&#x456;</trd>
  </trdgrp>
</drv>
</art>
