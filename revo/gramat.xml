<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gramatik</rad>/o
</kap>
<drv mrk="gramat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gramat.0o.SCI">
    <uzo tip="fak">LIN</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Scienco pri la lingvaj reguloj.
    </dif>
    <trd lng="es">gram&aacute;tica</trd>
    <trd lng="hu">nyelvtan</trd>
    <trd lng="nl">grammatica</trd>
    <trd lng="vo">glamat</trd>
  </snc>
  <snc mrk="gramat.0o.libro">
    <dif>
      Libro klariganta tiujn regulojn:
      <ekz>
        a&ccirc;eti latinan, esperantan <tld/>on.
      </ekz>
    </dif>
    <trd lng="hu">nyelvtank&ouml;nyv</trd>
    <trdgrp lng="nl">
      <trd>grammatica</trd>,
      <trd>grammaticaboek</trd>
    </trdgrp>
    <trd lng="vo">glamatabuk</trd>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="cs">mluvnice</trd>
  <trdgrp lng="de">
    <trd>Grammatik</trd>,
    <trd>Sprachlehre</trd>
  </trdgrp>
  <trd lng="fr">grammaire</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sv">grammatik</trd>
</drv>

<drv mrk="gramat.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Rilata al lingvaj reguloj, al la aran&gcirc;o de vortoj en parolo:
      <ekz>
        oni povas anka&ubreve; preni la tutan vorton, t. e. la radikon kune kun
        &gcirc;ia <tld/>a fini&gcirc;o
        <fnt><bib>F</bib>, <lok>&FE; 27</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">grammatical</trd>
</drv>

<drv mrk="gramat.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <uzo tip="fak">LIN</uzo>
    <dif>
      Vorto precipe helpanta en la rilatigo de aliaj vortoj, en la
      konstruo de frazoj, ekzemple prepozicio, konjunkcio:
      <ekz>
        fantaziaj <tld/>iloj, kiel <ctl>a&ubreve;tem</ctl>, <ctl>dos</ctl> a&ubreve;
        <ctl>ud</ctl>
        <fnt><aut>Roland Levreaud, Gaston Waringhien</aut>,
        <bib>SPIV</bib>, <lok>anta&ubreve;parolo, p. 4a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">mot grammatical</trd>
</drv>

<drv mrk="gramat.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Spertulo en <tld/>a scienco.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_m;&c_a;&c_t;&c_y;&c_k;</trd>
  <trd lng="de">Grammatiker</trd>
  <trd lng="fr">grammairien</trd>
  <trdgrp lng="hu">
    <trd>grammatikus</trd>,
    <trd>nyelvtan&iacute;r&oacute;</trd>
  </trdgrp>
  <trd lng="nl">grammaticus</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;&c_t;&c_i;&c_k;</trd>
</drv>

<drv mrk="gramat.gxenerala0o">
  <kap>&gcirc;enerala <tld/>o</kap>
  <snc>
  <uzo tip="fak">LIN</uzo>
    <dif>
      Scienco pri la reguloj komunaj al &ccirc;iuj lingvoj.
    </dif>
  </snc>
  <trd lng="fr">grammaire g&eacute;n&eacute;rale</trd>
</drv>

<drv mrk="gramat.kompara0o">
  <kap>kompara <tld/>o</kap>
  <snc>
  <uzo tip="fak">LIN</uzo>
    <dif>
      Scienco pri la samecoj kaj malsamecoj de pluraj lingvoj,        devenintaj el unu sama, kiujn oni komparas kaj klarigas unu        per alia.
    </dif>
  </snc>
  <trd lng="fr">grammaire compar&eacute;e</trd>
</drv>

<drv mrk="gramat.historia0o">
  <kap>historia <tld/>o</kap>
  <snc>
  <uzo tip="fak">LIN</uzo>
    <dif>
      Scienco studanta la historion de la &scirc;an&gcirc;oj en lingvaj reguloj.
    </dif>
  </snc>
  <trd lng="fr">grammaire historique</trd>
</drv>

<drv mrk="gramat.priskriba0o">
  <kap>priskriba <tld/>o</kap>
  <snc>
  <uzo tip="fak">LIN</uzo>
    <dif>
      Scienco priskribanta nur la momentan staton de lingvaj reguloj.
    </dif>
  </snc>
  <trd lng="fr">grammaire descriptive</trd>
</drv>
</art>
