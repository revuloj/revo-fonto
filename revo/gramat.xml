<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>gramatik</rad>/o</kap>
<drv mrk="gramat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">LIN</uzo>
    <dif>
      Scienco pri la lingvaj reguloj:
      <ekz>
        &gcirc;enerala <tld/>o
        <klr>(scienco pri la reguloj komunaj al &ccirc;iuj lingvoj)</klr>;
      </ekz>
      <ekz>
        kompara <tld/>o
        <klr>(scienco pri la samecoj kaj malsamecoj de pluraj lingvoj,
        devenintaj el unu sama, kiujn oni komparas kaj klarigas unu
        per alia)</klr>;
      </ekz>
      <ekz>
        historia <tld/>o
        <klr>(studanta la historion de la formi&gcirc;o de la reguloj)</klr>;
      </ekz>
      <ekz>
        priskriba <tld/>o
        <klr>(priskribanta nur la nunan staton de la reguloj)</klr>.
      </ekz>
    </dif>
    <trd lng="nl">grammatica</trd>
  </snc>
  <snc>
    <dif>
      Libro klariganta tiujn regulojn:
      <ekz>
        a&ccirc;eti latinan, esperantan <tld/>on.
      </ekz>
    </dif>
    <trdgrp lng="nl">
      <trd>grammatica</trd>,
      <trd>grammaticaboek</trd>
    </trdgrp>
  </snc>
  <trd lng="cs">mluvnice</trd>
  <trdgrp lng="de">
    <trd>Grammatik</trd>,
    <trd>Sprachlehre</trd>
  </trdgrp>
  <trd lng="fr">grammaire</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sv">grammatik</trd>
</drv>

<drv mrk="gramat.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Spertulo en <tld/>a scienco.
    </dif>
  </snc>
  <trd lng="de">Grammatiker</trd>
  <trd lng="fr">grammairien</trd>
  <trd lng="nl">grammaticus</trd>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;&c_t;&c_i;&c_k;</trd>
</drv>
</art>
