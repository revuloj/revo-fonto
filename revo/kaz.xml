<drv mrk="kaz.lok0o">
  <kap>lok<tld/>o</kap>
  <snc>
    <uzo tip="fak">GRA</uzo>
    <dif>
      En kelkaj lingvoj, <ref tip="super" cel="kaz.0o.GRA">kazo</ref>
      indikanta lokon a&ubreve; tempon:
      <ekz>
        en Esperanto la adverbaj formoj kiel <ctl>hejme</ctl>,
        <ctl>eksterlande</ctl>, <ctl>&ccirc;i-sabate</ctl> ktp povas
        funkcii simile al lok<tld/>o;
      </ekz>
      <ekz>
        konfuzi&gcirc;is la signifoj de la praa ablativo (t.e. de la deira
        kazo = de, el), de la instrumenta <tld/>o (= per) kaj de
        lok<tld/>o (= en)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=51659">Ablativo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_s;&c_n;&c_y; <ind>&c_s;&c_k;&c_l;&c_o;&c_n;</ind></trd>
  <trd lng="de">ortsanzeigender Fall</trd>
  <trd lng="en">locative case</trd>
  <trdgrp lng="hu">
    <trd>helyhat&aacute;roz&oacute;-<ind>eset</ind></trd>,
    <trd>ablativus</trd>
  </trdgrp>
  <trd lng="nl">locatief</trd>
  <trdgrp lng="pl">
    <trd>miejscownik</trd>,
    <trd>przypadek miejsca</trd>
  </trdgrp>
  <trd lng="pt">locativo</trd>
  <trd lng="ro">locativ&abreve;</trd>
  <trd lng="ru">&c_m;&c_je;&c_s;&c_t;&c_n;&c_y;&c_j; &c_p;&c_a;&c_d;&c_je;&c_zh;</trd>
  <trd lng="tr">de hali</trd>
</drv>

