<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kaz</rad>/o
</kap>
<drv mrk="kaz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kaz.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <dif>
      &Ccirc;iu el la diversaj formoj, kiujn en kelkaj lingvoj alprenas
      <ref cel="nom.0o.LIN">nomoj</ref> la&ubreve; sia gramatika
      rolo:
      <ekz>
        en Esperanto <tld/>oj ekzistas nur du: nominativo kaj
        akuzativo<fnt>Z</fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="deklin.0o">deklinacio</ref>
    <trd lng="be">&c_s;&c_k;&c_l;&c_o;&c_n;</trd>
    <trdgrp lng="de">
      <trd>Fall</trd>,
      <trd>Kasus</trd>
    </trdgrp>
    <trd lng="nl">naamval</trd>
    <trd lng="ru">&c_p;&c_a;&c_d;&c_je;&c_zh;</trd>
  </snc>

  <snc mrk="kaz.0o.okazo">
    <dif>
      <ref tip="dif" cel="okaz.0o.kazo">Okazo<sncref/></ref>:
      <ekz>
        en tiu <tld/>o mi prenas pluvombrelon;
      </ekz>
      <ekz>
        la le&gcirc;o ne anta&ubreve;vidis tiun <tld/>on.
      </ekz>
    </dif>
    <trd lng="ru">&c_s;&c_l;&c_u;&c_ch;&c_a;&c_j;</trd>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_a;&c_d;&c_a;&c_k;</trd>
  <trd lng="cs">p&rcaron;&iacute;pad</trd>
  <trd lng="de">Fall</trd>
  <trd lng="en">case</trd>
  <trd lng="es">caso</trd>
  <trd lng="nl">geval</trd>
</drv>

<drv mrk="kaz.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="kaz.0aro.GRA">
	<uzo tip="fak">GRA</uzo>
	<ref tip="dif" cel="deklin.0o">deklinacio</ref>
  </snc>
</drv>
</art>
