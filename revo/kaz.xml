  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_l;&c_o;&c_n; &c_m;&c_e;&c_t;&c_y;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_s;&c_k;&c_l;&c_o;&c_n;</ind></trd>
