<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: psik.xml,v 1.21 2020/01/09 00:10:36 revo Exp $">
<kap>
  <rad>psik</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt> 
</kap>

<drv mrk="psik.0o">
  <kap><tld/>o</kap>
  <snc mrk="psik.0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      <ref tip="dif" cel="mens.0o">Menso</ref>, precipe kiel
      studobjekto de psikologio, kiu rigardas &gcirc;in loko de
      &ccirc;iuj konsciaj kaj nekonsciaj inklinoj, komprenoj,
      emocioj, voloj...;
      <ref tip="sin" cel="psihx.p0o.animo">psi&hcirc;o</ref>:
      <ekz>
        tiuj kutimoj enradiki&gcirc;as profunde en nia <tld/>o;
      </ekz>
      <ekz>
        <tld/>aj fenomenoj.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="anim.0o">animo</ref>,
    <ref cel="spirit.0o">spirito</ref>.
  </refgrp>
  <trd lng="be">&c_p;&c_s;&c_ib;&c_h;&c_ib;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
  <trd>du&scaron;e</trd>,
  <trd>du&scaron;evno</trd>,
  <trd>psych&eacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
  	<trd>Psyche</trd>,
  	<trd>Geist</trd>
  </trdgrp>
  <trdgrp lng="en">
  	<trd>mind</trd>,
  	<trd>psyche</trd>
  </trdgrp>
  <trd lng="es">psique</trd>
  <trdgrp lng="fr">
    <trd>psych&eacute;</trd>,
    <trd>psychisme</trd>
  </trdgrp>
  <trdgrp lng="hu">
  	<trd>pszich&eacute;</trd>,
  	<trd>l&eacute;lek</trd>
  </trdgrp>
  <trdgrp lng="nl">
  	<trd>geest</trd>,
  	<trd>psyche</trd>
  </trdgrp>
  <trdgrp lng="pt">
      <trd>psique</trd>,  
      <trd>psiqu&ecirc; <klr>(evi.)</klr></trd>,
  </trdgrp>
  <trd lng="ru">&c_p;&c_s;&c_i;&c_h;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
  <trd>du&scaron;a</trd>,
  <trd>du&scaron;evno</trd>,
  <trd>psych&eacute;</trd>
  </trdgrp>
</drv>

<drv mrk="psik.0analizo">
  <kap><tld/>analizo</kap>
  <snc mrk="psik.0analizo.PSI">
    <uzo tip="fak">FIL</uzo>
    <uzo tip="fak">PSI</uzo>
    <dif>
      Psikologia klinika metodo, ellaborita de
      <ref tip="vid" cel="freuxd.0o">Fre&ubreve;do</ref>, celanta
      malkovri per diversaj procedoj kaj konsciigi la subkonsciajn
      ide-sistemojn, kies neperceptita &ccirc;eesto ka&ubreve;zas mensajn a&ubreve;
      e&ccirc; korpajn perturbojn, kaj kies origino estas la libido:
      <ekz>
        <tld/>analizo elmontris, kiom estas utile, esplori la plej sekretajn refaldojn
        de nia subkonscio, por ke ni liberi&gcirc;u de la malklaraj fortoj, kiuj nin katenas
        <fnt>
          <aut>C. Baudouin</aut>
          <vrk>La Arto de Memdisciplino: Psikagogio</vrk>
          <lok>1926</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_s;&c_ib;&c_h;&c_a;&c_a;&c_n;&c_a;&c_l;&c_ib;&c_z;</trd>
  <trd lng="cs">psychoanal&yacute;za</trd>
  <trd lng="de">Psychoanalyse</trd>
  <trd lng="en">psychoanalysis</trd>
  <trd lng="es">psicoan&aacute;lisis</trd>
  <trd lng="fr">psychanalyse</trd>
  <trdgrp lng="hu">
  	<trd>pszichoanal&iacute;zis</trd>,
  	<trd>l&eacute;lekelemz&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">psychoanalyse</trd>
  <trd lng="ru">&c_p;&c_s;&c_i;&c_h;&c_o;&c_a;&c_n;&c_a;&c_l;&c_i;&c_z;</trd>
  <trd lng="sk">psychoanal&yacute;za</trd>
</drv>
</art>
