   <trd lng="nl">hoogte</trd>
   <trdgrp lng="pl">
     <trd>wy&zdot;yna</trd>,
     <trd>wzniesienie</trd>,
     <trd>pag&oacute;rek</trd>
   </trdgrp>
   <trdgrp lng="pt">
     <trd>alto</trd>,
     <trd>mont&iacute;culo</trd>,
     <trd>colina</trd>
   </trdgrp>
   <trd lng="ro">deal</trd>
   <trdgrp lng="ru">
      <trd>&c_v;&c_o;&c_z;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
