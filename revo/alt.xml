<drv mrk="alt.0o">
   <kap><tld/>o</kap>
   <snc mrk="alt.0o.dimensio">
     <dif>
       Vertikala 
<ref tip="super" cel="dimens.0o.ampleksa">dimensio</ref> de objekto:
       <ekz>
         tricent ulnoj estu la longo de la arkeo, kvindek ulnoj 
         &gcirc;ia lar&gcirc;o, kaj tridek ulnoj &gcirc;ia <tld/>o
         <fnt><bib>MT</bib> <lok>&Gen; 6:15</lok></fnt>.
       </ekz>
     </dif>
     <trd lng="en">height</trd>
     <trdgrp lng="fr">
       <trd>hauteur<klr tip="ind"> (dimension verticale)</klr></trd>,
       <trd>taille<klr tip="ind"> (hauteur)</klr></trd>
     </trdgrp>
   </snc>
   <snc mrk="alt.0o.MAT">
     <uzo tip="fak">MAT</uzo>
     <fnt><bib>PIV1</bib></fnt>
     <subsnc mrk="alt.0o.triangulo">
       <dif>
         <klr>(de <ref cel="angul.tri0o">triangulo</ref>)</klr>
         <ref tip="super" cel="ort.0anto">Ortanto</ref> al latero de 
         &gcirc;i, trairanta la <ref cel="kontraux.0a.triangulo">
         kontra&ubreve;an</ref> verticon; alternative: la 
         <ref cel="strek.0o.segmento">streko</ref>
         kunliganta tiun verticon kaj la sekcopunkton de la ortanto 
         kun la latero; la longo de tiu streko:
         <ekz>
           piedo de <tld/>o <klr>(la intersekco de la <tld/>o kun la 
           latero, al kiu &gcirc;i estas orta)</klr>;
         </ekz>
         <ekz>
           la tri <tld/>oj de triangulo havas unu komunan punkton;
         </ekz>
         <ekz>
           areo de triangulo egalas al duono de la produto de unu 
           &gcirc;ia latero per la responda <tld/>o.
         </ekz>
       </dif>
       <bld lok="../bld/e_altoj.svg" tip="svg">
         <tld/>oj de triangulo
       </bld>
       <trd lng="nl">hoogte<klr tip="ind"> (v.e.hoek)</klr></trd>
     </subsnc>
     <subsnc mrk="alt.0o.trapezo">
       <dif>
         <klr>(de <ref cel="trapez.0o.figuro">trapezo</ref>
         a&ubreve; <ref cel="parale2.0o">paralelogramo</ref>)</klr>
         &Ccirc;iu <ref cel="strek.0o.segmento">streko</ref> orte 
         kunliganta punktojn de du paralelaj lateroj; la longo de tia 
         streko:
         <ekz>
          areo de paralelogramo egalas al la produto de unu &gcirc;ia
          <ref cel="baz.0o.figuro">bazo</ref> per la responda <tld/>o.
         </ekz>
       </dif>
     </subsnc>
     <trd lng="de">H&ouml;he</trd>
     <trd lng="en">altitude</trd>
     <trd lng="fr">hauteur<klr tip="ind"> (g&eacute;om.)</klr></trd>
     <trd lng="he">&nun;&yod;&tsadi;&bet;<klr tip="ind"> 
     (&bet;&he;&nun;&dalet;&samekh;&he;)</klr></trd>
   </snc>
  <trd lng="be">&c_v;&c_y;&c_sh;&c_y;&c_n;&c_ja;</trd>
  <trd lng="bg">&c_v;&c_i;&c_s;&c_o;&c_ch;&c_i;&c_n;&c_a;</trd>
   <trd lng="bo">&#xF58;&#xF50;&#xF7C;&#xF0B;&#xF5A;&#xF51;&#xF0B;</trd>
  <trd lng="cs">v&yacute;&scaron;ka
  <klr>(n&ecaron;&ccaron;eho)</klr></trd>
  <trd lng="es">altura</trd>
  <trdgrp lng="fi">
     <trd>korkeus</trd>,
     <trd>pituus (pystysuunnassa)</trd>
   </trdgrp>
  <trd lng="he">&#x2137;&vav;&bet;&he;</trd>
  <trd lng="id">ke<ind>tinggi</ind>an</trd>
  <trd lng="it">altezza</trd>
  <trd lng="nl">hoogte</trd>
  <trd lng="pl">wysoko&sacute;&cacute;</trd>
  <trd lng="pt">altura</trd>
  <trd lng="ru">&c_v;&c_y;&c_s;&c_o;&c_t;&c_a;</trd>
  <trd lng="sk">v&yacute;&scaron;ka</trd>
  <trd lng="th">&#xE04;&#xE27;&#xE32;&#xE21;&#xE2A;&#xE39;&#xE07;</trd>
   <trd lng="tp">sewi</trd>
</drv>

