<drv mrk="alt.0igi">
   <kap><tld/>igi</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="alt.0igi.KOMUNE">
     <dif>
       <refgrp tip="vid">
         <ref cel="lev.0i">Levi</ref>,
         <ref cel="super.0igi">superigi</ref>:
       </refgrp>
      <ekz>
        por bone lin vidigi al la du amikinoj, &scirc;i <tld/>igis la novnaskiston
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz><ekz>
        la dukino <klr>[&#8230;]</klr> povu &ccirc;iam <tld/>igi la frunton, kaj ne ru&gcirc;i&gcirc;u pro sia
        estinteco
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekdua</lok>
        </fnt>;
      </ekz><ekz>
        ta&ubreve;ge <tld/>igi a&ubreve; mal<tld/>igi la vo&ccirc;on
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>igi sin al la balkono 
        <fnt>
          <bib>ChV</bib>
          <lok>3</lok>
        </fnt>;
      </ekz><ekz>
        ni e&ccirc; <tld/>igos la plafonon por doni vojon
        <fnt>
          <bib>KrM</bib>
          <lok>3. Ne Gutas Mielo El La &Ccirc;ielo.</lok>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="de">hochheben</trd>
