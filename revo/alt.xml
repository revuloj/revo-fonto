 <drv mrk="alt.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc mrk="alt.0a.dimensio">
     <dif>
       Granda la&ubreve; la dimensio <tld/>o;
       havanta difinitan grandon la&ubreve; tiu dimensio:
       <ekz>
         <tld/>a arbo, monto, domo;
       </ekz>
       <ekz>
         de rigardo tro <tld/>a <klr>(direktata <tld/>en)</klr>
         malsani&gcirc;as la okulo
          <fnt><bib>PrV</bib></fnt>;
       </ekz>
       <ekz>
         rokaj muroj tie staris <tld/>e
         <fnt><bib>Fab1</bib> <lok>Ligo de amikeco</lok></fnt>;
       </ekz>
       <ekz>
         la ne&gcirc;o ku&scirc;is ekstere tiel <tld/>e
         <fnt><bib>Fab1</bib> <lok>Ligo de amikeco</lok></fnt>;
       </ekz>
       <ekz>
         muro duonmetron <tld/>a;
       </ekz>
       <ekz>
         hometo <tld/>a je iom pli ol unu klafto.
       </ekz>
     </dif>
     <trd lng="en">tall</trd>
     <trdgrp lng="fi">
       <trd>korkea</trd>,
       <trd>pitk&auml;<klr> (ihminen, puu)</klr></trd>
     </trdgrp>
  <trd lng="it">alto</trd>
   </snc>
   <snc mrk="alt.0a.distanco">
     <dif>
       Trovi&gcirc;anta je granda distanco super la tero:
       <ekz>
         <tld/>aj nuboj;
       </ekz>
       <ekz>
         li supreniris <tld/>en
         <fnt><bib>NT</bib>, <lok>&Efe; 4:8</lok></fnt>;
       </ekz>
       <ekz>
         &jcirc;eti <tld/>en en la aeron<fnt>Z</fnt>;
       </ekz>
       <ekz>
         la luno estis grandega nokta lampo, <tld/>e supre
         <fnt><bib>Fab1</bib> <lok>Vojkamarado</lok></fnt>.
       </ekz>
     </dif>
     <trd lng="en">high</trd>
     <trdgrp lng="fi">
       <trd>korkea(lla oleva)</trd>,
       <trd>yl&auml;-</trd>
     </trdgrp>
   <trd lng="it">alto</trd>
   </snc>
   <snc mrk="alt.0a.FIG">
     <uzo tip="stl">FIG</uzo>
     <dif>
       Granda la&ubreve; metafora senco; nobla, supera:
       <ekz>
         <tld/>a frekvenco, pezo, prezo, temperaturo;
       </ekz>
       <ekz>
         <tld/>a penso, sento, celo, lernejo, ofico, rango,
         &scirc;ato, sa&gcirc;o;
       </ekz>
       <ekz>
         mal&scirc;ate rigardi iun de <tld/>e<fnt>Z</fnt>;
       </ekz>
       <ekz>
        en plej <tld/>a mizero al Dio esperu<fnt><bib>PrV</bib></fnt>.
       </ekz>
     </dif>
     <trd lng="en">high</trd>
     <trdgrp lng="fi">
       <trd>suuri</trd>,
       <trd>korkea</trd>
     </trdgrp>
    <trd lng="it">alto</trd>
   </snc>
   <snc mrk="alt.0a.MUZ">
     <uzo tip="fak">MUZ</uzo>
     <dif>
       Enhavanta multajn vibrojn en difinita tempoda&ubreve;ro,
       <tld/>frekvenca,
       <ref tip="vid" cel="akut.0a.FON">akuta<sncref/></ref>:
       <ekz>
         la tono de tiu piano estas tro <tld/>a por mia vo&ccirc;o.
       </ekz>
     </dif>
     <trd lng="en">alto</trd>
     <trdgrp lng="it">
     <trd>alto</trd>,
     <trd>acuto</trd>
   </trdgrp>
   </snc>
