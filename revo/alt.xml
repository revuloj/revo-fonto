      <ekz>
        la in&gcirc;eniero <klr>[&#8230;]</klr> faras sian plej bonan, por <tld/>igi la vaporpremon
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 31</lok>
        </fnt>;
      </ekz><ekz>
        iom <tld/>igi la kotizojn
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz><ekz>
        tio <tld/>igas la suman nombron al &ccirc;. 1100
        <fnt>
          <bib>EeP</bib>
          <lok>Tria Parto</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>igi sian kulturan nivelon
        <fnt>
          <bib>ChV</bib>
          <lok>19</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>igi &gcirc;enerale la nivelon de instruado
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        fari&gcirc;o okazis, kiu &gcirc;is supermezureco <tld/>igis la &gcirc;eneralan miregon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Tria</lok>
        </fnt>;
      </ekz><ekz>
         <uzo tip="stl">FIG</uzo>
         mi <tld/>igis vin el inter la popolo kaj faris vin princo super
         Mia popolo <nom>Izrael</nom>
         <fnt><bib>MT</bib> <lok>&Re1; 14:7</lok></fnt>.
