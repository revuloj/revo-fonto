   <trdgrp lng="sv">
     <trd>h&ouml;jd</trd>,
     <trd>kulle</trd>
   </trdgrp>
</drv>

<drv mrk="alt.0ajxeto">
  <kap><tld/>a&jcirc;eto</kap>
