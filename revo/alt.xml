<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <ofc>*</ofc><rad>alt</rad>/a
  </kap>
  <drv mrk="alt.0a">
    <kap><ofc>*</ofc><tld/>a</kap>
    <snc mrk="alt.0a.dimensio">
      <dif>
        Granda la&ubreve; la dimensio <tld/>o;
        havanta difinitan grandon la&ubreve; tiu dimensio:
        <ekz>
          <tld/>a arbo, monto, domo;
        </ekz>
        <ekz>
          de rigardo tro <tld/>a <klr>(direktata <tld/>en)</klr>
          malsani&gcirc;as la okulo<fnt>Z</fnt>;
        </ekz>
        <ekz>
          rokaj muroj tie staris <tld/>e
          <fnt><bib>Fab1</bib>, <lok>Ligo de amikeco</lok></fnt>;
        </ekz>
        <ekz>
          la ne&gcirc;o ku&scirc;is ekstere tiel <tld/>e
          <fnt><bib>Fab1</bib>, <lok>Ligo de amikeco</lok></fnt>;
        </ekz>
        <ekz>
          muro duonmetron <tld/>a;
        </ekz>
        <ekz>
          hometo <tld/>a je iom pli ol unu klafto.
        </ekz>
      </dif>
      <trd lng="en">tall</trd>
    </snc>
    <snc mrk="alt.0a.distanco">
      <dif>
        Trovi&gcirc;anta je granda distanco super la tero:
        <ekz>
          <tld/>aj nuboj;
        </ekz>
        <ekz>
          li supreniris <tld/>en
          <fnt><bib>NT</bib>, <lok>&Efe; 4:8</lok></fnt>;
        </ekz>
        <ekz>
          &jcirc;eti <tld/>en en la aeron<fnt>Z</fnt>;
        </ekz>
        <ekz>
          la luno estis grandega nokta lampo, <tld/>e supre
          <fnt><bib>Fab1</bib>, <lok>Vojkamarado</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="en">high</trd>
    </snc>
    <snc mrk="alt.0a.FIG">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Granda la&ubreve; metafora senco; nobla, supera:
        <ekz>
          <tld/>a frekvenco, pezo, prezo, temperaturo;
        </ekz>
        <ekz>
          <tld/>a penso, sento, celo, lernejo, ofico, rango,
          &scirc;ato, sa&gcirc;o;
        </ekz>
        <ekz>
          mal&scirc;ate rigardi iun de <tld/>e<fnt>Z</fnt>;
        </ekz>
        <ekz>
          en plej <tld/>a mizero al Dio esperu<fnt><bib>PrV</bib></fnt>.
        </ekz>
      </dif>
      <trd lng="en">high</trd>
    </snc>
    <snc mrk="alt.0a.MUZ">
      <uzo tip="fak">MUZ</uzo>
      <dif>
        Enhavanta multajn vibrojn en difinita tempoda&ubreve;ro,
        <tld/>frekvenca:
        <ekz>
          la tono de tiu piano estas tro <tld/>a por mia vo&ccirc;o.
        </ekz>
      </dif>
      <trd lng="en">alto</trd>
    </snc>
    <trd lng="be">&c_v;&c_y;&c_s;&c_o;&c_k;&c_ib;</trd>
    <trd lng="cs">vysok&yacute;</trd>
    <trd lng="de">hoch</trd>
    <trd lng="es">alto</trd>
    <trdgrp lng="fr">
      <trd>&eacute;lev&eacute;<klr tip="ind"> (haut)</klr></trd>,
      <trd>haut</trd>
    </trdgrp>
    <trd lng="he">&gimel;&bet;&vav;&he;</trd>
    <trd lng="it">alto</trd>
    <trd lng="nl">hoog</trd>
    <trd lng="pl">wysoki</trd>
    <trdgrp lng="pt">
      <trd>alto</trd>,
      <trd>elevado</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_y;&c_s;&c_o;&c_k;&c_i;&c_j;</trd>
    <trd lng="sk">vysok&yacute;</trd>
    <trd lng="sv">h&ouml;g</trd>
    <trdgrp lng="tp"><trd>sewi</trd>, <trd>suli</trd></trdgrp>
    <trd lng="tr">y&uuml;ksek</trd>
</drv>

<drv mrk="alt.0o">
    <kap><tld/>o</kap>
    <snc mrk="alt.0o.dimensio">
      <dif>
        Vertikala <ref tip="super" cel="dimens.0o.ampleksa">dimensio</ref>
        de objekto:
        <ekz>
          tricent ulnoj estu la longo de la arkeo, kvindek ulnoj &gcirc;ia
          lar&gcirc;o, kaj tridek ulnoj &gcirc;ia <tld/>o
          <fnt><bib>MT</bib>, <lok>&Gen; 6:15</lok></fnt>.
        </ekz>
      </dif>
      <trd lng="en">height</trd>
      <trdgrp lng="fr">
        <trd>hauteur<klr tip="ind"> (dimension verticale)</klr></trd>,
        <trd>taille<klr tip="ind"> (hauteur)</klr></trd>
      </trdgrp>
    </snc>
    <snc mrk="alt.0o.MAT">
      <uzo tip="fak">MAT</uzo>
      <fnt><bib>PIV1</bib></fnt>
      <subsnc mrk="alt.0o.triangulo">
        <dif>
          <klr>(de <ref cel="angul.tri0o">triangulo</ref>)</klr>
          <ref tip="super" cel="ort.0anto">Ortanto</ref> al latero de
&gcirc;i,
          trairanta la <ref
cel="kontraux.0a.triangulo">kontra&ubreve;an</ref>
          verticon; alternative: la <ref
cel="strek.0o.segmento">streko</ref>
          kunliganta tiun verticon kaj la
          sekcopunkton de la ortanto kun la latero; la longo de tiu streko:
          <ekz>
            piedo de <tld/>o <klr>(la intersekco de la <tld/>o kun la
latero,
            al kiu &gcirc;i estas orta)</klr>;
          </ekz>
          <ekz>
            la tri <tld/>oj de triangulo havas unu komunan punkton;
          </ekz>
          <ekz>
            areo de triangulo egalas al duono de la produto de unu &gcirc;ia
            latero per la responda <tld/>o.
          </ekz>
        </dif>
        <trd lng="nl">hoogte<klr tip="ind"> (v.e.hoek)</klr></trd>
        <bld tip="svg" lok="../bld/e_altoj.svg" alt="300" lrg="300">
          <tld/>oj de triangulo
        </bld>
      </subsnc>
      <subsnc mrk="alt.0o.trapezo">
        <dif>
          <klr>(de <ref cel="trapez.0o.figuro">trapezo</ref>
          a&ubreve; <ref cel="parale2.0o">paralelogramo</ref>)</klr>
          &Ccirc;iu <ref cel="strek.0o.segmento">streko</ref> orte
kunliganta
          punktojn de du paralelaj lateroj; la longo de tia streko:
          <ekz>
            areo de paralelogramo egalas al la produto de unu &gcirc;ia
            <ref cel="baz.0o.figuro">bazo</ref> per la responda <tld/>o.
          </ekz>
        </dif>
      </subsnc>
      <trd lng="de">H&ouml;he</trd>
      <trd lng="en">altitude</trd>
      <trd lng="fr">hauteur<klr tip="ind"> (g&eacute;om.)</klr></trd>
      <trd lng="he">&nun;&yod;&tsadi;&bet;<klr tip="ind">
(&bet;&he;&nun;&dalet;&samekh;&he;)</klr></trd>
      <trd lng="pl">wysoko&sacute;&cacute;</trd>
    </snc>
    <trd lng="be">&c_v;&c_y;&c_sh;&c_y;&c_n;&c_ja;</trd>
    <trd lng="es">altura</trd>
    <trd lng="he">&gimel;&vav;&bet;&he;</trd>
    <trd lng="nl">hoogte</trd>
    <trd lng="pt">altura</trd>
    <trd lng="ru">&c_v;&c_y;&c_s;&c_o;&c_t;&c_a;</trd>
    <trd lng="sk">v&yacute;&scaron;ka</trd>
    <trd lng="tp">sewi</trd>
</drv>

<drv mrk="alt.0eco">
    <kap><ofc>*</ofc><tld/>eco</kap>
    <snc mrk="alt.0eco.eco">
      <dif>
        Eco de tio, kio estas <tld/>a:
        <ekz>
          &gcirc;i estis bone videbla pro sia <tld/>eco kaj pro la multo de
          siaj bran&ccirc;oj
          <fnt><bib>MT</bib>, <lok>&Jeh; 19:11</lok></fnt>;
        </ekz>
        <ekz>
          du bienoj tie trovi&gcirc;is en &ccirc;irka&ubreve; 1300-metra
<tld/>eco
          <fnt><bib>Revadoj</bib>, <lok>Sepa promenado</lok></fnt>;
        </ekz>
        <ekz>
          <tld/>eco de prezo, muzika noto, de socia pozicio.
        </ekz>
      </dif>
      <adm>
        PIV1 donas du fakajn variojn de tiu senco: geografian (alto super
        certa nivelo) kaj astronomian (=altitudo, t.e. angulo). Mi hezitas
        enkonduki ilin, des pli ke ili logike pli rilatas al mezuro ol al
        eco.
        <aut>MB</aut>
      </adm>
      <adm>
        Por la astronomia senco, la terminoj "altangulo"
        (anka&ubreve; PIVa) kaj "altitudo" bone anstata&ubreve;as &gcirc;in
        <aut>HJ</aut>
      </adm>
      <trd lng="fr">hauteur<klr tip="ind"> (fait d'&ecirc;tre
haut)</klr></trd>
    </snc>
    <snc mrk="alt.0eco.dimensio">
      <dif>
        <ref tip="dif" cel="alt.0o.dimensio"><tld lit="A"/>o
        <sncref ref="alt.0o.dimensio"/></ref>:
        <ekz>
          lia <tld/>eco estis ses ulnoj kaj unu manlar&gcirc;o
          <fnt><bib>MT</bib>, <lok>&Sa1; 17:4</lok></fnt>.
        </ekz>
      </dif>
      <adm>
        Male al tio, kion mi faris &ccirc;e "longeco" kaj "lar&gcirc;eco"
        mi ne emas meti indikon ARK, &ccirc;ar mi dubas, &ccirc;u la
        lingvo vere same evoluis kun "alteco". <aut>MB</aut>
      </adm>
    </snc>
    <trd lng="be">&c_v;&c_y;&c_sh;&c_y;&c_n;&c_ja;</trd>
    <trd lng="nl">hoogte</trd>
    <trdgrp lng="en">
      <trd>height</trd>,
      <trd>altitude</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>altura</trd>,
      <trd>altitud</trd>
    </trdgrp>
    <trd lng="de">H&ouml;he</trd>
    <trd lng="he">&gimel;&vav;&bet;&he;</trd>
    <trdgrp lng="pt">
      <trd>altura</trd>,
      <trd>altitude</trd>,
      <trd>emin&ecirc;encia</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_y;&c_s;&c_o;&c_t;&c_a;</trd>
    <trd lng="sk">v&yacute;&scaron;ka</trd>
</drv>

<drv mrk="alt.0ajxo">
    <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
    <uzo tip="fak">GEOG</uzo>
    <snc>
      <dif>
        Alta loko, monteto:
        <ekz>
          tiu &ccirc;i granda <tld/>a&jcirc;o ne estas natura monto
          <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>.
        </ekz>
      </dif>
    </snc>
    <trdgrp lng="be">
      <trd>&c_u;&c_z;&c_v;&c_y;&c_sh;&c_sh;&c_a;</trd>,
      <trd>&c_u;&c_z;&c_g;&c_o;&c_r;&c_a;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Anh&ouml;he</trd>
    <trd lng="en">heights</trd>
    <trdgrp lng="fr">
      <trd>hauteur<klr tip="ind"> (butte)</klr></trd>,
      <trd>butte<klr tip="ind"> (hauteur)</klr></trd>
    </trdgrp>
    <trd lng="he">&resh;&mem;&he;</trd>
    <trd lng="nl">hoogte</trd>
    <trdgrp lng="pt">
      <trd>alto</trd>,
      <trd>mont&iacute;culo</trd>,
      <trd>colina</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_o;&c_z;&c_v;&c_y;&c_sh;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_s;&c_o;&c_t;&c_a;</trd>
    </trdgrp>
    <trd lng="sk">vyso&ccaron;ina</trd>
    <trdgrp lng="sv">
      <trd>h&ouml;jd</trd>,
      <trd>kulle</trd>
    </trdgrp>
</drv>

<drv mrk="alt.0igi">
    <kap><tld/>igi</kap>
    <gra><vspec>tr</vspec></gra>
    <snc>
      <dif>
        Igi <tld/>a:
        <ekz>
          <tld/>igi la prezojn;
        </ekz>
        <ekz>
          ricevi rangoplialtigon;
        </ekz>
        <ekz>
          precipe dank' al la lingvo la homo <tld/>i&gcirc;is super
          la bestoj<fnt>Z</fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="lev.0i">levi</ref>.
      </refgrp>
    </snc>
    <trdgrp lng="be">
      <trd>&c_u;&c_z;&c_v;&c_y;&c_s;&c_ib;&c_c;&c_mol;</trd>,
      <trd>&c_p;&c_a;&c_v;&c_y;&c_s;&c_ib;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">erh&ouml;hen</trd>
    <trd lng="en">raise</trd>
    <trdgrp lng="de">
      <trd>erh&ouml;hen</trd>,
      <trd>anheben</trd>,
      <trd>lupfen<klr> [sudgermane]</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>&eacute;lever<klr> (hausser)</klr></trd>,
      <trd>hausser</trd>,
      <trd>rehausser</trd>
    </trdgrp>
    <trdgrp lng="he">
      <trd>&lamed;&he;&gimel;&bet;&yod;&he;
\<ind>&gimel;&bet;&he;</ind>\</trd>,
      <trd>&lamed;&he;&resh;&yod;&fmem;
\<ind>&resh;&vav;&fmem;</ind>\</trd>,
      <trd>&lamed;&he;&ayin;&lamed;&vav;&tav;
\<ind>&ayin;&lamed;&he;</ind>\</trd>
    </trdgrp>
    <trd lng="nl">verhogen</trd>
    <trdgrp lng="pt">
      <trd>elevar</trd>,
      <trd>altear</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_v;&c_y;&c_sh;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_v;&c_y;&c_s;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>zv&yacute;&scaron;i&tcaron;</trd>,
      <trd>pov&yacute;&scaron;i&tcaron;</trd>
    </trdgrp>
    <trd lng="sv">h&ouml;ja</trd>
</drv>

<drv mrk="alt.mal0a">
    <kap>mal<tld/>a</kap>
    <snc mrk="alt.mal0a.dimensio">
      <dif>
        Negranda de supro &gcirc;is malsupro:
        <ekz>
          la altranguloj mal<tld/>e klini&gcirc;is
          <fnt><bib>Far1</bib>, <lok>&ccirc;ap. 6</lok></fnt>;
        </ekz>
        <ekz>
          mal<tld/>a reliefo.
        </ekz>
      </dif>
    </snc>
    <trd lng="de">flach</trd>
    <snc mrk="alt.mal0a.distanco">
      <dif>
        Je negranda distanco super la tero.
      </dif>
    </snc>
    <trd lng="de">niedrig</trd>

    <snc mrk="alt.mal0a.FIG">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Malgranda, malsupra la&ubreve; valorskalo:
        <ekz>
          landoj kie oni perlaboras mal<tld/>an salajron
          <fnt><aut>Remigiusz Mielcarek</aut>, <vrk>Enmigrintoj en ora
          ka&gcirc;o</vrk>, <bib>Monato</bib>, <lok>jaro 1994a, numero 1a,
p.
          15a</lok></fnt>.
        </ekz>
      </dif>
      <ref tip="vid" cel="humil.0a">humila</ref>
    </snc>
    <trd lng="de">niedrig</trd>

    <snc mrk="alt.mal0a.MUZ">
      <uzo tip="fak">MUZ</uzo>
      <dif>
        Kun malrapidaj vibroj, ne granda frekvenco:
        <ekz>
          lia vo&ccirc;o estis tiom mal<tld/>a, ke je kelkaj vortoj &gcirc;i
sinkis en
          la ra&ubreve;kon
          <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 22a, p. 220a</lok></fnt>.
        </ekz>
      </dif>
      <ref tip="vid" cel="bas.0a">basa</ref>
    </snc>
    <trd lng="de">tief</trd>

    <trd lng="fr">bas</trd>
    <trd lng="ru">&c_n;&c_i;&c_z;&c_k;&c_i;&c_j;</trd>
  <trd lng="tp">lili</trd>
</drv>

<drv mrk="alt.mal0igi">
    <kap>mal<tld/>igi</kap>
    <gra><vspec>tr</vspec></gra>
    <snc>
      <dif>
        Igi mal<tld/>a:
        <ekz>
          <uzo tip="stl">FIG</uzo>
          la malvirtulojn Li mal<tld/>igas &gcirc;is la tero
          <fnt><bib>MT</bib>, <lok>&Psa; 147:6</lok></fnt>.
        </ekz>
      </dif>
    </snc>
    <trd lng="fr">abaisser</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_n;&c_i;&c_z;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_s;&c_n;&c_i;&c_z;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_n;&c_i;&c_z;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
</drv>
</art>
