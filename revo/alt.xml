   <snc mrk="alt.0ajxo.GEOG">
     <uzo tip="fak">GEOG</uzo>
     <dif>
       Alta loko, monteto:
       <ekz>
         tiu &ccirc;i granda <tld/>a&jcirc;o ne estas natura monto
         <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>.
       </ekz>
     </dif>
     <trdgrp lng="th">
       <trd>&#3626;&#3606;&#3634;&#3609;&#3607;&#3637;&#3656;&#3626;&#3641;&#3591;</trd>,
       <trd>&#3616;&#3641;&#3648;&#3586;&#3634;</trd>
     </trdgrp>
   </snc>
   <trdgrp lng="be">
     <trd>&c_u;&c_z;&c_v;&c_y;&c_sh;&c_sh;&c_a;</trd>,
     <trd>&c_u;&c_z;&c_g;&c_o;&c_r;&c_a;&c_k;</trd>
   </trdgrp>
   <trd
   lng="bg">&c_v;&c_malmol;&c_z;&c_v;&c_i;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
   <trd lng="de">Anh&ouml;he</trd>
   <trd lng="en">heights</trd>
   <trdgrp lng="es">
     <trd>lugar alto</trd>,
