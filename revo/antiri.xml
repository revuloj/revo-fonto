  <trd lng="de">L&ouml;wenmaul</trd>
  <trd lng="en">snapdragon</trd>
  <trdgrp lng="fr">
    <trd>muflier</trd>,
    <trd>gueule-de-loup</trd>
  </trdgrp>
  <trd lng="hu">oroszl&aacute;nsz&aacute;j</trd>
  <trd lng="nl">leeuwenbek</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_n;&c_t;&c_i;&c_r;&c_r;&c_i;&c_n;&c_u;&c_m;</trd>,
    <trd>&c_l;&c_mol;&c_v;&c_i;&c_n;&c_y;&c_j; &c_z;&c_je;&c_v;</trd>
  </trdgrp>
</drv>
</art>
