  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="skrofu1.0oj">skrofulariacoj</ref>
      (<trd lng="la">Antirrhinum</trd>), kies floro similas iomete la
