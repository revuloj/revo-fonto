<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>galvaniz</rad>/i
</kap>
<drv mrk="galvan1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">ELE</uzo>
  <snc num="1">
    <dif>
      Surkovri ion per maldika tavolo de metalo helpe de elektroliza
      kontinua kurento:
      <ekz>
        oni ordinare <tld/>as metalajn surfacojn per zinka tavolo,
        tiele defendante ilin kontra&ubreve; rusti&gcirc;o.
      </ekz>
    </dif>
  <trdgrp lng="de">
     <trd>galvanisieren</trd>,
     <trd>verzinken</trd></trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Submeti al kontinua kurento. 
    </dif>
    <refgrp tip="sin">
      <ref cel="elektr">elektri</ref>,
      <ref cel="faradi.0i">faradizi</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>galvanisieren</trd>,
     <trd>mit <ind>Gleichstrom</ind> behandeln</trd></trdgrp>
  </snc>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
