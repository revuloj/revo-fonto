<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>galvaniz</rad>/i</kap>

<drv mrk="galvan1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Surkovri ion per maldika tavolo de metalo helpe de elektroliza
      kontinua kurento:
      <ekz>
        oni ordinare <tld/>as metalajn surfacojn per zinka tavolo,
        tiele defendante ilin kontra&ubreve; rusti&gcirc;o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>galvanisieren</trd>,
      <trd>verzinken</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>galvaniseren</trd>,
      <trd>verzinken</trd>
    </trdgrp>
    <trd lng="sv">galvanisera</trd>
  </snc>
  <snc>
    <dif>
      Submeti al kontinua kurento.
    </dif>
    <refgrp tip="vid">
      <ref cel="elektr.0umi">elektrumi</ref>,
      <ref cel="faradi.0i">faradizi</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>galvanisieren</trd>,
      <trd>mit <ind>Gleichstrom</ind> behandeln</trd>
    </trdgrp>
    <trd lng="nl">galvaniseren</trd>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>galvanisieren</trd>
  </trdgrp>
  <trd lng="el">&gamma;&alfa;&lambda;&beta;&alfa;&ny;&jota_ton;&zeta;&omega;</trd>
  <trd lng="es">galvanizar</trd>
  <trd lng="fr">galvaniser</trd>
  <trdgrp lng="hu">
    <trd>galvaniz&aacute;l</trd>,
    <trd>galv&aacute;noz</trd>
  </trdgrp>
  <trd lng="pt">galvanizar</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="tr">galvanizlemek</trd>
</drv>
</art>
