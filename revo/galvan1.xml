<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: galvan1.xml,v 1.23 2023/11/11 10:00:38 revo Exp $">
<kap><rad>galvaniz</rad>/i</kap>

<drv mrk="galvan1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Surkovri ion per maldika tavolo de metalo helpe de elektroliza
      kontinua kurento:
      <ekz>
        oni ordinare <tld/>as metalajn surfacojn per zinka tavolo,
        tiele defendante ilin kontra&ubreve; rusti&gcirc;o.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>galvanisieren</trd>,
      <trd>verzinken</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>galvaniseren</trd>,
      <trd>verzinken</trd>
    </trdgrp>
    <trd lng="pl">galwanizowa&cacute;</trd>
    <trd lng="sv">galvanisera</trd>
  </snc>
  <snc>
    <dif>
      Submeti al kontinua kurento.
    </dif>
    <refgrp tip="vid">
      <ref cel="elektr.0umi">elektrumi</ref>,
      <ref cel="faradi.0i">faradizi</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>galvanisieren</trd>,
      <trd>mit <ind>Gleichstrom</ind> behandeln</trd>
    </trdgrp>
    <trd lng="nl">galvaniseren</trd>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_g;&c_a;&c_l;&c_v;&c_a;&c_n;&c_i;&c_z;&c_i;&c_r;&c_a;&c_m;</trd>
  <trd lng="ca">galvanitzar</trd>
  <trd lng="cs">galvanizovat</trd>
  <trdgrp lng="de">
    <trd>galvanisieren</trd>
  </trdgrp>
  <trd lng="el">&gamma;&alfa;&lambda;&beta;&alfa;&ny;&jota_ton;&zeta;&omega;</trd>
  <trd lng="en">galvanize</trd>
  <trd lng="es">galvanizar</trd>
  <trd lng="fr">galvaniser</trd>
  <trdgrp lng="hu">
    <trd>galvaniz&aacute;l</trd>,
    <trd>galv&aacute;noz</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x96FB;&#x6C17;&#x30E1;&#x30C3;&#x30AD;&#x3059;&#x308B;  <pr>&#x3067;&#x3093;&#x304D;&#x3081;&#x3063;&#x304D;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x76F4;&#x6D41;&#x96FB;&#x6C17;&#x3092;&#x901A;&#x3059;  <pr>&#x3061;&#x3087;&#x304F;&#x308A;&#x3085;&#x3046;&#x3067;&#x3093;&#x304D;&#x3092;&#x3068;&#x304A;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="pt">galvanizar</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_n;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">galvanizova&tcaron;</trd>
  <trd lng="tr">galvanizlemek</trd>
  <trd lng="zh">&#x7535;&#x9540; <pr>di&#xE0;nd&#xF9;</pr></trd>
</drv>
</art>
