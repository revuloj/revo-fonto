<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: berenic.xml,v 1.2 2025/09/11 20:29:32 afido Exp $">
    <kap>
      <rad>Berenic</rad>/o <fnt><bib>PIV1</bib></fnt>,
      <var><kap><rad var="k">Berenik</rad>/o</kap></var>
    </kap>

<drv mrk="berenic.0o">
  <kap><tld/>o, <var><kap><tld var="k"/>o</kap></var></kap>
  <snc mrk="berenic.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Grekdevena virina <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">nomo</ref>:
      <ekz>
        mi ricevis la ideon de tiu amikino de praavinjo, la re&gcirc;ino <tld/>o, mi &ccirc;iam
        admiris &scirc;ian guston pri piedvestoj
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Tria</lok>
        </fnt>;
      </ekz>
      <ekz>
        Harplekta&jcirc;o de <tld var="k"/>o
        <klr>(vd <ref cel="har.berenicaH0aro">Berenica Hararo</ref>)</klr>
        <fnt>
          <bib>SrT</bib>
          <lok>Libro VIII.</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Berenice</trd>,
      <trd>Berenike</trd>
    </trdgrp>
    <trd lng="en">Berenice</trd>
    <trd lng="grc">&Beta;&epsilon;&rho;&epsilon;&ny;&jota_ton;&kappa;&eta;</trd>
    <trd lng="uk">&c_B;&c_je;&c_r;&c_je;&c_n;&c_ib;&c_k;&c_a;</trd>
  </snc>
</drv>

  </art>

