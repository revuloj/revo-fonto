<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>defil</rad>/i
</kap>
<drv mrk="defil.0i">
<kap><tld/>i</kap>
<gra><vspec>ntr</vspec></gra>
 <snc mrk="defil.0i.MIL">
 <uzo tip="fak">MIL</uzo>
  <dif>
  Parade mar&scirc;i anta&ubreve; iu estro a&ubreve; estraro.  
  </dif>
  <trd lng="be">&c_d;&c_e;&c_f;&c_ib;&c_l;&c_ja;&c_v;&c_a;&c_c;&c_mol;</trd>
   <trdgrp lng="de">
     <trd>defilieren</trd>,
     <trd>vorbeimarschieren</trd>
   </trdgrp>
   <trd lng="es">desfilar</trd>
   <trd lng="fr">d&eacute;filer<klr tip="ind"> (marcher en file)</klr></trd>
   <trdgrp lng="hu">
    <trd>felvonul</trd>,
    <trd>elvonul<klr tip="ind"> (d&iacute;szmenetben)</klr></trd>
   </trdgrp>
   <trd lng="pt">desfilar</trd>
   <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_f;&c_i;&c_l;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_sh;&c_je;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
   <trd lng="sk">defilova&tcaron;</trd>
 </snc>
 <snc mrk="defil.0i.modo">
  <dif>
  Mar&scirc;i anta&ubreve; spektantaro por montri vesta&jcirc;ojn.
   <ekz>
    Sed kiel profesiulino pri <tld/>ado por modaj produktoj &scirc;i tute 
    ne havas preferojn<fnt><aut>Gilbert Ledon</aut>:<vrk>La krimo de 
    Gisele</vrk>, <bib>Monato</bib></fnt>
    </ekz>
   </dif>
  <trd lng="pt">desfilar</trd>
</snc>  
</drv>
</art>
