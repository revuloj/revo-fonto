  <kap><tld/>o</kap>
  <snc mrk="budapesxt.0o.cxefurbo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" cel="urb.0o" lst="voko:&ccirc;efurboj">&Ccirc;efurbo</ref>
