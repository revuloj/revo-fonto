<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kopl.xml,v 1.10 2023/11/30 18:30:04 revo Exp $">
<kap>
  <rad>kopl</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kopl.0o">
  <kap><tld/>o</kap>
  <snc mrk="kopl.0o.POE">
  <uzo tip="fak">POE</uzo>
    <dif>
      Hispandevena mallonga lirika <ref cel="poezi.0ajxo">poezia&jcirc;o</ref>
      improvizita, origine duversa:
      <ekz>
        <nom>Cris&oacute;stomo</nom> faris tre belajn <tld/>ojn
        <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 12a, p.
        88a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="strof.0o">strofo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_p;&c_l;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>spojen&iacute;</trd>,
    <trd>sp&rcaron;a&zcaron;en&iacute;</trd>
  </trdgrp>
  <trd lng="de">Copla</trd>
  <trd lng="es">copla</trd>
  <trd lng="fr">copla<klr> (po&egrave;me)</klr></trd>
  <trd lng="hu">copla</trd>
  <trd lng="pl">kuplet</trd>
  <trd lng="pt">copla</trd>
  <trd lng="ro">cuplet <klr>(c&acirc;ntec)</klr></trd>
  <trd lng="ru">&c_k;&c_o;&c_p;&c_l;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>spojenie</trd>,
    <trd>spriahnutie</trd>
  </trdgrp>
</drv>
</art>

