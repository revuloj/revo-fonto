<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: batrak.xml,v 1.12 2019/01/29 06:10:14 revo Exp $">
<kap>
<rad>batrak</rad>/oj
</kap>
<drv mrk="batrak.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="vost.sen0uloj">Senvostuloj</ref>:
      <ekz>
        la senvostaj amfibioj (<tld/>oj, ekz. ranoj) na&gcirc;as en la akvo per ritma pu&scirc;ado
        de la malanta&ubreve;aj piedoj, kion helpas la na&gcirc;membrano inter la piedfingroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=29122"
              >Movi&gcirc;o de la vertebruloj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_s;&c_h;&c_v;&c_o;&c_s;&c_t;&c_y;&c_ja; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="de">Froschlurche</trd>
  <trd lng="en">anura</trd>
  <trd lng="fr">batracien</trd>
  <trdgrp lng="hu">
    <trd>b&eacute;k&aacute;k<klr tip="amb"> (rendje)</klr></trd>,
    <trd>farokn&eacute;lk&uuml;liek<klr tip="amb"> (rendje)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_je;&c_m;&c_n;&c_o;&c_v;&c_o;&c_d;&c_n;&c_o;&c_je;</trd>,
    <trd>&c_b;&c_je;&c_s;&c_h;&c_v;&c_o;&c_s;&c_t;&c_a;&c_ja;
    &c_a;&c_m;&c_f;&c_i;&c_b;&c_i;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">bezchovostov&eacute; &zcaron;ivo&ccaron;&iacute;chy</trd>
</drv>
</art>
