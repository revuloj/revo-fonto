<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: ekved.xml,v 1.3 2022/12/08 08:33:38 revo Exp $">
    <kap>
      <rad>ekved</rad>/oj
    </kap>

<drv mrk="ekved.0oj">
  <kap><tld/>oj</kap>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_familioj" cel="famili.0o.BIO">Familio</ref>
      el la ordo <ref tip="malprt" cel="huf.nepar0uloj">neparhufuloj</ref>
      (<trd lng="la">Equidae</trd>), kun nefenditaj hufoj:
      <ekz>
        <ref tip="prt" cel="ekv.0o">ekvo</ref>
        estas genro de <klr>[la]</klr> familio de <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9017">Ekvo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;evalo, azeno kaj zebro ne estas sciencaj, sed popularaj kategorioj, la familio
        <tld/>oj do havas nur unu vivantan genron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9018">Ekvedoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="huf.unu0uloj">unuhufuloj</ref>
    <trd lng="be">&c_k;&c_o;&c_n;&c_je;&c_v;&c_y;&c_ja; <klr>(&c_s;&c_ja;&c_m;&#8217;&c_ja;)</klr></trd>
    <trd lng="ca">&egrave;quids</trd>
    <trdgrp lng="de">
      <trd>Pferde</trd>,
      <trd>Einhufer</trd>
    </trdgrp>
    <trd lng="fr">&eacute;quid&eacute;s</trd>
    <trd lng="hu">l&oacute;f&eacute;l&eacute;k</trd>
    <trd lng="it">equidi</trd>
    <trd lng="pl">koniowate</trd>
    <trd lng="pt">&eacute;q&uuml;idas</trd>
    <trdgrp lng="ru">
      <trd>&c_l;&c_o;&c_sh;&c_a;&c_d;&c_i;&c_n;&c_y;&c_je;</trd>,
      <trd>&c_l;&c_o;&c_sh;&c_a;&c_d;&c_i; <klr tip="amb">(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="uk">&#x43A;&#x43E;&#x43D;&#x44F;&c_ch;&c_ib;</trd>
</drv>

  </art>

