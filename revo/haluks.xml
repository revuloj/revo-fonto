<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: haluks.xml,v 1.18 2023/03/13 10:33:36 revo Exp $">
    <kap><rad>haluks</rad>/o</kap>

<drv mrk="haluks.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <uzo tip="stl">RAR</uzo>
    <dif>
      <ref tip="super" cel="fingr.dik0o.pieda">Dikfingro</ref> de 
      la <ref tip="malprt" cel="pied.0o.ANA">piedo</ref>:
      <ekz>
        dikfingro estas la plej dika fingro, a&ubreve; mana (science nomata anka&ubreve; polekso) a&ubreve; pieda (nomata
        anka&ubreve; <tld/>o)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=21124">Dikfingro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve;e al la aliaj piedfingroj, la <tld/>o havas nur du falangojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=121632">Halukso</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_z;&c_ib;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_v;&c_ja;&c_l;&c_ib;&c_k;&c_ib; <ind>&c_p;&c_a;&c_l;&c_je;&c_c;</ind></trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_h;&c_a;&c_l;&c_u;&c_k;&c_s;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_je;&c_c; &c_n;&c_a; &c_k;&c_r;&c_a;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">h&agrave;l&middot;lux</trd>
  <trd lng="cs">palec u nohy</trd>
  <trd lng="de">gro&szlig;er Zeh</trd>
  <trd lng="fa">&a_sh;&a_s;&a_t; &f_p;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>hallux</trd>,
    <trd>gros <ind>orteil</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;regujj <klr tip="amb">(l&aacute;bon)</klr></trd>,
    <trd>nagy <ind>l&aacute;bujj</ind></trd>
  </trdgrp>
  <trd lng="it">alluce</trd>
  <trd lng="nl">grote <ind>teen</ind></trd>
  <trdgrp lng="pl">
    <trd>paluch</trd>,
    <trd>haluks</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>h&aacute;lux</trd>, 
   <trd>ded&atilde;o<klr tip="amb"> (do p&eacute;)</klr></trd> 
  </trdgrp>
  <trd lng="sk">palec na nohe</trd>
  <trd lng="uk">&c_v;&c_je;&#x43B;&c_i;&#x43A;&c_i;&c_j; &#x43F;&c_a;&#x43B;&c_je;&c_c;&#x44C; &#x43D;&#x43E;&c_g;&c_i;</trd>
</drv>
  </art>

