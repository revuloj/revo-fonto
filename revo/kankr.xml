<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kankr</rad>/o
</kap>
<drv mrk="kankr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Iu el <ref tip="malprt" cel="astake.0oj">astakedoj</ref>,
      rivera man&gcirc;ebla dekpieda krustulo: 
      <ekz>
        grandaj kankroj etendis siajn fortajn pin&ccirc;ilojn
        <fnt>(<aut>Andersen, trad. Zamenhof</aut>: <vrk>Fabeloj,
        Malbona princo</vrk>)</fnt>  
      </ekz>
      <ekz>li scias, kie la <tld/>oj pasigas la vintron
        <klr>(li estas tre ruza)</klr><fnt><bib>PrV</bib></fnt>;</ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Flu&szlig;krebs</trd>,
    <trd>Krebs</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>crawfish</trd>,
    <trd>crayfish</trd>
  </trdgrp>
  <trd lng="fr">&eacute;crevisse</trd>
  <trd lng="nl">kreeft</trd>
  <trd lng="pl">rak</trd>
  <trd lng="ru">&c_r;&c_a;&c_k; <klr>(&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;)</klr></trd>
</drv>
<drv mrk="kankr.K0o">
  <kap><tld lit="K"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Cancer</trd>).
    </dif>
  </snc>
  <trd lng="de">Krebs</trd>
  <trdgrp lng="en">
    <trd>Cancer</trd>,
    <trd>Crab</trd>
  </trdgrp>
  <trd lng="fr">cancer</trd>
  <trd lng="nl">kreeft</trd>
  <trd lng="pl">rak</trd>
  <trd lng="ru">&c_R;&c_a;&c_k; <klr>(&c_s;&c_o;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;, &c_z;&c_n;&c_a;&c_k; &c_z;&c_o;&c_d;&c_i;&c_a;&c_k;&c_a;)</klr></trd>
</drv>
</art>
