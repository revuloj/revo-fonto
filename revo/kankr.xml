<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kankr</rad>/o
</kap>
<drv mrk="kankr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Speco de riveraj man&gcirc;eblaj krustuloj (<trd
      lng="la">astacus</trd>): 
      <ekz>
        li scias, kie la <tld/>oj pasigas la vintron<fnt>Z</fnt>
        <klr>(li estas tre ruza)</klr>;
      </ekz>
      <ekz>
        <tld/>a pin&ccirc;ilo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Krebs</trd>
  <trd lng="fr">&eacute;crevisse</trd>
  <trd lng="nl">kreeft</trd>
</drv>
<drv mrk="kankr.K0o">
  <kap><tld lit="K"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Cancer</trd>).
    </dif>
  </snc>
  <trd lng="de">Krebs</trd>
  <trd lng="fr">cancer</trd>
  <trd lng="nl">kreeft</trd>
</drv>
</art>
