<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kankr.xml,v 1.47 2023/12/03 08:00:22 revo Exp $">
<kap><ofc>*</ofc>
<rad>kankr</rad>/o</kap>
<drv mrk="kankr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Iu el <ref tip="malprt" cel="astake.0oj">astakedoj</ref>,
      rivera man&gcirc;ebla dekpieda 
      <ref tip="lst" cel="krust.0ulo" lst="voko:krustuloj">krustulo</ref>:
      <ekz>
	grandaj kankroj etendis siajn fortajn pin&ccirc;ilojn
	<fnt><bib>Fab4</bib>, <lok>Malbona princo</lok></fnt>;
      </ekz>
      <ekz>
	<ind>li scias, kie la <tld/>oj pasigas la vintron</ind>
	<klr>(li estas tre ruza)</klr>
	<fnt><bib>PrV</bib></fnt>.
        <trd lng="nl">hij weet waar <ind>Abraham</ind> de mosterd
        haalt</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_k;</trd>
  <trd lng="bg">&c_r;&c_a;&c_k;</trd>
  <trd lng="br">grilh-dour</trd>
  <trd lng="ca">cranc</trd>
  <trd lng="cs">rak</trd>
  <trdgrp lng="de">
    <trd>Flusskrebs</trd>,
    <trd>Krebs</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>crawfish</trd>,
    <trd>crayfish</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cangrejo</trd>,
    <trd>acamaya</trd>
  </trdgrp>
  <trd lng="fr">&eacute;crevisse</trd>
  <trd lng="he">&samekh;&resh;&tet;&fnun; &he;&nun;&he;&resh;&vav;&tav;</trd>
  <trd lng="hu">r&aacute;k</trd>
  <trd lng="id">kepiting</trd>
  <trdgrp lng="nl">
    <trd>kreeft<klr tip="ind"> (rivier-)</klr></trd>,
    <trd>rivierkreeft</trd>
  </trdgrp>
  <trd lng="no">kreps</trd>
  <trd lng="pl">rak</trd>
  <trd lng="ro">rac</trd>
  <trd lng="ru">&c_r;&c_a;&c_k; <klr>(&c_zh;&c_i;&c_v;&c_o;&c_t;&c_n;&c_o;&c_je;)</klr></trd>
  <trd lng="sk">rak</trd>
  <trd lng="zh">&#x5C0F;&#x9F8D;&#x8766; <pr>xi&#x1CE;ol&#xF3;ngxi&#x101;</pr></trd>
</drv>

<drv mrk="kankr.K0o">
  <kap><tld lit="K"/>o</kap>
  <uzo tip="fak">AST</uzo>
  <mlg>Cnc</mlg>
  <mlg>&#x264B;&#xFE0E;</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="stel.0figuro" lst="voko:zodiakaj_signoj" val="4">Stelfiguro</ref> kaj
      signo de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Cancer</trd>):
      <ekz>
      la <ind>tropiko de la <tld lit="K"/>o</ind>;
      <trd lng="de">Wendekreis des Krebses</trd>
      <trdgrp lng="nl">
        <trd>Kreeftskeerkring</trd>,
        <trd>noorderkeerkring</trd>
      </trdgrp>
      </ekz>
      <ekz>La jaro favoros al Leonoj, <tld lit="K"/>oj kaj Fi&scirc;oj pri
      amoraj aferoj
        <fnt>
          <aut>Dmitrij Cibulevskij</aut>
          <vrk>Nostalgie</vrk>
          <bib>Monato</bib>
          <lok>jaro 2001a, numero 5a, p. 34a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_R;&c_a;&c_k;</trd>
  <trd lng="bg">&c_R;&c_a;&c_k;</trd>
  <trd lng="br">Krank<klr tip="ind"> (steredeg ar C'h.)</klr></trd>
  <trdgrp lng="ca">
    <trd>(el) Cranc</trd>,
    <trd>c&agrave;ncer</trd>
  </trdgrp>
  <trd lng="cs">rak</trd>
  <trd lng="de">Krebs</trd>
  <trdgrp lng="en">
    <trd>Cancer</trd>,
    <trd>Crab</trd>
  </trdgrp>
  <trd lng="es">C&aacute;ncer <klr>(constelaci&oacute;n)</klr></trd>
  <trd lng="fr">Cancer<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="he">&mem;&zayin;&lamed; &samekh;&resh;&tet;&fnun;</trd>
  <trd lng="hu">R&aacute;k</trd>
  <trdgrp lng="id">
    <trd>Kanser</trd>,
    <trd>Karkata</trd>,
    <trd>Sartan</trd>
  </trdgrp>
  <trd lng="ja">&#x30B6;&#x30EA;&#x30AC;&#x30CB;  <pr>&#x3056;&#x308A;&#x304C;&#x306B;</pr></trd>
  <trd lng="nl">Kreeft<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trd lng="no">Kreps<klr tip="ind"> (stjernetegn)</klr></trd>
  <trd lng="pl">Rak  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Racul  <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="ru">&c_R;&c_a;&c_k; <klr>(&c_s;&c_o;&c_z;&c_v;&c_je;&c_z;&c_d;&c_i;&c_je;, &c_z;&c_n;&c_a;&c_k;
&c_z;&c_o;&c_d;&c_i;&c_a;&c_k;&c_a;)</klr></trd>
  <trd lng="sk">rak</trd>
  <trd lng="uk">&c_r;&c_a;&#x43A;</trd>
  <trd lng="zh">&#x5DE8;&#x87F9;&#x5EA7; <pr>J&#xF9;xi&#xE8;zu&#xF2;</pr></trd>
</drv>
</art>
