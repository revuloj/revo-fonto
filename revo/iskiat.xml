<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: iskiat.xml,v 1.21 2023/11/19 10:22:14 revo Exp $">
<kap>
  <rad>iskiatik</rad>/o
</kap>
<drv mrk="iskiat.0o">
  <kap><tld/>o</kap>
  <snc mrk="iskiat.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <ref tip="vid" cel="iski.0o">Iskia</ref> nervo:
      <ekz>
        <tld/>a nervo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=253899">Refleksologio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Ischiasnerv</trd>
    <trd lng="fr">nerf sciatique</trd>
    <trd lng="hu">&uuml;l&odblac;ideg</trd>   
    <trd lng="it">nervo <ind>sciatico</ind></trd>
    <trdgrp lng="pt">
      <trd><klr tip="amb">(nervo)</klr> ci&aacute;tico</trd>,
      <trd><klr tip="amb">(nervo)</klr> isqui&aacute;tico</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x5750;&#x9AA8;&#x795E;&#x7D93; <pr>zu&#xF2;g&#x1D4;sh&#xE9;nj&#x12B;ng</pr></trd>,
    <trd>&#x5750;&#x9AA8;&#x795E;&#x7D93;&#x75DB; <pr>zu&#xF2;g&#x1D4;sh&#xE9;nj&#x12B;ngt&#xF2;ng</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="iskiat.0o.MED">
    <uzo tip="fak">MED</uzo>
    <fnt><bib>PV</bib></fnt>
    <ref tip="dif" cel="iskiat1.0o">iskiatalgio</ref>
    <trd lng="de">Ischialgie</trd>
    <trdgrp lng="hu">
      <trd>isi&aacute;sz</trd>,
      <trd>&uuml;l&odblac;idegzs&aacute;ba</trd>
    </trdgrp> 
   <trdgrp lng="it">
    <trd>sciatica<klr> (comune)</klr></trd>,
    <trd>sciatalgia</trd>
   </trdgrp>
    <trd lng="pl">ischias</trd>   
  </snc>
  <trd lng="be">&c_ib;&c_sh;&c_y;&c_ja;&c_s;</trd>
  <trd lng="cs">nerv sedac&iacute;</trd>
  <trd lng="ja">&#x5EA7;&#x9AA8;&#x795E;&#x7D4C;&#x75DB;  <pr>&#x3056;&#x3053;&#x3064;&#x3057;&#x3093;&#x3051;&#x3044;&#x3064;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>ischias-zenuw</trd>,
    <trd>ischias-ziekte</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_sh;&c_i;&c_a;&c_s;</trd>
  <trd lng="sk">sedac&iacute; nerv</trd>
  <adm>
   Trad be, hu, nl, ru: bv kontroli, ke ili plu validas
   post splitigxo de la sencoj (nervo/neuxralgio).
   <aut>MB</aut>
 </adm>
</drv>
</art>
