<drv mrk="ark.paf0o">
  <kap>paf<tld/>o</kap>
  <snc mrk="ark.paf0o.MIL">
    <dif>
      <tld lit="A"/>oforma <ref tip="super" cel="arm.0ilo">armilo</ref>
      por
      pafi <ref tip="vid" cel="sag.0o">sagojn</ref>:
      <ekz>
        prenu do nun viajn armilojn, vian sagujon kaj vian paf<tld/>on,
        kaj iru sur la kampon kaj &ccirc;asu por mi &ccirc;asa&jcirc;on
        <fnt><bib>MT</bib>, <lok>&Gen; 27:3</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        trostre&ccirc;i la paf<tld/>on <klr>(tropostuli)</klr>.
      </ekz>
