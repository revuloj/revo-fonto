<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>perdrik</rad>/o
</kap>
<drv mrk="perdrik.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="fazan.0edoj">fazanedoj</ref>
      (<trd lng="la">Perdix</trd>),
      sova&gcirc;a birdo kun bongusta viando.
    </dif>
  </snc>
  <trd lng="cs">koroptev</trd>
  <trd lng="de">Rebhuhn</trd>
  <trd lng="en">partridge</trd>
  <trd lng="fr">perdrix</trd>
  <trd lng="pl">kuropatwa</trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_o;&c_p;&c_a;&c_t;&c_k;&c_a;</trd>
</drv>
</art>
