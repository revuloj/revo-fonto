<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kazak.xml,v 1.12 2022/03/21 17:51:57 revo Exp $">
<kap>
  <rad>kazak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kazak.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="vest.super0o">Supervesto</ref>
      kovranta la trunkon, penda eventuale &gcirc;is genuoj,
      kun kaptruo, ventra kaj dorsa penda&jcirc;oj,
      ofte senmanika, striktigebla per zono:
      <ekz>
        li portis bluan <tld/>on sen kolumo, kun duonmanikoj kaj kurtaj
        baskoj
        <fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 37a, p.
        284a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_z;&c_a;&c_ch;&c_y;&c_n;&c_a;</trd>
  <trd lng="ca">casaca</trd>
  <trdgrp lng="cs">
    <trd>kazajka</trd>,
    <trd>kazak</trd>
  </trdgrp>
  <trd lng="de">Kasack</trd>
  <trd lng="es">casaca</trd>
  <trd lng="fr">casaque</trd>
  <trd lng="hu">zubbony</trd>
  <trdgrp lng="nl">
    <trd>kazak</trd>,
    <trd>jockeyjasje</trd>
  </trdgrp>
  <trd lng="pl">kozak</trd>     
  <trd lng="ro">cazaci {pl}</trd>
  <trd lng="ru">&c_k;&c_a;&c_z;&c_a;&c_k;&c_i;&c_n;</trd>
  <trd lng="sk">kazajka</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x437;&#x430;&#x43A;&#x438;&#x43D; (&#x447;&#x43E;&#x43B;&#x43E;&#x432;&#x456;&#x447;&#x438;&#x439; &#x43A;&#x430;&#x43F;&#x442;&#x430;&#x43D;)</trd>
</drv>
</art>

