<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc>
<rad>frit</rad>/i</kap>
<drv mrk="frit.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kuir.0i">Kuiri</ref> man&gcirc;a&jcirc;on 
      en varmega oleo a&ubreve; graso:
      <ekz>
        <tld/>itaj terpomoj, fi&scirc;oj.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="rost.0i">rosti</ref>,
    <ref cel="bak.0i">baki</ref>.
  </refgrp>
  <trdgrp lng="de">
    <trd>braten</trd>,
    <trd>frittieren</trd>
  </trdgrp>
  <trd lng="en">fry</trd>
  <trd lng="es">fre&iacute;r</trd>
  <trd lng="fr">frire</trd>
  <trd lng="hu">(zs&iacute;rban) <ind>s&uuml;t</ind></trd>  
  <trd lng="nl">bakken</trd>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_r;&c_i;&c_t;&c_mol; &c_v;
    &c_m;&c_a;&c_s;&c_l;&c_je;</trd>,
    <trd>&c_zh;&c_a;&c_r;&c_i;&c_t;&c_mol; &c_v;&c_o;
    &c_f;&c_r;&c_i;&c_t;&c_ju;&c_r;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">fritera</trd>
</drv>

<drv mrk="frit.0o">
  <kap><tld/>o</kap>
  <fnt><bib>HejmVort</bib></fnt>
  <snc>
    <dif>
      Bastoneto tran&ccirc;ita el terpomo kaj <tld/>ita:
      <ekz>
        en Belgio <tld/>oj estas <tld/>itaj dufoje anta&ubreve; surtabligo.
      </ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="ter.0pomfingro">terpomfingro</ref>
  <trdgrp lng="de">
    <trd>Pommes frites</trd>,
    <trd>Fritten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>French <ind>fry</ind><klr> (Usono)</klr></trd>,
    <trd>potatoe <ind>chip</ind><klr> (Britujo)</klr></trd>
  </trdgrp>
  <trd lng="es"><ind>papas</ind> a la francesa</trd>
  <trd lng="fr">frite<klr tip="ind"> (pommes frites)</klr></trd>
  <trdgrp lng="hu">
    <trd>s&uuml;lt krumpli</trd>,
    <trd>(s&uuml;lt) <ind>has&aacute;bburgonya</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>friet</trd>,
    <trd>frites</trd>,
    <trd>patat<klr tip="ind"> (frites)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol; &c_v;&c_o;
    &c_f;&c_r;&c_i;&c_t;&c_ju;&c_r;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol;
    &c_zh;&c_a;&c_r;&c_je;&c_n;&c_y;&c_j;
    &c_s;&c_o;&c_l;&c_o;&c_m;&c_k;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">pommes frites</trd>
</drv>

<drv mrk="frit.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <tld lit="F"/>ita man&gcirc;a&jcirc;o.
    </dif>
  </snc>
  <trd lng="es">fritura</trd>
  <trd lng="fr">friture</trd>
  <trd lng="hu">s&uuml;lt (&eacute;tel)</trd>  
</drv>

<drv mrk="frit.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Profunda pato por <tld/>i:
      <ekz>
        <ind>elsalti el la <tld/>ilo en la fajron</ind><fnt>Z</fnt>
        <klr>(por eviti malbonon sin &jcirc;eti en alian.
        <ref tip="vid" cel="pluv.0o">pluvo</ref>)</klr>.
        <trd lng="hu">cs&ouml;b&ouml;rb&odblac;l v&ouml;d&ouml;rbe esik</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="es">freidora</trd>
  <trdgrp lng="fr">
    <trd>friteuse</trd>,
    <trd>sauteuse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>serpeny&odblac;</trd>,
    <trd>s&uuml;t&odblac;ed&eacute;ny</trd>,
    <trd>pecsenyes&uuml;t&odblac; (ed&eacute;ny)</trd>
  </trdgrp>
  <trd lng="nl">friteuse</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_o;&c_v;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_f;&c_r;&c_i;&c_t;&c_ju;&c_r;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
</drv>
</art>
