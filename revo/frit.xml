<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: frit.xml,v 1.27 2022/03/20 13:07:55 revo Exp $">
<kap>
  <ofc>1</ofc><rad>frit</rad>/i
</kap>

<drv mrk="frit.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kuir.0i">Kuiri</ref> man&gcirc;a&jcirc;on 
      en varmega oleo a&ubreve; graso:
      <ekz>
        <tld/>itaj terpomoj, fi&scirc;oj.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="rost.0i">rosti</ref>,
    <ref cel="bak.0i">baki</ref>.
  </refgrp>
  <trd lng="be">&c_s;&c_m;&c_a;&c_zh;&c_y;&c_c;&c_mol; <klr>(&c_u; &c_a;&c_l;&c_je;&c_ib;, &c_f;&c_r;&c_y;&c_t;&c_u;&c_r;&c_y;)</klr></trd>
  <trd lng="bg">&c_p;&c_malmol;&c_r;&c_zh;&c_a;<klr tip="amb"> &c_s; &c_m;&c_n;&c_o;&c_g;&c_o;
  &c_m;&c_a;&c_z;&c_n;&c_i;&c_n;&c_a;</klr></trd>
  <trdgrp lng="de">
    <trd>braten</trd>,
    <trd>frittieren</trd>
  </trdgrp>
  <trd lng="en">fry</trd>
  <trd lng="es">fre&iacute;r</trd>
  <trd lng="fr">frire</trd>
  <trd lng="hu">(zs&iacute;rban) <ind>s&uuml;t</ind></trd>  
  <trd lng="nl">bakken</trd>
  <trdgrp lng="pt">
    <trd>fritar</trd>,
    <trd>frigir</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_a;&c_r;&c_i;&c_t;&c_mol; &c_v;
    &c_m;&c_a;&c_s;&c_l;&c_je;</trd>,
    <trd>&c_zh;&c_a;&c_r;&c_i;&c_t;&c_mol; &c_v;&c_o;
    &c_f;&c_r;&c_i;&c_t;&c_ju;&c_r;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">fritera</trd>
  <trd lng="th">&#xE17;&#xE2D;&#xE14;</trd>
  <trd lng="tr">k&inodot;zartmak</trd>
  <trd lng="sk">sma&zcaron;i&tcaron;</trd>
  <trdgrp lng="cs">
    <trd>fritovat</trd>,
    <trd>sma&zcaron;it</trd>,
    <trd>usma&zcaron;it</trd>,
    <trd>&scaron;kva&rcaron;it</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43C;&#x430;&#x436;&#x438;&#x442;&#x438;</trd>,
    <trd>&#x43F;&#x440;&#x430;&#x436;&#x438;&#x442;&#x438; (&#x443; &#x444;&#x440;&#x438;&#x442;&#x44E;&#x440;&#x43D;&#x438;&#x446;&#x456;)</trd>
  </trdgrp>
</drv>

<drv mrk="frit.0o">
  <kap><tld/>o</kap>
  <fnt><bib>HejmVort</bib></fnt>
  <snc>
    <dif>
      Bastoneto tran&ccirc;ita el terpomo kaj <tld/>ita:
      <ekz>
        en Belgio <tld/>oj estas <tld/>itaj dufoje anta&ubreve; surtabligo.
      </ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="ter.0pomfingro">terpomfingro</ref>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_zh;&c_a;&c_n;&c_y;&c_ja; &c_b;&c_u;&c_l;&c_mol;&c_b;&c_a;</trd>,
    <trd>&c_b;&c_u;&c_l;&c_mol;&c_b;&c_a;-&c_f;&c_r;&c_y;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_malmol;&c_r;&c_zh;&c_je;&c_n;&c_i; &c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_i;</trd>
  <trdgrp lng="de">
    <trd>Pommes frites</trd>,
    <trd>Fritten</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>French <ind>fry</ind><klr> (Usono)</klr></trd>,
    <trd>potatoe <ind>chip</ind><klr> (Britujo)</klr></trd>
  </trdgrp>
  <trd lng="es"><ind>papas</ind> a la francesa</trd>
  <trd lng="fr">frite<klr tip="ind"> (pommes frites)</klr></trd>
  <trdgrp lng="hu">
    <trd>s&uuml;lt krumpli</trd>,
    <trd>(s&uuml;lt) <ind>has&aacute;bburgonya</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>friet</trd>,
    <trd>frites</trd>,
    <trd>patat<klr tip="ind"> (frites)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fritas</trd>,
    <trd>batatas fritas</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol; &c_v;&c_o;
    &c_f;&c_r;&c_i;&c_t;&c_ju;&c_r;&c_je;</trd>,
    <trd>&c_k;&c_a;&c_r;&c_t;&c_o;&c_f;&c_je;&c_l;&c_mol;
    &c_zh;&c_a;&c_r;&c_je;&c_n;&c_y;&c_j;
    &c_s;&c_o;&c_l;&c_o;&c_m;&c_k;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">pommes frites</trd>
  <trdgrp lng="th">
    <trd>&#xE21;&#xE31;&#xE19;&#xE1D;&#xE23;&#xE31;&#xE48;&#xE07;&#xE17;&#xE2D;&#xE14;</trd>,
    <trd>&#xE40;&#xE1F;&#xE23;&#xE19;&#xE0A;&#xE4C;&#xE1F;&#xE23;&#xE32;&#xE22;&#xE2A;&#xE4C;</trd>
  </trdgrp>
  <trd lng="tr">k&inodot;zarm&inodot;&scedil; patates</trd>
  <trd lng="sk">frita</trd>
  <trd lng="cs">frita</trd>
</drv>

<drv mrk="frit.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <tld lit="F"/>ita man&gcirc;a&jcirc;o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_m;&c_a;&c_zh;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_s;&c_m;&c_a;&c_zh;&c_a;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_m;&c_a;&c_zh;&c_o;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="es">fritura</trd>
  <trd lng="fr">friture</trd>
  <trd lng="hu">s&uuml;lt (&eacute;tel)</trd>  
  <trd lng="pt">fritura</trd>
<trd lng="th">&#xE02;&#xE2D;&#xE07;&#xE17;&#xE2D;&#xE14;</trd>
  <trdgrp lng="uk">
    <trd>&#x444;&#x440;&#x456;</trd>,
    <trd>&#x43F;&#x435;&#x447;&#x435;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="frit.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Profunda pato por <tld/>i:
      <ekz>
        <ind>elsalti el la <tld/>ilo en la fajron</ind><fnt>Z</fnt>
        <klr>(por eviti malbonon sin &jcirc;eti en alian.
        <ref tip="vid" cel="pluv.0o">pluvo</ref>)</klr>.
        <trd lng="hu">cs&ouml;b&ouml;rb&odblac;l v&ouml;d&ouml;rbe esik</trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_t;&c_e;&c_l;&c_mol;&c_n;&c_ja;</trd>,
    <trd>&c_f;&c_r;&c_y;&c_t;&c_u;&c_r;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_f;&c_r;&c_i;&c_t;&c_ju;&c_r;&c_n;&c_i;&c_k;</trd>
  <trd lng="cs">p&aacute;nev na sma&zcaron;en&iacute;</trd>
  <trd lng="de">Friteuse</trd>
  <trd lng="es">freidora</trd>
  <trdgrp lng="fr">
    <trd>friteuse</trd>,
    <trd>sauteuse</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>serpeny&odblac;</trd>,
    <trd>s&uuml;t&odblac;ed&eacute;ny</trd>,
    <trd>pecsenyes&uuml;t&odblac; (ed&eacute;ny)</trd>
  </trdgrp>
  <trd lng="nl">friteuse</trd>
  <trd lng="pt">frigideira</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_o;&c_v;&c_o;&c_r;&c_o;&c_d;&c_a;</trd>,
    <trd>&c_f;&c_r;&c_i;&c_t;&c_ju;&c_r;&c_n;&c_i;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">pra&zcaron;i&ccaron; <klr>(zariadenie)</klr></trd>
<trd lng="th">&#xE01;&#xE23;&#xE30;&#xE17;&#xE30;</trd>
  <trdgrp lng="uk">
    <trd>&#x444;&#x440;&#x438;&#x442;&#x44E;&#x440;&#x43D;&#x438;&#x446;&#x44F;</trd>,
    <trd>&#x441;&#x43A;&#x43E;&#x432;&#x43E;&#x440;&#x43E;&#x434;&#x430;</trd>
  </trdgrp>
</drv>
</art>
