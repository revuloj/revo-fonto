<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hipoci.xml,v 1.24 2023/11/15 17:08:01 revo Exp $">
<kap>
  <rad>hipocikloid</rad>/o
</kap>
<drv mrk="hipoci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
  <snc>
    <dif>
      Ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>, naskita de
      punkto de cirklo, kiu ruli&gcirc;as interne de alia cirklo:
      <ekz>
        la cikloido, epicikloidoj, kaj <tld/>oj havas la propra&jcirc;on, ke &ccirc;iu estas simila
        al &gcirc;ia evoluto
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=105750">Cikloido</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hipoci.longigita0o">longigita <tld/>o</ref>,
      <ref cel="hipoci.mallongigita0o">mallongigita <tld/>o</ref>;
      <ref cel="cikloi.0o">cikloido</ref>,
      <ref cel="epicik.0o">epicikloido</ref>.
    </refgrp>
  </snc>
  <bld lok="../bld/k_hipocikloido.svg" tip="svg">
    <tld/>oj kun diversaj valoroj de la rilato inter la du radiusoj
  </bld>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_a;&c_c;&c_y;&c_k;&c_l;&c_jo;&c_ib;&c_d;&c_a;</trd>
  <trd lng="bg">&c_h;&c_i;&c_p;&c_o;&c_c;&c_i;&c_k;&c_l;&c_o;&c_i;&c_d;</trd>
  <trd lng="ca">hipocicloide</trd>
  <trd lng="de">Hypozykloide</trd>
  <trd lng="en">hypocycloid</trd>
  <trd lng="fr">hypocyclo&iuml;de</trd>
  <trd lng="hu">hipociklois</trd>  
  <trd lng="ja">&#x5185;&#x30B5;&#x30A4;&#x30AF;&#x30ED;&#x30A4;&#x30C9;  <pr>&#x306A;&#x3044;&#x30B5;&#x30A4;&#x30AF;&#x30ED;&#x30A4;&#x30C9;</pr></trd>
  <trd lng="nl">hypocyclo&iuml;de</trd>
  <trd lng="pl">hipocykloida</trd>
  <trd lng="pt">hipocicl&oacute;ide</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_o;&c_c;&c_i;&c_k;&c_l;&c_o;&c_i;&c_d;&c_a;</trd>
  <trd lng="uk">&c_g;&c_ib;&#x43F;&#x43E;&c_c;&c_i;&#x43A;&#x43B;&#x43E;&c_ji;&c_d;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x5706;&#x5185;&#x65CB;&#x8F6E;&#x7EBF; <pr>yu&#xE1;nn&#xE8;ixu&#xE1;nl&#xFA;nxi&#xE0;n</pr></trd>,
    <trd>&#x53CC;&#x7EBD;&#x7EBF; <pr>shu&#x101;ngni&#x1D4;xi&#xE0;n</pr></trd>,
    <trd>&#x5706;&#x5185;&#x87BA;&#x7EBF; <pr>yu&#xE1;nn&#xE8;ilu&#xF3;xi&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hipoci.longigita0o">
  <kap>longigita <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc>
    <dif>
      Ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>, naskita de
      punkto ligita al <ref cel="cirkl.0o.linio">cirklo</ref> kaj
      ekstera
      al &gcirc;i, kiu cirklo ruli&gcirc;as interne de 
      alia cirklo.
    </dif>
    <ref tip="super" cel="hipotr.0o">hipotro&hcirc;oido.</ref>
  </snc>
  <bld lok="../bld/k_hipotrohxoido.svg" tip="svg">
    longigita kaj mallongigita <tld/>oj
  </bld>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_a;&c_c;&c_y;&c_k;&c_l;&c_jo;&c_ib;&c_d;&c_a; &c_p;&c_a;&c_d;&c_o;&c_w;&c_zh;&c_a;&c_n;&c_a;&c_ja;</trd>
  <trd lng="bg">&c_p;&c_r;&c_o;&c_d;&c_malmol;&c_l;&c_g;&c_o;&c_v;&c_a;&c_t;&c_i; <ind>&c_h;&c_i;&c_p;&c_o;&c_c;&c_i;&c_k;&c_l;&c_o;&c_i;&c_d;</ind></trd>
  <trd lng="ca">hipocicloide allargada</trd>
  <trd lng="de">verl&auml;ngerte <ind>Hypozykloide</ind></trd>
  <trd lng="en">prolate <ind>hypocycloid</ind></trd>
  <trd lng="fr">hypocyclo&iuml;de allong&eacute;e</trd>
  <trd lng="hu">ny&uacute;jtott <ind>hipociklois</ind></trd>  
  <trd lng="nl">verlengde <ind>hypocyclo&iuml;de</ind></trd>
  <trd lng="pl">hipocykloida wyd&lstroke;u&zdot;ona</trd>
  <trd lng="ru">&c_u;&c_d;&c_l;&c_i;&c_n;&c_jo;&c_n;&c_n;&c_a;&c_ja;
  <ind>&c_g;&c_i;&c_p;&c_o;&c_c;&c_i;&c_k;&c_l;&c_o;&c_i;&c_d;&c_a;</ind></trd>
</drv>

<drv mrk="hipoci.mallongigita0o">
  <kap>mallongigita <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      Ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>, naskita de
      punkto ligita al <ref cel="cirkl.0o.linio">cirklo</ref> kaj
      interna al &gcirc;i, kiu cirklo ruli&gcirc;as interne de 
      alia cirklo.
    </dif>
    <ref tip="super" cel="hipotr.0o">hipotro&hcirc;oido.</ref>
    <rim>
      Ekzistas anka&ubreve; la formo <ctl>kurtigita <tld/>o</ctl>, ekz-e
      en <fnt><bib>MatVort</bib></fnt>.
    </rim>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_p;&c_a;&c_c;&c_y;&c_k;&c_l;&c_jo;&c_ib;&c_d;&c_a; &c_p;&c_a;&c_k;&c_a;&c_r;&c_o;&c_ch;&c_a;&c_n;&c_a;&c_ja;</trd>
  <trd lng="bg">&c_s;&c_malmol;&c_k;&c_r;&c_a;&c_t;&c_je;&c_n; <ind>&c_h;&c_i;&c_p;&c_o;&c_c;&c_i;&c_k;&c_l;&c_o;&c_i;&c_d;</ind></trd>
  <trd lng="ca">hipocicloide escur&ccedil;ada</trd>
  <trd lng="de">verk&uuml;rzte <ind>Hypozykloide</ind></trd>
  <trd lng="en">curtate <ind>hypocycloid</ind></trd>
  <trd lng="fr">hypocyclo&iuml;de raccourcie</trd>
  <trd lng="hu">kurt&iacute;tott <ind>hipociklois</ind></trd>
  <trd lng="nl">verkorte <ind>hypocyclo&iuml;de</ind></trd>
  <trd lng="pl">hypocykloida skr&oacute;cona</trd>
  <trd lng="ru">&c_u;&c_k;&c_o;&c_r;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_a;&c_ja;
  <ind>&c_g;&c_i;&c_p;&c_o;&c_c;&c_i;&c_k;&c_l;&c_o;&c_i;&c_d;&c_a;</ind></trd>
</drv>
</art>
