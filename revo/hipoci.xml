<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>hipocikloid</rad>/o
</kap>
<drv mrk="hipoci.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 35</lok></fnt>
  <snc>
    <dif>
      Ebena <ref tip="super" cel="kurb.0o.MAT">kurbo</ref>, naskita de punkto 
      de cirklo, kiu ruli&gcirc;as interne de alia cirklo.
    </dif>
    <refgrp tip="vid">
      <ref cel="cikloi.0o">cikloido</ref>,
      <ref cel="epicik.0o">epicikloido</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Hypozykloide</trd>
  <trd lng="en">hypocycloid</trd>
  <trd lng="fr">hypocyclo&iuml;de</trd>
  <trd lng="nl">hypocycloide</trd>
  <trd lng="ru">&c_g;&c_i;&c_p;&c_o;&c_c;&c_i;&c_k;&c_l;&c_o;&c_i;&c_d;&c_a;</trd>
</drv>
</art>
