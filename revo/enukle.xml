<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: enukle.xml,v 1.17 2023/11/07 20:34:11 revo Exp $">
<kap>
  <rad>enukle</rad>/i <fnt><bib>PV</bib></fnt>,
  <var><kap><rad>enukleaci</rad>/i <fnt><bib>PV</bib></fnt></kap></var>
</kap>
<drv mrk="enukle.0i">
  <kap><tld/>i, <var><kap>enukleacii</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="enukle.0i.KIR">
    <uzo tip="fak">KIR</uzo>
    <dif>
      Eltran&ccirc;i kunradike tumoron a&ubreve; okulon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_s;&c_k;&c_r;&c_a;&c_b;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_l;&c_u;&c_sh;&c_ch;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">enukleovat</trd>
  <trdgrp lng="de">
    <trd>aussch&auml;len</trd>
  </trdgrp>
  <trd lng="fr">&eacute;nucl&eacute;er</trd>
  <trdgrp lng="hu">
    <trd>enukle&aacute;l</trd>,
    <trd>tumort/szemgoly&oacute;t <ind>kih&aacute;moz</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wy&lstroke;uskiwa&cacute;</trd>,
    <trd>wy&lstroke;upia&cacute;<klr> (oko)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>enuclear</trd>,
    <trd>descapsular</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_l;&c_u;&c_shch;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_k;&c_a;&c_b;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">enukelova&tcaron;</trd>
</drv>

<drv mrk="enukle.0ado">
  <kap><tld/>ado, <var><kap>enukleacio</kap></var></kap>
  <snc mrk="enukle.0ado.KIR">
    <uzo tip="fak">KIR</uzo>
    <dif>
      Metodo a&ubreve; ago <tld/>i tumoron a&ubreve; okulon:
      <ekz>
        kiel kirurgoj kompetentaj en la kuracado a&ubreve; <tld/>ado
        <fnt>
          <aut>Leonardo Cassanho Forster</aut>
          <vrk><url ref="http://www.oconsolador.com.br/ano9/414/esperanto.html"> Ago kaj Reago</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_k;&c_r;&c_a;&c_b;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_y;&c_l;&c_u;&c_sh;&c_ch;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="cs">enukleace <klr>(bulvu)</klr></trd>
  <trdgrp lng="de">
    <trd>Aussch&auml;len</trd>,
    <trd>Enukleation<klr> [fak]</klr></trd>
  </trdgrp>
  <trd lng="fr">&eacute;nucl&eacute;ation</trd>
  <trd lng="hu">enucleation</trd>
  <trd lng="pl">wy&lstroke;uszczanie</trd>
  <trd lng="pt">enuclea&ccedil;&atilde;o</trd>
  <trd lng="sk">vy&ncaron;atie oka</trd>
  <trdgrp lng="zh">
    <trd>&#x773C;&#x7403;&#x6458;&#x9664;&#x672F; <pr>y&#x1CE;nqi&#xFA;zh&#x101;ich&#xFA;sh&#xF9;</pr></trd>,
    <trd>&#x773C;&#x7403;&#x6458;&#x9664;&#x672F; <pr>y&#x1CE;nqi&#xFA;zh&#x101;ich&#xFA;sh&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
