<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: tombak.xml,v 1.16 2018/11/03 11:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tombak</rad>/o
</kap>
<drv mrk="tombak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="latun.0o">Latuno</ref>
      (kunfanda&jcirc;o el <ref tip="prt" cel="kupr.0o">kupro</ref>
      kaj <ref tip="prt" cel="zink.0o">zinko</ref>) kun ru&gcirc;e flava koloro,
      havanta inter 5 kaj 28 procentoj da zinko:
      <ekz>
        oni produktis anka&ubreve; samovarojn latunajn a&ubreve; el <tld/>o, kio estis alojo de kupro kaj 
        zinko de ru&gcirc;simila koloro, el fero, el &scirc;talo kaj e&ccirc; el porcelano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=52026">Samovaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/8/82/Germania_10_pfennig.JPG/396px-Germania_10_pfennig.JPG"
      prm="PD"
      >moneroj kun ekstera tavolo el <tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Germania_10_pfennig.JPG"
              >&WCW;/File:Germania_10_pfennig.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">tombac</trd>
  <trd lng="cs">tombak</trd>
  <trd lng="de">Tombak</trd>
  <trdgrp lng="fr">
  <trd>tombac</trd>,
  <trd>tombak</trd>
</trdgrp>
  <trdgrp lng="hu">
    <trd>tombak</trd>,
    <trd>r&eacute;zarany</trd>
  </trdgrp>
  <trd lng="nl">tombak</trd>
  <trdgrp lng="pl">
    <trd>tombak</trd>,
    <trd>fa&lstroke;szywe <ind>z&lstroke;oto</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pechisbeque</trd>,
    <trd>ouro falso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_m;&c_p;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_a;&c_ja;
    <ind>&c_l;&c_a;&c_t;&c_u;&c_n;&c_mol;</ind></trd>
  </trdgrp>
  <trd lng="sk">tombak</trd>
</drv>
</art>
