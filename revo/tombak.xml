<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tombak.xml,v 1.22 2025/12/28 20:35:00 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>tombak</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="tombak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="latun.0o">Latuno</ref>
      (kunfanda&jcirc;o el <ref tip="prt" cel="kupr.0o">kupro</ref>
      kaj <ref tip="prt" cel="zink.0o">zinko</ref>) kun ru&gcirc;e flava koloro,
      havanta inter 5 kaj 28 procentoj da zinko:
      <ekz>
        oni produktis anka&ubreve; samovarojn latunajn a&ubreve; el <tld/>o, kio estis alojo de kupro kaj
        zinko de ru&gcirc;simila koloro, el fero, el &scirc;talo kaj e&ccirc; el porcelano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=52026">Samovaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/8/82/Germania_10_pfennig.JPG/396px-Germania_10_pfennig.JPG" prm="PD">moneroj kun ekstera tavolo el <tld/>o
        <fnt>
          <lok><url ref="&WCW;/File:Germania_10_pfennig.JPG">&WCW;/File:Germania_10_pfennig.JPG</url></lok>
        </fnt>
    </bld>
  <trdgrp lng="en">
    <trd>tombak</trd>,
    <trd>Dutch <ind>brass</ind></trd>,
    <trd>German <ind>brass</ind></trd>
  </trdgrp>
  </snc>
    <trdgrp lng="be">
    <trd>&c_t;&c_a;&c_m;&c_b;&c_a;&c_k;</trd>,
    <trd>&c_t;&c_a;&c_m;&c_p;&c_a;&c_k;</trd>
  </trdgrp>
  <trd lng="ca">tombac</trd>
  <trd lng="cs">tombak</trd>
  <trd lng="de">Tombak</trd>
  <trdgrp lng="fr">
  <trd>tombac</trd>,
  <trd>tombak</trd>
</trdgrp>
  <trdgrp lng="hu">
    <trd>tombak</trd>,
    <trd>r&eacute;zarany</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C8;&#x30E0;&#x30D0;&#x30C3;&#x30AF;</trd>,
    <trd>&#x4EBA;&#x9020;&#x91D1;  <pr>&#x3058;&#x3093;&#x305E;&#x3046;&#x304D;&#x3093;</pr></trd>,
    <trd>&#x6A21;&#x9020;&#x54C1;  <pr>&#x3082;&#x305E;&#x3046;&#x3072;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">tombak</trd>
  <trdgrp lng="pl">
    <trd>tombak</trd>,
    <trd>fa&lstroke;szywe <ind>z&lstroke;oto</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pechisbeque</trd>,
    <trd>ouro falso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_m;&c_p;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_a;&c_ja;
    <ind>&c_l;&c_a;&c_t;&c_u;&c_n;&c_mol;</ind></trd>
  </trdgrp>
  <trd lng="sk">tombak</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_o;&c_m;&c_p;&c_a;&c_k;</trd>,
    <trd>&c_ch;&c_je;&c_r;&c_v;&c_o;&c_n;&c_a; &c_l;&c_a;&c_t;&c_u;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;. &c_d;&c_je;&c_sh;&c_je;&c_v;&c_a; &c_p;&c_ib;&c_d;&c_r;&c_o;&c_b;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
