<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tombak</rad>/o
</kap>
<drv mrk="tombak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="latun.0o">Latuno</ref>
      (kunfanda&jcirc;o el kupro kaj zinko) kun ru&gcirc;flava koloro,
      havanta
      inter 5 kaj 28 elcentoj da zinko.
    </dif>
  </snc>
  <trd lng="ca">tombac</trd>
  <trd lng="de">Tombak</trd>
  <trdgrp lng="hu">
    <trd>tombak</trd>,
    <trd>r&eacute;zarany</trd>
  </trdgrp>
  <trd lng="nl">tombak</trd>
  <trdgrp lng="pl">
    <trd>tombak</trd>,
    <trd>fa&lstroke;szywe <ind>z&lstroke;oto</ind></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>pechisbeque</trd>,
    <trd>ouro falso</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_m;&c_p;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_s;&c_n;&c_a;&c_ja;
    <ind>&c_l;&c_a;&c_t;&c_u;&c_n;&c_mol;</ind></trd>
  </trdgrp>
</drv>
</art>
