<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>silur</rad>/o
</kap>
<drv mrk="silur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Fi&scirc;o vivanta en riveroj kaj en maro, povanta atingi longon de 5
      metroj kaj havanta la proprecon produkti elektron kaj ka&ubreve;zi
      elektrajn skuojn (<trd lng="la">Silurus</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_o;&c_m;</trd>
  <trd lng="de">Wels</trd>
  <trd lng="fa">&a_A;&a_s;&a_b;&a_l;&a_h;</trd>
  <trd lng="hu">harcsa</trd>
  <trd lng="nl">meerval</trd>
  <trd lng="ru">&c_s;&c_o;&c_m;</trd>
</drv>
</art>
