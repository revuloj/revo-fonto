<drv mrk="terur.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="terur.0igxi.ek">
    <dif>
      Eksenti <tld/>on<sncref ref="terur.0o.timego"/>:
      <ekz>
        tiam, li eksciti&gcirc;as, <tld/>i&gcirc;as, panikas, kaj kure foriras
        mensogante al la gardisto&#x2026;
        <fnt><aut>Joh&aacute;n Balano</aut>:
           <vrk>&Ccirc;u ni kunvenis vane?</vrk>.
           <lok>&Ccirc;ap. 20&#xAA;.</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_u;&c_zh;&c_a;&c_s;&c_n;&c_u;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </snc>
  <snc mrk="terur.0igxi.stati">
    <dif>
      Esti en <tld/>o<sncref ref="terur.0o.timego"/>:
      <ekz>
        vi <tld/>i&gcirc;as pri tio, ke ni volas forigi la privatan
        proprieton
        <fnt>
          <aut>K. Markso kaj F. Engelso, tr. Vilhelmo Lutermano</aut>:
          <vrk>Manifesto de la Komunista Partio</vrk>.
          <lok>Monda Asembleo Socia, 2015, ISBN 978-2-918300-91-5</lok>
        </fnt>;
      </ekz><ekz>
        mi pli kaj pli <tld/>i&gcirc;is pro la rigida kaj senindulga
        filozofio, por kiu la vivo de la homo estas neniel pli valora
        ol tiu de la bruto
        <fnt>
          <aut>S&aacute;ndor Szathm&aacute;ri</aut>:
          <vrk>Voja&gcirc;o al Kazohinio</vrk>. <lok>1938. &Ccirc;ap. 6&#xAA;</lok>
        </fnt>;
      </ekz>
    </dif>
    <trd lng="ru">&c_b;&c_y;&c_t;&c_mol; &c_v; <ind>&c_u;&c_zh;&c_a;&c_s;&c_je;</ind></trd>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_h;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="id">
    <trd>cemas</trd>,
    <trd>khawatir</trd>,
    <trd>takut</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6050;&#x308C;&#x308B;  <pr>&#x304A;&#x305D;&#x308C;&#x308B;</pr></trd>,
    <trd>&#x9707;&#x3048;&#x3042;&#x304C;&#x308B;  <pr>&#x3075;&#x308B;&#x3048;&#x3042;&#x304C;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>poczu&cacute; nagle  <ind>przera&zdot;enie</ind></trd>,
    <trd>przerazi&cacute; si&eogonek;</trd>
  </trdgrp>
