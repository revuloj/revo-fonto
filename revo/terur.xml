<kap>
  <ofc>*</ofc>
  <rad>terur</rad>/o
</kap>

<drv mrk="terur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="terur.0o.timego">
    <dif>
      Konsterna, stuporiga <ref tip="sin" cel="tim.0ego">timego</ref>:
      <ekz>
        grandega hundo metis sur min sian anta&ubreve;an piedegon, kaj mi de
        <tld/>o ne sciis, kion fari
        <fnt><bib>F</bib>, <lok>&FE;27.</lok></fnt>;
      </ekz><ekz>
        muta de <tld/>o <fnt><bib>VdE</bib></fnt>;
      </ekz><ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>tra vitro de <tld/>o pligrandi&gcirc;as la mezuro</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">a <ind>f&eacute;lelem</ind> nagy&iacute;t&oacute;n &aacute;t n&eacute;z</trd>
        <trd lng="ru">&c_u; <ind>&c_s;&c_t;&c_r;&c_a;&c_h;&c_a;</ind> &c_g;&c_l;&c_a;&c_z;&c_a; &c_v;&c_je;&c_l;&c_i;&c_k;&c_i;</trd>
      </ekz><ekz>
        <uzo tip="stl">FRAZ</uzo>
        atakis <tld/>o, ektremis la kruro.
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
    <trd lng="be">&c_zh;&c_a;&c_h;</trd>
    <trd lng="cs">hr&uring;za</trd>
    <trd lng="de">Schrecken</trd>
    <trdgrp lng="en">
      <trd>terror</trd>,
      <trd>fright</trd>,
      <trd>fear</trd>
    </trdgrp>
    <trd lng="es">terror</trd>
    <trd lng="fr">terreur<klr> (sentiment)</klr></trd>
    <trd lng="hu">r&eacute;m&uuml;let</trd>
    <trdgrp lng="id">
      <trd>teror</trd>,
      <trd><baz>takut</baz>ketakutan</trd>,
      <trd><baz>ngeri</baz>kengerian</trd>
    </trdgrp>
    <trd lng="it">terrore</trd>
    <trdgrp lng="nl">
      <trd>schrik</trd>,
      <trd>ontzetting</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>przera&zdot;enie</trd>,
      <trd>zgroza</trd>,
      <trd>trwoga</trd>,
      <trd>terror</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>terror</trd>,
      <trd>horror</trd>
    </trdgrp>
    <trd lng="zh">&#x6016; <pr>b&#xF9;</pr></trd>
</snc>
  <snc mrk="terur.0o.hororo">
    <dif>
      &Scirc;oko, konsterni&gcirc;o ka&ubreve;zita de ies malbona situacio a&ubreve; stato (ne
      nepre de tiu de la spertanto), eventuale miksita kun abomeno,
      indigni&gcirc;o a&ubreve; kompato:
      <ekz>
         la viza&gcirc;oj, pali&gcirc;intaj de <tld/>o <klr>&#x2026;</klr> klini&gcirc;is super
         la malhela figuro, kiu <klr>&#x2026;</klr> ku&scirc;is sur la blanka ne&gcirc;a
         sterna&jcirc;o <klr>(temas pri la kadavro de Marta)</klr>
         <fnt><bib>Marta</bib></fnt>;
      </ekz><ekz>
        tremo de <tld/>o trakuradis lin je la nura penso, ke <nom>Ligia</nom>
        povus trovi&gcirc;i meze de tiu &hcirc;aoso, sur tiuj teruraj stratoj,
        kie oni dispremadis sur la tero homajn interna&jcirc;ojn
        <fnt>
          <aut>H. Sienkewicz, tr. Lidja Zamenhof</aut>:
          <vrk>Quo vadis</vrk>, <lok>&Ccirc;ap. 45&#xAA;</lok>
        </fnt>;
      </ekz><ekz>
        kia estis mia surprizo, mia <tld/>o, mia maltrankvilo, kiam mi
        vidis en &leftquot;san-benito&rightquot; kaj sub mitro, viza&gcirc;on, kiu similis al
        tiu de <nom>Panglos</nom>! <klr>(t.e. &scirc;i vidis, ke Pangloso estas
        ekzekutota en a&ubreve;todafeo)</klr>
        <fnt><aut>Voltero, trad. Lanti</aut>:
          <vrk>Kandid a&ubreve; la Optimismo</vrk>.
          <lok>SAT: Paris, 1929. &Ccirc;ap. 8&#xAA;</lok>
        </fnt>;
      </ekz><ekz>
        kaj tiuj fieraj homoj tute rigidi&gcirc;is de <tld/>o, ke tia
        maleminenta persono kura&gcirc;as sidi meze de ili &ccirc;e la tablo
        <fnt><bib>Fab3</bib>, <lok>&leftquot;&Ccirc;io sur sian &gcirc;ustan lokon!&rightquot;</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="horor.0o.abomeno">hororo<sncref/></ref>
    <trd lng="en">horror</trd>
  <trd lng="id">hororo</trd>
  </snc>
  <snc mrk="terur.0o._ajxo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="terur.0ajxo"><tld lit="T"/>a&jcirc;o</ref>
      <klr>(uzo metonimia)</klr>:
      <ekz>
        finfine li fari&gcirc;is la <tld/>o de la vila&gcirc;o, kaj oni fu&gcirc;is je
        lia proksimi&gcirc;o, &ccirc;ar li estas viro treege forta, kaj tute
        neregebla en kolero
        <fnt>
          <aut>A. Conan Doyle, tr. Hoss Firooznia</aut>:
          <vrk>La makulita bendo</vrk>
          <lok>La aventuroj de &Scirc;erloko Holmso. Kaliningrado: Sezonoj, 2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bo">&t_da;&t_nga;&t_nga;&t_sa;&t_tsheg;&t_-a;&t_tsha;&t_ba;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>hr&uring;za</trd>,
    <trd>postrach</trd>,
    <trd>zd&ecaron;&scaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6050;&#x6016;  <pr>&#x304D;&#x3087;&#x3046;&#x3075;</pr></trd>,
    <trd>&#x975E;&#x5E38;&#x306A;&#x6050;&#x308D;&#x3057;&#x3055;  <pr>&#x3072;&#x3058;&#x3087;&#x3046;&#x306A;&#x304A;&#x305D;&#x308D;&#x3057;&#x3055;</pr></trd>,
    <trd>&#x8105;&#x5A01;  <pr>&#x304D;&#x3087;&#x3046;&#x3044;</pr></trd>,
    <trd>&#x6050;&#x6016;&#x306E;&#x7684;  <pr>&#x304D;&#x3087;&#x3046;&#x3075;&#x306E;&#x307E;&#x3068;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hr&ocirc;za</trd>,
    <trd>postrach</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_zh;&c_a;&c_s;</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_a;&c_h;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_h;</trd>
  </trdgrp>
</drv>

<drv mrk="terur.0a">
  <kap><tld/>a <fnt><bib>VdE</bib></fnt></kap>
  <snc mrk="terur.0a.drv">
    <dif>
      Rilata al <tld/>o:
    </dif>
    <subsnc mrk="terur.0a.timiga">
      <ekz>
        <tld/>a akcidento;
      </ekz><ekz>
        <uzo tip="stl">FRAZ</uzo>
        ne tiel <tld/>a estas la diablo, kiel oni lin pentras
        <fnt><bib>DL</bib></fnt>.
      </ekz>
      <trdgrp lng="ru">
        <trd>&c_zh;&c_u;&c_t;&c_k;&c_i;&c_j;</trd>,
        <trd>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_u;&c_zh;&c_a;&c_s;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
        <trd>&c_s;&c_t;&c_r;&c_a;&c_sh;&c_n;&c_y;&c_j;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="terur.0a.konsterna">
      <ekz>
         kortu&scirc;ate de kompato pli ol de abomeno &ccirc;e la vido de tiu
         <tld/>a mizerula&ccirc;o, <nom>Kandid</nom> donis al li la du florenojn
         <fnt><aut>Voltero, trad. Lanti</aut>:
          <vrk>Kandid a&ubreve; la Optimismo</vrk>.
          <lok>SAT: Paris, 1929. &Ccirc;ap. 4&#xAA;</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>a estas la stato de la aferoj<fnt>Z</fnt>;
      </ekz><ekz>
        tiuj kalumnioj donis <tld/>ajn fruktojn<fnt>Z</fnt>.
      </ekz>
      <trdgrp lng="ru">
        <trd>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
        <trd>&c_p;&c_o;&c_t;&c_r;&c_ja;&c_s;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>
      </trdgrp>
    </subsnc>
    <trdgrp lng="id">
      <trd>ngeri</trd>,
      <trd><baz>ngeri</baz>mengerikan</trd>,
      <trd><baz>takut</baz>menakutkan</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>przera&zdot;aj&aogonek;cy</trd>,
      <trd>przera&zacute;liwy</trd>,
      <trd>wiej&aogonek;cy <ind>groz&aogonek;</ind></trd>,
      <trd>zatrwa&zdot;aj&aogonek;cy</trd>,
      <trd>okropny</trd>
    </trdgrp>
  </snc>
  <snc mrk="terur.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre forta:
      <ekz>
        &scirc;i havis <tld/>an malamon kontra&ubreve; la pli juna <klr>[filino]</klr>
        <fnt><bib>F</bib>, <lok>&FE;13.</lok></fnt>;
      </ekz><ekz>
        &scirc;i tordi&gcirc;is kiel iu, kiu suferas <tld/>an doloron, kaj &scirc;iaj
        membroj konvulsiis <tld/>e
        <fnt><aut>A. Conan Doyle, tr. Hoss Firooznia</aut>:
          <vrk>La makulita bendo</vrk>
          <lok>La aventuroj de &Scirc;erloko Holmso. Kaliningrado: Sezonoj, 2014</lok>
        </fnt>;
      </ekz><ekz>
        estus <tld/>a eraro ju&gcirc;i la francojn la&ubreve; tiuj strangaj specimenoj
        <fnt><bib>BonaLingvo</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="id">dahsyat</trd>
    <trdgrp lng="pl">
      <trd>przera&zdot;aj&aogonek;cy</trd>,
      <trd>zatrwa&zdot;aj&aogonek;cy</trd>,
      <trd>straszny</trd>,
      <trd>horendalny</trd>,
      <trd>okropny</trd>,
      <trd>wstrz&aogonek;saj&aogonek;cy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_k;&c_r;&c_a;&c_j;&c_n;&c_i;&c_j;</trd>,
      <trd>&c_s;&c_i;&c_l;&c_mol;&c_n;&c_je;&c_j;&c_sh;&c_i;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="terur.0a.malbonega">
    <dif>
      <ref tip="dif" cel="bon.mal0ega">Malbonega</ref>:
      <ekz>
        unu vesperon, kiam li sidis hejme, ekstere fari&gcirc;is <tld/>a
        malbonvetero
        <fnt><bib>Fab1</bib>, <lok>Malbonkonduta knabo</lok></fnt>;
      </ekz><ekz>
        &ccirc;io supermezura estas <tld/>a <fnt><bib>PrV</bib></fnt>;
      </ekz><ekz>
        subite lin trafis ia <tld/>a misfarto
        <fnt><bib>MkM</bib>, <lok>&ccirc;ap. 16&#xAA;</lok></fnt>;
      </ekz><ekz>
        mi sorbis &gcirc;in, sulkigis la nazon pro la <tld/>a gusto, kaj
        ver&scirc;is la reston en la defluejon
        <fnt>
          <aut>Harry Harrison, tr. R. Fossmeier k.a.</aut>:
          <vrk>Naski&gcirc;o de la Rustimuna &Scirc;talrato</vrk>.
          <lok>Jekaterinburg: Sezonoj, 1996. &Ccirc;ap. 8&#xAA;.</lok>
        </fnt>;
      </ekz><ekz>
        kiuj tamen kura&gcirc;as, tiuj elbu&scirc;igas apartan personan dialekton,
        eble &ccirc;arman kaj amuzan, eble <tld/>an kaj suferigan
        <fnt><aut>Edmond Privat</aut>:
          <vrk>Esprimo de sentoj en Esperanto</vrk></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="id">
      <trd><ind>sangat</ind> buruk</trd>,
      <trd><ind>buruk</ind> sekali</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_t;&c_v;&c_r;&c_a;&c_t;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_h;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_sh;&c_je;&c_n;</trd>,
    <trd>&c_u;&c_zh;&c_a;&c_s;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="bo">&t_ra;&t_mata;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>d&ecaron;sn&yacute;</trd>,
    <trd>hrozn&yacute;</trd>,
    <trd>hr&uring;zn&yacute;</trd>,
    <trd>p&rcaron;&iacute;&scaron;ern&yacute;</trd>
  </trdgrp>
  <trd lng="de">schrecklich</trd>
  <trd lng="en">terrible</trd>
  <trd lng="es">terrible</trd>
  <trdgrp lng="ja">
    <trd>&#x6050;&#x308D;&#x3057;&#x3044;  <pr>&#x304A;&#x305D;&#x308D;&#x3057;&#x3044;</pr></trd>,
    <trd>&#x6016;&#x3044;  <pr>&#x3053;&#x308F;&#x3044;</pr></trd>,
    <trd>&#x3072;&#x3069;&#x3044;</trd>,
    <trd>&#x3082;&#x306E;&#x3059;&#x3054;&#x3044;</trd>,
    <trd>&#x6FC0;&#x3057;&#x3044;  <pr>&#x306F;&#x3052;&#x3057;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="ms">dahsyat</trd>
  <trdgrp lng="fr">
    <trd>terrible</trd>,
    <trd>terrifiant</trd>
  </trdgrp>
  <trdgrp lng="hu">
      <trd>r&eacute;m&iacute;t&odblac;</trd>,
      <trd>sz&ouml;rny&udblac;</trd>
      <trd>iszonyatos</trd>,
      <trd>rettenetes</trd>
    </trdgrp>
  <trdgrp lng="it">
      <trd>terribile</trd>,
      <trd>terrificante</trd>
    </trdgrp>
  <trdgrp lng="nl">
    <trd>verschrikkelijk</trd>,
    <trd>ontzettend</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>desiv&yacute;</trd>,
    <trd>hrozn&yacute;</trd>,
    <trd>stra&scaron;n&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_zh;&c_a;&c_h;&#x43B;&c_i;&c_v;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x53EF;&#x6015; <pr>k&#x11B;p&#xE0;</pr></trd>,
    <trd>&#x4E86;&#x4E0D;&#x5F97; <pr>li&#x1CE;obude</pr></trd>,
    <trd>&#x5389;&#x5BB3; <pr>l&#xEC;hai</pr></trd>,
    <trd>&#x53B2;&#x5BB3; <pr>l&#xEC;hai</pr></trd>,
    <trd>&#x6050;&#x6016; <pr>k&#x1D2;ngb&#xF9;</pr></trd>,
    <trd>&#x591F;&#x545B; <pr>g&#xF2;uqi&#xE0;ng</pr></trd>,
    <trd>&#x5920;&#x55C6; <pr>g&#xF2;uqi&#xE0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="terur.0e">
  <kap><tld/>e</kap>
  <snc mrk="terur.0e.timige">
    <dif>
      En maniero <tld/>a<sncref ref="terur.0a.drv"/>:
      <ekz>
        li <tld/>e punis la kulpulojn;
      </ekz><ekz>
        <ctl>terure!</ctl> ekkriis la re&gcirc;idino; <ctl>oni &gcirc;in ja en&scirc;losis?</ctl>
        <fnt><bib>Fab2</bib>, <lok>La ombro</lok></fnt>
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>przera&zdot;aj&aogonek;co</trd>,
      <trd>budz&aogonek;c <ind>przera&zdot;enie</ind></trd>,
      <trd>budz&aogonek;c <ind>zgroz&eogonek;</ind></trd>,
      <trd>zatrwa&zdot;aj&aogonek;co</trd>,
      <trd>okropnie</trd>
    </trdgrp>
  </snc>
  <snc mrk="terur.0e.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre forte, <ref tip="super" cel="ekstre.0e">ekstreme</ref>, eksterordinare:
      <ekz>
        <tld/>e gajaj &scirc;ajnis al ili iliaj propraj
        &scirc;ercoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Dio de dormo</lok>
        </fnt>;
      </ekz><ekz>
        vi trovos &ccirc;ion <tld/>e malorda;
      </ekz>
      <ekz>
        <tld/>e kiel mi volas man&gcirc;i
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto dua</lok>
        </fnt>!
      </ekz>
      <ekz>
        &scirc;i estis <tld/>e malbela, &scirc;ia malsupra lipo pendis &gcirc;is la brusto
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la birdo estis ja <tld/>e granda
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>e sendanka
        <fnt>
          <bib>Fab2</bib>
          <lok>La najtingalo</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>e sola
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia knabo ku&scirc;as paralizulo en la domo, kaj <tld/>e suferas
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 8:6</lok>
        </fnt>;
      </ekz><ekz>
        <tld/>e malbona vetero
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz><ekz>
        <klr>[tiu]</klr> forto de inercio <klr>[&#x2026;]</klr> <tld/>e malhelpis &ccirc;iun nian
        pa&scirc;on
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Tria Kongreso Esperantista en Cambridge en la 12a de a&ubreve;gusto
          1907</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>przera&zdot;aj&aogonek;co</trd>,
      <trd>zatrwa&zdot;aj&aogonek;co</trd>,
      <trd>strasznie</trd>,
      <trd>horendalnie</trd>,
      <trd>okropnie</trd>,
      <trd>wstrz&aogonek;saj&aogonek;co</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_h;&c_l;&c_ib;&c_v;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>hrozn&ecaron;</trd>,
    <trd>hr&uring;zn&ecaron;</trd>
  </trdgrp>
  <trd lng="de">schrecklich</trd>
  <trd lng="en">terribly</trd>
  <trd lng="es">terriblemente</trd>
  <trd lng="fr">terriblement</trd>
  <trdgrp lng="hu">
    <trd>rettenetesen</trd>,
    <trd>sz&ouml;rnyen</trd>
  </trdgrp>
  <trd lng="it">terribilmente</trd>
  <trdgrp lng="ja">
    <trd>&#x6050;&#x308D;&#x3057;&#x304F;  <pr>&#x304A;&#x305D;&#x308D;&#x3057;&#x304F;</pr></trd>,
    <trd>&#x3072;&#x3069;&#x304F;</trd>,
    <trd>&#x306F;&#x306A;&#x306F;&#x3060;&#x3057;&#x304F;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verschrikkelijk</trd>,
