<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: terur.xml,v 1.31 2021/05/29 14:33:21 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>terur</rad>/o
</kap>

<drv mrk="terur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="terur.0o.KOMUNE">
    <dif>
      Timego:
      <ekz>
        <ind>tra vitro de <tld/>o pligrandi&gcirc;as la mezuro</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">a <ind>f&eacute;lelem</ind> nagy&iacute;t&oacute;n
        &aacute;t n&eacute;z</trd>
      </ekz>
      <ekz>
        atakis <tld/>o, ektremis la kruro
        <fnt><bib>PrV</bib></fnt>
      </ekz>
      <ref tip="vid" cel="horor.0o.KOMUNE">hororo</ref>,
    </dif>
  <trd lng="be">&c_zh;&c_a;&c_h;</trd>
  <trd lng="cs">hr&uring;za</trd>
  <trd lng="de">Schrecken</trd>
  <trdgrp lng="en">
    <trd>terror</trd>,
    <trd>fright</trd>,
    <trd>fear</trd>
  </trdgrp>
  <trd lng="es">terror</trd>
  <trd lng="fr">terreur<klr> (sentiment)</klr></trd>
  <trd lng="hu">r&eacute;m&uuml;let</trd>  
  <trd lng="it">terrore</trd>  
  <trdgrp lng="nl">
    <trd>schrik</trd>,
    <trd>ontzetting</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przera&zdot;enie</trd>,
    <trd>zgroza</trd>,
    <trd>trwoga</trd>,
    <trd>terror</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>terror</trd>,
    <trd>horror</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_zh;&c_a;&c_s;</trd>
  </snc>
  <trd lng="bo">&#xF51;&#xF44;&#xF44;&#xF66;&#xF0B;&#xF60;&#xF5A;&#xF56;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>hr&uring;za</trd>,
    <trd>postrach</trd>,
    <trd>zd&ecaron;&scaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hr&ocirc;za</trd>,
    <trd>postrach</trd>
  </trdgrp>
</drv>

<drv mrk="terur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="terur.0i.KOMUNE">
    <dif>
      Timegigi:
      <ekz>
        nura ombro <tld/>is lin;
      </ekz>
      <ekz>
        son&gcirc;o <tld/>is lin<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ili kuras <tld/>ite<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_h;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">spla&scaron;it <klr>(kon&ecaron;)</klr></trd>
  <trd lng="de">erschrecken</trd>
  <trdgrp lng="en">
    <trd>terrify</trd>,
    <trd>frighten</trd>
  </trdgrp>
  <trd lng="es">aterrorizar</trd>
  <trdgrp lng="fr">
    <trd>terrifier</trd>,
    <trd>terroriser</trd>
  </trdgrp>
  <trd lng="it">terrorizzare</trd>
  <trd lng="nl">schrik aanjagen</trd>
  <trdgrp lng="pl">
    <trd>przera&zdot;a&cacute;</trd>,
    <trd>zatrwa&zdot;a&cacute;</trd>,
    <trd>budzi&cacute; <ind>przera&zdot;enie</ind></trd>,
    <trd>budzi&cacute; <ind>trwog&eogonek;</ind></trd>,
    <trd>budzi&cacute; <ind>zgroz&eogonek;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_zh;&c_a;&c_s;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol;
    <ind>&c_u;&c_zh;&c_a;&c_s;</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>podesi&tcaron;</trd>,
    <trd>spla&scaron;i&tcaron;</trd>,
    <trd>ve&lcaron;mi nastra&scaron;i&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="terur.0a">
  <kap><tld/>a</kap>
  <snc mrk="terur.0a.timiga">
    <dif>
      Timegiga:
      <ekz>
        <tld/>a akcidento;
      </ekz>
      <ekz>
        <tld/>a estas la stato de la aferoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiuj kalumnioj donis <tld/>ajn fruktojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>przera&zdot;aj&aogonek;cy</trd>,
      <trd>przera&zacute;liwy</trd>,
      <trd>wiej&aogonek;cy <ind>groz&aogonek;</ind></trd>,
      <trd>zatrwa&zdot;aj&aogonek;cy</trd>,
      <trd>okropny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_u;&c_zh;&c_a;&c_s;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_a;&c_sh;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="terur.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre forta:
      <ekz>
        &scirc;i havis <tld/>an malamon kontra&ubreve; li<fnt>Z</fnt>
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>przera&zdot;aj&aogonek;cy</trd>,
      <trd>zatrwa&zdot;aj&aogonek;cy</trd>,
      <trd>straszny</trd>,
      <trd>horendalny</trd>,
      <trd>okropny</trd>,
      <trd>wstrz&aogonek;saj&aogonek;cy</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_a;&c_sh;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_h;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trdgrp lng="bg">
    <trd>&c_s;&c_t;&c_r;&c_a;&c_sh;&c_je;&c_n;</trd>,
    <trd>&c_u;&c_zh;&c_a;&c_s;&c_je;&c_n;</trd>
  </trdgrp>
  <trd lng="bo">&#xF62;&#xFA8;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>d&ecaron;sn&yacute;</trd>,
    <trd>hrozn&yacute;</trd>,
    <trd>hr&uring;zn&yacute;</trd>,
    <trd>p&rcaron;&iacute;&scaron;ern&yacute;</trd>
  </trdgrp>
  <trd lng="de">schrecklich</trd>
  <trd lng="en">terrible</trd>
  <trd lng="es">terrible</trd>
  <trd lng="ms">dahsyat</trd>
  <trdgrp lng="fr">
    <trd>terrible</trd>,
    <trd>terrifiant</trd>
  </trdgrp>
  <trdgrp lng="hu">
      <trd>r&eacute;m&iacute;t&odblac;</trd>,
      <trd>sz&ouml;rny&udblac;</trd>
      <trd>iszonyatos</trd>,
      <trd>rettenetes</trd>
    </trdgrp>
  <trdgrp lng="it">
      <trd>terribile</trd>,
      <trd>terrificante</trd>
    </trdgrp>
  <trdgrp lng="nl">
    <trd>verschrikkelijk</trd>,
    <trd>ontzettend</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>desiv&yacute;</trd>,
    <trd>hrozn&yacute;</trd>,
    <trd>stra&scaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="terur.0e">
  <kap><tld/>e</kap>
  <snc mrk="terur.0e.timige">
    <dif>
      Timegige:
      <ekz>
        li <tld/>e punis la kulpulojn.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>przera&zdot;aj&aogonek;co</trd>,
      <trd>budz&aogonek;c <ind>przera&zdot;enie</ind></trd>,
      <trd>budz&aogonek;c <ind>zgroz&eogonek;</ind></trd>,
      <trd>zatrwa&zdot;aj&aogonek;co</trd>,
      <trd>okropnie</trd>
    </trdgrp>
  </snc>
  <snc mrk="terur.0e.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre forte, ekstreme, eksterordinare:
      <ekz>
        <tld/>e gajaj &scirc;ajnis al ili iliaj propraj
        &scirc;ercoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi trovos &ccirc;ion <tld/>e malorda;
      </ekz>
      <ekz>
        estas <tld/>e, kiel mi volas man&gcirc;i<fnt>Z</fnt>
      </ekz>
      <ekz>
        <tld/>e malbela virino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e granda<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sendanka<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sola<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e malbona vetero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu forto de inercio <tld/>e malhelpis &ccirc;iun nian
        pa&scirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e suferi.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>przera&zdot;aj&aogonek;co</trd>,
      <trd>zatrwa&zdot;aj&aogonek;co</trd>,
      <trd>strasznie</trd>,
      <trd>horendalnie</trd>,
      <trd>okropnie</trd>,
      <trd>wstrz&aogonek;saj&aogonek;co</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_h;&c_l;&c_ib;&c_v;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>hrozn&ecaron;</trd>,
    <trd>hr&uring;zn&ecaron;</trd>
  </trdgrp>
  <trd lng="de">schrecklich</trd>
  <trd lng="en">terribly</trd>
  <trd lng="es">terriblemente</trd>
  <trd lng="fr">terriblement</trd>
  <trdgrp lng="hu">
    <trd>rettenetesen</trd>,
    <trd>sz&ouml;rnyen</trd>
  </trdgrp>
  <trd lng="it">terribilmente</trd>
  <trdgrp lng="nl">
    <trd>verschrikkelijk</trd>,
    <trd>ontzettend</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_o;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_sh;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="sk">hrozne</trd>
</drv>

<drv mrk="terur.0igi">
  <kap><tld/>igi</kap>
  <snc mrk="terur.0igi.KOMUNE">
    <dif>
      Igi <tld/>anta:
      <ekz>
        la kolero <tld/>igis lin.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol;
  <klr>&c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;</klr>
  <ind>&c_zh;&c_a;&c_h;&c_l;&c_ib;&c_v;&c_y;&c_m;</ind></trd>
  <trdgrp lng="cs">
    <trd>d&ecaron;sit</trd>,
    <trd>vyd&ecaron;sit</trd>
  </trdgrp>
  <trd lng="fr">rendre <ind>terrible</ind></trd>
  <trdgrp lng="en">
    <trd>to terrorize <klr>(usone)</klr></trd>,
    <trd>terrorise <klr>(brite)</klr></trd>, <trd>to fill with
    dread</trd>, <trd>appall <klr>(usone)</klr></trd>, <trd>appal
    <klr>(brite)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megr&eacute;m&iacute;t</trd>,
    <trd>megrettent</trd>,
    <trd>elborzaszt</trd>
  </trdgrp>
  <trd lng="nl">schrikwekkend maken</trd>
  <trdgrp lng="pl">
    <trd>przera&zdot;a&cacute;</trd>,
    <trd>budzi&cacute;<ind> groz&eogonek;</ind></trd>,
    <trd>trwo&zdot;y&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol;
  <klr>&c_k;&c_o;&c_g;&c_o;-&c_l;.</klr>
  <ind>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_y;&c_m;</ind></trd>
  <trdgrp lng="sk">
    <trd>desi&tcaron;</trd>,
    <trd>vydesi&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="terur.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="terur.0ajxo.KOMUNE">
    <dif>
      <tld lit="T"/>a afero.
    </dif>
  </snc>
<trdgrp lng="be">
  <trd>&c_zh;&c_a;&c_h;</trd>,
  <trd>&c_n;&c_je;&c_sh;&c_t;&c_a; <ind>&c_zh;&c_a;&c_h;&c_l;&c_ib;&c_v;&c_a;&c_je;</ind></trd>
</trdgrp>  <trd lng="de">etwas schreckliches</trd>
  <trd lng="en">horror</trd>
  <trd lng="es">algo <ind>terrible</ind></trd>
  <trd lng="fr">chose <ind>terrible</ind></trd>
  <trdgrp lng="hu">
    <trd>sz&ouml;rny&udblac;s&eacute;g</trd>,
    <trd>borzaszt&oacute; dolog</trd>
  </trdgrp>
  <trd lng="nl">iets <ind>schrikwekkends</ind></trd>
  <trdgrp lng="pl">
    <trd>okropno&sacute;&cacute;</trd>,
    <trd>postrach</trd>
  </trdgrp>
</drv>

<drv mrk="terur.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="terur.0eco.KOMUNE">
    <dif>
      Timegindeco:
      <ekz>
        nun mi plene sentas la tutan <tld/>econ de l'
        dan&gcirc;ero<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_h;</trd>
  <trd lng="cs">hr&uring;znost</trd>
  <trd lng="de">Schrecken</trd>
  <trd lng="en">terribleness</trd>
  <trd lng="fr">caract&egrave;re <ind>terrible</ind></trd>
  <trd lng="hu">iszonyatoss&aacute;g</trd>
  <trdgrp lng="nl">
    <trd>schrik</trd>,
    <trd>ontzetting</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>okropno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_zh;&c_a;&c_s;</trd>
  <trdgrp lng="sk">
    <trd><klr>(hr&ocirc;zo)</klr>stra&scaron;nos&tcaron;</trd>,
    <trd>desivos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="terur.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="terur.0ismo.POL">
    <ref tip="dif" cel="teror.0o">teroro</ref>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_r;&c_o;&c_r;</trd>
  <trd lng="de">Schrecken</trd>
  <trdgrp lng="en">
    <trd>terror</trd>,
    <trd>terrorization <klr>(usone)</klr></trd>, <trd>terrorisation
    <klr>(brite)</klr></trd>
  </trdgrp>
  <trd lng="fr">terreur<klr> (politique)</klr></trd>
  <trd lng="it">terrore<klr> (politica)</klr></trd>
  <trd lng="hu">terror</trd>  
  <trd lng="nl">terreur</trd>
  <trd lng="pl">terror</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_r;&c_o;&c_r;</trd>
</drv>
</art>
