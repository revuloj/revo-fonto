<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>terur</rad>/o
</kap>
<drv mrk="terur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Timego:
      <ekz>
        <ind>tra vitro de <tld/>o pligrandi&gcirc;as la mezuro</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">a <ind>f&eacute;lelem</ind> nagy&iacute;t&oacute;n &aacute;t n&eacute;z</trd>
      </ekz>
      <ekz>
        atakis <tld/>o, ektremis la kruro
        <fnt><bib>PrV</bib></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_h;</trd>
  <trd lng="cs">hr&uring;za</trd>
  <trd lng="hu">r&eacute;m&uuml;let</trd>  
  <trdgrp lng="nl">
    <trd>schrik</trd>,
    <trd>ontzetting</trd>
  </trdgrp>
  <trd lng="de">Schrecken</trd>
  <trd lng="ru">&c_u;&c_zh;&c_a;&c_s;</trd>
</drv>

<drv mrk="terur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Timegigi:
      <ekz>
        nura ombro <tld/>is lin;
      </ekz>
      <ekz>
        son&gcirc;o <tld/>is lin<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ili kuras <tld/>ite<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_h;&c_a;&c_c;&c_mol;</trd>
  <trd lng="nl">schrik aanjagen</trd>
  <trd lng="de">erschrecken</trd>
  <trdgrp lng="ru">
    <trd>&c_u;&c_zh;&c_a;&c_s;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_u;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; <ind>&c_u;&c_zh;&c_a;&c_s;</ind></trd>
  </trdgrp>
</drv>

<drv mrk="terur.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Timegiga:
      <ekz>
        <tld/>a akcidento;
      </ekz>
      <ekz>
        <tld/>a estas la stato de la aferoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiuj kalumnioj donis <tld/>ajn fruktojn<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_u;&c_zh;&c_a;&c_s;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_a;&c_sh;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre forta:
      <ekz>
        &scirc;i havis <tld/>an malamon kontra&ubreve; li<fnt>Z</fnt>
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_a;&c_sh;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_h;&c_l;&c_ib;&c_v;&c_y;</trd>
  <trd lng="en">terror</trd>
    <trdgrp lng="hu">
      <trd>r&eacute;m&iacute;t&odblac;</trd>,
      <trd>sz&ouml;rny&udblac;</trd>
      <trd>iszonyatos</trd>,
      <trd>rettenetes</trd>
    </trdgrp>  
  <trdgrp lng="nl">
    <trd>verschrikkelijk</trd>,
    <trd>ontzettend</trd>
  </trdgrp>
  <trd lng="de">schrecklich</trd>
</drv>

<drv mrk="terur.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Timegige:
      <ekz>
        li <tld/>e punis la kulpulojn.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre forte, ekstreme, eksterordinare:
      <ekz>
        <tld/>e gajaj &scirc;ajnis al ili iliaj propraj &scirc;ercoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi trovos &ccirc;ion <tld/>e malorda;
      </ekz>
      <ekz>
        estas <tld/>e, kiel mi volas man&gcirc;i<fnt>Z</fnt>
      </ekz>
      <ekz>
        <tld/>e malbela virino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e granda<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sendanka<fnt>Z</fnt>,
      </ekz>
      <ekz>
        sola<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e malbona vetero<fnt>Z</fnt>;
      </ekz>
      <ekz>
        tiu forto de inercio <tld/>e malhelpis &ccirc;iun nian pa&scirc;on<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e suferi.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_h;&c_l;&c_ib;&c_v;&c_a;</trd>
  <trdgrp lng="nl">
    <trd>verschrikkelijk</trd>,
    <trd>ontzettend</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rettenetesen</trd>,
    <trd>sz&ouml;rnyen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_o;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_sh;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="de">schrecklich</trd>
</drv>

<drv mrk="terur.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>anta:
      <ekz>
        la kolero <tld/>igis lin.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <klr>&c_k;&c_a;&c_g;&c_o;&c_s;&c_mol;&c_c;&c_ib;</klr> <ind>&c_zh;&c_a;&c_h;&c_l;&c_ib;&c_v;&c_y;&c_m;</ind></trd>
  <trdgrp lng="hu">
    <trd>megr&eacute;m&iacute;t</trd>,
    <trd>megrettent</trd>,
    <trd>elborzaszt</trd>
  </trdgrp>
  <trd lng="nl">schrikwekkend maken</trd>
  <trd lng="ru">&c_s;&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <klr>&c_k;&c_o;&c_g;&c_o;-&c_l;.</klr> <ind>&c_u;&c_zh;&c_a;&c_s;&c_n;&c_y;&c_m;</ind></trd>
  <trd lng="de">erschrecken</trd>
</drv>

<drv mrk="terur.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld lit="T"/>a afero.
    </dif>
  </snc>
  <trdgrp lng="hu">
    <trd>sz&ouml;rny&udblac;s&eacute;g</trd>,
    <trd>borzaszt&oacute; dolog</trd>
  </trdgrp>
  <trd lng="nl">iets <ind>schrikwekkends</ind></trd>
  <trd lng="de">etwas schreckliches</trd>
</drv>

<drv mrk="terur.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Timegindeco:
      <ekz>
        nun mi plene sentas la tutan <tld/>econ de l' dan&gcirc;ero<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_h;</trd>
  <trdgrp lng="nl">
    <trd>schrik</trd>,
    <trd>ontzetting</trd>
  </trdgrp>
  <trd lng="hu">iszonyatoss&aacute;g</trd>  
  <trd lng="ru">&c_u;&c_zh;&c_a;&c_s;</trd>
  <trd lng="de">Schrecken</trd>
</drv>

<drv mrk="terur.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="teror.0o">Teroro</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_r;&c_o;&c_r;</trd>
  <trd lng="hu">terror</trd>  
  <trd lng="nl">terreur</trd>
  <trd lng="de">Schrecken</trd>
  <trd lng="ru">&c_t;&c_je;&c_r;&c_r;&c_o;&c_r;</trd>
</drv>
</art>
