          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=216343">Sesdekuma
          sistemo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="um.0.nombrosistemo">-um</ref>
    <trd lng="be">&c_sh;&c_a;&c_s;&c_mol;&c_c;&c_ib;&c_d;&c_z;&c_je;&c_s;&c_ja;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>
