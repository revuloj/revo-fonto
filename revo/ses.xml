<drv mrk="ses.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld lit="S"/>arange, post kvin anta&ubreve;aj:
      <ekz>
        Ni&gcirc;erio, la <tld/>e plej granda lando la&ubreve; nafto-rezervoj
        <fnt>
          <bib>Monato</bib>
          <aut>Princo Henriko Oguinye</aut>
          <vrk><url ref="&Monato;/2009/009928.php">&Ccirc;u amnestio kondukos
          al paco?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    </snc>
    <trd lng="be">&c_p;&c_a;-&c_sh;&c_o;&c_s;&c_t;&c_a;&c_je;</trd>
    <trd lng="br">da <ind>c'hwec'hvet</ind></trd>
    <trd lng="ca">en sis&eacute; lloc</trd>
    <trdgrp lng="de">
      <trd>sechstens</trd>
    </trdgrp>
    <trd lng="fi">kuudenneksi</trd>
    <trd lng="fr">sixi&egrave;mement</trd>
    <trd lng="hu">hatodszor</trd>
    <trdgrp lng="ja">
    <trd>&#x7B2C;&#x516D;&#x306B;  <pr>&#12384;&#12356;&#12416;&#12387;&#x306B;</pr></trd>,
    <trd>&#x516D;&#x756A;&#x76EE;&#x306B;  <pr>&#12416;&#12387;&#12400;&#12435;&#12417;&#x306B;</pr></trd>
  </trdgrp>
  <trd lng="nl">ten <ind>zesde</ind></trd>
