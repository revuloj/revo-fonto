<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mecx.xml,v 1.27 2024/01/25 20:10:35 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>me&ccirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="mecx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mecx.0o.KOMUNE">
    <dif>
      &Scirc;nuro, kiu kontaktigas brula&jcirc;on kaj fajron.
    </dif>
    <subsnc mrk="mecx.0o.LUM">
      <dif>
        Sorba &scirc;nuro, kiu staras en
        lampo a&ubreve; en mezo de kandelo por konduki
        brula&jcirc;on &gcirc;is la flamo:
        <ekz>
          <tld/>on senfajri&gcirc;antan li ne estingos
          <fnt><bib>MT</bib>, <lok>&Jes; 42:3</lok></fnt>.
        </ekz>
      </dif>                      
      <trd lng="br">mouchenn</trd>
      <trd lng="ca">ble</trd>
      <trd lng="de">Docht</trd>
      <trdgrp lng="hu">
        <trd>kan&oacute;c</trd>,
        <trd>l&aacute;mpab&eacute;l</trd></trdgrp>
      <trd lng="nl">pit<klr tip="ind"> (v.kaars)</klr></trd>
      <trd lng="pl">knot</trd>
      <trdgrp lng="pt">
        <trd>pavio</trd>,
        <trd>mecha</trd>,
        <trd>torcida <klr tip="amb">(mecha)</klr></trd>    
      </trdgrp>
      <trd lng="ro">fitil</trd>
    </subsnc>
    <subsnc mrk="mecx.0o.ARM">
      <uzo tip="fak">MIL</uzo>
      <dif>
        &Scirc;nuro preparita por konduki fajron &gcirc;is
        eksploda&jcirc;o:
        <ekz>
          bruligu la <tld/>on!
          <fnt>
            <aut>trad. J. Ti&hcirc;onov</aut>,
            <vrk><url ref="http://filmoj.net/gxuste-tiu-munchhausen">
            &Gcirc;uste tiu M&uuml;nchhausen</url></vrk>,
            <lok>2009</lok>
          </fnt>.
        </ekz>
      </dif>                
      <trd lng="br">mouch</trd>
      <trd lng="ca">metxa</trd>
      <trdgrp lng="de">
        <trd>Z&uuml;ndschnur</trd>,
        <trd>Lunte</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>kan&oacute;c</trd>,
        <trd>gy&uacute;jt&oacute;zsin&oacute;r</trd></trdgrp>
      <trd lng="nl">lont</trd>
      <trd lng="pl">lont</trd>
      <trdgrp lng="pt">
        <trd>rastilho</trd>,
        <trd>estopim</trd>,
        <trd>morr&atilde;o</trd>    
        <trd>murr&atilde;o</trd>    
      </trdgrp>
    </subsnc>                   
  </snc>
  <trd lng="be">&c_k;&c_n;&c_o;&c_t;</trd>
  <trd lng="bg">&c_f;&c_i;&c_t;&c_i;&c_l;</trd>
  <trd lng="br">poulc'henn</trd>
  <trd lng="en">wick</trd>     
  <trd lng="es">mecha</trd>
  <trd lng="fr">m&egrave;che</trd>
  <trdgrp lng="ja">
    <trd>&#x706F;&#x5FC3;  <pr>&#x3068;&#x3046;&#x3057;&#x3093;</pr></trd>,
    <trd>&#x82AF;  <pr>&#x3057;&#x3093;</pr></trd>,
    <trd>&#x706B;&#x7E04;  <pr>&#x3072;&#x306A;&#x308F;</pr></trd>,
    <trd>&#x5C0E;&#x706B;&#x7DDA;  <pr>&#x3069;&#x3046;&#x304B;&#x305B;&#x3093;</pr></trd>,
    <trd>&#x30AC;&#x30FC;&#x30BC;&#x82AF;  <pr>&#x30AC;&#x30FC;&#x30BC;&#x3057;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_i;&c_t;&c_i;&c_l;&c_mol;</trd>
  <trdgrp lng="zh">
    <trd>&#x706F;&#x854A; <pr>d&#x113;ngru&#x1D0;</pr></trd>,
    <trd>&#x71C8;&#x854A; <pr>d&#x113;ngru&#x1D0;</pr></trd>,
    <trd>&#x706F;&#x82AF; <pr>d&#x113;ngx&#x12B;n</pr></trd>,
    <trd>&#x71C8;&#x82AF; <pr>d&#x113;ngx&#x12B;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mecx.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url ref="&AdE;/fundamento/universala_vortaro.html#av-5106">me&ccirc;'a&jcirc;'</url></lok></fnt></kap>
  <snc>
    <dif>
    <sup>2</sup><ref tip="sin" cel="tindr.0o">Tindro</ref>; substanco,
    uzata por ekigi fajron.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_c;&c_e;&c_r;&c_a;</trd>,
    <trd>&c_t;&c_r;&c_u;&c_t;</trd>,
    <trd>&c_g;&c_u;&c_b;&c_a; <klr tip="amb">(&c_t;&c_r;&c_u;&c_t;)</klr></trd>
  </trdgrp>
  <trd lng="de">Feuerschwamm</trd>
  <trd lng="fr">amadou</trd>
  <trdgrp lng="ja">
    <trd>&#x706B;&#x53E3;  <pr>&#x307B;&#x304F;&#x3061;</pr></trd>,
    <trd>&#x7CF8;&#x308D;&#x3046;&#x305D;&#x304F;  <pr>&#x3044;&#x3068;&#x308D;&#x3046;&#x305D;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_t;&c_r;&c_u;&c_t;&#x43D;&c_i;&#x43A;</trd>
</drv>

</art>
