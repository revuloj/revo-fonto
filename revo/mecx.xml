<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mecx.xml,v 1.21 2017/12/21 15:10:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>me&ccirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="mecx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mecx.0o.KOMUNE">
    <dif>
      &Scirc;nuro, kiu kontaktigas brula&jcirc;on kaj fajron.
    </dif>
    <subsnc mrk="mecx.0o.LUM">
      <dif>
        Sorba &scirc;nuro, kiu staras en
        lampo a&ubreve; en mezo de kandelo por konduki
        brula&jcirc;on &gcirc;is la flamo:
        <ekz>
          <tld/>on senfajri&gcirc;antan li ne estingos
          <fnt><bib>MT</bib>, <lok>&Jes; 42:3</lok></fnt>.
        </ekz>
      </dif>                      
      <trd lng="br">mouchenn</trd>
      <trd lng="ca">ble</trd>
      <trd lng="de">Docht</trd>
      <trdgrp lng="hu">
        <trd>kan&oacute;c</trd>,
        <trd>l&aacute;mpab&eacute;l</trd></trdgrp>
      <trd lng="nl">pit<klr tip="ind"> (v.kaars)</klr></trd>
      <trd lng="pl">knot</trd>
      <trdgrp lng="pt">
        <trd>pavio</trd>,
        <trd>mecha</trd>,
        <trd>torcida <klr tip="amb">(mecha)</klr></trd>    
      </trdgrp>
      <trd lng="ro">fitil</trd>
    </subsnc>
    <subsnc mrk="mecx.0o.ARM">
      <uzo tip="fak">MIL</uzo>
      <dif>
        &Scirc;nuro preparita por konduki fajron &gcirc;is
        eksploda&jcirc;o:
        <ekz>
          bruligu la <tld/>on!
          <fnt>
            <aut>trad. J. Ti&hcirc;onov</aut>,
            <vrk><url ref="http://filmoj.net/gxuste-tiu-munchhausen">
            &Gcirc;uste tiu M&uuml;nchhausen</url></vrk>,
            <lok>2009</lok>
          </fnt>.
        </ekz>
      </dif>                
      <trd lng="br">mouch</trd>
      <trd lng="ca">metxa</trd>
      <trdgrp lng="de">
        <trd>Z&uuml;ndschnur</trd>,
        <trd>Lunte</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>kan&oacute;c</trd>,
        <trd>gy&uacute;jt&oacute;zsin&oacute;r</trd></trdgrp>
      <trd lng="nl">lont</trd>
      <trd lng="pl">lont</trd>
      <trdgrp lng="pt">
        <trd>rastilho</trd>,
        <trd>estopim</trd>,
        <trd>morr&atilde;o</trd>    
        <trd>murr&atilde;o</trd>    
      </trdgrp>
    </subsnc>                   
  </snc>
  <trd lng="be">&c_k;&c_n;&c_o;&c_t;</trd>
  <trd lng="bg">&c_f;&c_i;&c_t;&c_i;&c_l;</trd>
  <trd lng="br">poulc'henn</trd>
  <trd lng="en">wick</trd>     
  <trd lng="es">mecha</trd>
  <trd lng="fr">m&egrave;che</trd>
  <trd lng="ru">&c_f;&c_i;&c_t;&c_i;&c_l;&c_mol;</trd>
</drv>

<drv mrk="mecx.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o <fnt><bib>F</bib>, <lok>Universala Vortaro,
  <url
  ref="http://www.akademio-de-esperanto.org/fundamento/universala_vortaro.html#av-5106">me&ccirc;'a&jcirc;'</url></lok></fnt></kap>
  <snc>
    <dif>
    <sup>2</sup><ref tip="sin" cel="tindr.0o">Tindro</ref>; substanco,
    uzata por ekigi fajron.
    </dif>
  </snc>
  <trd lng="de">Feuerschwamm</trd>
  <trd lng="fr">amadou</trd>
</drv>

</art>
