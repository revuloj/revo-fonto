<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fortik.xml,v 1.26 2021/05/29 10:56:53 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>fortik</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="fortik.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="fortik.0a.materialo">
    <dif>
      Havanta firman, rezistan konsiston; malfacile rompebla,
      difektebla, venkebla:
      <ekz>
        <tld/>a masona&jcirc;o, fadeno, membro;
      </ekz>
      <ekz>
        <tld/>a atleto;
      </ekz>
      <ekz>
        <tld/>a sano, volo, argumento
        <klr>(malfacile refutebla)</klr>;
      </ekz>
      <ekz>
        <tld/>a firmo, amikeco<fnt>Z</fnt>, drapo;
      </ekz>
      <ekz>
        <tld/>e ligi;
      </ekz>
      <ekz>
        vitro kaj feli&ccirc;o ne estas <tld/>aj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="firm.0a.fiksita">firma<sncref/></ref>,
      <ref cel="fer.0a">fera</ref>,
      <ref cel="sxtal.0a">&scirc;tala</ref>,
      <ref cel="rok.0o">roko</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_o;&c_c;&c_n;&c_y;</trd>,
      <trd>&c_t;&c_r;&c_y;&c_v;&c_a;&c_l;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>kr&auml;ftig</trd>,
      <trd>fest</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>durable</trd>,
      <trd>firm</trd>,
      <trd>robust</trd>,
      <trd>sturdy</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>robuste</trd>,
      <trd>r&eacute;sistant<klr tip="ind"> (robuste)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>er&odblac;s</trd>,
      <trd>szil&aacute;rd</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>kukuh</trd>,
      <trd>teguh</trd>
    </trdgrp>
   <trdgrp lng="it">
     <trd>forte</trd>,
     <trd>solido</trd>,
     <trd>robusto</trd>,
     <trd>resistente</trd>,
     <trd>vigoroso</trd>,
     <trd>saldo<klr> (forte, solido)</klr></trd>
  </trdgrp>
    <trdgrp lng="nl">
      <trd>robuust</trd>,
      <trd>sterk<klr tip="ind"> (robuust)</klr></trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>forte</trd>,
    <trd>s&oacute;lido</trd>,
    <trd>rijo</trd>,
    <trd>robusto</trd>,
    <trd>membrudo</trd>,
    <trd>vigoroso</trd>,
    <trd>possante</trd>
  </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_je;&c_p;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="fortik.0a.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Provizita je muregoj:
      <ekz>
        <tld/>a urbo, loko.
      </ekz>
    </dif>
    <trd lng="be">&c_u;&c_m;&c_a;&c_c;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
    <trd lng="de">befestigt</trd>
    <trdgrp lng="fr">
      <trd>fortifi&eacute;<klr tip="ind"> (par des
      remparts)</klr></trd>,
      <trd>fort<klr tip="ind"> (fortifi&eacute;)</klr></trd>
    </trdgrp>
    <trd lng="hu">er&odblac;d&iacute;tett</trd>
    <trd lng="it">fortificato</trd>
    <trd lng="nl">versterkt</trd>
    <trd lng="pt">fortificado</trd>
    <trd lng="ru">&c_u;&c_k;&c_r;&c_je;&c_p;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j;
    <klr>(&c_v;&c_o;&c_je;&c_n;..)</klr></trd>
  </snc>
  <trdgrp lng="cs">
    <trd>d&uring;kladn&yacute;</trd>,
    <trd>masivn&iacute;</trd>,
    <trd>pevn&yacute;</trd>,
    <trd>robustn&iacute;</trd>,
    <trd>siln&yacute;</trd>,
    <trd>statn&yacute;</trd>,
    <trd>st&aacute;l&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fortified</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>mas&iacute;vny</trd>,
    <trd>pevn&yacute;</trd>,
    <trd>statn&yacute;</trd>,
    <trd>st&aacute;ly</trd>
  </trdgrp>
</drv>

<drv mrk="fortik.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fortik.0igi.KOMUNE">
    <dif>
      Igi ion <tld/>a, a&ubreve; pli <tld/>a:
      <ekz>
        <tld/>igi palacon, vila&gcirc;on;
      </ekz>
      <ekz>
        oni <klr>...</klr> <tld/>igas per dika muro el tero
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj, la
        lo&gcirc;ejoj
        de
        la termitoj</lok></fnt>;
      </ekz>
      <ekz>
        <tld/>igi la amon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;ado de lingvo internacia<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="firm.0igi">firmigi</ref>,
      <ref cel="kiras.0i">kirasi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_m;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_u;&c_z;&c_m;&c_a;&c_c;&c_n;&c_ja;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>opevnit</trd>,
    <trd>zpevnit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>festigen</trd>,
    <trd>befestigen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fortify</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fortifier</trd>,
    <trd>rendre <ind>robuste</ind></trd>,
    <trd>affermir<klr tip="ind"> (fortifier)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>er&odblac;&iacute;t</trd>,
    <trd>szil&aacute;rd;&iacute;t</trd>
  </trdgrp>
  <trd lng="id">mem<ind>benteng</ind>i</trd>
  <trdgrp lng="it">
     <trd>fortificare</trd>,
     <trd>rafforzare</trd>,
     <trd>irrobustire</trd>,
     <trd>rendere <ind>resistente</ind></trd>,
     <trd>invigorire</trd>
  </trdgrp>
  <trd lng="nl">versterken</trd>
  <trd lng="ru">&c_u;&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>
  <trd lng="sk">opevni&tcaron;</trd>
</drv>

<drv mrk="fortik.al0igi">
  <kap>al<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="fortik.al0igi.KOMUNE">
    <dif>
      Tre solide alfiksi, nodi, najli a&ubreve; iel teni &ccirc;e io:
      <ekz>
        viaj &scirc;nuregoj malforti&gcirc;is; ili jam ne al<tld/>igas
        la maston
        <fnt><bib>MT</bib>, <lok>&Jes; 33:23</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>arrimer</trd>,
    <trd>fixer solidement</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fissare<klr> (legare, saldare, puntare)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="fortik.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="fortik.0ajxo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      <tld lit="F"/>a <ref tip="super" cel="konstr.0ajxo.domo">konstrua&jcirc;o</ref> por defendi lokon,
      urbon
      kontra&ubreve; atakantoj:
      <ekz>
        ruinoj de malnova <tld/>a&jcirc;o
        <fnt><bib>Voja&gcirc;Imp</bib>, <lok>el odeso &gcirc;is
        Konstantinopolo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_a;&c_r;&c_t;&c_e;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_u;&c_m;&c_a;&c_c;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bo">&#xF58;&#xF41;&#xF62;&#xF0B;</trd>
  <trdgrp lng="de">
    <trd>Festung</trd>,
    <trd>Burg</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>fort</trd>,
    <trd>fortification</trd>
  </trdgrp>
  <trd lng="fr">fortifications</trd>
  <trdgrp lng="hu">
    <trd>er&odblac;&iacute;tm&eacute;ny</trd>,
    <trd>v&aacute;r</trd>
  </trdgrp>
  <trd lng="id">benteng</trd>
  <trdgrp lng="it">
    <trd>forte<klr> (mil.)</klr></trd>,
    <trd>fortificazione</trd>
  </trdgrp>
  <trd lng="nl">vesting</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="fortik.cxirkaux0ajxo">
  <kap>&ccirc;irka&ubreve;<tld/>a&jcirc;o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="fortik.cxirkaux0ajxo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MIL</uzo>
    <dif>
      <ref tip="super" cel="fortik.0ajxo.ARKI"><tld lit="F"/>a&jcirc;o</ref>,
      kiun malamika armeo konstruas &ccirc;irka&ubreve; urbon por
      &gcirc;in sie&gcirc;i.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>fortification des assi&eacute;geants</trd>,
    <trd>circonvallation<klr tip="ind"> (et
    contrevallation)</klr></trd>,
    <trd>contrevallation<klr tip="ind"> (et circonvallation)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>opere d'<ind>assedio</ind></trd>
  </trdgrp>
  <adm>
    PIV1 konas anka&ubreve; "cirkonvalacio", kiun &gcirc;i konsideras
    sinonimo de
    &ccirc;irka&ubreve;~a&jcirc;o. Tamen &scirc;ajnas, ke en la franca
    "circonvallation" kaj
    "contrevallation" estas du malsamaj partoj de la
    &ccirc;irka&ubreve;~a&jcirc;o (unu
    rigardas al la sie&gcirc;ita urbo, la alia al la malanta&ubreve;o).
    <aut>MB</aut>
  </adm>
</drv>

<drv mrk="fortik.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="fortik.mal0a.KOMUNE">
    <dif>
      Ne facile rezistanta, ne longe eltenanta;
      <ref tip="vid" cel="romp.igxem0a.KOMUNE">rompi&gcirc;ema</ref>,
      <ref tip="sin" cel="fragil.0a.KOMUNE">fragila</ref>:
      <ekz>
        ili estis tro mal<tld/>aj por terkultivado
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.radio-vatikana-esperanto.org/elsendo.php?d=2009-08-23">
          La sklavkomercado...</url></vrk>,
          <lok>Radio Vatikana, 2009-08-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>chatrn&yacute;</trd>,
    <trd>nepevn&yacute;</trd>,
    <trd>snadno po&scaron;koditeln&yacute;</trd>,
    <trd>vetch&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>feeble</trd>,
    <trd>fragile</trd>,
    <trd>frail</trd>,
    <trd>infirm</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;licat</trd>,
    <trd>fragile</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chatrn&yacute;</trd>,
    <trd>nepevn&yacute;</trd>,
    <trd>vetch&yacute;</trd>
  </trdgrp>
</drv>
</art>
