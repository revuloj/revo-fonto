<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fortik</rad>/a
</kap>
<drv mrk="fortik.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc num="1">
    <dif>
      Havanta firman, rezistan konsiston; malfacile rompebla, difektebla,
      venkebla:
      <ekz>
        <tld/>a masona&jcirc;o, fadeno, membro;
      </ekz>
      <ekz>
        <tld/>a atleto;
      </ekz>
      <ekz>
        <tld/>a sano, volo, argumento
        <klr>(malfacile refutebla)</klr>;
      </ekz>
      <ekz>
        <tld/>a firmo, amikeco<fnt>Z</fnt>, drapo;
      </ekz>
      <ekz>
        <tld/>e ligi;
      </ekz>
      <ekz>
        vitro kaj feli&ccirc;o ne estas <tld/>aj
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="fort.0a">forta</ref>,
      <ref cel="firm.0a">firma</ref>,
      <ref cel="fer.0a">fera</ref>,
      <ref cel="sxtal.0a">&scirc;tala</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="rok.0o">roko</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>kr&auml;ftig</trd>,
      <trd>fest</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>robuste</trd>,
      <trd>r&eacute;sistant<klr tip="ind"> (robuste)</klr></trd></trdgrp> 
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_je;&c_p;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_ch;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc num="2">
    <dif>
      Provizita per remparoj:
      <ekz>
        <tld/>a urbo, loko.
      </ekz>
    </dif>
    <trd lng="de">befestigt</trd>
    <trdgrp lng="fr">
      <trd>fortifi&eacute;<klr tip="ind"> (par des remparts)</klr></trd>,
      <trd>fort<klr tip="ind"> (fortifi&eacute;)</klr></trd></trdgrp> 
    <trd lng="ru">&c_u;&c_k;&c_r;&c_je;&c_p;&c_l;&c_jo;&c_n;&c_n;&c_y;&c_j; <klr>(&c_v;&c_o;&c_je;&c_n;.)</klr></trd>
  </snc>
</drv>
<drv mrk="fortik.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi ion <tld/>a, a&ubreve; pli <tld/>a:
      <ekz>
        <tld/>igi muron, palacon, vila&gcirc;on;
      </ekz>
      <ekz>
        <tld/>igi la amon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i&gcirc;ado de lingvo internacia<fnt>Z</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>festigen</trd>,
    <trd>befestigen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>fortifier</trd>,
    <trd>rendre <ind>robuste</ind></trd>,
    <trd>affermir<klr tip="ind"> (fortifier)</klr></trd></trdgrp> 
  <trd lng="ru">&c_u;&c_k;&c_r;&c_je;&c_p;&c_i;&c_t;&c_mol;</trd>
</drv>
<drv mrk="fortik.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <tld lit="F"/>a konstrua&jcirc;o por defendi lokon, urbon kontra&ubreve; armeo.  
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Festung</trd>,
    <trd>Burg</trd>
  </trdgrp>
  <trd lng="fr">fortifications</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_r;&c_je;&c_p;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_je;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
