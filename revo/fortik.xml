    <trd>&#x8EDF;&#x5F31;&#x306A;  <pr>&#x306A;&#12435;&#12376;&#12419;&#x304F;&#x306A;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>chatrn&yacute;</trd>,
