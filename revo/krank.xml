<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: krank.xml,v 1.15 2018/09/19 23:10:11 revo Exp $">
    <kap>
      <rad>krank</rad>/o <fnt><bib>PV</bib></fnt>
    </kap>
    <drv mrk="krank.0o">
      <kap><tld/>o</kap>
      <snc mrk="krank.0o.TEK">
        <uzo tip="fak">TEK</uzo>
        <dif>
          Krampoforme fleksita parto de <ref
          cel="sxaft.0o">&scirc;afto</ref>,
          a&ubreve; simile fiksita peco kapabla fari plenan rotacion,
          servanta por transformi rotacion en baskulan a&ubreve; similan
          movon, a&ubreve; inverse:
          <ekz>
            per <ind>kulisa <tld/>o</ind> oni transformas kontinuan
            cirklan movon en rektlinian alternan movon
            <fnt><bib>PIV1</bib></fnt>;
            <trd lng="hu">kulissz&aacute;s <ind>forgatty&uacute;</ind></trd>
            <trd lng="ru">&c_k;&c_r;&c_i;&c_v;&c_o;&c_sh;&c_i;&c_p;&c_n;&c_o;-&c_k;&c_u;&c_l;&c_i;&c_s;&c_n;&c_y;&c_j;
            &c_m;&c_je;&c_h;&c_a;&c_n;&c_i;&c_z;&c_m;</trd>
          </ekz><ekz>
            <ind>man<tld/>o</ind>;
            <trd lng="hu">k&eacute;zi <ind>forgatty&uacute;</ind></trd>
            <trd lng="ru">&c_k;&c_r;&c_i;&c_v;&c_o;&c_sh;&c_i;&c_p;&c_n;&c_a;&c_ja; &c_r;&c_u;&c_k;&c_o;&c_ja;&c_t;&c_k;&c_a;</trd>
          </ekz><ekz>
            frunta, sekuriga <tld/>o.
          </ekz>
        </dif>
        <trd lng="ca">manovella</trd>
        <trd lng="de">Kurbel</trd>
        <trdgrp lng="en">
          <trd>crank</trd>,
          <trd>starter handle <klr>(p.p. motoro)</klr></trd>
        </trdgrp>
        <trdgrp lng="ru">
          <trd>&c_k;&c_r;&c_i;&c_v;&c_o;&c_sh;&c_i;&c_p;</trd>,
          <trd>&c_k;&c_o;&c_l;&c_je;&c_n;&c_o; <klr>(&c_v;&c_a;&c_l;&c_a;)</klr></trd></trdgrp>
      </snc>
  <trd lng="cs">klika na ot&aacute;&ccaron;en&iacute;</trd>
  <trd lng="fr">manivelle</trd>
  <trdgrp lng="hu">
        <trd>forgatty&uacute;</trd>,
        <trd>forgat&oacute;kar</trd>,
        <trd>kurbli</trd>
  </trdgrp>
  <trdgrp lng="nl">
        <trd>kruk</trd>,
        <trd>zwengel</trd>,
        <trd>crank</trd>
  </trdgrp>
  <trd lng="pl">korba</trd>    
  <trd lng="pt">manivela</trd>
  <trd lng="ro">manivel&abreve;</trd>
  <trd lng="sk">k&lcaron;uka na ot&aacute;&ccaron;anie</trd>
</drv>
   
   <drv mrk="krank.0osxafto">
      <kap><tld/>o&scirc;afto</kap>
      <snc>
        <uzo tip="fak">TEK</uzo>
        <dif>
        <tld lit="K"/>o kun ortangulaj fleksoj, sur kiujn agadas la
        <ref tip="vid" cel="biel.0o">bieloj</ref> en eksplodmotoro.
        </dif>
      </snc>
      <trd lng="fr">vilebrequin</trd>
      <trd lng="pl">wa&lstroke; korbowy</trd>     
      <trd lng="ro">arbore cotit</trd>
   </drv>   
  </art>
