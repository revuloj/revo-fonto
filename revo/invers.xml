<drv mrk="invers.0igo">
  <kap><tld/>igo</kap>
  <snc mrk="invers.0igo.KOMUNE">
    <dif>
      Ago <tld/>igi <sncref ref="invers.0igi.KOMUNE"/>.
    </dif>
    <trd lng="fr">inversion</trd>
    <trdgrp lng="hu">
      <trd>megford&iacute;t&aacute;s</trd>,
      <trd>kiford&iacute;t&aacute;s <klr tip="ind">(pl. ruh&aacute;&aacute;)</klr></trd>
    </trdgrp>
    <trd lng="pl">odwracanie</trd>
    <trd lng="pt">invers&atilde;o</trd>
    <trd lng="zh">&#x53CD;&#x8F49; <pr>f&#x1CE;nzhu&#x1CE;n</pr></trd>
</snc>
  <snc mrk="invers.0igo.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="invers.0igo.gxenerale">
      <uzo tip="klr">(fakula &jcirc;argono)</uzo>
      <fnt><bib>MatVort</bib><lok>&leftquot;de matrico&rightquot;</lok></fnt>
      <dif>
        Ago <ref cel="invers.0igi.MAT"><tld/>igi <sncref/></ref>.
      </dif>
    </subsnc>
    <subsnc mrk="invers.0igo.transformo">
      <uzo tip="fak">MAT</uzo>
      <fnt><bib>MatTerm</bib><lok>p. 29</lok></fnt>
      <dif>
        <klr>(en dudimensia afina
        <ref cel="spac.euxklida0o">e&ubreve;klida spaco</ref>, kun centro
        <frm am="O">O</frm> kaj potenco <frm am="lambda^2">&lambda;<sup>2</sup></frm>) </klr>
        <ref tip="super" cel="form.trans0o.MAT">Transformo</ref>, kiu
        &jcirc;etas punkton <frm am="M">M</frm> al tia punkto
        <frm am="M&minute;">M'</frm> de
        la duonrekto <frm am="OM">OM</frm>, ke
        <frm am="OM*OM&minute;=lambda^2">OM&middot;OM' =
        &lambda;<sup>2</sup></frm>:
        <ekz>
          &ccirc;iu punkto de la cirklo kun centro en <frm am="O">O</frm> kaj
          radiuso
          <frm am="lambda">&lambda;</frm> estas sen&scirc;an&gcirc;a per la <tld/>igo;
        </ekz>
        <ekz>
          <tld/>igo estas <ref cel="involu1.0o">involucio</ref>;
        </ekz>
        <ekz>
          cirkla <tld/>igo
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=83283">Inverso
            (matematiko)</url></lok>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>igo en punkto
          <fnt>
            <bib>Viki</bib>
            <lok><url ref="https://eo.wikipedia.org/?curid=83283">Inverso
            (matematiko)</url></lok>
          </fnt>.
        </ekz>
      </dif>
      <rim>
        Tian transformon oni anka&ubreve; nomas <ctl><tld/>igo rilate al cirklo
        kun centro en <frm am="O">O</frm> kaj radiuso
        <frm am="lambda">&lambda;</frm></ctl>.
        Troveblas sinonima termino <ctl>inversio</ctl> en
        <fnt><bib>MatVort</bib></fnt>, sed neniu el niaj
        aliaj fontoj &gcirc;in konfirmas.
      </rim>
      <trd lng="de">Inversion<klr tip="ind"> (am Kreis)</klr></trd>
      <trd lng="en">inversion<klr tip="ind"> (with respect to a
      circle)</klr></trd>
      <trd lng="fr">inversion<klr tip="ind"> (par rapport &agrave; un
      cercle)</klr></trd>
      <trd lng="pl">inwersja<klr tip="ind"> (wzgl&eogonek;dem
      okr&eogonek;gu)</klr></trd>
      <trd lng="ru">&c_i;&c_n;&c_v;&c_je;&c_r;&c_s;&c_i;&c_ja;<klr tip="ind"> (&c_o;&c_t;&c_n;&c_o;&c_s;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;
      &c_o;&c_k;&c_r;&c_u;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_i;)</klr></trd>
    </subsnc>
    <trdgrp lng="hu">
      <trd>invert&aacute;l&aacute;s</trd>,
      <trd>inverzi&oacute;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_n;&c_v;&c_e;&c_r;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_a;&c_b;&c_a;&c_r;&c_a;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ru">&c_i;&c_n;&c_v;&c_je;&c_r;&c_s;&c_i;&c_ja;</trd>
</drv>

<drv mrk="invers.0igebla">
  <kap><tld/>igebla</kap>
  <snc mrk="invers.0igebla.KOMUNE">
    <dif>
      Tia, ke eblas &gcirc;in <tld/>igi.
    </dif>
    <trdgrp lng="hu">
      <trd>megford&iacute;that&oacute;</trd>,
      <trd>kiford&iacute;that&oacute; <klr tip="ind">(pl. ruha)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_b;&c_r;&c_a;&c_t;&c_i;&c_m;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="invers.0igebla.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>EKV</bib><lok>&para;185</lok></fnt>
    <dif>
      <klr>(p.p. elemento en multiplike signata
      <ref cel="grup.0o.MAT">grupo</ref> a&ubreve; en
      <ref cel="ring.0o.MAT">ringo</ref>)</klr>
      Tia, ke ekzistas &gcirc;ia
      <ref cel="invers.0o.neuxtriganto"><tld/>o</ref>:
      <ekz>
        en la ringo de entjeroj nur <frm>1</frm> kaj <frm>-1</frm>
        estas <tld/>igeblaj.
      </ekz>
    </dif>
    <ref tip="sin" cel="invers.0hava">invershava.</ref>
    <rim>
      La <tld/>igeblajn elementojn de ringo oni foje nomas
      <ctl>unuoj</ctl>, kio estas evitinda pro la ebla
      konfuzo kun la <ref cel="unu.0o.neuxtra">unuo</ref> de la ringo.
      Ial la citita fonto uzas <ctl><tld/>igebla</ctl> nur p.p.
      bildigoj.
      Ni ne opinias tiun limigon utila.
    </rim>
    <trd lng="en">invertible</trd>
    <trd lng="hu">invert&aacute;lhat&oacute;</trd>
    <trd lng="pl">odwracalny</trd>
    <trd lng="ru">&c_d;&c_o;&c_p;&c_u;&c_s;&c_k;&c_a;&c_ju;&c_shch;&c_i;&c_j; &c_o;&c_b;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_a;&c_r;&c_a;&c_ch;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="cs">reverzibiln&iacute; <klr>(turb&iacute;na)</klr></trd>
  <trd lng="de">umkehrbar</trd>
  <trd lng="es">invertible</trd>
  <trd lng="fr">inversible</trd>
  <trdgrp lng="ja">
    <trd>&#x9006;&#x8EE2;&#x3067;&#x304D;&#x308B;  <pr>&#x304E;&#x3083;&#x304F;&#x3066;&#x3093;&#x3067;&#x304D;&#x308B;</pr></trd>,
    <trd>&#x53EF;&#x9006;&#x306E; <pr>&#x304B;&#x304E;&#x3083;&#x304F;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">omkeerbaar</trd>
  <trd lng="pt">invert&iacute;vel</trd>
  <trdgrp lng="sk">
    <trd>vratn&yacute;</trd>,
    <trd>zvratn&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="invers.ne0igebla">
  <kap>ne<tld/>igebla</kap>
  <snc mrk="invers.ne0igebla.KOMUNE">
    <dif>
      Ne <ref cel="invers.0igebla.KOMUNE">inversigebla<sncref/></ref>:
      <ekz>
        kelkaj efikoj estas ne<tld/>igeblaj (a&ubreve;, pli precize, restas dum
        tre longega tempo en nia klimatsistemo)
        <fnt>
          <bib>Monato</bib>
          <aut>Marco van Hulten</aut>
          <vrk><url ref="&Monato;/2015/011605.php">Interrilato de la
          oceano kaj klimat&scirc;an&gcirc;i&gcirc;o</url></vrk>
