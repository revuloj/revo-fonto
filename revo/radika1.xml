<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: radika1.xml,v 1.21 2023/10/20 19:20:10 revo Exp $">
    <kap>
      <rad>radikal</rad>/o
    </kap>
    <drv mrk="radika1.0o">
      <kap><tld/>o</kap>
      <snc mrk="radika1.0o.GRA">
        <uzo tip="fak">GRA</uzo>
        <dif>
          Sen&scirc;an&gcirc;a parto de vorto (unu a&ubreve; pluraj
          <ref tip="prt" cel="radik.0o.GRA">radikoj</ref> kun la
          eventualaj <ref tip="prt" cel="prefik.0o">prefiksoj</ref>
          kaj <ref tip="prt" cel="sufiks.0o">sufiksoj</ref>),
          entenanta la leksikan signifon de la vorto,
          kontraste al la fleksiaj
          <ref cel="fin.0ajxo.GRA">fina&jcirc;oj</ref>:
          <ekz>
            en la kunmetitaj vortoj de la tipo <ctl>&ccirc;iopova</ctl> oni
            evitas la akuzativan fina&jcirc;on ene de la <tld/>o;
          </ekz><ekz>
            la U.V. entenas 2629 radikojn, 309 <tld/>ojn kaj 88
            plenajn vortojn <fnt><bib>PIV1</bib></fnt>.
          </ekz>
        </dif>
        <trd lng="be">&c_a;&c_s;&c_n;&c_o;&c_v;&c_a; <klr>(&c_s;&c_l;&c_o;&c_v;&c_a;)</klr></trd>
        <trdgrp lng="en">
          <trd>radical</trd>,
          <trd>root</trd>
        </trdgrp>
        <trd lng="es">ra&iacute;z</trd>
        <trd lng="hu">sz&oacute;t&odblac;</trd>
        <trd lng="nl">stam<klr tip="ind"> (v.e.woord)</klr></trd>
        <trd lng="pl">temat <klr>(j&eogonek;z.)</klr></trd>     
        <trd lng="ro">tem&abreve;</trd>
        <trd lng="ru">&c_o;&c_s;&c_n;&c_o;&c_v;&c_a;<klr tip="ind">
           (&c_s;&c_l;&c_o;&c_v;&c_a;)</klr></trd>
      </snc>
      <snc mrk="radika1.0o.KEM">
        <uzo tip="fak">KEM</uzo>
        <dif>
          Parto de <ref tip="malprt" cel="moleku.0o">molekulo</ref>,
          stabila atomgrupo, dum reakcioj bloke kaj
          sen&scirc;an&gcirc;e transiranta de unu &hcirc;emia kombina&jcirc;o
          en alian; ekz-e metilo <frm>CH<sub>3</sub></frm>,
          metileno <frm>CH<sub>2</sub></frm> ktp.
        </dif>
        <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_k;&c_a;&c_l;</trd>
        <trd lng="de">Radikal</trd>
        <trd lng="en">radical</trd>
        <trd lng="es">radical</trd>
        <trd lng="hu">gy&ouml;k</trd>
        <trd lng="nl">radicaal</trd>
        <trd lng="pt">radical</trd>
        <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_k;&c_a;&c_l;<klr tip="ind">
          (&c_h;&c_i;&c_m;.)</klr></trd>
      </snc>
  <trd lng="cs">kmen <klr>(slova)</klr></trd>
  <trd lng="fr">radical</trd>
  <trdgrp lng="ja">
    <trd>&#x8A9E;&#x5E79;  <pr>&#x3054;&#x304B;&#x3093;</pr></trd>,
    <trd>&#x57FA;  <pr>&#x3082;&#x3068;&#x3044;</pr></trd>,
    <trd>&#x6839;&#x53F7;  <pr>&#x3053;&#x3093;&#x3054;&#x3046;</pr></trd>,
    <trd>&#x30EB;&#x30FC;&#x30C8;&#x8A18;&#x53F7;  <pr>&#x308B;&#x30FC;&#x3068;&#x304D;&#x3054;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="sk">kme&ncaron; <klr>(slova)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x440;&#x430;&#x434;&#x438;&#x43A;&#x430;&#x43B;</trd>,
    <trd>&#x43E;&#x441;&#x43D;&#x43E;&#x432;&#x430;</trd>
  </trdgrp>
</drv>
    <drv mrk="radika1.0a">
      <kap><tld/>a</kap>
      <snc mrk="radika1.0a.GRA">
        <dif>
          Rilata al <tld/>o,<sncref ref="radika1.0o.GRA"/>.
        </dif>
        <trd lng="be">&c_a;&c_s;&c_n;&c_o;&c_w;&c_n;&c_y;</trd>
        <trd lng="hu">sz&oacute;t&odblac;-</trd>
        <trd lng="pl">tematyczny</trd>
        <trd lng="ru">&c_o;&c_s;&c_n;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>
      </snc>
      <snc mrk="radika1.0a.KEM">
        <dif>
          Rilata al <tld/>o,<sncref ref="radika1.0o.KEM"/> a&ubreve;
          entenanta <tld/>on,<sncref ref="radika1.0o.KEM"/>.
        </dif>
        <trd lng="be">&c_r;&c_a;&c_d;&c_y;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
        <trd lng="hu">gy&ouml;k-</trd>
        <trd lng="ru">&c_r;&c_a;&c_d;&c_i;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
      </snc>
      <ref tip="hom" cel="radika.0a">radikala</ref>
      <trd lng="fr">radical</trd>
      <trdgrp lng="ja">
    <trd>&#x6839;&#x672C;&#x7684;&#x306A;  <pr>&#x3053;&#x3093;&#x307D;&#x3093;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x5FB9;&#x5E95;&#x7684;&#x306A;  <pr>&#x3066;&#x3063;&#x3066;&#x3044;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x6025;&#x9032;&#x7684;&#x306A;  <pr>&#x304D;&#x3085;&#x3046;&#x3057;&#x3093;&#x3066;&#x304D;&#x306A;</pr></trd>,
    <trd>&#x6025;&#x9032;&#x6D3E;&#x306E;  <pr>&#x304D;&#x3085;&#x3046;&#x3057;&#x3093;&#x306F;&#x306E;</pr></trd>,
    <trd>&#x904E;&#x6FC0;&#x306A;  <pr>&#x304B;&#x3052;&#x304D;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="uk">&#x440;&#x430;&#x434;&#x438;&#x43A;&#x430;&#x43B;&#x44C;&#x43D;&#x438;&#x439;</trd>
</drv>
  </art>
