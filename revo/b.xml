<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
    <ofc>*</ofc><rad>b</rad> <fnt><bib>PIV1</bib></fnt>
</kap>
<subart>
<drv mrk="b.0.konsonanto">
  <kap><tld/></kap>
    <uzo tip="fak">TIP</uzo>
    <uzo tip="fak">GRA</uzo>
    <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      <ref tip="dif" cel="konson1.0o">Konsonanto</ref>, vo&ccirc;a <ref
      tip="dif" cel="plosiv.0o">plosiva</ref> <ref tip="dif"
      cel="labial.0o">labialo</ref> [b].
    </dif>
  <trd lng="fr">b<klr> (consonne)</klr></trd>  
  </snc>
  <snc>
     <dif>
      La dua <ref tip="dif" cel="grafem.0o">grafemo</ref> de la
      Esperanta
      alfabeto, uzata i.a. kiel:
     </dif>
  <trd lng="fr">b<klr> (lettre)</klr></trd>
  <subsnc>
  <uzo tip="fak">MUZ</uzo>
  <dif>
  (maj.) nomo de la sepa tono de la C-ma&jcirc;ora gamo (494 Hz;
  absolut-solfe&gcirc;a si):<ekz> <tld lit="B"/> bemola, diesa</ekz>;
  </dif>
  <trd lng="fr">si</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">KEM</uzo>
  <dif>
  (maj.) simbolo de boro:<ekz> <tld lit="B"/></ekz>;
  </dif>
  <trd lng="fr">bore</trd>
  </subsnc>
  <subsnc>
  <uzo tip="fak">FIZ</uzo>
  <dif>
  (maj.) simbolo de magneta fluksdenso:<ekz> <tld lit="B"/></ekz>;
  </dif>
  <trd lng="fr">densit&eacute; de flux <ind>magn&eacute;tique</ind></trd>
  </subsnc>
  </snc>  
</drv>
</subart>

<subart>
<drv mrk="b.0.nomo">
  <kap><tld/>o</kap>
  <snc> 
    <dif>
      Nomo de tiuj <ref tip="dif" cel="fonem.0o">fonemo</ref> kaj
      grafemo.    
    </dif>
  <trd lng="fr">b</trd>
  </snc>
</drv>
</subart>
</art>

