<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kosmona&ubreve;t</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kosmonauxt.0o">
  <kap><tld/>o</kap>
  <snc mrk="kosmonauxt.0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Kosmo&scirc;ipisto; homo, kiu voja&gcirc;as al la <ref tip="vid"
      cel="kosm.0o.AST">kosmo</ref>, ekster la teron:
      <ekz>
        usonaj <tld/>oj reflugos kosmen post kelkaj semajnoj
        <fnt>
        <aut>R. Peres-Franko</aut>
        <vrk>Re: Kurantaj aferoj</vrk>
        <lok>soc.culture.esperanto, 2005-06-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_k;&c_o;&c_s;&c_m;&c_o;&c_n;&c_a;&c_v;&c_t;</trd>
  <trd lng="ca">astronauta</trd>
  <trdgrp lng="de">
    <trd>Astronaut</trd>,
    <trd>Kosmonaut</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>astronauta</trd>,
    <trd>cosmonauta</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_k;&f_y;&a_h;&a_A;&a_n;&zwnj;&a_n;&a_w;&a_r;&a_d;</trd>,
    <trd>&a_f;&a_D;&a_A;&a_n;&a_w;&a_r;&a_d;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>astronaute</trd>,
    <trd>cosmonaute</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&udblac;rhaj&oacute;s</trd>,
    <trd>asztronauta</trd>,
    <trd>kozmonauta</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>astronauta</trd>,
    <trd>cosmonauta</trd>
  </trdgrp>
  <trd lng="nl">kosmonaut</trd>
  <trd lng="pt">cosmonauta</trd>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_m;&c_o;&c_n;&c_a;&c_v;&c_t;</trd>
</drv>
</art>

