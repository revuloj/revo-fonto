<drv mrk="encikl1.0o">
  <kap><ofc>2</ofc><tld/>o</kap>
  <snc mrk="encikl1.0o.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Verko, enhavanta la resuman tuton de la homaj konoj pri unu
      a&ubreve;
      &ccirc;iuj sciencoj, ordinare farata en formo de vortaro:
      <ekz>
        la&ubreve;longe de la muro staris du libro&scirc;rankoj, el kiuj
        tra vitroj
        estis vidataj luksaj binda&jcirc;oj de <tld/>o kaj aliaj libroj
        <fnt><aut>Ivan G. &Scirc;irjaev</aut>, <vrk>Sen
        titolo</vrk>, <lok>&ccirc;apitro 11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_c;&c_y;&c_k;&c_l;&c_ja;&c_p;&c_e;&c_d;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_n;&c_c;&c_i;&c_k;&c_l;&c_o;&c_p;&c_je;&c_d;&c_i;&c_ja;</trd>
  <trd lng="ca">enciclop&egrave;dia</trd>
  <trdgrp lng="cs">
    <trd>encyklopedie</trd>,
    <trd>nau&ccaron;. slovn&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Enzyklop&auml;die</trd>,
     <trd>Konversationslexikon</trd>,
     <trd>Lexikon</trd>,
     <trd>Universallexikon</trd>,
     <trd>universelles <ind>Nachschlagewerk</ind></trd>
  </trdgrp>
  <trd lng="en">encyclopedia</trd>
  <trd lng="es">enciclopedia</trd>
  <trd lng="fr">encyclop&eacute;die</trd>
  <trdgrp lng="hu">
     <trd>enciklop&eacute;dia</trd>,
     <trd>tudom&aacute;nyt&aacute;r</trd>
  </trdgrp>
  <trd lng="ja">&#30334;&#31185;&#20107;&#20856;  <pr>&#12402;&#12419;&#12387;&#12363;&#12376;&#12390;&#12435;</pr></trd>
  <trd lng="nl">encyclopedie</trd>
  <trd lng="pl">encyklopedia</trd>
  <trd lng="pt">enciclop&eacute;dia</trd>
  <trd lng="ru">&c_e;&c_n;&c_c;&c_i;&c_k;&c_l;&c_o;&c_p;&c_je;&c_d;&c_i;&c_ja;</trd>
