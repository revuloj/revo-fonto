  <trd lng="uk">&c_je;&c_n;&c_c;&c_i;&c_k;&c_l;&c_o;&c_p;&c_je;&c_d;&c_ib;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#30334;&#31185;&#20840;&#20070; <pr>b&#462;ik&emacron;qu&aacute;nsh&umacron;</pr></trd>,
    <trd>&#30693;&#35672;&#25163;&#20874; <pr>zh&imacron;shish&#466;uc&egrave;</pr></trd>,
    <trd>&#30334;&#31185;&#36766;&#20856; <pr>b&#462;ik&emacron;c&iacute;di&#462;n</pr></trd>,
    <trd>&#24037;&#20855;&#20070; <pr>g&omacron;ngj&ugrave;sh&umacron;</pr></trd>,
    <trd>&#21442;&#32771;&#20070; <pr>c&amacron;nk&#462;osh&umacron;</pr></trd>
  </trdgrp>
</drv>
<drv mrk="encikl1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Resumanta la tuton de la homaj konoj pri unu a&ubreve; &ccirc;iuj
      fakoj:
      <ekz>
        <tld/>a vortaro, spirito.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_n;&c_c;&c_y;&c_k;&c_l;&c_ja;&c_p;&c_e;&c_d;&c_y;&c_ch;&c_n;&c_y;</trd>
