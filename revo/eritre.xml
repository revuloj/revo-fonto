<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>eritre</rad>/o
</kap>

<drv mrk="eritre.0o">
  <kap>
    <tld lit="E"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="eritre.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la nordorienta <ref tip="malprt"
      cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Asmero.
      Teritorio de Respubliko Eritreo.
      ISO-3166 kodo: ER.
      &Gcirc;i limas kun
      <ref tip="vid" cel="etiop.0ujo.GEOG">Etiopujo</ref>,
      <ref tip="vid" cel="gxibuti.0o.GEOG">&Gcirc;ibutio</ref>,
      <ref tip="vid" cel="sudan.0o.GEOG">Sudano</ref>
      kaj <ref cel="mar.RugxaM0o">Ru&gcirc;a Maro</ref>.
    </dif>
    <trd lng="bg">&c_Je;&c_r;&c_i;&c_t;&c_r;&c_je;&c_ja;</trd>
    <trd lng="de">Eritrea</trd>
    <trd lng="en">Eritrea</trd>
    <trd lng="fa">&a_A;&a_r;&f_y;&a_t;&a_r;&a_h;</trd>
    <trdgrp lng="pl">
      <trd>Erytrea</trd>
    </trdgrp>
  </snc>
</drv>

</art>

