<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>krim</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="krim.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="krim.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Ago, per kiu morala a&ubreve; civila le&gcirc;o estas plej grave
      malobservata (mortigo, malvirgigo ktp).
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="kulp.0o">kulpo</ref>,
    <ref cel="pek.0o">peko</ref>
  </refgrp>
  <trd lng="be">&c_z;&c_l;&c_a;&c_ch;&c_y;&c_n;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="ca">crim</trd>
  <trdgrp lng="de">
    <trd>Straftat</trd>,
    <trd>Verbrechen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>crime</trd>,
    <trd>felony</trd>,
    <trd>offense</trd>,
    <trd>offence</trd>
  </trdgrp>
  <trd lng="es">crimen</trd>
  <trdgrp lng="fr">
    <trd>crime</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&udblac;ncselekm&eacute;ny</trd>,
    <trd>b&udblac;ntett</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>misdrijf</trd>,
    <trd>misdaad</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>przest&eogonek;pstwo</trd>,
    <trd>zbrodnia</trd>
  </trdgrp>
  <trd lng="pt">crime</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_z;&c_l;&c_o;&c_d;&c_je;&c_ja;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="krim.0a">
  <kap><tld/>a</kap>
  <snc mrk="krim.0a.iu">
    <dif>
      Kulpa je krimo:
      <ekz>
        <tld/>a homo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi pensas, ke nia malatento estas <tld/>a, kiam ni ignoras tion.
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_l;&c_a;&c_ch;&c_y;&c_n;&c_n;&c_y;</trd>
    <trdgrp lng="en">
      <trd>criminal</trd>,
      <trd>felonious</trd>
    </trdgrp>
    <trd lng="es">criminal</trd>
    <trd lng="hu">b&udblac;n&ouml;s</trd>
    <trdgrp lng="pl">
      <trd>winny <ind>przest&eogonek;pstwa</ind></trd>,
      <trd>winny <ind>zbrodni</ind></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>misdadig</trd>,
      <trd>crimineel<klr tip="ind"> (bn.)</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_s;&c_t;&c_u;&c_p;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="krim.0a.io">
    <dif>
      Konsistiganta <tld/>on:
      <ekz>
        <tld/>a faro.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_l;&c_a;&c_ch;&c_y;&c_n;&c_n;&c_y;</trd>,
      <trd>&c_k;&c_r;&c_y;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    </trdgrp>
    <trd lng="de">strafbar</trd>
    <trd lng="en">criminal</trd>
    <trdgrp lng="hu">
      <trd>b&udblac;n-</trd>,
      <trd>b&udblac;n&ouml;s</trd>
    </trdgrp>
    <trd lng="nl">strafbaar</trd>
    <trdgrp lng="pl">
      <trd>przest&eogonek;pczy</trd>,
      <trd>zbrodniczy</trd>,
      <trd>wyst&eogonek;pny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_r;&c_je;&c_s;&c_t;&c_u;&c_p;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_k;&c_r;&c_i;&c_m;&c_i;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="krim.0a.prikrima">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>a kodo
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="krimin.0a">kriminala</ref>
    </refgrp>
    <trd lng="be">&c_k;&c_r;&c_y;&c_m;&c_ib;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
    <trd lng="en">criminal</trd>
    <trd lng="es">criminal</trd>
    <trdgrp lng="hu">
      <trd>b&uuml;ntet&odblac;</trd>,
      <trd>b&udblac;n&uuml;gyi</trd>
    </trdgrp>
    <trd lng="nl">crimineel<klr tip="ind"> (bn.)</klr></trd>
    <trd lng="pl">kryminalny</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_r;&c_i;&c_m;&c_i;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_u;&c_g;&c_o;&c_l;&c_o;&c_v;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="de">verbrecherisch</trd>
  <trd lng="fr">criminel<klr> (adj.)</klr></trd>
</drv>

<drv mrk="krim.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Plenumi <tld/>on:
      <ekz>
        se Ligano uzos la militon, &gcirc;i estos konsiderata kiel
        <tld/>inta kontra&ubreve; &ccirc;iuj ceteraj Liganoj;
      </ekz>
      <ekz>
        tajloro <tld/>is, botisto pendas
        <klr>(senkulpulo pagas pro kulpulo.
        <ref tip="sin"
        cel="pag.0i">pagi</ref>)</klr><fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_mol;&c_d;&c_z;&c_je;&c_j;&c_s;&c_mol;&c_n;&c_ib; &c_c;&c_mol;
  <ind>&c_z;&c_l;&c_a;&c_ch;&c_y;&c_n;&c_s;&c_t;&c_v;&c_a;</ind></trd>
  <trd lng="de">ein <ind>Verbrechen</ind> <ind>begehen</ind></trd>
  <trd lng="en">commit a crime</trd>
  <trd lng="es">cometer un <ind>crimen</ind></trd>
  <trd lng="fr">commettre un <ind>crime</ind></trd>
  <trdgrp lng="hu">
    <trd>b&udblac;n&ouml;zik</trd>,
    <trd>b&udblac;nt&eacute;nyt k&ouml;vet el</trd>,
    <trd>b&udblac;ntettet hajt v&eacute;gre</trd>
  </trdgrp>
  <trd lng="nl">een <ind>misdrijf</ind> begaan</trd>
  <trdgrp lng="pl">
    <trd>pope&lstroke;ni&cacute; <ind>zbrodni&eogonek;</ind></trd>,
    <trd>pope&lstroke;ni&cacute; <ind>przest&eogonek;pstwo</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_o;&c_v;&c_je;&c_r;&c_sh;&c_i;&c_t;&c_mol;
  <ind>&c_p;&c_r;&c_je;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</ind></trd>
</drv>

<drv mrk="krim.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="krim.0eto.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Kontra&ubreve;le&gcirc;a puninda ago, malpli grava ol <tld/>o.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="delikt.0o">delikto</ref>
  </refgrp>
  <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_a;&c_p;&c_a;&c_r;&c_u;&c_sh;&c_e; &c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Gesetzesversto&szlig;</trd>
  <trd lng="en">misdemeanour</trd>
  <trd lng="fr">d&eacute;lit</trd>
  <trdgrp lng="hu">
    <trd>v&eacute;ts&eacute;g</trd>,
    <trd>kih&aacute;g&aacute;s</trd>,
    <trd>szab&aacute;lys&eacute;rt&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>licht <ind>misdrijf</ind></trd>,
    <trd>lichte <ind>misdaad</ind></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wykroczenie</trd>,
    <trd>przewinienie</trd>,
    <trd>wyst&eogonek;pek</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_o;&c_s;&c_t;&c_u;&c_p;&c_o;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_v;&c_o;&c_n;&c_a;&c_r;&c_u;&c_sh;&c_je;&c_n; &c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="krim.0ego">
  <kap><tld/>ego</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Abomeninda <tld/>o.
    </dif>
    <trd lng="be">&c_c;&c_ja;&c_zh;&c_k;&c_a;&c_je; <ind>&c_z;&c_l;&c_a;&c_ch;&c_y;&c_n;&c_s;&c_t;&c_v;&c_a;</ind></trd>
    <trdgrp lng="de">
      <trd>schwere <ind>Straftat</ind></trd>,
      <trd>schweres <ind>Verbrechen</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>atrocity</trd>,
      <trd>outrage</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>s&uacute;lyos b&udblac;ntett</trd>,
      <trd>r&eacute;mtett</trd>,
      <trd>atrocit&aacute;s</trd>,
    </trdgrp>
     <trdgrp lng="nl">
    <trd>zwaar <ind>misdrijf</ind></trd>,
    <trd>zware <ind>misdaad</ind></trd>
  </trdgrp>
    <trd lng="pl">zbrodnia</trd>
    <trd lng="ru">&c_t;&c_ja;&c_zh;&c_k;&c_o;&c_je; <ind>&c_p;&c_r;&c_je;&c_s;&c_t;&c_u;&c_p;&c_l;&c_je;&c_n;&c_i;&c_je;</ind></trd>
  </snc>
</drv>

<drv mrk="krim.0ulo">
  <kap><ofc>*</ofc><tld/>ulo</kap>
  <snc mrk="krim.0ulo.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Homo kiu faris <tld/>on.
    </dif>
  </snc>
  <trd lng="de">Verbrecher</trd>
  <trd lng="nl">misdadiger</trd>
  <trdgrp lng="pl">
    <trd>przest&eogonek;pca</trd>,
    <trd>zbrodniarz</trd>
  </trdgrp>
</drv>

<drv mrk="krim.0osciencisto">
  <kap><tld/>osciencisto, <var><kap><tld/>fakulo</kap></var></kap>
  <snc mrk="krim.0osciencisto.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Fakulo pri <tld/>scienco:
      <ekz>
        ekzistas sepdek kvin parfumoj, kiujn <tld/>fakulo &ccirc;e neceso
        devas 
        distingi unu de la aliaj
        <fnt><aut>A. Conan Doyle, trad. W. Auld</aut>, <vrk><url 
        ref="http://www.esperanto.org/Ondo/Chashund.htm">La &Ccirc;ashundo de
        
        la Baskerviloj</url></vrk>, <lok>Sezonoj,
        
        1998</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">criminologue</trd>
  <trd lng="nl">criminoloog</trd>
</drv>

<drv mrk="krim.0oscienco">
  <kap><tld/>oscienco, <var><kap><tld/>scienco</kap></var></kap>
  <snc mrk="krim.0oscienco.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Studado de <tld/>oj, <tld/>antoj, iliaj rilatoj al socio, medio, 
      le&gcirc;aro.
    </dif>
  </snc>
  <trd lng="fr">criminologie</trd>
  <trd lng="nl">criminologie</trd>
</drv>
</art>
