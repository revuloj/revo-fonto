<art mrk="$Id: gingiv.xml,v 1.25 2023/11/11 11:36:35 revo Exp $">
<kap>
  <rad>gingiv</rad>/o
</kap>
<drv mrk="gingiv.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc mrk="gingiv.0o.ANA">
    <dif>
      <ref tip="dif" cel="karn.dent0o.ANA">Dentokarno</ref>:
      <ekz>
        la viruso ka&ubreve;zas malaltigitan defendkapablon de la kato, sekve de kio povas
        ataki la organismon ordinaraj bakterioj a&ubreve; tiniaj infektoj, tio montri&gcirc;as &ccirc;e
        katoj per pligrandi&gcirc;intaj limfaj ganglioj, inflamaj malsani&gcirc;oj de <tld/>oj,
        konjunktivito, inflamoj de ha&ubreve;to <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=107150">Virusa imunodeficito de katoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_z;&c_ja;&c_s;&c_n;&c_a;</trd>
  <trd lng="bg">&c_z;&c_malmol;&c_b;&c_je;&c_n; &c_v;&c_je;&c_n;&c_je;&c_c;</trd>
  <trd lng="ca">geniva</trd>
  <trdgrp lng="cs">
    <trd>d&aacute;se&ncaron;</trd>,
    <trd>gingiva</trd>
  </trdgrp>
  <trd lng="de">Zahnfleisch</trd>
  <trd lng="es">encia</trd>
  <trd lng="fr">gencive</trd>
  <trd lng="hu">fog&iacute;ny</trd>
  <trd lng="id">gusi</trd>
  <trd lng="it">gengiva</trd>
  <trdgrp lng="ja">
    <trd>&#x6B6F;&#x8089;  <pr>&#x3057;&#x306B;&#x304F;</pr></trd>,
    <trd>&#x6B6F;&#x830E;  <pr>&#x306F;&#x3050;&#x304D;</pr></trd>
  </trdgrp>
  <trd lng="nl">tandvlees</trd>
  <trd lng="pl">dzi&aogonek;s&lstroke;o</trd>
  <trd lng="pt">gengiva</trd>
  <trd lng="ru">&c_d;&c_je;&c_s;&c_n;&c_a;</trd>
  <trd lng="sk">&dcaron;asno</trd>
  <trd lng="tr">di&scedil;eti</trd>
  <trd lng="uk">&#x44F;&#x441;&#x43D;&#x430;</trd>
  <trd lng="zh">&#x7259;&#x9F66; <pr>y&#xE1;y&#xED;n</pr></trd>
</drv>
</art>
