<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: angin.xml,v 1.27 2025/03/02 08:19:43 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>angin</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="angin.0o">
  <kap><tld/>o</kap>
  <snc mrk="angin.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kutime, <ref tip="lst" cel="inflam.0o" lst="voko:malsanoj">inflamo</ref> de
      <ref cel="palat.0a.ANA">palataj</ref>
      <ref cel="tonsil.0o">tonsiloj</ref>
      kaj <ref cel="mukoz.0o">mukozo</ref> de gor&gcirc;o:
      <ekz>
        pro <tld/>o mi devis resti ses tagojn en lito
        <fnt>
          <bib>KrB</bib>
          <lok>Ia stranga atako al mi ligas subite la manon</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia viza&gcirc;o blui&gcirc;is, kiam la laborestro legis la &ccirc;apitron pri la <tld/>o tonsila
        <fnt>
          <bib>VRA</bib>
          <lok>Folke Fridell</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia patrino mortis pro <tld/>o &ndash; malsano kiu
        nuntempe estus facile kuracebla per antibiotikoj
        <fnt>
          <aut>K. Kniivil&auml;, M. Sandelin</aut>,
          <vrk><url ref="http://www.liberafolio.org/Members/kallekn/forpasis-anna-maria-lange">
          Forpasis Anna-Maria Lange</url></vrk>,
          <lok>libera folio, 2016-08-06</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="inflam.gorgx0o">gor&gcirc;inflamo</ref>,
      <ref cel="laring1.0o">laringito</ref>
    </refgrp>
    <rim>
      En <fnt><bib>JED</bib></fnt> ankora&ubreve; aperas la formo &leftquot;an&gcirc;ino&rightquot;.
    </rim>
    <trdgrp lng="zh">
      <trd>&#x54BD;&#x5CE1;&#x708E; <pr>y&amacron;nxi&aacute;y&aacute;n</pr></trd>,
      <trd>&#x54BD;&#x5CFD;&#x708E; <pr>y&amacron;nxi&aacute;y&aacute;n</pr></trd>,
      <trd>&#x55D3;&#x5B50;&#x75BC; <pr>s&#x1CE;ngzit&eacute;ng</pr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_g;&c_ib;&c_n;&c_a;</trd>
  <trd lng="cs">ang&iacute;na</trd>
  <trdgrp lng="de">
    <trd>Angina</trd>,
    <trd>Halsentz&uuml;ndung</trd>
  </trdgrp>
  <trd lng="en">angina</trd>
  <trd lng="es">angina</trd>
  <trd lng="fa">&a_A_madda;&a_n;&f_zh;&f_y;&a_n;</trd>
  <trd lng="fr">angine</trd>
  <trd lng="he">&alef;&#x5B7;&nun;&gimel;&#x5B4;&yod;&nun;&#x5B8;&he;</trd>
  <trdgrp lng="hu">
    <trd>torokgyullad&aacute;s</trd>,
    <trd>angina</trd>
  </trdgrp>
  <trd lng="id">angina</trd>
  <trd lng="ja">&#x30A2;&#x30F3;&#x30AE;&#x30CA;</trd>
  <trdgrp lng="nl">
    <trd>angina</trd>,
    <trd>keelontsteking</trd>
  </trdgrp>
  <trd lng="pl">angina</trd>
  <trd lng="ru">&c_a;&c_n;&c_g;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">ang&iacute;na</trd>
  <trd lng="sv">halsfluss</trd>
  <trd lng="uk">&c_a;&c_n;&c_gu;&c_ib;&c_n;&c_a;</trd>
</drv>
</art>
