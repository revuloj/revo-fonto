<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: sapf.xml,v 1.2 2024/12/09 08:38:22 afido Exp $">
    <kap>
      <ofc>10</ofc><rad>Sapf</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="sapf.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Antikva greka <ref tip="lst" lst="voko:historiaj_personoj" cel="poet.0o">poetino</ref>
      (630, insulo Lesbo &ndash; 570 a.K.):
      <ekz>
        ni rimarkas verkojn de Alkeo, Anakreono, E&ubreve;ripido, Platono, <tld/>o
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Sappho</trd>
    <trd lng="el">&Sigma;&alfa;&pi;&phi;&omega_ton;</trd>
    <trd lng="en">Sappho</trd>
    <trd lng="fr">Sappho</trd>
  </snc>
<trdgrp lng="be">
  <trd>&c_S;&c_a;&c_p;&c_f;&c_o;</trd>,
  <trd>&c_S;&c_a;&c_f;&c_o;</trd>,
  <trd>&c_P;&c_s;&c_a;&c_p;&c_f;&c_o;</trd>
</trdgrp>
</drv>

  </art>

