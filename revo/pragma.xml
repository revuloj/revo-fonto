<art mrk="$Id: pragma.xml,v 1.23 2026/02/23 16:00:22 revo Exp $">
<kap>
  <rad>pragmat</rad>/a
</kap>
<drv mrk="pragma.0a">
  <kap><tld/>a <fnt><bib>PIV1</bib></fnt></kap>
  <snc mrk="pragma.0a.KOMUNE">
    <dif>
      Celanta efikon en la realo; <ref tip="sin" cel="prakti.0ema">praktikema</ref>:
      <ekz>
        la tria nivelo de la problemo estas la <tld/>a, tio estas la 
        demando pri tio, &ccirc;u tiuj tri ekzekutoj utilas al la kuba 
        revolucio a&ubreve; &ccirc;u ili malutilas al &gcirc;i  
        <fnt>
          <aut>H. D. Steffan</aut>,
          <vrk><url ref="http://membres.lycos.fr/vilhelmo/mortpuno.html">Tri
          mortpafoj en Kubo</url></vrk>,
          <lok>2003-04-14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Anne-Sophie</nom> estas <tld/>a, &scirc;i ne komprenas la eduk-sistemon, 
        &leftquot;oni petas de vi studi kaj elekti <em>vian</em> metion, sed se               
        vi provis  nenion, kiel vi povas scii, kio pla&ccirc;os al vi        
        a&ubreve; ne?&rightquot;
        <fnt>
          <aut>D. Kishegy</aut>,
          <vrk><url ref="http://hades-presse.com/archives/n1/sve.shtml">SVE</url></vrk>,
          <lok>Hades, 2004, numero 1a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">pragm&agrave;tic</trd>
  <trd lng="de">pragmatisch</trd>
  <trd lng="en">pragmatic</trd>
  <trd lng="es">pragm&aacute;tico</trd>
  <trdgrp lng="fa">
    <trd>&a_ayn;&a_m;&a_l;&zwnj;&f_g;&a_r;&a_A;&f_y;&a_A;&a_n;&a_h;</trd>,
    <trd>&a_ayn;&a_m;&a_l;&zwnj;&f_g;&a_r;&a_A;</trd>
  </trdgrp>
  <trd lng="fr">pragmatique<klr> (adj.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#23455;&#38555;&#30340;&#12394;  <pr>&#12376;&#12387;&#12373;&#12356;&#12390;&#12365;&#12394;</pr></trd>,
    <trd>&#23455;&#29992;&#20027;&#32681;&#30340;&#12394;  <pr>&#12376;&#12388;&#12424;&#12358;&#12375;&#12421;&#12366;&#12390;&#12365;&#12394;</pr></trd>
  </trdgrp>
  <trd lng="nl">pragmatisch</trd>
  <trd lng="pl">pragmatyczny</trd>
  <trd lng="uk">&c_p;&c_r;&c_a;&c_g;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#23454;&#29992; <pr>sh&iacute;y&ograve;ng</pr></trd>,
    <trd>&#23526;&#29992; <pr>sh&iacute;y&ograve;ng</pr></trd>,
    <trd>&#23454;&#38469; <pr>sh&iacute;j&igrave;</pr></trd>,
    <trd>&#23526;&#38555; <pr>sh&iacute;j&igrave;</pr></trd>,
    <trd>&#23454;&#29992;&#24615; <pr>sh&iacute;y&ograve;ngx&igrave;ng</pr></trd>,
    <trd>&#23526;&#29992;&#24615; <pr>sh&iacute;y&ograve;ngx&igrave;ng</pr></trd>,
    <trd>&#21153;&#23454; <pr>w&ugrave;sh&iacute;</pr></trd>,
    <trd>&#21209;&#23526; <pr>w&ugrave;sh&iacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pragma.0ismo">
  <kap><tld/>ismo <fnt><bib>PV</bib></fnt></kap>
  <snc mrk="pragma.0ismo.KOMUNE">
    <dif>
      <ref tip="super" cel="kondut.0o">Konduto</ref> celanta precipe praktikan utilon:
      <ekz>
        <tld/>ismo por iu unuavice signifas gajni monon per Esperanto, ricevi turisman
        informon a&ubreve; legi menuon en la Internacia Lingvo &ndash; kio ja sendube estas utila
        &ndash;, sed por alia &gcirc;i povas anta&ubreve; &ccirc;io signifi la eblecon legi bonan
        originala&jcirc;on, a&ubreve; literaturan tradukon, a&ubreve; fakan verkon
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        tro <tld/>isma sinteno
        <fnt>
          <bib>Monato</bib>
          <aut>Petro Desmet'</aut>
          <vrk><url ref="&Monato;/2006/008993.php">La unua Zamenhof-monumento de la 21a jarcento:
          dika Pomego!</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu decido respegulas la kreskantan <tld/>ismon en &Ccirc;inio &ndash; iom-post-ioma
        malgravi&gcirc;o de la ideologiaj motivoj kaj prioritati&gcirc;o de la faktoroj ekonomiaj
        <fnt>
          <bib>LOdE</bib>
          <lok> 2000, No 11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="pragma.0ismo.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      <ref tip="super" cel="doktri.0o">Doktrino</ref>, kiu akceptas kiel kriterion de l' vereco nur
      la
      praktikan valoron.
    </dif>
    <refgrp tip="vid">
      <ref cel="empiri.0ismo">empiriismo</ref>,
      <ref cel="oportu.0ismo">oportunismo</ref>,
      <ref cel="util.0ismo">utilismo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_m;&c_a;&c_t;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">pragmatisme</trd>
  <trd lng="de">Pragmatismus</trd>
  <trd lng="en">pragmatism</trd>
  <trd lng="es">pragmatismo</trd>
  <trdgrp lng="fa">
    <trd>&a_ayn;&a_m;&a_l;&zwnj;&f_g;&a_r;&a_A;&f_y;&f_y;</trd>,
    <trd>&a_A;&a_S;&a_A;&a_l;&a_t; &a_ayn;&a_m;&a_l;</trd>
  </trdgrp>
  <trd lng="fr">pragmatisme</trd>
  <trd lng="hu">pragmatizmus</trd>
  <trdgrp lng="ja">
    <trd>&#12503;&#12521;&#12464;&#12510;&#12486;&#12451;&#12474;&#12512;</trd>,
    <trd>&#23455;&#29992;&#20027;&#32681;  <pr>&#12376;&#12388;&#12424;&#12358;&#12375;&#12421;&#12366;</pr></trd>
  </trdgrp>
  <trd lng="nl">pragmatisme</trd>
  <trd lng="pl">pragmatyzm</trd>
  <trd lng="pt">pragmatismo</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_g;&c_m;&c_a;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="uk">&c_p;&c_r;&c_a;&c_g;&c_m;&c_a;&c_t;&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="zh">
    <trd>&#23454;&#29992;&#20027;&#20041; <pr>sh&iacute;y&ograve;ngzh&#468;y&igrave;</pr></trd>,
    <trd>&#23526;&#29992;&#20027;&#32681; <pr>sh&iacute;y&ograve;ngzh&#468;y&igrave;</pr></trd>
  </trdgrp>
</drv>
