<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hemofili.xml,v 1.8 2023/11/22 06:32:58 revo Exp $">
<kap>
  <rad>hemofili</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="hemofili.0o">
  <kap><tld/>o</kap>
  <snc mrk="hemofili.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Hereda sango<ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>, precipe de
      viroj, pro kiu
      sango tro fluemas, ne koaguli&gcirc;as:
      <ekz>
        biologio <klr>...</klr> sukcesis klarigi iujn malsanojn kaj
        strangajn heredecojn, kiel la <tld/>o
        <fnt>
          <aut>G. Lagrange</aut>,
          <vrk><url ref="http://www.satesperanto.org/La-Vivo.html">
          La Vivo</url></vrk>,
          <lok>Sennacieca Revuo, 1965</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i havis sangomalsanon, <tld/>on
        <fnt>
          <aut>S. Lang, trad. G. Berveling</aut>,
          <vrk>La Talpo&ccirc;asisto</vrk>,
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_je;&c_m;&c_a;&c_f;&c_ib;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="ca">hemof&iacute;lia</trd>
  <trdgrp lng="de">
    <trd>H&auml;mophilie</trd>,
    <trd>Bluterkrankheit</trd>
  </trdgrp>
  <trd lng="fr">h&eacute;mophilie</trd>
  <trd lng="id">hemofilia</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_je;&#x43C;&#x43E;&c_f;&c_ib;&#x43B;&c_ib;&#x44F;</trd>,
    <trd>&#x43A;&c_r;&#x43E;&c_v;&#x43E;&c_t;&#x43E;&c_ch;&c_i;&c_v;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
  <trd lng="zh">&#34880;&#x53CB;&#x75C5; <pr>xi&#x11B;y&#x1D2;ub&#xEC;ng</pr></trd>
</drv>
</art>

