<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: zefir.xml,v 1.27 2024/07/24 01:42:59 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>zefir</rad>/o
</kap>
<drv mrk="zefir.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MET</uzo>
    <dif>
      Agrabla milda <ref tip="super" cel="vent.0o.MET">vento</ref>:
      <ekz>
        jen estas mia frato de la okcidento, <klr>[&#8230;]</klr> li havas odoron
        de maro kaj alportas kun si agrablan malvarmon &dash; &ccirc;u tio estas la malgranda <tld lit="Z"/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>?
      </ekz>
      <ekz>
        <tld lit="Z"/>o estis dio de la okcidenta vento en la antikva Grekio,
        kun minusklo estas <tld/>o tre milda agrabla vento responda al la
        indico 2 de la boforta skalo, pli malforta ol brizeto,
        plie <tld/>o estas tre malpeza &scirc;tofo el zefirlano <klr>[&#8230;]</klr> uzata por bebaj trika&jcirc;oj
        k.s.
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49867">Zefiro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_e;&c_f;&c_ib;&c_r;
<klr>(&c_v;&c_je;&c_t;&c_r;&c_y;&c_k;)</klr></trd>
  <trd lng="ca">z&egrave;fir</trd>
  <trd lng="cs">zef&iacute;r</trd>
  <trd lng="de">Zephir</trd>
  <trd lng="el">
&zeta;&epsilon_ton;&phi;&ypsilon;&rho;&omikron;&sigma_fina;</trd>
  <trd lng="en">zephyr</trd>
  <trd lng="es">c&eacute;firo</trd>
  <trd lng="fa">&a_n;&a_s;&f_y;&a_m;</trd>
  <trdgrp lng="fr">
    <trd>brise</trd>,
    <trd>z&eacute;phyr</trd>
  </trdgrp>
  <trd lng="hu">zef&iacute;r</trd>
  <trd lng="id">zefir</trd>
  <trd lng="it">zefiro</trd>
  <trdgrp lng="ja">
    <trd>&#x305D;&#12424;&#x98A8;  <pr>&#x305D;&#12424;&#x304B;&#x305C;</pr></trd>,
    <trd>&#x897F;&#x98A8;  <pr>&#x306B;&#12375;&#x304B;&#x305C;</pr></trd>,
    <trd>&#x8EFD;&#x98A8;  <pr>&#12369;&#12356;&#12405;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">zefier</trd>
  <trd lng="pl">zefir</trd>
  <trd lng="pt">z&eacute;firo</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_je;&c_f;&c_i;&c_r;</trd>,
    <trd>&c_l;&c_jo;&c_g;&c_k;&c_i;&c_j;
    &c_v;&c_je;&c_t;&c_je;&c_r;&c_o;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>v&aacute;nok</trd>,
    <trd>zefyr</trd>
  </trdgrp>
  <trd lng="sv">v&auml;stanfl&auml;kt</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_je;&c_f;&c_ib;&c_r;</trd>,
    <trd>&#x43B;&c_je;&c_g;&#x43A;&c_i;&c_j; &c_v;&c_ib;&c_t;&c_je;&c_r;&c_je;&c_c;&#x44C;</trd>,
    <trd>&#x43B;&c_je;&c_g;&#x43A;&c_a; &c_t;&#x43A;&c_a;&#x43D;&c_i;&#x43D;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F90;&#x98A8; <pr>x&#xFA;f&emacron;ng</pr></trd>,
    <trd>&#x5F90;&#x98CE; <pr>x&#xFA;f&emacron;ng</pr></trd>,
    <trd>&#x7D30;&#34180; <pr>x&#xEC;b&#xF3;</pr></trd>,
    <trd>&#x7EC6;&#34180; <pr>x&#xEC;b&#xF3;</pr></trd>
  </trdgrp>
</drv>
</art>
