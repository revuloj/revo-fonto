<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>zefir</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="zefir.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MET</uzo>
    <dif>
      Agrabla milda <ref tip="super" cel="vent.0o.MET">vento</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_e;&c_f;&c_ib;&c_r; <klr>(&c_v;&c_je;&c_t;&c_r;&c_y;&c_k;)</klr></trd>
  <trd lng="de">Zephir</trd>
  <trd lng="el">&zeta;&epsilon_ton;&phi;&ypsilon;&rho;&omikron;&sigma_fina;</trd>
  <trd lng="en">zephyr</trd>
  <trdgrp lng="fr">
    <trd>brise</trd>,
    <trd>z&eacute;phyr</trd>
  </trdgrp>
  <trd lng="es">c&eacute;firo</trd>
  <trd lng="hu">zef&iacute;r</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_je;&c_f;&c_i;&c_r;</trd>,
    <trd>&c_l;&c_jo;&c_g;&c_k;&c_i;&c_j;
    &c_v;&c_je;&c_t;&c_je;&c_r;&c_o;&c_k;</trd>
  </trdgrp>
  <trd lng="nl">zefier</trd>
  <trd lng="pt">z&eacute;firo</trd>
  <trd lng="sv">v&auml;stanfl&auml;kt</trd>
</drv>
</art>
