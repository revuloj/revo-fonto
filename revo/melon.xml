<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>melon</rad>/o
</kap>
<drv mrk="melon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="melon.0o.specio">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="kukum.0o.genro">kukumo</ref>
      (<trd lng="la">Cucumis melo</trd>), kun grandaj globformaj
      a&ubreve; ovoformaj fruktoj kun sukplena dol&ccirc;a karno plejofte
      flava a&ubreve; verdeta.
    </dif>
  </snc>
  <snc mrk="melon.0o.frukto">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla frukto de <tld/>o <sncref ref="melon.0o.specio"/>.
    </dif>
  </snc>
  <trd lng="de">Melone</trd>
  <trd lng="fr">melon</trd>
</drv>
<drv mrk="melon.akvo0o">
  <kap><ofc>*</ofc>akvo<tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Globforma frukto de afrika kukurbaco (<trd lng="la">Citrullus lanatus</trd>),
      kun ru&gcirc;a, flava a&ubreve; blanka sukplena karno.
    </dif>
  </snc>
  <trd lng="de">Wassermelone</trd>
</drv>
</art>
