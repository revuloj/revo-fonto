<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>melon</rad>/o
</kap>
<drv mrk="melon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="melon.0o.specio">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="kukum.0o.genro">kukumo</ref>
      (<trd lng="la">Cucumis melo</trd>), kun grandaj globformaj
      a&ubreve; ovoformaj fruktoj kun sukplena dol&ccirc;a karno plejofte
      flava a&ubreve; verdeta.
    </dif>
  </snc>
  <snc mrk="melon.0o.frukto">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref> de <tld/>o <sncref ref="melon.0o.specio"/>.
    </dif>
  </snc>
  <trd lng="de">Melone</trd>
  <trd lng="en">melon</trd>
  <trd lng="es">mel&oacute;n</trd>
  <trd lng="fr">melon</trd>
  <trdgrp lng="hu">
    <trd>s&aacute;rgadinnye</trd>,
    <trd>cukordinnye</trd>
  </trdgrp>
  <trd lng="nl">meloen</trd>
  <trd lng="pt">mel&atilde;o</trd>
  <trd lng="ru">&c_d;&c_y;&c_n;&c_ja;</trd>
  <trd lng="sv">melon</trd>
</drv>

<drv mrk="melon.akvo0o">
  <kap>
  <ofc>*</ofc>akvo<tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Globforma <ref tip="super" cel="frukt.0o.KUI">frukto</ref>
      de afrika kukurbaco (<trd lng="la">Citrullus lanatus</trd>),
      kun ru&gcirc;a, flava a&ubreve; blanka sukplena karno.
    </dif>
  </snc>
  <trd lng="de">Wassermelone</trd>
  <trd lng="en">watermelon</trd>
  <trd lng="es">sand&iacute;a</trd>
  <trd lng="hu">g&ouml;r&ouml;gdinnye</trd>
  <trd lng="nl">watermeloen</trd>
  <trd lng="pt">melancia</trd>
  <trd lng="ru">&c_a;&c_r;&c_b;&c_u;&c_z;</trd>
  <trd lng="sv">vattenmelon</trd>
</drv>
</art>
