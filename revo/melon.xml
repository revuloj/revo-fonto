<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: melon.xml,v 1.27 2020/02/27 17:10:29 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>melon</rad>/o
</kap>
<drv mrk="melon.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="melon.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="kukum.0o.genro">kukumo</ref>
      kun grandaj globformaj a&ubreve; ovoformaj fruktoj kun sukplena
      dol&ccirc;a karno plej ofte flava a&ubreve; verdeta (<trd lng="la">Cucumis
      melo</trd>):
      <ekz>
        la <tld/>oj kaj tritiko, semitaj en novembro, jam estis
        maturi&gcirc;antaj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/2/22/Cucumis_melo_34.jpg/640px-Cucumis_melo_34.jpg"
      prm="CC0"
      ><tld/>o kun frukto (Cucumis melo)
        <fnt>
          <aut>The Photographer, propra verko</aut>
          <lok><url ref="&WCW;/File:Cucumis_melo_34.jpg"
              >https://commons.wikimedia.org/wiki/File:Cucumis_melo_34.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <snc mrk="melon.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Man&gcirc;ebla <ref tip="super" cel="frukt.0o.KUI">frukto</ref>
      de <tld/>o <sncref ref="melon.0o.BOT"/>:
      <ekz>
        ni memoras la fi&scirc;ojn, kiujn ni man&gcirc;is en Egiptujo senpage, la
        kukumojn kaj la
        <tld/>ojn kaj la poreojn kaj la bulbojn kaj la ajlojn
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 11:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o, kovrita de delikataj vejnetoj
        <fnt>
          <bib>IK</bib>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/8/89/Citrullus_lanatus5SHSU.jpg/640px-Citrullus_lanatus5SHSU.jpg"
      prm="CC BY 3.0"
      >akvo<tld/>o (Citrullus lanatus)
        <fnt>
          <aut>Shu Suehiro, propra verko</aut>
          <lok><url ref="&WCW;/File:Citrullus_lanatus5SHSU.jpg"
              >https://commons.wikimedia.org/wiki/File:Citrullus_lanatus5SHSU.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="br">aval-sukrin</trd>
  </snc>
  <trd lng="bg">&c_p;&c_mol;&c_p;&c_malmol;&c_sh;</trd>
  <trd lng="br">sukrinenn</trd>
  <trd lng="cs">meloun</trd>
  <trd lng="de">Melone</trd>
  <trd lng="en">melon</trd>
  <trd lng="es">mel&oacute;n</trd>
  <trd lng="fa">&a_kh;&a_r;&a_b;&a_z;&a_h;</trd>
  <trd lng="fr">melon</trd>
  <trdgrp lng="he">
    <trd>&mem;&lamed;&vav;&fnun;<klr tip="ind">
    (&alef;&bet;&tet;&yod;&het; &tsadi;&he;&vav;&bet;)</klr></trd>,
    <trd>&alef;&bet;&tet;&yod;&het; &tsadi;&he;&vav;&bet;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>s&aacute;rgadinnye</trd>,
    <trd>cukordinnye</trd>
  </trdgrp>
  <trd lng="nl">meloen</trd>
  <trd lng="pl">melon</trd>
  <trd lng="pt">mel&atilde;o</trd>
  <trd lng="ru">&c_d;&c_y;&c_n;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>&zcaron;lt&aacute; dy&ncaron;a</trd>,
    <trd>&zcaron;lt&yacute; mel&oacute;n</trd>
  </trdgrp>
  <trd lng="sv">melon</trd>
</drv>

<drv mrk="melon.akvo0o">
  <kap><ofc>*</ofc>akvo<tld/>o</kap>
  <snc>
    <uzo tip="fak">KUI</uzo>
    <dif>
      Globforma <ref tip="super" cel="frukt.0o.KUI">frukto</ref>
      de afrika kukurbaco kun ru&gcirc;a, flava a&ubreve; blanka sukplena karno
      (<trd lng="la">Citrullus lanatus</trd>, <trd lng="la">Citrullus
      vulgaris</trd>):
      <ekz>
        la almozulo sidas sur la &ccirc;irka&ubreve;hakita marmora &scirc;tono, refre&scirc;igas
        sin per sukplena
        akvo<tld/>o kaj forrevas sian vivon
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la tablo ekzemple vi vidos akvo<tld/>on, &#8208; &gcirc;i kostas sepcent
        rublojn
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto tria</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_d;&c_i;&c_n;&c_ja;</trd>
  <trd lng="br">sukrinenn-dour</trd>
  <trd lng="cs">meloun vodn&iacute;</trd>
  <trd lng="de">Wassermelone</trd>
  <trd lng="en">watermelon</trd>
  <trd lng="es">sand&iacute;a</trd>
  <trd lng="fa">&a_h;&a_n;&a_d;&a_w;&a_A;&a_n;&a_h;</trd>
  <trd lng="fr">past&egrave;que</trd>
  <trd lng="he">&alef;&bet;&tet;&yod;&het;</trd>
  <trd lng="hu">g&ouml;r&ouml;gdinnye</trd>
  <trd lng="nl">watermeloen</trd>
  <trdgrp lng="pl">
    <trd>arbuz</trd>,
    <trd>kawon</trd>
  </trdgrp>
  <trd lng="pt">melancia</trd>
  <trd lng="ru">&c_a;&c_r;&c_b;&c_u;&c_z;</trd>
  <trd lng="sk">vodn&yacute; mel&oacute;n</trd>
  <trd lng="sv">vattenmelon</trd>
</drv>
</art>
