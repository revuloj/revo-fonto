<vortaro>
<art mrk="$Id: glauxk.xml,v 1.13 2022/03/19 19:18:23 revo Exp $">
<kap>
  <rad>gla&ubreve;konit</rad>/o
</kap>
<drv mrk="glauxk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Speco de verda <ref tip="super" cel="minera.0o">mineralo</ref>:
      <ekz>
        en Kretaceo, organikaj ruba&jcirc;oj kaj folietoj da kvarco kaj glimo amasi&gcirc;is en
        malprofundaj maroj, tie formi&gcirc;is <tld/>o, verda argilo, kiu entenas feroksidon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=26103">Okro</url></lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_w;&c_k;&c_a;&c_n;&c_ib;&c_t;</trd>
  <trd lng="ca">glauconita</trd>
  <trd lng="de">Glaukonit</trd>
  <trd lng="hu">glaukonit</trd>
  <trd lng="nl">glauconiet</trd>
  <trdgrp lng="pt">
    <trd>glauconita</trd>,
    <trd>glauc&ocirc;nia</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_l;&c_a;&c_u;&c_k;&c_o;&c_n;&c_i;&c_t;</trd>
  <trd lng="uk">&#x433;&#x43B;&#x430;&#x443;&#x43A;&#x43E;&#x43D;&#x456;&#x442;</trd>
</drv>
</art>
