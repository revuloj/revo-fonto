<kap>
<rad>stakt</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="stakt.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BIB</uzo>
    <dif>
      <ref tip="super" cel="arom.0ajxo">Aroma&jcirc;o</ref> uzita de 
      la antikvaj hebreoj por prepari <ref cel="incens.0o">incenso</ref>n:  
    </dif>
    <ekz>
      prenu al vi [&#x2026;] balzamon kaj <tld/>on kaj galbanon bonodoran kaj puran
      olibanon, po egalaj partoj el &ccirc;io<fnt><bib>MT</bib></fnt>.
    </ekz>
  </snc>
  <trd lng="cs">staktum</trd>
  <trd lng="de">Stakte</trd>
  <trd lng="en">stacte</trd>
  <trd lng="fr">stact&eacute;</trd>
  <trdgrp lng="hu">
    <trd>mirham&eacute;zga</trd>,
    <trd>stacta</trd>
  </trdgrp>
  <trd lng="ja">&#x30B7;&#x30A7;&#x30D8;&#x30EC;&#x30C6;&#x9999;  <pr>&#x30B7;&#x30A7;&#x30D8;&#x30EC;&#x30C6;&#x304B;&#x304A;&#x308A;</pr></trd>
  <trd lng="ko">&#xC18C;&#xD569;&#xD5A5;</trd>
  <trd lng="sk">kadidlo <klr>(&zcaron;ivica)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x43C;&#x438;&#x440;&#x43D;&#x430;</trd>,
    <trd>&#x430;&#x440;&#x43E;&#x43C;&#x430;&#x442;&#x438;&#x447;&#x43D;&#x430; &#x441;&#x43C;&#x43E;&#x43B;&#x430;</trd>
  </trdgrp>
</drv>
</art>
