<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stakt.xml,v 1.14 2025/10/07 14:32:48 revo Exp $">
<kap>
<rad>stakt</rad>/o
<fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stakt.0o">
  <kap><tld/>o</kap>
  <snc mrk="stakt.0o.aromajxo">
    <uzo tip="fak">BIB</uzo>
    <dif>
      <ref tip="super" cel="arom.0ajxo">Aroma&jcirc;o</ref> uzita de
      la antikvaj hebreoj por prepari <ref cel="incens.0o">incenso</ref>n:
      <ekz>
        prenu al vi aroma&jcirc;on: balzamon kaj <tld/>on kaj galbanon bonodoran kaj puran
        olibanon, po egalaj partoj el &ccirc;io;
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 30:34</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_k;&c_t;&c_y;</trd>,
    <trd>&c_s;&c_t;&c_y;&c_r;&c_a;&c_k;&c_s;&c_a;&c_v;&c_y; &c_b;&c_a;&c_l;&c_mol;&c_z;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="cs">staktum</trd>
  <trd lng="de">Stakte</trd>
  <trd lng="en">stacte</trd>
  <trd lng="fr">stact&eacute;</trd>
  <trdgrp lng="hu">
    <trd>mirham&eacute;zga</trd>,
    <trd>stacta</trd>
  </trdgrp>
  <trd lng="ja">&#x30B7;&#x30A7;&#x30D8;&#x30EC;&#x30C6;&#x9999;
  <pr>&#x30B7;&#x30A7;&#x30D8;&#x30EC;&#x30C6;&#x304B;&#x304A;&#x308A;</pr></trd>
  <trd lng="ko">&#xC18C;&#xD569;&#xD5A5;</trd>
  <trd lng="sk">kadidlo <klr>(&zcaron;ivica)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_m;&c_i;&c_r;&c_n;&c_a;</trd>,
    <trd>&c_a;&c_r;&c_o;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_n;&c_a; &c_s;&c_m;&c_o;&c_l;&c_a;</trd>
  </trdgrp>
</drv>
</art>
