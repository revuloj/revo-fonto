<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: limnologi.xml,v 1.8 2024/01/15 19:02:15 revo Exp $">
<kap>
  <rad>limnologi</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>

<drv mrk="limnologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="limnologi.0o.SCI">
    <uzo tip="fak">EKOL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Mar&ccirc;oscienco: studo de enlandaj akvomedioj, kiaj mar&ccirc;oj, lagoj, 
      riveroj...:
      <ekz>
        kiam mi estis infano, mi &scirc;atis legi miraklajn historiojn pri 
        estonta mondo, kiam hejmmastrumantinoj rekvalifiki&gcirc;os al 
        docentoj de <tld/>o
        <fnt>
          <aut>S. Lem, trad. K. Du&scirc;enko, trad. J. Finkel</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Lem/index.html">
          Surloka esploro</url></vrk>,
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;i-foja esplorado inkluzivas fakojn de geologio, ekologio, 
        <tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.espero.com.cn/z05-00a/05z-00-207.htm">
          Sciencistoj de &Ccirc;inio...</url></vrk>,
          <lok>2005-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_m;&c_n;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="de">Limnologie</trd>
  <trd lng="fr">limnologie</trd>
  <trdgrp lng="nl">
    <trd>zoetwaterbiologie</trd>,
    <trd>limnologie</trd>
  </trdgrp>
  <trd lng="pl">limnologia</trd>
  <trd lng="pt">limnologia</trd>
  <trd lng="ro">limnologie</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_ib;&#x43C;&#x43D;&#x43E;&#x43B;&#x43E;&c_g;&c_ib;&#x44F;</trd>,
    <trd>&#x43E;&c_z;&c_je;&c_r;&#x43E;&c_z;&#x43D;&c_a;&c_v;&c_s;&c_t;&c_v;&#x43E;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6E56;&#x6CBC;&#x5B66; <pr>h&#xFA;zh&#x1CE;oxu&#xE9;</pr></trd>,
    <trd>&#x6E56;&#x6CBC;&#x5B78; <pr>h&#xFA;zh&#x1CE;oxu&#xE9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="limnologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="limnologi.0isto.SCI">
    <uzo tip="fak">EKOL</uzo>
    <uzo tip="fak">SCI</uzo>
    <dif>
      Mar&ccirc;osciencisto, fakulo pri <ref tip="vid" cel="limnologi.0o.SCI"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_m;&c_n;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="de">Limnologe</trd>
  <trdgrp lng="fr">
    <trd>limnologiste</trd>,
    <trd>limnologue</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zoetwaterbioloog</trd>,
    <trd>limnoloog</trd>
  </trdgrp>
  <trd lng="pl">limnolog</trd>
  <trdgrp lng="pt">
    <trd>limnologista</trd>,
    <trd>limn&oacute;logo</trd>
  </trdgrp>
</drv>
</art>

