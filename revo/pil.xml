<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>5</ofc>
  <rad>pil</rad>/o
</kap>
<drv mrk="pil.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Fonto de elektra energio, uzanta la transformi&gcirc;on de la
      &hcirc;emia
      energio:
      <ekz>
        etetaj akumulatoroj alternos &scirc;argi la <tld/>ojn de
        korstimulilo<fnt><aut>Gianfranco Cazzaro</aut>: <vrk>Mikroskope
        vastaj horizontoj</vrk>, <bib>Monato</bib><lok>jaro 1997a,
        numero
        2a, p. 12a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akumul1.0o">akumulatoro</ref>,
      <ref cel="bateri.0o.ELE">baterio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;
    &c_e;&c_l;&c_je;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_b;&c_a;&c_t;&c_a;&c_r;&c_e;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Monozelle</trd>,
    <trd>Batterie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>battery</trd>,
    <trd>battery cell</trd>,
    <trd>single cell battery</trd>,
    <trd>pile<klr> (electric)</klr></trd>
  </trdgrp>
  <trd lng="es">pila<klr> (el&eacute;ctrica)</klr></trd>
  <trdgrp lng="fa">
    <trd>&f_p;&f_y;&a_l;<klr tip="ind"> (&a_b;&a_A;&a_t;&a_r;&f_y;)</klr></trd>,
    <trd>&a_b;&a_A;&a_t;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">pile<klr> (&eacute;lectrique)</klr></trd>
  <trd lng="hu">elem<klr> (villamos)</klr></trd>
  <trd lng="nl">element<klr tip="ind"> (v.batterij)</klr></trd>
  <trd lng="pt">pilha</trd>
  <trd lng="pl">stos<klr tip="amb"> elektryczny</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t; <klr
    tip="amb">(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;)</klr></trd>,
    <trd>&c_b;&c_a;&c_t;&c_a;&c_r;&c_je;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
</drv>
</art>
