<art mrk="$Id: agav.xml,v 1.30 2023/10/28 08:32:44 revo Exp $">
<kap>
  <rad>agav</rad>/o
</kap>
<drv mrk="agav.0o">
  <kap><tld/>o</kap>
  <snc mrk="agav.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> de <ref tip="malprt" cel="liliac.0oj">liliacoj</ref>
      (<trd lng="la">Agave</trd>), simila al aloo:
      <ekz>
        por la antikvaj popoloj de la lando Meksiko, <tld/>o estis dio-planto, la nobla
        vegetalo el kiu oni eltiris &ccirc;ion por subteni la vivon
        <fnt>
          <aut>Fidel Figueroa</aut>
          <vrk><url ref="&Monato;/2000/007033.php">Nepre &gcirc;ia nomo estas Meksiko</url></vrk>
          <bib>Monato</bib>
          <lok>2000/10, p. 14</lok>
          </fnt>;
      </ekz>
      <ekz>
        <tld/>oj <klr>[&#x2026;]</klr> estas ofte arbaj plantoj kun malalta trunko,
        disvasti&gcirc;intaj en tropikaj, subtropikaj, moderaj klimatoj, la lancoformaj,
        mallar&gcirc;aj folioj kreskas el la bazo <klr>[kaj]</klr> estas karnecaj, dentaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=129968">Agavo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/3/3b/Agave_July_2011-1.jpg/427px-Agave_July_2011-1.jpg" prm="CC BY-SA     3.0">florantaj <tld/>oj, Porto Covo, Portugalujo
        <fnt>
          <aut>Alvesgaspar, propra verko</aut>
          <lok><url ref="&WCW;/File:Agave_July_2011-1.jpg">&WCW;/File:Agave_July_2011-1.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="zh">
    <trd>&#x9F8D;&#x820C;&#x862D;&#x5C6C;&#x690D;&#x7269; <pr>l&#xF3;ngsh&#xE9;l&#xE1;nsh&#x1D4;zh&#xED;w&#xF9;</pr></trd>,
    <trd>&#x9F99;&#x820C;&#x5170;&#x5C5E;&#x690D;&#x7269; <pr>l&#xF3;ngsh&#xE9;l&#xE1;nsh&#x1D4;zh&#xED;w&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_g;&c_a;&c_v;&c_a;</trd>
  <trd lng="cs">ag&aacute;ve</trd>
  <trd lng="de">Agave</trd>
  <trd lng="en">agave</trd>
  <trd lng="es">agave</trd>
  <trd lng="fa">&a_A_madda;&f_g;&a_A;&a_w;</trd>
  <trd lng="fr">agave</trd>
  <trd lng="he">&#x5E6;&#x5DE;&#x5D7; &#x5D0;&#x5D2;&#x5D1;&#x5D4;</trd>
  <trd lng="hu">ag&aacute;v&eacute;</trd>
  <trd lng="it">agave</trd>
  <trdgrp lng="ja">
    <trd>&#x30EA;&#x30E5;&#x30A6;&#x30BC;&#x30C4;&#x30E9;&#x30F3;</trd>,
    <trd>&#x30A2;&#x30AC;&#x30D9;</trd>
  </trdgrp>
  <trd lng="nl">agave</trd>
  <trd lng="pl">agawa</trd>
  <trd lng="pt">agave</trd>
  <trd lng="ru">&c_a;&c_g;&c_a;&c_v;&c_a;</trd>
  <trd lng="sk">ag&aacute;ve</trd>
  <trd lng="sv">agave</trd>
  <trd lng="uk">&c_a;&c_gu;&c_a;&c_v;&c_a;</trd>
</drv>
