<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stift</rad>/o
</kap>
<drv mrk="stift.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Metala &scirc;topnajleto, uzata por kunfiksi du pecojn:
      <ekz>
        al&gcirc;ustiga, duobla, fiksa, konusa <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de elektra &scirc;topilo;
      </ekz>
      <ekz>
        <ind>fendo<tld/>o</ind>,
        <trd lng="hu">sasszeg</trd>
      </ekz>
      <ekz>
        <ind>&scirc;ra&ubreve;bo<tld/>o</ind>.
        <trd lng="hu">csavarszeg</trd>
      </ekz>      
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_y;&c_f;&c_t;</trd>
  <trd lng="de">Stift</trd>
  <trd lng="en">pin</trd>
  <trdgrp lng="fr">
    <trd>goupille</trd>,
    <trd>broche<klr tip="ind"> (de fiche &eacute;lectrique)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csap<klr tip="ind"> (r&ouml;gz&iacute;t&odblac;)</klr></trd>,
    <trd>csapszeg</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stift</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_t;&c_i;&c_f;&c_t;</trd>
</drv>
</art>
