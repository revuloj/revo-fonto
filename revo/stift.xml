<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stift.xml,v 1.23 2023/11/26 12:39:41 revo Exp $">
<kap>
  <rad>stift</rad>/o <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="stift.0o">
  <kap><tld/>o</kap>
  <snc mrk="stift.0o.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Metala stangeto, uzata por kunfiksi du pecojn:
      <ekz>
        najlo estas <tld/>o kun kapo;
      </ekz>
      <ekz>
        al&gcirc;ustiga, cilindra, duobla, fiksa, konusa, stre&ccirc;a <tld/>o;
      </ekz>
      <ekz>
        <ind>fendo<tld/>o</ind>;
        <trd lng="de">Splint</trd>
        <trd lng="en">split pin</trd>
        <trd lng="fr">goupille fendue</trd>
        <trd lng="hu">sasszeg</trd>
      </ekz>
      <ekz>
        <ind>&scirc;ra&ubreve;bo<tld/>o</ind>;
        <trdgrp lng="de">
          <trd>Gewindestift</trd>,
          <trd>Madenschraube</trd>
        </trdgrp>
        <trd lng="en">headless screw</trd>
        <trd lng="hu">csavarszeg</trd>
      </ekz>
      <ekz>
        <tld/>o de elektra &scirc;topilo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bolt.0o">bolto</ref>,
      <ref cel="kejl.0o">kejlo</ref>,
      <ref cel="najl.0o">najlo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_y;&c_f;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>kol&iacute;k</trd>,
    <trd>kol&iacute;&ccaron;ek</trd>,
    <trd>n&yacute;tek</trd>,
    <trd>&ccaron;ep</trd>
  </trdgrp>
  <trd lng="de">Stift <klr>(Technik)</klr></trd>
  <trd lng="en">pin</trd>
  <trdgrp lng="fr">
    <trd>goupille</trd>,
    <trd>broche<klr tip="ind"> (de fiche &eacute;lectrique)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csap<klr tip="ind"> (r&ouml;gz&iacute;t&odblac;)</klr></trd>,
    <trd>csapszeg</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12367;&#12373;&#12403;&#26643;  <pr>&#12367;&#12373;&#12403;&#12379;&#12435;</pr></trd>,
    <trd>&#12461;&#12540;  <pr>&#12365;&#12540;</pr></trd>,
    <trd>&#12500;&#12531;</trd>,
    <trd>&#12467;&#12483;&#12479;</trd>,
    <trd>&#30446;&#37336;  <pr>&#12417;&#12367;&#12366;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stift</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sztyft</trd>,
    <trd>zawleczka</trd>,
    <trd>ko&lstroke;ek</trd>,
    <trd>klin</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_t;&c_i;&c_f;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>kol&iacute;k</trd>,
    <trd>nit</trd>,
    <trd>&ccaron;ap</trd>
  </trdgrp>
  <trd lng="uk">&c_sh;&c_t;&c_i;&c_f;&c_t;</trd>
</drv>

<drv mrk="stift.0i">
  <kap><tld/>i</kap>
  <snc mrk="stift.0i.MAS">
    <dif>
      Kunigi, fiksi per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_m;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; <ind>&c_sh;&c_t;&c_y;&c_f;&c_t;&c_a;&c_m;</ind></trd>,
    <trd>&c_z;&c_a;&c_p;&c_ib;&c_r;&c_a;&c_c;&c_mol; <ind>&c_sh;&c_t;&c_y;&c_f;&c_t;&c_a;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>upevnit kol&iacute;&ccaron;kem</trd>,
    <trd>za&ccaron;epovat</trd>
  </trdgrp>
  <trd lng="de">verstiften</trd>
  <trd lng="fr">goupiller</trd>
  <trd lng="ja">&#12367;&#12373;&#12403;&#26643;&#12391;&#30041;&#12417;&#12427;  <pr>&#12367;&#12373;&#12403;&#12379;&#12435;&#12391;&#12392;&#12393;&#12417;&#12427;</pr></trd>
  <trd lng="pl">klinowa&cacute;</trd>
  <trdgrp lng="sk">
    <trd>upevni&tcaron; kol&iacute;kom</trd>,
    <trd>za&ccaron;apova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_k;&c_r;&c_ib;&c_p;&c_l;&c_ju;&c_v;&c_a;&c_t;&c_i;
    &c_sh;&c_t;&c_i;&c_f;&c_t;&c_o;&c_m;/&c_ch;&c_je;&c_k;&c_o;&c_ju;/&c_z;&c_a;&c_g;&c_v;&c_ib;&c_z;&c_d;&c_k;&c_o;&c_m;/&c_sh;&c_p;&c_o;&c_n;&c_k;&c_o;&c_ju;</trd>,
    <trd>&c_z;&c_a;&c_sh;&c_p;&c_l;&c_ib;&c_n;&c_t;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="stift.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="stift.mal0i.MAS">
    <dif>
      Disigi, malfermi, senigante je <tld/>o:
      <ekz>
        mal<tld/>u, kalkulu &gcirc;is 10 kaj &jcirc;etu
        <klr>[grenadon]</klr>
        <fnt>
          <aut>trad. senlime</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Diktator.htm">
          La Granda Diktatoro</url></vrk>,
          <lok>2005</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_m;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_z;&c_a;&c_m;&c_a;&c_c;&c_a;&c_v;&c_a;&c_n;&c_a;&c_je; &c_sh;&c_t;&c_y;&c_f;&c_t;&c_a;&c_m;)</klr></trd>
  <trd lng="cs">uvolnit kol&iacute;&ccaron;ek</trd>
  <trd lng="de"><ind>Stift</ind> ziehen</trd>
  <trd lng="fr">d&eacute;goupiller</trd>
  <trdgrp lng="pl">
    <trd>odklinowa&cacute;</trd>,
    <trd>wyjmowa&cacute;  <ind>zawleczk&eogonek;</ind></trd>
  </trdgrp>
  <trd lng="sk">uvo&lcaron;ni&tcaron; kol&iacute;&ccaron;ek</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_j;&c_m;&c_a;&c_t;&c_i; &c_sh;&c_t;&c_i;&c_f;&c_t;</trd>,
    <trd>&c_sh;&c_p;&c_o;&c_n;&c_k;&c_u;</trd>,
    <trd>&c_ch;&c_je;&c_k;&c_u;</trd>
  </trdgrp>
</drv>
</art>
