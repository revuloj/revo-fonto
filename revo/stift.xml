<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stift</rad>/o
</kap>
<drv mrk="stift.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Metala &scirc;topnajleto, uzata por kunfiksi du pecojn:
      <ekz>
        al&gcirc;ustiga, duobla, fiksa, konusa <tld/>o;
      </ekz>
      <ekz>
        fendo<tld/>o, &scirc;ra&ubreve;bo<tld/>o.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Stift</trd>
  <trdgrp lng="nl">
    <trd>stift</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_t;&c_i;&c_f;&c_t;</trd>
</drv>
</art>
