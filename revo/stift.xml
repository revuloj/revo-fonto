<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stift.xml,v 1.20 2023/09/04 15:14:31 revo Exp $">
<kap>
  <rad>stift</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stift.0o">
  <kap><tld/>o</kap>
  <snc mrk="stift.0o.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Metala &scirc;topnajleto, uzata por kunfiksi du pecojn:
      <ekz>
        al&gcirc;ustiga, cilindra, duobla, fiksa, konusa, stre&ccirc;a <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de elektra &scirc;topilo;
      </ekz>
      <ekz>
        <ind>fendo<tld/>o</ind>;
        <trd lng="hu">sasszeg</trd>
      </ekz>
      <ekz>
        <ind>&scirc;ra&ubreve;bo<tld/>o</ind>.
        <trd lng="hu">csavarszeg</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_y;&c_f;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>kol&iacute;k</trd>,
    <trd>kol&iacute;&ccaron;ek</trd>,
    <trd>n&yacute;tek</trd>,
    <trd>&ccaron;ep</trd>
  </trdgrp>
  <trd lng="de">Stift</trd>
  <trd lng="en">pin</trd>
  <trdgrp lng="fr">
    <trd>goupille</trd>,
    <trd>broche<klr tip="ind"> (de fiche &eacute;lectrique)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csap<klr tip="ind"> (r&ouml;gz&iacute;t&odblac;)</klr></trd>,
    <trd>csapszeg</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304F;&#x3055;&#x3073;&#x6813;  [<ind>&#x304F;&#x3055;&#x3073;&#x305B;&#x3093;</ind>]</trd>,
    <trd>&#x30AD;&#x30FC;  [<ind>&#x304D;&#x30FC;</ind>]</trd>,
    <trd>&#x30D4;&#x30F3;</trd>,
    <trd>&#x30B3;&#x30C3;&#x30BF;</trd>,
    <trd>&#x76EE;&#x91D8;  [<ind>&#x3081;&#x304F;&#x304E;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>stift</trd>,
    <trd>pin</trd>
  </trdgrp>
  <trd lng="ru">&c_sh;&c_t;&c_i;&c_f;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>kol&iacute;k</trd>,
    <trd>nit</trd>,
    <trd>&ccaron;ap</trd>
  </trdgrp>
  <trd lng="uk">&c_sh;&c_t;&c_i;&c_f;&c_t;</trd>
</drv>

      
<drv mrk="stift.0i">
  <kap><tld/>i</kap>
  <snc mrk="stift.0i.MAS">
    <dif>
      Kunigi, fiksi per <tld/>o. 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_m;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; <ind>&c_sh;&c_t;&c_y;&c_f;&c_t;&c_a;&c_m;</ind></trd>,
    <trd>&c_z;&c_a;&c_p;&c_ib;&c_r;&c_a;&c_c;&c_mol; <ind>&c_sh;&c_t;&c_y;&c_f;&c_t;&c_a;&c_m;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>upevnit kol&iacute;&ccaron;kem</trd>,
    <trd>za&ccaron;epovat</trd>
  </trdgrp>
  <trd lng="de">verstiften</trd>
  <trd lng="fr">goupiller</trd>
  <trd lng="ja">&#x304F;&#x3055;&#x3073;&#x6813;&#x3067;&#x7559;&#x3081;&#x308B;  [<ind>&#x304F;&#x3055;&#x3073;&#x305B;&#x3093;&#x3067;&#x3068;&#x3069;&#x3081;&#x308B;</ind>]</trd>
  <trdgrp lng="sk">
    <trd>upevni&tcaron; kol&iacute;kom</trd>,
    <trd>za&ccaron;apova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x43A;&#x440;&#x456;&#x43F;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438; &#x448;&#x442;&#x438;&#x444;&#x442;&#x43E;&#x43C;/&#x447;&#x435;&#x43A;&#x43E;&#x44E;/&#x437;&#x430;&#x433;&#x432;&#x456;&#x437;&#x434;&#x43A;&#x43E;&#x43C;/&#x448;&#x43F;&#x43E;&#x43D;&#x43A;&#x43E;&#x44E;</trd>,
    <trd>&#x437;&#x430;&#x448;&#x43F;&#x43B;&#x456;&#x43D;&#x442;&#x43E;&#x432;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="stift.mal0i">
  <kap>mal<tld/>i</kap>
  <snc mrk="stift.mal0i.MAS">
    <dif>
      Disigi, malfermi, senigante je <tld/>o:
      <ekz>
        mal<tld/>u, kalkulu &gcirc;is 10 kaj &jcirc;etu
        <klr>[grenadon]</klr>!
        <fnt>
          <aut>trad. senlime</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Diktator.htm">
          La Granda Diktatoro</url></vrk>,
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_z;&c_m;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_z;&c_a;&c_m;&c_a;&c_c;&c_a;&c_v;&c_a;&c_n;&c_a;&c_je; &c_sh;&c_t;&c_y;&c_f;&c_t;&c_a;&c_m;)</klr></trd>
  <trd lng="cs">uvolnit kol&iacute;&ccaron;ek</trd>
  <trd lng="fr">d&eacute;goupiller</trd>
  <trd lng="sk">uvo&lcaron;ni&tcaron; kol&iacute;&ccaron;ek</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x439;&#x43C;&#x430;&#x442;&#x438; &#x448;&#x442;&#x438;&#x444;&#x442;</trd>,
    <trd>&#x448;&#x43F;&#x43E;&#x43D;&#x43A;&#x443;</trd>,
    <trd>&#x447;&#x435;&#x43A;&#x443;</trd>
  </trdgrp>
</drv>
</art>
