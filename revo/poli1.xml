<art mrk="$Id: poli1.xml,v 1.12 2022/05/01 17:51:19 revo Exp $">
<kap>
  <rad>Poli</rad>/o
</kap>

<drv mrk="poli1.P0o">
  <kap><tld/>o 
  <fnt><bib>Viki</bib>, <lok><url 
  ref="&Viki;/Pollando">Pollando</url></lok></fnt></kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <uzo tip="stl">RAR</uzo>
    <dif>
      <ref tip="dif" cel="pol.P0ujo">Polujo</ref>:
      <ekz>
        turistoj el &Ccirc;e&hcirc;io, Hungario, Germanio, <tld/>o, Italio kaj Ukrainio
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2012/010472.php">Varmaj vortoj por malvarmaj akvoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        tie &ccirc;i mi kolektos informojn pri unu el ili, Gdinjo, en
        norda <tld/>o <klr>[&#8230;]</klr>
        <fnt>
          <vrk>
            <url ref=
           "http://www.geocities.com/lilandr/seatlo/GxudS/gxu_gdy.html">
            Gdynia, Polska, &Gcirc;emelurbo de Seatlo
            </url>
          </vrk>
       </fnt>
      </ekz>
    </dif>
    <ref tip="sin" cel="pol.P0lando">Pollando</ref>
    <ref tip="hom" cel="poli2.0o">polio- (medicino)</ref>
    <url ref="&AdE;/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/">AdE
      Listo de Rekomendataj Landnomoj</url>
    <trd lng="af">Pole</trd>
    <trd lng="be">&c_P;&c_o;&c_l;&c_mol;&c_sh;&c_ch;&c_a;</trd>
    <trd lng="cs">Polsko</trd>
    <trd lng="de">Polen</trd>
    <trd lng="en">Poland</trd>
    <trd lng="es">Polonia</trd>
    <trdgrp lng="fr">
      <trd>Pologne</trd>,
      <trd>R&eacute;publique de Pologne</trd>
    </trdgrp>
    <trd lng="fy">Poalen</trd>
    <trd lng="hu">Lengyelorsz&aacute;g</trd>
    <trd lng="id">Polandia</trd>
    <trd lng="io">Polonia</trd>
    <trd lng="it">Polonia</trd>
    <trd lng="la">Polonia</trd>
    <trd lng="lt">Lenkija</trd>
    <trd lng="nl">Polen</trd>
    <trd lng="pl">Polska</trd>
    <trd lng="pt">Pol&oacute;nia</trd>
    <trd lng="ru">&c_P;&c_o;&c_l;&c_mol;&c_sh;&c_a;</trd>
    <trd lng="sk">Po&lcaron;sko</trd>
    <trd lng="tok">ma Posuka</trd>
    <trd lng="uk">&c_P;&c_o;&c_l;&c_mol;&c_shch;&c_a;</trd>
    <trd lng="vo">Pol&auml;n</trd>
  </snc>
</drv>
</art>


