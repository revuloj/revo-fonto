<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>stof</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stof.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="stof.0o.MEZ">
    <dif>
      Malnova rusa
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">mezurunuo</ref>
      por flua&jcirc;oj, valoranta iom pli ol 1 litro:
      <ekz>
        unu <tld/>o da biero
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pind.0o.MEZ">pindo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_o;&c_f;</trd>
  <trd lng="de">Stof</trd>
  <trd lng="fr">pinte</trd>
  <trd lng="hu"><klr>(orosz)</klr> pint</trd>  
  <trd lng="id">pint</trd>
  <trd lng="nl">stoop</trd>
  <trd lng="ru">&c_sh;&c_t;&c_o;&c_f;</trd>
</drv>
</art>
