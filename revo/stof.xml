<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stof.xml,v 1.20 2023/10/30 01:33:11 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>stof</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stof.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="stof.0o.MEZ">
    <dif>
      Malnova <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">mezurunuo</ref> por flua&jcirc;oj,
      kutima en
      landoj &ccirc;irka&ubreve; la Balta Maro kaj varianta inter 1,2 kaj 3,2 litroj:
      <ekz>
        unu sitelo (<esc>vedro</esc>) egalas al dek (a&ubreve; ok) <tld/>oj;
      </ekz>
      <ekz>
        unu <tld/>o da biero
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kvart1.0o">kvarto</ref>,
      <ref cel="pind.0o.MEZ">pindo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_o;&c_f;</trd>
  <trd lng="cs">stof</trd>
  <trdgrp lng="de">
    <trd>Stof</trd>,
    <trd>Stoof</trd>,
    <trd>St&uuml;bchen</trd>
  </trdgrp>
  <trd lng="en">flagon</trd>
  <trd lng="fr">pinte</trd>
  <trd lng="hu"><klr>(orosz)</klr> pint</trd>
  <trd lng="id">pint</trd>
  <trd lng="ja">&#x30B7;&#x30E5;&#x30C8;&#x30D5;</trd>
  <trd lng="nl">stoop</trd>
  <trd lng="pl">sztof  <klr>(miara cieczy)</klr></trd>
  <trd lng="ru">&c_sh;&c_t;&c_o;&c_f;</trd>
  <trd lng="sk">stof</trd>
  <trd lng="uk">&c_sh;&c_t;&#x43E;&c_f; (&c_s;&c_t;&c_a;&c_r;&c_a; &#x43C;&c_ib;&c_r;&c_a; &c_jeu;&#x43C;&#x43D;&#x43E;&c_s;&c_t;&c_ib;)</trd>
</drv>
</art>
