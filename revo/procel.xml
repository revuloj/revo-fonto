  <trd lng="de">R&ouml;hrennasen</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_u;&c_b;&c_k;&c_o;&c_n;&c_o;&c_s;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
