<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: procel.xml,v 1.9 2022/10/23 19:30:44 revo Exp $">
<kap>
  <rad>procelari</rad>/o
</kap>

<drv mrk="procel.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref> de sudoceanaj marbirdoj 
      kun longaj flugiloj (<trd lng="la">Procellaria</trd>) el la ordo de <tld/>oformaj birdoj:
      <ekz>
        sur la rokkornicoj nestas oregreta pingveno kaj raraj marbirdoj kiel migra
        albatroso kaj granda procelario.
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=266029"
              >Insulo Gough</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/a/a3/Procellaria_aequinoctialis_in_flight.jpg/355px-Procellaria_aequinoctialis_in_flight.jpg"
      prm="CC BY-SA 3.0"
      ><tld/>o (Procellaria aequinoctialis), Novzelando
        <fnt>
          <aut>Mjobling, propra verko</aut>
          <lok><url ref="&WCW;/File:Procellaria_aequinoctialis_in_flight.jpg"
              >https://commons.wikimedia.org/wiki/File:Procellaria_aequinoctialis_in_flight.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="de">Sturmv&ouml;gel</trd>
  <trd lng="en">Procellaria</trd>
</drv>


<drv mrk="procel.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref
      tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Procellariiformes</trd>), kun tubformaj naztruoj:
      <ekz>
        <ref tip="prt" cel="albatr.0o">albatroso</ref>j estas grandaj marbirdoj de la <tld/> ordo <tld lit="P"/>oformaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=53854"
              >Albatrosoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">procel&middot;lariformes</trd>
  <trd lng="de">R&ouml;hrennasen</trd>
  <trdgrp lng="hu">
    <trd>viharmad&aacute;r-alak&uacute;ak</trd>,
    <trd>v&eacute;szmad&aacute;r-alak&uacute;ak</trd>,
    <trd>cs&odblac;r&ouml;sorr&uacute;ak<klr tip="amb"> (rend)</klr></trd>,
  </trdgrp>
  <trdgrp lng="nl">
    <trd>buissnaveligen<klr tip="ind"> (orde)</klr></trd>,
    <trd>stormvogelachtigen<klr tip="ind"> (orde)</klr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>procelariiformes</trd>,  
    <trd>procelariformes</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_r;&c_u;&c_b;&c_k;&c_o;&c_n;&c_o;&c_s;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>,
    <trd>&c_b;&c_u;&c_r;&c_je;&c_v;&c_je;&c_s;&c_t;&c_n;&c_i;&c_k;&c_i;  <klr
    tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  </trdgrp>
  <trd lng="fr">Procellariiformes</trd>
  <trd lng="en">Procellariiformes</trd>
</drv>
</art>
