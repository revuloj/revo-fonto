<art mrk="$Id: kolumbi.xml,v 1.6 2023/09/01 15:46:40 revo Exp $">
<kap>
  <rad>Kolumbi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kolumbi.britaK0o">
  <kap>Brita <tld/>o</kap>
  <snc mrk="kolumbi.britaK0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Provinco de <ref tip="malprt" cel="kanad.K0o.GEOG">Kanado</ref>
      en &gcirc;ia sud-okcidenta parto:
      <ekz>
        Viktorio estas la &ccirc;efurbo de Brita <tld/>o,
        la plej okcidenta provinco de Kanado
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Viktorio_%28Brita_Kolumbio%29">
          Viktorio (Brita Kolumbio)</url>, 2011-10-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_B;&c_r;&c_y;&c_t;&c_a;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_K;&c_a;&c_l;&c_ju;&c_m;&c_b;&c_ib;&c_ja;</trd>
  <trd lng="en">British Columbia</trd>
  <trd lng="fr">Colombie-Britannique</trd>
  <trd lng="ja">&#x30D6;&#x30EA;&#x30C6;&#x30A3;&#x30C3;&#x30B7;&#x30E5;&#x30FB;&#x30B3;&#x30ED;&#x30F3;&#x30D3;&#x30A2;&#x5DDE;  [<ind>&#x30D6;&#x30EA;&#x30C6;&#x30A3;&#x30C3;&#x30B7;&#x30E5;&#x30FB;&#x3053;&#x308D;&#x3093;&#x3073;&#x3042;&#x3057;&#x3085;&#x3046;</ind>]</trd>
  <trd lng="pl">Kolumbia Brytyjska</trd>     
  <trd lng="ro">Columbia Britanic&abreve;</trd>
  <trd lng="uk">&c_B;&c_r;&c_i;&c_t;&c_a;&#x43D;&c_s;&#x44C;&#x43A;&c_a; &#x41A;&#x43E;&#x43B;&c_u;&#x43C;&c_b;&c_ib;&#x44F;</trd>
</drv>
</art>

