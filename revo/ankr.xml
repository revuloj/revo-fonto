<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ankr</rad>/o
</kap>
<drv mrk="ankr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">MAR</uzo>
    <dif>
      Fera ilo kun du hokegoj, kiun oni enakvigas por fiksi &scirc;ipon:
      <ekz>
        &jcirc;eti, levi <tld/>on.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Similspeca objekto por alterigi aerostatojn.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_k;&c_a;&c_r;</trd>
  <trd lng="es">ancla</trd>
  <trd lng="fr">ancre</trd>
  <trdgrp lng="hu">
    <trd>horgony</trd>,
    <trd>vasmacska</trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_k;&c_o;&c_r;&c_mol;</trd>
</drv>

<drv mrk="ankr.0i">
  <kap><tld/>(ad)i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Restadi en loko, kie oni &jcirc;etis <tld/>on.
    </dif>
  </snc>
  <trd lng="es">anclar</trd>
  <trdgrp lng="fr">
    <trd>mouiller</trd>,
    <trd>s'<ind>ancrer</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>lehorgonyoz</trd>,
    <trd>horgonyt vet</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_s;&c_t;&c_a;&c_t;&c_mol; &c_n;&c_a; <ind>&c_ja;&c_k;&c_o;&c_r;&c_mol;</ind></trd>
</drv>

<drv mrk="ankr.0ejo">
  <kap><tld/>(ad)ejo</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Loko, kie oni povas <tld/>adi, <ref tip="sin" cel="rod.0o">rodo</ref>.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>ancrage</trd>,
    <trd>mouillage</trd>
  </trdgrp>
  <trd lng="hu">horgonyz&oacute;hely</trd>
  <trd lng="ru">&c_r;&c_je;&c_j;&c_d; <klr tip="amb">(&c_m;&c_o;&c_r;.)</klr></trd>
</drv>
</art>
