  <rad>Justen</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="justen.0o">
  <kap><tld/>o</kap>
  <snc mrk="justen.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena:
      <ekz>
        mia okjara fileto <tld/>o &scirc;atis vagadi senfare
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/epc/epc9110/patrina_tago.html">
          Matene de la Patrina Tago</url></vrk>,
          <lok>El Popola &Ccirc;inio, 1991-10, p. 48a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="fr">Justin</trd>
    <trd lng="pl">Justynian</trd>
  </snc>
</drv>
</art>

