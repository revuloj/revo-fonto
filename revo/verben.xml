<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>verben</rad>/o
</kap>
<drv mrk="verben.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="verben1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Verbena</trd>), ornama bonodora vegeta&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Eisenkraut</trd>
  <trdgrp lng="nl">
    <trd>ijzerkruid</trd>,
    <trd>verbena</trd>,
    <trd>ijzerhard</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_b;&c_je;&c_n;&c_a;</trd>
</drv>
</art>
