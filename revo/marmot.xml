<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>marmot</rad>/o
</kap>
<drv mrk="marmot.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="sciure.0oj">sciuredoj</ref>
      (<trd lng="la">Marmota</trd>).
    </dif>
    <bld lok="../bld/marmot.jpg">
      <ind>monta <tld/>o</ind>
      <trd lng="la">Marmota marmota</trd>
      <trd lng="de">Alpenmurmeltier</trd>
    </bld>
    <trd lng="de">Murmeltier</trd>
    <trdgrp lng="en">
      <trd>marmot</trd>,
      <trd>woodchuck</trd>
    </trdgrp>
  </snc>
  <trd lng="nl">marmot</trd>
  <trd lng="ru">&c_s;&c_u;&c_r;&c_o;&c_k;</trd>
</drv>
</art>
