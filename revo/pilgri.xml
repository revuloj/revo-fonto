<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>pilgrim</rad>/i
</kap>
<drv mrk="pilgri.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Voja&gcirc;i por pie viziti sanktan lokon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_o;&c_m;&c_n;&c_ib;&c_ch;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_a;&c_n;&c_d;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wallfahren</trd>,
    <trd>pilgern</trd>
  </trdgrp>
  <trd lng="en">go on a pilgrimage</trd>
  <trd lng="hu">zar&aacute;ndokol</trd>
  <trd lng="es">peregrinar</trd>
  <trdgrp lng="pt">
    <trd>peregrinar</trd>,
    <trd>ir em <ind>romaria</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_l;&c_o;&c_m;&c_n;&c_i;&c_ch;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_n;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="pilgri.0o">
  <kap><tld/>o, <tld/>ado</kap>
  <snc>
    <dif>
      Voja&gcirc;o por pie viziti sanktan lokon.
    </dif>
    <rim>Ne konfuzu "pilgrimo" kun "pilgrimanto, pilgrimulo".</rim>
  </snc>
  <trd lng="en">pilgrimage</trd>
</drv>

<drv mrk="pilgri.0anto">
  <kap><tld/>anto, <tld/>ulo</kap>
  <snc>
    <dif>
      Homo kiu pilgrimas.
    </dif>
  </snc>
  <trd lng="en">pilgrim</trd>
</drv>

</art>
