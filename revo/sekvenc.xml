<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: sekvenc.xml,v 1.5 2022/02/20 17:28:25 revo Exp $">
<kap>
    <rad>sekvenc</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="sekvenc.0o">
  <kap><tld/>o</kap>
  <snc mrk="sekvenc.0o.vico">
    <dif>
       <ref tip="super" cel="vic.0o">Vico</ref> de interligitaj objektoj
       a&ubreve;
       aperoj:
       <ekz>
         <tld/>o de sonfiguroj;
      </ekz>
      <ekz>
         <uzo tip="fak">MUZ</uzo>
           kvintfala <tld/>o
         <fnt>
           <bib>Viki</bib>,
           <lok>Sekvenco</lok>
         </fnt>;
      </ekz>
      <ekz>
         <uzo tip="fak">FOT</uzo>
         <tld/>o de sinsekvaj fotoj de la sama sceno;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        proceduro estas nomhava <tld/>o de instrukcioj. 
      </ekz>  
    </dif>
    <ref tip="sin" cel="seri.0o">serio</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>posloupnost</trd>,
    <trd>po&rcaron;ad&iacute;</trd>,
    <trd>sekvence</trd>,
    <trd>sled</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Folge</trd>,
    <trd>Sequenz</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s&eacute;quence</trd>,
    <trd>suite</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>poradie</trd>,
    <trd>postupnos&tcaron;</trd>,
    <trd>sekvencia</trd>,
    <trd>sled</trd>
  </trdgrp>
</drv>
</art>

