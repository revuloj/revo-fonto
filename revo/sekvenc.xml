<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sekvenc.xml,v 1.9 2024/03/24 20:00:00 revo Exp $">
<kap>
    <rad>sekvenc</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="sekvenc.0o">
  <kap><tld/>o</kap>
  <snc mrk="sekvenc.0o.vico">
    <dif>
       <ref tip="super" cel="vic.0o">Vico</ref> de interligitaj objektoj
       a&ubreve;
       aperoj:
       <ekz>
         <tld/>o de sonfiguroj;
      </ekz>
      <ekz>
         <uzo tip="fak">MUZ</uzo>
           kvintfala <tld/>o
         <fnt>
           <bib>Viki</bib>,
           <lok>Sekvenco</lok>
         </fnt>;
      </ekz>
      <ekz>
         <uzo tip="fak">FOT</uzo>
         <tld/>o de sinsekvaj fotoj de la sama sceno;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        proceduro estas nomhava <tld/>o de instrukcioj.
      </ekz>
    </dif>
    <ref tip="sin" cel="seri.0o">serio</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_e;&c_k;&c_v;&c_e;&c_n;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_p;&c_a;&c_s;&c_mol;&c_l;&c_ja;&c_d;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>posloupnost</trd>,
    <trd>po&rcaron;ad&iacute;</trd>,
    <trd>sekvence</trd>,
    <trd>sled</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Folge</trd>,
    <trd>Sequenz</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s&eacute;quence</trd>,
    <trd>suite</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sekwencja</trd>,
    <trd>ci&aogonek;g</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>poradie</trd>,
    <trd>postupnos&tcaron;</trd>,
    <trd>sekvencia</trd>,
    <trd>sled</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_je;&#x43A;&c_v;&c_je;&#x43D;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&#x43A;&c_ib;&#x43B;&#x44C;&#x43A;&c_a; &#x43A;&c_a;&c_r;&c_t; &#x43E;&c_d;&#x43D;&c_ib;&c_jeu;&c_ji; &#x43C;&c_a;&c_s;&c_t;&c_ib;</trd>,
    <trd>&#x43F;&#x43E;&c_s;&#x43B;&c_ib;&c_d;&#x43E;&c_v;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6B21;&#x5E8F; <pr>c&#xEC;x&#xF9;</pr></trd>,
    <trd>&#x5E8F;&#x5217; <pr>x&#xF9;li&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>

