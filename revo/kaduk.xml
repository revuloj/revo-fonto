<drv mrk="kaduk.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="kaduk.0a.ruinigxa">
    <dif>
      <ref tip="super" cel="fortik.mal0a">Malfortika</ref>, ruini&gcirc;anta,
      balda&ubreve; falonta a&ubreve; pereonta:
      <ekz>
        mi vidas malnovajn, mizerajn, <tld/>ajn kabanojn, kvaza&ubreve; mi trovi&gcirc;us en provinca
        urbeto
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        du vicoj da domoj, &ccirc;e kiuj &ccirc;ie pendis malmodernaj <tld/>aj balkonoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi penos faligi lin <klr>[&#x2026;]</klr> kiel barilon <tld/>an
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 62:3</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klin.0igxi">klini&gcirc;i</ref>,
      <ref cel="putr.0a">putra</ref>,
      <ref cel="sxance.0igxi">&scirc;anceli&gcirc;i</ref>,
      <ref cel="sxim">&scirc;ima</ref>,
      <ref cel="velk.0a">velka</ref>
    </refgrp>
    <trd lng="ca">l&agrave;bil</trd>
    <trdgrp lng="de">
      <trd>verfallen</trd>,
      <trd>bauf&auml;llig</trd>
    </trdgrp>
    <trdgrp lng="fi">
      <trd>r&auml;nsistynyt</trd>,
      <trd>huonokuntoinen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;labr&eacute;</trd>,
      <trd>d&eacute;cati<klr tip="ind">
          (d&eacute;labr&eacute;)</klr></trd>,
      <trd>en <ind>ruines</ind></trd>,
      <trd>vermoulu</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>d&uuml;ledez&odblac;</trd>,
      <trd>omladoz&oacute;</trd>,
      <trd>roskatag</trd>,
      <trd>megroggyant</trd>
    </trdgrp>
    <trd lng="nl">bouwvallig</trd>
    <trdgrp lng="pt">
      <trd>caduco</trd>,
      <trd>decr&eacute;pito</trd>,
      <trd>fr&aacute;gil</trd>,
      <trd>arruinado</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_je;&c_t;&c_h;&c_i;&c_j;</trd>
    <trd lng="sv">fallf&auml;rdig</trd>
  </snc>
  <snc mrk="kaduk.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Balda&ubreve; fini&gcirc;onta, malaperonta, neda&ubreve;ra,
      nefirma:
      <ekz>
        &ccirc;ie malnova <tld/>a ne&gcirc;o, kaj la vojo estis kotejo
        <fnt>
          <bib>Kiso</bib>
          <lok>G&auml;rda</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur la nubo la suno pentris <tld/>ajn &ccirc;ielarkajn
        figurojn
        <fnt>
          <vrk><url ref="&EduInf;lern/librejo/0023/0008.php">La fundo de l' mizero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        vantaj estas la homaj esperoj <klr>[&#x2026;]</klr>, kiel <tld/>aj kompare kun la
        dekretoj, kiujn Eternulo skribis
        <fnt>
          <aut>B. Prus, trad. Kabe</aut>
          <vrk><url ref="http://esperanto.davidgsimpson.com/inko/i2s-pola-antologio.pdf"
              >El la Legendoj de Antikva Egipto</url></vrk>
          <lok>Pola Antologio, p. 56</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>aj kutimoj
        <fnt>
          <vrk><url
          ref="https://www.academia.edu/34189499/Lingvistiko_instrumetodoj_kaj_Esperanto_Ne_forlasu_la_plenkreskulojn_">Lingvistiko,
          instrumetodoj kaj Esperanto: Ne forlasu la... - Academia.edu</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sen mi viaj aferoj &ndash; volu min pardoni &ndash; estis tre <tld/>aj
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Unua</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="efemer.0a">efemera</ref>
    <trd lng="ca">ef&iacute;mer</trd>
    <trdgrp lng="de">
      <trd>&uuml;berkommen</trd>,
      <trd>verg&auml;nglich</trd>,
      <trd>verfallen</trd>,
      <trd>verkommen</trd>
    </trdgrp>

    <trd lng="en">fragile</trd>
    <trdgrp lng="fi">
      <trd>kuihtuva</trd>,
      <trd>h&auml;vi&auml;v&auml;</trd>
      <trd>mit&auml;t&ouml;n</trd>
    </trdgrp>    <trdgrp lng="fr">
      <trd>fragile</trd>,
      <trd>labile</trd>,
      <trd>pr&eacute;caire</trd>
    </trdgrp>
    <trdgrp lng="he">
      <trd>&shin;&bet;&yod;&resh;</trd>,
      <trd>&shin;&bet;&resh;&yod;&resh;&yod;&resh;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elhal&oacute;</trd>,
      <trd>megsz&udblac;n&oacute;</trd>,
      <trd>szertefoszl&oacute;</trd>
    </trdgrp>
    <trd lng="nl">tot <ind>verdwijnen</ind> gedoemd</trd>
  </snc>
  <snc mrk="kaduk.0a.homo">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Tre <ref tip="super" cel="jun.mal0a">maljuna</ref>, <ref cel="fort.mal0a">malforta</ref>,
      <ref cel="san.mal0ema">malsanema</ref>, balda&ubreve; mortonta:
      <ekz>
        <tld/>aj maljunuloj
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        hejme li regalas sin per dol&ccirc;aj bonegaj vinoj kaj dorlotas siajn <tld/>ajn
        membrojn en kusenoj el lanugo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la duona kripligita vo&ccirc;o <klr>[&#x2026;]</klr> predikas terure la&ubreve;te per la tremanta
        <tld/>a korpo
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu el la stratvendistoj, <tld/>aspekta maljunulo, staranta apud <tld/>a
        pu&scirc;veturilo, alparolis lin
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        li faris tion kun pli granda vigleco, ol lia <tld/>a mieno supozigus ebla
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="ca">decr&egrave;pit</trd>
    <trdgrp lng="de">
      <trd>hinf&auml;llig</trd>,
      <trd>gebrechlich</trd>,
      <trd>anf&auml;llig</trd>
    </trdgrp>

    <trdgrp lng="fi">
      <trd>raihnainen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;cr&eacute;pit</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>roskatag</trd>,
      <trd>megrokkant</trd>,
      <trd>elgyeng&uuml;lt</trd>
    </trdgrp>
    <trd lng="nl">afgeleefd</trd>
    <trd lng="ru"><ofc>*</ofc>&c_d;&c_r;&c_ja;&c_h;&c_l;&c_y;&c_j;</trd>
    <trd lng="sv">skr&ouml;plig</trd>
  </snc>
  <snc mrk="kaduk.0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Perdinta sian juran efikon pro transpaso de difinita templimo,
      senvalidi&gcirc;inta:
      <ekz>
        <tld/>a kla&ubreve;zo.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>obsolet</trd>,
      <trd>passat</trd>
    </trdgrp>
    <trd lng="de">hinf&auml;llig</trd>
    <trd lng="en">inoperative</trd>
    <trdgrp lng="fi">
      <trd>vanhentunut</trd>,
      <trd>p&auml;tem&auml;tt&ouml;m&auml;ksi k&auml;ynyt</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_A;&a_j;&a_r;&a_A;&a_n;&a_A;&f_p;&a_dh;&f_y;&a_r;</trd>,
      <trd>&a_m;&a_n;&a_s;&a_w;&a_kh;</trd>
    </trdgrp>
    <trd lng="fr">caduc<klr tip="ind"> (jur.)</klr></trd>
    <trdgrp lng="he">
      <trd>&lamed;&alef; &bet;<ind>&tav;&vav;&qof;&fpe;</ind></trd>,
      <trd>&lamed;&alef; <ind>&pe;&vav;&ayin;&lamed;</ind></trd>
    </trdgrp>
    <trd lng="hu">&eacute;rv&eacute;ny&eacute;t vesz&iacute;tett</trd>
    <trd lng="nl">vervallen</trd>
    <trd lng="ru">&c_u;&c_s;&c_t;&c_a;&c_r;&c_je;&c_v;&c_sh;&c_i;&c_j;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_y;</trd>,
    <trd>&c_l;&c_ja;&c_d;&c_a;&c_sh;&c_ch;&c_y;</trd>,
    <trd>&c_s;&c_a;&c_s;&c_t;&c_a;&c_r;&c_e;&c_l;&c_y;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_ja;&c_p;&c_a;&c_l;&c_y;</trd>,
    <trd>&c_d;&c_r;&c_u;&c_z;&c_l;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">caduc</trd>
  <trdgrp lng="cs">
    <trd>chab&yacute;</trd>,
    <trd>chatrn&yacute;</trd>,
    <trd>se&scaron;l&yacute;</trd>,
    <trd>uboh&yacute;</trd>,
    <trd>vetch&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rickety</trd>,
    <trd>dilapidated</trd>,
    <trd>decrepit</trd>,
    <trd>fragile</trd>
  </trdgrp>
  <trd lng="es">caduco</trd>
  <trdgrp lng="fa">
    <trd>&a_f;&a_r;&a_t;&a_w;&a_t;</trd>,
    <trd>&a_f;&a_r;&a_s;&a_w;&a_d;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&resh;&ayin;&vav;&ayin;</trd>,
    <trd>&mem;&tav;&mem;&vav;&tet;&tet;</trd>,
    <trd>&mem;&tav;&pe;&vav;&resh;&resh;</trd>,
    <trd>&tav;&shin;&vav;&shin;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8001;&#x673D;&#x306E;  <pr>&#x308D;&#x3046;&#x304D;&#x3085;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x8352;&#x5EC3;&#x3057;&#x305F;  <pr>&#x3053;&#x3046;&#x306F;&#x3044;&#x3057;&#x305F;</pr></trd>,
    <trd>&#x8001;&#x8870;&#x3057;&#x305F;  <pr>&#x308D;&#x3046;&#x3059;&#x3044;&#x3057;&#x305F;</pr></trd>,
    <trd>&#x8001;&#x3044;&#x307C;&#x308C;&#x305F;  <pr>&#x304A;&#x3044;&#x307C;&#x308C;&#x305F;</pr></trd>,
    <trd>&#x6D88;&#x3048;&#x3066;&#x3044;&#x304F;  <pr>&#x304D;&#x3048;&#x3066;&#x3044;&#x304F;</pr></trd>,
    <trd>&#x306F;&#x304B;&#x306A;&#x3044;</trd>,
    <trd>&#x7121;&#x52B9;&#x306E;  <pr>&#x3080;&#x3053;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x5931;&#x52B9;&#x3057;&#x305F;  <pr>&#x3057;&#x3063;&#x3053;&#x3046;&#x3057;&#x305F;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podupad&lstroke;y</trd>,
    <trd>upadaj&aogonek;cy</trd>,
    <trd>zgrzybia&lstroke;y</trd>,
  </trdgrp>
  <trd lng="ro">caduc</trd>
  <trdgrp lng="sk">
    <trd>schradnut&yacute;</trd>,
    <trd>vetch&yacute;</trd>,
    <trd>&uacute;bohy</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_a;&c_r;&c_je;&c_z;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_je;&c_n;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_u;&c_p;&c_a;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_r;&c_ja;&c_h;&c_l;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9839;&#x58DE; <pr>tu&iacute;hu&agrave;i</pr></trd>,
    <trd>&#x5E74;&#x4E45;&#x5931;&#x4FEE; <pr>ni&aacute;nji&#x1D4;sh&imacron;xi&umacron;</pr></trd>,
    <trd>&#x6B8B;&#x8D25; <pr>c&aacute;nb&agrave;i</pr></trd>,
    <trd>&#x6B8B;&#x7834; <pr>c&aacute;np&ograve;</pr></trd>,
    <trd>&#x8001;&#x6001; <pr>l&#x1CE;ot&agrave;i</pr></trd>,
