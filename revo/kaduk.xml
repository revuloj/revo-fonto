<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kaduk.xml,v 1.27 2018/04/12 15:10:13 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kaduk</rad>/a <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kaduk.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="kaduk.0a.ruinigxa">
    <dif>
      Malfortika, ruini&gcirc;anta, balda&ubreve; falonta a&ubreve;
      pereonta:
      <ekz>
        <tld/>a dometo<fnt>Z</fnt>,
      </ekz><ekz>
        barilo<fnt>Z</fnt>;
      </ekz><ekz>
        malhela kaj <tld/>a urbo apud la Rejno.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klin.0igxi">klini&gcirc;i</ref>,
      <ref cel="putr.0a">putra</ref>,
      <ref cel="sxance.0igxi">&scirc;anceli&gcirc;i</ref>,
      <ref cel="sxim">&scirc;ima</ref>,
      <ref cel="velk.0a">velka</ref>.
    </refgrp>
    <trd lng="ca">l&agrave;bil</trd>
    <trdgrp lng="fi">
      <trd>r&auml;nsistynyt</trd>,
      <trd>huonokuntoinen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;labr&eacute;</trd>,
      <trd>d&eacute;cati<klr tip="ind">
          (d&eacute;labr&eacute;)</klr></trd>,
      <trd>en <ind>ruines</ind></trd>,
      <trd>vermoulu</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>d&uuml;ledez&odblac;</trd>,
      <trd>omladoz&oacute;</trd>,
      <trd>roskatag</trd>,
      <trd>megroggyant</trd>
    </trdgrp>
    <trd lng="nl">bouwvallig</trd>
    <trdgrp lng="pt">
      <trd>caduco</trd>,
      <trd>decr&eacute;pito</trd>,
      <trd>fr&aacute;gil</trd>,
      <trd>arruinado</trd>
    </trdgrp>
    <trd lng="ru">&c_v;&c_je;&c_t;&c_h;&c_i;&c_j;</trd>
    <trd lng="sv">fallf&auml;rdig</trd>
  </snc>
  <snc mrk="kaduk.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Balda&ubreve; fini&gcirc;onta, malaperonta, neda&ubreve;ra,
      nefirma:
      <ekz>
        sur la nubo la suno pentris <tld/>ajn &ccirc;ielarkajn
        figurojn<fnt>B</fnt>;
      </ekz><ekz>
        la lago ku&scirc;is sub <tld/>a nebulo<fnt>B</fnt>;
      </ekz><ekz>
        <tld/>aj esperoj<fnt>B</fnt>;
      </ekz><ekz>
        homaj intencoj estas <tld/>aj kompare kun la dekretoj de l'
        &ccirc;ielo<fnt>B</fnt>;
      </ekz><ekz>
        sen mi viaj aferoj estus tre <tld/>aj<fnt>Z</fnt>;
      </ekz><ekz>
        <tld/>a vidpunkto;
      </ekz><ekz>
        la entrepreno estas en <tld/>a stato.
      </ekz>
    </dif>
    <ref tip="vid" cel="efemer.0a">efemera</ref>
    <trd lng="ca">ef&iacute;mer</trd>
    <trd lng="en">fragile</trd>
    <trdgrp lng="fi">
      <trd>kuihtuva</trd>,
      <trd>h&auml;vi&auml;v&auml;</trd>
      <trd>mit&auml;t&ouml;n</trd>
    </trdgrp>    <trdgrp lng="fr">
      <trd>fragile</trd>,
      <trd>labile</trd>,
      <trd>pr&eacute;caire</trd>
    </trdgrp>
    <trdgrp lng="he">
      <trd>&shin;&bet;&yod;&resh;</trd>,
      <trd>&shin;&bet;&resh;&yod;&resh;&yod;&resh;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elhal&oacute;</trd>,
      <trd>megsz&udblac;n&oacute;</trd>,
      <trd>szertefoszl&oacute;</trd>
    </trdgrp>
    <trd lng="nl">tot <ind>verdwijnen</ind> gedoemd</trd>
  </snc>
  <snc mrk="kaduk.0a.homo">
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Tre <ref tip="super" cel="jun.mal0a">maljuna</ref>, malforta,
      malsanema, balda&ubreve; mortonta:
      <ekz>
        mia patro estis neniel <tld/>a, kiam li mortis<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="ca">decr&egrave;pit</trd>
    <trdgrp lng="fi">
      <trd>raihnainen</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;cr&eacute;pit</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>roskatag</trd>,
      <trd>megrokkant</trd>,
      <trd>elgyeng&uuml;lt</trd>
    </trdgrp>
    <trd lng="nl">afgeleefd</trd>
    <trd lng="ru"><ofc>*</ofc>&c_d;&c_r;&c_ja;&c_h;&c_l;&c_y;&c_j;</trd>
    <trd lng="sv">skr&ouml;plig</trd>
  </snc>
  <snc mrk="kaduk.0a.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Perdinta sian juran efikon pro transpaso de difinita templimo,
      senvalidi&gcirc;inta:
      <ekz>
        <tld/>a kla&ubreve;zo.
      </ekz>
    </dif>
    <trdgrp lng="ca">
      <trd>obsolet</trd>,
      <trd>passat</trd>
    </trdgrp>
    <trd lng="en">inoperative</trd>
    <trdgrp lng="fi">
      <trd>vanhentunut</trd>,
      <trd>p&auml;tem&auml;tt&ouml;m&auml;ksi k&auml;ynyt</trd>
    </trdgrp>
    <trdgrp lng="fa">
      <trd>&a_A;&a_j;&a_r;&a_A;&a_n;&a_A;&f_p;&a_dh;&f_y;&a_r;</trd>,
      <trd>&a_m;&a_n;&a_s;&a_w;&a_kh;</trd>
    </trdgrp>
    <trd lng="fr">caduc<klr tip="ind"> (jur.)</klr></trd>
    <trdgrp lng="he">
      <trd>&lamed;&alef; &bet;<ind>&tav;&vav;&qof;&fpe;</ind></trd>,
      <trd>&lamed;&alef; <ind>&pe;&vav;&ayin;&lamed;</ind></trd>
    </trdgrp>
    <trd lng="hu">&eacute;rv&eacute;ny&eacute;t vesz&iacute;tett</trd>
    <trd lng="nl">vervallen</trd>
    <trd
      lng="ru">&c_u;&c_s;&c_t;&c_a;&c_r;&c_je;&c_v;&c_sh;&c_i;&c_j;</trd>
  </snc>
  <trd lng="ca">caduc</trd>
  <trdgrp lng="cs">
    <trd>chab&yacute;</trd>,
    <trd>chatrn&yacute;</trd>,
    <trd>se&scaron;l&yacute;</trd>,
    <trd>uboh&yacute;</trd>,
    <trd>vetch&yacute;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>rickety</trd>,
    <trd>dilapidated</trd>,
    <trd>decrepit</trd>,
    <trd>fragile</trd>
  </trdgrp>
  <trd lng="es">caduco</trd>
  <trdgrp lng="fa">
    <trd>&a_f;&a_r;&a_t;&a_w;&a_t;</trd>,
    <trd>&a_f;&a_r;&a_s;&a_w;&a_d;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&resh;&ayin;&vav;&ayin;</trd>,
    <trd>&mem;&tav;&mem;&vav;&tet;&tet;</trd>,
    <trd>&mem;&tav;&pe;&vav;&resh;&resh;</trd>,
    <trd>&tav;&shin;&vav;&shin;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podupad&lstroke;y</trd>,
    <trd>upadaj&aogonek;cy</trd>,
    <trd>zgrzybia&lstroke;y</trd>,
  </trdgrp>
  <trd lng="ro">caduc</trd>
  <trdgrp lng="sk">
    <trd>schradnut&yacute;</trd>,
    <trd>vetch&yacute;</trd>,
    <trd>&uacute;bohy</trd>
  </trdgrp>
</drv>

<drv mrk="kaduk.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="kaduk.0ajxo.KOMUNE">
    <dif>
      Difekto a&ubreve; videbla signo de <tld/>eco:
      <ekz>
        la surdeco estas unu el la plej oftaj <tld/>a&jcirc;oj de la
        maljunuloj.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">signe de decad&egrave;ncia</trd>
  <trdgrp lng="en">
    <trd>ailment</trd>,
    <trd>sign of <ind>decay</ind></trd>
  </trdgrp>
  <trdgrp lng="fi">
    <trd>vika</trd>,
    <trd>rikkin&auml;inen osa</trd>,
    <trd>rappeuma</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>affection<klr tip="ind"> (due &agrave; la
        vieillesse)</klr></trd>,
    <trd>d&eacute;ch&eacute;ance</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&het;&vav;&lamed;&yod;</trd>,
    <trd>&nun;&yod;&vav;&vav;&fnun;</trd>,
    <trd>&he;&tav;&pe;&vav;&resh;&resh;&vav;&tav;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;regkori fogyat&eacute;koss&aacute;g</trd>,
    <trd>&ouml;regkori defektus</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>iets <ind>aftands</ind></trd>,
    <trd>iets <ind>bouwvalligs</ind></trd>
  </trdgrp>
  <trd lng="pl">upadek <klr tip="amb">(psychiczny)</klr></trd>
  <trd lng="ro">decaden&tcedil;&abreve;</trd>
</drv>

<drv mrk="kaduk.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="kaduk.0eco.KOMUNE">
    <dif>
      Stato de iu a&ubreve; io <tld/>a.
    </dif>
    <refgrp tip="vid">
      <ref cel="agoni.0o">agonio</ref>,
      <ref cel="krepus.0o">krepusko</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>decad&egrave;ncia</trd>,
    <trd>declivi</trd>,
    <trd>envelliment</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>se&scaron;lost</trd>,
    <trd>vetchost</trd>
  </trdgrp>
  <trd lng="es">caducidad</trd>
  <trdgrp lng="fi">
    <trd>rappio</trd>,
    <trd>raihnaus</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;clin</trd>,
    <trd>d&eacute;cr&eacute;pitude</trd>,
    <trd>senescence</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>elesetts&eacute;g</trd>,
    <trd>roskatags&aacute;g</trd>,
    <trd>gyenges&eacute;g</trd>
  </trdgrp>
  <trd lng="nl">bouwvalligheid</trd>
  <trdgrp lng="pl">
    <trd>schy&lstroke;ek</trd>,
    <trd>zgrzybia&lstroke;o&sacute;&cacute;</trd>,
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_ja;&c_h;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_je;&c_t;&c_h;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_s;&c_t;&c_a;&c_r;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">vetchos&tcaron;</trd>
</drv>

<drv mrk="kaduk.0ega">
  <kap><tld/>ega</kap>
  <snc mrk="kaduk.0ega.KOMUNE">
    <dif>
      Perdinta siajn mensajn kapablojn pro granda maljuneco;
      <ref tip="sin" cel="senil.0a">senila</ref>.
    </dif>   
  </snc>
  <trdgrp lng="ca">
    <trd>senil</trd>,
    <trd>xaruc</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>velmi se&scaron;l&yacute;</trd>,
    <trd>velmi uboh&yacute;</trd>
  </trdgrp>
  <trd lng="en">senile</trd>
  <trdgrp lng="es">
    <trd>decr&eacute;pito</trd>,
    <trd>senil</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s&eacute;nile</trd>,
    <trd>g&acirc;teux</trd>
  </trdgrp>
  <trd lng="he">&samekh;&nun;&yod;&lamed;&yod;</trd>
  <trdgrp lng="hu">
    <trd>szenilis</trd>,
    <trd>agyal&aacute;gyult</trd>
  </trdgrp>
  <trd lng="nl">seniel</trd>
  <trd lng="pl">starczy</trd>
  <trd lng="sk">ve&lcaron;mi &uacute;boh&yacute;</trd>
</drv>

<drv mrk="kaduk.0egeco">
  <kap><tld/>egeco</kap>
  <snc mrk="kaduk.0egeco.KOMUNE">
    <dif>
      Stato de <tld/>ega persono;
      <ref tip="sin" cel="senil.0eco">senileco</ref>.
    </dif>   
  </snc>
  <trdgrp lng="ca">
    <trd>senilitat</trd>,
    <trd>senectut</trd>
  </trdgrp>
  <trd lng="en">senility</trd>
  <trdgrp lng="fr">
    <trd>s&eacute;nilit&eacute;</trd>,
    <trd>g&acirc;terie<klr tip="ind">
        (s&eacute;nilit&eacute;)</klr></trd>
  </trdgrp>
  <trd lng="he">&samekh;&nun;&yod;&lamed;&yod;&vav;&tav;</trd>
  <trdgrp lng="hu">
    <trd>szenilit&aacute;s</trd>,
    <trd>aggkori elbutul&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">seniliteit</trd>
  <trd lng="pl">uwi&aogonek;d starczy</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_r;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;
  &c_m;&c_a;&c_r;&c_a;&c_z;&c_m;</trd>
  <trd lng="es">senilidad</trd>
</drv>

<drv mrk="kaduk.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kaduk.0igi.KOMUNE">
    <dif>
      Igi <tld/>a, malfortikigi, malvalidigi:
      <ekz>
        laceco ekstreme <tld/>igis lin, ke liaj okuloj apena&ubreve;
        povis 
        vidi 
        <fnt>
          <aut>&Ccirc;ina Radio Internacia</aut>,
          <vrk><url
          ref="http://esperanto.cri.cn/681/2009/06/29/1s98744.htm">
          Fama libro Historio Referenca por Regado</url></vrk>,
          <lok>2009-06-29</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="es">
      <trd>ajar</trd>,
      <trd>estropear</trd>,
      <trd>envejecer <klr tip="amb">(verb. trans.)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="cs">kadukovat</trd>
  <trd lng="fr">d&eacute;labrer</trd>
  <trd lng="sk">kadukova&tcaron;</trd>
</drv>

<drv mrk="kaduk.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kaduk.0igxi.KOMUNE">
    <dif>
      I&gcirc;i <tld/>a:
      <ekz>
        <tld/>i&gcirc;inta morale kaj mense<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konsum.0igxi">konsumi&gcirc;i</ref>,
      <ref cel="velk.0i">velki</ref>.
    </refgrp>
    <trdgrp lng="es">
      <trd>envejecer <klr tip="amb">(verb. intrans.)</klr></trd>,
      <trd>ajarse</trd>,
      <trd>estropearse</trd>,
      <trd>caducarse</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ca">
    <trd>decaure</trd>,
    <trd>degradar-se</trd>,
    <trd>esfondrar-se</trd>
  </trdgrp>
  <trdgrp lng="fi">
    <trd>r&auml;nsisty&auml;</trd>,
    <trd>raihnaantua</trd>,
    <trd>rapistua</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>crouler</trd>,
    <trd>se <ind>d&eacute;labrer</ind></trd>,
    <trd>tomber en <ind>ruine</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megrokkan</trd>,
    <trd>megroggyan</trd>,
    <trd>omladozik</trd>,
    <trd>legyeng&uuml;l</trd>,
    <trd>&eacute;rv&eacute;ny&eacute;t veszti</trd>,
  </trdgrp>
  <trd lng="nl">in <ind>verval</ind> geraken</trd>
  <trd lng="pl">zmarnie&cacute;</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_r;&c_ja;&c_h;&c_l;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_je;&c_t;&c_sh;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="tr">harabeye d&ouml;nmek</trd>
</drv>

<drv mrk="kaduk.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="kaduk.0ulo.KOMUNE">
    <dif>
      Maljunulo perdanta siajn korpajn kaj mensajn kapablojn:
      <ekz>
        <ind><tld/>ulejo</ind>.
        <trd lng="ca">geri&agrave;tric</trd>
        <trd lng="fr">asile de vieillards</trd>
        <trd lng="nl">bejaardentehuis</trd>
      </ekz>
    </dif>
    <trd lng="es">anciano</trd>
  </snc>
  <trdgrp lng="ca">
    <trd>anci&agrave;</trd>,
    <trd>vell</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>se&scaron;l&yacute; sta&rcaron;ec</trd>,
    <trd>vetch&yacute; sta&rcaron;ec</trd>
  </trdgrp>
  <trd lng="en">elderly <klr>(person)</klr></trd>
  <trdgrp lng="fi">
    <trd>raihnas vanhus</trd>
  </trdgrp>
  <trd lng="fr">vieillard</trd>
  <trd lng="he">&qof;&shin;&yod;&shin;</trd>
  <trdgrp lng="hu">
    <trd>elaggott ember</trd>,
    <trd>aggasty&aacute;n</trd>
  </trdgrp>
  <trd lng="nl">grijsaard</trd>
  <trdgrp lng="pl">
    <trd>ramol</trd>,
    <trd>starzec</trd>
  </trdgrp>
  <trd lng="sk">vetch&yacute; starec</trd>
</drv>
</art>
