<drv mrk="kaduk.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="kaduk.0ajxo.KOMUNE">
    <dif>
      Io <tld/>a:
      <ekz>
        li jam ne retrovis la dometon <klr>[&#x2026;]</klr>, la <tld/>a&jcirc;o estis tute forlekita
        de la fajro
        <fnt>
          <aut>M. A. Bulgakov, trad. Sergio Pokrovskij</aut>
          <vrk><url ref="https://www.esperanto.mv.ru/MM/EPILOGO.html"
              >Fragmentoj el La majstro kaj Margarita</url></vrk>
          <lok>Epilogo</lok>
        </fnt>;
      </ekz>
      <ekz>
        alportu la malnovan kale&scirc;on! <klr>[&#x2026;]</klr> unu rado mankas kaj la tuta supra
        parto <klr>[jen]</klr> la malnova <tld/>a&jcirc;o
        <fnt>
          <aut>Selma Lagerl&ouml;f, trad. Stellan Engholm</aut>
          <vrk><url ref="https://eduinf.waw.pl/esp/lern/librejo/0106/0005.php"
              >G&ouml;sta Berling</url></vrk>
          <lok>Dua &Ccirc;apitro: La kristnaska nokto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la prezidanto grimpis sur la supron de la palisaro, kaj tiam ekkrakis sub li la
        putri&gcirc;inta <tld/>a&jcirc;o
        <fnt>
          <aut>Ferenc Moln&aacute;r, trad. Ladislao Spierer</aut>
          <vrk><url ref="https://libro.ee/file.php?id=1374"
              >La Knaboj de Pa&ubreve;lo-Strato</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="detru.0itajxo">detruita&jcirc;o</ref>,
      <ref cel="ruin.0o">ruino</ref>,
      <ref cel="vrak.0o">vrako</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_n;&c_ja;&c_p;&c_a;&c_d;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_ib;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_n;&c_je;&c_m;&c_a;&c_ch;</trd>
  </trdgrp>
  <trd lng="ca">signe de decad&egrave;ncia</trd>
  <trdgrp lng="en">
    <trd>ailment</trd>,
    <trd>sign of <ind>decay</ind></trd>
  </trdgrp>
  <trdgrp lng="fi">
    <trd>vika</trd>,
    <trd>rikkin&auml;inen osa</trd>,
    <trd>rappeuma</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>affection<klr tip="ind"> (due &agrave; la
        vieillesse)</klr></trd>,
    <trd>d&eacute;ch&eacute;ance</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&het;&vav;&lamed;&yod;</trd>,
    <trd>&nun;&yod;&vav;&vav;&fnun;</trd>,
    <trd>&he;&tav;&pe;&vav;&resh;&resh;&vav;&tav;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&ouml;regkori fogyat&eacute;koss&aacute;g</trd>,
    <trd>&ouml;regkori defektus</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8870;&#x3048;&#x305F;&#x90E8;&#x5206;  <pr>&#x304A;&#x3068;&#x308D;&#x3048;&#x305F;&#x3076;&#x3076;&#x3093;</pr></trd>,
    <trd>&#x8001;&#x673D;&#x7B87;&#x6240;  <pr>&#x308D;&#x3046;&#x304D;&#x3085;&#x3046;&#x304B;&#x3057;&#x3087;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>iets <ind>aftands</ind></trd>,
    <trd>iets <ind>bouwvalligs</ind></trd>
  </trdgrp>
  <trd lng="pl">upadek <klr tip="amb">(psychiczny)</klr></trd>
  <trd lng="ro">decaden&tcomma;&abreve;</trd>
  <trd lng="uk">&c_m;&c_o;&c_t;&c_l;&c_o;&c_h;</trd>
</drv>

<drv mrk="kaduk.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="kaduk.0eco.KOMUNE">
    <dif>
      Stato de iu a&ubreve; io <tld/>a:
      <ekz>
        mi nepre kondukus vin al <nom>Babel</nom>, malgra&ubreve; <klr>[&#x2026;]</klr> la <tld/>eco de miaj ostoj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        Petro <klr>[&#x2026;]</klr> maljuna, zorgoplena <klr>[&#x2026;]</klr> &scirc;ajnis al ili &ccirc;i-momente
        kvaza&ubreve; personigo de <tld/>eco kaj malforto
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LIII</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_r;&c_u;&c_z;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_ja;&c_d;&c_a;&c_sh;&c_ch;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_n;&c_ja;&c_p;&c_a;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>decad&egrave;ncia</trd>,
    <trd>declivi</trd>,
    <trd>envelliment</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>se&scaron;lost</trd>,
    <trd>vetchost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hinf&auml;lligkeit</trd>,
    <trd>Verfallenheit</trd>
