<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ailant.xml,v 1.21 2023/08/30 18:23:31 revo Exp $">
<kap>
  <rad>ailant</rad>/o
</kap>
<drv mrk="ailant.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="simarub.0oj">simarubacoj</ref>
      (<trd lng="la">Ailanthus</trd>), orientaziaj kaj a&ubreve;straliaj
      arboj kun longaj, akre odorantaj folioj, kulturataj por bredado de
      silkra&ubreve;poj:
      <ekz>
        plantoj kun forta alelopatia efiko estas <tld/>o,
        hipoka&scirc;tano, agropiro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=248478">Alelopatio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/6/64/AilanthusAltissimaFlower2.jpg/640px-AilanthusAltissimaFlower2.jpg" prm="CC-BY-SA-3.0">floranta bran&ccirc;o de <tld/>o (Ailanthus altissima)
        <fnt>
          <aut>Fanghong</aut>
          <lok><url ref="&WCW;/File:AilanthusAltissimaFlower2.jpg">&WCW;/File:AilanthusAltissimaFlower2.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_a;&c_j;&c_l;&c_ja;&c_n;&c_t;</trd>
  <trd lng="ca">g&egrave;nere Ailanthus</trd>
  <trd lng="de">G&ouml;tterbaum</trd>
  <trdgrp lng="en">
    <trd>Ailanthus</trd>,
    <trd>tree of heaven</trd>
  </trdgrp>
  <trd lng="fr">ailante</trd>
  <trd lng="he">&#x5E1;&#x5D5;&#x5D2; &#x5E2;&#x5E5;</trd>
  <trd lng="hu">b&aacute;lv&aacute;nyfa</trd>
  <trd lng="ja">&#x30CB;&#x30EF;&#x30A6;&#x30EB;&#x30B7;</trd>
  <trd lng="nl">hemelboom</trd>
  <trd lng="pl">bo&zdot;odrzew</trd>
  <trd lng="pt">ailanto</trd>
  <trd lng="ru">&c_a;&c_j;&c_l;&c_a;&c_n;&c_t;</trd>
  <trd lng="uk">&c_a;&c_j;&c_l;&c_ja;&c_n;&c_t;</trd>
</drv>
</art>
