  <trd lng="de">G&ouml;tterbaum</trd>
  <trd lng="fr">ailante</trd>
  <trd lng="nl">hemelboom</trd>
  <trd lng="pt">ailanto</trd>
  <trd lng="ru">&c_a;&c_j;&c_l;&c_a;&c_n;&c_t;</trd>
</drv>
