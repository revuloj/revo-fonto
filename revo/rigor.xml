<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>rigor</rad>/a
</kap>
<drv mrk="rigor.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Senindulge severa:
      <ekz>
        <tld/>a mastro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a ekzameno, regulo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ekkoni la <tld/>an realon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_ib;</trd>,
    <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>rigoros</trd>,
    <trd>unerbittlich</trd>,
    <trd>streng</trd>,
    <trd>hart</trd>
  </trdgrp>
  <trd lng="en">rigorous</trd>
  <trd lng="es">riguroso <klr> (severo)</klr></trd>
  <trd lng="fr">rigoureux</trd>
  <trd lng="nl">streng</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
