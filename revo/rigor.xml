<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>rigor</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rigor.0a">
  <kap><tld/>a</kap>
  <snc mrk="rigor.0a.KOMUNE">
    <dif>
      Senindulge
      <ref tip="vid" cel="sever.0a">severa</ref>:
      <ekz>
        <tld/>a mastro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>a ekzameno, regulo;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ekkoni la <tld/>an realon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_ib;</trd>,
    <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">rigor&oacute;s</trd>
  <trdgrp lng="cs">
    <trd>nekompromisn&iacute;</trd>,
    <trd>p&rcaron;esn&yacute;</trd>,
    <trd>p&rcaron;&iacute;sn&yacute;</trd>,
    <trd>rigor&oacute;zn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>rigoros</trd>,
    <trd>unerbittlich</trd>,
    <trd>streng</trd>,
    <trd>hart</trd>
  </trdgrp>
  <trd lng="en">rigorous</trd>
  <trd lng="es">riguroso <klr> (severo)</klr></trd>
  <trd lng="fr">rigoureux</trd>
  <trdgrp lng="hu">
    <trd>szigor&uacute;</trd>,
    <trd>k&ouml;ny&ouml;rtelen</trd>,
    <trd>rigor&oacute;zus</trd>
  </trdgrp>
  <trd lng="nl">streng</trd>
  <trd lng="pt">rigoroso</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nekompromisn&yacute;</trd>,
    <trd>pr&iacute;sny</trd>,
    <trd>rigor&oacute;zny</trd>
  </trdgrp>
</drv>

<drv mrk="rigor.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="rigor.0ismo.KOMUNE">
    <dif>
      <tld lit="R"/>a sinteno, necedema severeco:
      <ekz>
        <klr>la</klr> etika idealo de L. Tolstoj <klr>...</klr> iom 
        relativigas etikan <tld/>ismon
        <fnt>
          <aut>M. Spekuljak</aut>,
          <vrk>[letero]</vrk>,
          <lok><url ref="">
          Hilelisto, 2001-01-01 (7)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">rigorisme</trd>
</drv>
</art>
