<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rigor.xml,v 1.30 2023/10/21 05:38:06 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>rigor</rad>/a <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rigor.0a">
  <kap><tld/>a</kap>
  <snc mrk="rigor.0a.KOMUNE">
    <dif>
      Senindulge <ref tip="super" cel="sever.0a">severa</ref>:
      <ekz>
        Platono priskribas kiel socio disvolvi&gcirc;as kun <tld/>a labordivido, kiu kondukas
        al pli granda efikeco
        <fnt><aut>Bo Sandelin</aut>: 
          <vrk><url ref="&Monato;/2001/007279.php">Anta&ubreve;merkantilisma ekonomia pensado</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/02, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        mankis la necesa ru&gcirc;a krajono de <tld/>a revizianto
        <fnt>
          <aut>Pejno Simono</aut>
          <vrk><url ref="&Monato;/2001/007110.php">Atentokapta sed ru&gcirc;krajoninda</url></vrk>
          <bib>Monato</bib>
          <lok>2001/01, p. 31</lok>
          </fnt>;
      </ekz>
      <ekz>
        britaj instancoj enkondukis <tld/>ajn kontrolojn kune kun pligrandigitaj
        monpunoj por tiuj, kiuj provis kontra&ubreve;le&gcirc;e enveturigi homojn
        <fnt>
          <aut>Antanas Grincevi&ccaron;ius</aut>
            <vrk><url ref="&Monato;/2000/007163.html">Ruze eniri, akuze eliri</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 6</lok>
          </fnt>;
      </ekz>
      <ekz>
        <tld/>e kontroli la agadon de la komercistoj
        <fnt>
          
          <vrk><url ref="&Monato;/1993/A930902.php">Noticoj</url></vrk>
          <bib>Monato</bib>
          <lok>1993/09, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        birdoj de <tld/>e protektata specio
        <fnt><aut>Katalin Smid&eacute;liusz</aut>: 
          <vrk><url ref="&Monato;/2000/006912.php">Cinike cianide -- &ccirc;u respondecas
          Okcidento?</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/04, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        verkanto devas trovi oran mezon inter populareco kaj scienca <tld/>eco en
        priskriboj de astronomiaj fenomenoj
        <fnt>
          <bib>Monato</bib>
          <aut>Anna kaj Mati Pentus</aut>
          <vrk><url ref="&Monato;/2003/008070.html">Franda&jcirc;o por scivolemuloj</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        raporti la&ubreve; seka <tld/>o
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Oni Fermas la Butikon</lok>
        </fnt>;
      </ekz>
      <ekz>
        &Ccirc;ar monduma stulta kodo / Anka&ubreve; tie &ccirc;i rigoras, / Kaj la sklavoj de la modo /
        Libertempe nur ... de&jcirc;oras
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Libertempo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="feroc.0a">feroca</ref>,
      <ref cel="kruel.0a">kruela</ref>,
      <ref cel="mol.mal0a.morale">malmola<sncref/></ref>,
      <ref cel="rigid.0a.necedeme">rigida<sncref/></ref>,
      <ref cel="indulg.sen0a">senindulga</ref>,
       <ref cel="kompat.sen0a">senkompata</ref>,
       <ref cel="kor.sen0a">senkora</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_ib;</trd>,
    <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">rigor&oacute;s</trd>
  <trdgrp lng="cs">
    <trd>nekompromisn&iacute;</trd>,
    <trd>p&rcaron;esn&yacute;</trd>,
    <trd>p&rcaron;&iacute;sn&yacute;</trd>,
    <trd>rigor&oacute;zn&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>rigoros</trd>,
    <trd>unerbittlich</trd>,
    <trd>hart (unerbittlich)</trd>,
  </trdgrp>
  <trd lng="en">rigorous</trd>
  <trd lng="es">riguroso <klr> (severo)</klr></trd>
  <trd lng="fr">rigoureux</trd>
  <trdgrp lng="hu">
    <trd>szigor&uacute;</trd>,
    <trd>k&ouml;ny&ouml;rtelen</trd>,
    <trd>rigor&oacute;zus</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x53B3;&#x683C;&#x306A;  <pr>&#x3052;&#x3093;&#x304B;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x53B3;&#x3057;&#x3044;  <pr>&#x304D;&#x3073;&#x3057;&#x3044;</pr></trd>,
    <trd>&#x53B3;&#x6B63;&#x306A;  <pr>&#x3052;&#x3093;&#x305B;&#x3044;&#x306A;</pr></trd>,
    <trd>&#x53B3;&#x5BC6;&#x306A;  <pr>&#x3052;&#x3093;&#x307F;&#x3064;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">streng</trd>
  <trdgrp lng="pl">
    <trd>ostry</trd>,
    <trd>surowy</trd>
  </trdgrp>
  <trd lng="pt">rigoroso</trd>
  <trdgrp lng="ro">
    <trd>sever</trd>,
    <trd>riguros</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_u;&c_r;&c_o;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_g;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nekompromisn&yacute;</trd>,
    <trd>pr&iacute;sny</trd>,
    <trd>rigor&oacute;zny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_t;&c_r;&#x43E;&c_g;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_u;&c_v;&#x43E;&c_r;&c_i;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="rigor.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="rigor.0ismo.KOMUNE">
    <dif>
      <tld lit="R"/>a sinteno, necedema severeco:
      <ekz>
        <klr>[la]</klr> etika idealo de <nom>L. Tolstoj</nom> <klr>[&#x2026;]</klr> iom 
        relativigas etikan <tld/>ismon
        <fnt>
          <aut>M. Spekuljak</aut>,
          <vrk>[letero]</vrk>,
          <lok><url ref="">
          Hilelisto, 2001-01-01 (7)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_g;&c_a;&c_r;&c_y;&c_z;&c_m;</trd>
  <trd lng="fr">rigorisme</trd>
  <trdgrp lng="ja">
    <trd>&#x53B3;&#x683C;&#x4E3B;&#x7FA9;  <pr>&#x3052;&#x3093;&#x304B;&#x304F;&#x3057;&#x3085;&#x304E;</pr></trd>,
    <trd>&#x30EA;&#x30B4;&#x30EA;&#x30BA;&#x30E0;</trd>
  </trdgrp>
  <trd lng="pl">rygoryzm</trd>
  <trd lng="ro">severitate</trd>
  <trd lng="uk">&c_r;&c_i;&c_g;&#x43E;&c_r;&c_i;&c_z;&#x43C;</trd>
</drv>
</art>
