<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kursiv</rad>/o
</kap>
<drv mrk="kursiv.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Klinita rapida skribado.
    </dif>
    <trd lng="en">running hand</trd>
    <trd lng="hu">foly&oacute;&iacute;r&aacute;s</trd>
  </snc>
  <snc>
    <uzo tip="fak">TIP</uzo>
    <dif>
      Preslitero, klinita al dekstro, kiel la <tld/>a skribado.
    </dif>
    <trdgrp lng="de">
      <trd>Kursivschrift</trd>,
      <trd>Kursive</trd>
    </trdgrp>
    <trd lng="en">Italic letter</trd>
    <trdgrp lng="es">
      <trd>cursiva</trd>,
      <trd>it&aacute;lica</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>d&odblac;lt bet&udblac;s &iacute;r&aacute;s</trd>,
      <trd>kurz&iacute;v &iacute;r&aacute;s</trd>
    </trdgrp>
    <trd lng="ru">&c_k;&c_u;&c_r;&c_s;&c_i;&c_v;</trd>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_r;&c_s;&c_ib;&c_w;</trd>
  <trd lng="nl">cursief</trd>
</drv>
</art>
