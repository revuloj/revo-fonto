<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kursiv</rad>/o
</kap>
<drv mrk="kursiv.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Klinita rapida skribado.
    </dif>
    <trdgrp lng="en">
      <trd>cursive</trd>,
      <trd>running hand</trd>
    </trdgrp>
    <trd lng="hu">folyóírás</trd>
    <trd lng="pl">pismo pochyłe</trd>
  </snc>
  <snc>
    <uzo tip="fak">TIP</uzo>
    <dif>
      Speco de presliteroj, klinitaj al dekstro, similantaj al la signoj
      de rapida manskribado.
    </dif>
    <ref tip="vid" cel="gras.0a.tip">grasa</ref>
    <rim>
      Ne konfuzi kun oblikva tiparfasono.
    </rim>
    <trdgrp lng="de">
      <trd>Kursivschrift</trd>,
      <trd>Kursive</trd>
    </trdgrp>
    <trd lng="en">italic<klr> letter, type</klr></trd>
    <trdgrp lng="es">
      <trd>cursiva</trd>,
      <trd>itálica</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>dőlt betűs írás</trd>,
      <trd>kurzív írás</trd>
    </trdgrp>
    <trd lng="it">corsivo</trd>
    <trd lng="pl">kursywa</trd>
    <trd lng="ru">курсив</trd>
  </snc>
  <trd lng="be">курсіў</trd>
  <trd lng="nl">cursief</trd>
  <trd lng="pt">cursivo</trd>
</drv>
</art>
