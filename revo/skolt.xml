<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: skolt.xml,v 1.29 2025/04/12 01:59:17 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>skolt</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="skolt.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="skolt.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Ano de armea <ref tip="vid" cel="tacxme.0o.MIL">ta&ccirc;mento</ref>
      (<ref tip="vid" cel="soldat.0o.militisto">soldato</ref>),
      &scirc;ipo, aviadilo k.s. okupi&gcirc;anta pri kolekto de informoj pri
      kontra&ubreve;ulo (ekz. &gcirc;ia pozicio, forto a&ubreve; irota vojo)
      kaj ties teritorio.
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_v;&c_je;&c_d;&c_n;&c_ib;&c_k;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_v;&c_ib;&c_zh;</trd>
    </trdgrp>
    <trd lng="de">Aufkl&auml;rer</trd>
    <trd lng="en">scout</trd>
    <trd lng="es">explorador</trd>
    <trd lng="fr">&eacute;claireur</trd>
    <trd lng="hu">felder&iacute;t&odblac;</trd>
    <trd lng="it">esploratore<klr> (mil.)</klr></trd>
    <trd lng="nl">verkenner</trd>
    <trdgrp lng="pl">
      <trd>skaut</trd>,
      <trd>zwiadowca</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_d;&c_ch;&c_i;&c_k;</trd>
    <trdgrp lng="zh">
    <trd>&#26021;&#20505; <pr>ch&igrave;h&ograve;u</pr></trd>,
    <trd>&#25506;&#23376; <pr>t&agrave;nzi</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="skolt.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Ano de la movado, kiun regas la principoj de <tld/>ismo.
    </dif>
    <ref tip="sub" cel="lup.0ido.SPO">lupido</ref>
    <ref tip="sub" cel="rover.0o.SPO">rovero</ref>

    <trd lng="be">&c_s;&c_k;&c_a;&c_w;&c_t;</trd>
    <trd lng="ca">boy scout</trd>
    <trd lng="cs">skaut</trd>
    <trd lng="de">Pfadfinder</trd>
    <trdgrp lng="en">
      <trd>scout</trd>,
      <trd>boy <ind>scout</ind></trd>,
      <trd>guide<klr> (girl scout)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>boy scout</trd>,
      <trd>explorador<klr> [m.]</klr></trd>, <!-- vira -->
      <trd>exploradora<klr> [f.]</klr></trd> <!-- ina -->
    </trdgrp>
    <trdgrp lng="fr">
      <trd>scout</trd>,
      <trd>&eacute;claireur<klr> [m.]</klr></trd>, <!-- vira -->
      <trd>guide<klr> [f.]</klr></trd>, <!-- ina -->
      <trd>&eacute;claireuse<klr> [f.]</klr></trd>, <!-- ina -->
    </trdgrp>
    <trd lng="hu">cserk&eacute;sz</trd>
    <trdgrp lng="it">
      <trd>scout</trd>,
      <trd>esploratore<klr> [m.]</klr></trd>, <!-- vira -->
      <trd>guida<klr> [f.]</klr></trd> <!-- ina -->
    </trdgrp>
    <trdgrp lng="nl">
      <trd>padvinder</trd>,
      <trd>gids<klr tip="ind"> (padvinder) [f.]</klr></trd> <!-- ina -->
      <trd>padvindster<klr> [f.]</klr></trd> <!-- ina -->
      <trd>verkenner<klr tip="ind"> (padvinder) [m.]</klr></trd>, <!--
vira -->
      <trd>scout<klr> [m.]</klr></trd> <!-- vira -->
    </trdgrp>
    <trdgrp lng="pl">
      <trd>skaut<klr> (g&lstroke;&oacute;wnie w dawnych krajach
kapitalistycznych)</klr></trd>,
      <trd>harcerz<klr> (w Polsce) [m.]</klr></trd>, <!-- vira -->
      <trd>harcerka<klr> (w Polsce) [f.]</klr></trd>, <!-- ina -->
      <trd>pionier<klr> (g&lstroke;&oacute;wnie w dawnych krajach
socjalistycznych)</klr></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>escoteiro<klr> [m.]</klr></trd>, <!-- vira -->
      <trd>escoteira<klr> [f.]</klr></trd> <!-- ina -->
      <trd>guia<klr> [f.]</klr></trd> <!-- ina -->
    </trdgrp>
    <trd lng="ru">&c_s;&c_k;&c_a;&c_u;&c_t;</trd>
    <trdgrp lng="zh">
    <trd>&#20390;&#23519;&#20853; <pr>zh&emacron;nch&aacute;b&imacron;ng</pr></trd>,
    <trd>&#20597;&#23519;&#20853; <pr>zh&emacron;nch&aacute;b&imacron;ng</pr></trd>,
    <trd>&#20390;&#23519;&#32773; <pr>zh&emacron;nch&aacute;zh&ecaron;</pr></trd>,
    <trd>&#20597;&#23519;&#32773; <pr>zh&emacron;nch&aacute;zh&ecaron;</pr></trd>,
    <trd>&#23547;&#36335;&#20154; <pr>x&uacute;nl&ugrave;r&eacute;n</pr></trd>,
    <trd>&#23563;&#36335;&#20154; <pr>x&uacute;nl&ugrave;r&eacute;n</pr></trd>,
    <trd>&#26032;&#36335;&#24320;&#25299;&#32773; <pr>x&imacron;nl&ugrave;k&amacron;itu&ograve;zh&ecaron;</pr></trd>,
    <trd>&#26032;&#36335;&#38283;&#25299;&#32773; <pr>x&imacron;nl&ugrave;k&amacron;itu&ograve;zh&ecaron;</pr></trd>,
    <trd>&#24320;&#21019;&#32773; <pr>k&amacron;ichu&agrave;ngzh&ecaron;</pr></trd>,
    <trd>&#38283;&#21109;&#32773; <pr>k&amacron;ichu&agrave;ngzh&ecaron;</pr></trd>,
    <trd>&#24320;&#36335;&#20808;&#38155; <pr>k&amacron;il&ugrave;xi&amacron;nf&emacron;ng</pr></trd>,
    <trd>&#38283;&#36335;&#20808;&#37586; <pr>k&amacron;il&ugrave;xi&amacron;nf&emacron;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="bo">&t_sa;&t_naro;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="cs">
    <trd>jun&aacute;k</trd>,
    <trd>pr&uring;zkumn&iacute;k</trd>,
    <trd>p&aacute;tra&ccaron;</trd>,
    <trd>vyzv&ecaron;da&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#12508;&#12540;&#12452;&#12473;&#12459;&#12454;&#12488;</trd>,
    <trd>&#26021;&#20505;  <pr>&#12379;&#12387;&#12371;&#12358;</pr></trd>,
    <trd>&#20597;&#23519;&#20853;  <pr>&#12390;&#12356;&#12373;&#12388;&#12408;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="sk">prieskumn&aacute; lo&dcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_l;&c_a;&c_s;&c_t;&c_u;&c_n;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_u;&c_t;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_v;&c_ib;&c_d;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="skolt.0ismo">
  <kap><ofc>9</ofc><tld/>ismo <fnt><bib>PV</bib></fnt></kap>
  <snc mrk="skolt.0ismo.POL">
    <dif>
      Internacia junulara movado fondita en Anglujo en 1907,
      kies celo estas formi karakteron, senton de l' honoro, sanon,
      kaj iniciatan spiriton de junuloj, grupigante ilin en
      <ref tip="vid" cel="trup.0o.PED">trupojn</ref>,
      submetitajn al libere akceptita disciplino:
      <ekz>
         Carola Antskog <klr>...</klr> dedi&ccirc;is sin anka&ubreve; al
         la plibonigo de la vivo de gejunuloj per <tld/>ismo
         <fnt>
           <aut>A. S. T. Garrido</aut>,
           <vrk><url ref="http://www.liberafolio.org/2017/02/21/formi-unuecon-en-diverseco/">
           Formi unuecon en diverseco</url></vrk>,
           <lok>libera folio, 2017-02-21</lok>
         </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_w;&c_t;&c_y;&c_z;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>jun&aacute;ctv&iacute;</trd>,
    <trd>skauting</trd>
  </trdgrp>
  <trd lng="de">Pfadfinderbewegung</trd>
  <trdgrp lng="en">
    <trd>scouting<klr> (male)</klr></trd>,
    <trd>guiding<klr> (female)</klr></trd>
  </trdgrp>
  <trd lng="es">escultismo</trd>
  <trd lng="fr">scoutisme</trd>
  <trd lng="hu">cserk&eacute;szmozgalom</trd>
  <trd lng="it">scoutismo</trd>
  <trd lng="ja">&#12508;&#12540;&#12452;&#12473;&#12459;&#12454;&#12488;&#36939;&#21205;  <pr>&#12508;&#12540;&#12452;&#12473;&#12459;&#12454;&#12488;&#12358;&#12435;&#12393;&#12358;</pr></trd>
  <trd lng="nl">scouting</trd>
  <trdgrp lng="pl">
    <trd>skauting</trd>,
    <trd>harcerstwo</trd>
  </trdgrp>
  <trd lng="pt">escotismo</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_u;&c_t;&c_i;&c_z;&c_m;</trd>
  <trd lng="sk">skautsk&eacute; hnutie</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_l;&c_a;&c_s;&c_t;&c_u;&c_n;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_r;&c_u;&c_h;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_u;&c_t;&c_s;&c_mol;&c_k;&c_i;&c_j; &c_r;&c_u;&c_h;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_u;&c_t;&c_i;&c_z;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#31461;&#20891;&#36816;&#21160; <pr>t&oacute;ngj&umacron;ny&ugrave;nd&ograve;ng</pr></trd>,
    <trd>&#31461;&#36557;&#36939;&#21205; <pr>t&oacute;ngj&umacron;ny&ugrave;nd&ograve;ng</pr></trd>,
    <trd>&#22899;&#31461;&#20891; <pr>n&#474;t&oacute;ngj&umacron;n</pr></trd>,
    <trd>&#22899;&#31461;&#36557; <pr>n&#474;t&oacute;ngj&umacron;n</pr></trd>
  </trdgrp>
</drv>
</art>
