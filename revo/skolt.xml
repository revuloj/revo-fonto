<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>skolt</rad>/o
</kap>
<drv mrk="skolt.0o">
  <kap><tld/>o</kap>
  <snc mrk="skolt.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Ano de armea ta&ccirc;mento, okupi&gcirc;anta pri kolekto de informoj
      pri kontra&ubreve;ulo kaj ties teritorio.
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_v;&c_je;&c_d;&c_n;&c_ib;&c_k;</trd>, 
      <trd>&c_r;&c_a;&c_z;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_y;&c_k;</trd>, 
      <trd>&c_v;&c_ib;&c_zh;</trd>
    </trdgrp>
    <trd lng="de">Aufkl&auml;rer</trd>
    <trd lng="en">scout</trd>
    <trd lng="es">explorador</trd>
    <trd lng="fr">&eacute;claireur</trd>
    <trd lng="hu">felder&iacute;t&odblac;</trd>
    <trd lng="nl">verkenner</trd>
    <trdgrp lng="pl">
      <trd>skaut</trd>,
      <trd>zwiadowca</trd>
    </trdgrp>
    <trd lng="ru">&c_r;&c_a;&c_z;&c_v;&c_je;&c_d;&c_ch;&c_i;&c_k;</trd>
  </snc>
  <snc mrk="skolt.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Ano de la movado, kiun regas la principoj de <tld/>ismo.
    </dif>
    <trd lng="be">&c_s;&c_k;&c_a;&c_w;&c_t;</trd>
    <trd lng="ca">boy scout</trd>
    <trd lng="cs">skaut</trd>
    <trd lng="de">Pfadfinder</trd>
    <trdgrp lng="en">
      <trd>scout</trd>,
      <trd>boy scout</trd>,
      <trd>girl scout</trd>
    </trdgrp>
    <trd lng="es">boy scout</trd>
    <trd lng="fr">scout</trd>
    <trd lng="hu">cserk&eacute;sz</trd>
    <trdgrp lng="nl">
      <trd>padvinder</trd>,
      <trd>gids<klr tip="ind"> (padvinder)</klr></trd>,
      <trd>verkenner<klr tip="ind"> (padvinder)</klr></trd>,
      <trd>scout</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>skaut</trd>,
      <trd>harcerz</trd>,
      <trd>pionier</trd>
    </trdgrp>
    <trd lng="ru">&c_s;&c_k;&c_a;&c_u;&c_t;</trd>
  </snc>
</drv>

<drv mrk="skolt.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Movado kies celo estas formi karakteron, senton de l' honoro, sanon,
      kaj iniciatan spiriton de la junularo, grupigante ilin en trupojn,
      submetitajn al libere akceptita disciplino.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_a;&c_w;&c_t;&c_y;&c_z;&c_m;</trd>  
  <trd lng="de">Pfadfinderbewegung</trd>
  <trd lng="fr">scoutisme</trd>
  <trd lng="hu">cserk&eacute;szmozgalom</trd>
  <trd lng="nl">scouting</trd>
  <trdgrp lng="pl">
    <trd>skauting</trd>,
    <trd>harcerstwo</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_u;&c_t;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
