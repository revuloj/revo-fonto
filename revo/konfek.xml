<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: konfek.xml,v 1.25 2023/12/03 09:58:55 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>konfekci</rad>/o
</kap>
<drv mrk="konfek.0o">
  <kap><tld/>o</kap>
  <snc mrk="konfek.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.0o.TEKS">Vesto</ref>, produktata en fabriko la&ubreve; proksimumaj
      dimensioj,
      ne la&ubreve; la &gcirc;usta mezuro de &ccirc;iu kliento;
      industrio kaj komerco okupi&gcirc;anta pri tiaj vestoj:
      <ekz>
        &ccirc;ijare totala eksporta sumo de &ccirc;inaj
        tekstila&jcirc;oj kaj <tld/>oj eble superos cent miliardojn
        da usonaj dolaroj
        <fnt><lok><url ref="http://es.chinabroadcast.cn/1/2005/04/16/1@23499.htm">&Ccirc;ina
        Radio Internacia</url></lok></fnt>;
      </ekz>
      <ekz>
        sinjorino <nom>Zhang Lei</nom> naski&gcirc;is en familio
        okupi&gcirc;anta pri <tld/>o
        <fnt><vrk>Vestigu la urbanojn ankora&ubreve; pli belaj</vrk><lok><url ref="http://esperanto.cri.com.cn/esperanto/2003/Sep/155894.htm">&Ccirc;ina
        Radio Internacia</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">confecci&oacute;</trd>
  <trd lng="cs">konfekce</trd>
  <trdgrp lng="de">
    <trd>Stangenware</trd>,
    <trd>Fabrikware</trd>,
    <trd>Konfektionskleidung</trd>,
    <trd>Kleidung von der Stange</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ready-made suit <klr>(or dress)</klr></trd>,
    <trd>ready-made clothes</trd>,
    <trd>confection<klr tip="amb"> (clothing)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>confection<klr tip="ind"> (pr&ecirc;t-&agrave;-porter)</klr></trd>,
    <trd>pr&ecirc;t-&agrave;-porter</trd>
  </trdgrp>
  <trd lng="ja">&#x65E2;&#x88FD;&#x670D;  <pr>&#x304D;&#x305B;&#x3044;&#x3075;&#x304F;</pr></trd>
  <trd lng="nl">confectie</trd>
  <trd lng="pl">konfekcja</trd>
  <trdgrp lng="pt">
    <trd>confec&ccedil;&atilde;o</trd>,
    <trd>pr&ecirc;t-&agrave;-porter</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_n;&c_f;&c_je;&c_k;&c_c;&c_i;&c_o;&c_n;</trd>,
    <trd>&c_g;&c_o;&c_t;&c_o;&c_v;&c_a;&c_ja; &c_o;&c_d;&c_je;&c_zh;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">konfekcia</trd>
  <trdgrp lng="zh">
    <trd>&#x6210;&#x8863; <pr>ch&#xE9;ngy&#x12B;</pr></trd>,
    <trd>&#x73B0;&#x6210; <pr>xi&#xE0;nch&#xE9;ng</pr></trd>,
    <trd>&#x670D;&#x88C5; <pr>f&#xFA;zhu&#x101;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="konfek.0i">
  <kap>
    <tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="konfek.0i.TEKS">
    <uzo tip="klr">(pri vestoj)</uzo>
    <dif>
      <ref tip="super" cel="produk.0i.TEK">Produkti</ref> la&ubreve; normaj dimensioj, pogrande:
      <ekz>
        li surhavis novan redingoton, kiun li estis a&ccirc;etinta, <tld/>itan, en la tenejo
        de la <nom>Belle-Jardini&egrave;re</nom>
        <fnt>
          <aut>E. About, trad. G. Moch</aut>
          <vrk><url ref="&Gutenberg;files/65835/65835-0.txt">La Re&gcirc;o de la Montoj</url></vrk>
          <lok>1909</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>konfektionieren</trd>,
    <trd>anfertigen <klr>(Kleidung)</klr></trd>
  </trdgrp>
  <trd lng="fr">confectionner</trd>
  <trdgrp lng="ja">
    <trd>&#x4F5C;&#x308B;  <pr>&#x3064;&#x304F;&#x308B;</pr></trd>,
    <trd>&#x88FD;&#x9020;&#x3059;&#x308B;  <pr>&#x305B;&#x3044;&#x305E;&#x3046;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="zh">&#x7E2B;&#x88FD; <pr>f&#xE9;ngzh&#xEC;</pr></trd>
</drv>
</art>
