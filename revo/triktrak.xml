<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: triktrak.xml,v 1.13 2022/10/25 07:54:09 revo Exp $">
<kap>
  <rad>triktrak</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="triktrak.0o">
  <kap><tld/>o</kap>
  <snc mrk="triktrak.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Surtabla <ref tip="super" cel="lud.0o.ago">ludo<sncref/></ref>
      por du homoj, en kiu oni movas peonojn sur triangulaj markoj de 
      duparta breto la&ubreve; poentoj akiritaj per &jcirc;etkuboj:
      <ekz>
        <nom>Don Gaiferos</nom> for&jcirc;etas de si la tabulon de la <tld/>o
        <fnt>
          <bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 26a, p.
          541a</lok>
        </fnt>;
      </ekz>
      <ekz>
        usona mod-ilustristo, desegnis sian edzinon ludantan <tld/>on en la kelo de sia
        hejmo dum aerataka alarmo
        <fnt>
          <bib>MD14</bib>
          <vrk>Aspekti ri&ccirc;e nun aspektas nebone</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_k;&c_t;&c_r;&c_a;&c_k;</trd>
  <trd lng="bg">&c_t;&c_a;&c_b;&c_l;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>jaquet</trd>,
    <trd>tric-trac</trd>,
    <trd>backgammon</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Backgammon</trd>,
    <trd>Tric Trac</trd>
  </trdgrp>
  <trd lng="en">backgammon</trd>
  <trdgrp lng="fr">
    <trd>backgammon</trd>,
    <trd>jacquet<klr> (jeu)</klr></trd>,
    <trd>trictrac</trd>
  </trdgrp>
  <trd lng="hu">triktrak</trd>
  <trdgrp lng="pl">
    <trd>backgammon</trd>,
    <trd>tryk trak</trd>,
    <trd>tryktrak</trd>,
    <trd>trik trak</trd>,
    <trd>triktrak</trd>
  </trdgrp>
  <trd lng="pt">triquetraque</trd>
  <trd lng="uk">&c_t;&c_r;&c_i;&#x43A;&c_t;&c_r;&c_a;&#x43A; (&c_g;&c_r;&c_a;)</trd>
</drv>
</art>
