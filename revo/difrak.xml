<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>difrakt</rad>/o
    </kap>

    <drv mrk="difrak.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">FIZ</uzo>
      <snc>
        <dif>
          Perturbo de ondo, ekz-e lumo, kiam &gcirc;i renkontas
          obstaklon
          kun dimensio simila al &gcirc;ia ondolongo:
          <ekz>
            <tld/>o de iks-radioj tra kristala reto.
          </ekz>
        </dif>
      </snc>
      <ref tip="vid" cel="refrak.0o">refrakto</ref>
      <trdgrp lng="be">
        <trd>&c_d;&c_y;&c_f;&c_r;&c_a;&c_k;&c_c;&c_y;&c_ja;</trd>,
        <trd>&c_p;&c_je;&c_r;&c_a;&c_l;&c_a;&c_m;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;
        &c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_a;</trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>Beugung</trd>,
        <trd>Diffraktion</trd>
      </trdgrp>
      <trd lng="en">diffraction</trd>
      <trd lng="es">difracci&oacute;n</trd>
      <trd lng="fr">diffraction</trd>
      <trdgrp lng="hu">
        <trd>diffrakci&oacute;</trd>,
        <trd>f&eacute;nyelhajl&aacute;s</trd>,
        <trd>hull&aacute;melhajl&aacute;s</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>dyfrakcja</trd>,
        <trd>ugi&eogonek;cie <klr tip="amb">fal</klr></trd>
      </trdgrp>
      <trd lng="pt">difra&ccedil;&atilde;o</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_je;&c_l;&c_o;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
        <trd>&c_d;&c_i;&c_f;&c_r;&c_a;&c_k;&c_c;&c_i;&c_ja;</trd>
      </trdgrp>
    </drv>

    <drv mrk="difrak.0i">
      <kap><tld/>i</kap>
      <snc>
        <dif>
          Krei <tld/>on:
          <ekz>
            mallar&gcirc;a fendo <tld/>as la lumon.
          </ekz>
        </dif>
      </snc>
      <trd
      lng="be">&c_p;&c_je;&c_r;&c_a;&c_l;&c_a;&c_m;&c_l;&c_ja;&c_c;&c_mol;
      &c_s;&c_mol;&c_v;&c_ja;&c_t;&c_l;&c_o;</trd>
      <trdgrp lng="de">
        <trd>beugen</trd>
      </trdgrp>
      <trd lng="fr">diffracter</trd>
      <trdgrp lng="hu">
        <trd>elhajl&iacute;t <klr tip="ind"> (hull&aacute;mot)</klr></trd>,
        <trd>elt&eacute;r&iacute;t <klr tip="ind"> (hull&aacute;mot)</klr></trd>
      </trdgrp>
      <trd lng="pt">difratar</trd>
      <trdgrp lng="ru">
        <trd>&c_p;&c_r;&c_je;&c_l;&c_o;&c_m;&c_l;&c_ja;&c_t;&c_mol;<klr>
        (&c_l;&c_u;&c_ch;&c_i;)</klr></trd>,
        <trd>&c_d;&c_i;&c_f;&c_r;&c_a;&c_g;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </drv>

    <drv mrk="difrak.0a">
      <kap><tld/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>o:
          <ekz>
            <tld/>aj fenomenoj.
          </ekz>
        </dif>
        <trdgrp lng="de">
          <trd>Beugungs-</trd>,
        </trdgrp>
        <trdgrp lng="pl">
          <trd>dyfrakcyjny</trd>
        </trdgrp>
      </snc>
      <snc>
        <dif>
          <tld lit="D"/>anta:
          <ekz>
            <tld/>a kristala reto.
          </ekz>
        </dif>
        <trdgrp lng="de">
          <trd>beugend</trd>
        </trdgrp>
        <trd lng="fr">diffringent</trd>
        <trdgrp lng="pl">
          <trd>dyfrakcyjny</trd>
        </trdgrp>
      </snc>
      <trd
      lng="be">&c_d;&c_y;&c_f;&c_r;&c_a;&c_k;&c_c;&c_y;&c_j;&c_n;&c_y;</trd>
      <trd lng="hu">diffrakci&oacute;s</trd>
      <trd
      lng="ru">&c_d;&c_i;&c_f;&c_r;&c_a;&c_k;&c_c;&c_i;&c_o;&c_n;&c_n;&c_y;&c_j;</trd>
    </drv>
  </art>
