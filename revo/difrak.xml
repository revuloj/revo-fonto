<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>difrakt</rad>/o
</kap>
<drv mrk="difrak.0o">
<kap><tld/>o</kap>
<uzo tip="fak">FIZ</uzo>
<snc>
<dif>
Perturbo de ondo, ekz-e lumo, kiam &gcirc;i renkontas obstaklon
kun dimensio simila al &gcirc;ia ondolongo:
  <ekz>
    <tld/>o de iks-radioj tra kristala reto.
  </ekz>
</dif>
</snc>
  <trdgrp lng="de">
     <trd>Beugung</trd>,
     <trd>Brechung</trd>,
     <trd>Diffraktion</trd>
  </trdgrp>
<trd lng="fr">diffraction</trd>
<trdgrp lng="ru">
  <trd>&c_p;&c_r;&c_je;&c_l;&c_o;&c_m;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
  <trd>&c_d;&c_i;&c_f;&c_r;&c_a;&c_k;&c_c;&c_i;&c_ja;</trd>
</trdgrp>
</drv>
<drv mrk="difrak.0i">
  <kap><tld/>i</kap>
  <snc>
    <dif>
      Krei <tld/>on:
      <ekz>
        mallar&gcirc;a fendo <tld/>as la lumon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>beugen</trd>,
     <trd>brechen</trd>
  </trdgrp>
  <trd lng="fr">diffracter</trd>
</drv>
<drv mrk="difrak.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>aj fenomenoj.
      </ekz>
    </dif>
  <trdgrp lng="de">
     <trd>Beugungs-</trd>,
     <trd>Brechungs-</trd>
  </trdgrp>
  </snc>
  <snc>
    <dif>
      <tld lit="D"/>anta:
      <ekz>
        <tld/>a kristala reto.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>beugend</trd>,
      <trd>brechend</trd>
    </trdgrp>
    <trd lng="fr">diffringent</trd>
  </snc>
</drv>
</art>
