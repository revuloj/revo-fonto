<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tegmen.xml,v 1.36 2024/02/27 09:23:42 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>tegment</rad>/o
</kap>
<drv mrk="tegmen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
  <uzo tip="fak">KON</uzo>
    <dif>
      Supera <ref tip="super" cel="teg.0ajxo.tego">tega&jcirc;o</ref>
      de konstrua&jcirc;o, &scirc;irmanta kontra&ubreve; vetero:
      <ekz>
        birdo <klr>[&#8230;]</klr> surflugis sur la <tld/>on
        <fnt>
          <bib>F</bib>
          <lok>&FE; 31</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur kruta &scirc;tuparo li levis sin al la <tld/>o de la domo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne maltrankvili&gcirc;u, vi trovos <tld/>on, sub kiu vi povos lo&gcirc;i
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vivi sub la sama <tld/>o <klr>(kune)</klr>
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>V</lok>
        </fnt>;
      </ekz>
      <ekz>
        sub la gepatra <tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        li sin firme alkro&ccirc;is per amba&ubreve; manoj sur la <tld/>oj de du vagonoj
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la zinko de defluiloj kaj de <tld/>oj estis vendita la&ubreve;peze
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        vila&gcirc;oj kun malaltaj kabanoj, kovritaj per nigraj pajlaj <tld/>oj
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>III</lok>
        </fnt>;
      </ekz>
      <ekz>
        kamentuboj superstaris la pajlajn <tld/>ojn
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        hirundo <klr>[&#8230;]</klr> havis sian neston sub la kana <tld/>o de la &ccirc;asisto
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili venis sub la ombron de mia <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 19:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i suprenirigis ilin sur la <tld/>on, kaj ka&scirc;is ilin en
        la trunkoj de lino, kiuj ku&scirc;is &ccirc;e &scirc;i sur la <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 2:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj lo&gcirc;antoj de la urbo <klr>[&#8230;]</klr> en&scirc;losis sin tie kaj supreniris sur la
        <tld/>on de la turo
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 9:51</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Noa</nom> malfermis la <tld/>on de la arkeo, kaj li
        vidis, ke seki&gcirc;is la supra&jcirc;o de la tero
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 8:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas ankora&ubreve; birdo sur la <tld/>o
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        en fremda <tld/>o li flikas truon kaj en propra ne vidas la fluon
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_d;&c_a;&c_h;</trd>,
     <trd>&c_s;&c_t;&c_r;&c_a;&c_h;&c_a;</trd>
   </trdgrp>
  <trd lng="bo">&t_tha;&t_naro;&t_ga;&t_tsheg;&t_ka;&t_tsheg;</trd>
  <trd lng="ca">teulada</trd>
  <trd lng="cs">st&rcaron;echa</trd>
  <trd lng="de">Dach</trd>
  <trd lng="en">roof</trd>
  <trd lng="es">tejado</trd>
  <trd lng="fr">toit</trd>
  <trd lng="hu">tet&odblac;</trd>
  <trd lng="ja">&#x5C4B;&#26681;  <pr>&#12420;&#x306D;</pr></trd>
  <trd lng="nl">dak</trd>
  <trd lng="pl">dach</trd>
  <trdgrp lng="pt">
    <trd>telhado</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_y;&c_sh;&c_a;</trd>
  <trd lng="sk">strecha</trd>
  <trd lng="sv"><klr>(ytter-)</klr>tak</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_a;&c_h;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_r;&c_ib;&c_v;&c_l;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="tegmen.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="kovr.0i.surmeti">Kovri</ref> per <tld/>o:
      <ekz>
        la butiko estis jam <tld/>ita
        <fnt><aut>Bardhyl Selimi</aut>:
          <vrk><url ref="&Monato;/2001/007205.html">Voja&gcirc;impresoj, somero 2000</url></vrk>,
          <bib>Monato</bib>
          <lok>2001/01, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        fragmit-<tld/>ita domo
        <fnt>
          <bib>Monato</bib>
          <aut>Jean-Yves Santerre</aut>
          <vrk><url ref="&Monato;/2013/010991.php">Kie akvo kaj &ccirc;ielo najbaras</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>ita <klr>(supersignita)</klr> litero.
      </ekz>
    </dif>
    <trd lng="en">to roof</trd>
    <trd lng="be">&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol;
      <ind>&c_d;&c_a;&c_h;&c_a;&c_m;</ind></trd>
    <trd lng="cs">zast&rcaron;e&scaron;it</trd>
    <trd lng="de">&uuml;berdachen</trd>
    <trd lng="fr">couvrir <klr>(une maison)</klr></trd>
    <trd lng="hu">(tet&odblac;vel) <ind>fed</ind></trd>
    <trdgrp lng="pl">
      <trd>zadasza&cacute;</trd>,
      <trd>pokrywa&cacute; <ind>dachem</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;
      <ind>&c_k;&c_r;&c_y;&c_sh;&c_je;&c_j;</ind></trd>
    <trd lng="sk">zastre&scaron;i&tcaron;</trd>
    <trdgrp lng="uk">
      <trd>&c_p;&c_o;&c_k;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
      <trd>&c_r;&c_o;&c_b;&c_i;&c_t;&c_i; &c_d;&c_a;&c_h;</trd>
    </trdgrp>
  </snc>
  <snc mrk="tegmen.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="proviz.0i.prizorgi">Provizi</ref>
      komunan kadron al pluraj asocioj, temoj ks:
      <ekz>
        grava estas la Agado Espero, kiun iniciatis kaj <tld/>is UEA, por helpi
        militviktimojn
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 1 (63)</lok>
        </fnt>;
      </ekz>
      <ekz>
        mankis klerigaj aran&gcirc;oj pri interlingvistiko, literaturo, &jcirc;urnalismo kaj aliaj,
        kiujn anta&ubreve;e <tld/>is &leftquot;Somera E-Universitato&rightquot;
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 8-9</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="asoci.0i.kungrupigi">asocii<sncref/></ref>,
      <ref cel="patron.0i">patroni</ref>,
      <ref cel="reprezent.0i">reprezenti</ref>
    </refgrp>
    <trd lng="de">umrahmen <klr>(organisatorisch)</klr></trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x5C4B;&#26681;&#12434;&#12405;&#x304F;  <pr>&#12420;&#x306D;&#12434;&#12405;&#x304F;</pr></trd>,
    <trd>&#x5C4B;&#26681;&#12434;&#12388;&#12369;&#x308B;  <pr>&#12420;&#x306D;&#12434;&#12388;&#12369;&#x308B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="tegmen.sub0o">
  <kap><ofc>*</ofc>sub<tld/>o,
    <var><kap>sub<tld/>ejo</kap></var>
  </kap>
  <snc>
    <dif>
      Loko ku&scirc;anta senpere sub la <tld/>o, ofte uzata kiel stokejo:
      <ekz>
        <klr>[li]</klr> portis lin en la sub<tld/>on, en kiu li lo&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 17:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj venis supren kaj faris esploradon en la sub<tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni veturigis la sekan fojnon al la grandaj sub<tld/>ejoj de la staloj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
     </dif>
     <ref tip="vid" cel="mansar.0o">mansardo</ref>
     <rim>
       Amba&ubreve; formoj havas siajn misojn: &leftquot;sub<tld/>o&rightquot;, formita el adjektivo propre indikas nur
       abstraktan lokindikon (kp grando, seko), sed ne konkrtan &ccirc;ambron;
       dum la sufikso &leftquot;-ej&rightquot; nomas aferojn, en kiuj trovi&gcirc;as la karakteriza&jcirc;o (kp densejo). Se oni
       volas eviti tiujn malglata&jcirc;ojn, oni uzu duvorta&jcirc;on kiel
       &leftquot;subtegmenta stokejo&rightquot;.
     </rim>
  </snc>
  <trdgrp lng="be">
     <trd>&c_g;&c_a;&c_r;&c_a;</trd>,
     <trd>&c_g;&c_a;&c_r;&c_y;&c_sh;&c_ch;&c_a;</trd>,
     <trd>&c_p;&c_a;&c_d;&c_d;&c_a;&c_sh;&c_a;&c_k;</trd>
   </trdgrp>
  <trd lng="ca">&agrave;tic</trd>
  <trdgrp lng="cs">
    <trd>podkrov&iacute;</trd>,
    <trd>p&uring;da</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Dachboden</trd>,
     <trd>Speicher</trd>
   </trdgrp>
  <trd lng="en">attic</trd>
  <trd lng="es">&aacute;tico</trd>
  <trdgrp lng="fr">
     <trd>grenier</trd>,
     <trd>soupente</trd>,
     <trd>combles</trd>,
     <trd>galetas</trd>
   </trdgrp>
  <trdgrp lng="hu">
    <trd>padl&aacute;s</trd>,
    <trd>tet&odblac;t&eacute;r</trd>
   </trdgrp>
  <trd lng="ja">&#x5C4B;&#26681;&#x88CF;&#x90E8;&#x5C4B;  <pr>&#12420;&#x306D;&#12358;&#12425;&#12409;&#12420;</pr></trd>
  <trd lng="nl">zolder</trd>
  <trdgrp lng="pl">
     <trd>poddasze</trd>,
     <trd>strych</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_ch;&c_je;&c_r;&c_d;&c_a;&c_k;</trd>,
     <trd>&c_m;&c_a;&c_n;&c_s;&c_a;&c_r;&c_d;&c_a;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>manzarda</trd>,
    <trd>podkrovie</trd>,
    <trd>podstre&scaron;ie</trd>,
    <trd>povala</trd>,
    <trd>p&ocirc;jd</trd>
  </trdgrp>
  <trd lng="sv"><klr>(hus-)</klr>vind</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_d;&c_a;&c_sh;&c_sh;&c_ja;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_i;&c_shch;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="tegmen.0isto">
   <kap><tld/>isto</kap>
   <snc>
   <uzo tip="fak">KON</uzo>
     <dif>
      Homo, kiu konstruas <tld/>on.
      <ekz>
        <tld/>isto <klr>[&#8230;]</klr> riparis nian ferian domon kaj
        poste diris, ke nun neniu muso plu povos eniri
        <fnt>
          <aut>Stefan Maul</aut>,
          <vrk>
            <url ref="&Monato;/2003/008208.html">Monato,
            oktobro 2003</url>
          </vrk>,
          <lok>El mia ridpunkto. Du ne mensogis</lok>
       </fnt>;
      </ekz>
      <ekz>
        infanoj el bazlernejoj ne plu volas fari&gcirc;i <klr>[&#8230;]</klr> masonisto,
        &ccirc;arpentisto, <klr>[&#8230;]</klr> farbisto, <tld/>isto a&ubreve; terkulturisto
        <fnt>
          <bib>Monato</bib>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2014/011284.php">Batalo por la studontoj</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>iston ni konsideru kiel fakulon, kiu kovras
        <tld/>on per iu materialo protektanta domon kontra&ubreve;
        pluvakvo, forkondukanta la akvon eksteren a&ubreve; al defluiga
        sistemo
        <fnt>
          <aut>Jan Werner</aut>,
          <vrk>
          <url ref="http://fabien.galerio.org/kofo/cirkuleroj/kofo12.pdf">
            Konstru-forumo nr 12</url>
          </vrk>
          <lok>septembro 2008; La nova temo 20-a &ndash; ladisto,
          zinkisto, tubisto</lok>
       </fnt>.
      </ekz>
     </dif>
     <ref tip="sub" cel="tegol.0isto">tegolisto</ref>
   </snc>
   <trdgrp lng="be">
     <trd>&c_s;&c_t;&c_r;&c_a;&c_h;&c_a;&c_r;</trd>,
     <trd>&c_d;&c_a;&c_h;&c_a;&c_w;&c_sh;&c_ch;&c_y;&c_k;</trd>,
     <trd>&c_k;&c_r;&c_y;&c_t;&c_n;&c_ib;&c_k;</trd>
   </trdgrp>
   <trd lng="en">roofer</trd>
   <trd lng="fr">couvreur</trd>
   <trdgrp lng="pl">
     <trd>dekarz</trd>
   </trdgrp>
  <trd lng="sk">pokr&yacute;va&ccaron; strechy</trd>
  <trd lng="cs">pokr&yacute;va&ccaron;</trd>
  <trd lng="uk">&c_p;&c_o;&c_k;&c_r;&c_ib;&c_v;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="tegmen.0a_cxambro">
  <kap>
    <tld/>a &ccirc;ambro,
    <var><kap>sub<tld/>a &ccirc;ambro</kap></var>
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="cxambr.0o.doma">&Ccirc;ambro</ref> ku&scirc;anta senpere sub la <tld/>o:
      <ekz>
        li konstruis <klr>[&#8230;]</klr> &gcirc;is la <tld/>a &ccirc;ambro
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 3:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi lo&gcirc;is en sub<tld/>a &ccirc;ambro
        <fnt>
          <bib>IKr</bib>
          <lok>Bol&ccirc;jo</lok>
        </fnt>;
      </ekz>
      <ekz>
        supre en la sub<tld/>a &ccirc;ambreto staras duone vestita malgranda dancistino
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>.
      </ekz>
    </dif>
     <refgrp tip="sin">
       <ref cel="mansar.0o">mansardo</ref>
     </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_d;&c_a;&c_sh;&c_a;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_s;&c_a;&c_r;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Mansarde</trd>
  <trdgrp lng="pl">
    <trd>poddasze</trd>,
    <trd>mansarda</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_ib;&c_d;&c_d;&c_a;&c_sh;&c_sh;&c_ja;</trd>,
    <trd>&c_g;&c_o;&c_r;&c_i;&c_shch;&c_je;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_s;&c_a;&c_r;&c_d;&c_a;</trd>
  </trdgrp>
</drv>
