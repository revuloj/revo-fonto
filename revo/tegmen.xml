<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tegment</rad>/o
</kap>
<drv mrk="tegmen.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Supera tega&jcirc;o de domo, konsistanta ordinare el tegoloj a&ubreve; ardezoj:
      <ekz>
        pinta, plata <tld/>o;
      </ekz>
      <ekz>
        pajla, kana <tld/>o;
      </ekz>
      <ekz>
        <tld/>a (ku&scirc;anta senpere sub la <tld/>o) &ccirc;ambro<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Dach</trd>
  <trd lng="en">roof</trd>
  <trd lng="nl">dak</trd>
  <trd lng="ru">&c_k;&c_r;&c_y;&c_sh;&c_a;</trd>
  <trd lng="sv"><klr>(ytter-)</klr>tak</trd>
</drv>

<drv mrk="tegmen.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Kovri per <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>ita <klr>(supersignita)</klr> litero.
      </ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol; <ind>&c_k;&c_r;&c_y;&c_sh;&c_je;&c_j;</ind></trd>
</drv>

<drv mrk="tegmen.sub0o">
  <kap><ofc>*</ofc>sub<tld/>o</kap>
  <snc>
    <dif>
      Loko a&ubreve; &ccirc;ambro ku&scirc;anta senpere sub la <tld/>o
    </dif>
    <refgrp tip="sin">
      <ref cel="mansar.0o">mansardo</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">zolder</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_r;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_n;&c_s;&c_a;&c_r;&c_d;&c_a;</trd>
  </trdgrp>
  <trd lng="sv"><klr>(hus-)</klr>vind</trd>
</drv>
</art>
