<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tegmen.xml,v 1.26 2021/05/29 14:33:20 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>tegment</rad>/o
</kap>
<drv mrk="tegmen.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <snc>
   <uzo tip="fak">KON</uzo>
     <dif>
       Supera tega&jcirc;o de domo, konsistanta ordinare el tegoloj
       a&ubreve; ardezoj:
       <ekz>
         pinta, plata <tld/>o;
       </ekz>
       <ekz>
         kana <tld/>o;
       </ekz>
       <ekz>
         <ind>pajla <tld/>o</ind>;
         <trd lng="pl">strzecha</trd>
       </ekz>
       <ekz>
         <tld/>a (ku&scirc;anta senpere sub la <tld/>o) &ccirc;ambro
         <fnt>Z</fnt>.
       </ekz>
     </dif>
   </snc>
  <trdgrp lng="be">
     <trd>&c_d;&c_a;&c_h;</trd>,
     <trd>&c_s;&c_t;&c_r;&c_a;&c_h;&c_a;</trd>
   </trdgrp>
  <trd lng="bo">&#xF50;&#xF7C;&#xF42;&#xF0B;&#xF40;&#xF0B;</trd>
  <trd lng="ca">teulada</trd>
  <trd lng="cs">st&rcaron;echa</trd>
  <trd lng="de">Dach</trd>
  <trd lng="en">roof</trd>
  <trd lng="es">tejado</trd>
  <trd lng="fr">toit</trd>
  <trd lng="hu">tet&odblac;</trd>
  <trd lng="nl">dak</trd>
  <trd lng="pl">dach</trd>
  <trdgrp lng="pt">
    <trd>telhado</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_r;&c_y;&c_sh;&c_a;</trd>
  <trd lng="sk">strecha</trd>
  <trd lng="sv"><klr>(ytter-)</klr>tak</trd>
</drv>

<drv mrk="tegmen.0i">
   <kap><tld/>i<fnt>Z</fnt></kap>
   <gra><vspec>tr</vspec></gra>
   <snc>
     <dif>
       Kovri per <tld/>o:
       <ekz>
         <uzo tip="stl">FIG</uzo>
         <tld/>ita <klr>(supersignita)</klr> litero.
       </ekz>
     </dif>
   </snc>
   <trd lng="en">to roof</trd>
  <trd lng="be">&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol; 
<ind>&c_d;&c_a;&c_h;&c_a;&c_m;</ind></trd>
  <trd lng="cs">zast&rcaron;e&scaron;it</trd>
  <trd lng="de">&uuml;berdachen</trd>
  <trd lng="fr">couvrir <klr>(une maison)</klr></trd>
  <trd lng="hu">(tet&odblac;vel) <ind>fed</ind></trd>
  <trdgrp lng="pl">
     <trd>zadasza&cacute;</trd>,
     <trd>pokrywa&cacute; <ind>dachem</ind></trd>
   </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol; 
<ind>&c_k;&c_r;&c_y;&c_sh;&c_je;&c_j;</ind></trd>
  <trd lng="sk">zastre&scaron;i&tcaron;</trd>
</drv>

<drv mrk="tegmen.sub0o">
   <kap><ofc>*</ofc>sub<tld/>o</kap>
   <snc>
     <dif>
       Loko a&ubreve; &ccirc;ambro ku&scirc;anta senpere sub la <tld/>o
     </dif>
     <refgrp tip="sin">
       <ref cel="mansar.0o">mansardo</ref>.
     </refgrp>
   </snc>
  <trdgrp lng="be">
     <trd>&c_g;&c_a;&c_r;&c_a;</trd>,
     <trd>&c_g;&c_a;&c_r;&c_y;&c_sh;&c_ch;&c_a;</trd>,
     <trd>&c_p;&c_a;&c_d;&c_d;&c_a;&c_sh;&c_a;&c_k;</trd>
   </trdgrp>
  <trd lng="ca">&agrave;tic</trd>
  <trdgrp lng="cs">
    <trd>podkrov&iacute;</trd>,
    <trd>p&uring;da</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Dachboden</trd>,
     <trd>Speicher</trd>,
     <trd>Mansarde</trd>
   </trdgrp>
  <trd lng="en">attic</trd>
  <trd lng="es">&aacute;tico</trd>
  <trdgrp lng="fr">
     <trd>grenier</trd>,
     <trd>soupente</trd>,
     <trd>combles</trd>,
     <trd>galetas</trd>
   </trdgrp>
  <trdgrp lng="hu">
    <trd>padl&aacute;s</trd>,
    <trd>tet&odblac;t&eacute;r</trd>
   </trdgrp>
  <trd lng="nl">zolder</trd>
  <trdgrp lng="pl">
     <trd>poddasze</trd>,
     <trd>strych</trd>
   </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_ch;&c_je;&c_r;&c_d;&c_a;&c_k;</trd>,
     <trd>&c_m;&c_a;&c_n;&c_s;&c_a;&c_r;&c_d;&c_a;</trd>
   </trdgrp>
  <trdgrp lng="sk">
    <trd>manzarda</trd>,
    <trd>podkrovie</trd>,
    <trd>podstre&scaron;ie</trd>,
    <trd>povala</trd>,
    <trd>p&ocirc;jd</trd>
  </trdgrp>
  <trd lng="sv"><klr>(hus-)</klr>vind</trd>
</drv>

<drv mrk="tegmen.0isto">
   <kap><tld/>isto</kap>
   <snc>
   <uzo tip="fak">KON</uzo>
     <dif>
      Homo, kiu konstruas <tld/>on.
      <ekz>
        La alia estas <tld/>isto, kiu riparis nian ferian domon kaj
        poste diris, ke nun neniu muso plu povos eniri kaj turmenti nin.
        <fnt>
          <aut>Stefan MAUL</aut>,
          <vrk>
            <url ref="https://www.monato.be/2003/008208.html">Monato, 
            oktobro 2003</url>
          </vrk>,
          <lok>El mia ridpunkto. Du ne mensogis</lok>
       </fnt>;
      </ekz>
      <ekz>
        <tld lit="T"/>iston ni konsideru kiel fakulon, kiu kovras
        tegmenton per iu materialo protektanta domon kontra&ubreve;
        pluvakvo, forkondukanta la akvon eksteren a&ubreve; al defluiga
        sistemo
        <fnt>
          <aut>Jan Werner</aut>,
          <vrk>
        <url ref="http://fabien.galerio.org/kofo/cirkuleroj/kofo12.pdf">
            Konstru-forumo nr 12</url>
          </vrk>
          <lok>septembro 2008; La nova temo 20-a &ndash; ladisto,
          zinkisto, tubisto</lok>
       </fnt>.
      </ekz>
     </dif>
   </snc>
   <ref tip="sin" cel="tegol.0isto">tegolisto</ref>
   <trd lng="en">roofer</trd>
   <trd lng="fr">couvreur</trd>
   <trdgrp lng="pl">
     <trd>dekarz</trd>
   </trdgrp>
  <trd lng="sk">pokr&yacute;va&ccaron; strechy</trd>
  <trd lng="cs">pokr&yacute;va&ccaron;</trd>
</drv>
</art>
