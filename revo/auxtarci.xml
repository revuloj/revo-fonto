<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>a&ubreve;tarci</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="auxtarci.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="auxtarci.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <ref tip="dif" cel="suficx.mem0o.EKON">memsufi&ccirc;o</ref>
    <ekz>
      entreprenistoj <klr>...</klr> volus liberi&gcirc;i de
      la burokratiaj liga&jcirc;oj kaj de la naciaj <tld/>oj,
      kiuj malhelpas &ccirc;iun ilian movi&gcirc;on
      <fnt>
        <aut>A. Spinelli, E. Rossi, trad. U. Broccatelli</aut>,
        <vrk><url
        ref="http://www.altierospinelli.org/manifesto/eo/html/manifesto1944eo.html">
        La Manifesto de Ventotene</url></vrk>,
        <lok>[2007]</lok>
      </fnt>.
    </ekz>
    <refgrp tip="vid">
      <ref cel="auxtarki.0o">a&ubreve;tarkio</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Autarkie</trd>,
      <trd>Selbstversorgung</trd>,
      <trd>&ouml;konomische Unabh&auml;ngigkeit</trd>,
      <trd>wirtschaftliche <ind>Unabh&auml;ngigkeit</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>economic <ind>autarky</ind></trd>,
      <trd>economic <ind>autarchy</ind></trd>
    </trdgrp>
    <trd lng="es">autarqu&iacute;a</trd>
    <trd lng="fr">autarcie</trd>
    <trdgrp lng="hu">
      <trd>gazdas&aacute;gi <ind>&ouml;nell&aacute;t&aacute;s</ind></trd>,
      <trd>autarkia</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd><ind>autarchia</ind> economica</trd>,
      <trd><ind>economia</ind> chiusa</trd>
    </trdgrp>
    <trd lng="ru">&c_a;&c_v;&c_t;&c_a;&c_r;&c_k;&c_i;&c_ja;</trd>
  </snc>
</drv>

<drv mrk="auxtarci.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="auxtarci.0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Memsufi&ccirc;a.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>autark</trd>,
    <trd>wirtschaftlich <ind>unabh&auml;ngig</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>autarkic(al)</trd>,
    <trd>autarchic(al)</trd>
  </trdgrp>
  <trd lng="es">aut&aacute;rquico</trd>
  <trd lng="fr">autarcique</trd>
  <trdgrp lng="hu">
    <trd>&ouml;nell&aacute;t&oacute;</trd>,
    <trd>autark</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_v;&c_t;&c_a;&c_r;&c_k;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_v;&c_t;&c_a;&c_r;&c_k;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
