<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: auxtarci.xml,v 1.17 2021/03/04 18:15:49 revo Exp $">
<kap>
  <rad>a&ubreve;tarci</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="auxtarci.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="auxtarci.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="dif" cel="suficx.mem0o.EKON">Memsufi&ccirc;o</ref>:
      <ekz>
        entreprenistoj <klr>[&#x2026;]</klr> volus liberi&gcirc;i de
        la burokratiaj liga&jcirc;oj kaj de la naciaj <tld/>oj,
        kiuj malhelpas &ccirc;iun ilian movi&gcirc;on
        <fnt>
          <aut>A. Spinelli, E. Rossi, trad. U. Broccatelli</aut>,
          <vrk><url ref="http://www.altierospinelli.org/manifesto/eo/html/manifesto1944eo.html">
          La Manifesto de Ventotene</url></vrk>,
          <lok>[2007]</lok>
        </fnt>;
      </ekz>
      <ekz>
        energi<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2012/010595.php">Novtipa ekologia turismo</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxtarki.0o">a&ubreve;tarkio</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Autarkie</trd>,
      <trd>Selbstversorgung</trd>,
      <trd>&ouml;konomische Unabh&auml;ngigkeit</trd>,
      <trd>wirtschaftliche <ind>Unabh&auml;ngigkeit</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>economic <ind>autarky</ind></trd>,
      <trd>economic <ind>autarchy</ind></trd>
    </trdgrp>
    <trd lng="es">autarqu&iacute;a</trd>
    <trd lng="fr">autarcie</trd>
    <trdgrp lng="hu">
      <trd>gazdas&aacute;gi <ind>&ouml;nell&aacute;t&aacute;s</ind></trd>,
      <trd>autarkia</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd><ind>autarchia</ind> economica</trd>,
      <trd><ind>economia</ind> chiusa</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>autarkia</trd>,
      <trd>samowystarczalno&sacute;&cacute; <klr>ekonomiczna</klr></trd>
    </trdgrp>
    <trd lng="ru">&c_a;&c_v;&c_t;&c_a;&c_r;&c_k;&c_i;&c_ja;</trd>
  </snc>
  <trd lng="he">&#x5E2;&#x5E6;&#x5DE;&#x5D0;&#x5D5;&#x5EA; &#x5DB;&#x5DC;&#x5DB;&#x5DC;&#x5D9;&#x5EA;</trd>
</drv>

<drv mrk="auxtarci.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="auxtarci.0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Memsufi&ccirc;a:
      <ekz>
        Albanio forlasis la centralizitan, izolitan kaj <tld/>an ekonomian sistemon de
        la &leftquot;reala socialismo&#x201D;
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2005/008378.php">Venkis mal&scirc;paremo kaj koruptado</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>G&uuml;ssing</nom> estas energie <tld/>a kaj e&ccirc; eksportas energion
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="https://www.monato.be/2012/010595.php">Novtipa ekologia turismo</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>autark</trd>,
    <trd>wirtschaftlich <ind>unabh&auml;ngig</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>autarkic(al)</trd>,
    <trd>autarchic(al)</trd>
  </trdgrp>
  <trd lng="es">aut&aacute;rquico</trd>
  <trd lng="fr">autarcique</trd>
  <trdgrp lng="hu">
    <trd>&ouml;nell&aacute;t&oacute;</trd>,
    <trd>autark</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>autarkiczny</trd>,
    <trd>samowystarczalny <klr>ekonomicznie</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_v;&c_t;&c_a;&c_r;&c_k;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_a;&c_v;&c_t;&c_a;&c_r;&c_k;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
