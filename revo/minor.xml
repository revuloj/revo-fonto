<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>minor</rad>/a
</kap>
<drv mrk="minor.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <uzo tip="klr">(pri gamo)</uzo>
    <dif>
      Konsistanta el tri tonoj, tri duontonoj kaj unu kaj duona tonoj.
    </dif>
  </snc>
  <snc>
    <uzo tip="klr">(p.p. tercio)</uzo>
    <dif>
      Konsistanta el unu duontono.
    </dif>
    <refgrp tip="vid">
      <ref cel="majxor">ma&jcirc;ora</ref>.
    </refgrp>
  </snc>
  <trd lng="nl">mineur</trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="minor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatVort</bib></fnt>
  <snc mrk="minor.0o.matrico">
    <dif>
      <klr>(de elemento a<sub>ij</sub> en
      <ref cel="matric.n_p0o">(n,n)-matrico</ref> A)</klr>
      <ref cel="determ.0anto.matrico">Determinanto</ref> de la
      submatrico de A, rezultanta el forigo de la i-a horizontalo kaj
      de la j-a vertikalo.
    </dif>
    <ref tip="vid" cel="kofakt.0o">kofaktoro.</ref>
  </snc>
  <trd lng="de">Minor<klr tip="ind"> (Unterdeterminante)</klr></trd>
  <trd lng="en">minor<klr tip="ind"> (subdeterminant)</klr></trd>
  <trd lng="fr">mineur<klr tip="ind"> (sous-d&eacute;terminant)</klr></trd>
  <trd lng="ru">&c_m;&c_i;&c_n;&c_o;&c_r;<klr tip="ind"> (&c_s;&c_u;&c_b;&c_d;&c_je;&c_t;&c_je;&c_r;&c_m;&c_i;&c_n;&c_a;&c_n;&c_t;)</klr></trd>
</drv>
</art>
