<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kuf</rad>/o
</kap>
<drv mrk="kuf.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Simpla <ref tip="super" cel="vest.kap0o">kapvesto</ref> de virino.
    </dif>
    <trdgrp lng="en">
      <trd>coif</trd>,
      <trd>bonnet</trd>,
      <trd>wimple</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>coiffe</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_je;&c_p;&c_je;&c_c;</trd>,
      <trd>&c_ch;&c_je;&c_p;&c_ch;&c_i;&c_k;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
