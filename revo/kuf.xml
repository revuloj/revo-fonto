<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kuf</rad>/o
</kap>
<drv mrk="kuf.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kuf.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Simpla &scirc;tofa <ref tip="super" cel="vest.kap0o">kapvesto</ref> de
      virino:
      <ekz>
        alia angla esprimo signifas <ctl>Li havas abelon en la
        <tld/>o</ctl>
        &ndash; unu obsedon
        <fnt>
        <aut>Marjorie Boulton</aut>,
        <vrk>Vesproj por vespertoj</vrk>,
        <bib>Monato</bib>,
        <lok>jaro 1997a, numero 8a, p. 30a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;iaj densaj haroj estis ordigitaj sub la nokta <tld/>o kun
        rande
        alkudrita punto
        <fnt><bib>Mary</bib></fnt>;
      </ekz>
      <ekz>
        <ind>Ru&gcirc;<tld/>ulineto</ind>.
        <trd lng="de">Rotk&auml;ppchen</trd>
        <trd lng="fr">Petit Chaperon rouge</trd>
        <trd lng="nl">Roodkapje</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_a;&c_p;&c_t;&c_u;&c_r;</trd>
    <trdgrp lng="ca">
      <trd>c&ograve;fia</trd>
    </trdgrp>
    <trd lng="de">Haube</trd>
    <trdgrp lng="en">
      <trd>coif</trd>,
      <trd>bonnet</trd>,
      <trd>wimple</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>cofia</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>coiffe</trd>
    </trdgrp>
    <trd lng="hu">f&odblac;k&ouml;t&odblac;</trd>
    <trdgrp lng="nl">
      <trd>kap<klr tip="ind"> (hoofddeksel)</klr></trd>,
      <trd>muts</trd>
    </trdgrp>
    <trd lng="pt">touca</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_je;&c_p;&c_je;&c_c;</trd>,
      <trd>&c_ch;&c_je;&c_p;&c_ch;&c_i;&c_k;</trd>
    </trdgrp>
  </snc>
</drv>
</art>
