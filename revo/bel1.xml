<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bel1.xml,v 1.5 2021/03/04 20:23:07 revo Exp $">
<kap>
    <rad>bel</rad>/o <fnt><bib>PIV3</bib></fnt>
</kap>

<drv mrk="bel1.0o">
  <kap><tld/>o</kap>
  <snc>
  <dif>
  Dek <ref tip="vid" cel="bel1.0odeci">deci<tld/>oj</ref><klr>
  (deci<tld/>o estas la pli ofte uzata unuo)</klr>.
  </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_l;</trd>
  <trd lng="en">bel</trd>
  <trd lng="fr">bel</trd>
  <trd lng="he">&#x5D9;&#x5D5;&#x5E4;&#x5D9;</trd>
  <trd lng="pl">bel</trd>
</drv>

<drv mrk="bel1.0odeci">
  <kap>deci<tld/>o</kap>
  <mlg>dB</mlg>
  <snc mrk="bel1.0odeci.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Mezurunuo de sonintenso:
    </dif>
    <ekz>
    la intenso de meza homa vo&ccirc;o estas de &ccirc;irka&ubreve; 55 dB.
    </ekz>
  </snc>
  
  <snc mrk="bel1.0odeci.TEL">
  <uzo tip="fak">TEL</uzo>
  <uzo tip="fak">ELE</uzo>
  <dif>
  Mezurunuo de atenuo a&ubreve; amplifo de tensio, kurento a&ubreve; povumo.
  </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_c;&c_y;&c_b;&c_e;&c_l;</trd>
  <trd lng="en">decibel</trd>
  <trdgrp lng="fr">
    <trd>decibel</trd>,
    <trd>dB</trd>
  </trdgrp>
  <trd lng="he">&#x5D3;&#x5B5;&#x5E6;&#x5B4;&#x5D9;&#x5D1;&#x5BC;&#x5B5;&#x5DC;</trd>
  <trd lng="pl">decybel</trd>
  <trd lng="ro">decibel</trd>
</drv>
</art>

