   <trdgrp lng="de">
     <trd>Schaufel</trd>,
     <trd>Schippe</trd>
   </trdgrp>
   <trd lng="fr">pelle</trd>
   <trd lng="hu">lap&aacute;t</trd>
   <trdgrp lng="ja">
    <trd>&#x30B7;&#x30E3;&#x30D9;&#x30EB;</trd>,
    <trd>&#x30B9;&#x30B3;&#x30C3;&#x30D7;</trd>
  </trdgrp>
  <trd lng="pt">p&aacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_o;&c_p;&c_a;&c_t;&c_a; (&c_s;&c_o;&c_v;&c_k;&c_o;&c_v;&c_a;)</trd>,
    <trd>&c_s;&c_o;&c_v;&c_o;&c_k;</trd>,
    <trd>&c_ch;&c_je;&c_r;&c_p;&c_a;&c_k;</trd>,
    <trd>&c_k;&c_ib;&c_v;&c_sh; (&c_je;&c_k;&c_s;&c_k;&c_a;&c_v;&c_a;&c_t;&c_o;&c_r;&c_a;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x68A9; <pr>l&iacute;</pr></trd>,
    <trd>&#x9341; <pr>xi&amacron;n</pr></trd>,
    <trd>&#x9528; <pr>xi&amacron;n</pr></trd>,
    <trd>&#x9364; <pr>ch&amacron;</pr></trd>,
    <trd>&#x9538; <pr>ch&amacron;</pr></trd>,
    <trd>&#x93DF; <pr>ch&#x1CE;n</pr></trd>,
    <trd>&#x94F2; <pr>ch&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxovel.pan0ilo">
  <kap>pan<tld/>ilo</kap>
  <snc mrk="sxovel.pan0ilo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Longtenila <tld/>ilo, per kiu oni en&scirc;ovas kaj
      el&scirc;ovas baka&jcirc;ojn el granda kuirforno:
      <ekz>
        li &ccirc;e &scirc;tipo &ccirc;arpentis pan<tld/>ilon
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
    <trd lng="be">&c_l;&c_a;&c_p;&c_a;&c_t;&c_a; <klr>(&c_p;&c_je;&c_k;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
    <trd lng="cs">peka&rcaron;sk&aacute; lopata</trd>
    <trdgrp lng="de">
      <trd>Brotschaufel</trd>,
      <trd>Brotschieber</trd>,
      <trd>Backschieber</trd>
    </trdgrp>
    <trd lng="es">pala de panadero</trd>
    <trd lng="fr">pelle de boulanger</trd>
    <trd lng="id">sekop roti</trd>
    <trd lng="pl">&lstroke;opata do chleba</trd>
</drv>
</art>
