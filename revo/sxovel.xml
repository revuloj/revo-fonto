<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxovel.xml,v 1.29 2024/03/25 07:28:39 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>&scirc;ovel</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxovel.0i">
   <kap><ofc>*</ofc><tld/>i</kap>
   <gra><vspec>tr</vspec></gra>
   <snc mrk="sxovel.0i.KOMUNE">
     <dif>
       Forpreni kaj trans&scirc;uti ian materion per <tld/>ilo a&ubreve;
       per plata instrumento a&ubreve; objekto simila al <tld/>ilo:
       <ekz>
         estis <tld/>ita malgranda irvojeto<fnt>Z</fnt>;
       </ekz>
       <ekz>
         kun<tld/>i per la piedo velkintajn foliojn<fnt>Z</fnt>;
       </ekz>
       <ekz>
         <tld/>kvanto da tero<fnt>Z</fnt>;
       </ekz>
       <ekz><uzo tip="stl">FIG</uzo>
         <tld/>i per la piedo<fnt>Z</fnt>
         <klr>(tergrati <sup>Z</sup>)</klr>.
       </ekz>
     </dif>
   </snc>
   <trd lng="be">&c_z;&c_g;&c_r;&c_a;&c_b;&c_a;&c_c;&c_mol;</trd>
   <trd lng="bg">&c_k;&c_o;&c_p;&c_a;&c_ja;</trd>
   <trd lng="cs">h&aacute;zet lopatou</trd>
   <trdgrp lng="de">
     <trd>schaufeln</trd>,
     <trd>schippen</trd>
   </trdgrp>
   <trd lng="en">shovel</trd>
   <trdgrp lng="es">
     <trd>remover con la paleta</trd>,
     <trd>palear</trd>
   </trdgrp>
   <trdgrp lng="fa">
     <trd>&a_b;&f_y;&a_l; &a_z;&a_d;&a_n;</trd>,
     <trd>&a_b;&a_A;
     <ind>&a_kh;&a_A;&f_k;&#x200C;&a_A;&a_n;&a_d;&a_A;&a_z;</ind>
     &a_b;&a_r;&a_d;&a_A;&a_sh;&a_t;&a_n;</trd>
   </trdgrp>
   <trdgrp lng="fr"><trd>pelleter</trd><trd>creuser &agrave; la
<ind>pelle</ind></trd></trdgrp>
   <trd lng="hu">lap&aacute;tol</trd>
   <trdgrp lng="it">
     <trd>palare</trd>,
     <trd>spalare</trd>
   </trdgrp>
   <trdgrp lng="ja">
    <trd>&#x3059;&#x304F;&#x3046;</trd>,
    <trd>&#x3059;&#x304F;&#x3044;&#x53D6;&#x308B;  <pr>&#x3059;&#x304F;&#x3044;&#x3068;&#x308B;</pr></trd>,
    <trd>&#x304B;&#x304F;</trd>
  </trdgrp>
  <trd lng="nl">scheppen<klr tip="ind"> (met schop)</klr></trd>
  <trd lng="pl">szuflowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>remover</trd>,
    <trd>apanhar com <ind>p&aacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="ru">
     <trd>&c_s;&c_g;&c_r;&c_je;&c_b;&c_a;&c_t;&c_mol; <klr>(&c_l;&c_o;&c_p;&c_a;&c_t;&c_o;&c_j;)</klr></trd>,
     <trd>&c_z;&c_a;&c_ch;&c_je;&c_r;&c_p;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
   </trdgrp>
   <trdgrp lng="uk">
    <trd>&c_z;&c_g;&c_r;&c_ib;&c_b;&c_a;&c_t;&c_i; (&#x43B;&#x43E;&#x43F;&c_a;&c_t;&#x43E;&#x44E;)</trd>,
    <trd>&c_z;&c_b;&c_i;&c_r;&c_a;&c_t;&c_i; (&c_s;&#x43C;&c_ib;&c_t;&c_t;&#x44F;</trd>,
    <trd>&c_z;&c_je;&#x43C;&#x43B;&#x44E;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x93DF; <pr>ch&#x1CE;n</pr></trd>,
    <trd>&#x94F2; <pr>ch&#x1CE;n</pr></trd>,
    <trd>&#x6774; <pr>xi&#x101;n</pr></trd>,
    <trd>&#x9528; <pr>xi&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxovel.0ilo">
   <kap><tld/>ilo</kap>
   <snc mrk="sxovel.0ilo.longa">
     <dif>
       Ilo konsistanta el lar&gcirc;a, iom konkava, ordinare
       metala, plato kun longa tenilo.
     </dif>
     <trd lng="be">&c_sh;&c_u;&c_f;&c_je;&c_l;&c_mol;</trd>
     <trd lng="bg">&c_l;&c_o;&c_p;&c_a;&c_t;&c_a;</trd>
     <trd lng="cs">lopata</trd>
     <trd lng="en">shovel</trd>
     <trd lng="es">pala</trd>
     <trd lng="fa">&a_b;&f_y;&a_l;</trd>
     <trdgrp lng="it">
       <trd>pala</trd>,
       <trd>badile</trd>
     </trdgrp>
     <trd lng="nl">schop</trd>
     <trdgrp lng="pl">
       <trd>szufla</trd>,
       <trd>&lstroke;opata</trd>
     </trdgrp>
     <trd lng="ru">&c_s;&c_o;&c_v;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_l;&c_o;&c_p;&c_a;&c_t;&c_a;</trd>
   </snc>
   <snc mrk="sxovel.0ilo.mallonga">
     <dif>
       Samspeca ilo kun mallonga tenilo por hejmaj uzoj.
     </dif>
     <trd lng="be">&c_sh;&c_u;&c_f;&c_l;&c_ib;&c_k;</trd>
     <trd lng="bg">&c_l;&c_o;&c_p;&c_a;&c_t;&c_k;&c_a;</trd>
     <trd lng="cs">lopatka</trd>
     <trdgrp lng="de">
       <trd>Schaufel</trd>,
       <trd>Schippe</trd>
     </trdgrp>
     <trdgrp lng="en">
       <trd>scoop</trd>,
       <trd>dustpan<klr tip="amb"> (por polvo)</klr></trd>
     </trdgrp>
     <trdgrp lng="es">
       <trd>pala<klr> corta</klr></trd>,
       <trd>paleta</trd>
     </trdgrp>
     <trdgrp lng="fa">
       <trd>&a_kh;&a_A;&f_k;&#x200C;&a_A;&a_n;&a_d;&a_A;&a_z;</trd>,
       <trd>&a_b;&f_y;&a_l;&f_ch;&a_h;</trd>
     </trdgrp>
     <trd lng="it">paletta</trd>
     <trd lng="nl">stofblik</trd>
     <trd lng="pl">&lstroke;opatka</trd>
     <trd lng="pt">pazinha<klr> (de lixo)</klr></trd>
     <trd lng="ru">&c_s;&c_o;&c_v;&c_o;&c_k;</trd>
   </snc>
   <trd lng="fr">pelle</trd>
   <trd lng="hu">lap&aacute;t</trd>
   <trdgrp lng="ja">
    <trd>&#x30B7;&#x30E3;&#x30D9;&#x30EB;</trd>,
    <trd>&#x30B9;&#x30B3;&#x30C3;&#x30D7;</trd>
  </trdgrp>
  <trd lng="pt">p&aacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x43E;&#x43F;&c_a;&c_t;&c_a; (&c_s;&#x43E;&c_v;&#x43A;&#x43E;&c_v;&c_a;)</trd>,
    <trd>&c_s;&#x43E;&c_v;&#x43E;&#x43A;</trd>,
    <trd>&c_ch;&c_je;&c_r;&#x43F;&c_a;&#x43A;</trd>,
    <trd>&#x43A;&c_ib;&c_v;&c_sh; (&c_je;&#x43A;&c_s;&#x43A;&c_a;&c_v;&c_a;&c_t;&#x43E;&c_r;&c_a;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x68A9; <pr>l&#xED;</pr></trd>,
    <trd>&#x9341; <pr>xi&#x101;n</pr></trd>,
    <trd>&#x9528; <pr>xi&#x101;n</pr></trd>,
    <trd>&#x9364; <pr>ch&#x101;</pr></trd>,
    <trd>&#x9538; <pr>ch&#x101;</pr></trd>,
    <trd>&#x93DF; <pr>ch&#x1CE;n</pr></trd>,
    <trd>&#x94F2; <pr>ch&#x1CE;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="sxovel.pan0ilo">
  <kap>pan<tld/>ilo</kap>
  <snc mrk="sxovel.pan0ilo.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Longtenila <tld/>ilo, per kiu oni en&scirc;ovas kaj
      el&scirc;ovas baka&jcirc;ojn el granda kuirforno:
      <ekz>
        li &ccirc;e &scirc;tipo &ccirc;arpentis pan<tld/>ilon
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
  </snc>
    <trd lng="be">&c_l;&c_a;&c_p;&c_a;&c_t;&c_a; <klr>(&c_p;&c_je;&c_k;&c_a;&c_r;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>
    <trd lng="cs">peka&rcaron;sk&aacute; lopata</trd>
    <trd lng="es">pala de panadero</trd>
    <trd lng="fr">pelle de boulanger</trd>
    <trd lng="pl">&lstroke;opata do chleba</trd>
</drv>
</art>
