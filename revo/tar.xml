<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tar</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="tar.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Pezo de &ccirc;iuspeca envolvo kiel kesto, papero, sako, kiu enhavas la
      varon.
    </dif>
  </snc>
  <trd lng="de">Tara</trd>
  <trd lng="nl">tarra</trd>
  <trd lng="pl">tara</trd>
  <trd lng="ru">&c_t;&c_a;&c_r;&c_a;</trd>
</drv>

<drv mrk="tar.kun0a">
  <kap>kun<tld/>a</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Kalkulita kun la <tld/>o. Brutta [Z].
    </dif>
  </snc>
  <trd lng="de">brutto</trd>
  <trd lng="nl">bruto</trd>
  <trd lng="ru">&c_b;&c_r;&c_u;&c_t;&c_t;&c_o;</trd>
</drv>

<drv mrk="tar.sen0a">
  <kap>sen<tld/>a</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Kalkulita sen la <tld/>o. Neta [PIV]. Netta [Z].
    </dif>
  </snc>
  <trd lng="de">netto</trd>
  <trd lng="nl">netto</trd>
  <trd lng="ru">&c_n;&c_je;&c_t;&c_t;&c_o;</trd>
</drv>
</art>
