    <trd>dienstbeflissen</trd>,
    <trd>hilfsbereit</trd>
  </trdgrp>
  <trd lng="fr">serviable</trd>
  <trdgrp lng="nl">
    <trd>hulpvaardig</trd>,
    <trd>dienstvaardig</trd>
  </trdgrp>
  <trd lng="pl">us&lstroke;u&zdot;ny</trd>
  <trd lng="ro">dispus</trd>
  <trd lng="sk">k slu&zcaron;b&aacute;m</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_s;&c_l;&c_u;&c_zh;&c_l;&c_i;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_t;&c_o;&c_v;&c_i;&c_j; &c_d;&c_o; &c_p;&c_o;&c_s;&c_l;&c_u;&c_g;</trd>,
    <trd>&c_g;&c_o;&c_t;&c_o;&c_v;&c_i;&c_j; &c_d;&c_o; &c_s;&c_l;&c_u;&c_zh;&c_b;&c_i;</trd>
  </trdgrp>
</drv>
