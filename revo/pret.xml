    <trd>dienstvaardig</trd>
  </trdgrp>
  <trd lng="pl">us&lstroke;u&zdot;ny</trd>
  <trd lng="ro">dispus</trd>
  <trd lng="sk">k slu&zcaron;b&aacute;m</trd>
  <trdgrp lng="uk">
