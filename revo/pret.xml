<drv mrk="pret.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="pret.0eco.KOMUNE">
    <dif>
      Eco de io a&ubreve; iu <tld/>a:
      <ekz>
        mi timas, ke tia arma <tld/>eco povas ofendi la noblan Sargonon
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro 8a</lok>
        </fnt>;
      </ekz>
      <ekz>
        efektiva <tld/>eco por sin oferi
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi vin a&ubreve;skultos kun <tld/>eco por &ccirc;io
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        tiun &ccirc;i sorton &scirc;i akceptis kun <klr>[&#x2026;]</klr> plena kaj decida <tld/>eco
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>eco pionire oferi sian tempon
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        esprimi sian <tld/>econ helpi
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>22</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>eco al batalo kaj kontra&ubreve;staro
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro 51a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_t;&c_o;&c_w;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
