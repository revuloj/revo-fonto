<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>fe&ubreve;d</rad>/o <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="feuxd.0o">
  <kap><tld/>o</kap>
  <snc mrk="feuxd.0o.HIS">
    <uzo tip="fak">EKON</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Mezepoka nobela bieno donita de supera sinjoro al
      dependulo la&ubreve; difinitaj kondi&ccirc;oj:
      <ekz>
        <ind><tld/>a</ind> bieno, sinjoro, re&gcirc;imo.
        <trd lng="en">feudal</trd>
        <trd lng="fr">f&eacute;odal</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Lehen</trd>
  <trdgrp lng="en">
    <trd>feud</trd>,
    <trd>fief</trd>
  </trdgrp>
  <trd lng="es">feudo</trd>
  <trd lng="fr">fief</trd>
  <trdgrp lng="hu">
    <trd>h&udblac;b&eacute;rbirtok</trd>,
    <trd>feudum</trd>
  </trdgrp>
  <trd lng="pt">feudo</trd>
  <trdgrp lng="ru">
    <trd>&c_l;&c_je;&c_n;</trd>,
    <trd>&c_l;&c_je;&c_n;&c_n;&c_o;&c_je; &c_v;&c_l;&c_a;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_f;&c_je;&c_o;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="feuxd.0estro">
  <kap><tld/>estro, <var><kap><tld/>o&ccirc;efo</kap></var></kap>
  <snc mrk="feuxd.0estro.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Supera sinjoro, rilate al fe&ubreve;dulo;
      <ref tip="sin" cel="suzeren.0o.HIS">suzereno</ref>. 
    </dif>
  </snc>
  <trd lng="de">Lehnsherr</trd>
  <trd lng="en">feudal lord</trd>
  <trd lng="es">se&ntilde;or <ind>feudal</ind></trd>
  <trd lng="fr">suzerain</trd>
  <trd lng="hu">h&udblac;b&eacute;r&uacute;r</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_je;&c_o;&c_d;&c_a;&c_l;</trd>,
    <trd>&c_s;&c_ju;&c_z;&c_je;&c_r;&c_je;&c_n;</trd>
  </trdgrp>
</drv>

<drv mrk="feuxd.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="feuxd.0ismo.HIS">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <tld lit="F"/>a politika kaj socia sistemo.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Lehnswesen</trd>,
     <trd>Feudalismus</trd></trdgrp>
  <trd lng="en">feudalism</trd>
  <trd lng="es">feudalismo</trd>
  <trdgrp lng="fr">
    <trd>f&eacute;odalit&eacute;<klr tip="ind"> (r&eacute;gime)</klr></trd>,
    <trd>f&eacute;odalisme<klr tip="ind"> (r&eacute;gime)</klr></trd></trdgrp> 
  <trdgrp lng="hu">
     <trd>feudalizmus</trd>,
     <trd>h&udblac;b&eacute;ris&eacute;g</trd></trdgrp>
  <trd lng="ru">&c_f;&c_je;&c_o;&c_d;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="feuxd.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="feuxd.0ulo.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Tiu, kiu dependas de superulo a&ubreve; regnestro pro ricevita
      <tld/>o;
      <ref tip="sin" cel="vasal.0o.HIS">vasalo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_s;&c_a;&c_l;</trd>
  <trd lng="br">gwaz<klr tip="amb"> (dindan un aotrou)</klr></trd>
  <trd lng="ca">vassall</trd>
  <trdgrp lng="de">
    <trd>Vasall</trd>,
    <trd>Lehnsmann</trd>
  </trdgrp>
  <trd lng="el">&ypsilon;&pi;&omikron;&tau;&epsilon;&lambda;&eta_ton;&sigma_fina;</trd>
  <trd lng="en">vassal</trd>
  <trdgrp lng="fa">
    <trd>&a_w;&a_A;&a_s;&a_A;&a_l;</trd>,
    <trd>&a_kh;&a_r;&a_A;&a_j;&f_g;&a_z;&a_A;&a_r;</trd>
  </trdgrp>
  <trd lng="fr">vassal</trd>
  <trdgrp lng="hu">
    <trd>h&udblac;b&eacute;res</trd>,
    <trd>vazallus</trd>
  </trdgrp>
  <trd lng="nl">vazal</trd>
  <trd lng="pt">vassalo</trd>
  <trd lng="ru">&c_v;&c_a;&c_s;&c_s;&c_a;&c_l;</trd>
</drv>
</art>
