<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>staniol</rad>/o
</kap>
<drv mrk="stanio.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Alojo el stano kaj aluminio, uzata en folioj por envolvi &ccirc;okoladon,
      bombonojn ks (= stanfolio).
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Stanniol</trd>,
    <trd>Silberpapier</trd>
  </trdgrp>
  <trd lng="en">tin foil</trd>
  <trdgrp lng="nl">
    <trd>staniol</trd>,
    <trd>zilverpapier</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_a;&c_n;&c_i;&c_o;&c_l;&c_mol;</trd>,
    <trd>&c_f;&c_o;&c_l;&c_mol;&c_g;&c_a;</trd>
  </trdgrp>
</drv>
</art>
