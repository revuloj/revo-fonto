<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: net.xml,v 1.30 2025/11/25 08:40:31 revo Exp $">
  <kap>
    <ofc>*</ofc>
    <rad>net</rad>/a
    <fnt><bib>UV</bib></fnt>
  </kap>

<drv mrk="net.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="net.0a.klara">
    <dif>
      Nekonfuza kaj preciza:
      <ekz>
        <tld/>a skriba&jcirc;o, desegno, kli&scirc;a&jcirc;o;
      </ekz>
      <ekz>
        <tld/>aj konturoj;
      </ekz>
      <ekz>
        <klr>[tion]</klr> li diris per metala vo&ccirc;o kun <tld/>a, preska&ubreve; malnature
        <tld/>a artikado
        <fnt>
          <bib>ChV</bib>
          <lok>8</lok>
        </fnt>;
      </ekz>
      <ekz>
        por riveli <tld/>an veron necesas kribri la respektivajn faktojn
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Klariga aldono</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru <tld/>an kopion de &scirc;ia manuskripto
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Oka</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i devos a&ccirc;eti &leftquot;fran&gcirc;-reton&rightquot; (retharligilon) por konservi <tld/>aj la harojn,
        &ccirc;ar la vento blovis tre multe
        <fnt>
          <bib>TAA</bib>
          <lok>&Ccirc;apitro VIII.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="klar.0a">klara</ref>,
      <ref cel="pur.0a.neniu_fremda">pura<sncref/></ref>
    </refgrp>
    <trd lng="be">&c_ch;&c_y;&c_s;&c_t;&c_y;</trd>
    <trd lng="de">rein</trd>
    <trd lng="en">neat</trd>
    <trdgrp lng="hu">
      <trd>tiszta</trd>,
      <trd>letiszt&aacute;zott</trd>,
      <trd>tiszt&aacute;zati</trd>,
      <trd>nett</trd>
    </trdgrp>
    <trd lng="nl">net</trd>
    <trdgrp lng="pl">
      <trd>czysty</trd>,
      <trd>na <ind>czysto</ind></trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>limpo</trd>,
      <trd>claro</trd>,
      <trd>distinto</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_i;&c_s;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>,
      <trd>&c_b;&c_je;&c_l;&c_o;&c_v;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="net.0a.sentara">
    <dif>
      <ref tip="sub" cel="tar.sen0a">Sentara</ref>,
      senigita je &ccirc;iu ajn kroma&jcirc;o, kalkulata la&ubreve;
      sia nura
      propra kvanto:
      <ekz>
        <ind><tld/>a pezo</ind>
        (sentara maso, pezo de la varo mem ne enkalkulante la
        &ccirc;irka&ubreve;volva&jcirc;on);
        <trd lng="be">&c_v;&c_a;&c_g;&c_a; &c_n;&c_e;&c_t;&c_a;</trd>
        <trd lng="hu">nett&oacute; s&uacute;ly</trd>
        <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_y;&c_j;
        &c_v;&c_je;&c_s;</trd>
      </ekz><ekz>
        <tld/>a gajno, <tld/>a profito
        <klr>(definitiva profito, kiu restas post depreno de
        elspezoj
          faritaj a&ubreve; de perdoj okazintaj)</klr>;
      </ekz>
      <ekz>
       la meza mal<tld/>a salajro kreskis je 17,6 % &gcirc;is 435 e&ubreve;roj kaj la <tld/>a je 19,6 % &gcirc;is 317
       e&ubreve;roj
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2007/009205.php">Problemo pro salajroj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ch;&c_y;&c_s;&c_t;&c_y;</trd>,
      <trd>&c_n;&c_e;&c_t;&c_a;</trd>
    </trdgrp>
    <trd lng="de">netto</trd>
    <trd lng="en">net</trd>
    <trd lng="es">neto</trd>
    <trd lng="fr">net</trd>
    <trd lng="hu">nett&oacute;</trd>
    <trd lng="nl">netto</trd>
    <trdgrp lng="pl">
      <trd>czysty</trd>,
      <trd>na <ind>czysto</ind></trd>,
      <trd>netto</trd>
    </trdgrp>
    <trd lng="pt">l&iacute;quido</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_t;&c_t;&c_o;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>hladk&yacute;</trd>,
    <trd>netto</trd>,
    <trd>opracovan&yacute;</trd>,
    <trd>za&ccaron;i&scaron;t&ecaron;n&yacute;</trd>,
    <trd>&ccaron;ist&yacute; <klr>(v&yacute;nos,v&aacute;ha)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6B63;&#x5473;&#x306E;  <pr>&#x3057;&#x3087;&#x3046;&#x307F;&#x306E;</pr></trd>,
    <trd>&#x304D;&#x308C;&#x3044;&#x306A;</trd>,
    <trd>&#x6574;&#x3063;&#x305F;  <pr>&#x3068;&#x3068;&#x306E;&#x3063;&#x305F;</pr></trd>,
    <trd>&#x6E05;&#x66F8;&#x306E;  <pr>&#x305B;&#x3044;&#x3057;&#x3087;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hladk&yacute;</trd>,
    <trd>netto</trd>,
    <trd>&ccaron;ist&yacute; <klr>(v&yacute;nos, v&aacute;ha)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_g;&c_o;&c_t;&c_o;&c_v;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_ib;&c_n;&c_ch;&c_je;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_i;&c_j; (&c_p;&c_r;&c_o; &c_v;&c_a;&c_g;&c_u;</trd>,
    <trd>&c_d;&c_o;&c_h;&c_o;&c_d; &c_ib; &c_t;.&c_p;.)</trd>
  </trdgrp>
</drv>

<drv mrk="net.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Definitiva klara skriba&jcirc;o, sen korektoj kaj sen makuloj,
      ordinare
      kopiita la&ubreve; alia prepara pli malpli malklara
      skriba&jcirc;o.
    </dif>
    <trd lng="be">&c_ch;&c_y;&c_s;&c_t;&c_a;&c_v;&c_ib;&c_k;</trd>
    <trdgrp lng="cs">
      <trd>v&aacute;ha netto</trd>,
      <trd>&ccaron;ist&aacute; v&aacute;ha</trd>
    </trdgrp>
    <trd lng="de">Reinschrift</trd>
    <trdgrp lng="en">
      <trd>fair copy</trd>,
      <trd>final copy</trd>
    </trdgrp>
    <trd lng="fr">propre (subst.)</trd>
    <trd lng="hu">tiszt&aacute;zat</trd>
    <trd lng="nl">net</trd>
    <trd lng="pl">czystopis</trd>
    <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_o;&c_v;&c_i;&c_k;</trd>
    <trd lng="sk">&ccaron;ist&aacute; v&aacute;ha</trd>
  </snc>
  <trd lng="ja">&#x6E05;&#x66F8;  <pr>&#x305B;&#x3044;&#x3057;&#x3087;</pr></trd>
  <trdgrp lng="uk">
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_o;&c_v;&c_i;&c_k;</trd>,
    <trd>&c_ch;&c_i;&c_s;&c_t;&c_a; &c_v;&c_a;&c_g;&c_a;</trd>,
    <trd>&c_n;&c_je;&c_t;&c_t;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="net.mal0o">
  <kap><ofc>*</ofc>mal<tld/>o <fnt><bib>F</bib>, <lok>Universala
  Vortaro, <url
  ref="&AdE;/fundamento/universala_vortaro.html#av-5122">mal'net'</url></lok></fnt></kap>
  <snc>
    <dif>
      Unua prova skriba&jcirc;o kun korektoj, plibonigoj a&ubreve;
      makuloj, la&ubreve; kiu oni poste transskribas la <tld/>on:
      <ekz>
        la paperoj konsistis el mal<tld/>oj de <klr>[&#x2026;]</klr> artikoloj kaj paroladoj
        <fnt>
          <bib>ChC</bib>
          <lok>13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la versio projekciita <klr>[&#x2026;]</klr> estis mal<tld/>o, kiu poste estis sufi&ccirc;e
        multe reprilaborita surbaze de la tiam ricevitaj kritikoj
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="skiz.0o">skizo</ref>,
      <ref cel="skelet.0o.FIG">skeleto<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="kanvas.0o.FIG">kanvaso<sncref/></ref>,
      <ref cel="koncep.0o">koncepto</ref>,
      <ref cel="skem.0o">skemo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_ch;&c_a;&c_r;&c_n;&c_a;&c_v;&c_ib;&c_k;</trd>,
      <trd>&c_n;&c_a;&c_k;&c_ib;&c_d;</trd>
    </trdgrp>
    <trdgrp lng="cs">
      <trd>koncept</trd>,
      <trd>makul&aacute;&rcaron;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Konzept</trd>,
      <trd>Entwurf</trd>,
      <trd>Kladde</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>rough draft</trd>,
      <trd>first draft</trd>
    </trdgrp>
    <trd lng="es">borrador</trd>
    <trd lng="fr">brouillon (subst.)</trd>
    <trd lng="hu">piszkozat</trd>
    <trd lng="nl">klad</trd>
    <trd lng="pl">brudnopis</trd>
    <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_o;&c_v;&c_i;&c_k;</trd>
    <trd lng="sk">koncept <klr>(p&iacute;somnej pr&aacute;ce)</klr></trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x4E0B;&#x66F8;&#x304D;  <pr>&#x3057;&#x305F;&#x304C;&#x304D;</pr></trd>,
    <trd>&#x8349;&#x7A3F;  <pr>&#x305D;&#x3046;&#x3053;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_ch;&c_je;&c_r;&c_n;&c_je;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x8349;&#x7A3F; <pr>c&#x1CE;og&#x1CE;o</pr></trd>,
    <trd>&#x6587;&#x7A3F; <pr>w&eacute;ng&#x1CE;o</pr></trd>,
    <trd>&#x8349;&#x6848; <pr>c&#x1CE;o&agrave;n</pr></trd>,
    <trd>&#x521D;&#x7A3F; <pr>ch&umacron;g&#x1CE;o</pr></trd>,
    <trd>&#x7A3F; <pr>g&#x1CE;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="net.mal0a">
  <kap>
    mal<tld/>a
  </kap>
  <snc>
    <dif>
      <ref tip="sin" cel="koncep.0a">Koncepta</ref>, <ref tip="sin"
      cel="protot.0a.TEK">prototipa</ref>,
      ankora&ubreve; ne finpreparita:
      <ekz>
        kiam mi havas iun kanton en mal<tld/>a formo, mi montras &gcirc;in al &scirc;i
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>;
      </ekz>
      <ekz>
        post filmado &ccirc;e UK-o <klr>[&#x2026;]</klr> mal<tld/>a versio estis projekciita
        <fnt>
          <bib>Kon11</bib>
          <lok>Intervjuo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_ch;&c_a;&c_r;&c_n;&c_a;&c_v;&c_y;</trd>,
      <trd>&c_p;&c_a;&c_p;&c_ja;&c_r;&c_e;&c_d;&c_n;&c_ib;</trd>
    </trdgrp>
    <trd lng="de">im <ind>Entwurf</ind></trd>
    <trd lng="en">as draft</trd>
  </snc>
  <snc mrk="net.mal0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      (pri pezo) <ref tip="dif" cel="tar.sen0a">sentara</ref>;
      (pri prezo) kun &ccirc;iuj krompagoj (sendkostoj, impostoj ks):
      <ekz>
        29% de la mal<tld/>a enlanda produkto
        <fnt>
          <bib>Kon11</bib>
          <lok>Kial Nepalaj viroj iras labori en arabaj landoj?</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kapa mal<tld/>a nacia produkto en la ali&gcirc;ontaj landoj malsuperas duonon de la
        EU-avera&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Istv&aacute;n Ertl</aut>
          <vrk><url ref="&Monato;/2003/008140.html"
              >Pri orienti&gcirc;o kaj okcidenti&gcirc;o</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        la hala enirpago sufi&ccirc;is por pagi niajn elspezojn, do la mal<tld/>a profito &ccirc;e
        la kuiriloj fari&gcirc;is <tld/>a profito
        <fnt>
          <bib>KrM</bib>
          <lok>40. Kaj Rapide Kreskas La Afero.</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="brutt.0a">brutta</ref>
    <trdgrp lng="be">
      <trd>&c_v;&c_a;&c_l;&c_a;&c_v;&c_y;</trd>,
      <trd>&c_a;&c_g;&c_u;&c_l;&c_o;&c_v;&c_y;</trd>
    </trdgrp>
    <trd lng="de">brutto</trd>
    <trd lng="en">gross</trd>
    <trd lng="fr">brut</trd>
  </snc>
</drv>

