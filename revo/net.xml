<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>net</rad>/a
</kap>
<drv mrk="net.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Nekonfuza kaj preciza:
      <ekz>
        <tld/>a skriba&jcirc;o, desegno, kli&scirc;a&jcirc;o;
      </ekz>
      <ekz>
        <tld/>aj konturoj.
      </ekz>
    </dif>
    <trd lng="de">rein</trd>
    <trd lng="nl">net</trd>
    <trdgrp lng="pt">
      <trd>limpo</trd>,
      <trd>claro</trd>,
      <trd>distinto</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_i;&c_s;&c_t;&c_o;&c_v;&c_o;&c_j;</trd>,
      <trd>&c_b;&c_je;&c_l;&c_o;&c_v;&c_o;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Senigata je &ccirc;iu ajn kroma&jcirc;o, kalkulata la&ubreve; la nura propra kvanto:
      <ekz>
        <tld/>a pezo (sentara pezo, pezo de la varo mem ne enkalkulante la
        pezon de enhavo, envolva&jcirc;o);
      </ekz>
      <ekz>
        <tld/>a gajno, <tld/>a profito
        <klr>(definitiva profito, kiu restas post depreno de elspezoj
        faritaj a&ubreve; de perdoj okazintaj)</klr>.
      </ekz>
    </dif>
    <trd lng="de">netto</trd>
    <trd lng="en">net</trd>
    <trd lng="es">neto</trd>
    <trd lng="fr">net</trd>
    <trd lng="nl">netto</trd>
    <trd lng="pt">l&iacute;quido</trd>
    <trdgrp lng="ru">
      <trd>&c_ch;&c_i;&c_s;&c_t;&c_y;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_t;&c_t;&c_o;</trd>
    </trdgrp>
  </snc>
</drv>

<drv mrk="net.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Definitiva klara skriba&jcirc;o, sen korektoj kaj sen makuloj, ordinare
      kopiita la&ubreve; alia prepara pli malpli malklara skriba&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Reinschrift</trd>
  <trd lng="nl">net</trd>
  <trd lng="ru">&c_ch;&c_i;&c_s;&c_t;&c_o;&c_v;&c_i;&c_k;</trd>
</drv>

<drv mrk="net.mal0o">
  <kap>mal<tld/>o</kap>
  <snc>
    <dif>
      Unua prova skriba&jcirc;o kun korektoj, plibonigoj a&ubreve; makuloj, la&ubreve; kiu oni
      poste transskribas la <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Konzept</trd>,
    <trd>Entwurf</trd>,
    <trd>Kladde</trd>
  </trdgrp>
  <trd lng="nl">klad</trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_n;&c_o;&c_v;&c_i;&c_k;</trd>
</drv>
</art>
