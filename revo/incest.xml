<vortaro>
<art mrk="$Id$">
<kap>
  <rad>incest</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="incest.0o">
  <kap><tld/>o</kap>
  <snc mrk="incest.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Seksrilatoj inter proksimaj samfamilianoj:
      <ekz>
        la malpermeso de la <tld/>o estas unu el la plej universalaj
        tabuoj en la homa genro.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="adult.sang0o">sangadulto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_c;&c_e;&c_s;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Blutschande</trd>,
    <trd>Inzest</trd>
  </trdgrp>
  <trd lng="en">incest</trd>
  <trd lng="es">incesto</trd>
  <trd lng="fr">inceste</trd>
  <trd lng="hu">v&eacute;rfert&odblac;z&eacute;s</trd>
  <trd lng="nl">incest</trd>
  <trd lng="pt">incesto</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_n;&c_c;&c_je;&c_s;&c_t;</trd>,
    <trd>&c_k;&c_r;&c_o;&c_v;&c_o;&c_s;&c_m;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">incest</trd>
</drv>
</art>
