<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kultur.xml,v 1.34 2020/01/06 00:10:20 revo Exp $">
<kap>
  <ofc>1</ofc><rad>kultur</rad>/o <fnt><bib>PV</bib></fnt> 
</kap>
<drv mrk="kultur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kultur.0i.materie">
    <dif>
      Flegi la naturajn rimedojn kaj kondi&ccirc;ojn
      por homa utilo.
    </dif>
    <subsnc mrk="kultur.0i.AGR">
      <uzo tip="fak">AGR</uzo>
      <dif>
        Prilabori la teron, por fari &gcirc;in fruktodona,
        <ref tip="prt" cel="plug.0i.AGR">plugi</ref>,
        <ref tip="prt" cel="sterk.0i.AGR">sterki</ref>
        kaj tiel plu:
        <ekz>
          <tld/>i kampon, sian &gcirc;ardenon.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_k;&c_u;&c_l;&c_mol;&c_t;&c_y;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_a;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; <klr
        tip="amb">(&c_z;&c_ja;&c_m;&c_l;&c_ju;)</klr></trd>,
        <trd>&c_u;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol; <klr tip="amb">(&c_z;&c_ja;&c_m;&c_l;&c_ju;)</klr></trd>
      </trdgrp>
      <trdgrp lng="de">
        <trd>anbauen</trd>,
        <trd>bebauen</trd>,
        <trd>kultivieren</trd>
      </trdgrp>
      <trd lng="hu">m&udblac;vel</trd>
      <trd lng="nl">bebouwen</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
        <trd>&c_o;&c_b;&c_r;&c_a;&c_b;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_z;&c_je;&c_m;&c_l;&c_ju;)</klr></trd>,
        <trd>&c_v;&c_o;&c_z;&c_d;&c_je;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kultur.0i.flegi">
      <dif>
        Speciale flegi vegeta&jcirc;ojn, bestojn a&ubreve; homojn, por favori kaj
        perfektigi ilian korpan evoluon:
        <ekz>
          <tld/>i rozojn;
        </ekz>
        <ekz>
          sova&gcirc;aj kaj <tld/>itaj vegeta&jcirc;oj;
        </ekz>
        <ekz>
          <tld/>i abelojn, kuniklojn, kokinojn, kolombojn;
        </ekz>
        <ekz>
          <tld/>i la muskolojn de infano;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          ni <tld/>u la Zamenhofan spiriton.
        </ekz>
      </dif>
      <rim>
        En la moderna lingvo, ol <ctl>kulturi bestojn</ctl>
        oni pliofte diras <ref tip="sub" cel="bred.0i">bredi</ref>
        bestojn.
      </rim>
      <refgrp tip="vid">
        <ref cel="eduk.0i">eduki</ref>,
        <ref cel="nutr.0i">nutri</ref>,
        <ref cel="zorg.0i">zorgi</ref>
      </refgrp>
      <trd lng="be">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_y;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
      <trdgrp lng="de">
        <trd>z&uuml;chten</trd>,
        <trd>hegen</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>nevel</trd>,
        <trd>gondoz</trd>,
        <trd>termeszt</trd>,
        <trd>teny&eacute;szt</trd>
      </trdgrp>
      <trd lng="nl">kweken</trd>
      <trd lng="ru">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </subsnc>
  </snc>
  <snc mrk="kultur.0i.spirite">
    <dif>
      Perfektigi spiritan evoluon de homo per edukado:
      <ekz>
        <tld/>i la memoron, intelekton de infano;
      </ekz>
      <ekz>
        la Grekoj estis frue <tld/>ataj de re&gcirc;oj kaj kolonianoj
        venintaj
        el Egiptujo;
      </ekz>
      <ekz>
        <tld/>(it)a popolo;
      </ekz>
      <ekz>
        alte <tld/>ita menso, klasike <tld/>ita homo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="civili.0i">civilizi</ref>,
      <ref cel="kler.0igi">klerigi</ref>
    </refgrp>
    <trd lng="be">&c_a;&c_d;&c_u;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>heranbilden</trd>,
      <trd>veredeln</trd>,
      <trd>kultivieren</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kultur&aacute;l</trd>,
      <trd>nevel</trd>,
      <trd>kim&udblac;vel</trd>
    </trdgrp>
    <trd lng="nl">beschaven</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>chovat</trd>,
    <trd>kultivovat</trd>,
    <trd>obd&ecaron;lat</trd>,
    <trd>obd&ecaron;l&aacute;vat</trd>,
    <trd>p&ecaron;stovat</trd>,
    <trd>vyp&ecaron;stovat</trd>,
    <trd>vzd&ecaron;l&aacute;vat</trd>
  </trdgrp>
  <trd lng="en">culture</trd>
  <trd lng="es">cultivar</trd>
  <trd lng="fr">cultiver</trd>
  <trdgrp lng="pl">
    <trd>uprawia&cacute;</trd>,
    <trd>kszta&lstroke;ci&cacute;</trd>,
    <trd>hodowa&cacute;</trd>,
    <trd>chowa&cacute;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>cultiva</trd>,
    <trd>forma</trd>,
    <trd>cre&#537;te</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>obr&aacute;ba&tcaron;</trd>,
    <trd>pestova&tcaron;</trd>,
    <trd>vypestova&tcaron;</trd>,
    <trd>vzdel&aacute;va&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kultur.0o">
  <kap><ofc>1</ofc><tld/>o</kap>
  <snc mrk="kultur.0o.materia">
    <dif>
      Flego, bonteno de la naturaj kondi&ccirc;oj.
    </dif>
    <subsnc mrk="kultur.0o.AGR">
      <uzo tip="fak">AGR</uzo>
      <ref tip="dif" cel="kultur.ter0o.AGR">ter<tld/>o</ref>
      <trdgrp lng="de">
        <trd>Anbau</trd>,
        <trd>Kultivierung</trd>
      </trdgrp>
      <trd lng="hu">f&ouml;ldm&udblac;vel&eacute;s</trd>
      <trd lng="nl">bebouwen<klr tip="ind"> (z.nw.)</klr></trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_v;&c_a;&c_c;&c_i;&c_ja;</trd>,
        <trd>&c_v;&c_o;&c_z;&c_d;&c_je;&c_l;&c_y;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="kultur.0o.flego">
      <dif>
        Flegado de vegeta&jcirc;oj, bestoj a&ubreve; homoj por favori kaj perfektigi
        ilian korpan evoluon:
        <ekz>
          <tld/>o de legomoj, floroj.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>Z&uuml;chtung</trd>,
        <trd>Zucht</trd>,
        <trd>Hege</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>nevel</trd>,
        <trd>gondoz</trd>,
        <trd>teny&eacute;szt</trd>,
        <trd>termeszt</trd>
      </trdgrp>
      <trd lng="nl">teelt</trd>
      <trdgrp lng="ru">
        <trd>&c_k;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_v;&c_a;&c_c;&c_i;&c_ja;</trd>,
        <trd>&c_u;&c_h;&c_o;&c_d;</trd>
      </trdgrp>
    </subsnc>
  </snc>
  <snc mrk="kultur.0o.spirita">
    <dif>
      Prizorgo de homaj artoj, de ilia progreso, komunigo kaj transdono.
    </dif>
    <subsnc mrk="kultur.0o.ekzerco">
      <dif>
        Prizorgado kaj perfektigado en spirita evoluo de homo:
        <ekz>
          la naturaj kapabloj disvolvi&gcirc;as per <tld/>o.
        </ekz>
      </dif>
      <rim>
        <em>Kulturo</em> estas &ccirc;efe spirita,
        <ref tip="vid" cel="civili.0o">civilizo</ref>
        estas anka&ubreve; materia;
        <em>kulturo</em> postulas komprenon kaj senton pri
        belartoj, sed <em>civilizo</em> postulas krome
        la mildi&gcirc;on de sociaj rilatoj kaj la te&hcirc;nikan
        <ref tip="vid" cel="progre.0o">progreson</ref>
        de vivrimedoj.
      </rim>
      <trdgrp lng="de">
        <trd>Heranbildung</trd>,
        <trd>Veredelung</trd>,
        <trd>Kultivierung</trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>kim&udblac;vel&eacute;s</trd>,
        <trd>m&udblac;vel&odblac;d&eacute;s</trd>,
        <trd>kult&uacute;ra</trd>
      </trdgrp>
      <trd lng="nl">beschaving</trd>
      <trd lng="ru">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_a;</trd>
    </subsnc>
    <subsnc mrk="kultur.0o.HIS">
      <uzo tip="fak">HIS</uzo>
      <dif>
        Tuto de transgeneraciaj moroj, artoj, ideoj kaj
        vivmanieroj flegataj de iu popolo:
        <ekz>
          la azteka <tld/>o
          <fnt><bib>SatirRak</bib>, <lok>la guarbo</lok></fnt>;
        </ekz>
        <ekz>
          oni diris al mi: dank' al tradukado, la plej
          malproksimaj <tld/>oj nun alireblas al &ccirc;iu
          <fnt>
            <aut>C. Piron</aut>
            <vrk><url
            ref="http://claudepiron.free.fr/articlesenesperanto/mitojkajvero.htm">
            Kie la mitoj? Kie la realeco?</url></vrk>
            <lok>1979</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="tradic.0o.ado">tradicio</ref>
      </refgrp>
    </subsnc>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>chov</trd>,
    <trd>kultura</trd>,
    <trd>p&ecaron;stov&aacute;n&iacute;</trd>,
    <trd>vzd&ecaron;lanost</trd>
  </trdgrp>
  <trd lng="es">cultivo</trd>
  <trd lng="fr">culture</trd>
  <trdgrp lng="pl">
    <trd>kultura</trd>,
    <trd>uprawa</trd>
  </trdgrp>
  <trd lng="ro">cultur&abreve;</trd>
  <trdgrp lng="sk">
    <trd>chov</trd>,
    <trd>kult&uacute;ra</trd>,
    <trd>pestovanie</trd>,
    <trd>vzdelanos&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kultur.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <snc mrk="kultur.0a.KOMUNE">
    <dif>
      <tld lit="K"/>ita:
      <ekz>
        <tld/>a urbo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li da&ubreve;re partoprenis en la sveda <tld/>a vivo
        <fnt><bib>Kiso</bib>, <lok>anta&ubreve;parolo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>kulturell</trd>,
    <trd>kultiviert</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cultiv&eacute;</trd>,
    <trd>culturel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&udblac;vel&eacute;si</trd>,
    <trd>m&udblac;vel&odblac;d&eacute;si</trd>,
    <trd>kultur&aacute;lis</trd>
  </trdgrp>
  <trd lng="nl">beschaafd</trd>
  <trdgrp lng="pl">
    <trd>kulturalny</trd>,
    <trd>&sacute;wiat&lstroke;y</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>culturale</trd>,
    <trd>clar</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="kultur.brul0o">
  <kap>brul<tld/>o</kap>
  <snc mrk="kultur.brul0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      <tld lit="K"/>metodo, en kiu oni preparas novajn kampojn
      per bruligo de la natura vegeta kovra&jcirc;o:
      <ekz>
        por multaj brul<tld/>oj &ccirc;irka&ubreve; &ccirc;i tiu monto li faligis kaj 
        bruligis arbojn
        <fnt><aut>A. Kivi, trad. I. Ekstr&ouml;m</aut>, <vrk>Sep 
        fratoj</vrk>, <lok>1947</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sark.brul0i.AGR">brulsarki</ref>
    </refgrp>
  </snc>
  <trdgrp lng="en"><trd>slash-and-burn cultivation</trd>, <trd>shifting
  cultivation</trd>, <trd>swiddening</trd></trdgrp>
  <trd lng="fr">agriculture sur brulis</trd>
  <trd lng="pl">wypalanie teren&oacute;w</trd>     
  <trd lng="ro">arde zone</trd>
</drv>

<drv mrk="kultur.fizika0o">
  <kap>fizika <tld/>o</kap>  
  <snc mrk="kultur.fizika0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Korpa ekzercado,
      <ref tip="vid" cel="gimnas1.0o.SPO">gimnastika</ref>
      a&ubreve; alia:
      <ekz>
        neniuj ekstraktoj, glandoj, injekcioj, kiel ajn
        efikaj, faros el homo superhomon; tion oni ne povas
        atingi sole per la fizika <tld/>o
        <fnt>
          <aut>Lidia Zamenhof</aut>
          <vrk>La vojo al superhomo</vrk>
          <lok>La Praktiko, 1936:12 (60)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">culture physique</trd>
  <trd lng="hu">testkult&uacute;ra</trd>
  <trd lng="pl">kultura fizyczna</trd>
  <trdgrp lng="ro">
    <trd>cultur&abreve; fizic&abreve;</trd>,
    <trd>educa&#539;ie fizic&abreve;</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_i;&c_z;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; <ind>&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_a;</ind></trd>
</drv>

<drv mrk="kultur.gxarden0o">
  <kap>&gcirc;arden<tld/>o</kap>
  <snc mrk="kultur.gxarden0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Intensa <tld/>o de vegeta&jcirc;oj en &gcirc;ardenoj:
      <ekz>
        estas belaj la floroj de la kampoj kaj arbaroj <klr>...</klr>
        tamen la &gcirc;arden<tld/>o povas perfektigi ilian belecon
        <fnt>
        <aut>A. Giesswein</aut>,
        <vrk><url
        ref="http://www.tekstoj.nl/lm/lm22-2/mondliteraturo.html">Esperanto
        kaj mondliteraturo</url></vrk>,
        <lok>Literatura Mondo, 1922, p. 21a-22a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">horticulture</trd>
  <trd lng="fr">horticulture</trd>
  <trd lng="he">&gimel;&nun;&nun;&vav;&tav;</trd>
  <trd lng="hu">kertm&udblac;vel&eacute;s</trd>
  <trd lng="pl">ogrodnictwo</trd>
  <trd lng="pt">horticultura</trd>
  <trdgrp lng="ro">
    <trd>horticultur&abreve;</trd>,
    <trd>gr&abreve;din&abreve;rit</trd>
  </trdgrp>
</drv>

<drv mrk="kultur.kontraux0o">
  <kap>kontra&ubreve;<tld/>o</kap>
  <snc mrk="kultur.kontraux0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <tld lit="K"/>o, arta movado, modo, rigardata
      kiel kontra&ubreve;a al la kutimoj de la &ccirc;irka&ubreve;a socio:
      <ekz>
        granda procento de Idistoj estas eks-Esperantistoj.
        &Ccirc;iu <tld/>o havas sian kontra&ubreve;<tld/>on, &ccirc;u ne? 
        <fnt>
          <aut>K. Lendon</aut>,
          <vrk>Pretendoj martenaj</vrk>,
          <lok><url
          ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">
          soc.culture.esperanto</url>, 1998-10-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kultur.sub0o.POL">subkulturo</ref>
    </refgrp>
    <refgrp tip="ant">
      <ref cel="konfor.0ismo.KOMUNE">konformismo</ref>
    </refgrp>
  </snc>
  <trd lng="fr">contre-culture</trd>
  <trd lng="pl">kontrkultura</trd>     
  <trd lng="ro">contracultur&abreve;</trd>
</drv>

<drv mrk="kultur.plur0eco">
  <kap>plur<tld/>eco</kap>
  <snc mrk="kultur.plur0eco.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Kunekzisto de pluraj <tld/>oj
      en sama socio, socia
      <ref tip="vid" cel="divers.0eco.KOMUNE">diverseco</ref>,
      <ref tip="vid" cel="bunt.0eco.FIG">bunteco<sncref/></ref>:
      <ekz>
        Itala Esperantista Junularo invitas <klr>...</klr> al evento 
        kiu celas prezenti plur<tld/>econ en lernejoj
        <fnt>
          <aut>Tejo</aut>,
          <vrk><url ref="http://tejo.org/eu/node/725">
          Semajno de Kulturo Internacia</url></vrk>,
          <lok>2008-05-14</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">multiculturalisme</trd>
  <trd lng="pl">wielokulturowo&sacute;&cacute;</trd>     
  <trd lng="ro">multiculturalism</trd>
</drv>

<drv mrk="kultur.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="kultur.sen0a.neflegata">
    <uzo tip="klr">(pri vegeta&jcirc;o, besto)</uzo>
    <dif>
      Ne<tld/>ita, <ref tip="vid" cel="krud.0a.KOMUNE">kruda</ref>,
      <ref tip="vid" cel="sovagx.0a">sova&gcirc;a</ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_z;&c_ib;&c_k;&c_ib;</trd>,
      <trd>&c_n;&c_ja;&c_w;&c_r;&c_o;&c_b;&c_l;&c_je;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>ungepflegt</trd>,
      <trd>roh</trd>
    </trdgrp>
    <trd lng="nl">wild</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_i;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_o;&c_z;&c_d;&c_je;&c_l;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kultur.sen0a.spirite">
    <uzo tip="klr">(pri homo)</uzo>
    <dif>
      Ne<tld/>ita, <ref tip="vid" cel="brut.0a">bruta</ref>,
      <ref tip="vid" cel="kler.mal0a">malklera</ref>,
      <ref tip="vid" cel="krud.0a.MOR">kruda</ref>:
      <ekz>
        sen<tld/>aj ekscentraj esperantistoj
        <fnt><bib>Plumamikoj</bib>, <lok>kie komencu
        komencanto?</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_je;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_je;&c_s;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>kulturlos</trd>,
      <trd>unkultiviert</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>m&udblac;veletlen</trd>,
      <trd>kultur&aacute;latlan</trd>
    </trdgrp>
    <trd lng="nl">onbeschaafd</trd>
    <trd lng="pl">niekulturalny</trd>     
    <trd lng="ro">nepoliticos</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_je;&c_s;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">inculte</trd>
</drv>

<drv mrk="kultur.sen0ejo">
  <kap>sen<tld/>ejo</kap>
  <snc mrk="kultur.sen0ejo.AGR">
    <uzo tip="fak">AGR</uzo>
    <ref tip="dif" cel="kultiv.ne0ejo.AGR">senkultivejo</ref>
      <ekz>
        li longpa&scirc;e supreniris la&ubreve; la sen<tld/>ejo
        <fnt>
          <aut>V. Catal&agrave;, trad. J. Ventura i Freixas</aut>,
          <vrk>Soleco</vrk>,
          <lok>1967</lok>
        </fnt>.
      </ekz>
     <trd lng="fr">friche</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>neobd&ecaron;lan&eacute; pole</trd>,
    <trd>&uacute;hor</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>ug&oacute;r</trd>,
    <trd>od&lstroke;&oacute;g</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>&#539;elin&abreve;</trd>,
    <trd>p&abreve;m&acirc;nt abandonat</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>neobroben&eacute; pole</trd>,
    <trd>&uacute;hor</trd>
  </trdgrp>
</drv>

<drv mrk="kultur.sub0o">
  <kap>sub<tld/>o</kap>
  <snc mrk="kultur.sub0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Kulturo a&ubreve; moroj de eta homgrupo, parte similantaj, parte
      malsimilantaj la kulturon de la &ccirc;irka&ubreve;a socio:
      <ekz>
        junularaj sub<tld/>oj
        <fnt>
        <aut>[G. Mickle?]</aut>,
        <vrk><url
        ref="http://home.arcor.de/gmickle/skk/92ffln01.html">Lingvodefendado
        = lingvonaciismo!</url></vrk>,
        <lok>2004-03-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">sous-culture</trd>
  <trd lng="pl">subkultura</trd>     
  <trd lng="ro">subcultur&abreve;</trd>
</drv>

<drv mrk="kultur.ter0o">
  <kap>ter<tld/>o</kap>
  <snc mrk="kultur.ter0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Prilaborado pri tero, por igi &gcirc;in fruktodona;
      <ref tip="sin" cel="kultur.0o.AGR">kulturo</ref>,
      <ref tip="sin" cel="agriku.0o.AGR">agrikulturo</ref>.
    </dif>
  </snc>
  <trd lng="fr">travail de la terre</trd>
  <trdgrp lng="pl">
    <trd>rolnictwo</trd>,
    <trd>agrokultura</trd>,
    <trd>uprawa ziemi</trd>
  </trdgrp>  
  <trd lng="ro">agricultur&abreve;</trd>
</drv>

<drv mrk="kultur.ter0isto">
  <kap>ter<tld/>isto</kap>
  <snc mrk="kultur.ter0isto.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Persono profesie laboranta en la <tld/>ado de tero,
      kreskigo de vegeta&jcirc;oj, bredado de bestoj;
      <ref tip="sin" cel="agriku.0isto.AGR">agrikulturisto</ref>,
      <ref tip="sin" cel="kamp.0isto.AGR">kampisto</ref>:
      <ekz>
        &ccirc;u vi a&ubreve;dis pri ter<tld/>isto, kiu printempe semas siajn 
        kampojn, sed a&ubreve;tune, kiam la greno maturi&gcirc;is, ne rikoltas &gcirc;in?  
        <fnt>
          <aut>H. Seppik</aut>,
          <vrk><url ref="http://www.esperanto.mv.ru/Seppik/kajnun.html">
          La tuta Esperanto</url></vrk>,
          <lok>[1938?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cultivateur</trd>,
    <trd>agriculteur</trd>
  </trdgrp>
  <trd lng="pl">rolnik</trd>
  <trdgrp lng="ro">
    <trd>fermier</trd>,
    <trd>agricultor</trd>
  </trdgrp>
</drv>
