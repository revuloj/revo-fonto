<drv mrk="kultur.inter0a">
  <kap>
    inter<tld/>a
  </kap>
  <snc>
    <dif>
      Koncernanta la rilatojn inter pluraj <tld/>oj<sncref ref="kultur.0o.HIS"/>:
      <ekz>
        homoj kun minimuma kompreno pri inter<tld/>aj aferoj
        <fnt>
          <aut>Brian Moon</aut>
            <vrk><url ref="&Monato;/2000/007263.php">Ridindig(n)e neridinda (1)</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 4</lok>
          </fnt>;
      </ekz>
      <ekz>
        gravas inter<tld/>aj kaj komunumsubtenaj projektoj de E&ubreve;ropa Unio
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2005/008603.php">Toleremo ankora&ubreve; ne tolerata</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        agnoskigi la krestimulan naturon de la lingvo kaj veki miron pri &gcirc;ia tuja
        kompreneblo inter<tld/>a
        <fnt>
          <bib>BonaLingvo</bib>
          <lok>2. La Spirito De La Lingvo</lok>
        </fnt>;
      </ekz>
      <ekz>
        konferenco pri Inter<tld/>a Dialogo
        <fnt>
          <bib>Monato</bib>
          <aut>Zlatko Ti&scaron;ljar</aut>
          <vrk><url ref="&Monato;/2008/009425.php">Muso kaj elefantaro</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter<tld/>a alproksimi&gcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011257.php">En rubujon, kulturo kaj lingvo de Italio</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        digesti inter<tld/>ajn problemojn
        <fnt>
          <bib>Monato</bib>
          <aut>Mireille Grosjean</aut>
          <vrk><url ref="&Monato;/2011/010267.php">Svislando: la arto interkompreni&gcirc;i</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter<tld/>a edukado kaj internacia praktika kunlaboro
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 7 (69)</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="naci.inter0a">internacia</ref>,
      <ref cel="popol.inter0a">interpopola</ref>
    </refgrp>
  </snc>
  <trd lng="de">interkulturell</trd>
  <trdgrp lng="ja">
    <trd>&#x6587;&#x5316;&#x9593;&#x306E;  [<ind>&#x3076;&#x3093;&#x304B;&#x304B;&#x3093;&#x306E;</ind>]</trd>,
    <trd>&#x7570;&#x6587;&#x5316;&#x9593;&#x306E;  [<ind>&#x3044;&#x3076;&#x3093;&#x304B;&#x304B;&#x3093;&#x306E;</ind>]</trd>
  </trdgrp>
</drv>










