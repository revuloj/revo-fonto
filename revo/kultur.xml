<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>kultur</rad>/i
</kap>
<drv mrk="kultur.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kultur.0i.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Prilabori la teron, por fari &gcirc;in fruktodona:
      <ekz>
        <tld/>i kampon, sian &gcirc;ardenon.
      </ekz>
    </dif>
    <refgrp tip="prt">
      <ref cel="plug.0i.AGR">plugi</ref>
      <ref cel="sterk.0i.AGR">sterki</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_k;&c_u;&c_l;&c_mol;&c_t;&c_y;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_p;&c_r;&c_a;&c_c;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_z;&c_ja;&c_m;&c_l;&c_ju;)</klr></trd>,
      <trd>&c_u;&c_r;&c_a;&c_b;&c_l;&c_ja;&c_c;&c_mol; <klr tip="amb">(&c_z;&c_ja;&c_m;&c_l;&c_ju;)</klr></trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>anbauen</trd>,
      <trd>bebauen</trd>,
      <trd>kultivieren</trd>
    </trdgrp>
    <trd lng="hu">m&udblac;vel</trd>
    <trd lng="nl">bebouwen</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_a;&c_b;&c_a;&c_t;&c_y;&c_v;&c_a;&c_t;&c_mol; <klr>(&c_z;&c_je;&c_m;&c_l;&c_ju;)</klr></trd>,
      <trd>&c_v;&c_o;&c_z;&c_d;&c_je;&c_l;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kultur.0i.flegi">
    <dif>
      Speciale flegi vegeta&jcirc;ojn, bestojn a&ubreve; homojn, por favori kaj
      perfektigi ilian korpan evoluon:
      <ekz>
        <tld/>i rozojn;
      </ekz>
      <ekz>
        sova&gcirc;aj kaj <tld/>itaj vegeta&jcirc;oj;
      </ekz>
      <ekz>
        <tld/>i abelojn, kuniklojn, kokinojn, kolombojn;
      </ekz>
      <ekz>
        <tld/>i la muskolojn de infano;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ni <tld/>u la Zamenhofan spiriton.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bred.0i">bredi</ref>,
      <ref cel="eduk.0i">eduki</ref>,
      <ref cel="nutr.0i">nutri</ref>,
      <ref cel="zorg.0i">zorgi</ref>
    </refgrp>
    <trd lng="be">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_y;&c_v;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>z&uuml;chten</trd>,
      <trd>hegen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>nevel</trd>,
      <trd>gondoz</trd>,
      <trd>termeszt</trd>,
      <trd>teny&eacute;szt</trd>
    </trdgrp>
    <trd lng="nl">kweken</trd>
    <trd lng="ru">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_v;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="kultur.0i.spirite">
    <dif>
      Perfektigi spiritan evoluon de homo per edukado:
      <ekz>
        <tld/>i la memoron, intelekton de infano;
      </ekz>
      <ekz>
        la Grekoj estis frue <tld/>ataj de re&gcirc;oj kaj kolonianoj venintaj
        el Egiptujo;
      </ekz>
      <ekz>
        <tld/>(it)a popolo;
      </ekz>
      <ekz>
        alte <tld/>ita menso, klasike <tld/>ita homo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="civili.0i">civilizi</ref>,
      <ref cel="kler.0igi">klerigi</ref>
    </refgrp>
    <trd lng="be">&c_a;&c_d;&c_u;&c_k;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>heranbilden</trd>,
      <trd>veredeln</trd>,
      <trd>kultivieren</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kultur&aacute;l</trd>,
      <trd>nevel</trd>,
      <trd>kim&udblac;vel</trd>
    </trdgrp>
    <trd lng="nl">beschaven</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_s;&c_v;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <trd lng="en">culture</trd>
  <trd lng="fr">cultiver</trd>
</drv>

<drv mrk="kultur.0o">
  <kap><tld/>o</kap>
  <snc mrk="kultur.0o.AGR">
    <uzo tip="fak">AGR</uzo>
    <dif>
      Prilaborado pri tero, por igi &gcirc;in fruktodona.
    </dif>
    <trdgrp lng="de">
      <trd>Anbau</trd>,
      <trd>Kultivierung</trd>
    </trdgrp>
    <trd lng="hu">f&ouml;ldm&udblac;vel&eacute;s</trd>
    <trd lng="nl">bebouwen<klr tip="ind"> (z.nw.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_v;&c_a;&c_c;&c_i;&c_ja;</trd>,
      <trd>&c_v;&c_o;&c_z;&c_d;&c_je;&c_l;&c_y;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kultur.0o.flego">
    <dif>
      Flegado pri vegeta&jcirc;o, besto a&ubreve; homoj por favori kaj perfektigi ilian
      korpan evoluon:
      <ekz>
        <tld/>o de legomoj, floroj;
      </ekz>
      <ekz>
        <ind>la fizika <tld/>o</ind>
        <klr>(gimnastiko)</klr>.
        <trd lng="ru">&c_f;&c_i;&c_z;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; <ind>&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_a;</ind></trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Z&uuml;chtung</trd>,
      <trd>Zucht</trd>,
      <trd>Hege</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>nevel</trd>,
      <trd>gondoz</trd>,
      <trd>teny&eacute;szt</trd>,
      <trd>termeszt</trd>
    </trdgrp>
    <trd lng="nl">teelt</trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_u;&c_l;&c_mol;&c_t;&c_i;&c_v;&c_a;&c_c;&c_i;&c_ja;</trd>,
      <trd>&c_u;&c_h;&c_o;&c_d;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kultur.0o.spirita">
    <dif>
      Prizorgado kaj perfektigado je spirita evoluo de homo:
      <ekz>
        la naturaj kapabloj disvolvi&gcirc;as per <tld/>o;
      </ekz>
      <ekz>
        antikva <tld/>o;
      </ekz>
      <ekz>
        la azteka <tld/>o
        <fnt><bib>SatirRak</bib>, <lok>la guarbo</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      <em>Kulturo</em> estas &ccirc;efe spirita, <em>civilizo</em> estas anka&ubreve;
      materia; <em>kulturo</em> postulas komprenon kaj senton pri
      belartoj, beletristiko ktp, sed <em>civilizo</em> postulas krome la
      mildi&gcirc;on de la sociaj rilatoj kaj la teknikan perfekti&gcirc;on de la
      vivrimedoj; &gcirc;is nun <em>kulturo</em> povis esti nur nacia;
      estonte, dank' al Esperanto &gcirc;i povas fari&gcirc;i tutmonda, paralele al
      disvasti&gcirc;o de l' <em>civilizo</em>, kiu estas esence homara.
    </rim>
    <trdgrp lng="de">
      <trd>Heranbildung</trd>,
      <trd>Veredelung</trd>,
      <trd>Kultivierung</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>kim&udblac;vel&eacute;s</trd>,
      <trd>m&udblac;vel&odblac;d&eacute;s</trd>,
      <trd>kult&uacute;ra</trd>
    </trdgrp>
    <trd lng="nl">beschaving</trd>
    <trd lng="ru">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_a;</trd>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_a;</trd>
  <trd lng="fr">culture</trd>
</drv>

<drv mrk="kultur.0a">
  <kap><tld/>a<fnt>Z</fnt></kap>
  <snc mrk="kultur.0a.KOMUNE">
    <dif>
      <tld lit="K"/>ita:
      <ekz>
        <tld/>a urbo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        li da&ubreve;re partoprenis en la sveda <tld/>a vivo
        <fnt><bib>Kiso</bib>, <lok>anta&ubreve;parolo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>kulturell</trd>,
    <trd>kultiviert</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cultiv&eacute;</trd>,
    <trd>culturel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>m&udblac;vel&eacute;si</trd>,
    <trd>m&udblac;vel&odblac;d&eacute;si</trd>,
    <trd>kultur&aacute;lis</trd>
  </trdgrp>
  <trd lng="nl">beschaafd</trd>
  <trd lng="ru">&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="kultur.sen0a">
  <kap>sen<tld/>a</kap>
  <snc mrk="kultur.sen0a.neflegata">
    <uzo tip="klr">(pri vegeta&jcirc;o, besto)</uzo>
    <dif>
      Ne<tld/>ita, <ref tip="vid" cel="krud.0a">kruda</ref>, <ref
      tip="vid" cel="sovagx.0a">sova&gcirc;a</ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_d;&c_z;&c_ib;&c_k;&c_ib;</trd>,
      <trd>&c_n;&c_ja;&c_w;&c_r;&c_o;&c_b;&c_l;&c_je;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>ungepflegt</trd>,
      <trd>roh</trd>
    </trdgrp>
    <trd lng="nl">wild</trd>
    <trdgrp lng="ru">
      <trd>&c_d;&c_i;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_n;&c_je;&c_v;&c_o;&c_z;&c_d;&c_je;&c_l;&c_a;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kultur.sen0a.spirite">
    <uzo tip="klr">(pri homo)</uzo>
    <dif>
      Ne<tld/>ita, <ref tip="vid" cel="brut">bruta</ref>, <ref tip="vid"
      cel="kler.mal0a">malklera</ref>, <ref tip="vid"
      cel="krud.0a">kruda</ref>:
      <ekz>
        sen<tld/>aj ekscentraj esperantistoj
        <fnt><bib>Plumamikoj</bib>, <lok>kie komencu
        komencanto?</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_je;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_b;&c_je;&c_s;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_y;</trd>,
      <trd>&c_n;&c_je;&c_a;&c_d;&c_u;&c_k;&c_a;&c_v;&c_a;&c_n;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>kulturlos</trd>,
      <trd>unkultiviert</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>m&udblac;veletlen</trd>,
      <trd>kultur&aacute;latlan</trd>
    </trdgrp>
    <trd lng="nl">onbeschaafd</trd>
    <trdgrp lng="ru">
      <trd>&c_n;&c_je;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_b;&c_je;&c_s;&c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">inculte</trd>
</drv>

<drv mrk="kultur.gxarden0o">
  <kap>&gcirc;arden<tld/>o</kap>
  <snc mrk="kultur.gxarden0o.HOR">
    <uzo tip="fak">HOR</uzo>
    <dif>
      Intensa <tld/>o de vegeta&jcirc;oj en &gcirc;ardenoj:
      <ekz>
        estas belaj la floroj de la kampoj kaj arbaroj <klr>...</klr>
        tamen la &gcirc;arden<tld/>o povas perfektigi ilian belecon
        <fnt>
        <aut>Aleksandro Giesswein</aut>,
        <vrk><url
        ref="http://www.tekstoj.nl/lm/lm22-2/mondliteraturo.html">Esperanto
        kaj mondliteraturo</url></vrk>,
        <lok>Literatura Mondo, 1922, p. 21a-22a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">horticulture</trd>
  <trd lng="fr">horticulture</trd>
  <trd lng="he">&gimel;&nun;&nun;&vav;&tav;</trd>
</drv>
</art>
