<drv mrk="neglek.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="neglek.0i.KOMUNE">
    <dif>
      Malzorgi, flankelasi, neatenti:
      <ekz>
        la etulo ploras, dezirante man&gcirc;a&jcirc;on; kaj se oni <tld/>as a&ubreve;
        iel mistraktas &gcirc;in, &gcirc;i malsani&gcirc;as
        <fnt>
        <aut>Hori Y.</aut>
        <vrk>&leftquot;Purikula&rightquot; piko</vrk>
        <bib>Monato</bib>
        <lok>1997:8, p. 13a</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial do Fonto tiome <tld/>is bone bindigi la
        libron
        <fnt>
        <aut>D. Broadribb</aut>
        <vrk>La Purgatorio vidita de dektriajarcentulo</vrk>
        <bib>Monato</bib>
        <lok>2002.5, p. 27a</lok>
        </fnt>?
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ignor.0i">ignori</ref>,
      <ref cel="zorg.mal0i">malzorgi</ref>,
      <ref cel="atent.preter0i">preteratenti</ref>
    </refgrp>
    <refgrp tip="ant">
      <ref cel="fleg.0i">flegi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_je; &c_z;&c_v;&c_a;&c_zh;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_k;&c_ib;&c_d;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">negligir</trd>
  <trdgrp lng="cs">
    <trd>opomenout</trd>,
    <trd>p&rcaron;ech&aacute;zet ml&ccaron;en&iacute;m</trd>,
    <trd>zanedbat</trd>
  </trdgrp>
  <trd lng="de">vernachl&auml;ssigen</trd>
  <trd lng="en">neglect</trd>
  <trdgrp lng="es">
  <trd>descuidar</trd>,
  <trd>desatender</trd>,
  <trd>omitir</trd>  
  </trdgrp>
  <trdgrp lng="fr">
    <trd>n&eacute;gliger</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>mell&odblac;z</trd>,
    <trd>figyelmen k&iacute;v&uuml;l hagy</trd>,
    <trd>neglig&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x306A;&#x304A;&#x3056;&#x308A;&#x306B;&#x3059;&#x308B;</trd>,
    <trd>&#x304A;&#x308D;&#x305D;&#x304B;&#x306B;&#x3059;&#x308B;</trd>,
    <trd>&#x8EFD;&#x8996;&#x3059;&#x308B;  [<ind>&#x3051;&#x3044;&#x3057;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x6020;&#x308B;  [<ind>&#x304A;&#x3053;&#x305F;&#x308B;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>veronachtzamen</trd>,
    <trd>verwaarlozen</trd>,
    <trd>negligeren</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lekcewa&zdot;y&cacute;</trd>,
    <trd>zaniedbywa&cacute;</trd>,
    <trd>bagatelizowa&cacute;</trd>
  </trdgrp>    
  <trdgrp lng="pt">
    <trd>negligenciar</trd>,
    <trd>desleixar</trd>,
    <trd>descuidar</trd>,
    <trd>descurar</trd>,
    <trd>desatender</trd>,    
    <trd>desprezar</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>desconsidera</trd>,
    <trd>neglijarea</trd>,
    <trd>minimizarea</trd>
  </trdgrp>    
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_n;&c_je;&c_b;&c_r;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_n;&c_je;&c_b;&c_r;&c_je;&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zanedba&tcaron;</trd>
  <trd lng="sv">f&ouml;rsumma</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x430;&#x43D;&#x435;&#x434;&#x431;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x442;&#x430;&#x432;&#x438;&#x442;&#x438;&#x441;&#x44F; &#x43D;&#x435;&#x434;&#x431;&#x430;&#x43B;&#x43E;</trd>,
    <trd>&#x431;&#x435;&#x437; &#x443;&#x432;&#x430;&#x433;&#x438;</trd>,
    <trd>&#x43D;&#x435;&#x445;&#x442;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x437;&#x430;&#x43D;&#x435;&#x445;&#x430;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x43B;&#x435;&#x433;&#x43A;&#x43E;&#x432;&#x430;&#x436;&#x438;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="neglek.0o">
  <kap><tld/>o</kap>
  <snc mrk="neglek.0o.KOMUNE">
    <dif>
      Malzorgo, neatento al io a&ubreve; neplenumo de iu tasko:
      <ekz>
        la arkitekto, <klr>...</klr> post periodo de relativa forgeso
        kaj
        <tld/>o, nuntempe &gcirc;uas popolan admiron
        <fnt>
        <aut>A. Casquero</aut>
        <vrk>2002 - Internacia jaro de Antoni Gaud&iacute;</vrk>
        <bib>Monato</bib>
        <lok>2002:6, p. 18a</lok>
        </fnt>;
      </ekz>
      <ekz>
        raporto pri la <tld/>oj levi la vivkondi&ccirc;ojn de la indi&gcirc;enaj
        popoloj
        <fnt>
        <aut>W. M. A. De Smet</aut>
        <vrk>Anta&ubreve; cent jaroj</vrk>
        <bib>Monato</bib>
        <lok>1998:5, p. 25a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_d;&c_b;&c_a;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_ja;&c_d;&c_b;&c_a;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_n;&c_je;&c_a;&c_h;&c_a;&c_j;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="ca">neglig&egrave;ncia</trd>
  <trdgrp lng="cs">
    <trd>p&rcaron;ehl&iacute;&zcaron;en&iacute;</trd>,
    <trd>&uacute;strk</trd>
  </trdgrp>
  <trd lng="de">Vernachl&auml;ssigung</trd>
  <trd lng="en">negligence</trd>
  <trdgrp lng="es">
    <trd>negligencia</trd>,
    <trd>descuido</trd>
  </trdgrp>
  <trd lng="fr">n&eacute;gligence</trd>
  <trdgrp lng="hu">
    <trd>mell&odblac;z&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">verwaarlozing</trd>
  <trdgrp lng="pl">
    <trd>lekcewa&zdot;enie</trd>,
    <trd>zaniedbanie</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>dispre&#x21B;</trd>,
    <trd>neglijen&#x21B;&abreve;</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je;&c_b;&c_r;&c_je;&c_zh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_h;&c_a;&c_l;&c_a;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>odstrkovanie</trd>,
    <trd>prehliadanie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x435;&#x434;&#x431;&#x430;&#x43B;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x43D;&#x435;&#x434;&#x431;&#x430;&#x43B;&#x44C;&#x441;&#x442;&#x432;&#x43E;</trd>,
    <trd>&#x43D;&#x435;&#x445;&#x442;&#x443;&#x432;&#x430;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="neglek.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="neglek.0ema.KOMUNE">
    <dif>
      Kiu facile <tld/>as, kiu ne diligentas pri sia devo,
      sia tasko...:
      <ekz>
        li diris <klr>...</klr> ke mi ne prenas mian laboron
        serioze, ke mi estas <tld/>ema...
        <fnt>
          <aut>C. Piron</aut>
          <vrk>&Ccirc;u ni kunvenis vane?</vrk>
          <lok>1982</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_d;&c_b;&c_a;&c_j;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_je;&c_r;&c_u;&c_p;&c_l;&c_ib;&c_v;&c_y;</trd>
  </trdgrp>
  <trd lng="fr">n&eacute;gligent</trd>
  <trdgrp lng="ja">
    <trd>&#x6295;&#x3052;&#x3084;&#x308A;&#x306A;  [<ind>&#x306A;&#x3052;&#x3084;&#x308A;&#x306A;</ind>]</trd>,
    <trd>&#x7121;&#x9813;&#x7740;&#x306A;  [<ind>&#x3080;&#x3068;&#x3093;&#x3061;&#x3083;&#x304F;&#x306A;</ind>]</trd>,
    <trd>&#x4E0D;&#x6CE8;&#x610F;&#x306A;  [<ind>&#x3075;&#x3061;&#x3085;&#x3046;&#x3044;&#x306A;</ind>]</trd>
  </trdgrp>
  <trd lng="pl">niedba&lstroke;y</trd>
  <trdgrp lng="ro">
    <trd>neglijen&#x21B;i</trd>,
    <trd>impasibil</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43D;&#x435;&#x434;&#x431;&#x430;&#x43B;&#x438;&#x439;</trd>,
    <trd>&#x43B;&#x435;&#x433;&#x43A;&#x43E;&#x432;&#x430;&#x436;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="neglek.0inda">
  <kap><tld/>inda</kap>
  <snc mrk="neglek.0inda.KOMUNE">
    <dif>
      Relative tiom eta, <ref tip="super" cel="grav.mal0a">malgrava</ref>,
      ke &gcirc;i ne meritas prizorgon a&ubreve; enkalkulon:
      <ekz>
        tio havis pozitivan efikon: da&ubreve;ra ekonomia kresko kun inflacio
        <tld/>inda
        <fnt>
        <aut>R. Rotsaert</aut>
        <vrk>Nehaltigeblaj prezalti&gcirc;oj</vrk>
        <bib>Monato</bib>
        <lok>2000:11, p. 8a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_ja;&c_z;&c_n;&c_a;&c_ch;&c_n;&c_y;</trd>,
    <trd>&c_n;&c_ja; &c_v;&c_a;&c_r;&c_t;&c_y; &c_w;&c_v;&c_a;&c_g;&c_ib;</trd>
  </trdgrp>
  <trd lng="ca">negligible</trd>
  <trd lng="cs">zanedbateln&yacute;</trd>
  <trd lng="de">vernachl&auml;ssigbar</trd>
  <trd lng="en">negligable</trd>
  <trdgrp lng="es">
    <trd>insignificante</trd>,
    <trd>balad&iacute;</trd>,
    <trd>nimio</trd>,
    <trd>trivial</trd>
  </trdgrp>
  <trd lng="fr">n&eacute;gligeable</trd>
  <trdgrp lng="hu">
    <trd>elhanyagolhat&oacute;</trd>,
    <trd>figyelmen k&iacute;v&uuml;l hagyhat&oacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verwaarloosbaar</trd>,
    <trd>onbelangrijk</trd>
  </trdgrp>
  <trd lng="pl">nieistotny</trd>
  <trdgrp lng="ro">
    <trd>neimportant&abreve;</trd>,
    <trd>m&abreve;runt</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_n;&c_je; &c_z;&c_a;&c_s;&c_l;&c_u;&c_zh;&c_i;&c_v;&c_a;&c_ju;&c_shch;&c_i;&c_j;
    <ind>&c_v;&c_n;&c_i;&c_m;&c_a;&c_n;&c_i;&c_ja;</ind></trd>,
    <trd>&c_n;&c_i;&c_ch;&c_t;&c_o;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_z;&c_n;&c_a;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_t;&c_o;&c_r;&c_y;&c_m; &c_m;&c_o;&c_zh;&c_n;&c_o;
    <ind>&c_p;&c_r;&c_je;&c_n;&c_je;&c_b;&c_r;&c_je;&c_ch;&c_mol;</ind></trd>,
    <trd>&c_p;&c_r;&c_je;&c_n;&c_je;&c_b;&c_r;&c_je;&c_zh;&c_i;&c_m;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_je;&c_n;&c_je;&c_b;&c_r;&c_je;&c_zh;&c_i;&c_m;&c_o; &c_m;&c_a;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">zanedbate&lcaron;n&yacute;</trd>
</drv>
