    <trd>&c_s;&c_a;&#x43A;&c_v;&#x43E;&#x44F;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7BB1;&#x5B50; <pr>xi&#x101;ngzi</pr></trd>,
    <trd>&#x63D0;&#x7BB1; <pr>t&#xED;xi&#x101;ng</pr></trd>,
