<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>valiz</rad>/o
</kap>
<drv mrk="valiz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Per anso mane portebla kofreto uzata dum voja&gcirc;o kiel <ref
      tip="super" cel="pak.0ajxo">paka&jcirc;o</ref>:
      <ekz>
        ni elliti&gcirc;is je la 3a horo frumatene por pretigi la <tld/>ojn
        kaj
        voja&gcirc;saketojn<fnt><aut>Jos&eacute; de N. Marques</aut>: <vrk>Voja&gcirc;o
        al
        Ushuaia</vrk>, <bib>Monato</bib><lok>jaro 2002a, numero 2a, p.
        18a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_a;&c_l;&c_ib;&c_z;&c_k;&c_a;</trd>,
    <trd>&c_ch;&c_a;&c_m;&c_a;&c_d;&c_a;&c_n;</trd>,
    <trd>&c_s;&c_a;&c_k;&c_v;&c_a;&c_ja;&c_zh;</trd>
  </trdgrp>     
  <trd lng="bg">&c_k;&c_u;&c_f;&c_a;&c_r;</trd>             
  <trd lng="br">malizenn</trd>
  <trdgrp lng="ca">
    <trd>maleta</trd>,
    <trd>valisa</trd>
  </trdgrp>
  <trd lng="de">Koffer</trd>
  <trd lng="en">suitcase</trd>
  <trd lng="es">maleta</trd>
  <trd lng="fr">valise</trd>
  <trd lng="he">&mem;&zayin;&vav;&vav;&zayin;&he;</trd>
  <trd lng="hu">b&odblac;r&ouml;nd</trd>
  <trdgrp lng="nl">
    <trd>koffer</trd>,
    <trd>valies</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>valise</trd>, 
   <trd>maleta</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_m;&c_o;&c_d;&c_a;&c_n;</trd>,
    <trd>&c_s;&c_a;&c_k;&c_v;&c_o;&c_ja;&c_zh;</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>bavul</trd>,
    <trd>valiz</trd>
  </trdgrp>
</drv>
</art>
