<drv mrk="seks.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="seks.0o.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Biologia ecaro diferenciganta
      <ref tip="sub" cel="seks.vir0ulo">virseksulon</ref>
      disde <ref tip="sub" cel="in.0o">ino</ref>:
      <ekz>
        aroj da rigardantoj haltis anta&ubreve; apartaj mastoj,
        la&ubreve;grade kiel interesis ilin la figuroj, la a&gcirc;o
        a&ubreve; <tld/>o de la viktimoj
        <fnt>
        <aut>H. Sienkiewicz trad. L. Zamenhof</aut>
        <vrk>Quo vadis?</vrk><lok>&ccirc;apitro 62a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj rajtoj kaj liberecoj <klr>...</klr> validas same por
        &ccirc;iuj homoj, sen kia ajn diferencigo, &ccirc;u la&ubreve;
        raso, ha&ubreve;tkoloro, <tld/>o, lingvo, religio...
        <fnt>
        <vrk>La Universala Deklaracio de Homaj Rajtoj</vrk>,
        <lok>Art. 2a</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Ne konfuzu kun <ref cel="genr.0o.BIO">genro<sncref/></ref>.
    </rim>
    <adm>
      Mankas ekz BOT. <aut>MB</aut>
    </adm>
    <trd lng="be">&c_p;&c_o;&c_l;
    <klr>(&c_m;&c_u;&c_zh;&c_ch;&c_y;&c_n;&c_s;&c_k;&c_ib;,
    &c_zh;&c_a;&c_n;&c_o;&c_ch;&c_y;)</klr></trd>
    <trdgrp lng="ja">
      <trd>&#24615; [<ind>&#12379;&#12356;</ind>]</trd>
      <trd>&#24615;&#21029; [<ind>&#12379;&#12356;&#12409;&#12388;</ind>]</trd>
    </trdgrp>
    <trd lng="pl">p&lstroke;e&cacute;</trd>
    <trd lng="ru">&c_p;&c_o;&c_l; <klr
    tip="amb">(&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_j;,
    &c_zh;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
  </snc>
  <snc mrk="seks.0o.samseksularo">
    <dif>
      La tuto da sam<tld/>aj individuoj:
      <ekz>
        &ccirc;iuj plena&gcirc;aj lo&gcirc;antoj de amba&ubreve;
        <tld/>oj havas la rajton de vo&ccirc;donado
        <fnt><bib>FK</bib>, <lok>5, el la vivo kaj sciencoj,
        bagateloj</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_o;&c_l;
    <klr>(&c_m;&c_u;&c_zh;&c_ch;&c_y;&c_n;&c_s;&c_k;&c_ib;,
    &c_zh;&c_a;&c_n;&c_o;&c_ch;&c_y;)</klr></trd>
    <trd lng="pl">p&lstroke;e&cacute;</trd>
    <trd lng="ru">&c_p;&c_o;&c_l; <klr tip="amb">
    (&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_j;,
    &c_zh;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
  </snc>
  <snc mrk="seks.0o.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      La tuto da mensaj kaj fiziologiaj reagoj, faroj kaj emocioj
      rilataj al la <tld/>a vivo:
      <ekz>
        la&ubreve; la plej populara opinio, &gcirc;i &ndash; la <tld/>o
        &ndash;
        naskas &ccirc;ion: La plej altaj Filozofio, Scienco, Arto
        homaj eruptas el la vulkano de la <tld lit="S"/>o
        <fnt>
        <aut>A. Kris</aut>,
        <vrk>Scenaj obscenoj... drastaj kontrastoj</vrk>,
        <bib>LOdE</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="pl">seks</trd>
    <trd lng="ru">&c_s;&c_je;&c_k;&c_s;</trd>
  </snc>
  <snc mrk="seks.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <uzo tip="stl">ARK</uzo>
    <ref tip="dif" cel="genr.0o.GRA">genro<sncref/></ref>
    <ekz>
      grandegan malfacilecon prezentas por la lernanto la memorado de la
      <tld/>o de &ccirc;iu substantivo en la naturaj
      lingvoj<fnt>Z</fnt>;
    </ekz>
    <ekz>
      ekzistas tri gramatikaj <tld/>oj, la vira, la ina kaj la
      ne&ubreve;tra, kiu apartenas al la <ind>sen<tld/>a</ind>j aferoj.
      <trd lng="fr">asexu&eacute;</trd>
      <trdgrp lng="hu">
        <trd>ivartalan</trd>,
        <trd>nemi jelleg n&eacute;lk&uuml;li</trd>,
        <trd>aszexu&aacute;lis</trd>
      </trdgrp>
    </ekz>
    <trd lng="be">&c_r;&c_o;&c_d;
    <klr>(&c_g;&c_r;&c_a;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_y;)</klr>
    </trd>
    <trd lng="pl">rodzaj</trd>
    <trd lng="ru">&c_r;&c_o;&c_d;<klr
    tip="amb">(&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
  </snc>
  <trd lng="bg">&c_p;&c_o;&c_l;</trd>
  <trdgrp lng="cs">
    <trd>pohlav&iacute;</trd>,
    <trd>rod</trd>,
    <trd>sex</trd>,
    <trd>sexus</trd>
  </trdgrp>
  <trd lng="de">Geschlecht</trd>
  <trd lng="en">sex</trd>
  <trd lng="es">sexo</trd>
  <trd lng="fr">sexe</trd>
  <trdgrp lng="he">
    <trd>&mem;&yod;&fnun;</trd>,
    <trd>&mem;&#8503;&dalet;&resh;</trd>
  </trdgrp>
  <trd lng="hu">nem <klr tip="ind">(biol&oacute;giai, nyelvtani)</klr>
  </trd>
  <trd lng="id">seks</trd>
  <trd lng="io">sexuo</trd>
  <trd lng="it">sesso</trd>
  <trd lng="nl">geslacht</trd>
  <trdgrp lng="sk">
    <trd>pohlavie</trd>,
    <trd>rod</trd>,
    <trd>sex</trd>
  </trdgrp>
</drv>

<drv mrk="seks.0a">
  <kap><tld/>a</kap>
  <snc mrk="seks.0a.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Rilata al <tld/>o<sncref ref="seks.0o.BIO"/>:
      <ekz>
        <ind><tld/>a membro</ind><fnt>Z</fnt>
        <klr>(t.e. <ref cel="penis.0o">peniso</ref>)</klr>
        <trd lng="pl">cz&lstroke;onek</trd>
      </ekz>
    </dif>
    <trd lng="pl">p&lstroke;ciowy</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_v;&c_o;&c_j;</trd>
  </snc>
  <snc mrk="seks.0a.FIZL">
    <uzo tip="fak">FIZL</uzo>
    <dif>
      Rekte a&ubreve; malrekte rilata al
      <ref cel="kun.seks0igxi"><tld/>kuni&gcirc;o</ref>:
      <ekz>
        <tld/>aj interrilatoj;
      </ekz>
      <ekz>
        <tld/>a vivo;
      </ekz>
      <ekz>
        <tld/>aj fantazioj;
      </ekz>
      <ekz>
        <tld/>a amo.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>p&lstroke;ciowy</trd>,
      <trd>seksualny</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_l;&c_o;&c_v;&c_o;&c_j;</trd>,
      <trd>&c_s;&c_je;&c_k;&c_s;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_p;&c_o;&c_l;&c_o;&c_v;</trd>,
    <trd>&c_s;&c_je;&c_k;&c_s;&c_u;&c_a;&c_l;&c_je;&c_n;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pohlavn&iacute;</trd>,
    <trd>sexu&aacute;ln&iacute;</trd>
  </trdgrp>
  <trd lng="en">sexual</trd>
  <trd lng="es">sexual</trd>
  <trd lng="fr">sexuel</trd>
  <trd lng="id">seksual</trd>
  <trd lng="io">sexuala</trd>
  <trd lng="it">sessuale</trd>
  <trdgrp lng="ja">
    <trd>&#24615;&#12398; [<ind>&#12379;&#12356;&#12398;</ind>]</trd>,
    <trd>&#24615;&#30340;&#12394; [<ind>&#12379;&#12356;&#12390;&#12365;&#12394;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">seksueel</trd>
  <trd lng="sk">pohlavn&yacute;</trd>
</drv>

