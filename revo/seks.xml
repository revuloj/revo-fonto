<drv mrk="seks.0umilo">
  <kap><tld/>umilo,
  <var><kap><tld/>ilo</kap></var></kap>
  <uzo tip="stl">KOMUNE</uzo>
  <snc mrk="seks.0umilo.AMOR">
    <dif>
      Ekstera <tld/>organo:
      <ekz>
        &scirc;i <klr>...</klr> fingris lian balanon tiel sperte, ke lia
        
        <tld/>umilo stre&ccirc;i&gcirc;is
        <fnt><aut>C. Piron</aut>, <vrk>&Ccirc;u &scirc;i mortu
        tra-fike?</vrk>, 
        <lok>1982</lok></fnt>;
      </ekz>
      <ekz>
        Huni&ubreve;, kiu havas fiodoran <tld/>ilon, same prifriponas
        lin kaj 
        traktas lin kiel argilbulon la&ubreve;vole knedeblan
        <fnt>
          <aut>Lao She, trad. Wang Chongfang</aut>,
          <vrk><url
          ref="http://edu.i-lo.tarnow.pl/esp/lern/librejo/0026/">
          Kamelo &Scirc;jangzi</url></vrk>,
          <lok>1988</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_p;&c_o;&c_l;&c_o;&c_v; &c_o;&c_r;&c_g;&c_a;&c_n; (&c_i;&c_z;&c_p;&c_o;&c_l;&c_z;&c_v;&c_a;&c_n; &c_z;&c_a; &c_s;&c_je;&c_k;&c_s;)</trd>
  <trd lng="cs">pohlavn&iacute; org&aacute;n</trd>
  <trd lng="de">Geschlechtsorgan</trd>
  <trdgrp lng="en">
    <trd>sex organ</trd>,
    <trd>genitalia</trd>
  </trdgrp>
  <trd lng="es">&oacute;rgano sexual</trd>
  <trd lng="fr">organe sexuel</trd>
  <trdgrp lng="id">
    <trd><ind>organ</ind> genital</trd>,
    <trd><ind>organ</ind> seksual</trd>
  </trdgrp>
  <trd lng="it">organo <ind>sessuale</ind></trd>
  <trd lng="nl">geslachtsorgaan</trd> 
  <trd lng="pl">organ p&lstroke;ciowy</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_v;&c_o;&c_j;
  &c_o;&c_r;&c_g;&c_a;&c_n;</trd>
  <trd lng="sk">pohlavn&yacute; org&aacute;n</trd>
