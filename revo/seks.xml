  </trdgrp>
  <trdgrp lng="id">
    <trd>[<ind>kopulasi</ind>] berkopulasi</trd>,
    <trd>[<ind>setubuh</ind>] bersetubuh</trd>
  </trdgrp>
  <trd lng="it">fare <ind>sesso</ind></trd>
  <trdgrp lng="ja">
    <trd>&#24615;&#20132;&#12377;&#12427; [<ind>&#12379;&#12356;&#12371;&#12358;&#12377;&#12427;</ind>]</trd>,
    <trd>&#12475;&#12483;&#12463;&#12473;&#12377;&#12427; [<ind>&#12379;&#12387;&#12367;&#12377;&#12377;&#12427;</ind>]</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>uprawia&cacute; seks</trd>,
    <trd>kocha&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_a;&c_n;&c_i;&c_m;&c_a;&c_t;&c_mol;&c_s;&c_ja;
  &c_s;&c_je;&c_k;&c_s;&c_o;&c_m;</trd>
  <trd lng="sk">s&uacute;lo&zcaron;i&tcaron;</trd>
  <trd lng="tp">unpa</trd>
