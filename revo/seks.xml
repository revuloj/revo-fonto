<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>seks</rad>/o
</kap>
<drv mrk="seks.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="seks.0o.BIO">
    <dif>
      Biologia ecaro diferenciganta la viron de la ino:
      <ekz>
        <ind>vir<tld/>a</ind><fnt>Z</fnt>,
        <trdgrp lng="fr">
          <trd>masculin</trd>,
          <trd>m&acirc;le<klr tip="ind"> (adj.)</klr></trd>
        </trdgrp>
        <trd lng="nl">mannelijk</trd>
        <trd lng="ru">&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_g;&c_o; &c_p;&c_o;&c_l;&c_a;</trd>
      </ekz>
      <ekz>
        <ind>in<tld/>a</ind><fnt>Z</fnt>;
        <trdgrp lng="fr">
          <trd>femelle<klr tip="ind"> (adj.)</klr></trd>,
          <trd>f&eacute;minin</trd>
        </trdgrp>
        <trd lng="nl">vrouwelijk</trd>
        <trd lng="ru">&c_zh;&c_je;&c_n;&c_s;&c_k;&c_o;&c_g;&c_o; &c_p;&c_o;&c_l;&c_a;</trd>
      </ekz>
      <ekz>
        <ind>vir<tld/>ulo</ind><fnt>Z</fnt>,
        <trd lng="fr">m&acirc;le<klr tip="ind"> (subst.)</klr></trd>
        <trd lng="nl">mannetje</trd>
        <trd lng="ru">&c_s;&c_a;&c_m;&c_je;&c_c;</trd>
      </ekz>
      <ekz>
        <ind>vir<tld/>ulino</ind>;
        <trd lng="fr">femelle<klr tip="ind"> (subst.)</klr></trd>
        <trd lng="ru">&c_s;&c_a;&c_m;&c_k;&c_a;</trd>
      </ekz>
      <ekz>
        <ind>sam<tld/>a</ind>j lernejanoj;
        <trd lng="fr">de m&ecirc;me <ind>sexe</ind></trd>
        <trd lng="ru">&c_o;&c_d;&c_n;&c_o;&c_p;&c_o;&c_l;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        <tld/>kuni&gcirc;i<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <ind><tld/>matura</ind>.
        <trd lng="nl">geslachtsrijp</trd>
        <trd lng="ru">&c_p;&c_o;&c_l;&c_o;&c_v;&c_o;&c_z;&c_r;&c_je;&c_l;&c_y;&c_j;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun:
      <ref cel="genr.0o.BIO">genro (en biologio)</ref>.
    </refgrp>
    <adm>
      Mankas ekz BOT. <aut>MB</aut>
    </adm>
    <trd lng="es">sexo</trd>
    <trd lng="ru">&c_p;&c_o;&c_l; <klr tip="amb">(&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_j;, &c_zh;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
  </snc>
  <snc mrk="seks.0o.samseksularo">
    <dif>
      Tuta&jcirc;o de la sam<tld/>aj individuoj.
    </dif>
    <trd lng="es">sexo</trd>
    <trd lng="ru">&c_p;&c_o;&c_l; <klr tip="amb">(&c_m;&c_u;&c_zh;&c_s;&c_k;&c_o;&c_j;, &c_zh;&c_je;&c_n;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
  </snc>
  <snc mrk="seks.0o.GRA">
    <uzo tip="fak">GRA</uzo>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <!-- Formo, kiun ricevas la vortoj en kelkaj lingvoj por montri la
      <tld/>on: -->
      <ref tip="dif" cel="genr.0o.GRA">Genro</ref>:
      <ekz>
        grandegan malfacilecon prezentas por la lernanto la memorado de la
        <tld/>o de &ccirc;iu substantivo en la naturaj lingvoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        ekzistas tri gramatikaj <tld/>oj, la vira, la ina kaj la ne&ubreve;tra,
        kiu apartenas al la <ind>sen<tld/>a</ind>j aferoj.
        <trd lng="fr">asexu&eacute;</trd>
      </ekz>
    </dif>
    <trd lng="es">g&eacute;nero</trd>
    <trd lng="ru">&c_r;&c_o;&c_d; <klr tip="amb">(&c_g;&c_r;&c_a;&c_m;&c_m;&c_a;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;)</klr></trd>
  </snc>
  <trd lng="de">Geschlecht</trd>
  <trd lng="en">sex</trd>
  <trd lng="nl">geslacht</trd>
</drv>

<drv mrk="seks.sam0ema">
  <kap>sam<tld/>ema, sam<tld/>ama</kap>
  <snc>
    <dif>
      Sekse kaj emocie altirata de individuoj de la sama sekso:
      <ekz>
        por defendi amrilaton sam<tld/>aman
        <fnt><aut>Georgo Kama&ccirc;o</aut>:
        <vrk><url ref="&Monato;/rec/kmin.html">Berveling, poeto
        sentabua</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 12/1994</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gej.0a">geja</ref>,
      <ref cel="lesb.l0a">lesba</ref>.
    </refgrp>
  </snc>
  <trd lng="de">homosexuell</trd>
  <trd lng="en">homosexual</trd>
  <trd lng="fr">homosexuel<klr tip="ind"> (adj.)</klr></trd>
  <trd lng="nl">homoseksueel</trd>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_o;&c_s;&c_je;&c_k;&c_s;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="seks.sam0emulo">
  <kap>sam<tld/>emulo, sam<tld/>amulo</kap>
  <snc>
    <dif>
      <ref cel="seks.sam0ema">Sam<tld/>ama</ref> persono, precipe se viro:
      <ekz>
        en &quot;La sam<tld/>emulo&quot;, probable lia plej konata poemo, li
        uzas sintakson tre karakterizan
        <fnt><aut>Georgo Kama&ccirc;o</aut>:
        <vrk><url ref="&Monato;/rec/kmin.html">Berveling, poeto
        sentabua</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 12/1994</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="gej.0o">gejo.</ref>
  </snc>
  <trdgrp lng="de">
    <trd>Homosexueller</trd>,
    <trd>Schwuler</trd>
  </trdgrp>
  <trd lng="fr">homosexuel<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="nl">
    <trd>homoseksueel</trd>,
    <trd>homo</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_m;&c_o;&c_s;&c_je;&c_k;&c_s;&c_u;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="seks.sam0emulino">
  <kap>sam<tld/>emulino, sam<tld/>amulino</kap>
  <snc>
    <dif>
      <ref cel="seks.sam0ema">Sam<tld/>ama</ref> virino.
    </dif>
    <refgrp tip="sin">
      <ref cel="lesb.l0o">lesbo</ref>,
      <ref cel="lesb.l0anino.SOC">lesbanino</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Homosexuelle</trd>,
    <trd>Lesbe</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>homosexuelle<klr tip="ind"> (subst.)</klr></trd>,
    <trd>lesbienne</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lesbische</trd>,
    <trd>lesbienne</trd>
  </trdgrp>
  <trd lng="ru">&c_l;&c_je;&c_s;&c_b;&c_i;&c_ja;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
