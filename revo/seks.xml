<drv mrk="seks.ambaux0a">
  <kap>amba&ubreve;<tld/>a</kap>
  <snc mrk="seks.ambaux0a.BIO">
    <uzo tip="fak">BIO</uzo>
    <dif>
      Vira kaj ina, havanta amba&ubreve; seksojn;
      <ref tip="sin" cel="hermaf.0a.BIO">hermafrodita</ref>:
      <ekz>
        kelkaj arb-specioj estas amba&ubreve;<tld/>aj
        <fnt><bib>Viki</bib>,
        <lok>artikolo &leftquot;arbo&rightquot;, 2006-04-07</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="en">hermaphroditic</trd>
    <trdgrp lng="fr">
      <trd>androgyne<klr> (adj.)</klr></trd>,
      <trd>bissexu&eacute;</trd>,
      <trd>hermaphrodite<klr> (adj.)</klr></trd>
      <trd>mono&iuml;que</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd>biseksual</trd>,
      <trd>hermafrodit</trd>,
      <trd>monoseus</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>androgino<klr> (agg.)</klr></trd>,
      <trd>ermafrodita<klr> (agg.)</klr></trd>
    </trdgrp>
    <trd lng="ja">&#20001;&#24615;&#20855;&#26377;&#12398; [<ind>&#12426;&#12423;&#12358;&#12379;&#12356;&#12368;&#12422;&#12358;&#12398;</ind>]</trd>
