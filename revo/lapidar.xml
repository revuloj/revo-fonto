<art mrk="$Id: lapidar.xml,v 1.5 2022/03/22 18:26:50 revo Exp $">
<kap>
  <rad>lapidar</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lapidar.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="lapidar.0a.BELA">
    <dif>
      &Scirc;tongravurita:
      <ekz>
        kopioj de <tld/>aj surskriboj
        <fnt>
          <aut>Zhang Shuang, Wang Lihua</aut>,
          <vrk><url ref="http://www.espero.com.cn/se/txt/2011-09/23/content_392593.htm">
          Ekspozicio &leftquot;Lanting-Pavilono&rightquot;...</url></vrk>,
          <lok>espero.com.cn, 2011-09-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lapidar.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmultvorta,
      <ref tip="vid" cel="konciz.0a.KOMUNE">konciza</ref>,
      kiel estas kutime &scirc;tongravura&jcirc;oj:
      <ekz>
        li <tld/>e informas
        <fnt>
          <aut>H.-G. Kaiser</aut>,
          <vrk><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Poezio/cezar/la_cxielira_komando.html">
          La &ccirc;ielira komando</url></vrk>,
          <lok>1994</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>lapid&aacute;rn&iacute;</trd>,
    <trd>stru&ccaron;n&ecaron; v&yacute;sti&zcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lapidarny  <klr>(ryty w kamieniu)</klr></trd>,
    <trd>zwi&eogonek;z&lstroke;y</trd>,
    <trd>dosadny</trd>,
    <trd>wyrazisty</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>concis</trd>,
    <trd>explicit</trd>,
    <trd>lapidar</trd>
  </trdgrp>
  <trd lng="sk">stru&ccaron;ne v&yacute;sti&zcaron;n&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x430;&#x43F;&#x456;&#x434;&#x430;&#x440;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x43F;&#x435;&#x440;&#x435;&#x43D;. &#x43A;&#x43E;&#x440;&#x43E;&#x442;&#x43A;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x442;&#x438;&#x441;&#x43B;&#x438;&#x439;</trd>,
    <trd>&#x43B;&#x430;&#x43A;&#x43E;&#x43D;&#x456;&#x447;&#x43D;&#x438;&#x439; (&#x43F;&#x440;&#x43E; &#x441;&#x442;&#x438;&#x43B;&#x44C;)</trd>
  </trdgrp>
</drv>
</art>

