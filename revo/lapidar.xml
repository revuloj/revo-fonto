<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: lapidar.xml,v 1.7 2024/04/10 07:25:29 revo Exp $">
<kap>
  <rad>lapidar</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="lapidar.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="lapidar.0a.BELA">
    <dif>
      &Scirc;tongravurita:
      <ekz>
        kopioj de <tld/>aj surskriboj
        <fnt>
          <aut>Zhang Shuang, Wang Lihua</aut>,
          <vrk><url ref="http://www.espero.com.cn/se/txt/2011-09/23/content_392593.htm">
          Ekspozicio &leftquot;Lanting-Pavilono&rightquot;...</url></vrk>,
          <lok>espero.com.cn, 2011-09-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="lapidar.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Malmultvorta,
      <ref tip="vid" cel="konciz.0a.KOMUNE">konciza</ref>,
      kiel estas kutime &scirc;tongravura&jcirc;oj:
      <ekz>
        li <tld/>e informas
        <fnt>
          <aut>H.-G. Kaiser</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Poezio/cezar/la_cxielira_komando.html">
          La &ccirc;ielira komando</url></vrk>,
          <lok>1994</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lakon.0a">lakona</ref>,
      <ref cel="sobr.0a.FIG">sobra<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>lapidar</trd>,
      <trd>schn&ouml;rkellos</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_p;&c_ib;&c_d;&c_a;&c_r;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>lapid&aacute;rn&iacute;</trd>,
    <trd>stru&ccaron;n&ecaron; v&yacute;sti&zcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>lapidarny  <klr>(ryty w kamieniu)</klr></trd>,
    <trd>zwi&eogonek;z&lstroke;y</trd>,
    <trd>dosadny</trd>,
    <trd>wyrazisty</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>concis</trd>,
    <trd>explicit</trd>,
    <trd>lapidar</trd>
  </trdgrp>
  <trd lng="sk">stru&ccaron;ne v&yacute;sti&zcaron;n&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_l;&c_a;&c_p;&c_ib;&c_d;&c_a;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;. &c_k;&c_o;&c_r;&c_o;&c_t;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_t;&c_i;&c_s;&c_l;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_o;&c_n;&c_ib;&c_ch;&c_n;&c_i;&c_j; (&c_p;&c_r;&c_o; &c_s;&c_t;&c_i;&c_l;&c_mol;)</trd>
  </trdgrp>
</drv>
</art>

