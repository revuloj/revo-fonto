<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Romi</rad>/o
    </kap>
    <drv mrk="romi.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">HIS</uzo>
	<dif>
          Antikva <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>
          esti&gcirc;inta &ccirc;irka&ubreve; <ref tip="prt" cel="rom.0o">Romo</ref>,
          unue kiel re&gcirc;lando, poste respubliko, poste <ref tip="super"
            cel="imperi.0.o.monarkio">imperio</ref>.
	</dif>
      </snc>
      <trd lng="en">Ancient Rome</trd>
      <trdgrp lng="ru">
        <trd>&c_D;&c_r;&c_je;&c_v;&c_n;&c_je;&c_r;&c_i;&c_m;&c_s;&c_k;&c_o;&c_je; &c_g;&c_o;&c_s;&c_u;&c_d;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>,
        <trd>&c_D;&c_r;&c_je;&c_v;&c_n;&c_i;&c_j; &c_R;&c_i;&c_m;</trd></trdgrp>
    </drv>

    <drv mrk="romi.0ano">
      <kap><tld lit="r"/>ano</kap>
      <snc>
        <uzo tip="fak">HIS</uzo>
	<dif>
          Lo&gcirc;anto de <tld/>o:
          <ekz>
            ne &ccirc;iu <tld lit="r"/>ano estis <tld lit="r"/>a civitano.
          </ekz>
	</dif>
      </snc>
      <trd lng="ru">&c_d;&c_r;&c_je;&c_v;&c_n;&c_i;&c_j; <ind>&c_r;&c_i;&c_m;&c_l;&c_ja;&c_n;&c_i;&c_n;</ind></trd>
    </drv>

  </art>

