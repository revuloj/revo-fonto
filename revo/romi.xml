<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: romi.xml,v 1.14 2015/07/04 08:37:42 revo Exp $">
<kap>
<rad>Romi</rad>/o
</kap>
<drv mrk="romi.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>
      esti&gcirc;inta &ccirc;irka&ubreve; <ref tip="prt"
      cel="rom.0o">Romo</ref>,
      unue kiel re&gcirc;lando, poste respubliko, poste <ref tip="super"
      cel="imperi.0o.monarkio">imperio</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_t;&c_a;&c_r;&c_a;&c_zh;&c_y;&c_t;&c_n;&c_y; &c_R;&c_y;&c_m;</trd>
  <trd lng="cs">Římská říše</trd>
  <trdgrp lng="de">
    <trd>Rom</trd>,
    <trd>Altes Rom</trd>,
    <trd>R&ouml;merreich</trd>
  </trdgrp>
  <trd lng="en">Ancient Rome</trd>
  <trd lng="es">Antigua Roma</trd>
  <trdgrp lng="fr">
  <trd>Rome antique</trd>,
  <trd>Empire romain</trd>
</trdgrp>
  <trd lng="hu">R&oacute;ma<klr tip="amb"> (&oacute;kori &aacute;llam)</klr></trd>
  <trd lng="nl">het oude <ind>Rome</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_D;&c_r;&c_je;&c_v;&c_n;&c_je;&c_r;&c_i;&c_m;&c_s;&c_k;&c_o;&c_je;
    &c_g;&c_o;&c_s;&c_u;&c_d;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_D;&c_r;&c_je;&c_v;&c_n;&c_i;&c_j; &c_R;&c_i;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">Rímska ríša</trd>
</drv>

<drv mrk="romi.0ano">
  <kap><tld lit="r"/>ano</kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <dif>
      Lo&gcirc;anto de <tld/>o:
      <ekz>
        ne &ccirc;iu <tld lit="r"/>ano estis <tld lit="r"/>a civitano.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_a;&c_zh;&c_y;&c_t;&c_n;&c_y; &c_r;&c_y;&c_m;&c_l;&c_ja;&c_n;&c_ib;&c_n;</trd>
  <trd lng="de">R&ouml;mer</trd>
  <trd lng="es">romano</trd>
  <trd lng="fr">Romain</trd>
  <trd lng="hu">r&oacute;mai</trd>
  <trd lng="nl">Romein<klr tip="ind"> (inw. v.h. oude Rome)</klr></trd>
  <trd lng="ru">&c_d;&c_r;&c_je;&c_v;&c_n;&c_i;&c_j;
  <ind>&c_r;&c_i;&c_m;&c_l;&c_ja;&c_n;&c_i;&c_n;</ind></trd>
</drv>
</art>
