<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: romi.xml,v 1.18 2022/05/04 19:19:16 revo Exp $">
<kap>
<rad>Romi</rad>/o
</kap>
<drv mrk="romi.0o">
  <kap><tld/>o</kap>
  <snc mrk="romi.0o.HIS">
    <uzo tip="fak">HIS</uzo>
    <dif>
      Antikva <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>
      esti&gcirc;inta &ccirc;irka&ubreve; <ref tip="prt" cel="rom.0o">Romo</ref>,
      unue kiel re&gcirc;lando, poste respubliko, poste <ref tip="super"
      cel="imperi.0o.monarkio">imperio</ref>:
      <ekz>
        kiam la <tld lit="r"/>a imperiestro Tiberio faris iam gramatikan eraron, la
        gramatikisto Marcelo atentigis lin pri tio
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>koin&ecirc;</nom> <klr>[&#8230;]</klr> fari&gcirc;is kun la tempo la granda komuna lingvo de la tuta
        Orienta <tld/>a Imperio kaj el &gcirc;i originas la moderna greka lingvo
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la literaturaj idiomoj de Grekio, <tld/>o, Hindio, Francio, Hispanio estas
        prefere konsidereblaj kiel artefaritaj ol kiel naturaj formoj de la parolo
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la latina <klr>[&#8230;]</klr>, origine la lingvo de Latio, <klr>[&#8230;]</klr> etendi&gcirc;is
        al najbaraj teritorioj, poste superregis la aliajn lingvojn de Italio, kaj
        fine, portata de la <tld lit="r"/>aj legioj, konkeris la plej grandan parton
        de la Okcidenta Mondo
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la klasika <tld/>o la tuta filozofio kaj scienco estis greklingvaj
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 2 (52)</lok>
        </fnt>;
      </ekz>
      <ekz>
        iujn sportajn movojn, similajn al la nunaj futbalaj, oni praktikis anka&ubreve; en
        antikvaj &Ccirc;inio, Grekio kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2004/008319.html">Multviza&gcirc;a futbalo</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_t;&c_a;&c_r;&c_a;&c_zh;&c_y;&c_t;&c_n;&c_y; &c_R;&c_y;&c_m;</trd>
  <trd lng="cs">&Rcaron;&iacute;msk&aacute; &rcaron;&iacute;&scaron;e</trd>
  <trdgrp lng="de">
    <trd>Rom</trd>,
    <trd>Altes Rom</trd>,
    <trd>R&ouml;merreich</trd>
  </trdgrp>
  <trd lng="en">Ancient Rome</trd>
  <trd lng="es">Antigua Roma</trd>
  <trdgrp lng="fr">
  <trd>Rome antique</trd>,
  <trd>Empire romain</trd>
</trdgrp>
  <trd lng="hu">R&oacute;ma<klr tip="amb"> (&oacute;kori &aacute;llam)</klr></trd>
  <trd lng="nl">het oude <ind>Rome</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_D;&c_r;&c_je;&c_v;&c_n;&c_je;&c_r;&c_i;&c_m;&c_s;&c_k;&c_o;&c_je;
    &c_g;&c_o;&c_s;&c_u;&c_d;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_D;&c_r;&c_je;&c_v;&c_n;&c_i;&c_j; &c_R;&c_i;&c_m;</trd>
  </trdgrp>
  <trd lng="sk">R&iacute;mska r&iacute;&scaron;a</trd>
  <trdgrp lng="uk">
    <trd>&c_R;&c_i;&c_m;</trd>,
    <trd>&c_R;&c_i;&c_m;&c_s;&c_mol;&c_k;&c_a; &c_ib;&c_m;&c_p;&c_je;&c_r;&c_ib;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="romi.0ano">
  <kap><tld lit="r"/>ano</kap>
  <snc>
    <uzo tip="fak">HIS</uzo>
    <dif>
      <ref tip="super" cel="civit.0ano">Civitano</ref> a&ubreve; lo&gcirc;anto de <tld/>o:
      <ekz>
        la prokuratoro la&ubreve;te diris, levante sian kalikon: &ndash; por ni, por vi, Cezaro,
        patro de la <tld lit="r"/>anoj
        <fnt>
          <bib>MkM</bib>
          <lok>25</lok>
        </fnt>;
      </ekz>
      <ekz>
        tiu tradicio perdi&gcirc;is, kiam la <tld lit="r"/>anoj detruis la jerusaleman templon
        anta&ubreve; dumil jaroj
        <fnt>
          <bib>BonaLingvo</bib>
          <lok>6. Ies Malfido, Fido Mia</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="r"/>anoj konis tri specojn da proprieta&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2005/008698.php">Evitenda evoluo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_r;&c_a;&c_zh;&c_y;&c_t;&c_n;&c_y;
  &c_r;&c_y;&c_m;&c_l;&c_ja;&c_n;&c_ib;&c_n;</trd>
  <trd lng="de">R&ouml;mer</trd>
  <trd lng="en">Roman<klr> (person from ancient Rome)</klr></trd>
  <trd lng="es">romano</trd>
  <trd lng="fr">Romain</trd>
  <trd lng="hu">r&oacute;mai</trd>
  <trd lng="nl">Romein<klr tip="ind"> (inw. v.h. oude Rome)</klr></trd>
  <trd lng="ru">&c_d;&c_r;&c_je;&c_v;&c_n;&c_i;&c_j;
  <ind>&c_r;&c_i;&c_m;&c_l;&c_ja;&c_n;&c_i;&c_n;</ind></trd>
</drv>
</art>
