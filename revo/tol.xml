<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tol</rad>/o
</kap>
<drv mrk="tol.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="tol.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Teksa&jcirc;o farita el lino a&ubreve; kanabo:
      <ekz>
        Pantelejeva anka&ubreve; sendis al lia edzino tri pecojn da <tld/>o
        <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno 11a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_a;&c_t;&c_n;&c_o;</trd>
  <trd lng="de">Leinen</trd>
  <trd lng="fr">toile<klr tip="amb"> (mati&egrave;re)</klr></trd>
  <trd lng="hu">v&aacute;szon</trd>
  <trd lng="nl">linnen</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_l;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_o;&c_t;&c_n;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="tol.0ajxo">
  <kap><ofc>*</ofc><tld/>a&jcirc;o</kap>
  <snc mrk="tol.0ajxo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Tuko el <tld/>o:
      <ekz>
        &ccirc;emizojn, kolumojn, manumojn kaj ceterajn similajn objektojn ni
        nomas <tld/>a&jcirc;o, kvankam ili ne &ccirc;iam estas faritaj el
        <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 35</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_l;&c_ib;&c_z;&c_n;&c_a;</trd>
  <trd lng="de">W&auml;sche</trd>
  <trd lng="fr">toile<klr tip="amb"> (pi&egrave;ce de tissu)</klr></trd>
  <trd lng="hu">feh&eacute;rnem&udblac;</trd>
  <trd lng="nl">linnengoed</trd>
  <trd lng="ru">&c_b;&c_je;&c_l;&c_mol;&c_jo;</trd>
</drv>

<drv mrk="tol.lak0o">
  <kap>lak<tld/>o</kap>
  <snc mrk="tol.lak0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <ref tip="dif" cel="tol.vaks0o.TEKS">vaks<tld/>o</ref>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_r;&c_a;&c_t;&c_a;</trd>
  <trd lng="fr">toile cir&eacute;e</trd>
  <trd lng="hu">viaszosv&aacute;szon</trd>
  <trd lng="nl">zeildoek</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_jo;&c_n;&c_k;&c_a;</trd>
</drv>

<drv mrk="tol.vaks0o">
  <kap>vaks<tld/>o</kap>
  <snc mrk="tol.vaks0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="T"/>o &scirc;mirita sur unu flanko per lako, por ke &gcirc;i estu
      nepenetrebla de akvo:
      <ekz>
        apud la muro inter la fenestroj staris kanapo, kovrita per nigra
        sen&scirc;eligita vaks<tld/>o
        <fnt><bib>FK</bib>, <lok>4, rakontoj, Arturo, (rakonto de V.
        Devjatnin), 4</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lin.0oleatolo0oleumo">linoleumo</ref>,
      <ref cel="tapisx">vakstapi&scirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_r;&c_a;&c_t;&c_a;</trd>
  <trd lng="fr">toile cir&eacute;e</trd>
  <trd lng="hu">viaszosv&aacute;szon</trd>
  <trd lng="nl">zeildoek</trd>
  <trd lng="pt">encerado</trd>
  <trd lng="ru">&c_k;&c_l;&c_je;&c_jo;&c_n;&c_k;&c_a;</trd>
</drv>
</art>
