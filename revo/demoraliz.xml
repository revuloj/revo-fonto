<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: demoraliz.xml,v 1.11 2023/10/14 11:26:53 revo Exp $">
<kap>
  <rad>demoraliz</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="demoraliz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="demoraliz.0i.PSI">
    <dif>
      Sen<ref tip="ant" cel="kuragx.0igi">kura&gcirc;igi</ref>, 
      senfervorigi:
      <ekz>
        kial la vundo scii, ke neniam du fratanimoj
        stabile kunfandi&gcirc;os <klr>...</klr> <tld/>us &scirc;in
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_m;&c_a;&c_r;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;
  &c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">demoralizovat</trd>
  <trdgrp lng="fr">
    <trd>abattre<klr> (moralement)</klr></trd>,
    <trd>d&eacute;moraliser</trd>
  </trdgrp>
  <trd lng="he">&#x5DC;&#x5B0;&#x5E2;&#x5B7;&#x5E8;&#x5B0;&#x5E2;&#x5B5;&#x5E8; &#x5D0;&#x5B5;&#x5EA; &#x5D4;&#x5B7;&#x5DE;&#x5D5;&#x5B9;&#x5E8;&#x5B8;&#x5DC;</trd>
  <trdgrp lng="ja">
    <trd>&#x58EB;&#x6C17;&#x3092;&#x304F;&#x3058;&#x304F;  <pr>&#x3057;&#x304D;&#x3092;&#x304F;&#x3058;&#x304F;</pr></trd>,
    <trd>&#x610F;&#x6C17;&#x963B;&#x55AA;&#x3055;&#x305B;&#x308B;  <pr>&#x3044;&#x304D;&#x306F;&#x3070;&#x3082;&#x3055;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>demoralizowa&cacute;</trd>,
    <trd>odbiera&cacute; odwag&eogonek;</trd>,
    <trd>zniech&eogonek;ca&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>desanimar</trd>,
    <trd>acovardar</trd>,
    <trd>desmoralizar</trd>,
    <trd>aniquilar</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>demoralizova&#x165;</trd>,
    <trd>kazi&#x165; mor&#xE1;lku</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x434;&#x435;&#x43C;&#x43E;&#x440;&#x430;&#x43B;&#x456;&#x437;&#x43E;&#x432;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x43A;&#x43B;&#x430;&#x434;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x431;&#x435;&#x449;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="demoraliz.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="demoraliz.0o.PSI">
    <dif>
      Malinstigado, ruinigo de ies volo a&ubreve; kura&gcirc;o:
      <ekz>
        &gcirc;i estas <tld/>o de la tuta psikologio de l' infano
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_m;&c_a;&c_r;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_c;
  &c_y;&c_ja;</trd>
  <trd lng="cs">demoralizace</trd>
  <trd lng="fr">d&eacute;moralisation</trd>
  <trdgrp lng="pl">
    <trd>demoralizacja</trd>,
    <trd>zniech&eogonek;cenie</trd>
  </trdgrp>
  <trd lng="sk">demoraliz&#xE1;cia</trd>
</drv>
</art>

