<drv mrk="demoraliz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="demoraliz.0i.PSI">
    <dif>
      Sen<ref tip="ant" cel="kuragx.0igi">kura&gcirc;igi</ref>, 
      senfervorigi:
      <ekz>
        kial la vundo scii, ke neniam du fratanimoj
        stabile kunfandi&gcirc;os <klr>...</klr> <tld/>us &scirc;in
        <fnt><aut>S. Larbar</aut>, <vrk>Karuseloj</vrk>, 
        <lok>1987</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_m;&c_a;&c_r;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;
  &c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">demoralizovat</trd>
  <trdgrp lng="fr">
    <trd>abattre<klr> (moralement)</klr></trd>,
    <trd>d&eacute;moraliser</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>demoralizowa&cacute;</trd>,
    <trd>odbiera&cacute; odwag&eogonek;</trd>,
    <trd>zniech&eogonek;ca&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>desanimar</trd>,
    <trd>acovardar</trd>,
    <trd>desmoralizar</trd>,
    <trd>aniquilar</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>demoralizova콘</trd>,
    <trd>kazi콘 mor치lku</trd>
  </trdgrp>
</drv>

<drv mrk="demoraliz.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="demoraliz.0o.PSI">
    <dif>
      Malinstigado, ruinigo de ies volo a&ubreve; kura&gcirc;o:
      <ekz>
        &gcirc;i estas <tld/>o de la tuta psikologio de l' infano
        <fnt><bib>Metrop</bib></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_m;&c_a;&c_r;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_c;
  &c_y;&c_ja;</trd>
  <trd lng="cs">demoralizace</trd>
  <trd lng="fr">d&eacute;moralisation</trd>
  <trdgrp lng="pl">
    <trd>demoralizacja</trd>,
    <trd>zniech&eogonek;cenie</trd>
  </trdgrp>
  <trd lng="sk">demoraliz치cia</trd>
</drv>
