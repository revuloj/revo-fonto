<vortaro>
<art mrk="$Id: vivise.xml,v 1.16 2018/08/15 17:10:18 revo Exp $">
<kap>
  <ofc>6</ofc> <rad>vivisekci</rad>/o
  <fnt><bib>FK</bib></fnt> 
</kap>
<drv mrk="vivise.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="operac.0o.KIR">Operacio<sncref/></ref>, 
      praktikata sur vivanta animalo por studi la fiziologiajn
      fenomenojn:
      <ekz>
        memvola <tld/>o: du Amerikaj kuracistoj <klr>[&#8230;]</klr> publikigis <klr>[&#8230;]</klr>
        anoncon <klr>[por ser&ccirc;i iun por]</klr> farado de truo en la stomako kaj fermado
        &gcirc;in poste per vitro, por farado de esploroj super la funkciado de la stomako
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas disputita pro la bestoprotektaj le&gcirc;oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=325942">Vivisekcio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_v;&c_ib;&c_s;&c_e;&c_k;&c_c;&c_y;&c_ja;</trd>
  <trd lng="ca">vivisecci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>pitv&aacute;n&iacute; za &zcaron;iva</trd>,
    <trd>vivisekce</trd>
  </trdgrp>
  <trd lng="de">Vivisektion</trd>
  <trd lng="el">&zeta;&omega;&omikron;&tau;&omikron;&my;&jota_ton;&alfa;</trd>
  <trd lng="en">vivisection</trd>
  <trd lng="es">vivisecci&oacute;n</trd>
  <trd lng="fr">vivisection</trd>
  <trd lng="hu">&eacute;lveboncol&aacute;s</trd>
  <trd lng="nl">vivisectie</trd>
  <trd lng="pt">vivisse(c)&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_v;&c_i;&c_v;&c_i;&c_s;&c_je;&c_k;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sk">vivisekcia</trd>
</drv>
</art>
