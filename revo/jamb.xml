<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jamb.xml,v 1.31 2024/02/17 21:20:28 revo Exp $">
<kap>
  <rad>jamb</rad>/o
  <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="jamb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref> konsistanta
      el du silaboj, el kiuj la unua estas mallonga
      a&ubreve; senakcenta, kaj la dua longa a&ubreve; akcenta; simb.
      /&ypsilon;&#8208;/:
      <ekz>
        &leftquot;ornam'&rightquot; konsistigas <tld/>on;
      </ekz>
      <ekz>
        ne kredu nome, ke en <tld/>' aplombe / tiktakas teda takt'
        seninterrompe
        <fnt><bib>PGl</bib>, <lok>p. 100</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_m;&c_b;</trd>
  <trd lng="ca">iambe</trd>
  <trdgrp lng="cs">
    <trd>jamb</trd>,
    <trd>jambus</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Jambe</trd>,
    <trd>Jambus</trd>
  </trdgrp>
  <trd lng="en">iamb</trd>
  <trd lng="es">yambo</trd>
  <trdgrp lng="fr">
    <trd>iambe</trd>,
    <trd>&iuml;ambe</trd>
  </trdgrp>
  <trd lng="hu">jambus</trd>
  <trd lng="it">giambo</trd>
  <trdgrp lng="ja">
    <trd>&#x77ED;&#38263;&#x683C; <pr>&#12383;&#12435;&#12385;&#12423;&#12358;&#12363;&#12367;</pr></trd>,
    <trd>&#x5F31;&#x5F37;&#x683C; <pr>&#12452;&#12450;&#12531;&#12508;&#12473;</pr></trd>,
    <trd>&#25233;&#x63DA;&#x683C;  <pr>&#12424;&#x304F;&#12424;&#12358;&#x304B;&#x304F;</pr></trd>
  </trdgrp>
  <trd lng="nl">jambe</trd>
  <trd lng="pl">jamb</trd>
  <trdgrp lng="pt">
    <trd>iambo</trd>,
    <trd>jambo</trd>
  </trdgrp>
  <trd lng="ru">&c_ja;&c_m;&c_b;</trd>
  <trd lng="sk">jamb</trd>
  <trd lng="sv">jamb</trd>
  <trd lng="uk">&#x44F;&#x43C;&c_b;</trd>
</drv>

<drv mrk="jamb.hxor0o">
  <kap>&hcirc;or<tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref>, konsistanta
      el du mallongaj a&ubreve; senakcentaj silaboj metitaj inter du
      longaj a&ubreve; akcentaj silaboj; simb.
      /&#8208;&ypsilon;&ypsilon;&#8208;/:
      <ekz>
        &hcirc;or<tld/>o estas kunigo de trokeo kun <tld/>o;
      </ekz>
      <ekz>
        <klr>[pro la <ref cel="glit.0o.licenco">glito</ref>]</klr>
        anstata&ubreve; la du komencaj <tld/>oj staras &hcirc;or<tld/>a
        piedo
        <fnt><bib>LSF</bib>, <lok>p. 125</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_r;&c_y;&c_ja;&c_m;&c_b;</trd>
  <trd lng="ca">coriambe</trd>
  <trd lng="en">choriamb</trd>
  <trd lng="es">coriambo</trd>
  <trd lng="fr">chor&iuml;ambe</trd>
  <trd lng="hu">chorijambus</trd>
  <trd lng="ja">&#38263;&#x77ED;&#x77ED;&#38263;&#x683C;  <pr>&#12385;&#12423;&#12358;&#x305F;&#12435;&#x77ED;&#38263;&#x683C;</pr></trd>
  <trd lng="ru">&c_h;&c_o;&c_r;&c_i;&c_ja;&c_m;&c_b;</trd>
  <trd lng="it">coriambo</trd>
</drv>

<drv mrk="jamb.n0o">
  <kap>n-<tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="metr.0o.POE">Metro</ref>, &ccirc;efe
      konsistanta el n <tld/>oj; simb.
      /&ypsilon;&#8208;/<sup>n</sup>(&ypsilon;):
      <ekz>
        <ind>kvin<tld/>o</ind>.
        <trd lng="en">iambic pentameter</trd>
        <trd lng="fr">pentam&egrave;tre &iuml;ambique</trd>
        <trd lng="ru">
        &c_p;&c_ja;&c_t;&c_i;&c_s;&c_t;&c_o;&c_p;&c_n;&c_y;&c_j;
        &c_ja;&c_m;&c_b;
        </trd>
        <trd lng="it">pentapodia giambica</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">n-&c_s;&c_t;&c_o;&c_p;&c_n;&c_y; <ind>&c_ja;&c_m;&c_b;</ind></trd>
  <trd lng="ca">n-metre i&agrave;mbic</trd>
  <trd lng="ru">n-&c_s;&c_t;&c_o;&c_p;&c_n;&c_y;&c_j;
  <ind>&c_ja;&c_m;&c_b;</ind></trd>
  <trdgrp lng="it">
    <trd>-podia <ind>giambica</ind></trd>,
    <trd>-metro <ind>giambico</ind></trd>
  </trdgrp>
</drv>
</art>
