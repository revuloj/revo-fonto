<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>jamb</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="jamb.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref> konsistanta
      el du silaboj, el kiuj la unua estas mallonga
      a&ubreve; senakcenta, kaj la dua longa a&ubreve; akcenta; simb. 
      /&ypsilon;&dash;/:
      <ekz>
        "ornam'" konsistigas <tld/>on;
      </ekz>
      <ekz>
        ne kredu nome, ke en <tld/>' aplombe / tiktakas teda takt'
        seninterrompe
        <fnt><aut>&K;, Waringhien, Bernard</aut><vrk>Parnasa Gvidlibro; Pisa, 1984</vrk><lok>p. 100</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Jambe</trd>,
    <trd>Jambus</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>iambe</trd>,
    <trd>&iuml;ambe</trd></trdgrp> 
  <trd lng="ru">&c_ja;&c_m;&c_b;</trd>
  <trd lng="sv">jamb</trd>
</drv>
<drv mrk="jamb.hxor0o">
  <kap>&hcirc;or<tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref>, konsistanta
      el du mallongaj a&ubreve; senakcentaj silaboj metitaj inter du longaj 
      a&ubreve; akcentaj silaboj; simb. /&dash;&ypsilon;&ypsilon;&dash;/:
      <ekz>
        &hcirc;or<tld/>o estas kunigo de trokeo kun <tld/>o;
      </ekz>
      <ekz>
        <klr>[pro la <ref cel="glit.0o.licenco">glito</ref>]</klr> 
        anstata&ubreve; la du komencaj <tld/>oj staras &hcirc;or<tld/>a piedo
        <fnt><aut>&K;</aut><vrk>Lingvo, Stilo, Formo; Budapest, 1931</vrk><lok>p. 125</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">chor&iuml;ambe</trd>
</drv>
<drv mrk="jamb.n0o">
  <kap>n-<tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="metr.0o.POE">Metro</ref>, &ccirc;efe konsistanta el
      n <tld/>oj; simb. /&ypsilon;&dash;/<sup>n</sup>(&ypsilon;):
      <ekz>
        <ind>kvin<tld/>o</ind>.
        <trd lng="fr">pentam&egrave;tre &iuml;ambique</trd>
      </ekz>
    </dif>
  </snc>
</drv>
</art>
