<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>filistr</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="filist.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Tiu, kiu ne estas studento de universitato, aŭ ne militisto; fiburĝo.
    </dif>
    <trdgrp lng="de">
     <trd>Philister</trd>,
     <trd>Zivilist</trd></trdgrp>
     <trd lng="hu">filiszter</trd>
  </snc>
  <snc num="2">
    <dif>
      Homo sen idealo, kiu zorgas nur pri la ĉiutagaj, vulgaraj aferoj,
      k estas indiferenta pri la altaj, noblaj ideoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="proz.0a">proza</ref>,
      <ref cel="poezi">senpoezia</ref>.
    </refgrp>
    <trd lng="en">philistine</trd>
  </snc>
  <adm>
  Enkonduki kaj kontrastigi: filisxto. Aldoni ekz kaj plibonigi la
  francan tradukon. <aut>MB</aut>
  </adm>
  <trd lng="de">Spießbürger</trd>
  <trd lng="fr">philistin<klr tip="ind"> (personne vulgaire,
conformiste)</klr></trd>
  <trd lng="hu">nyárspolgár</trd>
  <trdgrp lng="pt">
    <trd>filisteu</trd>,
    <trd>burguês</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>филистер</trd>,
    <trd>обыватель</trd>,
    <trd>мещанин</trd>
  </trdgrp>
</drv>
</art>
