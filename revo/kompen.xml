<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kompendi</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="kompen.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Mallongigo de la tuta&jcirc;o de scienco a&ubreve; doktrino. 
    </dif>
    <refgrp tip="sin">
      <ref cel="sum.re0o">resumo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>compendium</trd>,
    <trd><klr>concise but comprehensive </klr>abridgment</trd>
  </trdgrp>
  <trd lng="nl">compendium</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_je;&c_n;&c_d;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_je;&c_n;&c_d;&c_i;&c_u;&c_m;</trd>
  </trdgrp>
</drv>
</art>
