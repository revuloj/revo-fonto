<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>kompendi</rad>/o <fnt>Z</fnt></kap>
<drv mrk="kompen.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="verk.0o.propra">Verko</ref> kiu resumas la tuton de scienco a&ubreve; doktrino.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="epitom.0o">epitomo</ref>,
    <ref cel="kompil.0ajxo">kompila&jcirc;o</ref>
  </refgrp>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_e;&c_n;&c_d;&c_y;&c_ju;&c_m;</trd>
  <trdgrp lng="de">
     <trd>Kompendium</trd>,
     <trd>Abriss</trd>,
     <trd>Handbuch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>compendium</trd>,
    <trd><klr>concise but comprehensive </klr>abridgment</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abr&eacute;g&eacute;</trd>,
    <trd>compendium</trd>,
    <trd>introduction<klr> (ouvrage)</klr></trd>
  </trdgrp>
  <trd lng="nl">compendium</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_je;&c_n;&c_d;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_je;&c_n;&c_d;&c_i;&c_u;&c_m;</trd>
  </trdgrp>
</drv>
</art>
