<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kompen.xml,v 1.18 2022/07/12 18:35:43 revo Exp $">
<kap>
  <rad>kompendi</rad>/o
</kap>
<drv mrk="kompen.0o">
  <kap><tld/>o</kap>
  <snc mrk="kompen.0o.verko">
    <dif>
      <ref tip="super" cel="verk.0o.propra">Verko</ref> resumanta
      la tuton de scienco a&ubreve; doktrino:
      <ekz>
        la <ctl>Dida&hcirc;eo</ctl> <klr>[&#8230;]</klr> estas, por tiel diri, 
        <tld/>o pri eklezia vivo
        <fnt><aut>trad. G. Berveling</aut>, <vrk><url ref="http://www.esperanto.gr/lit/els/did.pdf">la dida&hcirc;eo</url>, 
        enkonduko. - IABO, 1979</vrk></fnt>;
      </ekz>
      <ekz>
        kompendio de historiaj dokumentoj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="https://www.monato.be/2003/008022.html"
              >Tridek jaroj da sa&gcirc;o</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="K"/>o de la Kompara Gramatiko de la Hindogermanaj Lingvoj
        <fnt>
          <bib>Ret</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mil-kaj-unua provo sistemigi kaj <tld/>igi la vivon de Kristo surbaze de la
        Sankta Skribo
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 7 (69)</lok>
        </fnt>;
      </ekz>
      <ekz>
        griza <tld/>o, nelerta kaj neinteresa nombrado de vendoteknikoj kun nekonvinkaj
        ekzemploj
        <fnt>
          <bib>Monato</bib>
          <aut>Sergeo Dereviannykh</aut>
          <vrk><url ref="https://www.monato.be/2004/007562.html"
              >Kiu instigas nin al farado, kiel plej multe faras</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="artiko.0aro">artikolaro</ref>,
      <ref cel="epitom.0o">epitomo</ref>,
      <ref cel="kompil.0ajxo">kompila&jcirc;o</ref>,
      <ref cel="libr.man0o">manlibro</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_p;&c_e;&c_n;&c_d;&c_y;&c_ju;&c_m;</trd>
  <trd lng="ca">compendi</trd>
  <trdgrp lng="de">
    <trd>Kompendium</trd>,
    <trd>Abriss</trd>,
    <trd>Handbuch</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>compendium</trd>,
    <trd><klr>concise but comprehensive </klr>abridgment</trd>
  </trdgrp>
  <trd lng="es">compendio</trd>
  <trdgrp lng="fr">
    <trd>compendium</trd>,
    <trd>abr&eacute;g&eacute;</trd>,
    <trd>aide-m&eacute;moire</trd>,
    <trd>&eacute;pitom&eacute;</trd>,
    <trd>introduction<klr tip="ind"> (ouvrage)</klr></trd>,
    <trd>m&eacute;mento</trd>,
    <trd>vade-mecum</trd>
  </trdgrp>
  <trd lng="nl">compendium</trd>
  <trdgrp lng="pl">
    <trd>kompendium</trd>,
    <trd>konspekt</trd>,
    <trd>skr&oacute;t</trd>,
    <trd>zarys</trd>
  </trdgrp>   
  <trdgrp lng="pt">
    <trd>comp&ecirc;ndio</trd>,
    <trd>resumo</trd>,
    <trd>ep&iacute;tome</trd>,
    <trd>manual</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>compendiu</trd>,
    <trd>sinopsis</trd>,
    <trd>schi&#539;&abreve;</trd>
  </trdgrp>   
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_je;&c_n;&c_d;&c_i;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_m;&c_p;&c_je;&c_n;&c_d;&c_i;&c_u;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>manu&aacute;l</trd>,
    <trd>pr&iacute;ru&ccaron;ka</trd>,
    <trd>rukov&auml;&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>kompendium</trd>,
    <trd>p&rcaron;&iacute;ru&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_k;&c_o;&c_m;&c_p;&c_je;&c_n;&c_d;&c_ib;&c_j;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_o;&c_t;&c_k;&c_i;&c_j; &c_p;&c_o;&c_s;&c_ib;&c_b;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_k;&c_o;&c_r;&c_o;&c_t;&c_k;&c_i;&c_j; &c_p;&c_ib;&c_d;&c_r;&c_u;&c_ch;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
</drv>
</art>
