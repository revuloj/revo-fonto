<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>Kalvin</rad>/o <fnt>PIV1</fnt></kap>
<drv mrk="kalvin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <uzo tip="fak">KRI</uzo>
  <snc>
    <dif>
      Franclingve: Jean Chauvin, 1509-1564.
      Francdevena teologiisto svisa,
      pli konata sub la latinigita nomo
      Johannes <trd lng="la">Calvinus</trd>
      (refrancigita kiel Jean <trd lng="fr">Calvin</trd>),
      unu el la &ccirc;efoj de <ref cel="reform.0o">Reformacio</ref>,
      kies instruo formis bazon de pluraj konfesioj protestantaj.
    </dif>
  </snc>
    <trd lng="be">&c_K;&c_a;&c_l;&c_mol;&c_v;&c_ib;&c_n;<klr tip="ind"> (&c_Zh;&c_a;&c_n;)</klr></trd>
    <trd lng="en">Calvin<klr tip="ind"> (John)</klr></trd>
    <trd lng="es">Calvino<klr tip="ind"> (Juan)</klr></trd>
    <trd lng="hu">K&aacute;lvin<klr tip="ind"> (J&aacute;nos)</klr></trd>
    <trd lng="nl">Calvijn<klr tip="ind"> (Jan)</klr></trd>
    <trd lng="ru">&c_K;&c_a;&c_l;&c_mol;&c_v;&c_i;&c_n;<klr tip="ind"> (&c_Zh;&c_a;&c_n;)</klr></trd>
</drv>

<drv mrk="kalvin.k0ano">
  <kap><tld lit="k"/>ano</kap>
  <snc>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Membro de <tld lit="k"/>isma konfesio:
      <ekz>
        luteranoj kaj <tld lit="k"/>anoj estas kristanoj
        <fnt><bib>F</bib>, <lok>&FE;37.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_v;&c_ib;&c_n;&c_ib;&c_s;&c_t;</trd>
  <trd lng="es">calvinista</trd>
  <trd lng="fr">calviniste</trd>
  <trd lng="hu">k&aacute;lvinista</trd>
  <trd lng="nl">calvinist</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_mol;&c_v;&c_i;&c_n;&c_i;&c_s;&c_t;</trd>
</drv>

<drv mrk="kalvin.k0ismo">
  <kap><tld lit="k"/>ismo</kap>
  <snc mrk="kalvin.0ismo.doktrino">
    <uzo tip="fak">KRI</uzo>
    <dif>
      Religia instruo de <ref cel="kalvin.0o"><tld/>o</ref>,
      bazita sur <ref cel="destin.antaux0ismo">anta&ubreve;destinismo</ref>
      kaj nerezistebla graco.
    </dif>
  </snc>
  <snc mrk="kalvin.0ismo.konfesioj">
    <uzo tip="fak">KRI</uzo>
    <dif>
      &Ccirc;iu el la protestantaj konfesioj, bazitaj sur la teologiaj
      kaj ekleziologiaj principoj de 
      <ref cel="kalvin.0o"><tld/>o</ref>, ekz-e
      <ref tip="sub" cel="presbi1.0ismo">presbiterianismo</ref>.
    </dif>
    <ref tip="super" cel="protes1.0ismo">protestantismo</ref>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_mol;&c_v;&c_ib;&c_n;&c_ib;&c_z;&c_m;</trd>
  <trd lng="hu">k&aacute;lvinizmus</trd>
  <trd lng="en">Calvinism</trd>
  <trd lng="es">calvinismo</trd>
  <trd lng="fr">calvinisme</trd>
  <trd lng="nl">calvinisme</trd>
  <trd lng="ru">&c_k;&c_a;&c_l;&c_mol;&c_v;&c_i;&c_n;&c_i;&c_z;&c_m;</trd>
</drv>
</art>
