<drv mrk="sklav.0igi">
  <kap>
    <tld/>igi
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sklav.0igi.KOMUNE">
    <dif>
      Igi <tld/>o, submeti al <tld/>eco:
      <ekz>
        afrik-usonano, <tld/>igita en la sama plantejo kiel kelkaj de la novalvenintoj,
        rakontis, ke <klr>[&#x2026;]</klr>
        <fnt>
          <aut>S. A. Diouf, trad. F. Lappe kaj A. Pe&ccirc;&euml;nki</aut>
          <vrk><url ref="https://unesdoc.unesco.org/ark:/48223/pf0000370977_epo">El la holdoj de
          Clotilda al Africatown</url></vrk>
          <lok>Unesko-kuriero, 2019-10 &gcirc;is 12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_n;&c_ja;&c_v;&c_o;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ja;&c_v;&c_o;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">r&eacute;duire en <ind>esclavage</ind></trd>
  <trd lng="ja">&#x5974;&#x96B7;&#x306B;&#x3059;&#x308B;  <pr>&#x3069;&#x308C;&#x3044;&#x306B;&#x3059;&#x308B;</pr></trd>
  <trdgrp lng="pl">
    <trd>zniewala&cacute;</trd>,
    <trd>niewoli&cacute;</trd>,
    <trd>zaprzeda&cacute;</trd>
  </trdgrp>
  <trd lng="uk">&#x43F;&#x43E;&#x43D;&c_je;&c_v;&#x43E;&#x43B;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

