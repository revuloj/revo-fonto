<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>sklav</rad>/o
    </kap>

    <drv mrk="sklav.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc mrk="sklav.0o.socia">
	<dif>
	  Homo posedanta nenian socian rajton, kiu estas proprieto de
	  alia persono (mastro, sinjoro), lin militkaptinta, rabinta a&ubreve;
	  a&ccirc;etinta:
	  <ekz>
	    kaj li <klr>(Abram)</klr> havis &scirc;afojn kaj bovojn kaj
	    azenojn kaj <tld/>ojn kaj <tld/>inojn kaj azeninojn kaj
	    kamelojn
	    <fnt><bib>MT</bib>, <lok>&Gen; 12:16</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <snc mrk="sklav.0o.BIB">
	<uzo tip="fak">BIB</uzo>
	<uzo tip="fak">REL</uzo>
	<dif>
	  Kredanto rilate al sia(j) dio(j):
	  <ekz>
	    Ho Vi, mia Dio, helpu Vian <tld/>on, kiu fidas Vin
	    <fnt><bib>MT</bib>, <lok>&Psa; 86:2</lok></fnt>;
	  </ekz><ekz>
	    Mi trovis Davidon, Mian <tld/>on,
	    Per Mia sankta oleo Mi lin &scirc;miris
	    <fnt><bib>MT</bib>, <lok>&Psa; 89:20</lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <snc>
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Homo kontra&ubreve;vole a&ubreve; propravole senkondi&ccirc;e obeanta al la voloj de iu
	  alia:
	  <ekz>
	    kiu prunteprenas, tiu estas <tld/>o de la pruntedoninto<fnt>Z</fnt>
	  </ekz>
	</dif>
	<refgrp tip="sin">
	  <ref cel="lake.0o">lakeo</ref>,
	  <ref cel="traban.0o">trabanto</ref>.
	</refgrp>
      </snc>
      <snc>
	<uzo tip="stl">FIG</uzo>
	<dif>
	  Homo nekapabla rezisti al ia pasio, deziro a&ubreve; inklino:
	  <ekz>
	    ni ne devas esti <tld/>oj de niaj pasioj.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="be">
	<trd>&c_r;&c_a;&c_b;</trd>, 
	<trd>&c_n;&c_ja;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
      </trdgrp>
      <trd lng="de">Sklave</trd>
      <trd lng="en">slave</trd>
      <trd lng="nl">slaaf</trd>
      <trd lng="ru">&c_r;&c_a;&c_b;</trd>
    </drv>

    <drv mrk="sklav.0e">
      <kap><tld/>e</kap>
      <snc>
	<dif>
	  Humile kaj obeeme kiel <tld/>o:
	  <ekz>
	    <tld/>e kopii ion.
	  </ekz>
	</dif>
      </snc>
      <trd lng="be">&c_r;&c_a;&c_b;&c_s;&c_k;&c_ib;</trd>
      <trd lng="de">sklavisch</trd>
      <trd lng="nl">slaafs</trd>
      <trd lng="ru">&c_r;&c_a;&c_b;&c_s;&c_k;&c_i;</trd>
    </drv>

    <drv mrk="sklav.0i">
      <kap><tld/>i</kap>
      <gra><vspec>ntr</vspec></gra>
      <snc>
	<dif>
	  Esti <tld/>o:
	  <ekz>
	    <tld/>i al iu;
	  </ekz><ekz>
	    kie <tld/>o regadon havas, tie mastro, balda&ubreve; <tld/>as<fnt>Z</fnt>.
	  </ekz>
	</dif>
      </snc>
    </drv>

    <drv mrk="sklav.0eco">
      <kap><tld/>eco</kap>
      <snc>
	<dif>
	  Stato de <tld/>o:
	  <ekz>
	    teni iun en <tld/>eco<fnt>Z</fnt>;
	  </ekz><ekz>
	    alkonduki al <tld/>eco.
	  </ekz>
	</dif>
	<refgrp tip="sin">
	  <ref cel="servut.0o">servuto</ref>.
	</refgrp>
      </snc>
      <trd lng="be">&c_r;&c_a;&c_b;&c_s;&c_t;&c_v;&c_a;</trd>
      <trd lng="de">Sklaverei</trd>
      <trd lng="ru">&c_r;&c_a;&c_b;&c_s;&c_t;&c_v;&c_o;</trd>
    </drv>

    <drv mrk="sklav.eks0o">
      <kap>eks<tld/>o</kap>
      <snc>
	<dif>
	  Liberigita <tld/>o:
	  <ekz>
	    la saman ideon havis Petronius en la 1-a jc., montrante
	    la parolmanieron de eks<tld/>oj
	    <fnt><lok><url
	    ref="http://www.uni-paderborn.de/extern/fb/2/Kyb.Paed/INT/03-int.htm"
		  >Enkonduka lernolibro de interlingvistiko</url></lok></fnt>.
	  </ekz>
	</dif>
      </snc>
      <trdgrp lng="la">
	<trd>libertus</trd>,
	<trd>libertinus</trd>
      </trdgrp>
      <trd lng="ru">&c_v;&c_o;&c_l;&c_mol;&c_n;&c_o;&c_o;&c_t;&c_p;&c_u;&c_shch;&c_je;&c_n;&c_n;&c_i;&c_k;</trd>
    </drv>


  </art>
