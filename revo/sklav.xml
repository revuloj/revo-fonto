<drv mrk="sklav.0e">
  <kap><tld/>e</kap>
  <snc mrk="sklav.0e.KOMUNE">
    <dif>
      Humile kaj obeeme kiel <tld/>o:
      <ekz>
	<tld/>e kopii ion;
      </ekz>
      <ekz>
        mi ne havas la devon <tld/>e submeti min al viaj deziroj
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> amis &scirc;in <klr>[&#8230;]</klr> <tld/>e
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ri&ccirc;uloj pro la oro &parsl; Kisas <tld/>e manojn potenculajn
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        Se la natur' nin tenas <tld/>e, &parsl; Pli <tld/>e tenas nin kutim'
        <fnt>
          <bib>LSB</bib>
          <lok>Homoj &Ccirc;e L' Marbordo</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">sklavisch</trd>
  <trd lng="hu">szolgai m&oacute;don</trd>
  <trdgrp lng="ja">
    <trd>&#22900;&#38583;&#12398;&#12424;&#12358;&#12395;  <pr>&#12393;&#12428;&#12356;&#12398;&#12424;&#12358;&#12395;</pr></trd>,
    <trd>&#35328;&#12356;&#12394;&#12426;&#12395;  <pr>&#12356;&#12356;&#12394;&#12426;&#12395;</pr></trd>
  </trdgrp>
  <trd lng="nl">slaafs</trd>
  <trd lng="pl">niewolniczo</trd>
  <trd lng="ru">&c_r;&c_a;&c_b;&c_s;&c_k;&c_i;</trd>
  <trd lng="uk">&c_r;&c_a;&c_b;&c_s;&c_mol;&c_k;&c_i;</trd>
</drv>

<drv mrk="sklav.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sklav.0i.KOMUNE">
    <dif>
      Esti <tld/>o:
      <ekz>
        <klr>[konduki propran entreprenon]</klr> estas tute alia afero ol <tld/>i al la
        societo por hora salajro
        <fnt>
          <bib>HsT</bib>
          <lok>Sia propra mastro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni nuntempe <tld/>as al e&ccirc; pli da tempregulaj ordoniloj
        <fnt>
          <bib>Kon11</bib>
          <lok>Tiel la mondo...</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homoj <klr>[&#8230;]</klr> amas <tld/>i, ili fieras pri la rajto esti soldato kaj
        perdi la vivon per mitraloj, ili amas esti skur&gcirc;ataj kaj torturataj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 29</lok>
        </fnt>;
      </ekz>
      <ekz>
        kie <tld/>' regadon havas, tie mastro balda&ubreve; <tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_y;&c_c;&c_mol; <ind>&c_r;&c_a;&c_b;&c_o;&c_m;</ind></trd>,
    <trd>&c_b;&c_y;&c_c;&c_mol; &c_u; <ind>&c_r;&c_a;&c_b;&c_s;&c_t;&c_v;&c_je;</ind></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t otrokem</trd>,
    <trd>otro&ccaron;it</trd>
  </trdgrp>
  <trd lng="de">ein <ind>Sklave</ind>ndasein f&uuml;hren</trd>
  <trd lng="hu">rabszolga</trd>
  <trd lng="ja">&#22900;&#38583;&#12392;&#12375;&#12390;&#20181;&#12360;&#12427;  <pr>&#12393;&#12428;&#12356;&#12392;&#12375;&#12390;&#12388;&#12363;&#12360;&#12427;</pr></trd>
  <trd lng="pl">by&cacute; <ind>niewolnikiem</ind></trd>
  <trd lng="ru">&c_b;&c_y;&c_t;&c_mol; <ind>&c_r;&c_a;&c_b;&c_o;&c_m;</ind></trd>
  <trdgrp lng="sk">
    <trd>by&tcaron; otrokom</trd>,
    <trd>otro&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_t;&c_i; &c_r;&c_a;&c_b;&c_o;&c_m;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_b;&c_u;&c_v;&c_a;&c_t;&c_i; &c_v; &c_r;&c_a;&c_b;&c_s;&c_t;&c_v;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="sklav.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="sklav.0eco.POL">
    <dif>
      Stato de <tld/>o:
      <ekz>
        Mi elirigos vin el sub la jugo de Egiptujo, kaj Mi liberigos vin el ilia
        <tld/>eco
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 6:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> starigis al si estron, por reveni ribele al sia <tld/>eco
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 9:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        re&gcirc;o <klr>[&#8230;]</klr> per unu ektremo de la palpebro de&jcirc;etas la homojn de la supro
        de l' honoroj en la senfunda&jcirc;on de la <tld/>eco
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="servut.0o">servuto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_s;&c_t;&c_v;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>esclavatge</trd>,
    <trd>esclavitud</trd>
  </trdgrp>
  <trd lng="cs">otroctv&iacute;</trd>
  <trd lng="de">Sklaverei <klr>(individuell)</klr></trd>
  <trd lng="fr">esclavage</trd>
  <trd lng="hu">rabszolgas&aacute;g</trd>
  <trd lng="id">per<ind>budak</ind>an</trd>
  <trdgrp lng="ja">
    <trd>&#38583;&#23646;  <pr>&#12428;&#12356;&#12382;&#12367;</pr></trd>,
    <trd>&#22900;&#38583;&#29366;&#24907;  <pr>&#12393;&#12428;&#12356;&#12376;&#12423;&#12358;&#12383;&#12356;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>niewola</trd>,
    <trd>jasyr</trd>,
    <trd>zniewolenie</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_b;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="sk">
    <trd>otroctvo</trd>,
    <trd>poroba</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_a;&c_b;&c_s;&c_t;&c_v;&c_o;</trd>
  <trdgrp lng="zh">
