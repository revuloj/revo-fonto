<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: limona.xml,v 1.23 2024/01/15 19:02:15 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>limonad</rad>/o
</kap>
<drv mrk="limona.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:trinka&jcirc;oj" cel="trink.0ajxo">Trinka&jcirc;o</ref> farata el citrona
      suko <klr>(a&ubreve; alia acida fruktosuko)</klr>, akvo kaj sukero:
      <ekz>
        servu vi la <tld/>on al la gastoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Feni&ccirc;ka</nom> servis al li, kiel ordinare, alportis buljonon,
        <tld/>on, mole kuiritajn ovojn
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 24a, p..
        166a</lok></fnt>;
      </ekz>
      <ekz>
        <nom>Beata</nom> iras al la bufedo kaj revenas kun du glasoj da oran&gcirc;a <tld/>o
        <fnt>
          <bib>FSI</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_m;&c_a;&c_n;&c_a;&c_d;</trd>
  <trd lng="ca">llimonada</trd>
  <trd lng="cs">limon&aacute;da</trd>
  <trd lng="da">limonade</trd>
  <trdgrp lng="de">
    <trd>Limonade</trd>,
    <trd>Limo</trd>,
    <trd>s&uuml;&szlig;er <ind>Sprudel</ind></trd>
  </trdgrp>
  <trd lng="el">&lambda;&epsilon;&my;&omikron;&ny;&alfa_ton;&delta;&alfa;</trd>
  <trd lng="en">lemonade</trd>
  <trd lng="es">limonada</trd>
  <trd lng="fr">limonade</trd>
  <trd lng="hu">limon&aacute;d&eacute;</trd>
  <trd lng="id">limun</trd>
  <trd lng="io">limonado</trd>
  <trd lng="it">limonata</trd>
  <trd lng="ja">&#x30EC;&#x30E2;&#x30CD;&#x30FC;&#x30C9; <pr>&#x308C;&#x3082;&#x306D;&#x30FC;&#x3069;</pr></trd>
  <trd lng="ko">&#xB808;&#xBAA8;&#xB124;&#xC774;&#xB4DC;</trd>
  <trdgrp lng="nl">
    <trd>citroenlimonade</trd>,
    <trd>limonade</trd>
  </trdgrp>
  <trd lng="oc">limonada</trd>
  <trd lng="pl">lemoniada</trd>
  <trd lng="pt">limonada</trd>
  <trd lng="ru">&c_l;&c_i;&c_m;&c_o;&c_n;&c_a;&c_d;</trd>
  <trd lng="sk">limon&aacute;da</trd>
  <trd lng="tr">limonata</trd>
  <trd lng="uk">&#x43B;&#x438;&#x43C;&#x43E;&#x43D;&#x430;&#x434;</trd>
  <trdgrp lng="zh">
    <trd>&#x67E0;&#x6AAC;&#x6C34; <pr>n&#xED;ngm&#xE9;ngshu&#x1D0;</pr></trd>,
    <trd>&#x6AB8;&#x6AAC;&#x6C34; <pr>n&#xED;ngm&#xE9;ngshu&#x1D0;</pr></trd>,
    <trd>&#x6C7D;&#x6C34; <pr>q&#xEC;shu&#x1D0;</pr></trd>
  </trdgrp>
</drv>
</art>
