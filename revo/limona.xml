<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>limonad</rad>/o
</kap>
<drv mrk="limona.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="trink.0ajxo">Trinka&jcirc;o</ref> farata el citrona
      suko, akvo kaj sukero:
      <ekz>
        Feni&ccirc;ka servis al li, kiel ordinare, alportis buljonon, <tld/>on,
        mole kuiritajn ovojn
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 24a, p. 166a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Limonade</trd>,
    <trd>Limo</trd>,
    <trd>s&uuml;&szlig;er <ind>Sprudel</ind></trd>
  </trdgrp>
  <trd lng="el">&lambda;&epsilon;&my;&omikron;&ny;&alfa_ton;&delta;&alfa;</trd>
  <trd lng="en">lemonade</trd>
  <trd lng="es">limonada</trd>
  <trdgrp lng="fr">
    <trd>limonade</trd>
  </trdgrp>
  <trd lng="nl">limonade</trd>
  <trd lng="pl">lemoneada</trd>
  <trd lng="pt">limonada</trd>
  <trd lng="ru">&c_l;&c_i;&c_m;&c_o;&c_n;&c_a;&c_d;</trd>
</drv>
</art>
