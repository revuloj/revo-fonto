<kap>
  <rad>klus</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="klus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SHI</uzo>
  <snc mrk="klus.0o.SHI">
    <dif>
      &Ccirc;iu el la fa&ubreve;koj &ccirc;e l' vando de <ref tip="malprt" cel="sxip.0o">&scirc;ipo</ref>,
      tra kiu pasas kabloj kaj &ccirc;enoj:
      <ekz>
        la &ccirc;eno de la ankro trairas la ankro<tld/>on;
      </ekz>
      <ekz>
        &ccirc;e grandaj &scirc;ipoj <tld/>oj ofte estas ekipitaj per ruloj a&ubreve; pulioj por protekti la
        kablojn kaj la &scirc;ipvandon.
      </ekz>
    </dif>
  </snc>
  <bld lok="&WCU;/thumb/9/96/Ankerkl&uuml;se.jpg/360px-Ankerkl&uuml;se.jpg" prm="CC BY-SA 3.0">ankro<tld/>o
    <fnt>
      <aut>Obersachse, propra verko</aut>
      <lok><url ref="&WCW;/File:Ankerkl%C3%BCse.jpg">&WCW;/File:Ankerkl&uuml;se.jpg</url></lok>
    </fnt>
  </bld>
  <trd lng="be">&c_k;&c_l;&c_ju;&c_z;</trd>
  <trd lng="cs">kluz</trd>
  <trd lng="de">Kl&uuml;se</trd>
  <trdgrp lng="en">
    <trd>hawse hole</trd>,
    <trd>hawse pipe</trd>
  </trdgrp>
  <trd lng="fr">&eacute;cubier</trd>
  <trdgrp lng="ja">
    <trd>&#37672;&#37782;&#23380; <pr>&#12403;&#12423;&#12358;&#12373;&#65381;&#12371;&#12358;</pr></trd>,
    <trd>&#27178;&#35895; <pr>&#12424;&#12371;&#12383;&#12395;</pr></trd>
  </trdgrp>
  <trd lng="nl">kluisgat</trd>
  <trd lng="pl">kluza</trd>
  <trd lng="ru">&c_k;&c_l;&c_ju;&c_z;</trd>
  <trd lng="sk">provn&yacute; otvor <klr>(pre kotevn&uacute; re&tcaron;az)</klr></trd>
  <trdgrp lng="uk">
    <trd>&c_sh;&c_l;&c_ju;&c_z;</trd>,
    <trd>&c_k;&c_l;&c_ju;&c_z;</trd>
  </trdgrp>
  <trd lng="zh">&#37672;&#37832;&#23380; <pr>m&aacute;oli&agrave;nk&#466;ng</pr></trd>
</drv>
