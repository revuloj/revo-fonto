<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pacifik.xml,v 1.26 2024/02/16 19:24:09 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>Pacifik</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="pacifik.P0o">
  <kap><tld/>o</kap>
  <snc mrk="pacifik.P0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o">oceano</ref>
      inter Azio, Oceanio kaj Ameriko; anka&ubreve; nomata
      <ref tip="sin" cel="ocean.Silenta_O0o">Silenta Oceano, Pacifika Oceano</ref>:
      <ekz>
        vulkanoj el la eksplodema tipo trovi&gcirc;as la&ubreve;longe de
        zono &ccirc;irka&ubreve; <tld/>o
        <fnt>
        <aut>Evgeni Georgiev</aut>
        <vrk><url ref="&Monato;/2001/007234.html">Vulkanoj</url></vrk>
        <bib>Monato</bib>
        <lok>2001/02, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
      en vintro malvarmaj ventoj venas de Siberio kaj en regionoj la&ubreve; <tld/>o atakas sekaj ventoj
      <fnt>
        <aut>Hori Jasuo</aut>
        <vrk><url ref="&Monato;/2009/009842.php">Masko-manio</url></vrk>
        <bib>Monato</bib>
        <lok>2009/09, p. 13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_C;&c_ib;&c_h;&c_ib; <ind>&c_a;&c_k;&c_ib;&c_ja;&c_n;</ind></trd>
  <trd lng="bg">&c_t;&c_i;&c_h;&c_i; &c_o;&c_k;&c_je;&c_a;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>Pacifick&yacute; oce&aacute;n</trd>,
    <trd>Tich&yacute; oce&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stiller <ind>Ozean</ind></trd>,
    <trd>Pazifik</trd>
  </trdgrp>
  <trd lng="en">Pacific Ocean</trd>
  <trd lng="es">oc&eacute;ano <ind>Pac&iacute;fico</ind></trd>
  <trd lng="fr">Oc&eacute;an <ind>pacifique</ind></trd>
  <trd lng="hu">Csendes-&oacute;ce&aacute;n</trd>
  <trd lng="ja">&#x592A;&#x5E73;&#x6D0B;  <pr>&#x305F;&#x3044;&#x3078;&#x3044;&#x3088;&#x3046;</pr></trd>
  <trd lng="nl">Stille <ind>Oceaan</ind></trd>
  <trdgrp lng="pl">
    <trd>Pacyfik</trd>,
    <trd>Ocean Spokojny</trd>
  </trdgrp>
  <trdgrp lng="pt">
   <trd>oceano Pac&iacute;fico</trd>
  </trdgrp>
  <trd lng="ru">&c_T;&c_i;&c_h;&c_i;&c_j; &c_o;&c_k;&c_je;&c_a;&c_n;</trd>
  <trd lng="sk">Tich&yacute; oce&aacute;n</trd>
  <trd lng="uk">&c_T;&c_i;&c_h;&c_i;&c_j; &#x43E;&#x43A;&c_je;&c_a;&#x43D;</trd>
  <trd lng="zh">&#x592A;&#x5E73;&#x6D0B; <pr>T&#xE0;ip&#xED;ngY&#xE1;ng</pr></trd>
</drv>

<drv mrk="pacifik.trans0a">
  <kap>
    trans<tld lit="p"/>a
  </kap>
  <snc mrk="pacifik.trans0a.TUR">
    <dif>
      Transiranta <tld/>on:
      <ekz>
        dum tio &ccirc;i ri&ccirc;igas nian scion pri la vojoj de ebla trans<tld lit="p"/>a kultura
        kontakto, &gcirc;i ne povas &scirc;anceli la firme starantan kredon de la pli multaj
        a&ubreve;toritatuloj, ke la Polinezianoj devenis el Azio
        <fnt>
          <aut>C. C. Cummingsmith</aut>
          <vrk><url ref="https://scienca-revuo.info/article/707">KonTiki kaj transpacifika
          migrado</url></vrk>
          <lok>Scienca Revuo, 1951 (3,11)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="pacifik.trans0a.GEOG">
    <dif>
      Situanta aliflanke de <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_a;&c_n;&c_s;&c_c;&c_ib;&c_h;&c_a;&c_a;&c_k;&c_ib;&c_ja;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="fr">transpacifique</trd>
</drv>
</art>
