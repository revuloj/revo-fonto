<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>evangeli</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="evange.0o">
  <kap><tld/>o</kap>
  <snc mrk="evange.0o.BIB">
    <uzo tip="fak">BIB</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Rakonto pri la vivo, agoj kaj predikoj de
      <ref tip="vid" cel="jesu.0o.homo">Jesuo</ref>
      en iu el la kvar unuaj libroj de la
      <ref tip="malprt" cel="testam.Nova0o">Nova Testamento</ref>
      a&ubreve; el <ref tip="vid" cel="apokri.0o">apokrifo</ref>.
    </dif>
    <trd lng="de">Verk&uuml;ndigung (Christi)</trd>
  </snc>
  <snc mrk="evange.0o.KRI">
    <uzo tip="fak">KRI</uzo>
    <dif>
      La instruoj de Jesuo.
    </dif>
    <trd lng="de">Heilsbotschaft Christi</trd>
  </snc>
  <snc mrk="evange.0o.nova.testamento">
    <uzo tip="stl">EVI</uzo>
    <dif>
      La tuta Nova Testamento.
    </dif>
    <trd lng="de">Neues Testament</trd>
  </snc>
  <snc mrk="evange.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Instruo pri iu doktrino prezentata kiel nediskutebla vero.
    </dif>
  </snc>
  <trd
  lng="be">&c_e;&c_v;&c_a;&c_n;&c_g;&c_je;&c_l;&c_mol;&c_l;&c_je;</trd>
  <trd lng="de">Evangelium</trd>
  <trdgrp lng="en">
    <trd>gospel</trd>,
    <trd>evangel</trd>
  </trdgrp>
  <trd lng="es">evangelio</trd>
  <trd lng="fr">&eacute;vangile</trd>
  <trd lng="hu">evang&eacute;lium</trd>
  <trd lng="ca">evangeli</trd>
  <trd lng="nl">evangelie</trd>
  <trd lng="pl">Ewangelia</trd>
  <trd lng="pt">evangelho</trd>
  <trd lng="ru">&c_je;&c_v;&c_a;&c_n;&c_g;&c_je;&c_l;&c_i;&c_je;</trd>
</drv>

<drv mrk="evange.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Konforma al <tld/>o:
      <ekz>
        <tld/>a moralo;
      </ekz>
      <ekz>
        <tld/>a tono, rigardo.
      </ekz>
    </dif>
    <trd
    lng="be">&c_e;&c_v;&c_a;&c_n;&c_g;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
    <trd lng="hu">evang&eacute;liumi</trd>
    <trd lng="nl">evangelisch</trd>
    <trd lng="pl">ewangeliczny</trd>
    <trd
    lng="ru">&c_je;&c_v;&c_a;&c_n;&c_g;&c_je;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <snc>
    <dif>
      <ref tip="dif" cel="protes1.0a">protestanta</ref>
    </dif>
    <trd
    lng="be">&c_e;&c_v;&c_a;&c_n;&c_g;&c_je;&c_l;&c_ib;&c_ch;&c_n;&c_y;</trd>
     <trd lng="hu">evang&eacute;likus</trd>
     <trd lng="nl">protestants</trd>
     <trd lng="pl">ewangelicki</trd>
     <trd
     lng="ru">&c_je;&c_v;&c_a;&c_n;&c_g;&c_je;&c_l;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
  <trd lng="de">evangelisch</trd>
  <trd lng="en">evangelical</trd>
  <trd lng="es">evang&eacute;lico</trd>
  <trd lng="fr">&eacute;vang&eacute;lique</trd>
  <trd lng="ca">evang&egrave;lic</trd>
  <trd lng="pt">evang&eacute;lico</trd>
</drv>

<drv mrk="evange.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="evange.0isto.BIB">
    <uzo tip="fak">BIB</uzo>   
    <dif>
      <ref tip="vid" cel="evange.0o"><tld lit="E"/></ref>a&ubreve;toro:
      <ekz>
        <ctl>sed &ccirc;e la sunlevi&gcirc;o</ctl>, ne matene kiel estas tradukite, 
        kaj tiu indiko de la <tld/>isto estas grava, <ctl>Li revenis en 
        la templon kaj la tuta popolo venis al li</ctl>
        <fnt>
          <aut>A. Maggi</aut>,
          <vrk><url
          ref="http://www.studibiblici.it/ESPERANTO/Omelie/21_03_10rev.pdf">
          Kvina Karesma Diman&ccirc;o</url></vrk>,
          <lok>2010-03-21</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="evange.0isto.predikisto">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Kristana <ref tip="dif" cel="predik.0isto.REL">predikisto</ref>
      <ekz>
        tele-<tld/>istoj la&ubreve;das individuan sukceson
        <fnt>
          <aut>H. Tammam, P. Haenni, trad. M. Leon</aut>,
          <vrk><url ref="http://eo.mondediplo.com/article829.html">
          &Scirc;ika islamo kaj la egipta bur&gcirc;aro</url></vrk>, 
          <lok>le Monde diplomatique, 2003-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&eacute;vang&eacute;liste<klr> (subst.)</klr></trd>
  <trd lng="ca">evangelista</trd>
</drv>
</art>
