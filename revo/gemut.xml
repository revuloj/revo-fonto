<art mrk="$Id: gemut.xml,v 1.7 2025/03/30 07:25:15 afido Exp $">
<kap>
  <rad>gemut</rad>/a <fnt><bib>SPIV</bib></fnt>
</kap>

<drv mrk="gemut.0a">
  <kap><tld/>a</kap>
  <snc mrk="gemut.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="ujutn.0a">Ujutna</ref>:
      <ekz>
        &leftquot;mi nenion preparis&rightquot; &dash; &leftquot;ne gravas&rightquot;, diris la <tld/>a dano, &leftquot;sendube vi povos ion
        diri!&rightquot;
        <fnt>
          <bib>Kiso</bib>
          <lok>Tombeja Arabesko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la varmru&gcirc;a ardo de kandellumo dancigis viglajn ombrojn de la du geamantoj sur
        la muroj en la <tld/>a &ccirc;ambro
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2013/011110.php">Splito</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="agrabl.0a">agrabla</ref>,
      <ref cel="hejm.0eca">hejmeca</ref>,
      <ref cel="komfor.0a">komforta</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>agrabla</ke>, <ke>hejmeca</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_u;&c_t;&c_u;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_c;&c_ib;&c_h;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">gem&uuml;tlich</trd>
  <trd lng="en">cosy</trd>
  <trdgrp lng="fr">
    <trd>cosy</trd>,
    <trd>douillet</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_t;&c_i;&c_sh;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_d;&#1086;&#1084;&c_a;&c_sh;&#1085;&c_ib;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_je;&c_v;&#1085;&c_i;&c_j;</trd>,
    <trd>&#1087;&c_r;&c_i;&c_v;&c_ib;&c_t;&#1085;&c_i;&c_j;</trd>,
    <trd>&c_d;&#1086;&c_b;&c_r;&#1086;&c_d;&c_u;&c_sh;&#1085;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20174;&#20174;&#23481;&#23481; <pr>c&#243;ngc&#243;ngr&#243;ngr&#243;ng</pr></trd>,
    <trd>&#24736;&#28982;&#33258;&#24471; <pr>y&#333;ur&#225;nz&#236;d&#233;</pr></trd>,
    <trd>&#24930;&#24736;&#24736; <pr>m&#224;ny&#333;uy&#333;u</pr></trd>,
    <trd>&#24812;&#24847; <pr>qi&#232;y&#236;</pr></trd>,
    <trd>&#26155; <pr>x&#249;</pr></trd>,
    <trd>&#38543;&#24847; <pr>su&#237;y&#236;</pr></trd>
  </trdgrp>
</drv>
</art>

