<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gemut.xml,v 1.8 2025/08/11 16:41:38 afido Exp $">
<kap>
  <rad>gemut</rad>/a <fnt><bib>SPIV</bib></fnt>
</kap>

<drv mrk="gemut.0a">
  <kap><tld/>a</kap>
  <snc mrk="gemut.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="ujutn.0a">Ujutna</ref>:
      <ekz>
        &leftquot;mi nenion preparis&rightquot; &dash; &leftquot;ne gravas&rightquot;, diris la <tld/>a dano, &leftquot;sendube vi povos ion
        diri!&rightquot;
        <fnt>
          <bib>Kiso</bib>
          <lok>Tombeja Arabesko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la varmru&gcirc;a ardo de kandellumo dancigis viglajn ombrojn de la du geamantoj sur
        la muroj en la <tld/>a &ccirc;ambro
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2013/011110.php">Splito</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="agrabl.0a">agrabla</ref>,
      <ref cel="hejm.0eca">hejmeca</ref>,
      <ref cel="komfor.0a">komforta</ref>
    </refgrp>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>agrabla</ke>, <ke>hejmeca</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_u;&c_t;&c_u;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_c;&c_ib;&c_h;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">gem&uuml;tlich</trd>
  <trd lng="en">cosy</trd>
  <trdgrp lng="fr">
    <trd>cosy</trd>,
    <trd>douillet</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>sympatyczny</trd>,
    <trd>przyjemny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_t;&c_i;&c_sh;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_o;&c_m;&c_a;&c_sh;&c_n;&c_ib;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_je;&c_v;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_ib;&c_t;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_d;&c_o;&c_b;&c_r;&c_o;&c_d;&c_u;&c_sh;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20174;&#20174;&#23481;&#23481; <pr>c&oacute;ngc&oacute;ngr&oacute;ngr&oacute;ng</pr></trd>,
    <trd>&#24736;&#28982;&#33258;&#24471; <pr>y&omacron;ur&aacute;nz&igrave;d&eacute;</pr></trd>,
    <trd>&#24930;&#24736;&#24736; <pr>m&agrave;ny&omacron;uy&omacron;u</pr></trd>,
    <trd>&#24812;&#24847; <pr>qi&egrave;y&igrave;</pr></trd>,
    <trd>&#26155; <pr>x&ugrave;</pr></trd>,
    <trd>&#38543;&#24847; <pr>su&iacute;y&igrave;</pr></trd>
  </trdgrp>
</drv>
</art>

