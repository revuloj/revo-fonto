<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: gemut.xml,v 1.1 2022/03/06 16:40:25 afido Exp $">
    <kap>
      <rad>gemut</rad>/a
    </kap>

<drv mrk="gemut.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="ujutn.0a">Ujutna</ref>:
      <ekz>
        &leftquot;mi nenion preparis&rightquot; &dash; &leftquot;ne gravas&rightquot;, diris la <tld/>a dano, &leftquot;sendube vi povos ion
        diri!&rightquot;
        <fnt>
          <bib>Kiso</bib>
          <lok>Tombeja Arabesko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la varmru&gcirc;a ardo de kandellumo dancigis viglajn ombrojn de la du geamantoj sur
        la muroj en la <tld/>a &ccirc;ambro
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="https://www.monato.be/2013/011110.php"
              >Splito</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="agrabl.0a">agrabla</ref>,
      <ref cel="hejm.0eca">hejmeca</ref>,
      <ref cel="komfor.0a">komforta</ref>
    </refgrp>
    <trd lng="de">gem&uuml;tlich</trd>
    <trd lng="en">cosy</trd>
  </snc>
</drv>

  </art>

