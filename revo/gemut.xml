<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gemut.xml,v 1.6 2024/03/02 09:21:05 afido Exp $">
<kap>
  <rad>gemut</rad>/a <fnt><bib>SPIV</bib></fnt>
</kap>

<drv mrk="gemut.0a">
  <kap><tld/>a</kap>
  <snc mrk="gemut.0a.KOMUNE">
    <dif>
      <ref tip="dif" cel="ujutn.0a">Ujutna</ref>:
      <ekz>
        &leftquot;mi nenion preparis&rightquot; &dash; &leftquot;ne gravas&rightquot;, diris la <tld/>a dano, &leftquot;sendube vi povos ion
        diri!&rightquot;
        <fnt>
          <bib>Kiso</bib>
          <lok>Tombeja Arabesko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la varmru&gcirc;a ardo de kandellumo dancigis viglajn ombrojn de la du geamantoj sur
        la muroj en la <tld/>a &ccirc;ambro
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2013/011110.php">Splito</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="agrabl.0a">agrabla</ref>,
      <ref cel="hejm.0eca">hejmeca</ref>,
      <ref cel="komfor.0a">komforta</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_t;&c_u;&c_l;&c_mol;&c_n;&c_y;</trd>,
    <trd>&c_c;&c_ib;&c_h;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">gem&uuml;tlich</trd>
  <trd lng="en">cosy</trd>
  <trdgrp lng="fr">
    <trd>cosy</trd>,
    <trd>douillet</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_t;&c_i;&c_sh;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_d;&#x43E;&#x43C;&c_a;&c_sh;&#x43D;&c_ib;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_d;&c_u;&c_sh;&c_je;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&c_i;&c_v;&c_ib;&c_t;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_d;&#x43E;&c_b;&c_r;&#x43E;&c_d;&c_u;&c_sh;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4ECE;&#x4ECE;&#x5BB9;&#x5BB9; <pr>c&#xF3;ngc&#xF3;ngr&#xF3;ngr&#xF3;ng</pr></trd>,
    <trd>&#x60A0;&#28982;&#x81EA;&#x5F97; <pr>y&#x14D;ur&#xE1;nz&#xEC;d&#xE9;</pr></trd>,
    <trd>&#24930;&#x60A0;&#x60A0; <pr>m&#xE0;ny&#x14D;uy&#x14D;u</pr></trd>,
    <trd>&#x60EC;&#x610F; <pr>qi&#xE8;y&#xEC;</pr></trd>,
    <trd>&#x662B; <pr>x&#xF9;</pr></trd>,
    <trd>&#x968F;&#x610F; <pr>su&#xED;y&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>

