<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>afgani</rad>/o
</kap>

<drv mrk="afgani.0o">
  <kap>
    <tld lit="A"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="afgani.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>AF</mlg>
    <dif>
      <ref tip="dif" cel="afgan.0ujo.GEOG">Afganujo</ref>
    </dif>
    <trd lng="hu">Afganiszt&aacute;n</trd>
    <trd lng="it">Afganistan</trd>
    <trdgrp lng="pl">
      <trd>Afganistan</trd>,
      <trd>Islamska Republika <ind>Afganistanu</ind></trd>
    </trdgrp>
  </snc>
</drv>
<drv mrk="afgani.0o.mono">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="afgani.0o.mono.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="unu.mon0o">Monunuo</ref> de <ref tip="vid"
      cel="afgan.0ujo.GEOG">Afganujo</ref>.
      ISO-4217 simbolo: AFN.
      <ekz>
        1 afganio = 100 <ref tip="sub" cel="pul1.0o.EKON">puloj</ref>
        <fnt>
          <vrk>
            <url ref="http://www.esperanto.be/fel/jar/landoj.php">La
            landoj de l' tero</url>
          </vrk>
       </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>afgani</trd>
    </trdgrp>
  </snc>
</drv>
</art>

