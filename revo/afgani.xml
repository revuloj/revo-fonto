<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>afgani</rad>/o
</kap>

<drv mrk="afgani.0o">
  <kap>
    <tld lit="A"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 11</lok>
    </fnt>
  </kap>
  <snc mrk="afgani.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg>AF</mlg>
    <dif>
      <ref tip="dif" cel="afgan.0ujo.GEOG">Afganujo</ref>
    </dif>
    <trd lng="bg">&c_A;&c_f;&c_g;&c_a;&c_n;&c_i;&c_s;&c_t;&c_a;&c_n;</trd>
    <trdgrp lng="de">
      <trd>Afghanistan</trd>,
      <trd>Islamische Republik <ind>Afghanistan</ind></trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>Afghanistan</trd>,
      <trd>Islamic Republic of <ind>Afghanistan</ind></trd>
    </trdgrp>
    <trd lng="fa">&a_A;&a_f;&a_gh;&a_A;&a_n;&a_s;&a_t;&a_A;&a_n;</trd>
    <trd lng="hu">Afganiszt&aacute;n</trd>
    <trd lng="it">Afganistan</trd>
    <trdgrp lng="pl">
      <trd>Afganistan</trd>,
      <trd>Islamska Republika <ind>Afganistanu</ind></trd>
    </trdgrp>
    <trd lng="tr">Afganistan</trd>
  </snc>
</drv>
<drv mrk="afgani.0o.mono">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="afgani.0o.mono.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="unu.mon0o">Monunuo</ref> de <ref tip="vid"
      cel="afgan.0ujo.GEOG">Afganujo</ref>.
      ISO-4217 simbolo: AFN.
      <ekz>
        1 afganio = 100 <ref tip="sub" cel="pul1.0o.EKON">puloj</ref>
        <fnt>
          <vrk>
            <url ref="http://www.esperanto.be/fel/jar/landoj.php">La
            landoj de l' tero</url>
          </vrk>
       </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Afghani</trd>
    <trd lng="fa">&a_A;&a_f;&a_gh;&a_A;&a_n;&f_y;<klr tip="ind"> (&a_w;&a_A;&a_H;&a_d;
    &f_p;&a_w;&a_l;)</klr></trd>
    <trdgrp lng="pl">
      <trd>afgani</trd>
    </trdgrp>
  </snc>
</drv>
</art>

