<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>spor</rad>/o
</kap>
<drv mrk="spor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Plej ofte unu&ccirc;ela reprodukta organo.
    </dif>
  </snc>
  <trd lng="de">Spore</trd>
</drv>
<drv mrk="spor.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Speco de kapsula sako, enfermanta la
      <ref tip="prt" cel="spor.0o"><tld/>ojn</ref>.
    </dif>
  </snc>
  <trd lng="de">Sporangie</trd>
</drv>
<drv mrk="spor.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Grupo de <ref tip="super" cel="plant.0oj">plantoj</ref>
      reprodukti&gcirc;antaj per <tld/>oj kaj havantaj alternadon
      de generacioj.
    </dif>
  </snc>
  <trd lng="de">Sporenpflanzen</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
