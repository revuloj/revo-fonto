<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>spor</rad>/o
</kap>
<drv mrk="spor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Plej ofte unu&ccirc;ela reprodukta organo.
    </dif>
  </snc>
  <trd lng="de">Spore</trd>
  <trd lng="nl">spoor<klr tip="ind"> (v.plant)</klr></trd>
  <trd lng="ru">&c_s;&c_p;&c_o;&c_r;&c_a;</trd>
</drv>

<drv mrk="spor.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Speco de kapsula sako, enfermanta la
      <ref tip="prt" cel="spor.0o"><tld/>ojn</ref>.
    </dif>
  </snc>
  <ref tip="malprt" cel="spor.0oplantoj"><tld/>oplantoj</ref>
  <trd lng="de">Sporangie</trd>
  <trd lng="nl">sporenhouder</trd>
  <trd lng="ru">&c_s;&c_p;&c_o;&c_r;&c_a;&c_n;&c_g;&c_i;&c_j;</trd>
</drv>

<drv mrk="spor.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Grupo de <ref tip="super" cel="plant.0oj">plantoj</ref>
      reprodukti&gcirc;antaj per <tld/>oj kaj havantaj alternadon
      de generacioj.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Kryptogamen</trd>,
     <trd>Sporenpflanzen</trd>
  </trdgrp>
  <trd lng="nl">sporenplanten</trd>
  <trd lng="ru">&c_s;&c_p;&c_o;&c_r;&c_o;&c_v;&c_y;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;</trd>
</drv>
</art>
