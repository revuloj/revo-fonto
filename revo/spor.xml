<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>spor</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="spor.0o">
  <kap><tld/>o</kap>
  <snc mrk="spor.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Plej ofte unu&ccirc;ela reprodukta organo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_o;&c_r;&c_a;</trd>  
  <trd lng="de">Spore</trd>
  <trd lng="en">spore</trd>
  <trd lng="es">espora</trd>
  <trd lng="fr">spore</trd>
  <trd lng="hu">sp&oacute;ra</trd>
  <trd lng="it">spora</trd>
  <trd lng="nl">spoor<klr tip="ind"> (v.plant)</klr></trd>
  <trd lng="ru">&c_s;&c_p;&c_o;&c_r;&c_a;</trd>
</drv>

<drv mrk="spor.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="spor.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Speco de kapsula sako, enfermanta la
      <ref tip="prt" cel="spor.0o.BOT"><tld/>ojn</ref>
      en la
      <ref tip="malprt" cel="spor.0oplantoj.BOT"><tld/>oplantoj</ref>.
    </dif>
  </snc> 
  <trd lng="be">&c_s;&c_p;&c_a;&c_r;&c_a;&c_n;&c_g;&c_ib;&c_j;</trd>
  <trd lng="de">Sporangie</trd>
  <trd lng="fr">sporange</trd>
  <trd lng="hu">sp&oacute;ratart&oacute;</trd>
  <trd lng="it">sporangio</trd>
  <trd lng="nl">sporenhouder</trd>
  <trd lng="ru">&c_s;&c_p;&c_o;&c_r;&c_a;&c_n;&c_g;&c_i;&c_j;</trd>
</drv>

<drv mrk="spor.0oplantoj">
  <kap><tld/>oplantoj</kap>
  <snc mrk="spor.0oplantoj.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="grup.0o.BIO"
      lst="botanikaj_grupoj">Grupo</ref> de <ref tip="malprt"
      cel="plant.0oj">plantoj</ref>
      reprodukti&gcirc;antaj per <tld/>oj kaj havantaj alternadon
      de generacioj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_o;&c_r;&c_a;&c_v;&c_y;&c_ja; <ind>&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_y;</ind></trd>
  <trdgrp lng="de">
     <trd>Kryptogamen</trd>,
     <trd>Sporenpflanzen</trd>
  </trdgrp>
  <trd lng="fr">cryptogames</trd>
  <trdgrp lng="hu">
    <trd>sp&oacute;r&aacute;sok</trd>,
    <trd>kriptog&aacute;mok</trd>,
    <trd>rejtett ivar&uacute;ak</trd>,
    <trd>sp&oacute;r&aacute;s n&ouml;v&eacute;nyek</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>sporenplanten</trd>,
    <trd>cryptogamen</trd>
  </trdgrp>
  <trd lng="pl">skrytop&lstroke;ciowe<klr> ro&sacute;liny</klr></trd>
  <trd lng="pt">cript&oacute;gamas</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_o;&c_r;&c_o;&c_v;&c_y;&c_je; &c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_ja;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_o;&c_b;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_je;</trd>
  </trdgrp>
</drv>
</art>
