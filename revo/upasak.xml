<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: upasak.xml,v 1.6 2024/04/10 21:21:37 revo Exp $">
<kap>
  <rad>upasak</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="upasak.0o">
  <kap><tld/>o</kap>
  <snc mrk="upasak.0o.BUD">
    <uzo tip="fak">BUD</uzo>
    <dif>
      Budaisma
      <ref tip="super" cel="laik.0o.pasxtato">laiko</ref>:
      <ekz>
        el la kunveno kvinmil
        bik&scirc;uoj, bik&scirc;uinoj, <tld/>oj kaj <tld/>inoj stari&gcirc;inte kaj
        adorante la Budhon eliris
        <fnt>
          <aut>trad. Asano San&ccirc;i</aut>,
          <vrk><url
          ref="http://www001.upp.so-net.ne.jp/jble/sutro/sps02.html">Saddharma-Pundarika
          Sutro, &ccirc;apitro 2a</url></vrk>,
          <lok>La Japana Budhano, 1979-1980, numero 187-190a</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel aliaj <tld/>oj mi faris preska&ubreve; saman taskon
        <fnt>
          <aut>packoro</aut>,
          <vrk><url
          ref="http://yundanfenqin.spaces.live.com/">[blogo]</url></vrk>,
          <lok>2006-01-19</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_p;&c_a;&c_s;&c_a;&c_k;&c_a;</trd>
  <trd lng="cs">laick&yacute; buddhista</trd>
  <trdgrp lng="en">
    <trd>Upasaka <klr>(masculine/vira)</klr></trd>,
    <trd>Upasika <klr>(feminine/ina)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>la&iuml;c bouddhiste</trd>,
    <trd>upasaka</trd>
  </trdgrp>
  <trd lng="pl">Upasaka</trd>
  <trd lng="sk">laick&yacute; budhista</trd>
</drv>
</art>

