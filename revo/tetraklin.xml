<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: tetraklin.xml,v 1.1 2025/08/31 08:32:26 afido Exp $">
    <kap>
      <rad>tetraklin</rad>/o
      <fnt><bib>PIV2</bib></fnt>
    </kap>

<drv mrk="tetraklin.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Mediteranea, da&ubreve;rafolia, dioika <ref tip="super" cel="arb.0o.BOT">arbo</ref>
      kun skvamecaj folioj uzata por incenso kaj inkrusta&jcirc;oj;
      unuspecia <ref tip="lst" lst="voko:botanikaj_genroj" cel="genr.0o.BIO">genro</ref>
      el la familio <ref tip="malprt" cel="cipres1.0oj">cipresacoj</ref>
      (<trd lng="la">Tetraclinis</trd>):
      <ekz>
        tabureto el <tld/>a ligno inkrustita de eburo
        <fnt>
          <bib>MJA</bib>
          <lok>Parto Sepa</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/3/3b/Tetraclinis_articulata8.jpg/360px-Tetraclinis_articulata8.jpg"
      prm="PD"
      ><tld/>o (Tetraclinis articulata) en Kartageno, Hsipanujo
        <fnt>
          <aut>Nanosanchez, propra verko</aut>
          <lok><url ref="&WCW;/File:Tetraclinis_articulata8.jpg"
              >https://commons.wikimedia.org/wiki/File:Tetraclinis_articulata8.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Sandarakbaum</trd>,
      <trd>Gliederzypresse</trd>,
      <trd>Berberthuja</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>sandarac</trd>,
      <trd>Barbary thuja</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>cypr&egrave;s de l&#x2019;Atlas</trd>,
      <trd>thuya de Berb&eacute;rie</trd>
    </trdgrp>
    <trd lng="ru">&c_t;&c_je;&c_t;&c_r;&c_a;&c_k;&c_l;&c_i;&c_n;&c_i;&c_s;</trd>
  </snc>
</drv>

  </art>

