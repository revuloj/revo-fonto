<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>vat</rad>/o
</kap>
<drv mrk="vat1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <snc>
    <dif>
      Potenco produktata de unu ampero sub tensio de unu volto.
    </dif>
  </snc>
  <trd lng="de">Watt</trd>
  <trd lng="nl">watt</trd>
</drv>
<drv mrk="vat1.0horo">
  <kap><tld/>horo</kap>
  <snc>
    <dif>
      Laboro de unu <tld/>o dum unu horo.
    </dif>
  </snc>
  <trd lng="de">Wattstunde</trd>
</drv>
</art>
