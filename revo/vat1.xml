<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Vat</rad>/o
</kap>
<drv mrk="vat1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Anglalingve: James <trd lng="en">Watt</trd>, 1736-1819. Brita fizikisto:
      <ekz>
        <tld lit="v"/>a regulilo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_V;&c_a;&c_t;</trd>,
    <trd>&c_U;&c_a;&c_t;</trd>
  </trdgrp>
  <trd lng="fr">Watt</trd>
  <trd lng="hu">Watt</trd>
  <trdgrp lng="pt">
   <trd>Watt</trd>
  </trdgrp>

  <trdgrp lng="ru">
    <trd>&c_V;&c_a;&c_t;&c_t;</trd>,
    <trd>&c_U;&c_a;&c_t;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="vat1.v0o">
  <kap><tld lit="v"/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="pov.0o.FIZ">povo</ref> en la Internacia Sistemo de
      unuoj; simb. W:
      <ekz>
        unu <tld lit="v"/>o egalas al la povo, &ccirc;e kiu laboro je unu
        <ref cel="jxul.jx0o">&jcirc;ulo</ref> estas produktata en
        unu <ref cel="sekund.0o.tempo">sekundo</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_t; <klr>(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a; &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
  <trd lng="de">Watt</trd>
  <trd lng="en">watt</trd>
  <trd lng="fr">watt<klr tip="ind"> (unit&eacute; de mesure)</klr></trd>
  <trd lng="hu">watt</trd>
  <trd lng="nl">watt</trd>
  <trdgrp lng="pt">
   <trd>watt</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_a;&c_t;&c_t;</trd>
</drv>

<drv mrk="vat1.v0horo">
  <kap><tld lit="v"/>horo</kap>
  <uzo tip="fak">FIZ</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      <ref cel="energi.0o.FIZ">energio</ref>; simb. Wh:
      <ekz>
        unu <tld lit="v"/>horo egalas al la laboro produktata de povo
        je unu <tld lit="v"/>o dum unu horo, alidire 3600 &jcirc;uloj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_t;-&c_g;&c_a;&c_d;&c_z;&c_ib;&c_n;&c_a;</trd>
  <trd lng="de">Wattstunde</trd>
  <trd lng="en">watt-hour</trd>
  <trd lng="fr"><ind>watt</ind>-heure</trd>
  <trd lng="hu">watt&oacute;ra</trd>
  <trd lng="nl">wattuur</trd>
  <trd lng="ru">&c_v;&c_a;&c_t;&c_t;-&c_ch;&c_a;&c_s;</trd>
</drv>
</art>
