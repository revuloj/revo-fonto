<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bud</rad>/o
</kap>
<drv mrk="bud.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bud.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Senluksa barako a&ubreve; butiketo el lignaj tabuloj:
      <ekz>
        &gcirc;ardena <tld/>o por kulturiloj;
      </ekz>
      <ekz>
        postena soldata <tld/>o;
      </ekz>
      <ekz>
        <tld/>o de &scirc;uflikisto;
      </ekz>
      <ekz>
        butika <tld/>o<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kaban.0o">kabano</ref>,
      <ref cel="kajut.0o">kajuto</ref>
    </refgrp>
  </snc>            
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_p;&c_ib;&c_k;</trd>,
    <trd>&c_k;&c_ib;&c_jo;&c_s;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>lochenn</trd>,
    <trd>logell</trd>,
    <trd>stal<klr tip="ind"> (en un diskouezhadeg)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>booth</trd>,
    <trd>kiosk</trd>,
    <trd>cabana</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>kiosko</trd>,
    <trd>puesto</trd>,
    <trd>caseta</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cabine</trd>,
    <trd>&eacute;dicule <klr tip="ind">(kiosque)</klr></trd>,
    <trd>kiosque <klr tip="ind">(&eacute;dicule)</klr></trd>,
    <trd>resserre</trd>,
    <trd>stand <klr tip="ind">(dans une exposition)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&oacute;d&eacute;</trd>,
    <trd>f&uuml;lke</trd>
  </trdgrp> 
  <trdgrp lng="nl">
    <trd>kraam</trd>,
    <trd>huisje <klr tip="ind">(bv. van schildwacht)</klr></trd>,
    <trd>schuurtje</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_r;&c_jo;&c_k;</trd>,
    <trd>&c_k;&c_i;&c_o;&c_s;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="bud.prov0o">
  <kap>prov<tld/>o</kap>
  <snc mrk="bud.prov0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="B"/>o en vestvendejo, en kiu oni povas lasi la vestojn 
      kaj surmeti aliajn por provo:
      <ekz>
        &leftquot;mi volus provmeti tiun mamzonon en la montrofenestro.&rightquot;
        &leftquot;Nu vidu, sinjorino&rightquot; respondas la vendistino, &leftquot;prefere vi faru 
        tion en la prov<tld/>o...&rightquot;
        <fnt>
          <aut>R. Cash</aut>,
          <vrk><url ref="http://groups.yahoo.com/group/esperanto-anekdotoj">
          esperanto-anekdotoj</url></vrk>,
          <lok>[vidita en 2007-03]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">cabine d'essayage</trd>
  <trd lng="nl">pashokje</trd>
</drv>

<drv mrk="bud.telefon0o">
  <kap>telefon<tld/>o</kap>
  <snc mrk="bud.telefon0o.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      <tld lit="B"/>o en publika loko, en kiu oni povas uzi telefonon:
      <ekz>
        la instruistoj subite a&ubreve;das la vo&ccirc;on de Susano ene de la 
        telefon<tld/>o
        <fnt>
          <aut>A. &Ubreve;esto</aut>,
          <vrk><url ref="http://www.dysmey.org/eo/eseoj/doktorokiu.html">
          Doktoro Kiu</url></vrk>,
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_a;&c_ja; <ind>&c_b;&c_u;&c_d;&c_k;&c_a;</ind></trd>  
  <trd lng="br">logell bellgomz</trd>
  <trd lng="en">telephone <ind>booth</ind></trd>
  <trd lng="fr">cabine t&eacute;l&eacute;phonique</trd>
  <trd lng="hu">telefonf&uuml;lke</trd>
  <trd lng="nl">telefooncel</trd>
</drv>
</art>
