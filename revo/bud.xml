<vortaro>
<art mrk="$Id: bud.xml,v 1.35 2023/10/29 19:11:04 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>bud</rad>/o
</kap>

<drv mrk="bud.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bud.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Senluksa <ref cel="barak.0o">barako</ref> a&ubreve; <ref cel="butik.0o">butiketo</ref> el 
      lignaj tabuloj:
      <ekz>
        sur la pordo de sia <tld/>o li skribis: &leftquot;eniro senpaga&rightquot;, granda amaso da publiko
        plenigis balda&ubreve; la <tld/>on
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o plena de mielkukoj <klr>[en]</klr> la foiro
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj mizeraj lignaj <tld/>oj por Holandaj &scirc;ipistoj <klr>[&#x2026;]</klr> staris sur
        la transa bordo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> lo&gcirc;adis en la lignaj <tld/>oj de la <nom>H&uuml;ske</nom>strato kaj vendadis
        bieron kaj spicojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        provizore konstruitaj lignaj butikaj <tld/>oj
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ie a&ubreve;di&gcirc;is muziko kaj staris <tld/>oj kun refre&scirc;iga&jcirc;oj
        <fnt>
          <bib>Fab3</bib>
          <lok>Io</lok>
        </fnt>;
      </ekz>
      <ekz>
        preteriri kradrost<tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Marko Naoki Lins</aut>
          <vrk><url ref="&Monato;/2004/008334.html">Centra demando</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        frukto-vendejoj kaj <ind>trink<tld/>o</ind>j
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
        <trdgrp lng="de">
          <trd>Getr&auml;nkebude</trd>,
          <trd>Schankbude</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <tld/>o sur kukumkampo
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 1:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de selfaristo
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        bariloj kun <ind>gardo<tld/>o</ind>j
        <klr>(vd <ref cel="posten.0o.GXENERALE">posteno</ref>, 
        <ref cel="gard.0istejo">gardistejo</ref>)</klr>
        <fnt>
          <bib>MkM</bib>
          <lok>6</lok>
        </fnt>;
        <trdgrp lng="de">
          <trd>Schilderh&auml;uschen</trd>,
          <trd>Wachh&auml;uschen</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <klr>[ili]</klr> alveninte al la <ind>pago<tld/>o</ind>j 
        <klr>(kp <ref cel="kas.0ejo">kasejo</ref>)</klr>, tutsimple deklaris: 
        &leftquot;saluton! mi ne volas pagi&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>Jaume Clotet</aut>
          <vrk><url ref="&Monato;/2012/010745.php">Mi ne volas pagi ...</url></vrk>
          <lok>2012</lok>
        </fnt>;
        <trd lng="de">Kassenh&auml;uschen</trd>
      </ekz>
      <ekz>
        dum la konferenco Korea E-Asocio funkciigis <ind>inform<tld/>o</ind>n
        <fnt>
          <bib>LOdE</bib>
          <lok>1999, No 12 (62)</lok>
        </fnt>.
        <trdgrp lng="de">
          <trd>Informationsbude</trd>,
          <trd>Informationspunkt</trd>
        </trdgrp>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kaban.0o">kabano</ref>,
      <ref cel="kajut.0o">kajuto</ref>,
      <ref cel="kiosk.0o">kiosko</ref>,
      <ref cel="lauxb.0o">la&ubreve;bo</ref>,
      <ref cel="pavilo.0o">pavilono</ref>
      <ref cel="stand.0o">stando</ref>,
      <ref cel="tend.0o">tendo</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x644A;&#x5B50; <pr>t&#x101;nzi</pr></trd>,
    <trd>&#x68DA;&#x5B50; <pr>p&#xE9;ngzi</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_u;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_a;&c_p;&c_ib;&c_k;</trd>,
    <trd>&c_k;&c_ib;&c_jo;&c_s;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>lochenn</trd>,
    <trd>logell</trd>,
    <trd>stal<klr tip="ind"> (en un diskouezhadeg)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bouda</trd>,
    <trd>chatr&ccaron;</trd>,
    <trd>kiosek</trd>,
    <trd>koliba</trd>,
    <trd>ku&ccaron;a</trd>,
    <trd>k&uring;lna</trd>,
    <trd>k&uring;lna</trd>,
    <trd>p&rcaron;&iacute;st&rcaron;e&scaron;ek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Bude</trd>,
    <trd>Kabine</trd>,
    <trd>Kiosk</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>booth</trd>,
    <trd>kiosk</trd>,
    <trd>cabana</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>kiosko</trd>,
    <trd>puesto</trd>,
    <trd>caseta</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_gh;&a_r;&a_f;&a_h;</trd>
    <trd>&a_b;&a_A;&a_j;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cabine</trd>,
    <trd>&eacute;dicule <klr tip="ind">(kiosque)</klr></trd>,
    <trd>kiosque <klr tip="ind">(&eacute;dicule)</klr></trd>,
    <trd>resserre</trd>,
    <trd>stand <klr tip="ind">(dans une exposition)</klr></trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&samekh;&vav;&kaf;&he;</trd>,
    <trd>&dalet;&vav;&kaf;&fnun;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&oacute;d&eacute;</trd>,
    <trd>f&uuml;lke</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4EEE;&#x5C0F;&#x5C4B;  <pr>&#x304B;&#x308A;&#x3054;&#x3084;</pr></trd>,
    <trd>&#x30B9;&#x30BF;&#x30F3;&#x30C9;  <pr>&#x3059;&#x305F;&#x3093;&#x3069;</pr></trd>,
    <trd>&#x30D6;&#x30FC;&#x30B9;</trd>,
    <trd>&#x58F2;&#x5E97;  <pr>&#x3070;&#x3044;&#x3066;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kraam</trd>,
    <trd>huisje <klr tip="ind">(bv. van schildwacht)</klr></trd>,
    <trd>schuurtje</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>buda</trd>,
    <trd>budka</trd>
  </trdgrp>
  <trd lng="pt">barraca<klr> (de feira, de jornais)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_r;&c_jo;&c_k;</trd>,
    <trd>&c_k;&c_i;&c_o;&c_s;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>b&uacute;da</trd>,
    <trd>chatr&ccaron;</trd>,
    <trd>kiosok</trd>,
    <trd>koliba</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_u;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_l;&c_a;&c_r;&c_mol;&c_o;&c_k;</trd>,
    <trd>&c_k;&c_ib;&c_o;&c_s;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="bud.balot0o">
  <kap>balot<tld/>o</kap>
  <snc mrk="bud.balot0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      <tld lit="B"/>o lokita en balotejo por ka&scirc;i la elekton de
      partoprenantoj en sekreta vo&ccirc;donado:
      <ekz>
        La balotbudo estis adoptita en 1872 de Unui&gcirc;inta re&gcirc;lando, en
        1877 de Belgio kaj en 1903 de Germanio.
        <fnt><bib>Viki</bib>, <lok><url ref="&Viki;/Balotbudo">balotbudo</url></lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_b;&c_ib;&c_n;&c_a; &c_d;&c_l;&c_ja; &c_g;&c_a;&c_l;&c_a;&c_s;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>
  <trd lng="de">Wahlkabine</trd>
  <trd lng="en">voting booth</trd>
  <trd lng="fr">isoloir</trd>
</drv>

<drv mrk="bud.muzik0o">
  <kap>
    muzik<tld/>o
  </kap>
  <snc mrk="bud.muzik0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <tld lit="B"/>o kun podio kaj tegmento, ordinare en&gcirc;ardena a&ubreve; surplaca, en kiu oni
      muzikas por la &ccirc;irka&ubreve;a publiko;
      <ref tip="sin" cel="kiosk.muzik0o.ARKI">muzikkiosko</ref>:
      <ekz>
        <nom>Winston</nom> posedas la muzik<tld/>on en la Ponta Strato
        <fnt>
          <aut>-</aut>
          <vrk><url ref="https://epo.wikitrans.net/List_of_EastEnders_characters_(1986)">Listo de
          EastEnders-karakteroj (1986)</url></vrk>
          <lok>Wikitrans, 2017-09-10</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Musikpavillon</trd>,
      <trd>Musikkiosk</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x6F14;&#x594F;&#x53F0; <pr>y&#x1CE;nz&#xF2;ut&#xE1;i</pr></trd>,
    <trd>&#x97F3;&#x4E50;&#x53F0; <pr>y&#x12B;nyu&#xE8;t&#xE1;i</pr></trd>
  </trdgrp>
</snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; <ind>&c_k;&c_ib;&c_jo;&c_s;&c_k;</ind></trd>,
    <trd>&c_m;&c_u;&c_z;&c_y;&c_ch;&c_n;&c_y; <ind>&c_p;&c_a;&c_v;&c_ib;&c_l;&c_mol;&c_jo;&c_n;</ind></trd>
  </trdgrp>
  <trd lng="fr">kiosque &agrave; musique</trd>
</drv>

<drv mrk="bud.prov0o">
  <kap>prov<tld/>o</kap>
  <snc mrk="bud.prov0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <tld lit="B"/>o en vestvendejo, en kiu oni povas lasi la vestojn 
      kaj surmeti aliajn por provo:
      <ekz>
        &leftquot;mi volus provmeti tiun mamzonon en la montrofenestro.&rightquot;
        &leftquot;Nu vidu, sinjorino&rightquot; respondas la vendistino,
        &leftquot;prefere vi faru tion en la prov<tld/>o...&rightquot;
        <fnt>
          <aut>R. Cash</aut>
          <vrk><url ref="http://groups.yahoo.com/group/esperanto-anekdotoj">
          esperanto-anekdotoj</url></vrk>
          <lok>[vidita en 2007-03]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_r;&c_y;&c_m;&c_je;&c_r;&c_a;&c_ch;&c_n;&c_a;&c_ja;</trd>
    <trd lng="bg">&c_s;&c_malmol;&c_b;&c_l;&c_je;&c_k;&c_a;&c_l;&c_n;&c_ja;</trd>
    <trd lng="de">Umkleidekabine</trd>
    <trd lng="es">probador<klr tip="amb"> (cabina para probarse
    ropa)</klr></trd>
    <trd lng="fa">&f_k;&a_A;&a_b;&f_y;&a_n;
    &f_p;&a_r;&a_w;<klr tip="amb">
    (&a_l;&a_b;&a_A;&a_s;)</klr></trd>
    <trd lng="fr">cabine d'essayage</trd>
    <trd lng="nl">pashokje</trd>
    <trd lng="pl">przymierzalnia</trd>
    <trd lng="zh">&#x8A66;&#x8863;&#x9593; <pr>sh&#xEC;y&#x12B;ji&#x101;n</pr></trd>
</snc>
  <trd lng="ja">&#x8A66;&#x7740;&#x5BA4;  <pr>&#x3057;&#x3061;&#x3083;&#x304F;&#x3057;&#x3064;</pr></trd>
</drv>

<drv mrk="bud.telefon0o">
  <kap>telefon<tld/>o</kap>
  <snc mrk="bud.telefon0o.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      <tld lit="B"/>o en publika loko, en kiu oni povas uzi telefonon:
      <ekz>
        la instruistoj subite a&ubreve;das la vo&ccirc;on de Susano ene
        de la telefon<tld/>o
        <fnt>
          <aut>A. &Ubreve;esto</aut>
          <vrk><url ref="http://www.dysmey.org/eo/eseoj/doktorokiu.html">
          Doktoro Kiu</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> notis la numeron de l&#x2019; taksio kaj eniris telefon<tld/>on
        <fnt>
          <bib>ChR</bib>
          <lok>Spiona rondo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x7535;&#x8BDD;&#x4EAD; <pr>di&#xE0;nhu&#xE0;t&#xED;ng</pr></trd>,
    <trd>&#x516C;&#x7528;&#x7535;&#x8BDD;&#x4EAD; <pr>g&#x14D;ngy&#xF2;ngdi&#xE0;nhu&#xE0;t&#xED;ng</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_a;&c_ja;
  <ind>&c_b;&c_u;&c_d;&c_k;&c_a;</ind></trd>
  <trd lng="bg">&c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_a; &c_k;&c_a;&c_b;&c_i;&c_n;&c_a;</trd>
  <trd lng="br">logell bellgomz</trd>
  <trd lng="cs">telefonn&iacute; budka</trd>
  <trd lng="de">Telefonzelle</trd>
  <trd lng="en">telephone <ind>booth</ind></trd>
  <trd lng="fa">&a_b;&a_A;&a_j;&a_h;&#x200C;&f_y;
  &a_t;&a_l;&a_f;&a_n;</trd>
  <trd lng="fr">cabine t&eacute;l&eacute;phonique</trd>
  <trd lng="hu">telefonf&uuml;lke</trd>
  <trd lng="ja">&#x96FB;&#x8A71;&#x30DC;&#x30C3;&#x30AF;&#x30B9;  <pr>&#x3067;&#x3093;&#x308F;&#x30DC;&#x30C3;&#x30AF;&#x30B9;</pr></trd>
  <trd lng="nl">telefooncel</trd>
  <trdgrp lng="pl">
    <trd>budka telefoniczna</trd>,
    <trd>kabina telefoniczna</trd>
  </trdgrp>
  <trd lng="sk">telef&oacute;nna b&uacute;dka</trd>
  <trd lng="uk">&c_t;&c_je;&c_l;&c_je;&c_f;&c_o;&c_n;&c_n;&c_a; &c_b;&c_u;&c_d;&c_k;&c_a;</trd>
</drv>

<drv mrk="bud.vendo0o">
  <kap>
    vendo<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="B"/>o &ccirc;e kiu oni vendas ion, precipe en bazaroj kaj foiroj:
      <ekz>
        multegaj <klr>[&#x2026;]</klr> forkuris el la vendo<tld/>oj
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la placo anta&ubreve; la &ccirc;efstacidomo ni preterpasis vendo<tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Ulrich Lins</aut>
          <vrk><url ref="&Monato;/2005/008536.php">Nokta aventuro en Pjongjango</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_a;&c_p;&c_ib;&c_k;</trd>,
    <trd>&c_k;&c_ib;&c_jo;&c_s;&c_k; <klr>(&c_g;&c_a;&c_n;&c_d;&c_l;&c_jo;&c_v;&c_y;)</klr></trd>
  </trdgrp>

  <trd lng="de">Verkaufsbude</trd>
</drv>




