<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: artisx.xml,v 1.40 2023/10/28 08:33:09 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>arti&scirc;ok</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="artisx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="artisx.0o.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="astera.0oj">asteracoj</ref>
      (<trd lng="la">Cynara</trd>).
    </dif>
    <trd lng="fr">Cynara</trd>
  </snc>
  <snc mrk="artisx.0o.specio">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Specio de <tld/>o<sncref ref="artisx.0o.genro"/>
      (<trd lng="la">Cynara scolymus</trd>), kies kapitulo
      estas uzata kiel legomo:
      <ekz>
        man&gcirc;a&jcirc;o de &scirc;i multe &scirc;atata: fundoj de <tld/>oj far&ccirc;itaj per ma&ccirc;itaj fungoj, vera
        bongustega franda&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2006/008905.php">Negro i&gcirc;is nigrulo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&#x2026;]</klr> sed karoto, a&ubreve; rapo, a&ubreve; <tld/>o
        estos neniam bonaj
        <fnt>
          <aut>N. V. Gogol, trad. N. Z. Osjmak</aut>,
          <vrk>Skriboj de la frenezulo</vrk>,
          <lok>La Revuo, 1910, p. 524a</lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/8/8b/Cynara_July_2011-1.jpg/357px-Cynara_July_2011-1.jpg" prm="CC BY-SA 3.0">floraro de <tld/>o (Cynara scolymus), &Gcirc;ardeno de Plantoj, Parizo
        <fnt>
          <aut>Alvesgaspar, propra verko</aut>
          <lok><url ref="&WCW;/File:Cynara_July_2011-1.jpg">&WCW;/File:Cynara_July_2011-1.jpg</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="br">
      <trd>artichaodenn</trd>
      <trd>penn-<ind>artichaod</ind></trd>,
    </trdgrp>
    <trd lng="de">Artischocke</trd>
    <trd lng="en">artichoke</trd>
    <trd lng="fr">artichaut</trd>
    <trd lng="pl">karczoch</trd>
    <trd lng="pt">alcachofra</trd>
    <trdgrp lng="zh">
    <trd>&#x671D;&#x9BAE;&#x858A; <pr>ch&#xE1;oxi&#x101;nj&#xEC;</pr></trd>,
    <trd>&#x671D;&#x9C9C;&#x84DF; <pr>ch&#xE1;oxi&#x101;nj&#xEC;</pr></trd>,
    <trd>&#x6D0B;&#x84DF; <pr>y&#xE1;ngj&#xEC;</pr></trd>,
    <trd>&#x6D0B;&#x858A; <pr>y&#xE1;ngj&#xEC;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_r;&c_t;&c_y;&c_sh;&c_o;&c_k;</trd>
  <trd lng="bg">&c_a;&c_r;&c_t;&c_i;&c_sh;&c_o;&c_k;</trd>
  <trd lng="ca">carxofera</trd>
  <trd lng="de">Artischocke</trd>
  <trdgrp lng="fa">
    <trd>&f_k;&a_n;&f_g;&a_r; &a_f;&a_r;&a_n;&f_g;&f_y;</trd>,
    <trd>&a_A_madda;&a_r;&a_t;&f_y;&a_sh;&a_w;</trd>
  </trdgrp>
  <trd lng="es">alcachofa</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E8;&#x5D8;&#x5B4;&#x5D9;&#x5E9;&#x5C1;&#x5D5;&#x5B9;&#x5E7;</trd>
  <trd lng="hu">artics&oacute;ka</trd>
  <trd lng="id">artisyok</trd>
  <trd lng="it">carciofo</trd>
  <trdgrp lng="ja">
    <trd>&#x30A2;&#x30FC;&#x30C6;&#x30A3;&#x30C1;&#x30E7;&#x30FC;&#x30AF;</trd>,
    <trd>&#x30C1;&#x30E7;&#x30A6;&#x30BB;&#x30F3;&#x30A2;&#x30B6;&#x30DF;</trd>
  </trdgrp>
  <trd lng="nl">artisjok</trd>
  <trd lng="ro">anghinare</trd>
  <trd lng="ru">&c_a;&c_r;&c_t;&c_i;&c_sh;&c_o;&c_k;</trd>
  <trd lng="uk">&#x430;&#x440;&#x442;&#x438;&#x448;&#x43E;&#x43A;</trd>
</drv>

<drv mrk="artisx.hispana0o">
  <kap>hispana <tld/>o</kap>
  <snc mrk="artisx.hispana0o.BOT">
    <ref tip="dif" cel="kardon.0o.BOT">kardono</ref>
  </snc>
  <trd lng="ca">card</trd>
  <trdgrp lng="de">
    <trd>Spanische Artischocke</trd>,
    <trd>Kardone</trd>,
    <trd>Gem&uuml;se-Artischocke</trd>
  </trdgrp>
  <trd lng="en">cardoon</trd>
  <trd lng="hu">sz&uacute;r&oacute;s artocs&oacute;ka</trd>
  <trdgrp lng="it">
    <trd><ind>carciofo</ind> selvatico</trd>,
    <trd>cardone</trd>
    <trd>gobbo<klr> (carciofo selvatico)</klr></trd>
  </trdgrp>
  <trd lng="nl">kardoen</trd>
  <trd lng="pl">kard</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_r;&c_d;&c_o;&c_n;</trd>,
    <trd>&c_i;&c_s;&c_p;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;
<ind>&c_a;&c_r;&c_t;&c_i;&c_sh;&c_o;&c_k;</ind></trd>
  </trdgrp>
</drv>
</art>
