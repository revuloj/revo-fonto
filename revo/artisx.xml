<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>arti&scirc;ok</rad>/o
</kap>
<drv mrk="artisx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="artisx.0o.genro">
    <dif>
      Genro el familio <ref tip="super" cel="aster.0acoj">asteracoj</ref>
      (<trd lng="la">Cynara</trd>).
    </dif>
    <ref tip="prt" cel="kardon.0o">kardono</ref>
  </snc>
  <snc mrk="artisx.0o.specio">
    <dif>
      Specio de <tld/>o <sncref ref="artisx.0o.genro"/>
      (<trd lng="la">Cynara scolymus</trd>), kies kapitulo
      estas uzata kiel legomo.
    </dif>
    <trd lng="de">Artischocke</trd>
    <trd lng="fr">artichaut</trd>
  </snc>
</drv>
</art>
