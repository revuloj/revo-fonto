          <aut>A. Kor&jcirc;enkov, H. Gorecka</aut>,
          <vrk><url ref="&Sezonoj;2017/09/boirac/">
          &Eacute;mile Boirac</url></vrk>,
          <lok>La Ondo de Esperanto, 2017:8-9</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_r;&c_a;&c_m;&c_n;&c_e;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="fr">d&eacute;j&agrave;-vu</trd>
  <trd lng="nl">d&eacute;j&agrave; vu</trd>
  <trd lng="pl">d&eacute;j&agrave; vu</trd>
  <trd lng="ro">deja vu</trd>
</drv>
</art>

