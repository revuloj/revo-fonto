<art mrk="$Id: cxia.xml,v 1.24 2021/05/23 13:17:15 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>&ccirc;ia</rad></kap>
<drv mrk="cxia.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <uzo tip="klr">(kolektiva adj)</uzo>
    <dif>
      De &ccirc;iu speco, kvalito:
      <ekz>
        <tld/> homo povas tion fari;
      </ekz>
      <ekz>
        <tld/> dono estas bono
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &scirc;i meritis <tld/>n la&ubreve;don
        <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>;
      </ekz>
      <ekz>
        ne <tld/>n penson metu sur la langon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;e <tld/> vetero.
      </ekz>
    </dif>
    <ref tip="vid" cel="okaz.cxiu0e">&ccirc;iaokaze</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_s;&c_ja;&c_k;&c_ib;</trd>,
    <trd>&c_u;&c_s;&c_ja;&c_l;&c_ja;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="he">&#x5D0;&#x5EA; &#x5DB;&#x5DC;</trd>
  <trd lng="ms">semua</trd>
  <trdgrp lng="de">
    <trd>jederart</trd>,
    <trd>allerhand</trd>,
    <trd>jeglicher</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>all</trd>,
    <trd>every kind of</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>de toda clase</trd>,
    <trd>de todo tipo</trd>
  </trdgrp>
  <trd lng="fr">de toute <ind>sorte</ind></trd>
  <trdgrp lng="hu">
    <trd>mindenf&eacute;le</trd>,
    <trd>mindenfajta</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>allerlei</trd>,
    <trd>ieder <klr>(van type)</klr></trd>,
    <trd>alle <klr>(van type)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wszelki</trd>,
    <trd>wszelaki</trd>,
    <trd>ka&zdot;dy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>de todo o tipo</trd>,
    <trd>de toda esp&eacute;cie</trd>
  </trdgrp>
  <trd lng="ro">de orice fel</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_s;&c_ja;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_s;&c_ja;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">ka&zcaron;d&yacute;</trd>
  <trd lng="sv">all <ind>slags</ind></trd>
</drv>
</art>
