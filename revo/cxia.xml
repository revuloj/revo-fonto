<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cxia.xml,v 1.25 2021/06/10 06:41:29 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>&ccirc;ia</rad></kap>
<drv mrk="cxia.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra>kolektiva <vspec>adj.</vspec></gra>
  <snc>
    <dif>
      De &ccirc;iu <ref cel="spec.0o.klaso">speco</ref>, <ref cel="kvalit.0o">kvalito</ref>:
      <ekz>
        domoj plenaj de <tld/> bona&jcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 6:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        ligno por <tld/> bezono
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 35:24</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi estis preska&ubreve; en <tld/> malbono
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 5:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili maldol&ccirc;igis al ili la vivon <klr>[&#8230;]</klr> per <tld/>j laboroj, kiujn ili
        kruele metis sur ilin
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 1:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> laboro kaj <tld/> lerteco en la faroj estas nur konkurado de unu kontra&ubreve;
        alia
        <fnt>
          <bib>MT</bib>
          <lok>Predikanto 4:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        li perdadis <tld/>n spiritan ekvilibron
        <fnt>
          <bib>Fab3</bib>
          <lok>Bona humoro</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne <tld/>n penson metu sur la langon
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i meritis <tld/>n la&ubreve;don
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> deziris al &scirc;i <tld/>n feli&ccirc;on
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni tie &ccirc;i forlasos <tld/>n superfluan balaston
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        per deklaro sur la dua pa&gcirc;o la a&ubreve;toro jam forlasis <tld/>jn rajtojn siajn
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni povu facile trovi la proverbojn, traktantajn pri <tld/> dezirata temo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> dono estas bono
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="ajn.0a.KOMUNE">ajna<sncref/></ref>
    <refgrp tip="vid">
      <ref cel="arbitr.0a">arbitra</ref>,
      <ref cel="okaz.cxiu0e">&ccirc;iaokaze</ref>,
      <ref cel="cxio.0">&ccirc;io</ref>,
      <ref cel="cxiu.0">&ccirc;iu</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_s;&c_ja;&c_k;&c_ib;</trd>,
    <trd>&c_u;&c_s;&c_ja;&c_l;&c_ja;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="he">&alef;&tav; &kaf;&lamed;</trd>
  <trd lng="ms">semua</trd>
  <trdgrp lng="de">
    <trd>jederart</trd>,
    <trd>allerhand</trd>,
    <trd>jeglicher</trd>,
    <trd>jedweder</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>all</trd>,
    <trd>every kind of</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>de toda clase</trd>,
    <trd>de todo tipo</trd>
  </trdgrp>
  <trd lng="fr">de toute <ind>sorte</ind></trd>
  <trdgrp lng="hu">
    <trd>mindenf&eacute;le</trd>,
    <trd>mindenfajta</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>allerlei</trd>,
    <trd>ieder <klr>(van type)</klr></trd>,
    <trd>alle <klr>(van type)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wszelki</trd>,
    <trd>wszelaki</trd>,
    <trd>ka&zdot;dy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>de todo o tipo</trd>,
    <trd>de toda esp&eacute;cie</trd>
  </trdgrp>
  <trd lng="ro">de orice fel</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_s;&c_ja;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_s;&c_ja;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">ka&zcaron;d&yacute;</trd>
  <trd lng="sv">all <ind>slags</ind></trd>
</drv>
</art>
