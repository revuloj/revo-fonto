<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&ccirc;ia</rad></kap>
<drv mrk="cxia.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <uzo tip="klr">(kolektiva adj)</uzo>
    <dif>
      De &ccirc;iu speco, kvalito:
      <ekz>
        <tld/> homo povas tion fari;
      </ekz>
      <ekz>
        <tld/> dono estas bono
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        &scirc;i meritis <tld/>n la&ubreve;don
        <fnt><bib>Fab1</bib>, <lok>La virineto de maro</lok></fnt>;
      </ekz>
      <ekz>
        ne <tld/>n penson metu sur la langon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;e <tld/> vetero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_o;&c_zh;&c_n;&c_y;</trd>,
    <trd>&c_u;&c_s;&c_ja;&c_k;&c_ib;</trd>,
    <trd>&c_u;&c_s;&c_ja;&c_l;&c_ja;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">jederart</trd>
  <trd lng="fr">de toute <ind>sorte</ind></trd>
  <trdgrp lng="hu">
    <trd>mindenf&eacute;le</trd>,
    <trd>mindenfajta</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>wszelki</trd>,
    <trd>wszelaki</trd>,
    <trd>ka&zdot;dy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_s;&c_ja;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_s;&c_ja;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">ka&zcaron;d&yacute;</trd>
  <trd lng="sv">all <ind>slags</ind></trd>
</drv>
<drv mrk="cxia.0okaze">
  <kap><tld/>okaze</kap>
  <snc>
    <dif>
      Kia ajn estas la okazo, en &ccirc;iuspeca okazo.  
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a; &c_w;&c_s;&c_ja;&c_l;&c_ja;&c_k;&c_ib;&c_m;
  &c_v;&c_y;&c_p;&c_a;&c_d;&c_k;&c_u;</trd>
  <trd lng="fr">en tous cas</trd>
  <trd lng="hu">mindenesetre</trd>
  <trdgrp lng="pl">
    <trd>w ka&zdot;dym <ind>przypadku</ind></trd>,
    <trd>w ka&zdot;dym <ind>wypadku</ind></trd>,
    <trd>tak czy owak</trd>,
    <trd>tak czy siak</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_o; &c_v;&c_s;&c_ja;&c_k;&c_o;&c_m;
  &c_s;&c_l;&c_u;&c_ch;&c_a;&c_je;</trd>
</drv>
</art>
