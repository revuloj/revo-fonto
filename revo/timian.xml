    <trd lng="la">Thymus vulgaris</trd>
  </bld>
  <trd lng="de">Thymian</trd>
  <trd lng="fr">thym</trd>
  <trd lng="nl">tijm</trd>
  <trd lng="tr">kekik</trd>
