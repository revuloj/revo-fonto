<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: timian.xml,v 1.31 2023/10/21 13:52:03 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>timian</rad>/o
</kap>
<drv mrk="timian.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="lamiac.0oj">lamiacoj</ref>
      (<trd lng="la">Thymus</trd>);
      tre aroma <ref tip="lst" cel="arbust.0o" lst="voko:lamioparencoj">arbusto</ref>
      a&ubreve; duonarbusto kun verticiloj a&ubreve; kapetoj de 
      blankaj &gcirc;is violkoloraj floroj, kies korolo estas 
      tubetoforma kun du lipoj, kies malsupra havas tri lobojn;
      pluraj specioj estas uzataj 
      kiel <ref tip="lst" cel="spic.0o" lst="voko:spicoj">spico</ref>
      kaj kuracherbo:
      <ekz>
        ni iris super la alta <tld/>o, kiu tamen ankora&ubreve; bonodoris, kvankam la suno elsekigis &gcirc;iajn foliojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen petroselo kaj floranta <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de unu patrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        bela herbejo, kovrita de primoloj, sova&gcirc;aj <tld/>oj kaj miloj da aliaj floroj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        per virga ru&gcirc;o <tld/>o flamas
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili sukcesas senmaskigi fra&ubreve;dantojn, kiuj vendas malmultekostan
        mielon &leftquot;el &ccirc;iaj floroj&rightquot; sub la nomo de
        multekosta &leftquot;mielo de
        <tld/>o&rightquot;<fnt><aut>Robert Roze</aut>: <vrk>Vivisekcio de
        polenero</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero 11a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/7/7e/THYMUS_VULGARIS_-_AGUDA_-_IB-910_%28Tim%C3%B3%29.JPG/640px-THYMUS_VULGARIS_-_AGUDA_-_IB-910_%28Tim%C3%B3%29.JPG" prm="CC BY-SA 4.0"><ind>ordinara <tld/>o</ind> (Thymus vulgaris), Tor&agrave; (Segara Katalunio)
         <fnt>
           <aut>Isidre blanc, propra verko</aut>
           <lok><url ref="&WCW;/File:THYMUS_VULGARIS_-_AGUDA_-_IB-910_(Tim%C3%B3).JPG">&WCW;/File:THYMUS_VULGARIS_-_AGUDA_-_IB-910_(Tim%C3%B3).JPG</url></lok>
         </fnt>
      <trd lng="bg">&c_o;&c_b;&c_i;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n;&c_a; &c_m;&c_a;&c_shch;&c_je;&c_r;&c_k;&c_a;</trd>
      <trdgrp lng="hu">
        <trd>kerti <ind>kakukkf&udblac;</ind></trd>,
        <trd>pecsenyef&udblac;</trd>
      </trdgrp>
      <trdgrp lng="ca">
        <trd>farigola</trd>,
        <trd>timonet</trd>
      </trdgrp>
      <trd lng="la">Thymus vulgaris</trd>
    </bld>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_b;&c_o;&c_r;</trd>
  <trd lng="bg">&c_m;&c_a;&c_shch;&c_je;&c_r;&c_k;&c_a;</trd>
  <trd lng="ca">tim&oacute;</trd>
  <trd lng="de">Thymian</trd>
  <trd lng="en">thyme</trd>
  <trd lng="fr">thym</trd>
  <trd lng="hu">kakukkf&udblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x30BF;&#x30C1;&#x30B8;&#x30E3;&#x30B3;&#x30A6;&#x30BD;&#x30A6;</trd>,
    <trd>&#x30BF;&#x30A4;&#x30E0;  <pr>&#x305F;&#x3044;&#x3080;</pr></trd>
  </trdgrp>
  <trd lng="nl">tijm</trd>
  <trd lng="pl">tymianek</trd>
  <trdgrp lng="pt">
    <trd>tomilho</trd>,
    <trd>timo</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_t;&c_i;&c_m;&c_mol;&c_ja;&c_n;</trd>,
    <trd>&c_ch;&c_a;&c_b;&c_r;&c_je;&c_c;</trd>,
    <trd>&c_ch;&c_je;&c_b;&c_r;&c_je;&c_c;</trd>
  </trdgrp>
  <trd lng="tr">kekik</trd>
  <trd lng="sk">tymi&aacute;n</trd>
  <trdgrp lng="cs">
    <trd>dymi&aacute;n</trd>,
    <trd>mate&rcaron;&iacute;dou&scaron;ka</trd>,
    <trd>tymi&aacute;n</trd>
  </trdgrp>
  <trd lng="uk">&#x447;&#x435;&#x431;&#x440;&#x435;&#x446;&#x44C;</trd>
</drv>
</art>
