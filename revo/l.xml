<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: l.xml,v 1.12 2020/02/09 12:10:33 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>7</ofc><rad>lo</rad>
</kap>

<drv mrk="l.0.konsonanto">
  <kap><tld/></kap>
  <uzo tip="fak">LIN</uzo>
  <uzo tip="fak">GRA</uzo>
  <uzo tip="fak">FON</uzo>
  <snc>    
    <dif>
      Nomo de la deksesa litero de la esperanta alfabeto
      <ekz>
        nomoj de la literoj: <klr>[...]</klr>, ko, <tld/>, mo,
        <klr>[...]</klr>
        <fnt>
          <bib>F</bib>
          <lok>&FE; 1</lok>
        </fnt>
      </ekz>, uzata i.a. kiel:
    </dif>
    <subsnc>
      <dif>
        (maj.) signo de formo: <ekz> L-fero</ekz>;
      </dif>
      <trd lng="fr">L<klr> (forme de)</klr></trd>
      <trd lng="pl">L<klr> (w kszta&lstroke;cie)</klr></trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">MAT</uzo>
      <dif>
        (maj.) romana cifero: <ekz> L (50)</ekz>;
      </dif>
      <trd lng="fr">50<klr> (chiffre romain)</klr></trd>
      <trd lng="pl">50<klr> (cyfry rzymskie)</klr></trd>
    </subsnc>
    <subsnc>
      <uzo tip="fak">FIZ</uzo>
      <dif>
        (min.) mallongigo de litro: <ekz>l</ekz>.
      </dif>
      <trd lng="fr">litre<klr> (abr&eacute;viation)</klr></trd>
      <trd lng="pl">litr <klr> (skr&oacute;t)</klr></trd>
    </subsnc> 
  </snc>
  <snc mrk="l.0.telefone">
     <ref tip="vid" cel="hola.0">halo!</ref>
  </snc>
</drv>

</art>

