  <trd lng="ka">&#4311;&#4304;&#4309;&#4312;&#4321; &#4315;&#4334;&#4320;&#4312;&#4309;,</trd>
  <trd lng="kk">&c_b;&#1201;&c_r;&c_y;&c_l;&c_u;</trd>
