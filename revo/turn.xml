<drv mrk="turn.re0e">
  <kap>re<tld/>e</kap>
  <snc mrk="turn.re0e.retro">
    <dif>
      La&ubreve; kontra&ubreve;a direkto; malanta&ubreve;en irante;
      revenante al la deira
      punkto:
      <ekz>
        Mi iros re<tld/>e al mia unua edzo, &ccirc;ar pli bone estis al mi
        tiam, ol nun
        <fnt>
          <bib>MT</bib>
          <lok>&Hos; 2:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        li prenis ilin <klr>[la galo&scirc;ojn]</klr> re<tld/>e
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni ricevas re<tld/>e nian homan formon
        <fnt><bib>Fab1</bib> <lok>Sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        rigardi re<tld/>en;
      </ekz>
      <ekz>
        flamo movata de vento tien kaj re<tld/>en<fnt>B</fnt>;
      </ekz>
      <ekz>
        meti ion re<tld/>e en &gcirc;ian lokon.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_z;&c_a;&c_d;</trd>,
      <trd>&c_u; &c_p;&c_r;&c_o;&c_c;&c_ib;&c_l;&c_je;&c_g;&c_l;&c_y;
      <ind>&c_b;&c_o;&c_k;</ind></trd>,
      <trd>&c_a;&c_d;&c_v;&c_a;&c_r;&c_o;&c_t;&c_n;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vissza</trd>,
      <trd>visszafel&eacute;</trd>
    </trdgrp>
    <trdgrp lng="id">
      <trd><baz>balik</baz>terbalik</trd>,
      <trd><baz>balik</baz>berbalik</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>odwrotnie</trd>,
      <trd>wstecz</trd>,
      <trd>z powrotem</trd>,
      <trd>nazad<klr> (pot. rzadkie)</klr></trd>,
      <trd>w ty&lstroke;</trd>
    </trdgrp>
  </snc>
  <snc mrk="turn.re0e.repage">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Kompense:
      <ekz>
        doni re<tld/>e;
      </ekz>
      <ekz>
        kiam li elspezis spesdekon, li ricevis spesmilon re<tld/>e
        <fnt>
          <bib>Fab1</bib>
          <lok>Fluganta kofro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_a;&c_w;&c_z;&c_a;&c_m;&c_je;&c_n;</trd>
    <trd lng="de">wieder <klr>(zur&uuml;ck)</klr></trd>
    <trd lng="id">sebagai <ind>ganti</ind></trd>
    <trdgrp lng="pl">
      <trd>w zamian</trd>,
      <trd>zwrotnie</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>nazp&aacute;tek</trd>,
    <trd>nazp&ecaron;t <klr>(adv.)</klr></trd>,
    <trd>zp&aacute;tky</trd>
  </trdgrp>
