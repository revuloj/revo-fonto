<drv mrk="turn.akvo0o">
  <kap>akvo<tld/>o</kap>
  <snc mrk="turn.akvo0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      Loko de akvejo, kie akvo <tld/>i&gcirc;as kun ensu&ccirc;a forto:
      <ekz>
        ili haltigas a&ubreve; turnas la &scirc;ipon, por eviti
        dan&gcirc;erajn akvo<tld/>ojn
        <fnt><bib>Far2</bib> <lok>&ccirc;apitro 12a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la mondo <klr>[&#8230;]</klr> estas grandega akvo<tld/>o, en kiu la homoj estas &jcirc;etataj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="vortic.0o">vortico</ref>
    <ref tip="sin" cel="kirl.akvo0o.MAR">akvokirlo</ref>
    <trd lng="de">Strudel <klr>(Wasserwirbel)</klr></trd>
    <trdgrp lng="en">
      <trd>whirlpool</trd>,
      <trd>eddy</trd>,
      <trd>vortex</trd>
    </trdgrp>
    <trd lng="es">remolino</trd>
    <trd lng="fr">tourbillon<klr tip="amb"> (d'eau)</klr></trd>
    <trd lng="pl">wir wodny</trd>
    <trd lng="sv">str&ouml;mvirvel</trd>
  </snc>
  <snc mrk="turn.akvo0o.trombo">
    <dif>
      <ref tip="dif" cel="tromb.0o.MET">Trombo<sncref/></ref>:
      <ekz>
        la uragano kriegis, kvaza&ubreve; aro de satanoj,
        rabiaj akvo<tld/>oj batis la &ccirc;ielon, kaj la ventego balais la fundon
        <fnt>
          <bib>FdO</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Wasserhose</trd>
  </snc>
<trdgrp lng="id">
  <trd><baz>pusar</baz>pusaran <klr>(air)</klr></trd>,
  <trd><baz>kisar</baz>kisaran <klr>(air)</klr></trd>,
  <trd>olak <klr>(air)</klr></trd>
</trdgrp>
</drv>

<drv mrk="turn.kap0o">
  <kap>kap<tld/>o</kap>
  <snc mrk="turn.kap0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Sento pri nefirmeco en la spaco, pri manko de sampezo, pro kiu
      &scirc;ajnas turni&gcirc;i la objektoj &ccirc;irka&ubreve;e:
      <ekz>
        la vido de la multaj grandvalora&jcirc;oj povis ka&ubreve;zi kap<tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>Fabelo en sep rakontoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li eksentis fortan kap<tld/>on
        <fnt>
          <bib>Far3</bib>
          <lok>&ccirc;apitro 17a</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinjorino Anneto sentis kap<tld/>on de ravo
        <fnt>
          <aut>V. Devjatnin</aut>
          <vrk>Arturo</vrk>
          <lok>en: Fundamenta Krestomatio, 1904</lok>
        </fnt>;
      </ekz>
      <ekz>
        en kap<tld/>a alto
        <fnt><bib>Fab3</bib> <lok>Sub la saliko</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        li estis belulo kaj <tld/>is la kapojn al la virinoj
        <klr>(malsa&gcirc;igis ilin)</klr>
        <fnt>B</fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="vertigx.0o">verti&gcirc;o</ref>
    <trd lng="es">mareo</trd>
  </snc>
  <trd lng="cs">oto&ccaron;en&iacute; hlavy</trd>
  <trd lng="de">Schwindel</trd>
  <trdgrp lng="en">
    <trd>vertigo</trd>,
    <trd>dizziness</trd>
  </trdgrp>
  <trd lng="fr">vertige</trd>
  <trd lng="hu">sz&eacute;d&uuml;l&eacute;s</trd>
  <trdgrp lng="id">
    <trd>jangar</trd>,
    <trd>pening</trd>,
    <trd>pusing</trd>,
    <trd>puyeng</trd>,
    <trd><ind>sakit</ind> kepala</trd>,
    <trd>vertigo</trd>
  </trdgrp>
  <trd lng="ja">&#12417;&#x307E;&#12356;</trd>
  <trd lng="nl">duizeling</trd>
  <trd lng="pl">zawr&oacute;t g&lstroke;owy</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_o;&c_k;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sk">
    <trd>to&ccaron;enie hlavy</trd>,
    <trd>z&aacute;vrat</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>svindel</trd>,
    <trd>yrsel</trd>
  </trdgrp>
</drv>

<drv mrk="turn.kap0igxo">
  <kap>kap<tld/>i&gcirc;o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="turn.kap0igxo.kapturno">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="dif" cel="turn.kap0o.MED">Kap<tld/>o</ref>:
      <ekz>
        li ricevis kap<tld/>i&gcirc;on, kiam li ekpensis pri tio
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Stevens</nom>, sentante iom da kap<tld/>i&gcirc;o, <klr>[esperis]</klr>, ke &ccirc;iuj atingos la
        malsupron sen ia katastrofo
        <fnt>
          <bib>TAA</bib>
          <lok>&Ccirc;apitro VIII.</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Schwindel<klr>(gef&uuml;hl)</klr></trd>,
    <trd>Taumel</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>vertigo</trd>,
    <trd>dizziness</trd>
  </trdgrp>
  <trd lng="fr">vertige</trd>
  <trd lng="hu">sz&eacute;d&uuml;l&eacute;s</trd>
  <trdgrp lng="id">
    <trd>jangar</trd>,
    <trd>pening</trd>,
    <trd>pusing</trd>,
    <trd>puyeng</trd>,
    <trd><ind>sakit</ind> kepala</trd>,
    <trd>vertigo</trd>
  </trdgrp>
  <trd lng="ja">&#12417;&#x307E;&#12356;</trd>
  <trd lng="nl">duizeling</trd>
  <trd lng="pl">zawr&oacute;t g&lstroke;owy</trd>
  <trd lng="ru">&c_g;&c_o;&c_l;&c_o;&c_v;&c_o;&c_k;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="sv">
    <trd>svindel</trd>,
    <trd>yrsel</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_p;&c_a;&c_m;&c_o;&c_r;&c_o;&c_ch;&c_je;&c_n;&c_n;&c_ja;
    (&c_g;&c_o;&c_l;&c_o;&c_v;&c_i;)</trd>,
    <trd>&c_g;&c_o;&c_l;&c_o;&c_v;&c_o;&c_k;&c_r;&c_u;&c_zh;&c_ib;&c_n;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="turn.parol0o">
  <kap>parol<tld/>o</kap>
  <snc mrk="turn.parol0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="dif" cel="manier.dir0o">Dirmaniero</ref>:
      <ekz>
        fre&scirc;aj, surprizaj parol<tld/>oj
        <fnt><bib>Plumamikoj</bib> <lok>Dramoj en
        esperanto</lok></fnt>;
      </ekz>
      <ekz>
        Petronius esprimis dubon pri trafeco de iu parol<tld/>o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LI</lok>
        </fnt>;
      </ekz>
      <ekz>
        multo el tiuj formoj estas klarigebla per senkonscia traduko esperanten de
        formoj kaj parol<tld/>oj nacilingvaj
        <fnt>
          <bib>Zam</bib>
          <lok>Zamenhof stilisto kaj kreinto de lingvoformoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="prover.0o">proverbo</ref>
    </refgrp>
  </snc>
  <trd lng="cs">slovn&iacute; obrat</trd>
    <trdgrp lng="de">
      <trd>Redewendung</trd>,
      <trd>fester <ind>Ausdruck</ind></trd>,
      <trd>Redensart</trd>,
      <trd>stehende <ind>Redensart</ind></trd>
    </trdgrp>
  <trd lng="en">turn of phrase</trd>
  <trdgrp lng="es">
    <trd>locuci&oacute;n</trd>,
    <trd>giro</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>expression</trd>,
    <trd>tournure<klr> (de phrase)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>sz&oacute;fordulat</trd>,
    <trd>mond&aacute;s</trd>,
    <trd>sz&oacute;l&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8A00;&#x56DE;&#12375;  <pr>&#12356;&#12356;&#x307E;&#x308F;&#12375;</pr></trd>,
    <trd>&#34920;&#x73FE;&#x6CD5;  <pr>&#12402;&#12423;&#12358;&#12370;&#12435;&#x307B;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="nl">zegswijze</trd>
  <trdgrp lng="pl">
    <trd>zwrot<klr> j&eogonek;zykowy</klr></trd>,
    <trd>powiedzenie</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_r;&c_o;&c_t;
  &c_r;&c_je;&c_ch;&c_i;</trd>
  <trd lng="sk">slovn&yacute; zvrat</trd>
  <trd lng="uk">&c_m;&c_o;&c_v;&c_n;&c_i;&c_j; &c_z;&c_v;&c_o;&c_r;&c_o;&c_t;</trd>
</drv>
