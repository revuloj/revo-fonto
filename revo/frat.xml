<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>frat</rad>/o
</kap>
<drv mrk="frat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="frat.0o.GEN">
  <uzo tip="fak">GEN</uzo>
    <dif>
      Virseksa ido de samaj gepatroj, rilate al samdevenaj idoj:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mensogo kaj &scirc;telo estas du <tld/>oj<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="nl">broer</trd>
  </snc>
  <snc mrk="frat.0o.religia">
    <dif>
      Ano de kelkaj religiaj ordenoj kaj de kelkaj societoj.
    </dif>
    <trd lng="nl">broeder</trd>
  </snc>
  <snc mrk="frat.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Persono, kiun oni amas kiel <tld/>on, kaj kiu reciprokas tian amon:
      <ekz>
        amiko fari&gcirc;as <tld/>o en mizero
        <fnt><bib>MT</bib>, <lok>&Sen; 17:17</lok></fnt>;
      </ekz>
      <ekz>
        <ind>milit<tld/>o</ind>.
        <trd lng="fr">fr&egrave;re d'armes</trd>
        <trd lng="nl">wapenbroeder</trd>
      </ekz>
    </dif>
    <trd lng="nl">broeder</trd>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_t;</trd>
  <trd lng="cs">bratr</trd>
  <trd lng="de">Bruder</trd>
  <trd lng="en">brother</trd>
  <trd lng="es">hermano</trd>
  <trd lng="fr">fr&egrave;re</trd>
  <trd lng="he">&alef;&het;</trd>
  <trd lng="pl">brat</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_t;</trd>
  <trd lng="sv">bror</trd>
</drv>

<drv mrk="frat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      De <tld/>o:
      <ekz>
        <tld/>a amo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_c;&c_k;&c_ib;</trd>, 
    <trd>&c_b;&c_r;&c_a;&c_t;&c_e;&c_r;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">br&uuml;derlich</trd>
  <trd lng="es">fraternal</trd>
  <trd lng="fr">fraternel</trd>
  <trd lng="nl">broederlijk</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="frat.0ino">
  <kap><tld/>ino</kap>
  <snc mrk="frat.0ino.GEN">
    <dif>
      Inseksa ido de samaj gepatroj, rilate al samdevenaj idoj.
    </dif>
  </snc>
  <snc mrk="frat.0ino.religia">
    <dif>
      Anino de kelkaj religiaj ordenoj kaj de kelkaj societoj.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_ja;&c_s;&c_t;&c_r;&c_a;</trd>
  <trd lng="en">sister</trd>
  <trd lng="es">hermana</trd>
  <trd lng="fr">s&oelig;ur</trd>
  <trd lng="he">&alef;&het;&vav;&tav;</trd>
  <trd lng="nl">zuster</trd>
  <trd lng="pl">siostra</trd>
  <trd lng="ru">&c_s;&c_je;&c_s;&c_t;&c_r;&c_a;</trd>
  <trd lng="sv">syster</trd>
</drv>

<drv mrk="frat.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="frat.0eco.GEN">
    <dif>
      Parenceco inter <tld/>oj.
    </dif>
  </snc>
  <snc mrk="frat.0eco.sento">
    <dif>
      Ama, helpema, kvaza&ubreve;<tld/>a sento al iu, a&ubreve; &gcirc;enerale
      al la tuta homaro; homamo.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_t;&c_e;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="fr">fraternit&eacute;</trd>
  <trd lng="he">&alef;&het;&vav;&vav;&he;</trd>
  <trd lng="nl">broederschap</trd>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="frat.bo0o">
  <kap>bo<tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="frat.bo0o.edzinafrato">
    <dif>
      <tld lit="F"/>o de la edzino rilate al la edzo.
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_v;&c_a;&c_g;&c_je;&c_r;</trd>, 
      <trd>&c_sh;&c_u;&c_r;&c_y;&c_n;</trd>
    </trdgrp>
    <trd lng="ru">&c_sh;&c_u;&c_r;&c_i;&c_n;</trd>
  </snc>
  <snc mrk="frat.bo0o.edzafrato">
    <dif>
      <tld lit="F"/>o de la edzo rilate al la edzino.
    </dif>
    <trd lng="be">&c_d;&c_z;&c_je;&c_v;&c_je;&c_r;</trd>
    <trd lng="ru">&c_d;&c_je;&c_v;&c_je;&c_r;&c_mol;</trd>
  </snc>
  <snc mrk="frat.bo0o.fratinaedzo">
    <dif>
      La edzo de <tld/>ino rilate al &scirc;iaj ge<tld/>oj.
    </dif>
    <trd lng="be">&c_z;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="ru">&c_z;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="frat.bo0o.bofratinaedzo">
    <dif>
      La edzo de
      <ref cel="frat.bo0ino.edzinafratino">bo<tld/>ino<sncref/></ref>.
    </dif>
    <trd lng="ru">&c_s;&c_v;&c_o;&c_ja;&c_k;</trd>
  </snc>
  <trd lng="en">brother-in-law</trd>
</drv>

<drv mrk="frat.bo0ino">
  <kap>bo<tld/>ino</kap>
  <uzo tip="fak">GEN</uzo>
  <snc mrk="frat.bo0ino.edzinafratino">
    <dif>
      <tld lit="F"/>ino de la edzino rilate al la edzo.
    </dif>
    <trd lng="be">&c_sh;&c_v;&c_a;&c_g;&c_je;&c_r;&c_k;&c_a;</trd>
    <trd lng="ru">&c_s;&c_v;&c_o;&c_ja;&c_ch;&c_je;&c_n;&c_i;&c_c;&c_a;</trd>
  </snc>
  <snc mrk="frat.bo0ino.edzafratino">
    <dif>
      <tld lit="F"/>ino de la edzo rilate al la edzino.
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_a;&c_l;&c_o;&c_w;&c_k;&c_a;</trd>, 
      <trd>&c_z;&c_a;&c_l;&c_v;&c_ib;&c_c;&c_a;</trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_o;&c_l;&c_o;&c_v;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="frat.bo0ino.frataedzino">
    <dif>
      La edzino de <tld/>o rilate al liaj ge<tld/>oj.
    </dif>
    <trdgrp lng="be">
      <trd>&c_b;&c_r;&c_a;&c_t;&c_a;&c_v;&c_a;&c_ja;</trd>, 
      <trd>&c_n;&c_ja;&c_v;&c_je;&c_s;&c_t;&c_k;&c_a;</trd>
    </trdgrp>
    <trd lng="ru">&c_n;&c_je;&c_v;&c_je;&c_s;&c_t;&c_k;&c_a;</trd>
  </snc>
  <snc mrk="frat.bo0ino.bofrataedzino">
    <dif>
      La edzino de
      <ref cel="frat.bo0o.edzinafrato">bo<tld/>o<sncref/></ref>.
    </dif>
    <trd lng="be">&c_ja;&c_t;&c_r;&c_o;&c_w;&c_k;&c_a;</trd>
    <trd lng="ru">&c_n;&c_je;&c_v;&c_je;&c_s;&c_t;&c_k;&c_a;</trd>
  </snc>
  <trd lng="fr">sister-in-law</trd>
</drv>

<drv mrk="frat.duon0o">
  <kap>duon<tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Virseksa ido nur de sama patro a&ubreve; nur de sama patrino, rilate al
      la sampatraj a&ubreve; sampatrinaj idoj.
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_v;&c_o;&c_d;&c_n;&c_y; &c_b;&c_r;&c_a;&c_t;</trd>
  <trd lng="de">Halbbruder</trd>
  <trd lng="es">medio <ind>hermano</ind></trd>
  <trd lng="fr">demi-fr&egrave;re</trd>
  <trd lng="he">&alef;&het; &het;&vav;&resh;&gimel;</trd>
  <trd lng="nl">halfbroer</trd>
  <trd lng="ru">&c_s;&c_v;&c_o;&c_d;&c_n;&c_y;&c_j; &c_b;&c_r;&c_a;&c_t;</trd>
  <trd lng="sv">halvbror</trd>
</drv>

<drv mrk="frat.inter0igxi">
  <kap>inter<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Interrilati kvaza&ubreve;<tld/>e, kun <tld/>aj sentoj.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_b;&c_r;&c_a;&c_t;&c_a;&c_c;&c_c;&c_a;</trd>
  <trd lng="fr">fraterniser</trd>
  <trd lng="nl">verbroederen</trd>
  <trd lng="ru">&c_p;&c_o;&c_b;&c_r;&c_a;&c_t;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>

<drv mrk="frat.lakto0onutro0o">
  <kap>lakto<tld/>o, nutro<tld/>o</kap>
  <uzo tip="fak">GEN</uzo>
  <snc>
    <dif>
      Ido mamnutrita de fremda patrino, rilate al la propraj idoj de &ccirc;i
      tiu lasta.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_o;&c_ch;&c_n;&c_y; &c_b;&c_r;&c_a;&c_t;</trd>
  <trd lng="fr">fr&egrave;re de lait</trd>
  <trd lng="ru">&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_y;&c_j; &c_b;&c_r;&c_a;&c_t;</trd>
</drv>
</art>
