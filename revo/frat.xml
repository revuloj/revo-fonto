<drv mrk="frat.0a">
  <kap><tld/>a</kap>
  <snc mrk="frat.0a.KOMUNE">
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <klr>[ili]</klr> promesis al si <tld/>an amon &gcirc;is la morto
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="frat.0a.simila">
    <dif>
      Simila al rilato inter <tld/>oj:
      <ekz>
        <tld/>a interligo
        <fnt>
          <bib>MT</bib>
          <lok>Amos 1:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>aj popoloj de Danujo kaj Svedujo
        <fnt>
          <bib>Fab2</bib>
          <lok>Dano Holger</lok>
        </fnt>;
      </ekz>
      <ekz>
        sendante al Vi <tld/>an kaj samideanan saluton
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>El la mondo homarana (hilelista).</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_r;&c_a;&c_c;&c_k;&c_ib;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_e;&c_r;&c_s;&c_k;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_b;&c_r;&c_a;&c_t;&c_s;&c_k;&c_i;</trd>
  <trd lng="cs">bratrsk&yacute;</trd>
  <trd lng="de">br&uuml;derlich</trd>
  <trdgrp lng="en">
    <trd>brotherly</trd>,
    <trd>fraternal</trd>
  </trdgrp>
  <trd lng="es">fraterno</trd>
  <trd lng="fa">&a_b;&a_r;&a_A;&a_d;&a_r;&a_A;&a_n;&a_h;</trd>
  <trd lng="fr">fraternel</trd>
  <trd lng="hu">testv&eacute;ri</trd>
  <trd lng="io">fratulala</trd>
  <trd lng="nl">broederlijk</trd>
  <trd lng="pl">braterski</trd>
  <trdgrp lng="pt">
    <trd>fraterno</trd>,
    <trd>fraternal</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_t;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">bratsk&yacute;</trd>
  <trdgrp lng="uk">
