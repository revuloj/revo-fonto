<drv mrk="frat.bo0o">
  <kap><ofc>*</ofc>bo<tld/>o
  <fnt><bib>F</bib>, <lok>Universala Vortaro, bo'</lok></fnt></kap>
  <snc mrk="frat.bo0o.edza">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="lst" cel="frat.0o" lst="voko:parencoj"><tld lit="F"/>o</ref> de la edzo rilate al la edz(in)o.
    </dif>
    <trd lng="be">&c_d;&c_z;&c_je;&c_v;&c_je;&c_r;</trd>
    <trd lng="es">cu&ntilde;ado</trd>
    <trd lng="fa">&a_b;&a_r;&a_A;&a_d;&a_r;&a_sh;&a_w;&a_h;&a_r;</trd>
    <trd lng="ru">&c_d;&c_je;&c_v;&c_je;&c_r;&c_mol;</trd>
  </snc>
  <snc mrk="frat.bo0o.edzina">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="F"/>o de la edzino rilate al la edz(in)o:
      <ekz>
        &scirc;ia <tld/>o estas mia bo<tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 36</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_v;&c_a;&c_g;&c_je;&c_r;</trd>,
      <trd>&c_sh;&c_u;&c_r;&c_y;&c_n;</trd>
    </trdgrp>
    <trd lng="es">cu&ntilde;ado</trd>
    <trd lng="fa">&a_b;&a_r;&a_A;&a_d;&a_r;&a_z;&a_n;</trd>
    <trd lng="pl">szwagier</trd>
    <trd lng="ru">&c_sh;&c_u;&c_r;&c_i;&c_n;</trd>
  </snc>
  <snc mrk="frat.bo0o.frataedzo">
    <uzo tip="fak">GEN</uzo>
    <dif>
      La edzo de <tld/>(in)o rilate al ties ge<tld/>oj.
    </dif>
    <trd lng="be">&c_z;&c_ja;&c_c;&c_mol;</trd>
    <trd lng="es">cu&ntilde;ado</trd>
    <trdgrp lng="fa">
      <trd>&a_sh;&a_w;&a_h;&a_r;&a_kh;&a_w;&a_A;&a_h;&a_r;</trd>,
      <trd>&a_sh;&a_w;&a_h;&a_r; &a_b;&a_r;&a_A;&a_d;&a_r;</trd>
    </trdgrp>
    <trd lng="ru">&c_z;&c_ja;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="frat.bo0o.bofrataedzo">
    <uzo tip="fak">GEN</uzo>
    <dif>
      La edzo de bo<tld/>(in)o:
    </dif>
    <subsnc mrk="frat.bo0o.edzofrataedzo">
      <dif>
        La edzo de <tld/>(in)o de la edzo.
      </dif>
