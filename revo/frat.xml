    <trd>&c_b;&c_r;&c_a;&c_t;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_je;&c_r;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="vo">
