<drv mrk="frat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="frat.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Virseksa <ref tip="lst" cel="id.0o" lst="voko:parencoj">ido</ref>
      de samaj gepatroj, rilate al samdevenaj idoj:
      <ekz>
        li estas mia onklo, &ccirc;ar mia patro estas lia <tld/>o
        <fnt>
          <bib>F</bib>
          <lok>&FE; 18</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Kain</nom> levi&gcirc;is kontra&ubreve; sian <tld/>on <nom>Habel</nom> kaj mortigis lin
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 4:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Rebeka</nom> havis <tld/>on, kiu estis nomata <nom>Laban</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 24:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj filoj kaj liaj filinoj estis <klr>[&#8230;]</klr> trinkantaj vinon en la domo de
        ilia unuenaskita <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 1:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        en tiu momento aperis la tria <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;efa merito de <tld/>oj <nom>Grimm</nom> estas <klr>[&#8230;]</klr> kolekti tiel grandan nombron
        da fabeloj
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;emela <tld/>o
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        malrajta <tld/>o
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam <tld/>oj batalas, fremdulo ne eni&gcirc;u
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz><ekz>
        <uzo tip="stl">FIG</uzo>
        mensogo kaj &scirc;telo estas du <tld/>oj
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="hu">
      <trd>fiv&eacute;r</trd>,
      <trd>fi&uacute;testv&eacute;r</trd>
    </trdgrp>
  <trdgrp lng="pt">
    <trd>irm&atilde;o</trd>,
    <trd>mano</trd>
  </trdgrp>
  </snc>
  <snc mrk="frat.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Homo, kiun oni amas kiel <tld/>on, kaj kiu reciprokas tian
      amon:
      <ekz>
        amiko fari&gcirc;as <tld/>o en mizero
        <fnt><bib>MT</bib>, <lok>&Sen; 17:17</lok></fnt>;
      </ekz>
      <ekz>
        li estas <tld/>o-kamarado kun &ccirc;iuj ministroj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>;
      </ekz><ekz>
        <ind>milit<tld/>o</ind>.
        <trd lng="fr">fr&egrave;re d'armes</trd>
        <trd lng="hu">fegyvert&aacute;rs</trd>
        <trd lng="nl">wapenbroeder</trd>
        <trd lng="de">Waffenbruder</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kamara.0o">kamarado</ref>,
      <ref cel="koleg.0o.kunulo">kolego<sncref/></ref>,
      <ref cel="kompan.0ano.akompananto">kompaniano<sncref/></ref>,
      <ref cel="kun.0ulo">kunulo</ref>
    </refgrp>
    <trd lng="hu">testv&eacute;r</trd>
    <trdgrp lng="pt">
      <trd>irm&atilde;o</trd>,
      <trd>mano</trd>
    </trdgrp>
  </snc>
  <snc mrk="frat.0o.religia">
    <dif>
      Titolo de ano en kelkaj religiaj ordenoj kaj en kelkaj societoj:
      <ekz>
        a&ubreve;dinte la bruon, la mona&hcirc;oj komprenos, ke la tentoj sie&gcirc;as iun fraton, kaj &ccirc;iuj
        kune iros pre&gcirc;i
        <fnt>
          <bib>IKr</bib>
          <lok>TRI Musketeroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la predikanto direktas sin al <klr>[&#8230;]</klr> &leftquot;<tld/>oj per Kristo&rightquot;
        <klr>[&#8230;]</klr> la&ubreve; la kutimo
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj-homaranoj
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>El la mondo homarana (hilelista).</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld lit="F"/>oj de l&#8217; Karitato
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="monahx.0o">mona&hcirc;o</ref>
    <trdgrp lng="de">
      <trd>Ordensbruder</trd>,
      <trd>Klosterbruder</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>bar&aacute;t</trd>,
      <trd>testv&eacute;r</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>irm&atilde;o</trd>,
      <trd>frei</trd>
    </trdgrp>
  </snc>
  <snc mrk="frat.0o.homara">
    <dif>
      <ref tip="dif" cel="frat.hom0o">Hom<tld/>o</ref>:
      <ekz>
        karaj samideanoj, <tld/>oj kaj <tld/>inoj el la granda tutmonda homa familio
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Unua Kongreso Esperantista en Boulogne sur Mer en la 5a de a&ubreve;gusto
          1905</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu el ili vidu en sia proksimulo nur homon kaj <tld/>on
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        iu viro <klr>[&#8230;]</klr> vidis la kruelan kaj
        malnoblegan manieron la&ubreve; kiu oni traktis unu el liaj <tld/>oj,
        unu estulon kun du piedoj kaj sen plumoj, kiu havas animon 
        <fnt><aut>Voltero, trad. Lanti</aut>:
          <vrk>Kandid a&ubreve; la Optimismo</vrk>.
          <lok>SAT: Paris, 1929. &Ccirc;ap. 3&#170;</lok>
        </fnt>.
      </ekz>
    </dif>
  <trdgrp lng="de">
    <trd>Mitmensch</trd>,
    <trd>N&auml;chster <klr>(Mitmensch)</klr></trd>
  </trdgrp>
  </snc>
