<drv mrk="frat.ge0oj">
  <kap><ofc>*</ofc>ge<tld/>oj</kap>
  <snc mrk="frat.ge0oj.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="F"/>o(j) kaj <tld/>ino(j):
      <ekz>
        mia <tld/>o kaj <tld/>ino (ge<tld/>oj)
        <fnt><bib>F</bib>, <lok>&FE; 36</lok></fnt>;
      </ekz>
      <ekz>
        Petro, Anno kaj Elizabeto estas miaj ge<tld/>oj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili sin reciproke tiel amis, kvaza&ubreve; ili estus ge<tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        amo kaj &jcirc;aluzo estas ge<tld/>oj
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="cs">
      <trd>brat&rcaron;i a sestry</trd>,
      <trd>sourozenci oboj&iacute;ho pohlav&iacute;</trd>
    </trdgrp>
    <trd lng="de">Geschwister</trd>
    <trd lng="en">brothers and sisters</trd>
    <trd lng="es">hermanos y hermanas</trd>
    <trd lng="fa">&a_kh;&a_w;&a_A;&a_h;&a_r; &a_w; &a_b;&a_r;&a_A;&a_d;&a_r;&a_h;&a_A;</trd>
    <trd lng="fy">bruorren en susters</trd>
    <trd lng="io">gefrati</trd>
    <trd lng="nl">broers en zussen</trd>
    <trd lng="sk">bratia a sestry</trd>
    <trdgrp lng="ru">
      <trd>&c_b;&c_r;&c_a;&c_t; &c_i; &c_s;&c_je;&c_s;&c_t;&c_r;&c_a;</trd>,
      <trd>&c_b;&c_r;&c_a;&c_t;&c_mol;&c_ja; &c_i; &c_s;&c_jo;&c_s;&c_t;&c_r;&c_y;</trd>
    </trdgrp>
  </snc>
  <snc mrk="frat.ge0oj.RAR">
    <uzo tip="stl">RAR</uzo>
    <uzo tip="fak">GEN</uzo>
    <dif>
      <tld lit="F"/>o(j) a&ubreve;&#8239;<tld/>ino(j):
      <ekz>
        &ccirc;u li havis ge<tld/>ojn? &ndash; ne, li estis ilia sola infano
        <fnt>
          <bib>ChV</bib>
          <lok>14</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="cs">sourozenci</trd>
    <trd lng="en">siblings</trd>
    <trd lng="fa">&a_h;&a_m;&#8204;&a_n;&f_y;&a_A;&a_h;&a_A;</trd>
    <trd lng="io">frati</trd>
    <trd lng="nl">brussen</trd>
    <trd lng="sv">syskon</trd>
    <trdgrp lng="tok">
      <trd>jan sama</trd>,
      <trd>jan pi mama sama</trd>
    </trdgrp>
    <trd lng="tr">karde&scedil;ler</trd>
    <trd lng="vo">gems</trd>
  </snc>
  <trdgrp lng="fr">
    <trd>fratrie</trd>,
    <trd>fr&egrave;res et s&oelig;urs</trd>
  </trdgrp>
  <trd lng="hu">testv&eacute;rek <klr tip="amb">(fiv&eacute;r/ek
  &eacute;s n&odblac;v&eacute;r/ek)</klr></trd>
  <trd lng="pl">rodze&nacute;stwo</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_a;&c_t; &c_ib; &c_s;&c_je;&c_s;&c_t;&c_r;&c_a;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_i;&nbsp;&c_ib; &c_s;&c_je;&c_s;&c_t;&c_r;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="frat.ge0o">
