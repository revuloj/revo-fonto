<drv mrk="frat.0eco">
  <kap><tld/>eco</kap>
  <snc mrk="frat.0eco.GEN">
    <dif>
      Parenceco inter <tld/>oj.
    </dif>
    <trd lng="pl">braterstwo</trd>
  </snc>
  <snc mrk="frat.0eco.sento">
    <dif>
      Ama, helpema, kvaza&ubreve;<tld/>a sento al iu, a&ubreve;
      &gcirc;enerale al la tuta homaro; <ref cel="am.hom0o">homamo</ref>:
      <ekz>
        mil homoj, kiuj alveturis el la plej diversaj landoj <klr>[&#8230;]</klr> vivis kune
        en la plej sincera amikeco kaj <tld/>eco
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        justeco, egaleco kaj <tld/>eco inter la popoloj en la praktiko estas plene ebla
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni jam fari&gcirc;is kolegoj de vojo <klr>[&#8230;]</klr> ni trinku nun <tld/>econ, kaj ni
        estu pli familiara unu kun la dua
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_a;&c_t;&c_e;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
  <trd lng="cs">bratrstv&iacute;</trd>
  <trd lng="de">Br&uuml;derlichkeit</trd>
  <trdgrp lng="en">
    <trd>brotherhood</trd>,
    <trd>fraternity</trd>
  </trdgrp>
  <trd lng="es">fraternidad</trd>
  <trd lng="fa">&a_b;&a_r;&a_A;&a_d;&a_r;&f_y;</trd>
  <trd lng="fr">fraternit&eacute;</trd>
  <trd lng="fy">bruorskip</trd>
  <trd lng="he">&alef;&het;&vav;&vav;&he;</trd>
  <trd lng="hu">testv&eacute;ris&eacute;g</trd>
  <trd lng="io">fratuleso</trd>
  <trd lng="nl">broederschap</trd>
  <trdgrp lng="pl">
    <trd>braterstwo</trd>,
    <trd>bruderszaft</trd>
  </trdgrp>
  <trd lng="ru">&c_b;&c_r;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
  <trd lng="sk">fraternita</trd>
  <trd lng="tr">karde&scedil;lik</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_r;&c_a;&c_t;&c_je;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_b;&c_r;&c_a;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="frat.0ino">
  <kap><ofc>*</ofc><tld/>ino</kap>
  <snc mrk="frat.0ino.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      Inseksa <ref tip="lst" cel="id.0o" lst="voko:parencoj">ido</ref>
      de samaj gepatroj, rilate al samdevenaj idoj:
      <ekz>
        mia <tld/>ino estas tre bela knabino
        <fnt><bib>F</bib>, <lok>&FE; 33</lok></fnt>;
      </ekz>
      <ekz>
        efektive &scirc;i estas mia <tld/>ino, filino de mia patro, nur ne filino de mia
        patrino
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 20:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>inoj kaj duonpatrino ne rekonis &scirc;in
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi sekretos al edzino, &scirc;i sekretos al <tld/>ino
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Schwester</trd>
    <trdgrp lng="hu">
