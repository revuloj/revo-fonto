<vortaro>
<art mrk="$Id$">
<kap>
  <rad>morkel</rad>/o
</kap>
<drv mrk="morkel.0o">
  <kap><tld/>o</kap>
  <snc mrk="morkel.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro de <ref tip="super" cel="fung.sak0oj">sakfungoj</ref>
      (<trd lng="la">Morchella</trd>), kun bongusta man&gcirc;ebla
      fruktokorpo.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_a;&c_r;&c_zh;&c_o;&c_k;</trd>
  <trd lng="ca">m&uacute;rgola</trd>
  <trd lng="de">Morchel</trd>
  <trd lng="fr">morille</trd>
  <trd lng="hu">kucsmagomba</trd>
  <trdgrp lng="nl">
    <trd>morille</trd>,
    <trd>morielje</trd>
  </trdgrp>
  <trd lng="pt">fung&atilde;o <klr>(fungo comest&iacute;vel)</klr></trd>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_r;&c_ch;&c_o;&c_k;</trd>
</drv>
</art>
