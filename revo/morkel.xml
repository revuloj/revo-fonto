<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: morkel.xml,v 1.20 2022/05/01 12:05:12 revo Exp $">
<kap>
  <rad>morkel</rad>/o,
  <var><kap><rad>mor&hcirc;el</rad>/o</kap></var>
</kap>
<drv mrk="morkel.0o">
  <kap><tld/>o,
    <var><kap>mor&hcirc;elo</kap></var>
  </kap>
  <snc mrk="morkel.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Bongusta man&gcirc;ebla <ref tip="lst" cel="fung.0o.fruktokorpo" lst="voko:fungoj">fungo</ref>;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref>
       de <ref tip="malprt" cel="fung.sak0oj">sakfungoj</ref>
      (<trd lng="la">Morchella</trd>):
      <ekz>
        <tld/>oj estas <klr>[&#x2026;]</klr> grandaj askofungoj kun granda, fragile karneca
        fruktokorpo, havante kvaza&ubreve; pufe kavan stipon kaj nesimetrie rondforman a&ubreve;
        konusforman &ccirc;apelon, kies surfaco havas mallar&gcirc;ajn la&ubreve;longajn kaj la&ubreve;lar&gcirc;ajn
        listelojn, inter kiuj trovi&gcirc;as kavoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20962">Morkelo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/0/0e/Speise_Morchel_Morchella_esculenta.jpg/640px-Speise_Morchel_Morchella_esculenta.jpg" prm="CC BY 3.0">juna <tld/>o (Morchella esculenta), &ccirc;e Ulm, Germanujo
        <fnt>
          <aut>H. Krisp, propra verko</aut>
          <lok><url ref="&WCW;/File:Speise_Morchel_Morchella_esculenta.jpg">&WCW;/File:Speise_Morchel_Morchella_esculenta.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_s;&c_m;&c_a;&c_r;&c_zh;&c_o;&c_k;</trd>
  <trd lng="bg">&c_p;&c_u;&c_m;&c_p;&c_a;&c_l;&c_k;&c_a;</trd>
  <trd lng="ca">m&uacute;rgola</trd>
  <trd lng="de">Morchel</trd>
  <trd lng="en">morel</trd>
  <trd lng="fr">morille</trd>
  <trd lng="hu">kucsmagomba</trd>
  <trdgrp lng="nl">
    <trd>morille</trd>,
    <trd>morielje</trd>
  </trdgrp>
  <trd lng="pl">smardz</trd>
  <trd lng="pt">fung&atilde;o <klr>(fungo comest&iacute;vel)</klr></trd>
  <trd lng="ro">zb&acirc;rciog &#x21B;uguiat</trd>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_r;&c_ch;&c_o;&c_k;</trd>
  <trdgrp lng="uk">
    <trd>&#x437;&#x43C;&#x43E;&#x440;&#x448;&#x43E;&#x43A;</trd>,
    <trd>&#x437;&#x43C;&#x43E;&#x440;&#x448;&#x43A;&#x430;</trd>
  </trdgrp>
</drv>
</art>
