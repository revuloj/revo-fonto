<vortaro>
<art mrk="$Id$">
<kap>
  <rad>morkel</rad>/o, mor&hcirc;el/o<fnt>Z</fnt>
</kap>
<drv mrk="morkel.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro de <ref tip="super" cel="fung.sak0oj">sakfungoj</ref>
      (<trd lng="la">Morchella</trd>), kun bongusta man&gcirc;ebla
      fruktokorpo.
    </dif>
  </snc>
  <trd lng="de">Morchel</trd>
  <trdgrp lng="nl">
    <trd>morille</trd>,
    <trd>morielje</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_m;&c_o;&c_r;&c_ch;&c_o;&c_k;</trd>
</drv>
</art>
