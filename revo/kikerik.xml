<art mrk="$Id$">
<kap>
  <rad>kikerik</rad>/i <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="kikerik.0i">
  <kap><tld/>i</kap>
  <snc mrk="kikerik.0i.ZOO">
    <ref tip="dif" cel="kokeri.0i.ZOO">kokeriki</ref>
    <ekz>
      li &ccirc;irka&ubreve;rigardis <klr>[...]</klr> <tld/>antan kokon, 
      saltantan korvon
      <fnt><bib>Metrop</bib></fnt>..
