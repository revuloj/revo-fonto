<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: renom.xml,v 1.18 2022/09/13 17:41:05 revo Exp $">
<kap>
  <rad>renom</rad>/o
</kap>
<drv mrk="renom.0o">
  <kap><tld/>o</kap>
  <snc mrk="renom.0o.konateco">
    <dif>
      Granda diskonateco:
      <ekz>
        oni gloros la <tld/>on de Via granda boneco
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 145:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        disvasti&gcirc;is via <tld/>o inter la nacioj pro via beleco, &ccirc;ar &gcirc;i estis perfekta
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 16:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> havas la <tld/>on, ne senallogan por knabino provinca, de homo,
        kiu vivis facilanime kaj dibo&ccirc;e kaj al neniu faris ion malbonan
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de nia hetmano anka&ubreve; jam fortentis honestajn homojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la artisto allogis tiom da homoj
        <fnt>
          <bib>Monato</bib>
          <aut>Grigori Arosev</aut>
          <vrk><url ref="&Steloj;monato2/007301.html">Pentrado: &leftquot;Mia ateliero
          estas mia &gcirc;ardeno&rightquot;</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fam.0o">famo</ref>,
      <ref cel="reputa.0o">reputacio</ref>,
      <ref cel="glor.0o">gloro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_n;&c_a;&c_m;&c_e;</trd>,
    <trd>&c_r;&c_e;&c_p;&c_u;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>dobr&aacute; pov&ecaron;st</trd>,
    <trd>renom&eacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ruf</trd>,
    <trd>Leumund</trd>,
    <trd>Renommee</trd>,
    <trd>Ansehen</trd>
  </trdgrp>
  <trd lng="en">renown</trd>
  <trdgrp lng="fr">
    <trd>renomm&eacute;e</trd>,
    <trd>renom</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&iacute;rn&eacute;v</trd>,
    <trd>ismerts&eacute;g</trd>,
    <trd>renom&eacute;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>faam</trd>,
    <trd>vermaardheid</trd>
  </trdgrp>
  <trd lng="pl">renoma</trd>
  <trd lng="pt">renome</trd>
  <trdgrp lng="ro">
    <trd>faim&abreve;</trd>,
    <trd>renume</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_p;&c_u;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_r;&c_je;&c_n;&c_o;&c_m;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dobr&aacute; poves&tcaron;</trd>,
    <trd>dobr&eacute; meno</trd>,
    <trd>renom&eacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&#x43F;&c_u;&#x43B;&#x44F;&c_r;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&c_r;&c_je;&#x43F;&c_u;&c_t;&c_a;&c_c;&c_ib;&#x44F;</trd>,
    <trd>&c_r;&c_je;&#x43D;&#x43E;&#x43C;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="renom.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="renom.0a.konata">
    <dif>
      Tre konata, <ref tip="dif" cel="fam.0a">fama</ref>:
      <ekz>
        unu el la plej grandaj kaj <tld/>aj &jcirc;urnaloj
        <fnt>
          <bib>Monato</bib>
          <aut>Maul Stefan</aut>
          <vrk><url ref="&Steloj;monato2/007149.html">Reform-fiasko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        apena&ubreve; estas iu <tld/>a a&ubreve;toro grava en la Esperanto-kulturo, kies nomo neniam
        aperis en tiu &ccirc;i revuo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008107.html">A&ubreve;toroj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_y;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_v;&c_u;&c_t;&c_y;</trd>,
    <trd>&c_v;&c_ja;&c_d;&c_o;&c_m;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>angesehen</trd>,
    <trd>renommiert</trd>
  </trdgrp>
  <trd lng="pl">renomowany</trd>
  <trdgrp lng="ro">
    <trd>renumit</trd>,
    <trd>faimos</trd>
  </trdgrp>
</drv>



</art>
