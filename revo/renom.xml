<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: renom.xml,v 1.25 2025/04/05 06:43:42 revo Exp $">
<kap>
  <rad>renom</rad>/o
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="renom.0o">
  <kap><tld/>o</kap>
  <snc mrk="renom.0o.konateco">
    <dif>
      Granda diskonateco:
      <ekz>
        oni gloros la <tld/>on de Via granda boneco
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 145:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        disvasti&gcirc;is via <tld/>o inter la nacioj pro via beleco, &ccirc;ar &gcirc;i estis perfekta
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 16:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> havas la <tld/>on, ne senallogan por knabino provinca, de homo,
        kiu vivis facilanime kaj dibo&ccirc;e kaj al neniu faris ion malbonan
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de nia hetmano anka&ubreve; jam fortentis honestajn homojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Dua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o de la artisto allogis tiom da homoj
        <fnt>
          <aut>Franz-Georg R&ouml;ssler</aut>
            <vrk><url ref="&Monato;/2001/007301.php">Mia ateliero estas mia &gcirc;ardeno</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 16</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fam.0o">famo</ref>,
      <ref cel="presti1.0o">presti&gcirc;o</ref>,
      <ref cel="reputa.0o">reputacio</ref>
    </refgrp>
    <ref tip="vid" cel="glor.0o">gloro</ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>famo</ke>, <ke>diskonateco</ke>, (bona) <ke>reputacio</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_r;&c_e;&c_n;&c_a;&c_m;&c_e;</trd>,
    <trd>&c_r;&c_e;&c_p;&c_u;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_i;&c_z;&c_v;&c_je;&c_s;&c_t;&c_n;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>dobr&aacute; pov&ecaron;st</trd>,
    <trd>renom&eacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ruf</trd>,
    <trd>Leumund</trd>,
    <trd>Renommee</trd>,
    <trd>Ansehen</trd>
  </trdgrp>
  <trd lng="en">renown</trd>
  <trdgrp lng="fr">
    <trd>renomm&eacute;e</trd>,
    <trd>renom</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&iacute;rn&eacute;v</trd>,
    <trd>ismerts&eacute;g</trd>,
    <trd>renom&eacute;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21517;&#22768;  <pr>&#12417;&#12356;&#12379;&#12356;</pr></trd>,
    <trd>&#26377;&#21517;  <pr>&#12422;&#12358;&#12417;&#12356;</pr></trd>,
    <trd>&#35413;&#21028;  <pr>&#12402;&#12423;&#12358;&#12400;&#12435;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>faam</trd>,
    <trd>vermaardheid</trd>
  </trdgrp>
  <trd lng="pl">renoma</trd>
  <trd lng="pt">renome</trd>
  <trdgrp lng="ro">
    <trd>faim&abreve;</trd>,
    <trd>renume</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_p;&c_u;&c_t;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_r;&c_je;&c_n;&c_o;&c_m;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>dobr&aacute; poves&tcaron;</trd>,
    <trd>dobr&eacute; meno</trd>,
    <trd>renom&eacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_p;&c_u;&c_l;&c_ja;&c_r;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_je;&c_p;&c_u;&c_t;&c_a;&c_c;&c_ib;&c_ja;</trd>,
    <trd>&c_r;&c_je;&c_n;&c_o;&c_m;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21517;&#27668; <pr>m&#237;ngq&#236;</pr></trd>,
    <trd>&#21517;&#27683; <pr>m&#237;ngq&#236;</pr></trd>,
    <trd>&#21517;&#26395; <pr>m&#237;ngw&#224;ng</pr></trd>,
    <trd>&#30427;&#35465; <pr>sh&#232;ngy&#249;</pr></trd>,
    <trd>&#30427;&#35709; <pr>sh&#232;ngy&#249;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="renom.0a">
  <kap>
    <tld/>a
  </kap>
  <snc mrk="renom.0a.konata">
    <dif>
      Tre konata, <ref tip="dif" cel="fam.0a">fama</ref>:
      <ekz>
        unu el la plej grandaj kaj <tld/>aj &jcirc;urnaloj
        <fnt><aut>Maul Stefan</aut>:
          <vrk><url ref="&Monato;/2000/007149.php">Reform-fiasko</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/11, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        apena&ubreve; estas iu <tld/>a a&ubreve;toro grava en la Esperanto-kulturo, kies nomo neniam
        aperis en tiu &ccirc;i revuo
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2003/008107.html">A&ubreve;toroj</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="presti1.0a">presti&gcirc;a</ref>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru (bon-)<ke>fama</ke>, <ke>kun bona reputacio</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_s;&c_l;&c_y;&c_n;&c_n;&c_y;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_v;&c_u;&c_t;&c_y;</trd>,
    <trd>&c_v;&c_ja;&c_d;&c_o;&c_m;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>angesehen</trd>,
    <trd>renommiert</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21517;&#12398;&#36890;&#12387;&#12383; <pr>&#12394;&#12398;&#12392;&#12362;&#12387;&#12383;</pr></trd>,
    <trd>&#35413;&#21028;&#12398;&#12424;&#12356; <pr>&#12402;&#12423;&#12358;&#12400;&#12435;&#12398;&#12424;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="pl">renomowany</trd>
  <trdgrp lng="ro">
    <trd>renumit</trd>,
    <trd>faimos</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#26377;&#22768;&#26395; <pr>y&#466;ush&#275;ngw&#224;ng</pr></trd>,
    <trd>&#26377;&#32882;&#26395; <pr>y&#466;ush&#275;ngw&#224;ng</pr></trd>,
    <trd>&#22840;&#21475; <pr>ku&#257;k&#466;u</pr></trd>,
    <trd>&#35463;&#21475; <pr>ku&#257;k&#466;u</pr></trd>,
    <trd>&#20037;&#30427;&#19981;&#34928; <pr>ji&#468;sh&#232;ngb&#249;shu&#257;i</pr></trd>,
    <trd>&#21463;&#21040;&#23562;&#25964; <pr>sh&#242;ud&#224;oz&#363;nj&#236;ng</pr></trd>,
    <trd>&#21487;&#25964; <pr>k&#283;j&#236;ng</pr></trd>,
    <trd>&#26377;&#22768;&#35465; <pr>y&#466;ush&#275;ngy&#249;</pr></trd>,
    <trd>&#26377;&#32882;&#35709; <pr>y&#466;ush&#275;ngy&#249;</pr></trd>
  </trdgrp>
</drv>

