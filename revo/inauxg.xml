<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: inauxg.xml,v 1.24 2025/11/27 02:09:54 revo Exp $">
<kap>
  <ofc>4</ofc> <rad>ina&ubreve;gur</rad>/i <fnt><bib>PVEF</bib></fnt>,
  <var><kap><ofc>3</ofc> <rad var="ac">ina&ubreve;guraci</rad>/o <fnt><bib>JED</bib></fnt></kap></var>
</kap>

<drv mrk="inauxg.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Solene <ref tip="super" cel="ferm.mal0i.komenci">malfermi</ref>, komenci:
      <ekz><tld/>i ekspozicion, monumenton, kurson, novan metodon;</ekz>
      <ekz>
        la re&gcirc;o kaj la tuta popolo <tld/>is la domon de Dio
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 7:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fondo de UEA savis la esperantistojn el teoria&jcirc;oj kaj <tld/>is novan
        periodon, tiun de efektiva uzado de la lingvo
        <fnt>
          <bib>EeP</bib>
          <lok>Kvara Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Platono <klr>[&#8230;]</klr> <tld/>is la uzadon de dialogo por prezenti filozofiajn
        diskutojn
        <fnt>
          <bib>Rsp</bib>
          <lok>Anta&ubreve;parolo de la Tradukinto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Leopoldo la Dua ina&ubreve;guris la unuan <klr>[&#8230;]</klr> &scirc;iplifto<klr>[n]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Maxime Lechien</aut>
          <vrk><url ref="&Monato;/2003/007206.html">La parko de kanaloj en La Louvi&egrave;re</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konduk.en0i.FIG">enkonduki<sncref/></ref>,
      <ref cel="inicia.0i">iniciati</ref>,
      <ref cel="konsek.0i">konsekri</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol; <klr tip="amb">(&c_u;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_a;)</klr></trd>
  <trd lng="ca">inaugurar</trd>
  <trdgrp lng="de">
     <trd>einweihen</trd>,
     <trd>er&ouml;ffnen</trd>,
     <trd>enth&uuml;llen</trd>
  </trdgrp>
  <trd lng="en">to inaugurate</trd>
  <trd lng="es">inaugurar</trd>
  <trd lng="fa">&a_A;&a_f;&a_t;&a_t;&a_A;&a_H; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">inaugurer</trd>
  <trdgrp lng="hu">
     <trd>felavat</trd>,
     <trd>megnyit<klr tip="ind"> (int&eacute;zm&eacute;nyt)</klr></trd>,
     <trd>leleplez<klr tip="ind"> (szobrot)</klr></trd>
  </trdgrp>
  <trd lng="nl">inwijden</trd>
  <trdgrp lng="pl">
    <trd>inaugurowa&cacute;</trd>,
    <trd>rozpoczyna&cacute; uroczy&sacute;cie</trd>
  </trdgrp>
  <trd lng="pt">inaugurar</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o; &c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_r;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_o; &c_v;&c_ib;&c_d;&c_k;&c_r;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_p;&c_o;&c_ch;&c_i;&c_n;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_o;&c_ch;&c_a;&c_t;&c_k;&c_o;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_ib;&c_n;&c_a;&c_v;&c_gu;&c_u;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#24320;&#24149; <pr>k&amacron;im&ugrave;</pr></trd>,
    <trd>&#25581;&#24149; <pr>ji&emacron;m&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="inauxg.0o">
  <kap><tld/>o, <var><kap><tld var="ac"/>o</kap></var></kap>
  <snc>
    <dif>
      Solena malfermo, komenco:
      <ekz>
        alportu sian oferon por la <tld/>o de la altaro
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 7:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        festparolado en la <tld/>o de la 49-a UK en Hago
        <fnt>
          <bib>EeP</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la baz&scirc;tono estis metata en 1816, kaj la <tld var="ac"/>o okazis en 1830,
        la pre&gcirc;ejo estas bela reprezentano de la speco de klasikismo de Pe&scirc;to
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=382745">Kalvinana pre&gcirc;ejo
          (Ferencv&aacute;ros)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        solena <tld/>o de la renovigita katedralo, kun granda religia festo kaj koncertoj
        <fnt>
          <aut>Josette Ducloyer</aut>
          <vrk><url ref="&Monato;/2000/006851.php">Quimper</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 16</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_k;&c_r;&c_y;&c_c;&c_mol;&c_c;&c_jo; <klr tip="amb">(&c_u;&c_r;&c_a;&c_ch;&c_y;&c_s;&c_t;&c_a;&c_je;)</klr></trd>,
    <trd>&c_ib;&c_n;&c_a;&c_w;&c_g;&c_u;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Einweihung</trd>,
     <trd>Er&ouml;ffnung</trd>,
     <trd>Er&ouml;ffnungsfeier</trd>,
     <trd>Enth&uuml;llung</trd>
  </trdgrp>
   <trd lng="en">inauguration</trd>
  <trd lng="es">inaguraci&oacute;n</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_sh;&a_A;&f_y;&a_sh;</trd>,
    <trd>&a_A;&a_f;&a_t;&a_t;&a_A;&a_H;</trd>,
    <trd>&f_p;&a_r;&a_d;&a_h;&#8204;&a_b;&a_r;&a_d;&a_A;&a_r;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">inauguration</trd>
  <trdgrp lng="hu">
     <trd>felavat&aacute;s</trd>,
     <trd>megnyit&aacute;s<klr tip="ind"> (int&eacute;zm&eacute;ny&eacute;)</klr></trd>,
     <trd>avat&oacute;&uuml;nneps&eacute;g</trd>,
     <trd>leleplez&eacute;s<klr tip="ind"> (szobor&eacute;)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>inauguratie</trd>,
    <trd>inwijding</trd>,
    <trd>plechtige <ind>opening</ind></trd>
  </trdgrp>
  <trd lng="pl">inauguracja</trd>
  <trd lng="pt">inaugura&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je; &c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_n;&c_a;&c_u;&c_g;&c_u;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&c_r;&c_o;&c_ch;&c_i;&c_s;&c_t;&c_je; &c_v;&c_ib;&c_d;&c_k;&c_r;&c_i;&c_t;&c_t;&c_ja;</trd>,
    <trd>&c_ib;&c_n;&c_a;&c_v;&c_gu;&c_u;&c_r;&c_a;&c_c;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#25581;&#24149;&#24335; <pr>ji&emacron;m&ugrave;sh&igrave;</pr></trd>,
    <trd>&#24320;&#24149;&#24335; <pr>k&amacron;im&ugrave;sh&igrave;</pr></trd>,
    <trd>&#23601;&#32844;&#20856;&#31036; <pr>ji&ugrave;zh&iacute;di&#462;nl&#464;</pr></trd>,
    <trd>&#24320;&#24149;&#20856;&#31036; <pr>k&amacron;im&ugrave;di&#462;nl&#464;</pr></trd>,
    <trd>&#24320;&#24149;&#31036; <pr>k&amacron;im&ugrave;l&#464;</pr></trd>
  </trdgrp>
</drv>
</art>
