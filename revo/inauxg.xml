<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>ina&ubreve;gur</rad>/i
</kap>
<drv mrk="inauxg.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Solene malfermi, komenci:
      <ekz><tld/>i ekspozicion, monumenton, kurson, novan metodon.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="inicia.0i">iniciati</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">inaugurer</trd>
  <trd lng="nl">inwijden</trd>
  <trd lng="ru">&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o; &c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_mol;</trd>
</drv>
<drv mrk="inauxg.0o0ado">
  <kap><tld/>o, <tld/>ado</kap>
  <snc>
    <dif>
      Solena malfermo, komenco:
      <ekz>fari <tld/>on<fnt>Z</fnt>;</ekz>
      <ekz>
        <tld/>ado de la tomba monumento de D-ro Zamenhof okazis en
        a&ubreve;gusto 1927.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">inauguration</trd>
  <trd lng="nl">inwijding</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_o;&c_r;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_je; &c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je;</trd>,
    <trd>&c_i;&c_n;&c_a;&c_u;&c_g;&c_u;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
