<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: srilank.xml,v 1.15 2023/09/04 05:14:57 revo Exp $">
<kap>
  <rad>Srilank</rad>/o
  <fnt><bib>RLN</bib></fnt>
</kap>

<drv mrk="srilank.0o">
  <kap>
    <tld/>o
  </kap>
  <mlg kod="ISO-3166">LK</mlg>
  <snc mrk="srilank.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="malprt" cel="azi.A0o">Azia</ref>
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">&scirc;tato</ref>
      en la insulo <ref tip="sin" cel="cejlon.0o">Cejlono</ref>
      kun &ccirc;efurbo Kolombo, &gcirc;is 1972 nomata Cejlono;
      teritorio de Respubliko <tld/>o:
      <ekz>
        la ekzemplo de <tld/>o montras, ke por bona vivkvalito, mirakla ekonomia
        kresko nek necesas nek sufi&ccirc;as
        <fnt>
          <aut>A. Giridhar Rao</aut>
          <vrk><url ref="&Monato;/2008/009532.php">La barata ekonomio: miraklo neefika</url></vrk>
          <bib>Monato</bib>
          <lok>2008/10, p. 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        en 2014 petis enirpermeson en Japanion &ccirc;irka&ubreve; 5000 rifu&gcirc;intoj, precipe el
        Nepalo, Turkio, <tld/>o, Mjanmao kaj Vjetnamio
        <fnt>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="&Monato;/2015/011541.php">Statistikoj pensigaj</url></vrk>
          <bib>Monato</bib>
          <lok>2015/10, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Sh;&c_r;&c_y;-&c_L;&c_ja;&c_n;&c_k;&c_a;</trd>
  <trd lng="bg">&c_Sh;&c_r;&c_i; &c_L;&c_a;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>Sri Lanka</trd>,
    <trd>Rep&uacute;blica Socialista Democr&agrave;tica de Sri Lanka</trd>
  </trdgrp>
  <trd lng="de">Sri Lanka</trd>
  <trd lng="en">Sri Lanka</trd>
  <trd lng="fr">Sri Lanka</trd>
  <trd lng="id">Sri Lanka</trd>
  <trd lng="ja">&#x30B9;&#x30EA;&#x30E9;&#x30F3;&#x30AB;  [<ind>&#x3059;&#x308A;&#x3089;&#x3093;&#x304B;</ind>]</trd>
  <trd lng="nl">Sri Lanka</trd>
  <trdgrp lng="pl">
    <trd>Sri Lanka</trd>,
    <trd>Demokratryczna Socjalistyczna Republika <ind>Sri
    Lanka</ind></trd>
  </trdgrp>
  <trd lng="pt">Sri Lanka</trd>
  <trd lng="uk">&c_Sh;&c_r;&c_ib; &#x41B;&c_a;&#x43D;&#x43A;&c_a;</trd>
</drv>

</art>

