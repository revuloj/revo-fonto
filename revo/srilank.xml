<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>srilank</rad>/o
</kap>

<drv mrk="srilank.0ujo">
  <kap>
    <tld lit="S"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="srilank.0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <mlg kod="ISO-3166">LK</mlg>
    <dif>
      <ref tip="malprt" cel="azi.A0o">Azia</ref> 
      <ref tip="lst" cel="sxtat.0o" lst="voko:&scirc;tatoj">lando</ref>
      en la insulo <ref tip="malprt" cel="cejlon.0o">Cejlono</ref> 
      kun &ccirc;efurbo <ref tip="prt" cel="kolomb.0o">Kolombo</ref>.
      &Gcirc;is 1972 nomata Cejlono,
      teritorio de Respubliko Srilanko.
    </dif>
    <trd lng="bg">&c_Sh;&c_r;&c_i; &c_L;&c_a;&c_n;&c_k;&c_a;</trd>
    <trdgrp lng="ca">
      <trd>Sri Lanka</trd>,
      <trd>Rep&uacute;blica Socialista Democr&agrave;tica de Sri Lanka</trd>
    </trdgrp>
    <trd lng="de">Sri Lanka</trd>
    <trd lng="en">Sri Lanka</trd>    
    <trdgrp lng="pl">
      <trd>Sri Lanka</trd>,
      <trd>Demokratryczna Socjalistyczna Republika <ind>Sri
      Lanka</ind></trd>
    </trdgrp>
    <trd lng="pt">Sri Lanka</trd>
  </snc>
</drv>

</art>

