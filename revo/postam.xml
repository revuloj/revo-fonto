<vortaro>
<art mrk="$Id: postam.xml,v 1.15 2015/07/04 08:37:41 revo Exp $">
<kap>
  <rad>postament</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="postam.0o">
  <kap><tld/>o</kap>
  <snc mrk="postam.0o.meblo">
    <dif>
      Portebla <ref tip="super" cel="baz.0o.postamento">bazo</ref>, sur
      kiu
      oni altige metas objekton:
      <ekz>
        <tld/>o de fotografaparato;
      </ekz>
      <ekz>
        <tld/>o de pentra&jcirc;o;
      </ekz>
      <ekz>
        sur bronzaj <tld/>oj orbrilantaj kandelabroj etendis siajn
bran&ccirc;formajn brakojn
        <fnt><bib>Marta</bib>, <lok></lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>podstavec</trd>,
    <trd>postament</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Postament</trd>,
    <trd>Unterbau</trd>
  </trdgrp>
  <trdgrp lng="en">,
    <trd>postament</trd>,
    <trd>base</trd>,
    <trd>pedestal</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_p;&a_A;&f_y;&a_h;</trd>,
    <trd>&a_kh;&a_r;&f_k;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chevalet</trd>,
    <trd>pied <klr>(p. ex. d'appareil photo)</klr></trd>,
    <trd>tablette</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>talapzat</trd>,
    <trd>posztamens</trd>,
    <trd>&aacute;llv&aacute;ny</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voetstuk</trd>,
    <trd>postament</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>atr&aacute;s de</trd>,
    <trd>detr&aacute;s de</trd>,
    <trd>por tr&aacute;s de</trd>    
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_s;&c_t;&c_a;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">podstavec</trd>
</drv>
</art>
