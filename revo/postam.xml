<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: postam.xml,v 1.21 2024/02/17 07:52:35 revo Exp $">
<kap>
  <rad>postament</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="postam.0o">
  <kap><tld/>o</kap>
  <snc mrk="postam.0o.meblo">
    <dif>
      Portebla <ref tip="super" cel="baz.0o.postamento">bazo</ref>, sur
      kiu oni altige metas objekton:
      <ekz>
        sur bronzaj <tld/>oj orbrilantaj kandelabroj etendis siajn
        bran&ccirc;formajn brakojn
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        nigraj kurtenoj, nigra drapo &ccirc;irka&ubreve; <tld/>o de 
        busto <klr>[&#x2026;]</klr> certe efikas
        malgaje-solene en kunveno, dedi&ccirc;ita al memorigo de mortinto
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        stari kiel <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="estrad.0o">estrado</ref>,
      <ref cel="podi.0o">podio</ref>,
      <ref cel="piedes.0o">piedestalo</ref>,
      <ref cel="stativ.0o">stativo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_s;&c_t;&c_a;&c_m;&c_e;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_s;&c_t;&c_a;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>podstavec</trd>,
    <trd>postament</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Postament</trd>,
    <trd>Unterbau</trd>
  </trdgrp>
  <trdgrp lng="en">,
    <trd>postament</trd>,
    <trd>base</trd>,
    <trd>pedestal</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_p;&a_A;&f_y;&a_h;</trd>,
    <trd>&a_kh;&a_r;&f_k;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>chevalet</trd>,
    <trd>pied <klr>(p. ex. d'appareil photo)</klr></trd>,
    <trd>tablette</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>talapzat</trd>,
    <trd>posztamens</trd>,
    <trd>&aacute;llv&aacute;ny</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x67B6;&#x53F0;  <pr>&#x304B;&#x3060;&#x3044;</pr></trd>,
    <trd>&#x753B;&#x67B6;  <pr>&#x304C;&#x304B;</pr></trd>,
    <trd>&#x30B9;&#x30BF;&#x30F3;&#x30C9;  <pr>&#x3059;&#x305F;&#x3093;&#x3069;</pr></trd>,
    <trd>&#x53F0;&#x5EA7;  <pr>&#x3060;&#x3044;&#x3056;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voetstuk</trd>,
    <trd>postament</trd>
  </trdgrp>
  <trd lng="pl">piedesta&lstroke;</trd>
  <trdgrp lng="pt">
    <trd>atr&aacute;s de</trd>,
    <trd>detr&aacute;s de</trd>,
    <trd>por tr&aacute;s de</trd>    
  </trdgrp>
  <trd lng="ro">piedestal</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_s;&c_t;&c_a;&c_m;&c_je;&c_n;&c_t;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_s;&c_t;&c_a;&c_v;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">podstavec</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_s;&c_t;&c_a;&#x43C;&c_je;&#x43D;&c_t;</trd>,
    <trd>&#x43F;&#x2019;&c_jeu;&c_d;&c_je;&c_s;&c_t;&c_a;&#x43B;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5E95;&#x5EA7; <pr>d&#x1D0;zu&#xF2;</pr></trd>,
    <trd>&#x57FA;&#x4E1A; <pr>j&#x12B;y&#xE8;</pr></trd>,
    <trd>&#x57FA;&#x696D; <pr>j&#x12B;y&#xE8;</pr></trd>
  </trdgrp>
</drv>
</art>
