<art mrk="$Id: simeon.xml,v 1.6 2023/08/19 21:22:15 revo Exp $">
<kap>
  <rad>Simeon</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="simeon.0o">
  <kap><tld/>o, <var><kap><tld/></kap></var></kap>
  <snc mrk="simeon.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> hebredevena:
      <ekz>
        en Jerusalem estis viro, kies nomo estis <tld/>, kaj &ccirc;i tiu
        estis
        justa kaj pia
        <fnt><bib>NT</bib>, <lok>&Luk; 2:25</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">Sim&eacute;on</trd>
  <trd lng="it">Simeone</trd>
  <trd lng="pl">Szymon</trd>
  <trdgrp lng="uk">
    <trd>&c_S;&c_je;&#x43C;&c_je;&#x43D;</trd>,
    <trd>&c_S;&c_i;&#x43C;&c_je;&#x43E;&#x43D;</trd>
  </trdgrp>
</drv>
</art>

