<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>bors</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bors.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="bors.0o.medio">
    <uzo tip="fak">EKON</uzo>  
    <dif>
      Kunveno de komercistoj, makleristoj, kasistoj kaj aliaj
      personoj, kiuj komercas valorpaperojn a&ubreve; diversajn varojn.
    </dif>
    <trdgrp lng="pl">
      <trd>gie&lstroke;da</trd>,
      <trd>rynek spekulacyjny</trd>
    </trdgrp>
  </snc>  
  <snc mrk="bors.0o.merkato">
    <uzo tip="fak">EKON</uzo>  
    <dif>
      La spekulaciaj operacioj mem:
      <ekz>
        la <tld/>o estas firma, malvigla.
      </ekz>
    </dif>
    <trd lng="de">B&ouml;rsengesch&auml;fte</trd>
    <trdgrp lng="pl">
      <trd>gie&lstroke;da</trd>,
      <trd>operacje <ind>gie&lstroke;dowe</ind></trd>,
      <trd>operacje <ind>maklerskie</ind></trd>,
      <trd>sesja <ind>gie&lstroke;dowa</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="bors.0o.borsejo">
    <dif>
      Ejo destinita por financaj a&ubreve; komercaj spekulacioj;
      <ref tip="sin" cel="bors.0ejo.EKON"><tld/>ejo</ref>. 
    </dif>
    <trd lng="pl">gie&lstroke;da</trd>
  </snc> 
  <trd lng="be">&c_b;&c_ib;&c_r;&c_zh;&c_a;</trd>
  <trd lng="de">B&ouml;rse</trd>
  <trdgrp lng="en">
    <trd>stock market</trd>,
    <trd>money market</trd>,
    <trd>stock exchange</trd>
  </trdgrp>
  <trd lng="es"><ind>bolsa</ind> de valores</trd>
  <trd lng="fr">bourse <klr tip="ind">(commerce, finance)</klr></trd>
  <trdgrp lng="hu">
    <trd>t&odblac;zsde</trd>,
    <trd>b&ouml;rze</trd>
  </trdgrp>
  <trd lng="nl">beurs</trd>
  <trd lng="ru">&c_b;&c_i;&c_r;&c_zh;&c_a;</trd>
</drv>
  
<drv mrk="bors.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="bors.0ejo.EKON">
    <ref tip="dif" cel="bors.0o.borsejo">borso<sncref/></ref>
    <ekz>
      la re&gcirc;ino Viktorino malfermis la novan <tld/>ejon
      la 28-an de oktobro 1844
      <fnt>
        <aut>Vikipedio</aut>,
        <vrk><url
        ref="http://eo.wikipedia.org/wiki/Re%C4%9Da_Borso_de_Londono">
        Re&gcirc;a Borso de Londono</url></vrk>,
        <lok>2011-02-07</lok>
      </fnt>.
    </ekz>
  </snc>
</drv>
</art>
