<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: maraton.xml,v 1.16 2024/01/25 20:10:34 revo Exp $">
<kap>
  <rad>maraton</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="maraton.0o">
  <kap><tld/>o</kap>
  <snc mrk="maraton.0o.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Tre longa vetkuro 42-kilometra (oficiale 42195-metra):
      <ekz>
        diman&ccirc;e okazis en Panamo la 29a Internacia <tld lit="M"/>o de
        Panamurbo
        <fnt>
        <aut>R. Peres-Franko</aut>,
        <vrk><url ref="http://raporto.info/2005/08/08/panamo-cardona-venkis-en-la-29a-internacia-maratono-de-panamurbo">Panamo:
        Cardona venkis en maratono</url></vrk>,
        <lok>2005-08-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="maraton.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre longa sportado a&ubreve; agado, kiu provas la eltenkapablon de
      partoprenantoj:
      <ekz>
        teatra <tld/>o
        <fnt>
        <aut>Thomas S&uuml;lzle</aut>,
        <vrk><url ref="&Monato;/2003/007993.html">Komenco de la
        teatra maratono</url></vrk>,
        <bib>Monato</bib>,
        <lok>2002-12</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_r;&c_a;&c_t;&c_o;&c_n;</trd>
  <trd lng="bg">&c_m;&c_a;&c_r;&c_a;&c_t;&c_o;&c_n;</trd>
  <trd lng="ca">marat&oacute;</trd>
  <trd lng="cs">maratonsk&yacute; b&ecaron;h</trd>
  <trd lng="de">Marathon</trd>
  <trd lng="en">marathon</trd>
  <trd lng="fr">marathon<klr> (sport)</klr></trd>
  <trd lng="ja">&#x30DE;&#x30E9;&#x30BD;&#x30F3;  <pr>&#x307E;&#x3089;&#x305D;&#x3093;</pr></trd>
  <trd lng="nl">marathon</trd>
  <trdgrp lng="pl">
    <trd>maraton</trd>,
    <trd>bieg marato&nacute;ski</trd>
  </trdgrp>
  <trd lng="pt">maratona</trd>
  <trd lng="ro">maraton</trd>
  <trd lng="sk">marat&oacute;nsky beh</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x430;&#x440;&#x430;&#x444;&#x43E;&#x43D;&#x441;&#x44C;&#x43A;&#x438;&#x439; &#x431;&#x456;&#x433;</trd>,
    <trd>&#x43C;&#x430;&#x440;&#x430;&#x444;&#x43E;&#x43D;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x99AC;&#x62C9;&#x677E;&#x8CFD; <pr>m&#x1CE;l&#x101;s&#x14D;ngs&#xE0;i</pr></trd>,
    <trd>&#x9A6C;&#x62C9;&#x677E;&#x8D5B; <pr>m&#x1CE;l&#x101;s&#x14D;ngs&#xE0;i</pr></trd>,
    <trd>&#x9577;&#x8DDD;&#x96E2;&#x6BD4;&#x8CFD; <pr>ch&#xE1;ngj&#xF9;l&#xED;b&#x1D0;s&#xE0;i</pr></trd>,
    <trd>&#x957F;&#x8DDD;&#x79BB;&#x6BD4;&#x8D5B; <pr>ch&#xE1;ngj&#xF9;l&#xED;b&#x1D0;s&#xE0;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="maraton.duon0o">
  <kap>duon<tld/>o</kap>
  <snc mrk="maraton.duon0o.SPO">
  <uzo tip="fak">SPO</uzo>
  <dif>
    Vetkuro duone longa kiel <tld/>o:
    <ekz>
      du-tri fojojn jare mi kuras duon<tld/>on (21 km)
      <fnt>
        <aut>John Wells</aut>,
        <vrk><url ref="http://www.phon.ucl.ac.uk/home/wells/esp2.htm">Mia
        neprofesia pa&gcirc;o</url></vrk>,
        <lok>1998-11-11</lok>
      </fnt>.
    </ekz>
  </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_w;&c_m;&c_a;&c_r;&c_a;&c_t;&c_o;&c_n;</trd>
  <trd lng="ca">mitja <ind>marat&oacute;</ind></trd>
  <trd lng="de">Halbmarathon</trd>
  <trd lng="en">half marathon</trd>
  <trd lng="fr">semi-marathon</trd>
  <trd lng="nl">halve <ind>marathon</ind></trd>
  <trd lng="pt">meia-maratona</trd>
  <trdgrp lng="zh">
    <trd>&#x534A;&#x99AC; <pr>b&#xE0;nm&#x1CE;</pr></trd>,
    <trd>&#x534A;&#x9A6C; <pr>b&#xE0;nm&#x1CE;</pr></trd>
  </trdgrp>
</drv>
</art>

