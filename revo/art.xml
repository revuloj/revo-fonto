<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>art</rad>/o
</kap>
<drv mrk="art.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <tezrad/>
  <snc mrk="art.0o.tehxniko">
    <dif>
      Praktika apliko de scioj kaj reguloj akiritaj per iutema studo:
      <ekz>
        medicino, diplomatio, pedagogio, konstruado estas <tld/>oj;
      </ekz>
      <ekz>
        la&ubreve; &ccirc;iuj le&gcirc;oj de la
        <tld/>o<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vivon travivi estas <tld/>' malfacila
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        al la fi&scirc;o ne instruu na&gcirc;<tld/>on
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="teknik.0o">te&hcirc;niko</ref>,
      <ref cel="meti.0o">metio</ref>
    </refgrp>
  </snc>
  <snc mrk="art.0o.belarto">
    <ref tip="dif" cel="art.bel0o">belarto</ref>
    <ekz>
      la <tld/>o estas imito, sed ne simpla kopio de la naturo.
    </ekz>
  </snc>
  <snc mrk="art.0o.artefaro">
    <dif>
      Kreado de la homa faro kontraste al la kreado de la naturo:
      <ekz>
        tiu haveno estas kreita de naturo kaj aran&gcirc;ita de <tld/>o.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_j;&c_s;&c_t;&c_e;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">art</trd>
  <trd lng="de">Kunst</trd>
  <trd lng="en">art</trd>
  <trdgrp lng="fr">
    <trd>art</trd>,
    <trd>technique</trd>,
    <trd>savoir-faire</trd>
  </trdgrp>
  <trd lng="hu">m&udblac;v&eacute;szet</trd>
  <trd lng="ru">&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="art.0a">
  <kap><tld/>a</kap>
  <snc mrk="art.0a.tehxnika">
    <dif>
      Koncernanta <tld/>on<sncref ref="art.0o.tehxniko"/>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_m;&c_je;&c_l;&c_y;</trd>,
      <trd>&c_m;&c_a;&c_j;&c_s;&c_t;&c_e;&c_r;&c_s;&c_k;&c_ib;</trd>
    </trdgrp>
    <trd lng="de">Kunst-</trd>
    <trd lng="hu">m&udblac;v&eacute;szeti</trd>
    <trd lng="ru">&c_i;&c_s;&c_k;&c_u;&c_s;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="art.0a.belarta">
    <ref tip="dif" cel="art.bel0a">belarta</ref>
    <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_k;&c_ib;</trd>
    <trd lng="ca">art&iacute;stic</trd>
    <trd lng="de">kunstvoll</trd>
    <trd lng="fr">artistique</trd>
    <trd lng="hu">m&udblac;v&eacute;szi</trd>
    <trd
    lng="ru">&c_h;&c_u;&c_d;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="art.0a.artefarita">
    <ref tip="dif" cel="art.0efarita">artefarita</ref>
    <ekz>
      &ccirc;e la <tld/>a lumo;
    </ekz>
    <ekz>
      <tld/>e da&ubreve;rigata juneco;
    </ekz>
    <ekz>
      <tld/>a lingvo.
    </ekz>
    <trd lng="be">&c_sh;&c_t;&c_u;&c_ch;&c_n;&c_y;</trd>
    <trd lng="de">k&uuml;nstlich</trd>
    <trd lng="fr">artificiel</trd>
    <trd lng="hu">m&udblac;-</trd>
    <trd lng="ru">&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>

<drv mrk="art.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="art.0ajxo.tehxnika">
    <dif>
      Io farita la&ubreve; la reguloj de iu <tld/>o.
    </dif>
    <trd lng="de">Kunstwerk</trd>
    <trd lng="fr">&oelig;uvre</trd>
    <trd lng="hu">m&udblac;alkot&aacute;s</trd>
  </snc>
  <snc mrk="art.0ajxo.belarta">
    <ref tip="dif" cel="art.bel0ajxo">bel<tld/>a&jcirc;o</ref>
    <trd lng="be">&c_m;&c_a;&c_s;&c_t;&c_a;&c_c;&c_k;&c_ib; &c_t;&c_v;&c_o;&c_r;</trd>
    <trd lng="ca">obra d'<ind>art</ind></trd>
    <trd lng="de">Kunstwerk</trd>
    <trd lng="fr">&oelig;uvre d'art</trd>
    <trd lng="hu">m&udblac;v&eacute;szi dolog</trd>
    <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je; &c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_a;</trd>
  </snc>
  <snc mrk="art.0ajxo.artefaritajxo">
    <ref tip="dif" cel="art.0efaritajxo"><tld/>efarita&jcirc;o:</ref>
    <ekz>
      <tld/>a&jcirc;a hararo.
    </ekz>
  </snc>
</drv>

<drv mrk="art.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Delikata sento pri la beleco.
    </dif>
  </snc>
  <trd lng="de">Kunstsinn</trd>
  <trd lng="fr">sens <ind>artistique</ind></trd>
  <trd lng="hu">m&udblac;&eacute;rz&eacute;k</trd>
</drv>

<drv mrk="art.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu praktikas bel<tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_a;&c_s;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_d;&c_z;&c_je;&c_ja;&c_ch; &c_k;&c_u;&c_l;&c_mol;&c_t;&c_u;&c_r;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">artista</trd>
  <trd lng="de">K&uuml;nstler</trd>
  <trd lng="fr">artiste</trd>
  <trd lng="hu">m&udblac;v&eacute;sz</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_ja;&c_t;&c_je;&c_l;&c_mol; &c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_h;&c_u;&c_d;&c_o;&c_zh;&c_n;&c_i;&c_k;<klr> (&c_v; &c_sh;&c_i;&c_r;&c_o;&c_k;&c_o;&c_m; &c_s;&c_m;&c_y;&c_s;&c_l;&c_je; &c_s;&c_l;&c_o;&c_v;&c_a;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="art.0efarita">
  <kap><tld/>efarita</kap>
  <snc>
    <dif>
      Farita per homa <tld/>o, sed ne kreita de la naturo.
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_t;&c_u;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">artificial</trd>
  <trd lng="de">k&uuml;nstlich</trd>
  <trd lng="fr">artificiel</trd>
  <trd lng="hu">mesters&eacute;ges</trd>
  <trd lng="ru">&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="art.0efaritajxo">
  <kap><tld/>efarita&jcirc;o</kap>
  <snc>
    <dif>
      Objekto a&ubreve; fenomeno el homa agado.
    </dif>
    <refgrp tip="sin">
      <ref cel="art.0ajxo.artefaritajxo">arta&jcirc;o <sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>artefact</trd>
  </trdgrp>
</drv>

<drv mrk="art.bel0a">
  <kap>bel<tld/>a</kap>
  <snc>
    <dif>
      Konforma al la reguloj de la beleco, <tld/>oplena, <tld/>ista,
      <tld/>isma.
    </dif>
  </snc>
  <trd lng="ca">art&iacute;stic</trd>
  <trd lng="de">kunstvoll</trd>
  <trd lng="fr">artistique</trd>
  <trd lng="ru">&c_h;&c_u;&c_d;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="art.bel0o">
  <kap>bel<tld/>o</kap>
  <snc mrk="art.bel0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      &Ccirc;iu el la <ref tip="super"
      cel="art.0o.tehxniko"><tld/>oj<sncref/></ref>,
      kiuj celas belon: arkitekturo, beletro, muziko, skulptado,
      pentr<tld/>o...
    </dif>
  </snc>
  <trd lng="ca">belles <ind>arts</ind></trd>
  <trd lng="fr">beaux-arts</trd>
  <trd lng="hu">sz&eacute;pm&udblac;v&eacute;szet</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_i;&c_z;&c_ja;&c_shch;&c_n;&c_o;&c_je; &c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="art.bel0ajxo">
  <kap>bel<tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Bel<tld/>a verko.
    </dif>
    <refgrp tip="sin">
      <ref cel="art.0ajxo.belarta">arta&jcirc;o <sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="ca">obra d'<ind>art</ind></trd>
  <trd lng="de">Kunstwerk</trd>
  <trd lng="fr">&oelig;uvre d'art</trd>
  <trd lng="hu">m&udblac;t&aacute;rgy</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je; &c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_a;</trd>
</drv>

<drv mrk="art.danc0o">
  <kap>danc<tld/>o</kap>
  <snc mrk="art.danc0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Korpa <ref tip="super" cel="art.bel0o">bel<tld/>o</ref>
      pri ritma, ordinare la&ubreve;muzika sinmovo.
    </dif>
  </snc>
  <trd lng="ca">dansa</trd>
  <trd lng="de">Tanzkunst</trd>
  <trd lng="fr">danse<klr> (art)</klr></trd>
  <trd lng="hu">t&aacute;ncm&udblac;v&eacute;szet</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o; &c_t;&c_a;&c_n;&c_c;&c_a;</trd>,
    <trd>&c_h;&c_o;&c_r;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="art.desegn0o">
  <kap>desegn<tld/>o</kap>
  <snc mrk="art.desegn0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Grafika <ref tip="super" cel="art.bel0o">bel<tld/>o</ref> produktanta
      bildojn per strekoj.
    </dif>
  </snc>
  <trd lng="ca">dibuix</trd>
  <trdgrp lng="fr">
    <trd>dessin<klr> (art)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rajzm&udblac;v&eacute;szet</trd>,
    <trd>grafika</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_a;&c_f;&c_i;&c_k;&c_a;</trd>
</drv>

<drv mrk="art.juvel0o">
  <kap>juvel<tld/>o</kap>
  <snc mrk="art.juvel0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="art.bel0o">Bel<tld/>o</ref> pri planado
      kaj farado de juveloj el metalo, gemoj kaj aliaj materialoj.
    </dif>
  </snc>
  <trd lng="ca">joieria</trd>
  <trdgrp lng="fr">
    <trd>joaillerie</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;kszer&eacute;szet</trd>
  </trdgrp>
  <trd lng="ru">&c_ju;&c_v;&c_je;&c_l;&c_i;&c_r;&c_n;&c_o;&c_je; &c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="art.kin0o">
  <kap>kin<tld/>o</kap>
  <snc mrk="art.kin0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">KIN</uzo>
    <dif>
      Grafika <ref tip="super" cel="art.bel0o">bel<tld/>o</ref> produktanta
      projekcieblajn movi&gcirc;ajn bildojn.
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>cinema</trd>,
    <trd>set&egrave; <ind>art</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cin&eacute;ma</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>filmm&udblac;v&eacute;szet</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_i;&c_n;&c_je;&c_m;&c_a;&c_t;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
</drv>

<drv mrk="art.memor0o">
  <kap>memor<tld/>o</kap>
  <snc mrk="art.memor0o.TEK">
    <dif>
      <ref tip="super" cel="art.0o.tehxniko">Arto</ref> helpi kaj
      plibonigi la memorkapablon.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_n;&c_e;&c_m;&c_o;&c_n;&c_ib;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_n;&c_e;&c_m;&c_a;&c_t;&c_e;&c_h;&c_n;&c_ib;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Mnemonik</trd>,
    <trd>Mnemotechnik</trd>
  </trdgrp>
  <trd lng="fr">mn&eacute;motechnie</trd>
  <trdgrp lng="nl">
    <trd>geheugenkunst</trd>,
    <trd>mnemotechniek</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_n;&c_je;&c_m;&c_o;&c_n;&c_i;&c_k;&c_a;</trd>,
    <trd>&c_m;&c_n;&c_je;&c_m;&c_o;&c_t;&c_je;&c_h;&c_n;&c_i;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">mnemotechnika</trd>
</drv>

<drv mrk="art.ornam0o">
  <kap>ornam<tld/>o</kap>
  <snc mrk="art.ornam0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Forma <ref tip="super" cel="art.bel0o">bel<tld/>o</ref>
      produktanta ornamitajn a&jcirc;ojn por hejmo a&ubreve; &ccirc;iutaga uzo.
    </dif>
  </snc>
  <trd lng="ca">arts <ind>decoratives</ind></trd>
  <trdgrp lng="fr">
    <trd>arts d&eacute;coratifs</trd>
  </trdgrp>
  <trd lng="hu">d&iacute;sz&iacute;t&odblac;m&udblac;v&eacute;szet</trd>
  <trd lng="nl">decoratiekunst</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_k;&c_l;&c_a;&c_d;&c_n;&c_o;&c_je; &c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="art.pentr0o">
  <kap>pentr<tld/>o</kap>
  <snc mrk="art.pentr0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Grafika <ref tip="super" cel="art.bel0o">bel<tld/>o</ref> produktanta
      bildojn per koloriloj.
    </dif>
  </snc>
  <trd lng="ca">pintura</trd>
  <trdgrp lng="fr">
    <trd>peinture<klr> (art)</klr></trd>
  </trdgrp>
  <trd lng="hu">fest&eacute;szet</trd>
  <trd lng="nl">schilderkunst</trd>
  <trd lng="ru">&c_zh;&c_i;&c_v;&c_o;&c_p;&c_i;&c_s;&c_mol;</trd>
</drv>

<drv mrk="art.skulpt0o">
  <kap>skulpt<tld/>o</kap>
  <snc mrk="art.skulpt0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Plastika <ref tip="super" cel="art.bel0o">bel<tld/>o</ref>
      produktanta bareliefajn, alt-reliefajn a&ubreve; plen-reliefajn verkojn.
    </dif>
  </snc>
  <trd lng="ca">escultura</trd>
  <trdgrp lng="fr">
    <trd>sculpture<klr> (art)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>szobr&aacute;szat</trd>,
    <trd>szobr&aacute;szm&udblac;v&eacute;szet</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_a;&c_ja;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_s;&c_k;&c_u;&c_l;&c_mol;&c_p;&c_t;&c_u;&c_r;&c_a;<klr tip="amb"> (&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
</drv>
</art>
