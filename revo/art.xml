<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>art</rad>/o
</kap>
<drv mrk="art.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="art.0o.tehxniko">
    <dif>
      Praktika apliko de scioj kaj reguloj akiritaj per iutema studo:
      <ekz>
        medicino, diplomatio, pedagogio, konstruado estas <tld/>oj;
      </ekz>
      <ekz>
        la&ubreve; &ccirc;iuj le&gcirc;oj de la <tld/>o<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        vivon travivi estas <tld/>' malfacila<fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        al la fi&scirc;o ne instruu na&gcirc;<tld/>on<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="art.0o.belarto">
    <ref tip="dif" cel="art.bel0o">belarto:</ref>
    <ekz>
      la <tld/>o estas imito, sed ne simpla kopio de la naturo.
    </ekz>
  </snc>
  <snc mrk="art.0o.artefaro">
    <dif>
      Kreado de la homa faro kontraste al la kreado de la naturo:
      <ekz>
        tiu haveno estas kreita de naturo kaj aran&gcirc;ita de <tld/>o
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Kunst</trd>
  <trd lng="en">art</trd>
  <trdgrp lng="fr">
    <trd>art</trd>,
    <trd>technique</trd>,
    <trd>savoir-faire</trd>
  </trdgrp>
  <trd
  lng="ru">&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>
</drv>

<drv mrk="art.0a">
  <kap><tld/>a</kap>
  <snc mrk="art.0a.tehxnika">
    <dif>
      Koncernanta <tld/>on 1.
    </dif>
    <trd lng="de">Kunst-</trd>
    <trd
    lng="ru">&c_i;&c_s;&c_k;&c_u;&c_s;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="art.0a.belarta">
    <ref tip="dif" cel="art.bel0a">belarta</ref>
    <trd lng="de">kunstvoll</trd>
    <trd lng="fr">artistique</trd>
    <trd
    lng="ru">&c_h;&c_u;&c_d;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="art.0a.artefarita">
    <ref tip="dif" cel="art.0efarita">artefarita:</ref>
    <ekz>
      &ccirc;e la <tld/>a lumo;
    </ekz>
    <ekz>
      <tld/>e da&ubreve;rigata juneco;
    </ekz>
    <ekz>
      <tld/>a lingvo.
    </ekz>
    <trd lng="de">k&uuml;nstlich</trd>
    <trd lng="fr">artificiel</trd>
    <trd
    lng="ru">&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>

<drv mrk="art.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="art.0ajxo.tehxnika">
    <dif>
      Io farita la&ubreve; la reguloj de iu <tld/>o.
    </dif>
    <trd lng="de">Kunstwerk</trd>
  </snc>
  <snc mrk="art.0ajxo.belarta">
    <dif>
      Bel<tld/>a verko.
    </dif>
    <trd lng="de">Kunstwerk</trd>
    <trd lng="fr">&oelig;uvre d'art</trd>
    <trd
    lng="ru">&c_p;&c_r;&c_o;&c_i;&c_z;&c_v;&c_je;&c_d;&c_je;&c_n;&c_i;&c_je;
    &c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_a;</trd>
  </snc>
  <snc mrk="art.0ajxo.artefaritajxo">
    <dif>
      <tld/>efarita&jcirc;o:
      <ekz>
        <tld/>a&jcirc;a hararo.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="artifi.0o">artifiko</ref>.
    </refgrp>
  </snc>
</drv>

<drv mrk="art.0ismo">
  <kap><tld/>ismo</kap>
  <snc>
    <dif>
      Delikata sento pri la beleco.
    </dif>
  </snc>
  <trd lng="de">Kunstsinn</trd>
  <trd lng="fr">sens <ind>artistique</ind></trd>
</drv>

<drv mrk="art.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Tiu, kiu praktikas bel<tld/>on.
    </dif>
  </snc>
  <trd lng="de">K&uuml;nstler</trd>
  <trd lng="fr">artiste</trd>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_ja;&c_t;&c_je;&c_l;&c_mol;
    &c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_h;&c_u;&c_d;&c_o;&c_zh;&c_n;&c_i;&c_k;
    <klr>(&c_v; &c_sh;&c_i;&c_r;&c_o;&c_k;&c_o;&c_m;
    &c_s;&c_m;&c_y;&c_s;&c_l;&c_je;
    &c_s;&c_l;&c_o;&c_v;&c_a;)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="art.0efarita">
  <kap><tld/>efarita</kap>
  <snc>
    <dif>
      Farita per homa <tld/>o, sed ne kreita de la naturo.
    </dif>
  </snc>
  <trd lng="de">k&uuml;nstlich</trd>
  <trd lng="fr">artificiel</trd>
  <trd
  lng="ru">&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="art.bel0a">
  <kap>bel<tld/>a</kap>
  <snc>
    <dif>
      Konforma al la reguloj de la beleco, <tld/>oplena, <tld/>ista,
      <tld/>isma.
    </dif>
  </snc>
    <trd lng="de">kunstvoll</trd>
    <trd lng="fr">artistique</trd>
    <trd lng="ru">&c_h;&c_u;&c_d;&c_o;&c_zh;&c_je;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="art.bel0o">
  <kap>bel<tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      &Ccirc;iu el la <ref tip="super" cel="art.0o"><tld/>o</ref>j, kiuj celas
      belecon: arkitekturo, beletro,
      muziko, skulptado, pentr<tld/>o...
    </dif>
  </snc>
  <trd lng="fr">beaux-arts</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_i;&c_z;&c_ja;&c_shch;&c_n;&c_o;&c_je;
    &c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>

<drv mrk="danc.0arto">
  <kap>danc<tld/></kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Korpa <ref tip="super" cel="art.bel0o">bel<tld/>o</ref>
      pri ritma, &gcirc;enerale la&ubreve;muzika sinmovo.
    </dif>
  </snc>
  <trd lng="de">Tanzkunst</trd>
  <trd lng="fr">danse<klr> (art)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_s;&c_k;&c_u;&c_s;&c_s;&c_t;&c_v;&c_o; &c_t;&c_a;&c_n;&c_c;&c_a;</trd>,
    <trd>&c_h;&c_o;&c_r;&c_je;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="art.desegn0o">
  <kap>desegn<tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Grafika <ref tip="super" cel="art.bel0o">bel<tld/>o</ref> produktanta
      bildojn per strekoj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>dessin<klr> (art)</klr></trd>
  </trdgrp>
</drv>

<drv mrk="art.kin0o">
  <kap>kin<tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Grafika <ref tip="super" cel="art.bel0o">bel<tld/>o</ref> produktanta
      projekcieblajn movi&gcirc;ajn bildojn.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>cin&eacute;ma</trd>
  </trdgrp>
</drv>

<drv mrk="art.pentr0o">
  <kap>pentr<tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Grafika <ref tip="super" cel="art.bel0o">bel<tld/>o</ref> produktanta
      bildojn per koloriloj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>peinture<klr> (art)</klr></trd>
  </trdgrp>
  <trd lng="nl">schilderkunst</trd>
  <trd lng="ru">&c_zh;&c_i;&c_v;&c_o;&c_p;&c_i;&c_s;&c_mol;</trd>
</drv>

<drv mrk="art.pop0o">
  <kap>pop<tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Belarta stilo anglo-devena, prospera ekde la dua dunono de la 20a jarcento
      precipe en Granda-Britujo kaj Usono, 
      prezentanta a&jcirc;ojn tipajn de la konsumisma socio
      a&ubreve; produktitajn per amasaj rimedoj (serigrafio...).
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>pop-art</trd>
  </trdgrp>
</drv>

<drv mrk="art.skulpt0o">
  <kap>skulpt<tld/>o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Plastika <ref tip="super" cel="art.bel0o">bel<tld/>o</ref> produktanta
      bareliefajn, alt-reliefajn a&ubreve; plen-reliefajn verkojn.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>sculpture<klr> (art)</klr></trd>
  </trdgrp>
</drv>
</art>
