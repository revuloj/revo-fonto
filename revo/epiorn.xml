<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>epiornit</rad>/o
</kap>
<drv mrk="epiorn.0o">
  <kap><tld/>o</kap>
  <snc mrk="epiorn.0o.ZOO">
    <uzo tip="fak">PAL</uzo>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Granda fosilia birdo, simila al struto kaj vivinta en Madagaskaro.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_jo;&c_r;&c_n;&c_ib;&c_s;</trd>
  <trd lng="de">Aepyornis</trd>
  <trdgrp lng="fr">
    <trd>&aelig;pyornis</trd>,
    <trd>&eacute;pyornis</trd>
  </trdgrp>
  <trd lng="hu">Aepyornis</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_o;&c_r;&c_n;&c_i;&c_s;</trd>
</drv>
</art>
