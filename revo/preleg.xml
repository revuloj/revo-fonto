  <trdgrp lng="ru">
    <trd>&c_d;&c_o;&c_k;&c_l;&c_a;&c_d;&c_ch;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_t;&c_u;&c_p;&c_a;&c_ju;&c_shch;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_je;&c_k;&c_t;&c_o;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">predn&aacute;&scaron;ate&lcaron;</trd>
