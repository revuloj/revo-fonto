<drv mrk="preleg.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="preleg.0ejo.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Loko aran&gcirc;ita por <tld/>ado:
      <ekz>
        elokventaj personoj a&ubreve;digis siajn vo&ccirc;ojn en parlamentoj, en popolaj kunvenoj, en
        universitatoj, en <tld/>ejoj
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        protestis studentoj, okupante <tld/>ejon kaj klopodante bari la enirejon de la
        konferencejo
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2010/010037.php">Reformo fu&scirc;ita</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ne estus malfacile &ndash; per kelkaj ekzempleroj
        el la libro meti en ridegon tutan <tld/>ejon
        <fnt>
          <aut>V. O&scaron;lak</aut>,
          <vrk>Foje e&ccirc; pensi estas &gcirc;uo</vrk>,
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="auxdit.0o.ARKI">a&ubreve;ditorio</ref>,
      <ref cel="auxl.0o.ARKI">a&ubreve;lo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_d;&c_y;&c_t;&c_o;&c_r;&c_y;&c_ja; <klr>(&c_d;&c_l;&c_ja; &c_l;&c_je;&c_k;&c_c;&c_y;&c_j;)</klr></trd>
  <trd lng="cs">p&rcaron;edn&aacute;&scaron;kov&yacute; s&aacute;l</trd>
  <trd lng="de">H&ouml;rsaal</trd>
  <trd lng="fr">salle de conf&eacute;rence</trd>
  <trdgrp lng="pl">
    <trd>audytorium</trd>,
    <trd>widownia</trd>
  </trdgrp>
  <trd lng="sk">predn&aacute;&scaron;kov&aacute; s&aacute;la</trd>
  <trdgrp lng="zh">
    <trd>&#x4F1A;&#x8BAE;&#x5BA4; <pr>hu&igrave;y&igrave;sh&igrave;</pr></trd>,
    <trd>&#x6703;&#x8B70;&#x5BA4; <pr>hu&igrave;y&igrave;sh&igrave;</pr></trd>,
    <trd>&#x4F1A;&#x8BAE;&#x5385; <pr>hu&igrave;y&igrave;t&imacron;ng</pr></trd>,
    <trd>&#x6703;&#x8B70;&#x5EF3; <pr>hu&igrave;y&igrave;t&imacron;ng</pr></trd>
  </trdgrp>
</drv>
