  </trdgrp>
  <trd lng="fr">conf&eacute;rencier</trd>
  <trd lng="hu">el&odblac;ad&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x8B1B;&#x5E2B;  <pr>&#x3053;&#x3046;&#x3057;</pr></trd>,
    <trd>&#x8B1B;&#x6F14;&#x8005;  <pr>&#x3053;&#x3046;&#x3048;&#x3093;&#x3057;&#x3083;</pr></trd>
