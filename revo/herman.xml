<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: herman.xml,v 1.12 2023/11/15 16:45:19 revo Exp $">
<kap>
  <rad>Herman</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="herman.0o">
  <kap><tld/>o</kap>
  <snc mrk="herman.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        la granda pupo de via fratino <klr>...</klr> aspektas kiel viro
        kaj estas nomata <tld/>o
        <fnt><bib>Fab2</bib>, <lok>dio de dormo, Vendredo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_je;&c_r;&c_m;&c_a;&c_n;</trd>
  <trd lng="de">Hermann</trd>
  <trd lng="en">Herman</trd>
  <trd lng="fr">Hermann</trd>
  <trd lng="hu">Herman</trd> 
   <trdgrp lng="it">
   <trd>Ermano</trd>, 
   <trd>Ermanno</trd> 
  </trdgrp>
  <trd lng="nl">Herman</trd>
  <trd lng="pl">Herman</trd>
  <trdgrp lng="pt">
   <trd>Hermano</trd>, 
   <trd>Germano</trd> 
  </trdgrp>
  <trd lng="uk">&c_G;&c_je;&c_r;&#x43C;&c_a;&#x43D;</trd>
  <trd lng="zh">&#x8D6B;&#x5C14;&#x66FC; <pr>h&#xE8;&#x11B;rm&#xE0;n</pr></trd>
</drv>
</art>

