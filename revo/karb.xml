<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karb.xml,v 1.41 2023/12/03 08:00:23 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>karb</rad>/o
</kap>
<drv mrk="karb.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Malmola nigra <ref tip="super" cel="substa.0o.FIZ">substanco</ref>,
      konsistanta el preska&ubreve; pura <ref tip="prt" cel="karbon.0o">karbono</ref>,
      devenanta nature a&ubreve; arte de ligno, kaj uzata por hejti:
      <ekz>
        &scirc;ton<tld/>o a&ubreve; ter<tld/>o;
      </ekz>
      <ekz>
        <klr>[li]</klr> direktis sur &scirc;in siajn okulojn nigrajn kiel <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
<!-- tio &scirc;ajnas neinternacia, konfuza esprimo... &ccirc;u efektive trovi&gcirc;us fonto de tia uzo? -->
        <ind>blanka <tld/>o</ind>
        <klr>(fluakvo uzata por produkti elektron)</klr>;
        <trd lng="ca">energia hidroel&egrave;ctrica</trd>
        <trd lng="de">Wasserkraft</trd>
        <trd lng="fr">houille blanche</trd>
        <trd lng="nl">witte <ind>steenkool</ind></trd>
        <trd lng="pl">bia&lstroke;y w&eogonek;giel</trd>
      </ekz>
      <ekz>
        el la rubo de la vila&gcirc;ana domo elstaris nur
        kelke da nigraj <ind><tld/>igita</ind>j traboj
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
        <trd lng="ca">carbonitzat</trd>
        <trd lng="fr">carbonis&eacute;</trd>
        <trd lng="nl">verkoold</trd>
        <trd lng="ru">&c_o;&c_b;&c_u;&c_g;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
      </ekz>
      <ekz>
        la macoj <ind><tld/>i&gcirc;i</ind>s en la forno
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;APITRO II</lok>
        </fnt>;
        <trd lng="ca">carbonitzar-se</trd>
        <trd lng="de">verkohlen</trd>
      </ekz>
      <ekz>
        <ind>konservas e&ccirc; <tld/>o la strukturon de l' arbo</ind>
        <klr>(oni konservas &ccirc;iam signojn de sia deveno)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">la <ind>caque</ind> sent toujours le hareng</trd>
        <trd lng="nl">men kan zijn <ind>afkomst</ind> niet
        verloochenen</trd>
      </ekz>
    </dif>
    <refgrp tip="sub">
      <ref cel="antrac.0o">antracito</ref>,
      <ref cel="karb.ligno0o">ligno<tld/>o</ref>,
      <ref cel="karb.ter0o">ter<tld/>o</ref>,
      <ref cel="lignit.0o">lignito</ref>,
      <ref cel="torf.0o">torfo</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Peco el <tld/>o:
      <ekz>
        levi&gcirc;is fumo el Lia nazo, kaj ekstermanta fajro el Lia bu&scirc;o, <tld/>oj ekflamis
        de &gcirc;i
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 22:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        ardantaj kaj brulantaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 1:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> eksidis kaj per <tld/>o desegnis la konturojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam la skorioj estis jam elprenitaj kaj estingitaj, ni sur&jcirc;etis fre&scirc;ajn nukso<tld/>ojn
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 30</lok>
        </fnt>;
      </ekz>
      <ekz>
        mar&scirc;i sur ardantaj <tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 6:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la tuta disputo <ind>li sidis kvaza&ubreve; sur flamantaj <tld/>oj</ind>
        <klr>(senpaciencege, <ref tip="sin" cel="pingl.0o">pinglo</ref>)</klr>
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>X</lok>
        </fnt>.
        <trd lng="de">auf gl&uuml;henden <ind>Kohlen</ind> sitzen</trd>
        <trd lng="pl">siedzie&cacute; jak na roz&zdot;arzonych
          w&eogonek;glach</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_g;&c_a;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_v;&c_malmol;&c_g;&c_l;&c_i;&c_shch;&c_a;</trd>
  <trd lng="br">glaou</trd>
  <trd lng="ca">carb&oacute;</trd>
  <trdgrp lng="cs">
    <trd>uhel</trd>,
    <trd>uhl&iacute;</trd>
  </trdgrp>
  <trd lng="de">Kohle</trd>
  <trdgrp lng="en">
    <trd>coal</trd>,
    <trd>charcoal</trd>
  </trdgrp>
  <trd lng="es">carb&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>charbon<klr tip="ind"> (mati&egrave;re, morceau de
charbon)</klr></trd>,
    <trd>houille</trd>
  </trdgrp>
  <trd lng="hu">sz&eacute;n</trd>
  <trd lng="id">arang</trd>
  <trdgrp lng="ja">
    <trd>&#x70AD;  <pr>&#x3059;&#x307F;</pr></trd>,
    <trd>&#x77F3;&#x70AD;  <pr>&#x305B;&#x304D;&#x305F;&#x3093;</pr></trd>,
    <trd>&#x6728;&#x70AD;  <pr>&#x3082;&#x304F;&#x305F;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">kool<klr tip="ind"> (grondstof)</klr></trd>
  <trd lng="pl">w&eogonek;giel</trd>
  <trd lng="ro">c&abreve;rbune</trd>
  <trd lng="ru">&c_u;&c_g;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">uhlie</trd>
  <trd lng="sv">kol</trd>
  <trd lng="uk">&c_v;&c_u;&c_g;&c_ib;&c_l;&c_l;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x7164; <pr>m&#xE9;i</pr></trd>,
    <trd>&#x70AD; <pr>t&#xE0;n</pr></trd>,
    <trd>&#x7164;&#x70AD; <pr>m&#xE9;it&#xE0;n</pr></trd>,
    <trd>&#x6728;&#x70AD; <pr>m&#xF9;t&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="karb.0eto">
  <kap><tld/>eto</kap>
  <snc>
    <dif>
      Malgranda peco de <tld/>o a&ubreve; <ref cel="bragx.0o">bra&gcirc;o</ref>
      <ekz>
        De forno prenis &scirc;i <tld/>eton, <klr>[&#x2026;]</klr> Disblovis apud forn' fajreton
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
      <ekz>
        jen mia bona <tld/>eto ektu&scirc;is la glacian kupolon kaj precize, kiel cirkelo,
        desegnis cirklon
        <fnt>
          <aut>&c_A;&c_l;&c_je;&c_k;&c_s;&c_a;&c_n;&c_d;&c_r; &c_Sh;&c_a;&c_r;&c_o;&c_v;</aut>
          <vrk><url ref="http://miresperanto.com/por_infanoj/simplulo/18.htm">Pizerhomo Kaj
          Simplulo</url></vrk>
          <lok>La magia &scirc;losileto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_g;&c_je;&c_l;&c_mol;&c_ch;&c_y;&c_k;</trd>
  <trdgrp lng="br">
    <trd>glaou bev</trd>,
    <trd>glaou tan</trd>
  </trdgrp>
  <trd lng="ca">brasa</trd>
  <trdgrp lng="cs">
    <trd>mal&yacute; uhel</trd>,
    <trd>uhl&iacute;k</trd>
  </trdgrp>
  <trd lng="fr">braise</trd>
  <trdgrp lng="hu">
    <trd>par&aacute;zs</trd>,
    <trd>fasz&eacute;n</trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304A;&#x304D;</trd>,
    <trd>&#x6D88;&#x3057;&#x70AD;  <pr>&#x3051;&#x3057;&#x305A;&#x307F;</pr></trd>
  </trdgrp>
  <trd lng="nl">stuk <ind>houtskool</ind></trd>
  <trd lng="pl">w&eogonek;gielek</trd>
  <trd lng="ru">&c_u;&c_g;&c_o;&c_l;&c_jo;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>drobn&eacute; uhlie</trd>,
    <trd>uhl&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_u;&c_g;&c_l;&c_i;&c_k;</trd>,
    <trd>&c_v;&c_u;&c_g;&c_l;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_zh;&c_a;&c_r;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="karb.0ujo">
  <kap>
    <tld/>ujo
  </kap>
  <snc mrk="karb.0ujo.bargxujo">
    <dif>
      Pelvo en kiu oni bruligas <tld/>on; <ref tip="sin" cel="bragx.0ujo">bra&gcirc;ujo</ref>:
      <ekz>
        <klr>[por la altaro]</klr> li faris <klr>[&#x2026;]</klr> <tld/>ujojn
        <klr>(vd )</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 38:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        la solaj varmaj punktoj trovi&gcirc;is apud du ornamitaj <tld/>ujoj
        <fnt>
          <bib>StU</bib>
          <lok>Al Romo</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_zh;&c_a;&c_r;&c_o;&c_w;&c_n;&c_ja;</trd>,
      <trd>&c_m;&c_a;&c_n;&c_g;&c_a;&c_l;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Kohlebecken</trd>,
      <trd>Feuerschale</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      &Ccirc;ia ujo por transporti a&ubreve; konservi <tld/>on, &ccirc;u portebla hejma,
      &ccirc;u granda de vaporlokomotivo a&ubreve; &scirc;ipo:
      <ekz>
        kiam la Fatalo povis denove marveturi, li plenigis da <tld/>o la <tld/>ujojn
        <klr>(kp <ref cel="hold.0o">holdo</ref>, <ref cel="tendr.0o">tendro</ref>)</klr>,
        por ke li povu reveni Francujon kiel eble plej rapide
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekoka</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_u;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_b;&c_u;&c_n;&c_k;&c_je;&c_r;</trd>,
      <trd>&c_v;&c_u;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_ja; &c_ja;&c_m;&c_a;</trd>,
      <trd>&c_v;&c_u;&c_g;&c_a;&c_l;&c_mol;&c_n;&c_y; &c_s;&c_k;&c_l;&c_je;&c_p;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd><klr>(Kohlen-)</klr>Kiepe</trd>,
      <trd>Kohlebunker</trd>,
      <trd>Tender</trd>
    </trdgrp>
    <trd lng="en">coal bin</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x70AD;&#x7BB1;  <pr>&#x3059;&#x307F;&#x306F;&#x3053;</pr></trd>,
    <trd>&#x77F3;&#x70AD;&#x30D0;&#x30B1;&#x30C4;  <pr>&#x305B;&#x304D;&#x305F;&#x3093;&#x3070;&#x3051;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="zh">&#x7164;&#x7BB1; <pr>m&#xE9;ixi&#x101;ng</pr></trd>
</drv>

<drv mrk="karb.0desegnajxo">
  <kap><tld/>desegna&jcirc;o</kap>
  <uzo tip="fak">BELA</uzo>
  <snc>
    <dif>
      Desegna&jcirc;o farita per <tld/>krajono <sncref ref="karb.0krajono.krajono"/>.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_a;&c_l;&c_ju;&c_n;&c_a;&c_k; <ind>&c_v;&c_u;&c_g;&c_a;&c_l;&c_je;&c_m;</ind></trd>
  <trdgrp lng="ca">
    <trd>carb&oacute;</trd>,
    <trd>dibuix al carb&oacute;</trd>
  </trdgrp>
  <trd lng="de">Kohlezeichnung</trd>
  <trd lng="en">charcoal drawing</trd>
  <trd lng="fr">fusain<klr tip="ind"> (dessin)</klr></trd>
  <trd lng="hu">sz&eacute;nrajz</trd>
  <trd lng="nl">houtskooltekening</trd>
  <trd lng="pl">rysunek <ind>w&eogonek;glem</ind></trd>
  <trd lng="ru">&c_r;&c_i;&c_s;&c_u;&c_n;&c_o;&c_k;
<ind>&c_u;&c_g;&c_l;&c_je;&c_m;</ind></trd>
  <trdgrp lng="zh">
    <trd>&#x6728;&#x70AD;&#x753B; <pr>m&#xF9;t&#xE0;nhu&#xE0;</pr></trd>,
    <trd>&#x70AD;&#x7B14;&#x753B; <pr>t&#xE0;nb&#x1D0;hu&#xE0;</pr></trd>,
    <trd>&#x70AD;&#x7C89;&#x753B; <pr>t&#xE0;nf&#x11B;nhu&#xE0;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="karb.ligno0o">
  <kap>ligno<tld/>o</kap>
  <snc>
    <dif>
      <ref tip="super" cel="brul.0igajxo">Bruliga&jcirc;o</ref>,
      produktata per malrapida kaj nekompleta brulo de ligno en senaera
      atmosfero:
      <ekz>
        en portebla fajrujo brulis amaseto da ligno<tld/>o, plenigante
        la &ccirc;ambron per komfortiga ardo
        <fnt><bib>I&scirc;tar</bib></fnt>;
      </ekz>
      <ekz>
        la tuta kavo estis plenigita de ardantaj ligno<tld/>oj
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Zalmuna</nom> malmoligis la <klr>[vaksajn]</klr> cilindrojn super la
        ardantaj ligno<tld/>oj <klr>(t.e. <tld/>etoj el ligno<tld/>o)</klr>
        <fnt><bib>I&scirc;tar</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_w;&c_n;&c_ib;&c_n;&c_n;&c_y; <ind>&c_v;&c_u;&c_g;&c_a;&c_l;&c_mol;</ind></trd>
  <trd lng="bg">&c_d;&c_malmol;&c_r;&c_v;&c_je;&c_n;&c_i;
  &c_v;&c_malmol;&c_g;&c_l;&c_i;&c_shch;&c_a;</trd>
  <trd lng="br">glaou-koad</trd>
  <trd lng="ca"><ind>carb&oacute;</ind> vegetal</trd>
  <trd lng="cs">d&rcaron;ev&ecaron;n&eacute; uhl&iacute;</trd>
  <trd lng="de">Holzkohle</trd>
  <trd lng="en">charcoal</trd>
  <trd lng="fr">charbon de bois</trd>
  <trd lng="hu">fasz&eacute;n</trd>
  <trd lng="id"><ind>arang</ind> kayu</trd>
  <trd lng="ja">&#x6728;&#x70AD;  <pr>&#x3082;&#x304F;&#x305F;&#x3093;</pr></trd>
  <trd lng="nl">houtskool</trd>
  <trd lng="pl">w&eogonek;giel drzewny</trd>
  <trd lng="ro">mangal</trd>
  <trd lng="ru">&c_d;&c_r;&c_je;&c_v;&c_je;&c_s;&c_n;&c_y;&c_j;
&c_u;&c_g;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">dreven&eacute; uhlie</trd>
  <trd lng="sv">tr&auml;kol</trd>
  <trd lng="uk">&c_d;&c_je;&c_r;&c_je;&c_v;&c_n;&c_je; &c_v;&c_u;&c_g;&c_ib;&c_l;&c_l;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x70AD; <pr>t&#xE0;n</pr></trd>,
    <trd>&#x6728;&#x70AD; <pr>m&#xF9;t&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="karb.ter0o">
  <kap>ter<tld/>o,
    <var><kap>min<tld/>o</kap></var>,
    <var><kap>&scirc;ton<tld/>o</kap></var></kap>
  <uzo tip="fak">MIN</uzo>
  <snc>
    <dif>
      Minerala <ref tip="super" cel="karb.0o"><tld/>o</ref>,
      eltirata el la tera profundo por servi
      kiel <ref tip="super" cel="brul.0igajxo">bruliga&jcirc;o</ref>:
      <ekz>
        <klr>[en]</klr> la kameno <klr>[&#x2026;]</klr> bruletis &scirc;ton<tld/>o
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi voras la &scirc;ton<tld/>on el Supra Silezio
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 42</lok>
        </fnt>;
      </ekz>
      <ekz>
        iu &ccirc;uk&ccirc;o vagadis <klr>[&#x2026;]</klr> preter amaso da &scirc;ton<tld/>o destinita por la
        hejtado de la bazo
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>6. La neatendita bombeksplodo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kosovo <klr>[&#x2026;]</klr> posedas <klr>[&#x2026;]</klr> subterajn ri&ccirc;a&jcirc;ojn, el kiuj nur de
        &scirc;ton<tld/>o oni havas nekalkuleblajn rezervojn
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/007925.html">Voja&gcirc;impresoj, somero 2002: Kosovo
          anta&ubreve;eniras</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="min.0ejo">minejo</ref>,
      <ref cel="koaks.0o">koakso</ref>,
      <ref cel="antrac.0o">antracito</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;
&c_v;&c_u;&c_g;&c_a;&c_l;&c_mol;</trd>
  <trd lng="br">glaou-douar</trd>
  <trdgrp lng="ca">
    <trd>carb&oacute; mineral</trd>,
    <trd>hulla</trd>,
    <trd>antracita</trd>
  </trdgrp>
  <trd lng="de">Steinkohle</trd>
  <trd lng="en">coal</trd>
  <trd lng="fr">houille</trd>
  <trd lng="hu">k&odblac;sz&eacute;n</trd>
  <trd lng="id"><ind>batu</ind> bara</trd>
  <trd lng="ja">&#x77F3;&#x70AD;  <pr>&#x305B;&#x304D;&#x305F;&#x3093;</pr></trd>
  <trd lng="nl">steenkool</trd>
  <trd lng="pl">w&eogonek;giel kamienny</trd>
  <trd lng="ro">huil&abreve;</trd>
  <trd lng="ru">&c_k;&c_a;&c_m;&c_je;&c_n;&c_n;&c_y;&c_j;
&c_u;&c_g;&c_o;&c_l;&c_mol;</trd>
  <trd lng="sk">lignit</trd>
  <trd lng="cs">lignit</trd>
  <trd lng="uk">&c_k;&c_a;&c_m;&#x2019;&c_ja;&c_n;&c_je; &c_v;&c_u;&c_g;&c_ib;&c_l;&c_l;&c_ja;</trd>
  <trd lng="zh">&#x77F3;&#x70AD; <pr>sh&#xED;t&#xE0;n</pr></trd>
</drv>
</art>
