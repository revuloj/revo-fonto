<art mrk="$Id: ancx.xml,v 1.25 2024/02/05 15:23:12 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>an&ccirc;</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ancx.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="ancx.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Maldika elasta <ref tip="lst" lst="voko:botanikaj_genroj" cel="lang.0eto">langeto</ref>,
      kies vibrado kreas la tonojn &ccirc;e
      kelkaj <ref tip="malprt" cel="instru2.blov0o.MUZ">blovinstrumento</ref>j,
      kiel ekzemple hobojo, klarneto kaj saksofono:
      <ekz>
        la bu&scirc;peco, kiu konsistas el beko, (farita el bukso, ebonito a&ubreve; vitro),
        sur kie trovi&gcirc;as <tld/>o (el kano, foje el plasto) fiksita de &ccirc;irka&ubreve;ligo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20130">Klarneto</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_z;&c_y;&c_ch;&c_o;&c_k; <klr tip="amb">(&c_d;&c_u;&c_h;&c_o;&c_v;&c_a;&c_g;&c_a; &c_m;&c_u;&c_z;. &c_ib;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_e;&c_n;&c_t;&c_a;)</klr></trd>
  <trdgrp lng="de">
    <trd>Rohrblatt</trd>,
    <trd>Stimmblatt</trd>
  </trdgrp>
  <trd lng="en">reed</trd>
  <trd lng="es">leng&uuml;eta<klr tip="amb"> (en instrumentos musicales de
  viento)</klr></trd>
  <trd lng="fr">anche</trd>
  <trdgrp lng="he">
    <trd>&#x5E7;&#x5E0;&#x5D4;</trd>,
    <trd>&#x5E4;&#x5D5;&#x5DE;&#x5D9;&#x5EA;</trd>
  </trdgrp>
  <trd lng="hu">n&aacute;dnyelv</trd>
  <trd lng="it">ancia</trd>
  <trdgrp lng="ja">
    <trd>&#x30EA;&#x30FC;&#x30C9;</trd>,
    <trd>&#x7C27;  <pr>&#12375;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="nl">tongetje</trd>
  <trd lng="pl">stroik<klr tip="amb"> (w instrumentach muz.)</klr></trd>
  <trd lng="pt">palheta</trd>
  <trd lng="ru">&c_ja;&c_z;&c_y;&c_ch;&c_o;&c_k;<klr tip="amb"> (&c_u; &c_m;&c_u;&c_z;.
  &c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;&c_o;&c_v;)</klr></trd>
  <trd lng="sk">jaz&yacute;&ccaron;ek <klr>(hudobn&eacute;ho n&aacute;stroja)</klr></trd>
  <trd lng="uk">&#x44F;&c_z;&c_i;&c_ch;&#x43E;&#x43A;&#xA0;(&c_u; &c_d;&c_u;&c_h;&#x43E;&c_v;&c_i;&c_h; &c_ib;&#x43D;&c_s;&c_t;&c_r;&c_u;&#x43C;&c_je;&#x43D;&c_t;&c_a;&c_h;)</trd>
</drv>
