<kap>
  <rad>parvenu</rad>/o <fnt><aut>E. Millidge</aut>, <vrk>The
  Esperanto-English dictionary</vrk>, <lok>1912-1924</lok></fnt>
</kap>
<drv mrk="parven.0o">
  <kap><tld/>o</kap>
  <snc mrk="parven.0o.ADM">
    <dif>
      <ref tip="dif" cel="ricx.nov0ulo">Novri&ccirc;ulo</ref>,
      kiu venis en la unua generacio al ri&ccirc;eco
      a&ubreve; pli alta socia tavolo kaj havas videblajn
      kulturajn a&ubreve; edukajn mankojn:
      <ekz>
        lia &ccirc;efverko estas <nom>Stojan Mutika&scaron;a</nom>, romano pri la evoluo de kamparana knabo al
        bur&gcirc;a <tld/>o 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=125789">Svetozar &Ccirc;orovi&ccirc;</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la profesiaj ma&gcirc;angludistoj apartenas al jenaj grupoj: la unua grupo 
        estas <tld/>oj, la dua, altsalajraj sinekuruloj, <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=44714">Ma&gcirc;ango</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_r;&c_v;&c_e;&c_n;&c_ju;</trd>,
    <trd>&c_v;&c_y;&c_s;&c_k;&c_a;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Parven&uuml;</trd>,
    <trd>Empork&ouml;mmling</trd>
    <trd>Neureicher</trd>
  </trdgrp>
  <trd lng="en">parvenu</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_A;&a_z;&a_h; &a_b;&a_h; 
    &a_d;&a_w;&a_r;&a_A;&a_n;
    &a_r;&a_s;&f_y;&a_d;&a_h;</trd>
    <trd>&a_n;&a_w;&f_k;&f_y;&a_s;&a_h;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>nouveau riche</trd>,
    <trd>parvenu<klr> (subst.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>parven&uuml;</trd>,
    <trd>felkapaszkodott</trd>
  </trdgrp>
  <trd lng="ja">&#x6210;&#x4E0A;&#x304C;&#x308A;&#x8005;  <pr>&#x306A;&#x3042;&#x304C;&#x308A;&#x3057;&#x3083;</pr></trd>
  <trd lng="nl">parvenu</trd>
  <trd lng="pl">parweniusz</trd>
  <trd lng="pt">parvenu</trd>
  <trd lng="ro">parvenit</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_s;&c_k;&c_o;&c_ch;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_r;&c_v;&c_je;&c_n;&c_ju;</trd>
  </trdgrp>
  <trdgrp lng="uk">
