<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc> <rad>pa&gcirc;i</rad>/o</kap>
<drv mrk="pagxi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pagxi.0o.junulo">
    <dif>
      Nobela junulo akompananta kaj servanta re&gcirc;(in)on a&ubreve;
      sinjor(in)on.
    </dif>
    <trd lng="be">&c_p;&c_a;&c_zh;</trd>
    <trd lng="br">floc'h</trd>
    <trd lng="de">Page</trd>
    <trd lng="fr">page</trd>
    <trd lng="hu">apr&oacute;d</trd>
    <trd lng="nl">page</trd>
    <trd lng="pl">pa&zacute;</trd>
    <trd lng="pt">pajem</trd>
    <trd lng="ru">&c_p;&c_a;&c_zh;</trd>
  </snc>
  <snc mrk="pagxi.0o.karto">
    <uzo tip="fak">SPO</uzo> 
    <dif>
      <ref tip="super" cel="kart.lud0o">Ludkarto</ref> prezentanta
      <tld/>on.
    </dif>
    <ref tip="sin" cel="fant.0o">fanto</ref>   
    <trd lng="be">&c_v;&c_a;&c_l;&c_je;&c_t;</trd>
    <trd lng="br">lakez<klr tip="ind"> (kartenn c'hoari)</klr></trd>
    <trd lng="de">Bube</trd>
    <trd lng="fr">valet</trd>
    <trdgrp lng="hu">
      <trd>filk&oacute;</trd>,
      <trd>bubi</trd>,
      <trd>als&oacute;<klr tip="ind"> (k&aacute;rty&aacute;ban)</klr>
      </trd>
    </trdgrp>
    <trd lng="nl">boer<klr tip="ind"> (in kaartspel)</klr></trd>
    <trdgrp lng="pl">
      <trd>walet</trd>,
      <trd>ch&lstroke;opek<klr> (pot.)</klr></trd>,
      <trd>dupek<klr> (pot.)</klr></trd>
    </trdgrp>
    <trd lng="pt">valete</trd>
    <trd lng="ru">&c_v;&c_a;&c_l;&c_je;&c_t;</trd>
  </snc>
  <trd lng="en">page</trd>
</drv>
</art>
