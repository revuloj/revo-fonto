<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pagxi.xml,v 1.24 2024/04/29 07:22:17 revo Exp $">
<kap><ofc>*</ofc> <rad>pa&gcirc;i</rad>/o</kap>
<drv mrk="pagxi.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pagxi.0o.junulo">
    <dif>
      Nobela junulo akompananta kaj servanta re&gcirc;on, re&gcirc;inon, sinjoron a&ubreve;
      sinjorinon:
      <ekz>
        &leftquot;kiam li ekdormos, ili <klr>[&#8230;]</klr> ligos lin&rightquot; <klr>[&#8230;]</klr>, sed <tld/>o
        tre sindona al la juna sinjoro a&ubreve;dis &ccirc;ion kaj sciigis al li la komploton
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>oj anta&ubreve;metis al li konfita&jcirc;on kaj spiconuksojn, sed la
        maljuna re&gcirc;o pro mal&gcirc;ojo tute ne povis man&gcirc;i
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="eskvir.0o">eskviro</ref>
    <trd lng="be">&c_p;&c_a;&c_zh;</trd>
    <trd lng="br">floc'h</trd>
    <trd lng="de">Page</trd>
    <trd lng="en">page</trd>
    <trd lng="es">paje</trd>
    <trd lng="fa">&f_p;&f_y;&a_sh;&a_kh;&a_d;&a_m;&a_t;</trd>
    <trd lng="fr">page</trd>
    <trd lng="hu">apr&oacute;d</trd>
    <trd lng="nl">page</trd>
    <trd lng="pl">pa&zacute;</trd>
    <trd lng="pt">pajem</trd>
    <trd lng="ru">&c_p;&c_a;&c_zh;</trd>
  </snc>
  <snc mrk="pagxi.0o.karto">
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="dif" cel="fant.0o">Fanto</ref>:
      <ekz>
        la kartaro de Taroko <klr>[&#8230;]</klr> enhavas 56 kartojn, kun samaj emblemoj,
        kaj kiu iras &gcirc;is 10 kaj krome <tld/>o, &ccirc;evalo, re&gcirc;ino kaj re&gcirc;o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=255875">Hispana ludkartaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_v;&c_a;&c_l;&c_je;&c_t;</trd>
    <trd lng="fa">&a_s;&a_r;&a_b;&a_A;&a_z;<klr tip="ind"> (&a_w;&a_r;&a_q; &a_b;&a_A;&a_z;&f_y;)</klr></trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x5C0F;&#x59D3;  <pr>&#12371;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#x8FD1;&#x7FD2;  <pr>&#x304D;&#12435;&#12376;&#12421;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&c_zh;</trd>,
    <trd>&c_v;&c_a;&#x43B;&c_je;&c_t;</trd>
  </trdgrp>
</drv>
</art>
