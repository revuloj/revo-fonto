<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>biogas</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="biogas.0o">
  <kap><tld/>o</kap>
  <snc mrk="biogas.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="gas.brul0o.ENE">Brulgaso</ref>
      estigata de fermentado de organika&jcirc;oj,
      kun metano kaj karbondioksido kiel precipaj konsista&jcirc;oj:
      <ekz>
        <tld/>o esti&gcirc;as nature en la mar&ccirc;oj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Biogaso">
          </url>, 2005-09-08</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>on oni uzas kiel hejta&jcirc;on por kaldronoj kaj 
        elektrigiloj
        <fnt>
          <aut>-</aut>,
          <vrk>Reser&ccirc;o kaj Disvolvo</vrk>,
          <lok><url ref="http://www.bio-tec.net">biotec</url>,
          [vidita en 2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_i;&c_o;&c_g;&c_a;&c_z;</trd>
  <trd lng="de">Biogas</trd>
  <trd lng="fr">biogaz</trd>
</drv>
</art>

