<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: biogas.xml,v 1.12 2023/10/29 19:11:02 revo Exp $">
<kap>
  <rad>biogas</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="biogas.0o">
  <kap><tld/>o</kap>
  <snc mrk="biogas.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="gas.brul0o.ENE">Brulgaso</ref>
      estigata de fermentado de organika&jcirc;oj,
      kun metano kaj karbondioksido kiel precipaj konsista&jcirc;oj:
      <ekz>
        <tld/>o esti&gcirc;as nature en la mar&ccirc;oj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Biogaso">
          </url>, 2005-09-08</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>on oni uzas kiel hejta&jcirc;on por kaldronoj kaj 
        elektrigiloj
        <fnt>
          <aut>-</aut>,
          <vrk>Reser&ccirc;o kaj Disvolvo</vrk>,
          <lok><url ref="http://www.bio-tec.net">biotec</url>,
          [vidita en 2010]</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x751F;&#x5316;&#x6C14;&#x4F53; <pr>sh&#x113;nghu&#xE0;q&#xEC;t&#x1D0;</pr></trd>,
    <trd>&#x751F;&#x7269;&#x6C14; <pr>sh&#x113;ngw&#xF9;q&#xEC;</pr></trd>,
    <trd>&#x751F;&#x7269;&#x6C14;&#x4F53; <pr>sh&#x113;ngw&#xF9;q&#xEC;t&#x1D0;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="bg">&c_b;&c_i;&c_o;&c_g;&c_a;&c_z;</trd>
  <trd lng="ca">biog&agrave;s</trd>
  <trd lng="cs">bioplyn</trd>
  <trd lng="de">Biogas</trd>
  <trd lng="en">biogas</trd>
  <trd lng="fr">biogaz</trd>
  <trd lng="ja">&#x30D0;&#x30A4;&#x30AA;&#x30AC;&#x30B9;</trd>
  <trdgrp lng="pl">
    <trd>biogaz</trd>,
    <trd>gaz wysypiskowy</trd>
  </trdgrp>
  <trd lng="sk">bioplyn</trd>
  <trd lng="sv">biogas</trd>
  <trd lng="uk">&#x431;&#x456;&#x43E;&#x433;&#x430;&#x437;</trd>
</drv>
</art>

