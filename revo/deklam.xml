<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>deklam</rad>/i
</kap>
<drv mrk="deklam.0i">
  <kap><tld/>i</kap>
  <snc>
    <gra><vspec>tr</vspec></gra>
    <dif>
      La&ubreve;te vo&ccirc;legi a&ubreve; reciti, kun artisme ta&ubreve;ga tono:
      <ekz>
        <tld/>i versojn.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>deklamieren</trd>,
      <trd>vortragen</trd>,
      <trd>aufsagen</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="klr">(abs.)</uzo>
    <dif>
      Paroli per pasie emfaza tono:
      <ekz>
        oni multe <tld/>is pri sendependeco<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>beschw&ouml;ren</trd>,
      <trd>gro&szlig;e Worte machen</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_k;&c_l;&c_ja;&c_m;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">deklamovat</trd>
  <trd lng="es">declamar</trd>
  <trd lng="fr">d&eacute;clamer</trd>
  <trd lng="hu">szaval</trd>
  <trd lng="ru">&c_d;&c_je;&c_k;&c_l;&c_a;&c_m;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="deklam.ekspon0i">
  <kap>ekspon<tld/>i</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      &Ccirc;e foiro, bazaro ks, provi allogi la pasantojn, <tld/>ante
      pri la bonaj ecoj de la vendota&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">anpreisen</trd>
  <trd lng="fr">bonimenter</trd>
  <trdgrp lng="hu">
    <trd>vev&odblac;t csalogat</trd>,
    <trd>kiki&aacute;lt <klr>(v&aacute;s&aacute;rban)</klr></trd>
  </trdgrp>
</drv>
</art>
