<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stater.xml,v 1.10 2023/12/03 01:39:15 revo Exp $">
<kap>
  <rad>stater</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="stater.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Antikva greka <ref tip="lst" cel="unu.mezur0o" lst="voko:kvantoj">mezurunuo</ref> de
      maso kaj <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">monunuo</ref>:
      <ekz>
        iru al la maro kaj &jcirc;etu hoketon, kaj prenu la fi&scirc;on unue
        venintan,
        kaj malferminte &gcirc;ian bu&scirc;on, vi trovos <tld/>on
        <fnt><bib>NT</bib>, <lok>&Mat; 17:27</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_t;&c_e;&c_r; <klr>(&c_s;&c_t;&c_a;&c_r;&c_a;&c_zh;&c_y;&c_t;&c_n;&c_a;&c_g;&c_r;&c_e;&c_c;&c_k;&c_a;&c_ja; &c_m;&c_a;&c_n;&c_e;&c_t;&c_a;)</klr></trd>
  <trd lng="cs">stater</trd>
  <trd lng="de">Stater</trd>
  <trd lng="el">&sigma;&tau;&alfa;&tau;&eta_ton;&rho;</trd>
  <trd lng="fr">stat&egrave;re</trd>
  <trd lng="hu">sztat&eacute;r</trd>
  <trd lng="pl">stater  <klr>(jedn. monet.)</klr></trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_t;&c_je;&c_r;</trd>
  <trd lng="sk">stater</trd>
  <trd lng="uk">&c_s;&c_t;&c_a;&c_t;&c_je;&c_r; (&c_d;&c_a;&c_v;&#x43D;&#x44C;&#x43E;&c_g;&c_r;&c_je;&c_c;&#x44C;&#x43A;&c_a; &#x43C;&#x43E;&#x43D;&c_je;&c_t;&c_a;)</trd>
</drv>
</art>

