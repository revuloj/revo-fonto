<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stater</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="stater.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Antikva greka <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">mezurunuo</ref> de
      maso kaj <ref tip="lst" cel="unu.mon0o"
      lst="voko:monunuoj">monunuo</ref>:
      <ekz>
        iru al la maro kaj &jcirc;etu hoketon, kaj prenu la fi&scirc;on unue
        venintan,
        kaj malferminte &gcirc;ian bu&scirc;on, vi trovos <tld/>on
        <fnt><bib>NT</bib>, <lok>&Mat; 17:27</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Stater</trd>
  <trd lng="el">&sigma;&tau;&alfa;&tau;&eta_ton;&rho;</trd>
  <trd lng="fr">stat&egrave;re</trd>
  <trd lng="hu">sztat&eacute;r</trd>
  <trd lng="ru">&c_s;&c_t;&c_a;&c_t;&c_je;&c_r;</trd>
</drv>
</art>

