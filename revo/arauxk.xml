<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ara&ubreve;kari</rad>/o
</kap>
<drv mrk="arauxk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="arauxk.0acoj">ara&ubreve;kariacoj</ref>
      (<trd lng="la">Araucaria</trd>), kreskanta en tropikaj landoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Zimmertanne</trd>,
    <trd>Araukarie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>monkey puzzle</trd>,
    <trd>Chili pine</trd>
  </trdgrp>
  <trd lng="fr">araucaria</trd>
  <trd
  lng="ru">&#1072;&#1088;&#1072;&#1091;&#1082;&#1072;&#1088;&#1080;&#1103;</trd>
</drv>
<drv mrk="arauxk.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="konifer.0oplantoj">koniferoplantoj</ref>
      (<trd lng="la">Araucariaceae</trd>).
    </dif>
  </snc>
</drv>
</art>
