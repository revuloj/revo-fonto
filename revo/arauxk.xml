<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arauxk.xml,v 1.21 2023/10/28 08:33:08 revo Exp $">
<kap>
  <rad>ara&ubreve;kari</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="arauxk.0o">
  <kap><tld/>o</kap>
  <snc mrk="arauxk.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="konifer.0o" lst="voko:koniferoj">Konifero</ref>
      kun skvamecaj plurjaraj folioj kaj ovoformaj konusoj,
      kreskanta en tropikaj landoj,
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">genro</ref> el familio
      <ref tip="malprt" cel="arauxk1.0oj">ara&ubreve;kariacoj</ref>
      (<trd lng="la">Araucaria</trd>): 
      <ekz>
        vidi&gcirc;is denove la anta&ubreve;a &gcirc;ardeno:
	&ccirc;iamverda&jcirc;oj, <tld/>o kaj &ccirc;io
        <fnt>
          <aut>G. K. Chesterton, trad. C. Bean</aut>,
          <vrk>La Naiveco de Pastro Brown</vrk>,
          <lok>1937</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x5357;&#x6D0B;&#x6749; <pr>n&#xE1;ny&#xE1;ngsh&#x101;n</pr></trd>
</snc>
  <trd lng="be">&c_a;&c_r;&c_a;&c_w;&c_k;&c_a;&c_r;&c_y;&c_ja;</trd>
  <trd lng="ca">arauc&agrave;ria</trd>
  <trdgrp lng="de">
    <trd>Zimmertanne</trd>,
    <trd>Araukarie</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>monkey puzzle</trd>,
    <trd>Chili pine</trd>
  </trdgrp>
  <trd lng="fr">araucaria</trd>
  <trdgrp lng="he">
    <trd>&#x5E1;&#x5D5;&#x5D2; &#x5E2;&#x5E5;</trd>,
    <trd>&#x5D0;&#x5E8;&#x5D0;&#x5D5;&#x5E7;&#x5E8;&#x5D9;&#x5D4;</trd>
  </trdgrp>
  <trd lng="hu">arauk&aacute;ria</trd>
  <trd lng="ru">&c_a;&c_r;&c_a;&c_u;&c_k;&c_a;&c_r;&c_i;&c_ja;</trd>
  <trd lng="uk">&#x430;&#x440;&#x430;&#x443;&#x43A;&#x430;&#x440;&#x456;&#x44F;</trd>
</drv>
</art>
