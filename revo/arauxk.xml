<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ara&ubreve;kari</rad>/o
</kap>
<drv mrk="arauxk.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio
      <ref tip="super" cel="arauxk.0acoj">ara&ubreve;kariacoj</ref>
      (<trd lng="la">Araucaria</trd>), kreskanta en tropikaj landoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Zimmertanne</trd>,
    <trd>Araukarie</trd>
  </trdgrp>
</drv>
<drv mrk="araxk.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de pingloarboj (<trd lng="la">Araucaria</trd>)
    </dif>
  </snc>
</drv>
</art>
