<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>turk</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="turk.0o">
  <kap><tld/>o</kap>
  <snc mrk="turk.0o.ETN">
    <dif>
      Ano de la <ref cel="tjurk.0alingvo">tjurklingva</ref>
      nacio konsistiganta la plej grandan parton de la lo&gcirc;antaro
      de <ref tip="malprt" cel="turk.T0ujo"><tld lit="T"/>ujo</ref>.
    </dif>
    <ref tip="super" cel="tjurk.0o">tjurko</ref>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_a;&c_k;</trd>
  <trd lng="bg">&c_t;&c_u;&c_r;&c_ch;&c_i;&c_n;</trd>
  <trd lng="ca">turc<klr> (subst.)</klr></trd>
  <trd lng="de">T&uuml;rke</trd>
  <trdgrp lng="en">
    <trd>Turk</trd>,
    <trd>Ottoman</trd>
  </trdgrp>
  <trd lng="fa">&a_t;&a_r;&f_k;</trd>
  <trd lng="fr">turc</trd>
  <trd lng="hu">t&ouml;r&ouml;k</trd>
  <trd lng="pl">Turek</trd>
  <trd lng="pt">turco</trd>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_o;&c_k;</trd>
  <trd lng="sv">turk</trd>
  <trd lng="tr">T&uuml;rk</trd>
</drv>

<drv mrk="turk.0a">
  <kap><tld/>a</kap>
  <snc mrk="turk.0a.ETN">
    <dif>
      Rilata al la <tld/>oj:
      <ekz>
        <uzo tip="fak">LIN</uzo>
        la <ind><tld/>a lingvo</ind> apartenas al la
        <ref tip="super" cel="oguz.0alingvo">oguza</ref> bran&ccirc;o
        de la <ref cel="tjurk.0alingvo">tjurkaj lingvoj</ref>.
        <trd lng="be">&c_t;&c_u;&c_r;&c_e;&c_c;&c_k;&c_a;&c_ja;
        &c_m;&c_o;&c_v;&c_a;</trd>
        <trd lng="en">Turkish language</trd>
        <trd lng="fa">&a_t;&a_r;&f_k;&f_y; &a_A;&a_s;&a_t;&a_A;&a_n;&a_b;&a_w;&a_l;&f_y;</trd>
        <trd lng="hu">t&ouml;r&ouml;k nyelv</trd>
        <trd lng="nl">Turks</trd>
        <trd lng="pl">j&eogonek;zyk <ind>turecki</ind></trd>
        <trd lng="pt">turco</trd>
        <trd lng="ru">&c_t;&c_u;&c_r;&c_je;&c_c;&c_k;&c_i;&c_j;
        &c_ja;&c_z;&c_y;&c_k;</trd>
        <trdgrp lng="tr">
          <trd>T&uuml;rk&ccedil;e</trd>,
          <trd>T&uuml;rk dili</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_e;&c_c;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_t;&c_u;&c_r;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">turc<klr> (adj.)</klr></trd>
  <trd lng="de">t&uuml;rkisch</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_r;&f_k;&f_y;</trd>,
    <trd>&a_t;&a_r;&f_k;</trd>
  </trdgrp>
  <trd lng="en">Turkish</trd>
  <trd lng="fr">turc<klr> (adj.)</klr></trd>
  <trd lng="hu">t&ouml;r&ouml;k</trd>
  <trd lng="nl">Turks</trd>
  <trd lng="pl">turecki</trd>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_je;&c_c;&c_k;&c_i;&c_j;</trd>
  <trd lng="sv">turkisk</trd>
</drv>

<drv mrk="turk.T0ujo">
  <kap><tld lit="T"/>ujo</kap>
  <snc mrk="turk.T0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Granda <ref tip="super" cel="sxtat.0o">&scirc;tato</ref> okupanta
      la tutan
      <ref cel="anatoli.A0o">Anatolion</ref> kaj apudan pecon de
      Balkana duoninsulo en
      <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_T;&c_u;&c_r;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_T;&c_u;&c_r;&c_e;&c_ch;&c_ch;&c_y;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_T;&c_u;&c_r;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>Turquia</trd>,
    <trd>Rep&uacute;blica de Turquia</trd>
  </trdgrp>
  <trd lng="de">T&uuml;rkei</trd>
  <trd lng="en">Turkey</trd>
  <trd lng="fa">&a_t;&a_r;&f_k;&f_y;&a_h;</trd>
  <trd lng="fr">Turquie</trd>
  <trd lng="nl">Turkije</trd>
  <trd lng="pl">Turcja</trd>
  <trd lng="pt">Turquia</trd>
  <trd lng="ru">&c_T;&c_u;&c_r;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sv">Turkiet</trd>
  <trd lng="tr">T&uuml;rkiye</trd>
</drv>
</art>
