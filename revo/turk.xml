<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: turk.xml,v 1.32 2023/10/21 14:37:42 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>7</ofc>
  <rad>turk</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="turk.0o">
  <kap><ofc>*</ofc> <tld/>o</kap>
  <snc mrk="turk.0o.ETN">
    <dif>
      Ano de la <ref cel="tjurk.0alingvo">tjurklingva</ref>
      nacio konsistiganta la plej grandan parton de la lo&gcirc;antaro
      de <ref tip="malprt" cel="turk.T0ujo"><tld lit="T"/>ujo</ref>:
      <ekz>
        Turk' Turc &#x2010; Turk'uj' Turquie 
        <fnt><bib>F</bib>, <lok>Universala Vortaro, -uj'</lok></fnt>
      </ekz>
    </dif>
    <ref tip="super" cel="tjurk.0o">tjurko</ref>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_a;&c_k;</trd>
  <trd lng="bg">&c_t;&c_u;&c_r;&c_ch;&c_i;&c_n;</trd>
  <trd lng="ca">turc<klr> (subst.)</klr></trd>
  <trd lng="de">T&uuml;rke</trd>
  <trdgrp lng="en">
    <trd>Turk</trd>,
    <trd>Ottoman</trd>
  </trdgrp>
  <trd lng="fa">&a_t;&a_r;&f_k;</trd>
  <trd lng="fr">Turc<klr> (subst.)</klr></trd>
  <trd lng="fy">Turk</trd>
  <trd lng="hu">t&ouml;r&ouml;k</trd>
  <trd lng="id">Turk</trd>
  <trdgrp lng="io">
    <trd>Turkiano</trd>,
    <trd>Turko</trd>
  </trdgrp>
  <trd lng="ja">&#x30C8;&#x30EB;&#x30B3;&#x4EBA;  <pr>&#x30C8;&#x30EB;&#x30B3;&#x3058;&#x3093;</pr></trd>
  <trd lng="nl">Turk</trd>
  <trd lng="pl">Turek</trd>
  <trd lng="pt">turco</trd>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_o;&c_k;</trd>
  <trd lng="sv">turk</trd>
  <trd lng="tok">jan pi ma Tuki</trd>
  <trd lng="tr">T&uuml;rk</trd>
  <trd lng="uk">&c_t;&c_u;&c_r;&#x43E;&#x43A;</trd>
  <trd lng="vo">T&uuml;rk&auml;nan</trd>
</drv>

<drv mrk="turk.0a">
  <kap><tld/>a</kap>
  <snc mrk="turk.0a.ETN">
    <dif>
      Rilata al la <tld/>oj:
      <ekz>
        <uzo tip="fak">LIN</uzo>
        la <ind><tld/>a lingvo</ind> apartenas al la
        <ref tip="super" cel="oguz.0alingvo">oguza</ref> bran&ccirc;o
        de la <ref cel="tjurk.0alingvo">tjurkaj lingvoj</ref>.
        <trd lng="be">&c_t;&c_u;&c_r;&c_e;&c_c;&c_k;&c_a;&c_ja; &c_m;&c_o;&c_v;&c_a;</trd>
        <trd lng="en">Turkish language</trd>
        <trd lng="fa">&a_t;&a_r;&f_k;&f_y; &a_A;&a_s;&a_t;&a_A;&a_n;&a_b;&a_w;&a_l;&f_y;</trd>
        <trd lng="fy">Turksk</trd>
        <trd lng="hu">t&ouml;r&ouml;k nyelv</trd>
        <trdgrp lng="io">
          <trd>Turkiana linguo</trd>,
          <trd>Turka linguo</trd>
        </trdgrp>
        <trd lng="nl">Turks</trd>
        <trd lng="pl">j&eogonek;zyk <ind>turecki</ind></trd>
        <trd lng="pt">turco</trd>
        <trd lng="ru">&c_t;&c_u;&c_r;&c_je;&c_c;&c_k;&c_i;&c_j; &c_ja;&c_z;&c_y;&c_k;</trd>
        <trd lng="tok">toki Tuki</trd>
        <trdgrp lng="tr">
          <trd>T&uuml;rk&ccedil;e</trd>,
          <trd>T&uuml;rk dili</trd>
        </trdgrp>
        <trd lng="vo">T&uuml;rk&auml;nap&uuml;k</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_e;&c_c;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_t;&c_u;&c_r;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">turc<klr> (adj.)</klr></trd>
  <trd lng="cs">tureck&yacute;</trd>
  <trd lng="de">t&uuml;rkisch</trd>
  <trd lng="en">Turkish</trd>
  <trdgrp lng="fa">
    <trd>&a_t;&a_r;&f_k;&f_y;</trd>,
    <trd>&a_t;&a_r;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">turc<klr> (adj.)</klr></trd>
  <trd lng="fy">Turksk</trd>
  <trd lng="hu">t&ouml;r&ouml;k</trd>
  <trdgrp lng="io">
    <trd>Turkiana</trd>,
    <trd>Turka</trd>
  </trdgrp>
  <trd lng="ja">&#x30C8;&#x30EB;&#x30B3;&#x306E;  <pr>&#x3068;&#x308B;&#x3053;&#x306E;</pr></trd>
  <trd lng="nl">Turks</trd>
  <trd lng="pl">turecki</trd>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_je;&c_c;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">tureck&yacute;</trd>
  <trd lng="sv">turkisk</trd>
  <trd lng="tok">pi ma Tuki</trd>
  <trd lng="uk">&c_t;&c_u;&c_r;&c_je;&c_c;&#x44C;&#x43A;&c_i;&c_j;</trd>
  <trd lng="vo">T&uuml;rk&auml;nik</trd>
</drv>

<drv mrk="turk.T0ujo">
  <kap><ofc>*</ofc><tld lit="T"/>ujo</kap>
  <mlg kod="ISO-3166">TR</mlg>
  <snc mrk="turk.T0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Granda <ref tip="super" cel="sxtat.0o">&scirc;tato</ref> okupanta la
      tutan <ref cel="anatoli.0o">Anatolion</ref> kaj apudan pecon de
      Balkana duoninsulo en <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>:
      <ekz>
        Turk' Turc &#x2010; Turk'uj' Turquie 
        <fnt><bib>F</bib>, <lok>Universala Vortaro, -uj'</lok></fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_T;&c_u;&c_r;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_T;&c_u;&c_r;&c_e;&c_ch;&c_ch;&c_y;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_T;&c_u;&c_r;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>Turquia</trd>,
    <trd>Rep&uacute;blica de Turquia</trd>
  </trdgrp>
  <trd lng="de">T&uuml;rkei</trd>
  <trd lng="en">Turkey</trd>
  <trd lng="es">Turqu&iacute;a</trd>
  <trd lng="fa">&a_t;&a_r;&f_k;&f_y;&a_h;</trd>
  <trdgrp lng="fr">
    <trd>Turquie</trd>,
    <trd>R&eacute;publique de Turquie</trd>
  </trdgrp>
  <trd lng="fy">Turkije</trd>
  <trd lng="id">Turki</trd>
  <trd lng="io">Turkia</trd>
  <trd lng="nl">Turkije</trd>
  <trd lng="pl">Turcja</trd>
  <trd lng="pt">Turquia</trd>
  <trd lng="ro">Turcia</trd>
  <trd lng="ru">&c_T;&c_u;&c_r;&c_c;&c_i;&c_ja;</trd>
  <trd lng="sv">Turkiet</trd>
  <trd lng="tok">ma Tuki</trd>
  <trd lng="tr">T&uuml;rkiye</trd>
  <trd lng="vo">T&uuml;rk&auml;n</trd>
</drv>
</art>
