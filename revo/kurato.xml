<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>kurator</rad>/o
</kap>
<drv mrk="kurato.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kurato.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Individuo jure nomita por zorgi pri persono kaj havo de neplena&gcirc;a
      orfo a&ubreve; de iu kiu ne posedas la personajn rajtojn.
    </dif>                          
    <trd lng="br">kulator</trd>
    <trd lng="de">Vormund</trd>
    <trdgrp lng="en">
      <trd>conservator</trd>,
      <trd>guardian</trd>,
      <trd>trustee</trd>
    </trdgrp>
    <trd lng="fr">curateur</trd>
    <trdgrp lng="hu">
      <trd>gy&aacute;m</trd>,
      <trd>gondnok</trd>
    </trdgrp>
    <trd lng="nl">voogd</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_p;&c_je;&c_k;&c_u;&c_n;</trd>,
      <trd>&c_p;&c_o;&c_p;&c_je;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
    </trdgrp>
    <trd lng="sv">f&ouml;rmyndare</trd>
  </snc>
  <snc mrk="kurato.0o.JUR_postmorta">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Individuo jure nomita por zorgi pri la hereda&jcirc;o de persono
      a&ubreve; persongrupo.
    </dif>                  
    <trd lng="br">kulator</trd>
    <trdgrp lng="de">
      <trd>Nachlasspfleger</trd>,
      <trd>Kurator</trd>
    </trdgrp>
    <trd lng="fr">curateur</trd>
    <trd lng="hu">hagyat&eacute;ki <ind>gondnok</ind></trd>
  </snc>
  <snc mrk="kurato.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Individuo nomita por administri la havojn de persono,
      persongrupo a&ubreve; entrepreno.
    </dif>
    <trdgrp lng="de">
      <trd>Verm&ouml;gensverwalter</trd>,
      <trd>Kurator</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>vagyonkezel&odblac;</trd>,
      <trd>vagyongondnok</trd>
    </trdgrp>
  </snc>
  <snc mrk="kurato.0o.PED">
    <uzo tip="fak">PED</uzo>
    <uzo tip="stl">EVI</uzo>
    <dif>
      &Ccirc;efprofesoro de universitata fakultato en kelkaj nordaj landoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="dekan.0o.PED">dekano</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>Dekan einer Fakult&auml;t</trd>,
      <trd>Lehrstuhlinhaber</trd>
    </trdgrp>
    <trd lng="en">department head (Usono)</trd>
    <trdgrp lng="hu">
      <trd>d&eacute;k&aacute;n</trd>,
      <trd>tansz&eacute;kvezet&odblac;</trd>
    </trdgrp>
    <trd lng="nl">curator</trd>
    <trd lng="ru">&c_k;&c_u;&c_r;&c_a;&c_t;&c_o;&c_r;</trd>
  </snc>
  <snc mrk="kurato.0o.BIBL">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Estro de muzeo a&ubreve; biblioteko.
    </dif>
    <trd lng="de">Konservator</trd>
    <trd lng="en">conservator</trd>
    <trd lng="hu">konzerv&aacute;tor</trd>
    <trd lng="nl">conservator</trd>
  </snc>
</drv>

<drv mrk="kurato.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Ofico de <tld/>o:
      <ekz>
        la romanaj institucioj submetis la virinojn sub kontinua <tld/>eco;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        konfidi la <tld/>econ de tiuj popoloj al la progresintaj nacioj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Vormundschaft</trd>,
    <trd>Kuratel</trd>
  </trdgrp>      
  <trd lng="br">kulaterezh</trd>
  <trd lng="fr">curatelle</trd>
  <trdgrp lng="hu">
    <trd>gondnoks&aacute;g</trd>,
    <trd>gy&aacute;ms&aacute;g</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>curatele</trd>,
    <trd>voogdij</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_je;&c_k;&c_u;&c_n;&c_s;&c_t;&c_v;&c_o;</trd>,
   
<trd>&c_p;&c_o;&c_p;&c_je;&c_ch;&c_i;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_k;&c_u;&c_r;&c_a;&c_t;&c_o;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
  </trdgrp>
</drv>
</art>
