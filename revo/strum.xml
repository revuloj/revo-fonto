<vortaro>
  <art mrk="$Id: strum.xml,v 1.2 2023/09/04 15:14:32 afido Exp $">
    <kap>
      <rad>strum</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="strum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      &Scirc;veli&gcirc;o de la <ref cel="tiroid.0o">tiroido</ref>:
      <ekz>
        oni kuracis tiroid-insuficiencon post la forigo de tiroido pro ekzoftalmia
        <tld/>o (Bazedova malsano)
        <fnt>
          <aut>Konstantin Efetov</aut>
          <vrk><url ref="https://dvd.ikso.net/revuo/Kontakto/2010/2.pdf">La solvo de la mistero de la nekuracebla malsano</url></vrk>
          <lok>Kontakto, numero 236 (2010:2)</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsanulon <klr>[&#x2026;]</klr> turmentatan de <tld/>o ili lasis por morga&ubreve;
        <fnt>
          <aut>Jurij German, trad. Jurij Finkel</aut>
          <vrk><url ref="https://libro.ee/file.php?id=1057">La afero al kiu vi servas, 1957</url></vrk>
          <lok>p. 274</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Struma</trd>
    <trd lng="en">goitre</trd>
    <trd lng="fr">goitre</trd>
    <trd lng="ru">&c_z;&c_o;&c_b; <klr>(&c_s;&c_t;&c_r;&c_u;&c_m;&c_a;)</klr></trd>
  </snc>
  <trd lng="ja">&#x7532;&#x72B6;&#x817A;&#x816B;  [<ind>&#x3053;&#x3046;&#x3058;&#x3087;&#x3046;&#x305B;&#x3093;&#x3057;&#x3085;</ind>]</trd>
</drv>

  </art>

