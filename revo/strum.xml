<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: strum.xml,v 1.5 2024/01/31 09:22:36 afido Exp $">
    <kap>
      <rad>strum</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="strum.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      &Scirc;veli&gcirc;o de la <ref cel="tiroid.0o">tiroido</ref>:
      <ekz>
        oni kuracis tiroid-insuficiencon post la forigo de tiroido pro ekzoftalmia
        <tld/>o (Bazedova malsano)
        <fnt>
          <aut>Konstantin Efetov</aut>
          <vrk><url ref="https://dvd.ikso.net/revuo/Kontakto/2010/2.pdf">La solvo de la mistero de
          la nekuracebla malsano</url></vrk>
          <lok>Kontakto, numero 236 (2010:2)</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsanulon <klr>[&#8230;]</klr> turmentatan de <tld/>o ili lasis por morga&ubreve;
        <fnt>
          <aut>Jurij German, trad. Jurij Finkel</aut>
          <vrk><url ref="https://libro.ee/file.php?id=1057">La afero al kiu vi servas,
          1957</url></vrk>
          <lok>p. 274</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
    <trd lng="be">&c_v;&c_a;&c_l;&c_mol;&c_l;&c_ja;&c_k; <klr tip="amb">(&c_z;&c_a;&c_p;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;)</klr></trd>
    <trd lng="de">Struma</trd>
    <trd lng="en">goitre</trd>
    <trd lng="fr">goitre</trd>
    <trd lng="ja">&#30002;&#x72B6;&#x817A;&#x816B;  <pr>&#12371;&#12358;&#12376;&#12423;&#12358;&#x305B;&#12435;&#12375;&#12421;</pr></trd>
    <trd lng="pl">wole <klr>(u cz&lstroke;owieka)</klr></trd>
    <trd lng="ru">&c_z;&c_o;&c_b; <klr>(&c_s;&c_t;&c_r;&c_u;&c_m;&c_a;)</klr></trd>
</drv>
  </art>

