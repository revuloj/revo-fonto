<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>rus</rad>/o
    </kap>

    <drv mrk="rus.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <snc>
        <dif>
          Ano de orientslava <ref tip="lst" cel="etn.0o"
          lst="voko:popolanoj">etno</ref>:
          <ekz>
           Ili estas rusoj <fnt><bib>F</bib>, <lok>&FE; 16</lok></fnt>;
          </ekz>
          <ekz>
           (ekzemple rusoj a&ubreve; poloj, kiuj ne scias alian lingvon krom
           sia propra) <fnt><bib>F</bib>, <lok>&FE; 27</lok></fnt>;
          </ekz>
          <ekz>
Germanoj kaj francoj, kiuj lo&gcirc;as en Rusujo, estas Rusujanoj, kvankam ili
ne estas rusoj <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>;
          </ekz>
          <ekz>
            du maristoj <tld/>oj gaje saltis malsupren kaj preska&ubreve;
            renversis la kompatindan knabon
            <fnt><bib>Fab1</bib>, <lok>Kupra porko</lok></fnt>
          </ekz>.
        </dif>
        <trd lng="ru">&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;<klr> (&c_s;&c_u;&c_shch;.)</klr></trd>
      </snc>
      <snc>
      	<uzo tip="stl">RAR</uzo>
        <ref tip="dif" cel="rus.0ujano"><tld/>ujano</ref>
        <ekz>
          &ccirc;u ne estas pli simple tuj scii, ke se la lando nomi&gcirc;as <tld
          lit="R"/>ujo, la civitano havos la nomon <tld/>o?
          <fnt>
            <aut>Anna L&ouml;wenstein</aut>:
            <vrk>Rusoj lo&gcirc;as en Rusujo: landonomoj en Esperanto</vrk>.
            <lok>2007</lok>
          </fnt>.
        </ekz>
        <trd lng="ru">&c_r;&c_o;&c_s;&c_s;&c_i;&c_ja;&c_n;&c_i;&c_n;</trd>
      </snc>
      <trd lng="be">&c_r;&c_a;&c_s;&c_je;&c_je;&c_c;<klr> (&c_p;&c_a;&c_v;&c_o;&c_d;&c_l;&c_je; &c_n;&c_a;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s; &c_mol;&c_c;&c_ib;)</klr></trd>
      <trd lng="bg">&c_r;&c_u;&c_s;&c_n;&c_a;&c_k;</trd>
      <trd lng="ca">rus<klr> (subst.)</klr></trd>
      <trd lng="cs">Rus</trd>
      <trd lng="de">Russe</trd>
      <trd lng="en">Russian</trd>
      <trd lng="fr">Russe</trd>
      <trd lng="fy">Rus</trd>
      <trd lng="hu">orosz</trd>
      <trdgrp lng="io">
        <trd>Rusiano</trd>,
        <trd>Ruso</trd>
      </trdgrp>
      <trd lng="nl">Rus</trd>
      <trdgrp lng="pl">
        <trd>Rosjanin</trd>,
        <trd>Rusek<klr> (pogardliwie lub pot.)</klr></trd>,
        <trd>Ruski<klr> (pogardliwie lub pot.)</klr></trd>
      </trdgrp>
      <trd lng="pt">russo</trd>
      <trd lng="sk">Rus</trd>
      <trd lng="tp">jan Losi</trd>
      <trd lng="vo">Rus&auml;nan</trd>
    </drv>

    <drv mrk="rus.in0o">
      <kap><tld/>ino</kap>
      <snc>
        <dif>
          Inseksa <ref tip="super" cel="rus.0o"><tld/>o</ref>:
          <ekz>
            ekbrilis por ni la karaktero de l' <tld/>oj kaj <tld/>inoj
            en &gcirc;iaj la plej bonaj ecoj
            <fnt>
              <aut>Valdemar Langlet</aut>:
              <vrk>Voja&gcirc;impresoj</vrk>. <lok>1895</lok>
            </fnt>;
          </ekz>
          <ekz>
            la mastrino estis <tld/>ino, virino kvindekjara, pure
            vestita, kun inteligenta dol&ccirc;a viza&gcirc;o kaj serioza
            parolmaniero
            <fnt>
              <aut>I. Turgenjev, tr. Kabe</aut>:
              <vrk>Patroj kaj filoj</vrk>.
              <lok>Inko Tyreso 2001. &#8208; &Ccirc;ap. 8a.</lok>
            </fnt>;
          </ekz>
        </dif>
      </snc>
      <trd lng="bg">&c_r;&c_u;&c_s;&c_k;&c_i;&c_n;&c_ja;</trd>
      <trd lng="ca">russa<klr> (subst.)</klr></trd>
      <trd lng="cs">Ruska</trd>
      <trd lng="de">Russin</trd>
      <trdgrp lng="fy">
        <trd>Russise</trd>,
        <trd>Russin</trd>
      </trdgrp>
      <trdgrp lng="io">
        <trd>Rusianino</trd>,
        <trd>Rusino</trd>
      </trdgrp>
      <trdgrp lng="nl">
        <trd>Russische</trd>,
        <trd>Russin</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>Rosjanka</trd>,
        <trd>Ruska<klr> (pogardliwie lub pot.)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_r;&c_u;&c_s;&c_s;&c_k;&c_a;&c_ja;<klr> (&c_s;&c_u;&c_shch;.)</klr></trd>
      <trd lng="sk">Ruska</trd>
      <trd lng="vo">ji-Rus&auml;nan</trd>
    </drv>

    <drv mrk="rus.0a">
      <kap><ofc>*</ofc><ofc>Gram</ofc><tld/>a</kap>
      <snc>
        <dif>
          Rilata al <tld/>oj a&ubreve; ilia etno kaj lando:
          <ekz>
            <ind>la <tld/>a lingvo</ind>
            <trd lng="fy">Russysk</trd>
            <trdgrp lng="io">
              <trd>Rusiana linguo</trd>,
              <trd>Rusa linguo</trd>
            </trdgrp>
            <trd lng="nl">Russisch</trd>
            <trd lng="tp">toki Losi</trd>
            <trd lng="vo">Rus&auml;nap&uuml;k</trd>
          </ekz>
        </dif>
      </snc>
      <snc>
        <dif>
        la rusa = lingvo de Rusujo
          <ekz>
           &c_G;&c_R;&c_A;&c_M;&c_M;&c_A;&c_T;&c_I;&c_K;&c_A; (Gramatiko Rusa) <fnt><bib>F</bib>,
           <lok>Gramatiko</lok></fnt>;
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_r;&c_a;&c_s;&c_je;&c_j;&c_s;&c_k;&c_ib;</trd>
      <trd lng="bg">&c_r;&c_u;&c_s;&c_k;&c_i;</trd>
      <trd lng="ca">rus<klr> (adj.)</klr></trd>
      <trd lng="cs">rusk&yacute;</trd>
      <trd lng="de">russisch</trd>
      <trd lng="en">Russian</trd>
      <trd lng="fr">russe</trd>
      <trd lng="fy">Russysk</trd>
      <trd lng="hu">orosz</trd>
      <trdgrp lng="io">
        <trd>Rusiana</trd>,
        <trd>Rusa</trd>
      </trdgrp>
      <trd lng="nl">Russisch</trd>
      <trdgrp lng="pl">
        <trd>rosyjski</trd>,
        <trd>ruski<klr> (pogardliwie lub pot.)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j; <klr>(&c_p;&c_r;&c_i;&c_l;.)</klr></trd>
      <trd lng="sk">rusk&yacute;</trd>
      <trd lng="vo">Rus&auml;nik</trd>
      <trd lng="zh">&#20420;<klr> [&eacute;]</klr></trd>
    </drv>

    <drv mrk="rus.R0ujo">
      <kap><tld lit="R"/>lando,
      <var><kap><ofc>*</ofc><ofc>ekz</ofc><tld lit="R"/>ujo</kap></var>
      </kap>
      <snc>
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">POL</uzo>
        <dif>
          Plurnacia &scirc;tato, situanta en orienta
          <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref> kaj
          norda <ref tip="malprt" cel="azi.A0o">Azio</ref>,
          kies historia kerno estas la <tld/>a nacio:
          <ekz>
            germanoj kaj francoj, kiuj lo&gcirc;as en Rusujo, estas
            <tld/>ujanoj, kvankam ili ne estas <tld/>oj
            <fnt><bib>F</bib>, <lok>Ekzercaro, &para;37.</lok></fnt>;
          </ekz>
          <ekz>
            La &ccirc;efurbo de <tld lit="R"/>ujo &gcirc;is 1712 kaj
            ekde 1918 estas <ref tip="prt" cel="moskv.0o">Moskvo</ref>;
            de 1712 &gcirc;is 1918 la &ccirc;efurbo de <tld lit="R"/>ujo
            estis <ref tip="prt" cel="peterburg.0o">Peterburgo</ref>.
          </ekz>
        </dif>
        <rim>
          La nomo estas gentobaza<fnt>AdE</fnt>, oni uzas la vorton <tld
          lit="R"/>lando pro internacieco a&ubreve; Esperanta tradicio.
        </rim>
      </snc>
      <refgrp tip="sin">
        <ref cel="rusi.0o">Rusio</ref>,
        <ref cel="rus.R0aFederacio">Ruslanda Federacio</ref>
      </refgrp>
      <bld lok="../bld/Rusujo.png">
        Mapo de <tld lit="R"/>ujo
      </bld>
      <trd lng="be">&c_R;&c_a;&c_s;&c_je;&c_ja;</trd>
      <trd lng="bg">&c_R;&c_u;&c_s;&c_i;&c_ja;</trd>
      <trd lng="ca">R&uacute;ssia</trd>
      <trd lng="cs">Rusko</trd>
      <trd lng="de">Russland</trd>
      <trd lng="en">Russia</trd>
      <trd lng="fr">Russie</trd>
      <trd lng="fy">Rusl&acirc;n</trd>
      <trd lng="hu">Oroszorsz&aacute;g</trd>   
      <trd lng="id">Rusia</trd>
      <trd lng="io">Rusia</trd>
      <trd lng="nl">Rusland</trd>
      <trd lng="pl">Rosja</trd>
      <trd lng="pt">R&uacute;ssia</trd>
      <trd lng="ru">&c_R;&c_o;&c_s;&c_s;&c_i;&c_ja;</trd>
      <trd lng="sk">Rusko</trd>
      <trd lng="tp">ma Losi</trd>
      <trd lng="vo">Rus&auml;n</trd>
      <trdgrp lng="zh">
        <trd>&#20420;&#22269;</trd>,
        <trd><klr>tr. </klr>&#20420;&#22283;<klr> [&Eacute;gu&oacute;]</klr></trd>,
        <trd>&#20420;&#32599;&#26031;<klr> [&Eacute;lu&oacute;s&#299;]</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="rus.R0aFederacio">
      <kap><tld lit="R"/>landa Federacio</kap>
      <snc>
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">POL</uzo>
        <dif>
          Dua oficiala nomo de <tld lit="R"/>ujo.
        </dif>
      </snc>
      <trd lng="be">&c_R;&c_a;&c_s;&c_je;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_F;&c_e;&c_d;&c_e;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>
      <trd lng="ca">Federaci&oacute; Russa</trd>
      <trd lng="de">Russische F&ouml;deration</trd>
      <trd lng="en">Russian Federation</trd>
      <trd lng="fr">F&eacute;d&eacute;ration de Russie</trd>
      <trd lng="fy">Russyske Federaasje</trd>
      <trd lng="hu">Orosz F&ouml;der&aacute;ci&oacute;</trd>   
      <trd lng="io">Federativa Republiko di Rusia</trd>
      <trd lng="nl">Russische Federatie</trd>
      <trd lng="pl">Federacja <ind>Rosyjska</ind></trd>
      <trd lng="ru">&c_R;&c_o;&c_s;&c_s;&c_i;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_F;&c_je;&c_d;&c_je;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>
      <trd lng="vo">Fed Rus&auml;nik</trd>
    </drv>

    <drv mrk="rus.R0aImperio">
      <kap><tld lit="R"/>landa Imperio</kap>
      <snc>
      <uzo tip="fak">GEOG</uzo>
      <uzo tip="fak">POL</uzo>
        <dif>
          Oficiala nomo de <tld lit="R"/>ujo ekde 1721 &gcirc;is 1917.
        </dif>
      </snc>
      <trd lng="be">&c_R;&c_a;&c_s;&c_je;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_Ib;&c_m;&c_p;&c_e;&c_r;&c_y;&c_ja;</trd>
      <trd lng="ca">Imperi Rus</trd>
      <trd lng="de">Russisches Reich</trd>
      <trd lng="en">Russian Empire</trd>
      <trd lng="fr">Empire de Russie</trd>
      <trd lng="hu">Orosz Birodalom</trd>   
      <trd lng="nl">Russische Rijk</trd>
      <trdgrp lng="pl">
        <trd>Imperium <ind>Rosyjskie</ind></trd>,
        <trd>Wielka <ind>Rosja</ind></trd>
      </trdgrp>
      <trd lng="ru">&c_R;&c_o;&c_s;&c_s;&c_i;&c_j;&c_s;&c_k;&c_a;&c_ja; &c_I;&c_m;&c_p;&c_je;&c_r;&c_i;&c_ja;</trd>
    </drv>

    <drv mrk="rus.0ujano">
      <kap><tld/>landano,
      <var><kap><ofc>*</ofc><ofc>ekz</ofc><tld/>ujano</kap></var>
      </kap>
      <snc>
        <dif>
          Civitano de <tld lit="R"/>ujo sendepende de etno:
          <ekz>
            germanoj kaj francoj, kiuj lo&gcirc;as en Rusujo, estas
            <tld/>ujanoj, kvankam ili ne estas <tld/>oj
            <fnt><bib>F</bib>, <lok>Ekzercaro, &para;37.</lok></fnt>;
          </ekz><ekz>
            mia gepatra lingvo estas la <tld/>a, sed nun mi parolas pli
            pole, kaj mi nomas min ne &leftquot;<tld/>o&rightquot; sed &leftquot;<tld/>ujano&rightquot;
            <fnt>
              <aut>&Z;</aut>: <vrk>
              [Letero de 1901-03-08 al Th. Thorsteinsson, Kopenhago
              </vrk>,
              <lok>
              <url ref="http://www.math.uu.se/~kiselman/zamenhof.pdf">
              citite la&ubreve; Zamenhof 1929:523</url></lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_r;&c_a;&c_s;&c_je;&c_je;&c_c;<klr> (&c_p;&c_a;&c_v;&c_o;&c_d;&c_l;&c_je; &c_g;&c_r;&c_a;&c_m;&c_a;&c_d;&c_z;&c_ja;&c_n;&c_s;&c_t;&c_v;&c_a;)</klr></trd>
      <trd lng="bg">&c_r;&c_u;&c_s;&c_n;&c_a;&c_k;</trd>
      <trd lng="ca">rus<klr> (subst.)</klr></trd>
      <trd lng="cs">rusk&yacute; ob&ccaron;an</trd>  
      <trdgrp lng="de">
        <trd>Russe</trd>,
        <trd>russischer Staatsb&uuml;rger</trd>
      </trdgrp>
      <trd lng="en">Russian</trd>
      <trdgrp lng="fr">
        <trd>Russe<klr> (subst.)</klr></trd>
      </trdgrp>
      <trd lng="hu">oroszorsz&aacute;gi <klr tip="ind">(lakos)</klr></trd>
      <trdgrp lng="io">
        <trd>Rusiano</trd>,
        <trd>Ruso</trd>
      </trdgrp>
      <trd lng="nl">Russisch staatsburger</trd>
      <trdgrp lng="pl">
        <trd>Rosjanin</trd>,
        <trd>Rusek<klr> (pogardliwie lub pot.)</klr></trd>,
        <trd>Ruski<klr> (pogardliwie lub pot.)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_r;&c_o;&c_s;&c_s;&c_i;&c_ja;&c_n;&c_i;&c_n;</trd>
      <trd lng="sk">rusk&yacute; ob&ccaron;an</trd>
      <trd lng="tp">jan Losi</trd>
      <trd lng="vo">Rus&auml;nan</trd>
      <trdgrp lng="zh">
        <trd>&#20420;&#22269;&#20154;</trd>,
        <trd><klr>tr. </klr>&#20420;&#22283;&#20154;<klr> [&eacute;gu&oacute;r&eacute;n]</klr></trd>
      </trdgrp>
    </drv>

    <drv mrk="rus.0uja">
      <kap><tld/>landa, <var><kap><tld/>uja</kap></var></kap>
      <snc>
        <dif>
          Rilatanta a&ubreve; apartenanta al <tld lit="R"/>ujo:
          <ekz>
           gazeto &leftquot;<tld lit="R"/>landa Esperantisto&rightquot;.
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_r;&c_a;&c_s;&c_je;&c_j;&c_s;&c_k;&c_ib;</trd>
      <trd lng="bg">&c_r;&c_u;&c_s;&c_k;&c_i;</trd>
      <trd lng="ca">rus<klr> (adj.)</klr></trd>
      <trd lng="cs">rusk&yacute; <klr>(t&yacute;kaj&iacute;c&iacute; se Ruska)</klr></trd>
      <trd lng="de">russisch</trd>
      <trd lng="en">Russian</trd>
      <trdgrp lng="fr">
        <trd>russe<klr> (adj.)</klr></trd>
      </trdgrp>
      <trd lng="fy">Russysk</trd>
      <trd lng="hu">oroszorsz&aacute;gi</trd>
      <trdgrp lng="io">
        <trd>Rusiana</trd>,
        <trd>Rusa</trd>
      </trdgrp>
      <trd lng="nl">Russisch</trd>
      <trdgrp lng="pl">
        <trd>rosyjski</trd>,
        <trd>ruski <klr>(pot.)</klr></trd>
      </trdgrp>
      <trd lng="ru">&c_r;&c_o;&c_s;&c_s;&c_i;&c_j;&c_s;&c_k;&c_i;&c_j;</trd>
      <trd lng="sk">rusk&yacute;</trd>
      <trd lng="vo">Rus&auml;nik</trd>
    </drv>
  </art>
