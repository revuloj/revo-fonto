<drv mrk="rus.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rus.0o.naciano">
    <dif>
      Ano de orientslava <ref tip="lst" cel="etn.0o" lst="voko:popolanoj">etno</ref>:
      <ekz>
       ili estas rusoj <fnt><bib>F</bib>, <lok>&FE; 16</lok></fnt>;
      </ekz>
      <ekz>
       ekzemple rusoj a&ubreve; poloj, kiuj ne scias alian lingvon krom
       sia propra <fnt><bib>F</bib>, <lok>&FE; 27</lok></fnt>;
      </ekz>
      <ekz>
        germanoj kaj francoj, kiuj lo&gcirc;as en Rusujo, estas
        Rusujanoj,
        kvankam ili
        ne estas rusoj <fnt><bib>F</bib>, <lok>&FE; 37</lok></fnt>;
      </ekz>
      <ekz>
        du maristoj <tld/>oj gaje saltis malsupren kaj preska&ubreve;
        renversis la kompatindan knabon
        <fnt><bib>Fab1</bib>, <lok>Kupra porko</lok></fnt>
      </ekz>.
    </dif>
    <trd lng="ru">&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;<klr>
    (&c_s;&c_u;&c_shch;.)</klr></trd>
  </snc>
  <snc mrk="rus.0o.rusujano">
  	<uzo tip="stl">RAR</uzo>
    <dif>
      <ref tip="dif" cel="rus.0ujano"><tld lit="R"/>ujano</ref>:
      <ekz>
        la <tld/>oj lo&gcirc;as en <tld lit="R"/>ujo kaj la germanoj en
        Germanujo
        <fnt><bib>F</bib>, <lok>Ekzercaro, &para; 40</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;u ne estas pli simple tuj scii, ke se la lando
        nomi&gcirc;as <tld lit="R"/>ujo, la civitano havos la nomon <tld/>o
        <fnt>
          <aut>Anna L&ouml;wenstein</aut>:
          <vrk>Rusoj lo&gcirc;as en Rusujo: landonomoj en
          Esperanto</vrk>.
          <lok>2007</lok>
        </fnt>?
      </ekz>
    </dif>
    <trd lng="ru">&c_r;&c_o;&c_s;&c_s;&c_i;&c_ja;&c_n;&c_i;&c_n;</trd>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_je;&c_je;&c_c;<klr> (&c_p;&c_a;&c_v;&c_o;&c_d;&c_l;&c_je; &c_n;&c_a;&c_c;&c_y;&c_ja;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;)</klr></trd>
  <trd lng="bg">&c_r;&c_u;&c_s;&c_n;&c_a;&c_k;</trd>
  <trd lng="bo">&t_a;&t_shabchu;&t_tsheg;&t_ra;&t_shabchu;&t_tsheg;&t_sa;&t_shabchu;&t_tsheg;&t_ma;&t_gigu;&t_tsheg;</trd>
  <trd lng="ca">rus<klr> (subst.)</klr></trd>
  <trd lng="cs">Rus</trd>
  <trd lng="de">Russe</trd>
  <trd lng="en">Russian</trd>
  <trd lng="es">ruso<klr> (subst.)</klr></trd>
  <trd lng="fr">Russe</trd>
  <trd lng="fy">Rus</trd>
  <trd lng="hu">orosz</trd>
  <trdgrp lng="io">
    <trd>Rusiano</trd>,
    <trd>Ruso</trd>
  </trdgrp>
  <trd lng="ja">&#x30ED;&#x30B7;&#x30A2;&#x4EBA;  <pr>&#x30ED;&#x30B7;&#x30A2;&#x3058;&#x3093;</pr></trd>
  <trd lng="nl">Rus</trd>
  <trdgrp lng="pl">
    <trd>Rosjanin</trd>,
    <trd>Rusek<klr> (pogardliwie lub pot.)</klr></trd>,
    <trd>Ruski<klr> (pogardliwie lub pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">russo</trd>
  <trd lng="sk">Rus</trd>
  <trd lng="tok">jan pi ma Losi</trd>
  <trd lng="uk">&c_r;&c_o;&c_s;&c_ib;&c_ja;&c_n;&c_i;&c_n;</trd>
  <trd lng="vo">Rus&auml;nan</trd>
  <trdgrp lng="zh">
