<drv mrk="rus.0uja">
  <kap><tld/>landa, <var><kap><tld/>uja</kap></var></kap>
  <snc>
    <dif>
      Rilatanta a&ubreve; apartenanta al <tld lit="R"/>ujo:
      <ekz>
        gazeto &leftquot;<tld lit="R"/>landa Esperantisto&rightquot;
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        sen&ccirc;esa reciproka batalado de la diversaj gentoj kaj
        religioj en
        la vasta <tld lit="R"/>uja regno
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Dogmoj de Hilelismo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>landa prezidanto <nom>Putin</nom>
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
        </fnt>.
      </ekz>
    </dif>
