  </trdgrp>
</drv>

<drv mrk="rus.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc mrk="rus.0a.rilata">
    <dif>
      Rilata al <tld/>oj a&ubreve; ilia etno kaj lando:
      <ekz>
        la <ind><tld/>a lingvo</ind>
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>El la mondo homarana (hilelista)</vrk>
        </fnt>;
        <trd lng="fy">Russysk</trd>
        <trdgrp lng="io">
          <trd>Rusiana linguo</trd>,
          <trd>Rusa linguo</trd>
        </trdgrp>
        <trd lng="nl">Russisch</trd>
        <trd lng="tok">toki Losi</trd>
        <trd lng="vo">Rus&auml;nap&uuml;k</trd>
      </ekz>
      <ekz>
        super tiu diverseco pezigis ursan manon <tld/>a Imperio dum tuta
        centjaro dek-na&ubreve;a
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        en palaco lo&gcirc;is <tld/>a guberniestro
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur &ccirc;efa placo brilis oraj turoj de <tld/>a pre&gcirc;ejo
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        por disceligi la koleron de l' popolo, klopodis <tld/>a
        registaro perfide &jcirc;eti gentojn kontra&ubreve; unu la aliaj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        pola kaj litva landoj, tiam regataj de <tld/>a caro
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      <klr>(la)</klr> lingvo de la rusoj:
      <ekz>
         rekoni tekstojn en la <tld/>a
        <fnt>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/1995/004514.php">Por lingvemaj
          komputilemuloj ...</url></vrk>
          <bib>Monato</bib>
          <lok>1995/06, p. 22</lok>
        </fnt>;
      </ekz>
      <ekz>
        beletra&jcirc;oj tradukitaj el la <tld/>a
        <fnt>
          <aut>Boris Kolker</aut>
            <vrk><url ref="&Monato;/2000/006807.php">Fascina gazeto</url></vrk>
            <bib>Monato</bib>
            <lok>2000/03, p. 31</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_s;&c_je;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="bg">&c_r;&c_u;&c_s;&c_k;&c_i;</trd>
  <trd lng="bo">&t_a;&t_shabchu;&t_tsheg;&t_ra;&t_shabchu;&t_tsheg;&t_sa;&t_shabchu;&t_-a;&t_gigu;&t_tsheg;&t_sa;&t_kata;&t_da;&t_tsheg;</trd>
  <trd lng="ca">rus<klr> (adj.)</klr></trd>
  <trd lng="cs">rusk&yacute;</trd>
  <trd lng="de">russisch</trd>
  <trd lng="en">Russian</trd>
  <trd lng="es">ruso <klr> (adj.)</klr></trd>
  <trd lng="fr">russe</trd>
  <trd lng="fy">Russysk</trd>
  <trd lng="hu">orosz</trd>
  <trdgrp lng="io">
    <trd>Rusiana</trd>,
    <trd>Rusa</trd>
  </trdgrp>
  <trd lng="ja">&#x30ED;&#x30B7;&#x30A2;&#x306E;  <pr>&#x308D;&#x3057;&#x3042;&#x306E;</pr></trd>
  <trd lng="nl">Russisch</trd>
  <trdgrp lng="pl">
    <trd>rosyjski</trd>,
    <trd>ruski<klr> (pogardliwie lub pot.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_u;&c_s;&c_s;&c_k;&c_i;&c_j;<klr> (&c_p;&c_r;&c_i;&c_l;.)</klr></trd>
  <trd lng="sk">rusk&yacute;</trd>
  <trd lng="tok">pi ma Losi</trd>
  <trd lng="uk">&c_r;&c_o;&c_s;&c_ib;&c_j;&c_s;&c_mol;&c_k;&c_i;&c_j;</trd>
  <trd lng="vo">Rus&auml;nik</trd>
  <trd lng="zh">&#x4FC4; <pr>&eacute;</pr></trd>
</drv>

<drv mrk="rus.el0igi">
  <kap>
    el<tld/>igi
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="traduk.0i.transigi">Traduki</ref> el la <tld/>a lingvo:
      <ekz>
        kolekto el verkoj el<tld/>igitaj
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sinsekvaj reverkadoj &ndash; kaj la el<tld/>igo, kiu krome estas transigo al alia
        alfabeto &ndash; de la <nom>Drezen</nom>-studa&jcirc;o, supozeble kulpas pri la fakto ke
        <klr>[&#x2026;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>W. M. A. De Smet</aut>
          <vrk><url ref="&Monato;/2004/008418.html">Edward Croegaert, la forgesita lingvokreanto</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">aus dem Russischen <ind>&uuml;bersetzen</ind></trd>
</drv>

<drv mrk="rus.R0ujo">
  <kap><ofc>*</ofc><tld lit="R"/>ujo,
  <var><kap><tld lit="R"/>lando</kap></var>
  </kap>
  <mlg kod="ISO-3166">RU</mlg>
  <snc mrk="rus.R0ujo.GEOG">
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      Plurnacia &scirc;tato, situanta en orienta
      <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref> kaj
      norda <ref tip="malprt" cel="azi.A0o">Azio</ref>,
      kies historia kerno estas la <tld/>a nacio:
      <ekz>
        germanoj kaj francoj, kiuj lo&gcirc;as en Rusujo, estas
        <tld lit="R"/>ujanoj, kvankam ili ne estas <tld/>oj
        <fnt><bib>F</bib>, <lok>Ekzercaro, &para;37.</lok></fnt>;
      </ekz>
      <ekz>
        la &ccirc;efurbo de <tld lit="R"/>ujo &gcirc;is 1712 kaj
        ekde 1918 estas <ref tip="prt" cel="moskv.0o">Moskvo</ref>,
        de 1712 &gcirc;is 1918 la &ccirc;efurbo de <tld lit="R"/>ujo
        estis <ref tip="prt" cel="peterburg.0o">Peterburgo</ref>;
      </ekz>
      <ekz>
        la popoloj en <tld lit="R"/>lando komencis ribeli kontra&ubreve;
        carisma regado
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        la malamiko timata estas ne plu Sovetio (nuna <tld lit="R"/>lando)
        <fnt>
          <bib>Monato</bib>,
          <aut>S. Maul</aut>:
          <vrk>Malvarma milito denove?</vrk>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      La nomo estas gentobaza<fnt>
         <aut>&AE;</aut>
         <vrk>Landnomoj rekomendataj de la Akademio de Esperanto</vrk>
         <lok><url ref="&AdE;/decidoj/landnomoj/listo_de_rekomendataj_landnomoj/index.html">Oficialaj
         Informoj, numero 12 &#x2010; 2009 05 04</url></lok>
      </fnt>, oni uzas la vorton <tld lit="R"/>lando pro internacieco a&ubreve; Esperanta tradicio.
    </rim>
  </snc>
  <refgrp tip="sin">
    <ref cel="rusi.0o">Rusio</ref>,
    <ref cel="rus.R0aFederacio">Ruslanda Federacio</ref>
  </refgrp>
  <bld lok="../bld/Rusujo.png">
    Mapo de <tld lit="R"/>ujo
  </bld>
  <trd lng="be">&c_R;&c_a;&c_s;&c_je;&c_ja;</trd>
  <trd lng="bg">&c_R;&c_u;&c_s;&c_i;&c_ja;</trd>
  <trd lng="ca">R&uacute;ssia</trd>
  <trd lng="cs">Rusko</trd>
  <trd lng="de">Russland</trd>
  <trd lng="en">Russia</trd>
  <trd lng="es">Rusia</trd>
  <trd lng="fr">Russie</trd>
  <trd lng="fy">Rusl&acirc;n</trd>
  <trd lng="hu">Oroszorsz&aacute;g</trd>
  <trd lng="id">Rusia</trd>
  <trd lng="io">Rusia</trd>
  <trd lng="nl">Rusland</trd>
  <trd lng="pl">Rosja</trd>
  <trd lng="pt">R&uacute;ssia</trd>
  <trd lng="ru">&c_R;&c_o;&c_s;&c_s;&c_i;&c_ja;</trd>
  <trd lng="sk">Rusko</trd>
  <trd lng="tok">ma Losi</trd>
  <trd lng="vo">Rus&auml;n</trd>
  <trdgrp lng="zh">
    <trd>&#x4FC4;&#x56FD; <klr>(tr. &#x4FC4;&#x570B;)</klr> <pr>&Eacute;gu&oacute;</pr></trd>,
    <trd>&#x4FC4;&#x7F57;&#x65AF; <pr>&Eacute;lu&oacute;s&imacron;</pr></trd>
  </trdgrp>
</drv>

