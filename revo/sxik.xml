<kap>
  <rad>&scirc;ik</rad>/a
  <fnt>B</fnt>
</kap>
<drv mrk="sxik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      La&ubreve;mode bongusta:
      <ekz>
        <nom>Starbucks</nom> popularas inter junulinoj pro sia <tld/>a interna dekoracio
        <fnt>
          <bib>Monato</bib>
          <aut>Igarasi Takeo</aut>
          <vrk><url ref="&Monato;/2003/007563.html">Novtipaj kafejoj prosperas</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i sidi&gcirc;is kaj suspiris, deprenis <klr>[&#x2026;]</klr> la <tld/>ajn altkalkanumajn
        &scirc;uojn, kiujn &scirc;i a&ccirc;etis en Londono
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2006/008760.php">En fremda lando</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ni diligente preparis nin: ni surmetis niajn plej <tld/>ajn vestojn, <nom>Liza</nom> kombis
        min aparte bele kaj zorge &scirc;minkis sin
        <fnt>
          <bib>Monato</bib>
          <aut>Lena Karpunina</aut>
          <vrk><url ref="&Monato;/2006/008836.php">La revo pri cirko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la plej renomaj kaj <tld/>aj restoracioj de Parizo
        <fnt>
          <bib>Monato</bib>
          <aut>Yves Nicolas</aut>
          <vrk><url ref="&Monato;/2010/010225.php">Komuna agado, komunaj valoroj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        vi havas <tld/>an, ri&ccirc;an edzinon
        <fnt>
          <bib>Monato</bib>
          <aut>Trevor Steele</aut>
          <vrk><url ref="&Monato;/2015/011474.php">Tridek jarojn poste</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="elegan.0a">eleganta</ref>,
      <ref cel="pimp.0a">pimpa</ref>,
      <ref cel="bel.0a">bela</ref>,
      <ref cel="lind.0a">linda</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_sh;&c_y;&c_k;&c_o;&c_w;&c_n;&c_y;</trd>
  <trdgrp lng="de">
    <trd>schick</trd>,
    <trd>modisch</trd>,
    <trd>elegant</trd>
  </trdgrp>
  <trd lng="el">&kappa;&omikron;&my;&psi;&omikron_ton;&sigma_fina;</trd>
  <trdgrp lng="en">
    <trd>chic</trd>,
    <trd>fashionable</trd>
  </trdgrp>
  <trdgrp lng="es">
     <trd>de buen gusto</trd>,
     <trd>elegante</trd>
  </trdgrp>
  <trd lng="fa">&a_sh;&f_y;&f_k;</trd>
  <trd lng="fr">chic</trd>
  <trd lng="hu">sikkes</trd>
