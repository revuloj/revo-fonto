<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: centil.xml,v 1.23 2022/03/14 15:25:39 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>centilitr</rad>/o
</kap>
<drv mrk="centil.0o">
  <kap><tld/>o</kap>
  <mlg>cl</mlg>
  <snc>
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      kapacito, egala al centono de <ref tip="malprt" cel="litr.0o">litro</ref>:
      <ekz>
        la <nom>Chouffe</nom> estis krom en bareloj longtempe nur havebla en grandaj boteloj de 75
        <tld/>oj kaj 1,5 litro, sed ekde majo 2009 la biero anka&ubreve; haveblas en boteloj
        de 33 <tld/>oj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=320275">La Chouffe</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_a;&c_n;&c_t;&c_y;&c_l;&c_ib;&c_t;&c_a;&c_r;</trd>
    <trd lng="ca">centilitre</trd>
    <trd lng="de">Zentiliter</trd>
    <trd lng="el">&epsilon;&kappa;&alfa;&tau;&omikron;&sigma;&tau;&omikron_ton;&lambda;&jota;&tau;&rho;&omikron;</trd>
    <trdgrp lng="en">
      <trd>centiliter</trd>,
      <trd>centilitre</trd>
    </trdgrp>
    <trd lng="es">centilitro</trd>
    <trd lng="fr">centilitre</trd>
    <trd lng="hu">centiliter</trd>
    <trd lng="it">centilitro</trd>
    <trd lng="nl">centiliter</trd>
    <trd lng="pl">centylitr</trd>
    <trd lng="pt">centilitro</trd>
    <trd lng="ru">&c_s;&c_a;&c_n;&c_t;&c_i;&c_l;&c_i;&c_t;&c_r;</trd>
    <trd lng="sk">centiliter</trd>
    <trd lng="sv">centiliter</trd>
  </snc>
  <trd lng="cs">centilitr</trd>
  <trd lng="he">&#x5E1;&#x5E0;&#x5D8;&#x5D9;&#x5DC;&#x5D9;&#x5D8;&#x5E8;</trd>
  <trd lng="uk">&#x441;&#x430;&#x43D;&#x442;&#x438;&#x43B;&#x456;&#x442;&#x440;</trd>
</drv>
</art>
