<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: piastr.xml,v 1.16 2017/09/27 11:10:14 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>piastr</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="piastr.0o">
  <kap><tld/>o</kap>
  <snc mrk="piastr.0o.MON">
  <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="lst" cel="unu.monasub0o" lst="voko:monunuoj">Mona
      subunuo</ref> 
      uzata en Turkujo kaj pluraj okcidentaziaj regnoj
      kiel centono de
      <ref tip="malprt" cel="pund.0o.EKON">pundo</ref>,
      <ref tip="malprt" cel="lir1.0o">liro</ref>;
      tiomvalora <ref tip="super" cel="mon.0ero">monero</ref>:
      <ekz>
        du pundojn kaj tridek <tld/>ojn
        <fnt>
          <aut>L. N. Newell</aut>,
          <vrk><url
          ref="http://literaturo.org/HARLOW-Don/Esperanto/Literaturo/Noveloj/baksis/piulo.html">
          La &Jcirc;uro de Piulo</url></vrk>,
          <lok>en: Bak&scirc;i&scirc;, 1938</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_ja;&c_s;&c_t;&c_a;&c_r;</trd>
  <trd lng="cs">piastr</trd>
  <trd lng="de">Piaster</trd>
  <trd lng="fr">piastre</trd>
  <trd lng="hu">piaszter</trd>
  <trd lng="nl">piaster</trd>
  <trd lng="pt">piastra</trd>
  <trd lng="ru">&c_p;&c_i;&c_a;&c_s;&c_t;&c_r;</trd>
  <trd lng="sk">piaster</trd>
</drv>
</art>
