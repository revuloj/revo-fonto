<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: sofi1.xml,v 1.15 2024/10/14 19:23:53 revo Exp $">
    <kap><ofc>10</ofc>
      <rad>Sofi</rad>/o
    </kap>

<drv mrk="sofi1.0o">
  <kap><ofc>10</ofc><tld/>o</kap>
  <snc mrk="sofi1.0o.urbo">
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      La <ref tip="lst" lst="voko:&ccirc;efurboj" cel="urb.cxef0o.POL">&ccirc;efurbo</ref>
      de <ref tip="malprt" cel="bulgar.B0ujo">Bulgarujo</ref>:
      <ekz>
        post jaroj de studado en Tuniso, <tld/>o, Prago kaj Vieno li
        veturis en la jaro 1958 al Parizo
       <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/007013.php">Ekspozicio de famaj artistoj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 15</lok>
        </fnt>;
      </ekz>
      <ekz>
        la interbalkana beletra komisiono kun centra oficejo en <tld/>o
        (Bulgario)
        <fnt>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2004/008232.html">Albano
          balkane premiita</url></vrk>
           <bib>Monato</bib>
          <lok>2004/02, p. 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i prenis Ivanon en sian a&ubreve;ton kaj veturigis lin al sia lo&gcirc;ejo
        en <tld/>o
        <fnt>
          <aut>Julian Modest</aut>
          <vrk><url ref="&Monato;/2007/009024.php">Bela
          son&gcirc;o</url></vrk>
          <bib>Monato</bib>
          <lok>2007/03, p. 23</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Sofia</trd>
    <trd lng="fr">Sofia</trd>
  </snc>
  <snc mrk="sofi1.0o.provinco">
     <mlg>BG-23</mlg>
    <dif>
      Bulgara provinco &ccirc;irka&ubreve;anta la provincon <tld/>o-Urbo kaj kies administra centro
      estas en la landa &ccirc;efurbo.
    </dif>
    <trd lng="bg"> &c_S;&c_o;&c_f;&c_i;&c_j;&c_s;&c_k;&c_a; &c_o;&c_b;&c_l;&c_a;&c_s;&c_t;</trd>
    <trd lng="de">Oblast Sofia</trd>
  </snc>
  <rim>
    <ref tip="hom" cel="sofi.0o">Sofio</ref> estas anka&ubreve; virina nomo.
  </rim>
  <trd lng="be">&c_S;&c_a;&c_f;&c_ib;&c_ja; <klr>(&c_g;&c_o;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ja">&#12477;&#12501;&#12451;&#12450;</trd>
  <trd lng="nl">Sofia</trd>
  <trd lng="pl">Sofia</trd>
  <trdgrp lng="uk">
    <trd>&c_S;&c_o;&c_f;&c_ib;&c_ja; (&c_ib;&c_m;&#8217;&c_ja;)</trd>,
    <trd>&c_S;&c_o;&c_f;&c_ib;&c_ja; (&c_s;&c_t;&c_o;&c_l;&c_i;&c_c;&c_ja;)</trd>
  </trdgrp>
</drv>

<drv mrk="sofi1.0o_Urbo">
  <kap>
    <ofc>10</ofc><tld/>o-Urbo
  </kap>
  <mlg>BG&#8209;22</mlg>
  <snc>
    <dif>
      Bulgara provinco kun la sola urbo <ref tip="prt" cel="sofi1.0o.urbo">Sofio<sncref/></ref>.
    </dif>
  </snc>
  <trd lng="bg">&c_O;&c_b;&c_l;&c_a;&c_s;&c_t; &c_S;&c_o;&c_f;&c_i;&c_ja;-&c_g;&c_r;&c_a;&c_d;</trd>
  <trd lng="de">Oblast Sofia-Stadt</trd>
</drv>

  </art>

