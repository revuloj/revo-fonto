<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mering.xml,v 1.14 2022/03/23 14:01:46 revo Exp $">
<kap>
  <rad>mering</rad>/o
</kap>
<drv mrk="mering.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Malpeza <ref tip="super" cel="kuk.0o">kuko</ref> farita el
      &scirc;a&ubreve;mkirlita ovblanko kaj sukero.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_r;&c_e;&c_n;&c_g;&c_a;</trd>
  <trd lng="ca">merenga</trd>
  <trdgrp lng="cs">
    <trd>p&#x11B;nov&#xE9; pe&#x10D;ivo</trd>,
    <trd>sn&#x11B;hov&#xE1; pusinka</trd>
  </trdgrp>
  <trd lng="de">Baiser</trd>
  <trd lng="fr">meringue</trd>
  <trd lng="hu">habcs&oacute;k</trd>
  <trdgrp lng="nl">
    <trd>schuimpje</trd>,
    <trd>schuimgebakje</trd>,
    <trd>meringue</trd>
  </trdgrp>
  <trd lng="pl">beza</trd>
  <trdgrp lng="pt">
    <trd>merengue <klr tip="amb">(cul.)</klr></trd>,
    <trd>suspiro <klr tip="amb">(cul.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_r;&c_je;&c_n;&c_g;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">snehov&#xE1; pusinka</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&#x435;&#x440;&#x435;&#x43D;&#x491;&#x430;</trd>,
    <trd>&#x431;&#x435;&#x437;&#x435; (&#x441;&#x43E;&#x440;&#x442; &#x442;&#x456;&#x441;&#x442;&#x435;&#x447;&#x43A;&#x430;)</trd>
  </trdgrp>
</drv>

<drv mrk="mering.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>a pasto.
      <ekz>
        <tld/>ita citrontorto.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">cobrir amb <ind>merenga</ind></trd>
  <trd lng="cs">pot&#x159;&#xED;t sn&#x11B;hovou p&#x11B;nou</trd>
  <trd lng="fr">meringuer</trd>
  <trd lng="hu">cukros toj&aacute;shabbal von be</trd>
  <trd lng="sk">potrie&#x165; snehovou penou</trd>
</drv>
</art>
