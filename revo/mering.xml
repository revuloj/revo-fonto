<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: mering.xml,v 1.17 2024/01/25 20:10:36 revo Exp $">
<kap>
  <rad>mering</rad>/o
</kap>
<drv mrk="mering.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Malpeza <ref tip="super" cel="kuk.0o">kuko</ref> farita el
      &scirc;a&ubreve;mkirlita ovblanko kaj sukero.
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_r;&c_e;&c_n;&c_g;&c_a;</trd>
  <trd lng="ca">merenga</trd>
  <trdgrp lng="cs">
    <trd>p&#x11B;nov&#xE9; pe&#x10D;ivo</trd>,
    <trd>sn&#x11B;hov&#xE1; pusinka</trd>
  </trdgrp>
  <trd lng="de">Baiser</trd>
  <trd lng="fr">meringue</trd>
  <trd lng="hu">habcs&oacute;k</trd>
  <trd lng="ja">&#x30E1;&#x30EC;&#x30F3;&#x30B2;</trd>
  <trdgrp lng="nl">
    <trd>schuimpje</trd>,
    <trd>schuimgebakje</trd>,
    <trd>meringue</trd>
  </trdgrp>
  <trd lng="pl">beza</trd>
  <trdgrp lng="pt">
    <trd>merengue <klr tip="amb">(cul.)</klr></trd>,
    <trd>suspiro <klr tip="amb">(cul.)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_r;&c_je;&c_n;&c_g;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">snehov&#xE1; pusinka</trd>
  <trdgrp lng="uk">
    <trd>&#x43C;&c_je;&c_r;&c_je;&#x43D;&c_gu;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_je; (&c_s;&#x43E;&c_r;&c_t; &c_t;&c_ib;&c_s;&c_t;&c_je;&c_ch;&#x43A;&c_a;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4EE5;&#x53CA;&#x505A;&#x6210;&#x7684;&#x7CD5;&#x9905; <pr>y&#x1D0;j&#xED;zu&#xF2;ch&#xE9;ngdeg&#x101;ob&#x1D0;ng</pr></trd>,
    <trd>&#x4EE5;&#x53CA;&#x505A;&#x6210;&#x7684;&#x7CD5;&#x997C; <pr>y&#x1D0;j&#xED;zu&#xF2;ch&#xE9;ngdeg&#x101;ob&#x1D0;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mering.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>a pasto.
      <ekz>
        <tld/>ita citrontorto.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_k;&c_r;&c_y;&c_v;&c_a;&c_c;&c_mol; <ind>&c_m;&c_e;&c_r;&c_e;&c_n;&c_g;&c_a;&c_j;</ind></trd>
  <trd lng="ca">cobrir amb <ind>merenga</ind></trd>
  <trd lng="cs">pot&rcaron;&#xED;t sn&#x11B;hovou p&#x11B;nou</trd>
  <trd lng="fr">meringuer</trd>
  <trd lng="hu">cukros toj&aacute;shabbal von be</trd>
  <trd lng="ja">&#x30E1;&#x30EC;&#x30F3;&#x30B2;&#x3092;&#x306E;&#x305B;&#x308B;</trd>
  <trd lng="sk">potrie&tcaron; snehovou penou</trd>
</drv>
</art>
