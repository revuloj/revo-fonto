<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>mering</rad>/o
</kap>
<drv mrk="mering.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Malpeza <ref tip="super" cel="kuk.0o">kuko</ref> farita el
      &scirc;a&ubreve;mkirlita ovblanko kaj sukero.
    </dif>
  </snc>
  <trd lng="de">Baiser</trd>
  <trd lng="fr">meringue</trd>
  <trdgrp lng="nl">
    <trd>schuimpje</trd>,
    <trd>schuimgebakje</trd>,
    <trd>meringue</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_je;&c_r;&c_je;&c_n;&c_g;&c_a;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="mering.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      &Scirc;miri per <tld/>a pasto.
      <ekz>
        <tld/>ita citrontorto.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">meringuer</trd>
</drv>
</art>
