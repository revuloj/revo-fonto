<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sven.xml,v 1.37 2026/01/10 08:38:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>sven</rad>/i
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="sven.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="sven.0i.PSI">
    <dif>
      Perdi dum kelkaj momentoj la konscion kaj sinmovon:
      <ekz>
        &ccirc;iu barelo malbonodoras tiel, ke la homoj <tld/>as de tiu odoro
        <fnt>
          <bib>Fab4</bib>
          <lok>Knabino, kiu pa&scirc;is sur panon</lok>
        </fnt>;
      </ekz>
      <ekz>
        la timo simile al malseka mano ku&scirc;i&gcirc;is sur &scirc;ia koro, tiel ke &scirc;i preska&ubreve; <tld/>is
        <fnt>
          <bib>Fab4</bib>
          <lok>Anneto</lok>
        </fnt>;
      </ekz>
      <ekz>
        li falis <tld/>inta sur la plankon
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fidela Johano</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsato kaj soifo turmentis la knabon, li estis malsana kaj preska&ubreve; <tld/>inta
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        homoj kaj bestoj <tld/>i&gcirc;adis de la varmego
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IV</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i <tld/>as, &ccirc;ar &scirc;i vidas sangon
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi en miaj brakoj <tld/>is
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la koro <tld/>as, la genuoj tremas
        <fnt>
          <bib>MT</bib>
          <lok>Na&hcirc;um 2:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        li falas <tld/>e
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_z;&c_mol;&c_n;&c_je;&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_a;&c_m;&c_l;&c_je;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_t;&c_r;&c_a;&c_c;&c_ib;&c_c;&c_mol;
      <ind>&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</ind></trd>
    </trdgrp>
    <trdgrp lng="bg">
      <trd>&c_g;&c_u;&c_b;&c_ja; &c_s;&c_malmol;&c_z;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_p;&c_a;&c_d;&c_a;&c_m;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>ohnm&auml;chtig werden</trd>,
      <trd>in <ind>Ohnmacht</ind> fallen</trd>,
      <trd><ind>bewusstlos</ind> werden</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>desvanecerse</trd>,
      <trd>perder el <ind>conocimiento</ind></trd>,
      <trd>desmayarse</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>s'&eacute;vanouir</trd>,
      <trd>perdre <ind>connaissance</ind></trd>
    </trdgrp>
    <trd lng="hu">el&aacute;jul</trd>
    <trdgrp lng="it">
      <trd>svenire</trd>,
      <trd>perdere i <ind>sensi</ind></trd>,
      <trd>perdere <ind>conoscienza</ind></trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>bezwijmen</trd>,
      <trd>flauwvallen</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_ja;&c_t;&c_mol;
      <ind>&c_s;&c_o;&c_z;&c_n;&c_a;&c_n;&c_i;&c_je;</ind></trd>,
      <trd>&c_u;&c_p;&c_a;&c_s;&c_t;&c_mol; &c_v;
      <ind>&c_o;&c_b;&c_m;&c_o;&c_r;&c_o;&c_k;</ind></trd>
    </trdgrp>
    <trd lng="sv">svimma</trd>
    <trdgrp lng="zh">
      <trd>&#x53A5; <pr>ju&eacute;</pr></trd>,
      <trd>&#x6655; <pr>y&umacron;n</pr></trd>,
      <trd>&#x6688; <pr>y&umacron;n</pr></trd>
    </trdgrp>
  </snc>
  <snc mrk="sven.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="dif" cel="sven.for0i.KOMUNE">For<tld/>i</ref>
      <ekz>
        sonis muziko &ccirc;iam <klr>[&#x2026;]</klr> pli malla&ubreve;ta, pli pura, pli bela, &gcirc;is ie, sur
        la sojlo &ccirc;iela, &gcirc;i <tld/>is
        <fnt>
          <bib>SrT</bib>
          <lok>Libro IV.</lok>
        </fnt>!
      </ekz>
      <ekz>
        kion valoras ri&ccirc;o, se sano <tld/>as for
        <fnt>
          <bib>ChR</bib>
          <lok>Veli morten</lok>
        </fnt>?
      </ekz>
    </dif>
    <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_u;&c_c;&c_mol;</trd>
    <trd lng="bg">&c_i;&c_z;&c_ch;&c_je;&c_z;&c_v;&c_a;&c_m;</trd>
    <trdgrp lng="de">
      <trd>verschwinden</trd>,
      <trd>sich <ind>verfl&uuml;chtigen</ind></trd>,
      <trd>sich <ind>verlieren</ind></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>desaparecer</trd>,
      <trd>desvanecerse<klr> (desaparecer)</klr></trd>,
    </trdgrp>
    <trdgrp lng="fr">
      <trd>disparaitre</trd>,
      <trd>s'&eacute;vanouir<klr> (disparaitre)</klr></trd>,
    </trdgrp>
    <trdgrp lng="hu">
      <trd>elt&udblac;nik</trd>,
      <trd>elsz&aacute;ll</trd>,
      <trd>elhal</trd>,
      <trd>megsemmis&uuml;l</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>svanire</trd>,
      <trd>sfumare<klr> (svanire)</klr></trd>,
      <trd>languire<klr> (fig.)</klr></trd>,
    </trdgrp>
    <trd lng="nl">verdwijnen</trd>
    <trdgrp lng="ru">
      <trd>&c_i;&c_s;&c_ch;&c_je;&c_z;&c_n;&c_u;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_o;&c_p;&c_a;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_r;&c_a;&c_s;&c_s;&c_je;&c_ja;&c_t;&c_mol;&c_s;&c_ja;</trd>,
      <trd>&c_z;&c_a;&c_m;&c_je;&c_r;&c_je;&c_t;&c_mol; <klr
      tip="amb">(&c_o; &c_z;&c_v;&c_u;&c_k;&c_je;)</klr></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="bg">
    <trd>&c_g;&c_u;&c_b;&c_ja; &c_s;&c_malmol;&c_z;&c_n;&c_a;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_p;&c_a;&c_d;&c_a;&c_m;</trd>
  </trdgrp>
  <trd lng="cs">omdl&iacute;t</trd>
  <trd lng="en">faint</trd>
  <trdgrp lng="ja">
    <trd>&#x5931;&#x795E;&#x3059;&#x308B;  <pr>&#x3057;&#x3063;&#x3057;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6C17;&#x7D76;&#x3059;&#x308B;  <pr>&#x304D;&#x305C;&#x3064;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x6C17;&#x3092;&#x5931;&#x3046;  <pr>&#x304D;&#x3092;&#x3046;&#x3057;&#x306A;&#x3046;</pr></trd>,
    <trd>&#x6D88;&#x3048;&#x3046;&#x305B;&#x308B;  <pr>&#x304D;&#x3048;&#x3046;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>gin&aogonek;&cacute;</trd>,
    <trd>mdle&cacute;</trd>,
    <trd>zas&lstroke;abn&aogonek;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>omdlie&tcaron;</trd>,
    <trd>upadn&uacute;&tcaron; do mdl&ocirc;b</trd>,
    <trd>zamdlie&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_l;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_o;&c_m;&c_l;&c_ib;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_t;&c_o;&c_m;&c_n;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_l;&c_a;&c_b;&c_sh;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_v;&c_m;&c_i;&c_r;&c_a;&c_t;&c_i; (&c_p;&c_r;&c_o; &c_z;&c_v;&c_u;&c_k;)</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_n;. &c_z;&c_n;&c_i;&c_k;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_shch;&c_je;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_s;&c_ib;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_h;&c_o;&c_d;&c_i;&c_t;&c_i;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="sven.0o">
  <kap><tld/>o</kap>
  <snc mrk="sven.0o.PSI">
    <dif>
      Momenta senkonscii&gcirc;o;
      <ref tip="sin" cel="sinkop.0o.sveno">sinkopo<sncref/></ref>:
      <ekz>
        &gcirc;i sentis doloron, <tld/>on, &gcirc;i tute ne povis pensi pri ia feli&ccirc;o
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        li veki&gcirc;is el profunda <tld/>o
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        apena&ubreve; &scirc;i eniris en la malvarman akvon, &scirc;in subite atakis <tld/>o
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_n;&c_je;&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
    <trdgrp lng="de">
      <trd>Ohnmacht</trd>,
      <trd>Bewusstlosigkeit</trd>,
      <trd>Kreislaufkollaps</trd>,
      <trd>Synkope</trd>,
      <trd>Blackout</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>desvanecimiento</trd>
      <trd>s&iacute;ncope</trd>,
      <trd>desmayo</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>&eacute;vanouissement</trd>
      <trd>syncope</trd>
    </trdgrp>
    <trd lng="hu">&aacute;jul&aacute;s</trd>
    <trd lng="it">svenimento</trd>
    <trdgrp lng="nl">
      <trd>flauwte</trd>,
      <trd>syncope</trd>,
      <trd>flauwvallen<klr tip="ind"> (zn.)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_m;&c_o;&c_r;&c_o;&c_k;</trd>,
      <trd>&c_b;&c_je;&c_s;&c_p;&c_a;&c_m;&c_ja;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>,
      <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_ja;
      &c_s;&c_o;&c_z;&c_n;&c_a;&c_n;&c_i;&c_ja;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x7729;&#x6655; <pr>xu&agrave;ny&ugrave;n</pr></trd>,
    <trd>&#x7729;&#x6688; <pr>xu&agrave;ny&ugrave;n</pr></trd>,
    <trd>&#x660F;&#x5012; <pr>h&umacron;nd&#x1CE;o</pr></trd>,
    <trd>&#x6655;&#x53A5; <pr>y&umacron;nju&eacute;</pr></trd>,
    <trd>&#x6688;&#x53A5; <pr>y&umacron;nju&eacute;</pr></trd>,
    <trd>&#x6655;&#x5012; <pr>y&umacron;nd&#x1CE;o</pr></trd>,
    <trd>&#x6688;&#x5012; <pr>y&umacron;nd&#x1CE;o</pr></trd>,
    <trd>&#x5931;&#x53BB;&#x77E5;&#x89BA; <pr>sh&imacron;q&ugrave;zh&imacron;ju&eacute;</pr></trd>,
    <trd>&#x5931;&#x53BB;&#x77E5;&#x89C9; <pr>sh&imacron;q&ugrave;zh&imacron;ju&eacute;</pr></trd>,
    <trd>&#x660F;&#x53A5; <pr>h&umacron;nju&eacute;</pr></trd>,
    <trd>&#x4E2D;&#x7565; <pr>zh&omacron;ngl&uuml;&egrave;</pr></trd>,
    <trd>&#x6655; <pr>y&umacron;n</pr></trd>,
    <trd>&#x6688; <pr>y&umacron;n</pr></trd>
  </trdgrp>
</snc>
<!-- ne trovis tian uzon...
    <uzo tip="stl">FIG</uzo>
    <trd lng="be">&c_z;&c_mol;&c_n;&c_ib;&c_k;&c_n;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="es">desaparici&oacute;n</trd>
    <trd lng="fr">disparition</trd>
    <trdgrp lng="hu">
      <trd>elt&udblac;n&eacute;s</trd>,
      <trd>elhal&aacute;s</trd>,
      <trd>megsemmis&uuml;l&eacute;s</trd>
    </trdgrp>
    <trd lng="nl">het in <ind>rook</ind> opgaan</trd>
    <trd lng="ru">&c_i;&c_s;&c_ch;&c_je;&c_z;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
-->
  <trdgrp lng="cs">
    <trd>bezv&ecaron;dom&iacute;</trd>,
    <trd>mdloba</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5931;&#x795E;  <pr>&#x3057;&#x3063;&#x3057;&#x3093;</pr></trd>,
    <trd>&#x6C17;&#x7D76;  <pr>&#x304D;&#x305C;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="pl">omdlenie</trd>
  <trdgrp lng="sk">
    <trd>mdloba</trd>,
    <trd>zamdlenie</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_n;&c_je;&c_p;&c_r;&c_i;&c_t;&c_o;&c_m;&c_n;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_b;&c_je;&c_z;&c_p;&c_a;&c_m;&#x2019;&c_ja;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_u;&c_t;&c_r;&c_a;&c_t;&c_a; &c_s;&c_v;&c_ib;&c_d;&c_o;&c_m;&c_o;&c_s;&c_t;&c_ib;</trd>
  </trdgrp>
</drv>

<drv mrk="sven.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="sven.0ema.PSI">
    <dif>
      Preska&ubreve; <tld/>anta, sentanta grandegan malforti&gcirc;on:
      <ekz>
        li sentis sin <tld/>ema pro lacego post la
        dumnokta viglado
        <fnt><aut>J. Francis</aut>,
        <vrk>La Granda Kaldrono</vrk>,
        <lok>1978</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>zu <ind>Ohnmacht</ind> neigend</trd>,
      <trd>zur <ind>Bewusstlosigkeit</ind> neigend</trd>,
      <trd>schw&auml;chlich <klr>(Kreislauf, Ohnmacht)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_y; &c_d;&c_a; &c_s;&c_t;&c_r;&c_a;&c_t;&c_a;&c_w; <ind>&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  <trd lng="fr">d&eacute;faillant<klr> (se sentant mal)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x5352;&#x5012;&#x3057;&#x3084;&#x3059;&#x3044;  <pr>&#x305D;&#x3063;&#x3068;&#x3046;&#x3057;&#x3084;&#x3059;&#x3044;</pr></trd>,
    <trd>&#x6D88;&#x3048;&#x3084;&#x3059;&#x3044;  <pr>&#x304D;&#x3048;&#x3084;&#x3059;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="pl">p&oacute;&lstroke;omdla&lstroke;y</trd>
</drv>

<drv mrk="sven.0emo">
  <kap><tld/>emo</kap>
  <snc mrk="sven.0emo.PSI">
    <dif>
      Tia sento de korpa malforteco, mensa nebuleco, kia
      anta&ubreve;as <tld/>on; inklino al <tld/>oj:
      <ekz>
        spitante sufokan <tld/>emon li iris al la pordo
        <fnt>
          <aut>J. I. Francis</aut>,
          <vrk><url ref="http://www.autodidactproject.org/other/francis_deka.html">
          Okazos je la Deka</url></vrk>,
          <lok>en: Vitralo, 1960</lok>
        </fnt>
      </ekz>
      <ekz>
        difterio &dash; unua semajno: &gcirc;eneralaj simptomoj (febro, paleco, <tld/>emo), mukozaj simptomoj
        (griza, sanga)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=40782">Difterio</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>emo post tro longa varmega banado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=205106">Onsen</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Schw&auml;cheanfall</trd>,
      <trd>Kreislaufschw&auml;che</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_s;&c_h;&c_ib;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol; &c_d;&c_a; &c_s;&c_t;&c_r;&c_a;&c_t;&c_a;&c_w; <ind>&c_p;&c_r;&c_y;&c_t;&c_o;&c_m;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_ib;</ind></trd>
  <trd lng="es">debilidad<klr> (tendencia al
  desvanecimiento)</klr></trd>
  <trd lng="fr">faiblesse<klr> (tendance &agrave;
  l'&eacute;vanouissement)</klr></trd>
  <trd lng="pl">p&oacute;&lstroke;omdlenie</trd>
</drv>

<drv mrk="sven.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="sven.0igi.PSI">
    <dif>
      Ka&ubreve;zi <tld/>on al:
      <ekz>
        duone <tld/>igita per la bato, la kamparano perdis la kura&gcirc;on
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        Bastjano restis ku&scirc;anta, <tld/>igita de la falimpeto
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Urbo de Imperiestroj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bat.sven0i.KOMUNE">svenbati</ref>
    </refgrp>
