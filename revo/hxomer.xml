<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: hxomer.xml,v 1.2 2022/03/20 17:24:56 revo Exp $">
    <kap>
      <rad>&hcirc;omer</rad>/o
    </kap>

<drv mrk="hxomer.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BIB</uzo>
  <snc>
    <dif>
      Antikva <ref tip="lst" lst="voko:kvantoj" cel="unu.mezur0o">mezurunuo</ref>
      por &scirc;uteblaj aferoj, dek efoj, &ccirc; 220 &ndash; 360 litroj:
      <ekz>
        bat'o estas dekono de kor'o, dek bat'oj faras <tld/>on, &ccirc;ar <tld/>o entenas dek bat'ojn
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 45:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kvanto de <tld/>o da hordeo
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 27:16</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi akiris &scirc;in al mi per dek kvin ar&gcirc;entaj moneroj kaj <tld/>o kaj duono da hordeo
        <fnt>
          <bib>MT</bib>
          <lok>Ho&scirc;ea 3:2</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">H&oacute;mer</trd>
  </snc>
  <trd lng="uk">&#x445;&#x43E;&#x43C;&#x435;&#x440;</trd>
</drv>

  </art>

<!--
$Log$
-->
</vortaro>
