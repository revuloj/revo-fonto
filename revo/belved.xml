<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: belved.xml,v 1.21 2025/10/28 16:39:36 revo Exp $">
<kap>
  <rad>belveder</rad>/o
  <fnt><bib>V</bib></fnt>
</kap>
<drv mrk="belved.0o">
  <kap><tld/>o</kap>
  <snc mrk="belved.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      <ref tip="dif" cel="vid.bel0ejo">Belvidejo</ref>:
      <ekz>
        <klr>[li]</klr> lasis sian a&ubreve;ton tie por piediri al la rafunja <tld/>o
        <fnt>
          <bib>ChV</bib>
          <lok>5</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="B"/>a turo de <nom>Goethe</nom> trovi&gcirc;as plej alte ol &ccirc;iuj karlovaraj turoj
        super la urbo, sur la alta&jcirc;o &leftquot;Eterna vivo&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=534">Belvedera turo de Goethe</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        statuo de Apolono en la <tld lit="B"/>a salono de Vatikano
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#x2116;10</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="altan.0o">altano</ref>,
      <ref cel="balkon.0o">balkono</ref>
    </refgrp>
    <trd lng="be">&c_b;&c_e;&c_l;&c_mol;&c_v;&c_e;&c_d;&c_e;&c_r;</trd>
    <trdgrp lng="de">
      <trd>Aussichtsplattform</trd>,
      <trd>Aussichtsterrasse</trd>,
      <trd>Aussichtspunkt</trd>,
      <trd>Belvedere</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>viewing platform</trd>,
      <trd>scenic overlook</trd>
    </trdgrp>
    <trd lng="fr">belv&eacute;d&egrave;re</trd>
    <trdgrp lng="hu">
      <trd>kil&aacute;t&oacute;</trd>,
      <trd>belvedere</trd>
    </trdgrp>
    <trd lng="pl">belweder</trd>
    <trd lng="pt">mirante</trd>
    <trd lng="ru">&c_b;&c_je;&c_l;&c_mol;&c_v;&c_je;&c_d;&c_je;&c_r;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>belved&eacute;r</trd>,
    <trd>letohr&aacute;dek</trd>
  </trdgrp>
  <trd lng="he">belvedere</trd>
  <trd lng="ja">&#x5C55;&#x671B;&#x53F0;  <pr>&#x3066;&#x3093;&#x307C;&#x3046;&#x3060;&#x3044;</pr></trd>
  <trdgrp lng="sk">
    <trd>letohr&aacute;dok</trd>,
    <trd>rozh&lcaron;ad&ncaron;a</trd>
  </trdgrp>
  <trd lng="uk">&c_b;&c_je;&c_l;&c_mol;&c_v;&c_je;&c_d;&c_je;&c_r;</trd>
</drv>
</art>
