<art mrk="$Id: kataluni.xml,v 1.9 2024/05/01 21:21:30 revo Exp $">
<kap>
  <rad>Kataluni</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="kataluni.A0o">
  <kap><tld/>o</kap>
  <snc mrk="kataluni.A0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="dif" cel="katalun.K0ujo">Katalunujo</ref>:
      <ekz>
        la Ebro-delto, la sola kampara regiono en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Monato;/2003/008144.html">Surprizaj rezultoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ETA anoncis paf&ccirc;esigon en <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2004/008300.html">Kronologio de kanajla mistifikado</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        oni kalkulas, ke nuntempe en <tld/>o ekzistas pli ol 300 parolataj lingvoj, el kiuj jam 150
        estas identigitaj
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2004/008346.html">Lingva bunto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="af">Kataloni&euml;</trd>
    <trd lng="be">&c_K;&c_a;&c_t;&c_a;&c_l;&c_jo;&c_n;&c_ib;&c_ja;</trd>
    <trd lng="ca">Catalunya</trd>
    <trd lng="de">Katalonien</trd>
    <trd lng="en">Catalonia</trd>
    <trd lng="es">Catalu&ntilde;a</trd>
    <trd lng="eu">Katalunia</trd>
    <trd lng="fa">&f_k;&a_A;&a_t;&a_A;&a_l;&a_w;&a_n;&f_y;&a_A;</trd>
    <trd lng="fr">Catalogne</trd>
    <trd lng="fy">Kataloanje</trd>
    <trd lng="gl">Catalu&ntilde;a</trd>
    <trd lng="io">Katalunia</trd>
    <trd lng="it">Catalogna</trd>
    <trd lng="ko">&#xCE74;&#xD0C8;&#xB8E8;&#xB0D0; &#xC9C0;&#xBC29;</trd>
    <trd lng="nl">Cataloni&euml;</trd>
    <trd lng="no">Catalonia</trd>
    <trd lng="oc">Catalonha</trd>
    <trd lng="pl">Katalonia</trd>
    <trd lng="pt">Catalunha</trd>
    <trd lng="ru">&c_K;&c_a;&c_t;&c_a;&c_l;&c_o;&c_n;&c_i;&c_ja;</trd>
    <trd lng="sk">Katal&aacute;nsko</trd>
    <trd lng="sv">Katalonien</trd>
    <trd lng="tok">ma Katala</trd>
    <trd lng="tr">Katalonya</trd>
    <trd lng="vo">Katalon&auml;n</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x30AB;&#x30BF;&#x30ED;&#x30CB;&#x30A2;</trd>,
    <trd>&#x30AB;&#x30BF;&#x30EB;&#x30FC;&#x30CB;&#x30E3;</trd>
  </trdgrp>
  <trd lng="uk">&#x41A;&c_a;&c_t;&c_a;&#x43B;&#x43E;&#x43D;&c_ib;&#x44F;</trd>
  <trd lng="zh">&#x52A0;&#x6CF0;&#x7F57;&#x5C3C;&#x4E9A;
  <pr>Ji&amacron;t&#xE0;ilu&#xF3;n&#xED;y&#xE0;</pr></trd>
</drv>
