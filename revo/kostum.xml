<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kostum.xml,v 1.36 2021/06/07 04:31:58 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kostum</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kostum.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kostum.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Maniero sin vesti; vesto propra al iu nacio, klaso, tempo, ofico,
      cirkonstanco:
      <ekz>
        la <tld/>o de l' antikvaj Romianoj;
      </ekz>
      <ekz>
        klerika, militista, ceremonia, negli&gcirc;a <tld/>o;
      </ekz>
      <ekz>
        la kongresanoj dancis en naciaj <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tualet.0o.TEKS">tualeto</ref>,
      <ref cel="komple2.vesto0o.TEKS">vestokompleto</ref>
    </refgrp>
  </snc>
  <trd lng="af">pak</trd>
  <trd lng="am">&#x120D;&#x1265;&#x1235;</trd>
  <trd lng="ar">&#x62F;&#x639;&#x648;&#x649;</trd>
  <trd lng="az">kostyum</trd>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ju;&c_m;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="bn">&#x9AE;&#x9BE;&#x9AE;&#x9B2;&#x9BE;</trd>
  <trdgrp lng="cs">
    <trd>kost&yacute;m</trd>,
    <trd>kroj</trd>,
    <trd>&uacute;bor</trd>
  </trdgrp>
  <trd lng="da">kul&#xF8;r</trd>
  <trdgrp lng="de">
    <trd>Kleidung</trd>,
    <trd>Bekleidung</trd>,
    <trd>Tracht</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>costume</trd>,
    <trd><klr>style of </klr> dress</trd>,
    <trd>garb</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>costume</trd>,
    <trd>mise<klr> (costume)</klr></trd>
  </trdgrp>
  <trd lng="gu">&#xAA6;&#xABE;&#xAB5;&#xACB;</trd>
  <trd lng="hi">&#x938;&#x942;&#x91F;</trd>
  <trdgrp lng="hu">
    <trd>viselet</trd>,
    <trd>n&eacute;pviselet</trd>,
    <trd>koszt&uuml;m</trd>
  </trdgrp>
  <trd lng="hy">&#x56F;&#x578;&#x57D;&#x57F;&#x575;&#x578;&#x582;&#x574;</trd>
  <trd lng="mr">&#x916;&#x91F;&#x932;&#x93E;</trd>
  <trd lng="my">&#x101D;&#x1010;&#x103A;&#x1005;&#x102F;&#x1036;</trd>
  <trd lng="pa">&#xA2E;&#xA41;&#xA15;&#xA71;&#xA26;&#xA2E;&#xA47;</trd>
  <trdgrp lng="pl">
    <trd>kostium</trd>,
    <trd>str&oacute;j</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>traje t&iacute;pico</trd>,
    <trd>veste</trd>,    
    <trd>costume <klr tip="amb">(vest.)</klr></trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_ju;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>kost&yacute;m</trd>,
    <trd>kroj</trd>,
    <trd>&uacute;bor</trd>
  </trdgrp>
  <trd lng="sq">kostum</trd>
  <trd lng="te">&#xC26;&#xC3E;&#xC35;&#xC3E;</trd>
  <trd lng="ur">&#x633;&#x648;&#x679;</trd>
  <trd lng="vi">ph&#xF9; h&#x1EE3;p v&#x1EDB;i</trd>
</drv>

<drv mrk="kostum.ban0o">
  <kap>ban<tld/>o</kap>
  <snc mrk="kostum.ban0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Vesto a&ubreve; vestaro portata dum banado, na&gcirc;ado:
      <ekz>
        &scirc;i preferas unupecajn ban<tld/>ojn
        <fnt>
          <aut>-</aut>,
          <lok><url ref="http://eo.lernu.net/">lernu.net</url>,
          2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">plavky</trd>
  <trdgrp lng="fr">
    <trd>costume de bain</trd>,
    <trd>maillot de bain</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>str&oacute;j k&aogonek;pielowy</trd>,
    <trd>k&aogonek;piel&oacute;wki</trd>
  </trdgrp>
  <trd lng="sk">plavky</trd>
</drv>
</art>
