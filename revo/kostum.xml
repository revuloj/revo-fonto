<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kostum</rad>/o
</kap>
<drv mrk="kostum.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Maniero sin vesti; vesto propra al iu nacio, klaso, tempo, ofico,
      cirkonstanco:
      <ekz>
        la <tld/>o de l' antikvaj Romanoj;
      </ekz>
      <ekz>
        klerika, militista, ceremonia, negli&gcirc;a <tld/>o;
      </ekz>
      <ekz>
        la kongresanoj dancis en naciaj <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">kroj</trd>
  <trdgrp lng="en">
    <trd>costume</trd>,
    <trd><klr>style of </klr> dress</trd>,
    <trd>garb</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_s;&c_t;&c_ju;&c_m;</trd>
</drv>
</art>
