<art mrk="$Id: kostum.xml,v 1.105 2024/08/29 06:37:07 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kostum</rad>/o <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="kostum.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kostum.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.0o">Vesto</ref> propra al iu nacio, klaso, tempo, ofico,
      cirkonstanco:
      <ekz>
        funebra kostumo
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deksepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        flegistina <tld/>o
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        li donis al mi <tld/>on de novico
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XV</lok>
        </fnt>;
      </ekz>
      <ekz>
        li surmetas pastran <tld/>on kaj vizitas templojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XX</lok>
        </fnt>;
      </ekz>
      <ekz>
        la bankiero amis la militistajn <tld/>ojn
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXV</lok>
        </fnt>;
      </ekz>
      <ekz>
        bela junulo en <tld/>o de voja&gcirc;o
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de pilgrimanto
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili vidis la vila&gcirc;anojn en la naciaj <tld/>oj
        <fnt>
          <bib>KKl</bib>
          <lok>&Ccirc;apitro 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        li distingis altkreskan viron en &ccirc;as<tld/>o kaj kun altaj botoj
        <fnt>
          <bib>HsT</bib>
          <lok>Feli&ccirc;a homo</lok>
        </fnt>;
      </ekz>
      <ekz>
        fortika viro en eleganta <tld/>o
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de la tempoj de la praavoj, kun pudritaj haroj kaj rigida tola jupo
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova domo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la filoj havis nudan kolon kaj skotan <tld/>on, &ccirc;ar ili estis sportistoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        maljuna afabla virino en stranga <tld/>o, kiu estis verda kiel la folioj de la
        sambuka arbo kaj havis garna&jcirc;on el grandaj blankaj sambukaj flora&jcirc;oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        vestitaj per <ind>ski<tld/>o</ind>j, foje kun peltaj &ccirc;apeloj, la ski-amantoj &leftquot;atakis&rightquot;
        kelkcentmetran distancon
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <lok><url
          ref="http://steloj.de/esperanto/monato2/007067.html">http://steloj.de/esperanto/monato2/007067.html</url></lok>
        </fnt>;
        <trd lng="de">Skianug</trd>
      </ekz>
      <ekz>
        e&ccirc; dum varmegaj someraj tagoj &scirc;oforoj ne povos porti somer&ccirc;emizojn, mallongajn
        pantalonojn a&ubreve; sport<tld/>ojn
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="https://www.monato.be/2004/008290.html">Kondukistoj bonkondutaj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        tre maldensa estas la atmosfero je granda alteco <klr>[&#8230;]</klr> pro tio
        in&gcirc;enieroj kreis novan <ind>&scirc;irm<tld/>o</ind>n
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="https://www.monato.be/2013/010897.php">Salto el stratosfero</url></vrk>
          <lok>2013</lok>
        </fnt>;
        <trd lng="de">Schutzanzug</trd>
      </ekz>
      <ekz>
        la glacirompilo haltas en la maro por ellasi kura&gcirc;emulojn, vestitajn per
        akvoimunaj varmo<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2007/009120.php">Krozi en glacio</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        li portis elegantan matenan <tld/>on la&ubreve; la angla gusto
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>V</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tualet.0o.TEKS">tualeto</ref>,
      <ref cel="ornat.0o">ornato</ref>,
      <ref cel="unifor.0o">uniformo</ref>,
      <ref cel="komple2.vesto0o.TEKS">vestokompleto</ref>
    </refgrp>
  </snc>
