<drv mrk="kostum.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      Surmeti <tld/>on:
      <ekz>
        Sinjoro Dik por maskofest' &#11005; Sin &scirc;erce kiel kok' <tld/>is
        <fnt>
          <bib>LSB</bib>
          <lok>Miskompreno</lok>
        </fnt>;
      </ekz>
      <ekz>
        bele <tld/>ita hundino
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;11</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="tualet.0i">tualeti</ref>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_r;&c_a;&c_n;&c_a;&c_c;&c_mol; <ind>&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ju;&c_m;</ind></trd>
  <trdgrp lng="de">
    <trd>kost&uuml;mieren</trd>,
    <trd>ausstaffieren</trd>,
    <trd>herausputzen</trd>
  </trdgrp>
  <trd lng="ja">&#34915;&#x88C5;&#12434;&#30528;&#x305B;&#x308B;  <pr>&#12356;&#12375;&#12423;&#12358;&#12434;&#x304D;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="zh">
    <trd>&#x636F;&#x98ED; <pr>d&#xE1;och&#xEC;</pr></trd>,
    <trd>&#x4E54;&#x88C5;&#25171;&#x626E; <pr>qi&#xE1;ozhu&amacron;ngd&#x1CE;b&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kostum.ban0o">
  <kap>ban<tld/>o, <var><kap>na&gcirc;<tld/>o</kap></var></kap>
  <snc mrk="kostum.ban0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="vest.0o">Vesto</ref> a&ubreve; vestaro portata dum banado, na&gcirc;ado:
      <ekz>
        mi vidis ilin &ccirc;e l' marbordo en ban<tld/>o
        <fnt>
          <bib>LSB</bib>
          <lok>Homoj &Ccirc;e L' Marbordo</lok>
        </fnt>;
      </ekz>
      <ekz>
        junulino en na&gcirc;<tld/>o
        <fnt>
          <bib>KFl</bib>
          <lok>VIII. &Scirc;tupoj al idealo</lok>
        </fnt>;
      </ekz>
      <ekz>
        same agas <nom>Berta</nom> kun sia robo, sub kiu &scirc;i havas ban<tld/>on
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i preferas unupecajn ban<tld/>ojn
        <fnt>
          <aut>-</aut>,
          <lok><url ref="http://eo.lernu.net/">lernu.net</url>,
          2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k; <klr tip="amb">(&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ju;&c_m;)</klr></trd>
  <trd lng="cs">plavky</trd>
  <trd lng="de">Badebekleidung</trd>
  <trdgrp lng="fr">
    <trd>costume de bain</trd>,
    <trd>maillot de bain</trd>
  </trdgrp>
  <trd lng="ja">&#x6C34;&#30528;  <pr>&#x307F;&#x305A;&#x304E;</pr></trd>
  <trdgrp lng="pl">
    <trd>str&oacute;j k&aogonek;pielowy</trd>,
    <trd>k&aogonek;piel&oacute;wki</trd>
  </trdgrp>
  <trd lng="sk">plavky</trd>
  <trd lng="uk">&c_k;&c_u;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#x6CF3;&#34915; <pr>y&#x1D2;ngy&#x12B;</pr></trd>,
    <trd>&#x6CF3;&#x88C5; <pr>y&#x1D2;ngzhu&amacron;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kostum.nokt0o">
  <kap>
    nokt<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>o, kiun oni portas por dormado dum la nokto:
      <ekz>
        vestita de eleganta nokt<tld/>o, la grafino sidis apud brila fajro
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="vest.nokto0o">noktovesto</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="neglig.0o">negli&gcirc;o</ref>,
      <ref cel="pijxam.0o">pi&jcirc;amo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_zh;&c_a;&c_m;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Schlafanzug</trd>,
    <trd>Nachtgewand</trd>,
    <trd>Pyjama</trd>
  </trdgrp>
  <trd lng="en">pajamas</trd>
  <trd lng="fr">pyjamas</trd>
  <trdgrp lng="zh">
    <trd>&#30561;&#34915; <pr>shu&#xEC;y&#x12B;</pr></trd>,
    <trd>&#30561;&#34915;&#x88E4; <pr>shu&#xEC;y&#x12B;k&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kostum.popol0o">
  <kap>
    popol<tld/>o, <var><kap>naci<tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>o tradicia &ccirc;e iu popolo:
      <ekz>
        ili prezentis precipe dancojn kaj kantojn en bunta vario de popol<tld/>oj sur
        stratoj kaj placoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="https://www.monato.be/2003/007945.html">Bukedo de popolarto</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur slovakaj popol<tld/>oj oni povas vidi <klr>[&#8230;]</klr> broda&jcirc;ojn, trika&jcirc;ojn
        kaj kro&ccirc;eta&jcirc;ojn, kies ornama&jcirc;oj estis transprenitaj el renesancaj modeloj
        <fnt>
          <bib>Monato</bib>
          <aut>Stano Mar&ccaron;ek</aut>
          <vrk><url ref="https://www.monato.be/2010/009986.php">Popolajn kostumojn portas
          pupoj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        grupoj en lokaj naci-<tld/>oj el diversaj vila&gcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="https://www.monato.be/2009/009822.php">Katastrofa re&gcirc;inotago</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y; <ind>&c_s;&c_t;&c_r;&c_o;&c_j;</ind></trd>,
    <trd>&c_t;&c_r;&c_a;&c_d;&c_y;&c_c;&c_y;&c_j;&c_n;&c_y; <ind>&c_s;&c_t;&c_r;&c_o;&c_j;</ind></trd>
  </trdgrp>
  <trd lng="de">Volkstracht</trd>
  <trd lng="ja">&#x6C11;&#x4FD7;&#34915;&#x88C5;  <pr>&#x307F;&#12435;&#x305E;&#x304F;&#12356;&#12375;&#12423;&#12358;</pr></trd>
</drv>

