</drv>

<drv mrk="kostum.nokt0o">
  <kap>
    nokt<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>o, kiun oni portas por dormado dum la nokto:
      <ekz>
        vestita de eleganta nokt<tld/>o, la grafino sidis apud brila fajro
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekkvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="vest.nokto0o">noktovesto</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="neglig.0o">negli&gcirc;o</ref>,
      <ref cel="pijxam.0o">pi&jcirc;amo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_zh;&c_a;&c_m;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Schlafanzug</trd>,
    <trd>Nachtgewand</trd>,
    <trd>Pyjama</trd>
  </trdgrp>
  <trd lng="en">pajamas</trd>
  <trd lng="fr">pyjamas</trd>
  <trdgrp lng="zh">
    <trd>&#30561;&#34915; <pr>shu&igrave;y&imacron;</pr></trd>,
    <trd>&#30561;&#34915;&#35044; <pr>shu&igrave;y&imacron;k&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kostum.popol0o">
  <kap>
    popol<tld/>o, <var><kap>naci<tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>o tradicia &ccirc;e iu popolo:
      <ekz>
        ili prezentis precipe dancojn kaj kantojn en bunta vario de popol<tld/>oj sur
        stratoj kaj placoj
        <fnt>
          <bib>Monato</bib>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="https://www.monato.be/2003/007945.html">Bukedo de popolarto</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur slovakaj popol<tld/>oj oni povas vidi <klr>[&#8230;]</klr> broda&jcirc;ojn, trika&jcirc;ojn
        kaj kro&ccirc;eta&jcirc;ojn, kies ornama&jcirc;oj estis transprenitaj el renesancaj modeloj
        <fnt>
          <bib>Monato</bib>
          <aut>Stano Mar&ccaron;ek</aut>
          <vrk><url ref="https://www.monato.be/2010/009986.php">Popolajn kostumojn portas
          pupoj</url></vrk>
          <lok>2010</lok>
        </fnt>;
      </ekz>
      <ekz>
        grupoj en lokaj naci-<tld/>oj el diversaj vila&gcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Gerrit Berveling</aut>
          <vrk><url ref="https://www.monato.be/2009/009822.php">Katastrofa re&gcirc;inotago</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_r;&c_o;&c_d;&c_n;&c_y; <ind>&c_s;&c_t;&c_r;&c_o;&c_j;</ind></trd>,
    <trd>&c_t;&c_r;&c_a;&c_d;&c_y;&c_c;&c_y;&c_j;&c_n;&c_y; <ind>&c_s;&c_t;&c_r;&c_o;&c_j;</ind></trd>
  </trdgrp>
  <trd lng="de">Volkstracht</trd>
  <trd lng="ja">&#27665;&#20439;&#34915;&#35013;  <pr>&#12415;&#12435;&#12382;&#12367;&#12356;&#12375;&#12423;&#12358;</pr></trd>
</drv>

<drv mrk="kostum.rol0ado">
  <kap>
    rol<tld/>ado
  </kap>
  <snc>
    <dif>
      <tld lit="K"/>ado kiel figuro el mangao, animacio k.a.:
      <ekz>
        Miniono en rol<tld/>ado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=610187"
              >Minionoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        la internacia ligo de <tld/>istoj rekonis <klr>[&#8230;]</klr>Morojon kiel &leftquot;La Patrino
        de Kongresa <tld lit="K"/>ado&rightquot;, alivorte, &scirc;i estas la praavino de
        rol<tld/>ado
        <fnt>
          <aut>Brandon Sowers</aut>
          <vrk><url ref="https://bulteno.esperanto-usa.org/a/2023/04/el_la/"
              >Morojo, la praavino de rolkostumado</url></vrk>
          <lok>Usona Esperantisto, 2023-08-15</lok>
        </fnt>!
      </ekz>
