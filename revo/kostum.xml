<drv mrk="kostum.ban0o">
  <kap>ban<tld/>o</kap>
  <snc mrk="kostum.ban0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Vesto a&ubreve; vestaro portata dum banado, na&gcirc;ado:
      <ekz>
        &scirc;i preferas unupecajn ban<tld/>ojn
        <fnt>
          <aut>-</aut>,
          <lok><url ref="http://eo.lernu.net/">lernu.net</url>,
          2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k; <klr tip="amb">(&c_k;&c_a;&c_s;&c_mol;&c_c;&c_ju;&c_m;)</klr></trd>
  <trd lng="cs">plavky</trd>
  <trdgrp lng="fr">
    <trd>costume de bain</trd>,
    <trd>maillot de bain</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>str&oacute;j k&aogonek;pielowy</trd>,
    <trd>k&aogonek;piel&oacute;wki</trd>
