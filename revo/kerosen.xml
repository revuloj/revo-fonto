<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kerosen.xml,v 1.17 2023/09/01 15:07:02 revo Exp $">
<kap>
  <rad>kerosen</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kerosen.0o">
  <kap><tld/>o</kap>
  <snc mrk="kerosen.0o.KEM">
  <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="super" cel="petrol.0o">Petrola</ref> frakcio, akirata
      per distilado de <ref tip="malprt" cel="naft.0o">nafto</ref>
      &ccirc;e la temperaturoj inter 151&#x2DA;C kaj 273&#x2DA;C; uzata
      por lumigado
      (en petrollampoj), sed nun precipe kiel la &ccirc;efa
      brula&jcirc;o de la reakciaj motoroj:
      <ekz>
        brilis <klr>...</klr> granda <tld/>a lampo kaj kvar palacaj
        lanternoj
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 13a, p.
        146a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_z;&c_a;</trd>
  <trd lng="bo">&#xF66;&#xF0B;&#xF66;&#xFA3;&#xF74;&#xF58;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>kerosen</trd>,
    <trd>lampov&yacute; petrolej</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Brenn&ouml;l</trd>,
    <trd>Petroleum</trd>,
    <trd>Kerosin</trd>
  </trdgrp>
  <trd lng="en">kerosene</trd>
  <trd lng="es">queroseno</trd>
  <trd lng="fr">k&eacute;ros&egrave;ne</trd>
  <trd lng="hu">kerozin</trd>
  <trdgrp lng="ja">
    <trd>&#x706F;&#x6CB9;  [<ind>&#x3068;&#x3046;&#x3086;</ind>]</trd>,
    <trd>&#x30B1;&#x30ED;&#x30B7;&#x30F3;</trd>
  </trdgrp>
  <trd lng="nl">kerosine</trd>
  <trd lng="pl">nafta</trd>     
  <trd lng="ro">kerosen</trd>
  <trd lng="ru">&c_k;&c_je;&c_r;&c_o;&c_s;&c_i;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>keros&eacute;n</trd>,
    <trd>lampov&yacute; olej</trd>
  </trdgrp>
  <trd lng="uk">&#x433;&#x430;&#x441;</trd>
</drv>
</art>
