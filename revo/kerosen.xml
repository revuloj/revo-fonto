<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kerosen</rad>/o
</kap>
<drv mrk="kerosen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Petrola frakcio, akirita per distilado de la kruda
      <ref tip="malprt" cel="petrol.0o">petrolo</ref> inter
      151&ring;C kaj 273&ring;C; utiligata por lumigado (en petrollampoj),
      sed nun precipe kiel la &ccirc;efa brula&jcirc;o de la
      <ref tip="vid" cel="reakci.0o.2">reakciaj</ref> motoroj.
    </dif>
  </snc>
  <trd lng="en">kerosene</trd>
  <trd lng="ru">&c_k;&c_je;&c_r;&c_o;&c_s;&c_i;&c_n;</trd>
</drv>
</art>

