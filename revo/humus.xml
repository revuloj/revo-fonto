<vortaro>
  <art mrk="$Id: humus.xml,v 1.3 2023/05/04 16:16:18 afido Exp $">
    <kap>
      <rad>humus</rad>/o
    </kap>

<drv mrk="humus.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kacx.0o.KUI">Ka&ccirc;o</ref> el kikeroj 
      (a&ubreve; faboj, lentoj), sezamo kaj spicoj; <ref tip="sin" cel="kiker.0kacxo">kikerka&ccirc;o</ref>:
      <ekz>
        lenta somera <tld/>o
        <fnt>
          <vrk><url ref="https://apetito.ikso.net/recepto/lenta-somera-humuso"
              >Lenta somera humuso</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        man&gcirc;a&jcirc;oj kiel falafelo, <tld/>o, &scirc;ak&scirc;uko, kuskuso, sabi&hcirc;o kaj zataro estas vaste
        popularaj en Israelo
        <fnt>
          <vrk><url ref="&Viki;/Israela_kuirarto"
              >Israela kuirarto</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Hummus</trd>
    <trd lng="en">hummus</trd>
    <trd lng="fr">houmous</trd>
  </snc>
</drv>

  </art>

