<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <ofc>1</ofc>
    <rad>bankrot</rad>/i
  </kap>
  <drv mrk="bankro.0i">
    <kap><tld/>i</kap>
    <gra><vspec>ntr</vspec></gra>
    <snc num="1">
      <uzo tip="fak">EKON</uzo>
      <dif>
        &Ccirc;esigi la pagon de siaj &scirc;uldoj en komerco, k lasi
        deklari tiun 
        staton per ju&gcirc;o: <ekz><tld/>into, <tld/>ulo.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd>bankrottieren</trd>,
        <trd>Bankrott machen</trd>,
        <trd>Konkurs machen</trd>,
        <trd>Pleite machen</trd>,
        <trd>pleite gehen</trd>,
        <trd>in <ind>Konkurs gehen</ind></trd></trdgrp>
      <trd lng="en">go bankrupt</trd>
      <trd lng="nl">failliet gaan</trd>
      <trdgrp lng="pl">
        <trd>bankrutowa&cacute;</trd>,
        <trd>og&lstroke;asza&cacute; <ind>bankructwo</ind></trd>
      </trdgrp>
    </snc>
    <snc num="2">
      <uzo tip="stl">FIG</uzo>
      <dif>
        Fiaski, malsukcesi:
        <ekz>
          lia senlima indulgo <tld/>is<fnt>Z</fnt>;
        </ekz>
        <ekz>
          &ccirc;iuj niaj penoj <tld/>is.
        </ekz>
      </dif>
      <trdgrp lng="de">
        <trd><ind>Schiffbruch</ind>  erleiden</trd>,
        <trd>einbrechen</trd>,
        <trd>versagen</trd>,
        <trd>eine <ind>Pleite</ind> erleben</trd>,
        <trd><ind>Misserfolg</ind> haben</trd>
        </trdgrp>
      <trd lng="nl">mislukken</trd>
      <trd lng="pl">splajtowa&cacute;</trd>
    </snc>
    <trdgrp lng="be">
      <trd>&c_b;&c_a;&c_n;&c_k;&c_r;&c_u;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_z;&c_b;&c_a;&c_n;&c_k;&c_r;&c_u;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="fr">faire <ind>banqueroute</ind></trd>
    <trdgrp lng="hu">
      <trd>cs&odblac;dbe jut</trd>,
      <trd>cs&odblac;d&ouml;t mond</trd>
    </trdgrp>
    <trd
    lng="ru">&c_o;&c_b;&c_a;&c_n;&c_k;&c_r;&c_o;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
    <trdgrp lng="sk">
      <trd>skrachova&tcaron;</trd>,
      <trd>zbankrotova&tcaron;</trd>
    </trdgrp>
  </drv>

  <drv mrk="bankro.0o">
    <kap><tld/>o</kap>
    <snc>
      <uzo tip="fak">EKON</uzo>
      <dif>
      Stato de komercisto, kiu <tld/>is: <ekz>intenca <tld/>o, fripona
      <tld/>o.
      </ekz>
      </dif>
      <trd
      lng="be">&c_b;&c_a;&c_n;&c_k;&c_r;&c_u;&c_c;&c_t;&c_v;&c_a;</trd>
      <trdgrp lng="de">
        <trd>Bankrott</trd>,
        <trd>Konkurs</trd>,
        <trd>Pleite</trd>
      </trdgrp>
      <trd lng="en">bankruptcy</trd>
      <trd lng="fr">banqueroute</trd>
      <trd lng="hu">cs&odblac;d</trd>
      <trdgrp lng="nl">
        <trd>bankroet</trd>,
        <trd>faillissement</trd>
      </trdgrp>
      <trdgrp lng="pl">
        <trd>bankructwo</trd>,
        <trd>plajta</trd>
      </trdgrp>
      <trd
      lng="ru">&c_b;&c_a;&c_n;&c_k;&c_r;&c_o;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
      <trdgrp lng="sk">
        <trd>krach</trd>,
        <trd>bankrot</trd>
      </trdgrp>
    </snc>
    </drv>
</art>
