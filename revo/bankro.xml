<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bankro.xml,v 1.22 2021/03/04 19:18:48 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>bankrot</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="bankro.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="bankro.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      &Ccirc;esigi la pagon de siaj &scirc;uldoj en komerco,
      kaj lasi deklari tiun staton per ju&gcirc;o:
      <ekz>
        <tld/>into, <tld/>ulo.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>bankrottieren</trd>,
      <trd>Bankrott machen</trd>,
      <trd>Konkurs machen</trd>,
      <trd>Pleite machen</trd>,
      <trd>pleite gehen</trd>,
      <trd>in <ind>Konkurs gehen</ind></trd></trdgrp>
    <trd lng="en">go bankrupt</trd>
    <trd lng="nl">failliet gaan</trd>
    <trdgrp lng="pl">
      <trd>bankrutowa&cacute;</trd>,
      <trd>og&lstroke;asza&cacute; <ind>bankructwo</ind></trd>,
      <trd>plajtowa&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="bankro.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="vid" cel="fiask.0i">Fiaski</ref>,
      <ref tip="vid" cel="sukces.mal0i">malsukcesi</ref>:
      <ekz>
        lia senlima indulgo <tld/>is<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj niaj penoj <tld/>is.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd><ind>Schiffbruch</ind>  erleiden</trd>,
      <trd>einbrechen</trd>,
      <trd>versagen</trd>,
      <trd>eine <ind>Pleite</ind> erleben</trd>,
      <trd><ind>Misserfolg</ind> haben</trd>
      </trdgrp>
    <trd lng="nl">mislukken</trd>
    <trd lng="pl">splajtowa&cacute;</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_n;&c_k;&c_r;&c_u;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_b;&c_a;&c_n;&c_k;&c_r;&c_u;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bankrotovat</trd>,
    <trd>ud&#x11B;lat bankrot</trd>,
    <trd>zbankrotovat</trd>,
    <trd>zkrachovat</trd>
  </trdgrp>
  <trd lng="fr">faire <ind>banqueroute</ind></trd>
  <trd lng="he">&#x5DC;&#x5E4;&#x5E9;&#x5D5;&#x5D8; &#x5E8;&#x5D2;&#x5DC;</trd>
  <trdgrp lng="hu">
    <trd>cs&odblac;dbe jut</trd>,
    <trd>cs&odblac;d&ouml;t mond</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_a;&c_n;&c_k;&c_r;&c_o;&c_t;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>skrachova&tcaron;</trd>,
    <trd>vyjs&#x165; na mizinu</trd>,
    <trd>zbankrotova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="bankro.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Stato de komercisto, kiu <tld/>is:
      <ekz>
        intenca <tld/>o, fripona <tld/>o.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_n;&c_k;&c_r;&c_u;&c_c;&c_t;&c_v;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Bankrott</trd>,
      <trd>Konkurs</trd>,
      <trd>Pleite</trd>
    </trdgrp>
    <trd lng="en">bankruptcy</trd>
    <trd lng="fr">banqueroute</trd>
    <trd lng="hu">cs&odblac;d</trd>
    <trdgrp lng="nl">
      <trd>bankroet</trd>,
      <trd>faillissement</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>bankructwo</trd>,
      <trd>plajta</trd>
    </trdgrp>
    <trd lng="ru">&c_b;&c_a;&c_n;&c_k;&c_r;&c_o;&c_t;&c_s;&c_t;&c_v;&c_o;</trd>
    <trdgrp lng="sk">
      <trd>krach</trd>,
      <trd>bankrot</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>bankrot</trd>,
    <trd>konkurs</trd>,
    <trd>krach</trd>,
    <trd>&#xFA;padek</trd>
  </trdgrp>
  <trd lng="he">&#x5E4;&#x5BC;&#x5B0;&#x5E9;&#x5C1;&#x5B4;&#x5D9;&#x5D8;&#x5B7;&#x5EA; &#x5E8;&#x5B6;&#x5D2;&#x5B6;&#x5DC;</trd>
  <trdgrp lng="sk">
    <trd>konkurz</trd>,
    <trd>&#xFA;padok</trd>
  </trdgrp>
</drv>
</art>
