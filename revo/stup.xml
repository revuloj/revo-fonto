<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stup.xml,v 1.19 2024/03/25 07:28:37 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>stup</rad>/o
</kap>
<drv mrk="stup.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Implikaj krispaj fadenoj restantaj el &scirc;pinita kanabo a&ubreve; lino kaj
      uzataj por <ref tip="vid" cel="sxtop.0i.fermi">&scirc;topi<sncref/></ref>, 
      <ref tip="vid" cel="kalfat.0i">kalfatri</ref>:
      <ekz>
        la fortulo fari&gcirc;os kiel <tld/>o, kaj lia laboro kiel fajrero, kaj amba&ubreve; brulos, kaj neniu estingos
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 1:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        li dis&scirc;iris la &scirc;nurojn, kiel oni dis&scirc;iras fadenon el <tld/>o, kiam fajro &gcirc;in bruldifektis
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        embarasata kiel kokido en <tld/>o;
      </ekz>
      <ekz>
        <tld/>o, pe&ccirc;o kaj gudro por kalfatrado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8194">&Scirc;ipo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_k;&c_u;&c_l;&c_mol;&c_l;&c_je;</trd>, 
    <trd>&c_k;&c_u;&c_d;&c_z;&c_je;&c_l;&c_ja;</trd> 
  </trdgrp>
  <trdgrp lng="cs">
    <trd>cucek</trd>,
    <trd>koudel</trd>
  </trdgrp>
  <trd lng="de">Werg</trd>
  <trdgrp lng="en">
    <trd>tow<klr tip="amb"> (fiber)</klr></trd>,
    <trd>oakum</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>estopa</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csep&udblac;</trd>, 
    <trd>k&oacute;c</trd> 
  </trdgrp>
  <trd lng="ja">&#x9EBB;&#x304F;&#x305A;  <pr>&#x3042;&#x3055;&#x304F;&#x305A;</pr></trd>
  <trd lng="nl">werk<klr tip="ind"> (uitgeplozen touw)</klr></trd>
  <trd lng="pl">paku&lstroke;y<klr> {pl.}</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_k;&c_l;&c_ja;</trd>,
    <trd>&c_k;&c_u;&c_d;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">k&uacute;de&lcaron; <klr>(v&yacute;&ccaron;esky)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43B;&#x43E;&#x447;&#x447;&#x44F;</trd>,
    <trd>&#x43A;&#x443;&#x434;&#x435;&#x43B;&#x44F;</trd>
  </trdgrp>
  <trd lng="zh">&#x9EBB;&#x7D6E; <pr>m&#xE1;x&#xF9;</pr></trd>
</drv>
</art>
