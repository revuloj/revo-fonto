<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: juk.xml,v 1.36 2023/12/03 07:09:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>juk</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="juk.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="juk.0i.KOMUNE">
    <dif>
      Korpe <ref tip="super" cel="incit.0i.fizike">inciti</ref>,
      kvaza&ubreve; per multaj piketoj, ka&ubreve;zante deziron sin grati:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>grati iun, kie <tld/>as al li</ind>
        <klr>(flati &ccirc;e konate malforta flanko)</klr>.
      <trd lng="fr">gratter qn o&ugrave; cela d&eacute;mange</trd>
      </ekz>
    </dif>
    <subsnc mrk="juk.0i.kauxzo">
      <dif>
        Ka&ubreve;zi incitan senton en korpoparto:
        <ekz>
          la &scirc;trumpo <tld/>as min;
        </ekz>
        <ekz>
          s-ino Baniko havas aknojn, kiuj <tld/>as &scirc;iajn femurojn kaj ventron
          <fnt>
            <bib>KrB</bib>
          </fnt>;
        </ekz>
        <ekz>
          &scirc;i sciis, ke stranga nevuso nek <tld/>anta nek doloriga a&ubreve;guras problemon
          <fnt>
            <bib>Monato</bib>
            <aut>Lin Sheng</aut>
            <vrk><url ref="&Monato;/2006/008670.php">Forpasis mia karulino</url></vrk>
            <lok>2006</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="juk.0i.sento">
      <dif>
        Ricevi incitan senton en korpoparto:
        <ekz>
          kie <tld/>as, tie ni gratas
          <fnt>
            <bib>PrV</bib>
          </fnt>;
        </ekz>
        <ekz>
          <tld/>is &scirc;in la viza&gcirc;o kaj precipe la frunto super la brovoj
          <fnt>
            <bib>Far2</bib>
            <lok>&Ccirc;apitro XVI</lok>
          </fnt>;
        </ekz>
        <ekz>
          <uzo tip="stl">FIG</uzo>
          via ha&ubreve;to videble <tld/>as, sed mi ne konsilas peti min, ke mi vin gratu
          <fnt>
            <bib>QuV</bib>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <snc mrk="juk.0i.tenti">
    <dif>
      <ref tip="sin" cel="tent.0i">Tenti</ref>; 
      &gcirc;ene <ref tip="super" cel="instig.0i.iun_al_io">instigi</ref> al io; 
      ne <ref tip="ant" cel="las.0i.ne_gxeni">lasi</ref> trankvila:
      <ekz>
        tiklas kaj <tld/>as al mi en la malanta&ubreve;aj piedoj, kvaza&ubreve; mi devus na&gcirc;i
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>as la konscienco ripro&ccirc;anta
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2014/011285.php">Retmesa&gcirc;o, petmesa&gcirc;o</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        venos tempo, kiam oni <klr>[&#x2026;]</klr> havante <tld/>antajn orelojn, amasigos al
        si instruantojn, la&ubreve; siaj deziroj
        <fnt>
          <bib>NT</bib>
          <lok>II. Timoteo 4:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        li babilis kun mi, &ccirc;ar kredeble lia lango <tld/>is
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;in <tld/>is la senpacienca deziro ion komuniki, sed la &ccirc;eesto de infanoj &scirc;in
        malhelpis
        <fnt>
          <bib>ChC</bib>
        </fnt>;
      </ekz>
      <ekz>
        lin <tld/>as verki por legantocentoj
        <fnt>
          <bib>Monato</bib>
          <aut>Antonio Val&eacute;n</aut>
          <vrk><url ref="&Monato;/2005/008499.php">&Ccirc;u Ertl provizore stilekzercas?</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[jen]</klr> ek<tld/>as la po&scirc;telefono
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2011/010289.php">&Ccirc;u malsano patologia?</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam oficialulo, funkciulo, respondas al demando per &leftquot;komenti mi ne volas&rightquot;, tiam
        <tld/>as la &jcirc;urnalista nazo: jen iu, kiu volas ion ka&scirc;i
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2014/011303.php">&Ccirc;iam eblas revi ...</url></vrk>
          <lok>2014</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>la mano lin <tld/>as</ind>
        <klr>(li volonte batus, &scirc;telus ks)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">la main lui <ind>d&eacute;mange</ind></trd>
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="gxen.0i">&gcirc;eni</ref>,
    <ref cel="tikl.0i">tikli</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_ja;&c_r;&c_b;&c_je;&c_c;&c_mol;</trd>
  <trd lng="bo">&t_za;&t_tsheg;&t_la;&t_tsheg;&t_la;&t_nga;&t_tsheg;&t_ba;&t_tsheg;</trd>
  <trd lng="br">debroni&ntilde;</trd>
  <trd lng="ca">picar</trd>
  <trdgrp lng="cs">
    <trd>svrb&ecaron;t</trd>,
    <trd>sv&ecaron;dit</trd>
  </trdgrp>
  <trd lng="de">jucken</trd>
  <trd lng="en">itch</trd>
  <trd lng="es">picar</trd>
  <trd lng="fr">d&eacute;manger</trd>
  <trd lng="hu">viszket</trd>
  <trd lng="id">gatal</trd>
  <trd lng="it">prudere</trd>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x3086;&#x304C;&#x3089;&#x305B;&#x308B;</trd>,
    <trd>&#x3080;&#x305A;&#x3080;&#x305A;&#x3055;&#x305B;&#x308B;</trd>,
    <trd>&#x304B;&#x3086;&#x3044;</trd>
  </trdgrp>
  <trd lng="nl">jeuken</trd>
  <trdgrp lng="pl">
    <trd>sw&eogonek;dzi&cacute;</trd>,
    <trd>&sacute;wierzbie&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>co&ccedil;ar</trd>,
    <trd>causar comi&ccedil;&atilde;o</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_u;&c_d;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">svrbie&tcaron;</trd>
  <trd lng="uk">&c_s;&c_v;&c_je;&c_r;&c_b;&c_ib;&c_t;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x7619;&#x75D2; <pr>s&#xE0;oy&#x1CE;ng</pr></trd>,
    <trd>&#x75D2; <pr>y&#x1CE;ng</pr></trd>,
    <trd>&#x75D2;&#x75D2; <pr>y&#x1CE;ngyang</pr></trd>,
    <trd>&#x60F3;&#x53BB;&#x6293;&#x75D2; <pr>xi&#x1CE;ngq&#xF9;zhu&#x101;y&#x1CE;ng</pr></trd>,
    <trd>&#x6293;&#x75D2; <pr>zhu&#x101;y&#x1CE;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="juk.0a">
  <kap><tld/>a</kap>
  <snc mrk="juk.0a.KOMUNE">
    <dif>
      Kiu <tld/>as:
      <ekz>
        grati la <tld/>antan vangon;
      </ekz>
      <ekz>
        mi sentas <tld/>ajn brulojn
        <fnt>
          <bib>LSB</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        de tie venas <tld/>a zumo de gaja&ccirc;a muziko
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        trankviligi <tld/>an ha&ubreve;ton
        <fnt>
          <aut>one of Fae's humans</aut>,
          <vrk><url ref="http://lifewithfae.blogspot.com/2011/12/itchy-moving-juka-translogigado.html">
          juka translo&gcirc;i&gcirc;ado</url></vrk>,
          <lok>2011-12-30</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tikl.0a">tikla</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_ja;&c_k;&c_ib; <ind>&c_s;&c_mol;&c_v;&c_ja;&c_r;&c_b;&c_ib;&c_c;&c_mol;</ind></trd>
  <trd lng="cs">sv&ecaron;div&yacute;</trd>
  <trdgrp lng="de">
    <trd>juckend</trd>,
    <trd>kratzig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>itchy</trd>,
    <trd>itching</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>qui <ind>d&eacute;mange</ind></trd>,
    <trd>prurigineux</trd>
  </trdgrp>
  <trd lng="sk">&scaron;tekliv&yacute;</trd>
</drv>

<drv mrk="juk.0o">
  <kap><tld/>o</kap>
  <snc mrk="juk.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="sent.0o.impreso">Sento</ref> de piketoj en la ha&ubreve;to, ka&ubreve;zanta deziron
      sin
      grati:
      <ekz>
        de pluraj tagoj li sentas malagrablan <tld/>on &ccirc;e sia vira organo
        <fnt>
          <bib>KrB</bib>
        </fnt>;
      </ekz>
      <ekz>
        pro netolerebla ha&ubreve;ta <tld/>o, karakteriza por la malsano, la bestoj froti&gcirc;as
        kontra&ubreve; &ccirc;iuj malmolaj objektoj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2006/008861.php">Nova etapo en biologio kaj medicino</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_v;&c_je;&c_r;&c_b;</trd>
  <trd lng="br">debron</trd>
  <trdgrp lng="cs">
    <trd>svrab</trd>,
    <trd>svrb&ecaron;n&iacute;</trd>,
    <trd>sv&ecaron;d&ecaron;n&iacute;</trd>
  </trdgrp>
  <trd lng="de">Jucken</trd>
  <trd lng="en">itch</trd>
  <trd lng="es">picor</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;mangeaison</trd>,
    <trd>prurit</trd>
  </trdgrp>
  <trd lng="hu">viszket&eacute;s</trd>
  <trd lng="id">gatal</trd>
  <trd lng="it">prurito</trd>
  <trd lng="ja">&#x304B;&#x3086;&#x307F;</trd>
  <trd lng="nl">jeuk</trd>
  <trdgrp lng="pl">
    <trd>sw&eogonek;dzenie</trd>,
    <trd>&sacute;wierzbienie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>coceira</trd>,
    <trd>comi&ccedil;&atilde;o</trd>
  </trdgrp>
  <trd lng="ru">&c_z;&c_u;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>svrab</trd>,
    <trd>svrbenie</trd>
  </trdgrp>
  <trd lng="zh">&#x7619;&#x75D2; <pr>s&#xE0;oy&#x1CE;ng</pr></trd>
</drv>
</art>
