<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>juk</rad>/i
</kap>
<drv mrk="juk.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="incit.0i.fizike">Inciti</ref> ies ha&ubreve;ton, kvaza&ubreve; per multaj
      piketoj ka&ubreve;zante deziron sin grati:
      <ekz>
        grati la <tld/>antan vangon;
      </ekz>
      <ekz>
        la &scirc;trumpo <tld/>as min;
      </ekz>
      <ekz><uzo tip="stl">FIG</uzo>
        la mano lin <tld/>as<fnt>Z</fnt>
        <klr>(li volonte batus)</klr>
        <fnt><bib>PrV</bib></fnt></ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        grati iun, kie <tld/>as al li
        <klr>(flati &ccirc;e konate malforta flanko)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">jucken</trd>
  <trd lng="en">itch</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;manger</trd>
  </trdgrp>
  <trd lng="nl">jeuken</trd>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_je;&c_s;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_z;&c_u;&c_d;&c_je;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="juk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sento de piketoj en la ha&ubreve;to, ka&ubreve;zanta deziron sin grati.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;mangeaison</trd>,
    <trd>prurit</trd>
  </trdgrp>
</drv>
</art>
