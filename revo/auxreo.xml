<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>a&ubreve;reol</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="auxreo.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxreo.0o.REL">
    <uzo tip="fak">REL</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
Luma rondo &ccirc;irka&ubreve; la kapo de la sanktuloj, &ccirc;e pentra&jcirc;oj.
    </dif>
    <refgrp tip="sin">
      <ref cel="glor.0krono">glorkrono</ref>.
    </refgrp>
    <trd lng="be">&c_n;&c_ib;&c_m;&c_b;</trd>
    <trd lng="en">halo</trd>
    <trd lng="ru">&c_n;&c_i;&c_m;&c_b;</trd>
    <trd lng="pl">aureola</trd>
    <trd lng="sk">sv&auml;to&zcaron;iara</trd>
    <trd lng="it">aureola</trd>
  </snc>
  <snc mrk="auxreo.0o.famo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Presti&gcirc;o, brila famo:
      <ekz>
        la <tld/>o de la sukceso, de la venko.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_r;&c_e;&c_o;&c_l;</trd>,
      <trd>&c_n;&c_ib;&c_m;&c_b;</trd>,
      <trd>&c_z;&c_z;&c_ja;&c_n;&c_n;&c_je;</trd>,
      <trd>&c_a;&c_w;&c_r;&c_a;</trd>
    </trdgrp>
    <trd lng="en">aura</trd>
    <trd lng="pl">gloria</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_r;&c_je;&c_o;&c_l;</trd>,
      <trd>&c_n;&c_i;&c_m;&c_b;</trd>,
      <trd>&c_s;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_a;&c_u;&c_r;&c_a;</trd>
    </trdgrp>
    <trd lng="it">aura</trd>
  </snc>
  <refgrp tip="sin">
    <ref cel="nimb.0o">nimbo</ref>.
  </refgrp>
  <trd lng="es">aureola</trd>
  <trdgrp lng="fr">
    <trd>aur&eacute;ole<klr tip="ind"> (lumi&egrave;re,
prestige)</klr></trd>,
    <trd>nimbe</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>dicsf&eacute;ny</trd>,
    <trd>f&eacute;nykoszor&uacute;</trd>,
    <trd>nimbusz</trd>,
    <trd>aure&oacute;la</trd>
  </trdgrp>
</drv>
</art>
