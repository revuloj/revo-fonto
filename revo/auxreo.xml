<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>6</ofc>
<rad>a&ubreve;reol</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="auxreo.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxreo.0o.REL">
    <uzo tip="fak">REL</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Luma rondo &ccirc;irka&ubreve; la kapo de la sanktuloj, &ccirc;e
      pentra&jcirc;oj.
    </dif>
    <refgrp tip="sin">
      <ref cel="glor.0krono">glorkrono</ref>.
    </refgrp>
  </snc>
  <snc mrk="auxreo.0o.famo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Presti&gcirc;o, brila famo:
      <ekz>
        la <tld/>o de la sukceso, de la venko.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="nimb.0o">nimbo</ref>.
  </refgrp>
  <trdgrp lng="fr">
    <trd>aur&eacute;ole<klr tip="ind"> (lumi&egrave;re, prestige)</klr></trd>,
    <trd>nimbe</trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&#1086;&#1088;&#1077;&#1086;&#1083;</trd>,
    <trd>&#1085;&#1080;&#1084;&#1073;</trd>,
    <trd>&#1089;&#1080;&#1103;&#1085;&#1080;&#1077;</trd>
  </trdgrp>
</drv>
</art>
