<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>a&ubreve;reol</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="auxreo.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxreo.0o.REL">
    <uzo tip="fak">REL</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Luma rondo &ccirc;irka&ubreve; la kapo de la sanktuloj, &ccirc;e
      pentra&jcirc;oj.
    </dif>
    <refgrp tip="sin">
      <ref cel="glor.0krono">glorkrono</ref>.
    </refgrp>
    <trd lng="ru">&c_n;&c_i;&c_m;&c_b;</trd>
  </snc>
  <snc mrk="auxreo.0o.famo">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Presti&gcirc;o, brila famo:
      <ekz>
        la <tld/>o de la sukceso, de la venko.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_o;&c_r;&c_je;&c_o;&c_l;</trd>,
      <trd>&c_n;&c_i;&c_m;&c_b;</trd>,
      <trd>&c_s;&c_i;&c_ja;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_a;&c_u;&c_r;&c_a;</trd>
    </trdgrp>
  </snc>
  <refgrp tip="sin">
    <ref cel="nimb.0o">nimbo</ref>.
  </refgrp>
  <trdgrp lng="fr">
    <trd>aur&eacute;ole<klr tip="ind"> (lumi&egrave;re, prestige)</klr></trd>,
    <trd>nimbe</trd>
  </trdgrp>
</drv>
</art>
