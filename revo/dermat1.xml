<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dermatologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="dermat1.0o">
  <kap><tld/>o</kap>
  <snc mrk="dermat1.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Medicina fako, kiu studas la <ref tip="vid"
      cel="fiziol1.0o">fiziologion</ref> kaj la <ref tip="vid"
      cel="patolo.0o">patologion</ref> de la ha&ubreve;to, de la apudaj mukozoj
      kaj de la faneroj.
    </dif>
  <rim>
    Kelkaj esperantistoj provis enkonduki la sinonimon
    <em>ha&ubreve;tmedicino</em>
    <fnt>
      <aut>Vikipedio</aut>,
      <vrk><url
      ref="http://eo.wikipedia.org/wiki/Ha&ubreve;tmedicino">Ha&ubreve;tmedicino</url></vrk>
    </fnt>.
     Tiu termino estas pli esperanta sed probable malpli internacia.
     Kvankam &gcirc;i ne trovi&gcirc;as en PIV, &gcirc;i tamen estas la&ubreve;sence tute rajta.
  </rim>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_r;&c_m;&c_a;&c_t;&c_a;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_d;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="ca">dermatologia</trd>
  <trdgrp lng="de">
    <trd>Dermatologie</trd>,
    <trd>Lehre von den Hautkrankheiten</trd>
  </trdgrp>
  <trd lng="en">dermatology</trd>
  <trd lng="es">dermatolog&iacute;a</trd>
  <trd lng="fr">dermatologie</trd>
  <trdgrp lng="hu">
    <trd>dermatol&oacute;gia</trd>,
    <trd>b&odblac;rgy&oacute;gy&aacute;szat</trd>
  </trdgrp>
  <trd lng="nl">dermatologie</trd>
  <trd lng="pl">dermatologia</trd>
  <trd lng="pt">dermatologia</trd>
  <trd lng="ru">&c_d;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
  <trd lng="sk">dermatol&oacute;gia</trd>
</drv>

<drv mrk="dermat1.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="dermat1.0isto.MED">
    <uzo tip="fak">MED</uzo>
  <ref tip="dif" cel="dermat.0o">dermatologo</ref>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_r;&c_m;&c_a;&c_t;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="bg">&c_d;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="ca">dermat&ograve;leg</trd>
  <trdgrp lng="de">
    <trd>Dermatologe</trd>,
    <trd>Hautarzt</trd>
  </trdgrp>
  <trd lng="en">dermatologist</trd>
  <trd lng="es">dermat&oacute;logo</trd>
  <trd lng="fr">dermatologue</trd>
  <trdgrp lng="hu">
    <trd>dermatol&oacute;gus</trd>,
    <trd>b&odblac;rgy&oacute;gy&aacute;sz</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>dermatoloog</trd>,
    <trd>huidarts</trd>
  </trdgrp>
  <trd lng="pl">dermatolog</trd>
  <trd lng="pt">dermatologista</trd>
  <trd lng="ru">&c_d;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">dermatol&oacute;g</trd>
</drv>
</art>
