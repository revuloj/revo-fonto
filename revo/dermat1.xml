<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>dermatologi</rad>/o
</kap>
<drv mrk="dermat1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Parto de la <ref tip="malprt" cel="medici.0o">medicino</ref>, kiu pritraktas ha&ubreve;tmalsanojn.
    </dif>    
  </snc>
  <trdgrp lng="de">
    <trd>Dermatologie</trd>,
    <trd>Lehre von den Hautkrankheiten</trd>
  </trdgrp>
  <trd lng="fr">dermatologie</trd>
  <trdgrp lng="hu">
    <trd>dermatol&oacute;gia</trd>,
    <trd>b&odblac;rgy&oacute;gy&aacute;szat</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;&c_l;&c_o;&c_g;&c_i;&c_ja;</trd>
</drv>

<drv mrk="dermat1.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Fakulo pri <tld/>o, plejofte anka&ubreve; <ref tip="super" cel="kurac.0isto">kuracisto</ref> de ha&ubreve;tmalsanoj.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="dermat.0o">dermatologo</ref>
  </refgrp>
  <trdgrp lng="de">
    <trd>Dermatologe</trd>,
    <trd>Hautarzt</trd>
  </trdgrp>
  <trd lng="fr">dermatologue</trd>
  <trdgrp lng="hu">
    <trd>dermatol&oacute;gus</trd>,
    <trd>b&odblac;rgy&oacute;gy&aacute;sz</trd>
  </trdgrp>
  <trd lng="ru">&c_d;&c_je;&c_r;&c_m;&c_a;&c_t;&c_o;&c_l;&c_o;&c_g;</trd>
</drv>
</art>
