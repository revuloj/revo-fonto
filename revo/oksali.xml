<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>oksalid</rad>/o
</kap>
<drv mrk="oksali.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc mrk="oksali.0o.BOT">
    <dif>
      Genro de herboj kreskantaj en malsekaj lokoj, el familio <ref
      tip="super" cel="oksali1.0oj"><tld/>acoj</ref> (<trd
      lng="la">Oxalis</trd>).
    </dif>
    <refgrp tip="vid">
      <ref cel="okzal.0o">okzalo</ref>,
      <ref cel="foli.tri0o">trifolio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_s;&c_mol;&c_l;&c_ib;&c_c;&c_a;</trd>
  <trd lng="ca">g&egrave;nere Oxalis</trd>
  <trd lng="de">Sauerklee</trd>
  <trd lng="fr">oxalis</trd>
  <trd lng="hu">mad&aacute;rs&oacute;ska</trd>
  <trd lng="nl">klaverzuring</trd>
  <trdgrp lng="pt">
    <trd>oxidalic&aacute;cea</trd>,
    <trd>ox&aacute;lida</trd>
  </trdgrp>  <trd lng="ru">&c_k;&c_i;&c_s;&c_l;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
