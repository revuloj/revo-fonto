<vortaro>
<art mrk="$Id$">
<kap>
  <rad>oksalid</rad>/o
</kap>
<drv mrk="oksali.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="oksali1.0oj"><tld/>acoj</ref>
      (<trd lng="la">Oxalis</trd>), kreskanta en malsekaj lokoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="okzal.0o">okzalo</ref>,
      <ref cel="foli.tri0o">trifolio</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Sauerklee</trd>
  <trd lng="hu">mad&aacute;rs&oacute;ska</trd>
  <trd lng="nl">klaverzuring</trd>
  <trd lng="pt">ox&aacute;lida</trd>
  <trd lng="ru">&c_k;&c_i;&c_s;&c_l;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
