<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>oksalid</rad>/o
</kap>
<drv mrk="oksali.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="oksali.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Oxalis</trd>), kreskanta en malsekaj lokoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="okzal.0o">okzalo</ref>,
      <ref cel="foli.tri0o">trifolio</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Sauerklee</trd>
</drv>
<drv mrk="oksali.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Oxalidaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Sauerkleegew&auml;chse</trd>
</drv>
</art>
