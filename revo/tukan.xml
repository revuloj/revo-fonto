<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tukan.xml,v 1.17 2023/09/04 19:01:54 revo Exp $">
<kap>
  <ofc>9</ofc><rad>tukan</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="tukan.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="tukan.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mez- a&ubreve; sudamerika birdo el tropika &gcirc;angalo
      kun brilaj koloroj, tre dika beko,
      genro el familio ramfastedoj (<trd lng="la">Ramphastos</trd>), 
      ofte nomo anka&ubreve; la tuta familio:
      <ekz>
        <tld/>oj kun purpuraj kapoj, bluaj brustoj kaj flavaj flugiloj 
        &ccirc;iam flugis de unu rando de la intermonto al la alia
        <fnt><aut>H. Sienkiewicz, trad. M. Sygnarski</aut>, <vrk><url ref="http://esperanto.pl/page.php?tid=200000">Tra dezerto kaj 
        praarbaro</url></vrk>, <lok>Varsovio, 1978</lok></fnt>;
      </ekz>
      <ekz>
        kaj la frakoj de pingvenoj, /
        kaj la beko de <tld/>o, /
        kaj la grando de balenoj
        <fnt>
          <aut>S. Smigielski</aut>,
          <vrk><url ref="http://www.internacia-gazeto.com/articoli-2006/art_06_062_stanislaw_besteto.htm">
          Bestetoj kaj Bestegoj</url></vrk>,
          <lok>Internacia Gazeto, 2006-05-02</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_k;&c_a;&c_n;</trd>
  <trd lng="ca">tuc&agrave;</trd>
  <trd lng="de">Tukan<klr> (Vogel)</klr></trd>
  <trd lng="en">toucan</trd>
  <trd lng="fr">toucan</trd>
  <trd lng="hu">tuk&aacute;n</trd>
  <trd lng="nl">toekan</trd>
  <trd lng="pl">tukan</trd>
  <trd lng="pt">tucano</trd>
  <trd lng="ro">tucan </trd>
</drv>

<drv mrk="tukan.T0o">
  <kap><tld lit="T"/>o
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt></kap>
  <mlg>Tuc</mlg>
  <snc mrk="tukan.T0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Sud-hemisfera <ref tip="lst" cel="stel.0figuro.AST" lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Tucana</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_T;&c_u;&c_k;&c_a;&c_n; <klr>(&c_s;&c_u;&c_z;&c_o;&c_r;'&c_je;)</klr></trd>
  <trd lng="ca">el <ind>Tuc&agrave;</ind></trd>
  <trd lng="cs">tukan</trd>
  <trd lng="de">Tukan<klr> (Sternbild)</klr></trd>
  <trd lng="en">Toucan<klr> (constellation)</klr></trd>
  <trd lng="fr">Toucan<klr tip="ind"> (constellation)</klr></trd>
  <trd lng="hu">Tuk&aacute;n<klr> (csillagk&eacute;p</klr></trd>
  <trd lng="ja">&#x30AA;&#x30AA;&#x30CF;&#x30B7;</trd>
  <trd lng="nl">Toekan<klr tip="ind"> (sterrenbeeld)</klr></trd>
  <trd lng="pl">Tukan  <klr>(gwiazdozbi&oacute;r)</klr></trd>
  <trd lng="ro">Tucanul  <klr>(constela&#x21B;ie)</klr></trd>
  <trd lng="sk">tukan</trd>
  <trd lng="uk">&c_t;&c_u;&#x43A;&c_a;&#x43D;</trd>
</drv>
</art>

