<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: niob.xml,v 1.22 2022/03/24 10:41:40 revo Exp $">
<kap>
  <rad>niob</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="niob.0o">
  <kap><tld/>o</kap>
  <mlg kod="IUPAC">Nb</mlg>
  <snc mrk="niob.0o.KEM">
  <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="41">elemento</ref> kun
      atomnumero 41, atompezo 92,906;
      malofta transir<ref tip="super" cel="metal.0o">metalo</ref> 
      kun amfotera karaktero:
      <ekz>
        <tld/>o estas <klr>[&#x2026;]</klr> griza, mola, dratigebla transirmetalo
        trovata en niobito kaj uzata en specialaj &scirc;talaj alojoj, &gcirc;i blui&gcirc;as kiam &gcirc;i kontaktas aeron
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=22469">Niobio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_ib;&c_o;&c_b;&c_ib;&c_j;</trd>
  <trd lng="bg">&c_n;&c_i;&c_o;&c_b;&c_i;&c_j;</trd>
  <trd lng="br">niobiom</trd>
  <trd lng="ca">niobi</trd>
  <trd lng="cs">niobium</trd>
  <trd lng="de">Niob</trd>
  <trd lng="en">niobium</trd>
  <trd lng="es">niobio</trd>
  <trd lng="fa">&a_n;&f_y;&a_w;&a_b;&f_y;&a_m;</trd>
  <trd lng="fr">niobium</trd>
  <trd lng="hu">ni&oacute;bium</trd>
  <trd lng="id">niobium</trd>
  <trd lng="it">niobio</trd>
  <trd lng="la">Niobium</trd>
  <trd lng="nl">niobium</trd>
  <trd lng="pl">niob</trd>
  <trd lng="pt">ni&oacute;bio</trd>
  <trd lng="ru">&c_n;&c_i;&c_o;&c_b;&c_i;&c_j;</trd>
  <trd lng="sk">ni&oacute;b</trd>
  <trd lng="sv">niob</trd>
  <trd lng="uk">&#x43D;&#x456;&#x43E;&#x431;&#x456;&#x439;</trd>
</drv>
</art>
