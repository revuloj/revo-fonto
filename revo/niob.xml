  <trd lng="es">niobio</trd>
  <trd lng="fa">&a_n;&f_y;&a_w;&a_b;&f_y;&a_m;</trd>
  <trd lng="fr">niobium</trd>
  <trd lng="hu">ni&oacute;bium</trd>
  <trd lng="it">niobio</trd>
  <trd lng="la">Niobium</trd>
  <trd lng="nl">niobium</trd>
  <trd lng="pl">niob</trd>
  <trd lng="pt">ni&oacute;bio</trd>
  <trd lng="ru">&c_n;&c_i;&c_o;&c_b;&c_i;&c_j;</trd>
