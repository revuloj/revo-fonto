<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>arkt</rad>/a
</kap>
<drv mrk="arkt.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Nordpolusa.
    </dif>
  </snc>
  <trd lng="de">arktisch</trd>
  <trd lng="fr">arctique</trd>
  <trd
  lng="ru">&#1072;&#1088;&#1082;&#1090;&#1080;&#1095;&#1077;&#1089;&#1082;&#1080;&#1081;</trd>
</drv>
<drv mrk="arkt.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La <tld/>a regiono.
    </dif>
  </snc>
  <trd lng="de">Arktis</trd>
  <trd lng="fr">Arctique</trd>
  <trd lng="ru">&#1040;&#1088;&#1082;&#1090;&#1080;&#1082;&#1072;</trd>
</drv>
<drv mrk="arkt.A0aOceano">
  <kap><tld lit="A"/>a Oceano</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      Parto de la <ref tip="malprt" cel="ocean.0o">oceano</ref>
      situanta norde de Ameriko kaj E&ubreve;razio.
    </dif>
  </snc>
  <trd lng="de">Arktischer Ozean</trd>
</drv>
</art>
