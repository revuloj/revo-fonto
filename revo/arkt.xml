<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>arkt</rad>/a
    </kap>
    <drv mrk="arkt.0a">
      <kap><tld/>a</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Nordpolusa.
        </dif>
      </snc>
      <trd lng="de">arktisch</trd>
      <trd lng="fr">arctique</trd>
      <trd lng="ru">&c_a;&c_r;&c_k;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
    </drv>
    <drv mrk="arkt.0o">
      <kap><tld lit="A"/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          La <tld/>a regiono.
        </dif>
      </snc>
      <trd lng="de">Arktis</trd>
      <trd lng="fr">Arctique</trd>
      <trd lng="ru">&c_A;&c_r;&c_k;&c_t;&c_i;&c_k;&c_a;</trd>
    </drv>
    <drv mrk="arkt.A0aOceano">
      <kap><tld lit="A"/>a Oceano</kap>
      <uzo tip="fak">GEOG</uzo>
      <snc>
        <dif>
          Parto de la <ref tip="malprt" cel="ocean.0o">oceano</ref>
          situanta norde de Ameriko kaj E&ubreve;razio.
        </dif>
      </snc>
      <trd lng="de">Arktischer Ozean</trd>
      <trd lng="ru">&c_S;&c_je;&c_v;&c_je;&c_r;&c_n;&c_y;&c_j; &c_L;&c_je;&c_d;&c_o;&c_v;&c_i;&c_t;&c_y;&c_j; &c_o;&c_k;&c_je;&c_a;&c_n;</trd>
    </drv>
  </art>
