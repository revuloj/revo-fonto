<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: arkt.xml,v 1.25 2021/02/28 18:35:45 revo Exp $">
<kap>
  <ofc>9</ofc>
  <rad>Arkt</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="arkt.A0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La regiono &ccirc;irka&ubreve; la norda poluso.
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="antark.A0o">Antarkto</ref>.
  </refgrp>
  <trd lng="be">&c_A;&c_r;&c_k;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="bg">&c_A;&c_r;&c_k;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="ca">&Agrave;rtida</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E8;&#x5E7;&#x5D8;&#x5B4;&#x5D9;</trd>
  <trd lng="nl">Noordpoolgebied</trd>
  <trd lng="pt">&Aacute;rtico</trd>
  <trd lng="de">Arktis</trd>
  <trd lng="en">the <ind>Arctic</ind></trd>
  <trd lng="es">&Aacute;rtico</trd> 
  <trd lng="fa">&a_sh;&a_m;&a_A;&a_l;&f_g;&a_A;&a_n;</trd>
  <trd lng="fr">Arctique</trd>
  <trd lng="hu">&Eacute;szaki-sark</trd>
  <trd lng="it">Artico</trd>
  <trd lng="pl">Arktyka</trd>
  <trd lng="ru">&c_A;&c_r;&c_k;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">Arkt&iacute;da</trd>
  <trd lng="ms">Artik</trd>
  <trd lng="tl">Artiko</trd>
</drv>

<drv mrk="arkt.0a">
  <kap><ofc>9</ofc><tld lit="a"/>a <fnt><bib>PV</bib></fnt></kap>
  <snc>
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Rilata al <tld/>o; 
      tia, kiel (en) <tld/>o:
      <ekz>
        la <tld/>a Cirklo
        <fnt><bib>9OA</bib></fnt>;
      </ekz>
      <ekz>
        <tld lit="a"/>a klimato
        <fnt><bib>9OA</bib></fnt>;
      </ekz>
      <ekz>
        la <ref cel="ocean.ArktaO0o"><tld/>a Oceano</ref>
        <fnt><bib>9OA</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_r;&c_k;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="bg">&c_a;&c_r;&c_k;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;</trd>
  <trd lng="ca">&agrave;rtic</trd>
  <trd lng="de">arktisch</trd>
  <trd lng="en">arctic</trd>
  <trdgrp lng="es">
    <trd>&aacute;rtico</trd>,
    <trd>&aacute;rtica</trd>
  </trdgrp> 
  <trd lng="fr">arctique</trd>
  <trdgrp lng="hu">
    <trd>&eacute;szaki-sarki</trd>,
    <trd>arktikus</trd>
  </trdgrp>
  <trd lng="it">artico<klr> (agg.)</klr></trd>
  <trd lng="pl">arktyczny</trd>
  <trd lng="ru">&c_a;&c_r;&c_k;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">arktick&yacute;</trd>
</drv>
</art>
