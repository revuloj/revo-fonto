<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ar&gcirc;entan</rad>/o
</kap>
<drv mrk="argxen1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Alojo el kupro, nikelo kaj zinko, imitanta ar&gcirc;enton.
    </dif>
    <ref tip="vid" cel="alfeni.0o">alfenido.</ref>
  </snc>
  <adm>
    Vd admsub "alfenido". <aut>MB</aut>
  </adm>
  <trd lng="de">Neusilber</trd>
  <trd lng="fr">argentan</trd>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_g;&c_je;&c_n;&c_t;&c_a;&c_n;</trd>,
    <trd>&c_n;&c_je;&c_j;&c_z;&c_i;&c_l;&c_mol;&c_b;&c_je;&c_r;</trd>,
    <trd>&c_m;&c_je;&c_l;&c_mol;&c_h;&c_i;&c_o;&c_r;</trd>
  </trdgrp>
</drv>
</art>
