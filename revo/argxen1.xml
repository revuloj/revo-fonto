<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: argxen1.xml,v 1.12 2021/02/28 18:35:45 revo Exp $">
<kap>
  <rad>ar&gcirc;entan</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="argxen1.0o">
  <kap><tld/>o</kap>
  <snc mrk="argxen1.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="dif" cel="alfeni.0o">Alfenido</ref>:
      <ekz>
        sur la anta&ubreve;aj flankoj de la moneroj bildi&gcirc;as diversaj maraj bestoj, dorsflanke
        trovi&gcirc;as stiligita bildigo de la kvar protektaj spiritoj de Islando
        <klr>[&#x2026;monero de]</klr> 50 kronoj, krabo <klr>[el]</klr>
        <tld/>o <klr>[&#x2026;]</klr> 100 kronoj, la fi&scirc;o lumpo <klr>[same el]</klr> <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=215990">Islanda krono</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kiel materialo oni uzas <klr>[&#x2026;]</klr> por la valvokesto latunon a&ubreve; <tld/>on
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=356101">Valvo (blovinstrumento)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Neusilber</trd>
  <trdgrp lng="en">
    <trd>argentan</trd>,
    <trd>German <ind>silver</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>alpaca</trd>,
    <trd>argentan</trd>,
    <trd>maillechort</trd>
  </trdgrp>
  <trd lng="he">&#x5D0;&#x5E8;&#x5D2;&#x5E0;&#x5D8;&#x5D9;&#x5E0;&#x5D0;&#x5D9;</trd>
  <trdgrp lng="hu">
    <trd>&uacute;jez&uuml;st</trd>,
    <trd>argent&aacute;n</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_r;&c_g;&c_je;&c_n;&c_t;&c_a;&c_n;</trd>,
    <trd>&c_n;&c_je;&c_j;&c_z;&c_i;&c_l;&c_mol;&c_b;&c_je;&c_r;</trd>,
    <trd>&c_m;&c_je;&c_l;&c_mol;&c_h;&c_i;&c_o;&c_r;</trd>
  </trdgrp>
</drv>
</art>
