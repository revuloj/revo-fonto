<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gronland</rad>/o
</kap>

<drv mrk="gronland.0o">
  <kap>
    <tld lit="G"/>o
    <fnt>
      <vrk>Oficiala Informo de AdE</vrk>,
      <lok>numero 12</lok>
    </fnt>
  </kap>
  <snc mrk="gronland.0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Lando en la samnoma insulo kun &ccirc;efurbo Nuko.
      Teritorio de A&ubreve;tonoma Teritorio Gronlando.
      Dependa de <ref tip="vid" cel="dan.D0ujo">Danujo</ref>
      ISO-3166 kodo: GL.
    </dif>
    <trd lng="da">Gr&oslash;nland</trd>
    <trdgrp lng="pl">
      <trd>Grenlandia</trd>
    </trdgrp>
    <trd lng="sv">Gr&ouml;nland</trd>
  </snc>
  <snc mrk="gronland.0o.insulo">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      La plej granda monda insulo.
      &Gcirc;i trovi&gcirc;as en la norda <ref tip="vid"
      cel="atlant.0o.GEOG">Atlantiko</ref>.
    </dif>
    <trd lng="da">Gr&oslash;nland</trd>
    <trdgrp lng="pl">
      <trd>Grenlandia</trd>
    </trdgrp>
    <trd lng="sv">Gr&ouml;nland</trd>
  </snc>
</drv>

</art>

