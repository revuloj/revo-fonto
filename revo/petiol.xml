<art mrk="$Id: petiol.xml,v 1.15 2024/04/04 08:28:01 revo Exp $">
<kap>
  <rad>petiol</rad>/o
</kap>
<drv mrk="petiol.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="tig.0o">Tigo</ref> de <ref cel="foli.0o.BOT">folio</ref>:
      <ekz>
        la folioj <klr>[de pepono]</klr> estas grandaj, ties nervuroj kaj <tld/>oj estas
        dornecaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18905">Kukurbacoj</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        fuksio: <klr>[&#8230;]</klr> kontra&ubreve;sidantaj <tld/>aj, ofte ovo- a&ubreve; lancetformaj folioj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=20164">Fuksio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ch;&c_a;&c_r;&c_a;&c_n;&c_o;&c_k;</trd>,
    <trd>&c_h;&c_v;&c_o;&c_s;&c_mol;&c_c;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Blattstiel</trd>
  <trd lng="en">petiole</trd>
  <trd lng="fr">p&eacute;tiole</trd>
  <trd lng="hu">lev&eacute;lny&eacute;l</trd>
  <trd lng="ja">&#33865;&#x67C4;  <pr>&#12424;&#12358;&#12408;&#12356;</pr></trd>
  <trd lng="nl">bladsteel</trd>
  <trd lng="pl">ogonek  <ind>li&sacute;ciowy</ind></trd>
  <trd lng="ro">pe&#x21B;iol  <klr>(botanic&abreve;)</klr></trd>
  <trd lng="ru">&c_ch;&c_je;&c_r;&c_je;&c_sh;&c_o;&c_k;</trd>
  <trd lng="uk">&c_ch;&c_je;&c_r;&c_je;&c_sh;&#x43E;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#x53F6;&#x67C4; <pr>y&#xE8;b&#x1D0;ng</pr></trd>,
    <trd>&#33865;&#x67C4; <pr>y&#xE8;b&#x1D0;ng</pr></trd>
  </trdgrp>
</drv>
</art>
