<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kobalt</rad>/o
</kap>
<drv mrk="kobalt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 27, atompezo 58,9332, simbolo Co.
      <ref tip="super" cel="metal.0o">Metalo</ref>, kies diversaj
      kombina&jcirc;oj donas bluan koloron:
      <ekz>
        du trionoj de la elfositaj kupro kaj <tld/>o estis a&ccirc;etitaj kaj
        prilaboritaj de belgaj entreprenoj<fnt><aut>RR</aut>:
        <vrk>Historia fono</vrk>, <bib>Monato</bib><lok>jaro 2001a, numero
        3a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_b;&c_a;&c_l;&c_mol;&c_t;</trd>    
  <trd lng="ca">cobalt</trd>
  <trd lng="br">kobalt</trd>
  <trd lng="de">Kobalt</trd>
  <trd lng="en">cobalt</trd>
  <trd lng="es">cobalto</trd>
  <trdgrp lng="fr">
    <trd>cobalt</trd>
  </trdgrp>
  <trd lng="nl">cobalt</trd>
  <trd lng="ru">&c_k;&c_o;&c_b;&c_a;&c_l;&c_mol;&c_t;</trd>
</drv>
</art>
