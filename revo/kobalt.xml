<kap>
  <ofc>*</ofc>
  <rad>kobalt</rad>/o
</kap>
<drv mrk="kobalt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="Co">Co</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM" lst="voko:&hcirc;emiaj_elementoj" val="27">elemento</ref> kun
      atomnumero 27, atompezo 58,9332;
      transir<ref tip="super" cel="metal.0o">metalo</ref>, kies diversaj
      kombina&jcirc;oj donas bluan koloron:
      <ekz>
        du trionoj de la elfositaj <ref tip="vid" cel="kupr.0o">kupro</ref> kaj <tld/>o estis a&ccirc;etitaj kaj
        prilaboritaj de belgaj entreprenoj<fnt><aut>RR</aut>:
        <vrk>Historia fono</vrk>, <bib>Monato</bib><lok>jaro 2001a,
        numero
        3a, p. 9a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_b;&c_a;&c_l;&c_mol;&c_t;</trd>
  <trd lng="bg">&c_k;&c_o;&c_b;&c_a;&c_l;&c_t;</trd>
  <trd lng="br">kobalt</trd>
  <trd lng="ca">cobalt</trd>
  <trd lng="cs">kobalt</trd>
  <trd lng="de">Kobalt</trd>
  <trd lng="en">cobalt</trd>
  <trd lng="es">cobalto</trd>
  <trd lng="fr">cobalt</trd>
  <trd lng="id">kobalt</trd>
  <trd lng="it">cobalto</trd>
  <trd lng="ja">&#x30B3;&#x30D0;&#x30EB;&#x30C8;  <pr>&#x3053;&#x3070;&#x308B;&#x3068;</pr></trd>
  <trd lng="la">Cobaltum</trd>
  <trd lng="nl">kobalt</trd>
  <trd lng="pl">kobalt</trd>
  <trd lng="pt">cobalto</trd>
  <trd lng="ro">cobalt</trd>
  <trd lng="ru">&c_k;&c_o;&c_b;&c_a;&c_l;&c_mol;&c_t;</trd>
  <trd lng="sk">kobalt</trd>
  <trd lng="sv">kobolt</trd>
  <trd lng="uk">&#x43A;&#x43E;&#x431;&#x430;&#x43B;&#x44C;&#x442;</trd>
  <trd lng="zh">&#x9237; <pr>g&#x1D4;</pr></trd>
</drv>
