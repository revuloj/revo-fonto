<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>karnaval</rad>/o
</kap>
<drv mrk="karnav.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Tempo, dedi&ccirc;ata al amuza&jcirc;oj, kiu iras de la festo de l' Tri Re&gcirc;oj,
      &gcirc;is la Cindromerkredo, precipe dum la lastaj tagoj anta&ubreve; la granda
      fasto.
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Fastnacht</trd>,
     <trd>Fasching</trd>,
     <trd>Faselnacht</trd>,
     <trd>Karneval</trd>
  </trdgrp>
  <trd lng="nl">carnaval</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_n;&c_a;&c_v;&c_a;&c_l;</trd>
  <trd lng="sv">karneval</trd>
</drv>
</art>
