<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>karnaval</rad>/o
</kap>
<drv mrk="karnav.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="karnav.0o.antauxkaresma">
    <dif>
      Tempo, dedi&ccirc;ata al amuza&jcirc;oj (burleskaj procesioj,
      alivesti&gcirc;o ks), anta&ubreve;iranta
      <ref cel="karesm.0o">Karesmon</ref>.
    </dif>
    <trdgrp lng="br">
      <trd>Ened</trd>,
      <trd>Meurlarjez</trd>
    </trdgrp>
  </snc>
  <snc mrk="karnav.0o.amuzajxoj">
    <dif>
      Similaj amuza&jcirc;oj, sendepende de la dato, en kiu ili
      okazas.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_n;&c_a;&c_v;&c_a;&c_l;</trd>
  <trdgrp lng="de">
     <trd>Fastnacht</trd>,
     <trd>Fasching</trd>,
     <trd>Faselnacht</trd>,
     <trd>Karneval</trd>
  </trdgrp>
  <trd lng="fr">carnaval</trd>
  <trdgrp lng="hu">
     <trd>farsang</trd>,
     <trd>karnev&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">carnaval</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_n;&c_a;&c_v;&c_a;&c_l;</trd>
  <trd lng="sv">karneval</trd>
</drv>
</art>
