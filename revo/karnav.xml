<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>karnaval</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="karnav.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc mrk="karnav.0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Popola <ref tip="super" cel="fest.0o.KAL">festo</ref> kun 
      alivesti&gcirc;oj, burleskaj procesioj, festenoj, 
      en E&ubreve;ropo kaj Sudameriko ofte okazanta anta&ubreve; la
      <ref tip="vid" cel="fast.granda0o.KRI">granda fasto</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_n;&c_a;&c_v;&c_a;&c_l;</trd>
  <trdgrp lng="br">
    <trd>Ened</trd>,
    <trd>Meurlarjez</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Fastnacht</trd>,
     <trd>Fasching</trd>,
     <trd>Faselnacht</trd>,
     <trd>Karneval</trd>
  </trdgrp>
  <trd lng="fr">carnaval</trd>
  <trdgrp lng="hu">
     <trd>farsang</trd>,
     <trd>karnev&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">carnaval</trd>
  <trd lng="pt">carnaval</trd>
  <trd lng="ru">&c_k;&c_a;&c_r;&c_n;&c_a;&c_v;&c_a;&c_l;</trd>
  <trd lng="sv">karneval</trd>
</drv>
</art>
