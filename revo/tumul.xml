<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: tumul.xml,v 1.6 2025/04/08 14:12:33 revo Exp $">
<kap>
  <rad>tumul</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="tumul.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="tumul.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      <ref tip="dif" cel="mont.tomb0eto.ARKE">Tombmonteto</ref>:
      <ekz>
        la tombaro de la periodo de Militantaj Regnoj (475-221 a.K.)
        konsistas el 9 grandaj kaj mezgrandaj <tld/>oj
        <fnt>
        <aut>&Ccirc;ina Radio Internacia</aut>,
        <vrk><url ref="&CRI;1/2005/01/19/1@20017.htm">
        Brilaj arkeologiaj elteriga&jcirc;oj...</url></vrk>,
        <lok>2005-01-19</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="super">
      <ref cel="kairn.0o">kairno</ref>,
      <ref cel="amas.sxton0o.ARKE">&scirc;tonamaso</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="megali.0o">megalito</ref>,
      <ref cel="stupa.0o">stupao</ref>
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>tombomonteto</ke>.
  </rim>

  <trd lng="be">&c_k;&c_u;&c_r;&c_g;&c_a;&c_n;</trd>
  <trd lng="cs">rov</trd>
  <trd lng="de">Grabh&uuml;gel</trd>
  <trd lng="en">(burial) <ind>mound</ind></trd>
  <trd lng="es">t&uacute;mulo</trd>
  <trd lng="fr">tumulus</trd>
  <trd lng="pl">kurchan</trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>hrob</trd>,
    <trd>rov</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#1084;&#1086;&c_g;&c_i;&#1083;&c_a;</trd>,
    <trd>&#1084;&#1086;&c_g;&c_i;&#1083;&#1100;&#1085;&c_i;&c_j; &#1087;&c_a;&c_g;&#1086;&c_r;&c_b;</trd>,
    <trd>&#1082;&c_u;&c_r;&c_g;&c_a;&#1085;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#22367;&#22836; <pr>f&#233;nt&#243;u</pr></trd>,
    <trd>&#22707;&#38957; <pr>f&#233;nt&#243;u</pr></trd>,
    <trd>&#20898; <pr>zh&#466;ng</pr></trd>
  </trdgrp>
</drv>
</art>

