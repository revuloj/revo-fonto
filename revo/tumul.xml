<drv mrk="tumul.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="tumul.0o.ARKE">
    <uzo tip="fak">ARKE</uzo>
    <dif>
      <ref tip="dif" cel="mont.tomb0eto.ARKE">Tombmonteto</ref>:
    <ekz>
      la tombaro de la periodo de Militantaj Regnoj (475-221 a.K.)
      konsistas el 9 grandaj kaj mezgrandaj <tld/>oj
      <fnt>
      <aut>&Ccirc;ina Radio Internacia</aut>,
      <vrk><url ref="http://esperanto.cri.cn/1/2005/01/19/1@20017.htm">
      Brilaj arkeologiaj elteriga&jcirc;oj...</url></vrk>,
      <lok>2005-01-19</lok>
      </fnt>.
    </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_r;&c_g;&c_a;&c_n;</trd>
  <trd lng="cs">rov</trd>
  <trd lng="de">Grabh&uuml;gel</trd>
  <trd lng="en">(burial) <ind>mound</ind></trd>
  <trd lng="es">t&uacute;mulo</trd>
  <trd lng="fr">tumulus</trd>
  <trd lng="pl">kurchan</trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_g;&c_a;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>hrob</trd>,
    <trd>rov</trd>
  </trdgrp>
</drv>
