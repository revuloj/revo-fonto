<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: primus.xml,v 1.15 2023/10/20 16:38:17 revo Exp $">
<kap>
  <rad>primus</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="primus.0o">
  <kap><tld/>o</kap>
  <snc mrk="primus.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Eta, portebla <ref tip="super" cel="masxin.kuir0o">kuirma&scirc;ino</ref>,
      pli ofte kun propra rezervo de bruli&gcirc;a&jcirc;o:
      <ekz>
        mi metos la tekru&ccirc;on sur la <tld/>on kaj ni
        preparos la teon
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        <nom>Margarita</nom> <klr>[&#x2026;]</klr> turnis la fermilojn de la du <tld/>oj kaj ili estingi&gcirc;is
        <fnt>
          <bib>MkM</bib>
          <lok>21</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="bol.0krucxo">bolkru&ccirc;o</ref>,
<ref cel="masxin.te0o">tema&scirc;ino</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/5/55/Primus_stove.jpg/324px-Primus_stove.jpg" prm="CC BY-SA 3.0">gasa <tld/>o
        <fnt>
          <aut>Mikael Korpela, propra verko</aut>
          <lok><url ref="&WCW;/File:Primus_stove.jpg">&WCW;/File:Primus_stove.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <rim>
    <nom>Primus</nom> fakte estas sveda varmarko, sub kies nomo estas vendataj diversspecaj
    porteblaj kuiriloj.
  </rim>
  <trd lng="be">&c_p;&c_r;&c_y;&c_m;&c_u;&c_s;</trd>
  <trd lng="cs">petrolejov&yacute; va&rcaron;i&ccaron;</trd>
  <trdgrp lng="de">
    <trd>Primuskocher</trd>,
    <trd>Campingkocher</trd>
  </trdgrp>
  <trd lng="fa">&f_p;&a_r;&f_y;&a_m;&a_w;&a_s;</trd>
  <trd lng="fr">r&eacute;chaud</trd>
  <trdgrp lng="hu">
    <trd>gyorsf&odblac;z&odblac;</trd>,
    <trd>g&aacute;zrezs&oacute;</trd>,
    <trd>spirituszf&odblac;z&odblac;</trd>,
    <trd>primusz</trd>
  </trdgrp>
  <trd lng="ja">&#x643A;&#x5E2F;&#x7528;&#x77F3;&#x6CB9;&#x3053;&#x3093;&#x308D;  <pr>&#x3051;&#x3044;&#x305F;&#x3044;&#x3088;&#x3046;&#x305B;&#x304D;&#x3086;&#x3053;&#x3093;&#x308D;</pr></trd>
  <trd lng="nl">komfoor</trd>
  <trdgrp lng="pl">
    <trd>prymus</trd>,
    <trd>kuchenka turystyczna</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>fogareiro</trd>,
    <trd>r&eacute;chaud</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_m;&c_u;&c_s;</trd>
  <trd lng="sk">petrolejov&yacute; vari&ccaron;</trd>
  <trd lng="uk">&c_p;&c_r;&c_i;&c_m;&c_u;&c_s; (&c_g;&c_a;&c_s;&c_o;&c_v;&c_i;&c_j; &c_n;&c_a;&c_g;&c_r;&c_ib;&c_v;&c_a;&c_l;&c_mol;&c_n;&c_i;&c_j; &c_p;&c_r;&c_i;&c_l;&c_a;&c_d;)</trd>
</drv>
</art>

