<drv mrk="svastik.0o">
  <kap><tld/>o</kap>
  <snc mrk="svastik.0o.FORM">
    <dif>
      <ref tip="dif" cel="kruc.hoko0o.FORM">Hokokruco</ref>:
      <ekz>
        ortodoksaj oraj krucoj, /
        ru&gcirc;aj steloj kaj <tld/>oj /
        buntas, kiel farbaj &scirc;prucoj, /
        sur &ccirc;iel&#8217; de politiko
        <fnt>
          <aut>M. Gi&scirc;pling</aut>,
          <vrk>Elektonte</vrk>,
          <lok><url ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/ckk/ckk9912.htm">
          Cerbe kaj kore, 1999-12</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve;fa&scirc;ista bonintence trastrekita <tld/>o anta&ubreve; du jaroj
        apena&ubreve; povis eskapi
        punan verdikton
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2009/009902.php">Sango kaj honoro</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_v;&c_a;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="bg">&c_s;&c_v;&c_a;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
