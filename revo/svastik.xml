    <trd>esv&agrave;stica</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>h&aacute;kov&yacute; k&rcaron;&iacute;&zcaron;</trd>,
    <trd>svastika</trd>
  </trdgrp>
  <trd lng="de">Swastika</trd>
  <trd lng="en">swastika</trd>
  <trdgrp lng="es">
