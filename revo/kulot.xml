<kap>
  <rad>kulot</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="kulot.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="kulot.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="dif" cel="pantal.0eto">Pantaloneto</ref>:
      <ekz>
        plu&scirc;a <tld/>o kaj pantofloj por la festaj
        tagoj
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 1a, p.
        27a</lok></fnt>;
      </ekz>
      <ekz>
        la kostumo de viroj konsistas el <tld/>o, &ccirc;emizo kaj ve&scirc;to kun tran&ccirc;ilo &ccirc;e la
        zono
        <fnt>
          <vrk><url ref="&Monato;/2017/012052.php">Ne jupoj sed kulotoj</url></vrk>
          <lok>monato.be</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>pantaloneto</ke>.
  </rim>

  <trdgrp lng="be">
    <trd>&c_k;&c_ju;&c_l;&c_o;&c_t;&c_y;</trd>,
    <trd>&c_sh;&c_o;&c_r;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>jezdeck&eacute; kalhoty</trd>,
    <trd>kr&aacute;tk&eacute; kalhoty</trd>,
    <trd>&scaron;ortky</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kurze Hose</trd>,
    <trd>Kniebundhose</trd>,
    <trd>(Bermuda-)<ind>Shorts</ind></trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_sh;&a_l;&a_w;&a_A;&a_r;&f_k;</trd>
    <trd>&a_n;&f_y;&a_m;&#8204;&a_sh;&a_l;&a_w;&a_A;&a_r;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bermuda</trd>,
    <trd>culotte<klr> (longue, au genou)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&eacute;rdnadr&aacute;g</trd>,
    <trd>bermuda</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21322;&#12474;&#12508;&#12531;  <pr>&#12399;&#12435;&#12474;&#12508;&#12531;</pr></trd>,
    <trd>&#12461;&#12517;&#12525;&#12483;&#12488;  <pr>&#12365;&#12421;&#12429;&#12387;&#12392;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>korte <ind>broek</ind></trd>,
    <trd>bermudabroek</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>kr&oacute;tkie <ind>spodenki</ind></trd>,
    <trd>szorty</trd>,
    <trd>bermudy</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>bermuda</trd>,
    <trd>bermudas</trd>
  </trdgrp>
  <trd lng="sk">kr&aacute;tke nohavice</trd>
  <trdgrp lng="uk">
    <trd>&#1082;&#1086;&c_r;&#1086;&c_t;&#1082;&c_ib; &c_sh;&c_t;&c_a;&#1085;&c_i;</trd>,
    <trd>&c_sh;&c_t;&c_a;&#1085;&c_c;&c_ib;</trd>,
    <trd>&c_sh;&#1086;&c_r;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20839;&#20839; <pr>n&#232;inei</pr></trd>,
    <trd>&#20869;&#20869; <pr>n&#232;inei</pr></trd>
  </trdgrp>
</drv>
