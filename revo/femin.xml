<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: femin.xml,v 1.3 2019/10/04 12:10:24 revo Exp $">
<kap>
  <rad>femin</rad>/o <fnt><bib>NeoGlo</bib></fnt>
</kap>
<drv mrk="femin.0o">
  <kap><tld/>o</kap>
  <snc mrk="femin.0o.virino">
    <uzo tip="stl">RAR</uzo>
    <dif>
      <ref tip="dif" cel="vir.0ino">Virino</ref>:
      <ekz>
        adoleska, rustike vestita, dina femino descendas haste la &scirc;tuparon
        de burgo
        <fnt>
          <vrk>Esperantologio</vrk>,
          <lok><url
          ref="https://books.google.com/books?redir_esc=y&amp;id=UCIoAAAAMAAJ&amp;dq=%22femino%22+%22%C5%9Di%22&amp;focus=searchwithinvolume&amp;q=femino">internacia
          revue por la lingvistiko kaj bibliografio de Esperanto, volumo
          2, eldono 2</url>, 1961</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni estis &gcirc;uste aran&gcirc;antaj niajn vesta&jcirc;ojn, kiam neanoncite, e&ccirc; sen
        pordo-bruo, aperis en la &ccirc;ambro juna, beleta femino
        <fnt>
          <aut>J. Sigmond kaj S. Rodin</aut>:
          <vrk>Libazar' Kaj Tero</vrk>,
          <lok><url
          ref="https://books.google.com/books?id=vTGFDXzWbzgC&amp;lpg=PA98&amp;dq=%22femino%22%20%22%C5%9Di%22&amp;pg=PA98#v=onepage&amp;q=%22femino%22%20%22%C5%9Di%22&amp;f=false">pa&gcirc;o
          98</url>, 2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="femal.0o">femalo</ref>
  </snc>
  <!-- maloftan vorton traduku prefere per malofta&jcirc;o -->
  <trd lng="be">&c_zh;&c_a;&c_n;&c_ch;&c_y;&c_n;&c_a;</trd>
  <trd lng="cs">&zcaron;ena</trd>
  <trd lng="de">Frau</trd>
  <trd lng="el">&gamma;&ypsilon;&ny;&alfa;&jota_ton;&kappa;&alfa;</trd>
  <trd lng="en">woman</trd>
  <trd lng="es">mujer</trd>
  <trd lng="et">naine</trd>
  <trd lng="fy">frou</trd>
  <trd lng="he">&alef;&shin;&he;</trd>
  <trd lng="hu">n&odblac;</trd>
  <trd lng="io">muliero</trd>
  <trd lng="nl">vrouw</trd>
  <trd lng="pl">kobieta</trd>
  <trd lng="pt">mulher</trd>
  <trd lng="ru">&c_zh;&c_je;&c_n;&c_shch;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">&zcaron;ena</trd>
  <trd lng="tp">meli</trd>
  <trd lng="tr">kad&inodot;n</trd>
  <trd lng="vo">vom</trd>
</drv>
</art>

