<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ejnstejni</rad>/o
</kap>
<drv mrk="ejnstejni.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
     &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
     atomnumero 99, atompezo 252,0829, simbolo Es. <ref tip="super"
     cel="urani.trans0o">Transuranio</ref>. &Ccirc;iuj izotopoj de tiu
elemento
     estas radioaktivaj.
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_j;&c_n;&c_sh;&c_t;&c_e;&c_j;&c_n;&c_ib;&c_j;</trd>
  <trd lng="de">Einsteinium</trd>
  <trd lng="fr">einsteinium</trd>
  <trd lng="hu">einsteinium</trd>
  <trd lng="nl">einsteinium</trd>
  <trd lng="ru">&c_e;&c_j;&c_n;&c_sh;&c_t;&c_je;&c_j;&c_n;&c_i;&c_j;</trd>
</drv>
</art>

