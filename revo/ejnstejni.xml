<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ejnstejni</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="ejnstejni.0o">
  <kap><tld/>o</kap>
  <mlg>Es</mlg>
  <snc mrk="ejnstejni.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref>,
      <ref tip="super" cel="urani.trans0o">transuranio</ref> kun
      atomnumero 99, atompezo 252,0829, simbolo <ctl>Es</ctl>.
      &Ccirc;iuj izotopoj de tiu elemento estas radioaktivaj.
    </dif>
    <ref tip="sin" cel="ejnsxtejni.0o">ejn&scirc;tejnio</ref>
  </snc>
  <trd lng="be">&c_e;&c_j;&c_n;&c_sh;&c_t;&c_e;&c_j;&c_n;&c_ib;&c_j;</trd>
  <trd lng="de">Einsteinium</trd>
  <trd lng="fr">einsteinium</trd>
  <trd lng="hu">einsteinium</trd>
  <trd lng="nl">einsteinium</trd>
  <trd lng="ru">&c_e;&c_j;&c_n;&c_sh;&c_t;&c_je;&c_j;&c_n;&c_i;&c_j;</trd>
</drv>
</art>
