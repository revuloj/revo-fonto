<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sinus1.xml,v 1.8 2023/10/21 07:46:47 revo Exp $">
<kap>
  <rad>sinus</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="sinus1.0o">
  <kap><tld/>o</kap>
  <snc mrk="sinus1.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Sinua korpa kava&jcirc;o, ekzemple iu el la
      enkapaj kava&jcirc;oj malanta&ubreve; la nazkavoj:
      <ekz>
        la urogenera <tld/>o povas fari&gcirc;i vagino
        kaj &ccirc;evaginaj glandoj a&ubreve; prostato kaj
        prostata saketo
        <fnt>
          <aut>P. Jelenc</aut>,
          <vrk>intersekseco</vrk>,
          <lok>soc.culture.esperanto, 1994-02-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim mrk="sinus1.0o.RIM">
    Anka&ubreve; geometria nocio ricevis la nomon
    <ref tip="hom" cel="sinus.0o">sinuso</ref>.
  </rim>
  <trd lng="be">&c_s;&c_y;&c_n;&c_u;&c_s; <klr tip="amb">(&c_a;&c_n;&c_a;&c_t;.)</klr></trd>
  <trd lng="ca">si <klr>(anat.)</klr></trd>
  <trd lng="fr">sinus<klr> (ana.)</klr></trd>
  <trd lng="ja">&#x30B5;&#x30A4;&#x30F3;  <pr>&#12373;&#12356;&#12435;</pr></trd>
  <trdgrp lng="sk">
    <trd>dutina</trd>,
    <trd>z&#xE1;hyb</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>dutina</trd>,
    <trd>z&#xE1;hyb</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_i;&#x43D;&c_u;&c_s;</trd>,
    <trd>&c_z;&c_a;&#x43F;&c_a;&c_d;&c_i;&#x43D;&c_a;</trd>,
    <trd>&#x43F;&c_a;&c_z;&c_u;&c_h;&c_a;</trd>,
    <trd>&c_v;&c_i;&c_ji;&#x43C;&#x43A;&c_a; (&#x43B;&c_i;&c_s;&c_t;&#x43A;&c_a;)</trd>
  </trdgrp>
</drv>
</art>

