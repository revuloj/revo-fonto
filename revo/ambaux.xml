<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>amba&ubreve;</rad>
</kap>
<drv mrk="ambaux.0">
  <kap><ofc>*</ofc><tld/></kap>
  <snc>
    <dif>
      Du kune, du same, duopaj:
      <ekz>
        mi kaj mia frato estas <tld/> esperantistoj;
      </ekz>
      <ekz>
        <tld/> manoj, okuloj;
      </ekz>
      <ekz>
        oni devas fari diferencon inter la <tld/>
        kazoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>dekstra
        <klr>(uzanta <tld/> manojn kiel dekstran)</klr>;
        <trd lng="he">&dalet;&vav;-&yod;&dalet;&yod;</trd>
      </ekz>
      <ekz>
        <tld/>flanka;
      </ekz>
      <ekz>
        <tld/>flanke de la pordo staras statuoj;
      </ekz>
      <ekz>
        <tld/>tran&ccirc;a glavo
        <fnt><bib>MT</bib>, <lok>&Sen; 5:4</lok></fnt>.
      </ekz>
    </dif>
    <rim>
      Uzo de la artikolo apud <tld/> estas senutila kaj
      malkonsilinda.
    </rim>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_o;&c_d;&c_v;&c_a;</trd>,
    <trd>&c_a;&c_b;&c_je;&c_d;&c_z;&c_mol;&c_v;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>oba</trd>,
    <trd>ob&ecaron;</trd>
  </trdgrp>
  <trd lng="en">both</trd>
  <trd lng="es">ambos</trd>
  <trdgrp lng="he">
    <trd>&shin;&nun;&yod; &he;-</trd>,
    <trd>&dalet;&vav;-</trd>
  </trdgrp>
  <trd lng="hu">mindkett&odblac;</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_a;</trd>,
    <trd>&c_o;&c_b;&c_je;</trd>
  </trdgrp>
  <trd lng="de">beide</trd>
  <trdgrp lng="fr">
    <trd>tous <ind>deux</ind></trd>,
    <trd>l'<ind>un</ind> et l'autre</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>ambo</trd>,
    <trd>ambedue</trd>,
    <trd>entrambi</trd>
  </trdgrp>
  <trd lng="nl">beide</trd>
  <trdgrp lng="sk">
    <trd>obidve</trd>,
    <trd>obe</trd>
  </trdgrp>
  <trd lng="vo">bofik</trd>
</drv>
</art>
