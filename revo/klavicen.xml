<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klavicen</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="klavicen.0o">
  <kap><tld/>o</kap>
  <snc mrk="klavicen.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      E&ubreve;rop-devena granda klavara
      <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>
      populara precipe en la 17a 18a jarcentoj,
      en kiu kordojn pin&ccirc;as plumtuboj:
      <ekz>
        la <tld/>o estis tiel malnova, ke ties kordoj kantis da&ubreve;re kaj
        malla&ubreve;te responde al &ccirc;iuj &ccirc;irka&ubreve;e estigitaj sonoj
        <fnt>
        <aut>Konstantin Paustovskij, trad. A. Sidorov</aut>,
        <vrk><url ref="">Blinda kuiristo</url></vrk>,
        <lok>Nica Literatura Revuo, 1957-09 &gcirc;is 10, numero 3/1, p.
        10-14a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_ja;&c_v;&c_e;&c_s;&c_ib;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Cembalo</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>harpsichord</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>clavecin</trd>
  </trdgrp>
  <trd lng="hu">csembal&oacute;</trd>
  <trd lng="ru">&c_k;&c_l;&c_a;&c_v;&c_je;&c_s;&c_i;&c_n;</trd>
</drv>

<drv mrk="klavicen.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Senkrura, portebla
      <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>
      simila al <ref tip="vid" cel="klavicen.0o"><tld/>o</ref>
      pli&scirc;atata por privata uzo de muzik-diletantoj.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="spinet.0o">spineto</ref>
  </refgrp>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_n;&c_e;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Spinett</trd>,
    <trd>Ottavino</trd>,
    <trd>Virginal</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>spinet</trd>,
    <trd>ottavino</trd>,
    <trd>virginal</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;pinette</trd>,
    <trd>ottavino</trd>,
    <trd>virginal</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>spin&eacute;t</trd>,
    <trd>virgin&aacute;l</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_n;&c_je;&c_t;</trd>
</drv>
</art>
