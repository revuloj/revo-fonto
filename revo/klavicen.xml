<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>klavicen</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="klavicen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      E&ubreve;rop-devena granda klavara
      <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>
      populara precipe en la 17a 18a jarcentoj,
      kun kvar-okta sonamplekso,
      en kiu kordojn pin&ccirc;as plumtuboj.
    </dif>
    <refgrp tip="sin">
      <ref cel="klavcen.0o">klavceno</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Cembalo</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>harpsichord</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>clavecin</trd>
  </trdgrp>
</drv>

<drv mrk="klavincen.0eto">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Senkrura, portebla
      <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref>
      simila al <ref tip="vid" cel="klavicen.0o"><tld/>o</ref>
      pli&scirc;atata por privata uzo de muzik-diletantoj.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="spinet.0o">spineto</ref>
  </refgrp>
  <trdgrp lng="de">
    <trd>Spinett</trd>
    <trd>Ottavino</trd>,
    <trd>Virginal</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>spinet</trd>
    <trd>ottavino</trd>,
    <trd>virginal</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;pinette</trd>,
    <trd>ottavino</trd>,
    <trd>virginal</trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
-->
</vortaro>