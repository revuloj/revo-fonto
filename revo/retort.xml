<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: retort.xml,v 1.17 2017/07/04 13:10:16 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>retort</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="retort.0o">
  <kap><tld/>o</kap>
  <snc mrk="retort.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Vitra a&ubreve; fajenca sferforma vazo, kun longa, kurbe fleksita kolo,
      uzata de &hcirc;emiistoj por varmigi flua&jcirc;ojn:
      <ekz>
        viva esta&jcirc;o ne povas esti kreita en la <tld/>o de
        &hcirc;emiisto<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_t;&c_o;&c_r;&c_t;&c_a;</trd>
  <trd lng="bg">&c_r;&c_je;&c_t;&c_o;&c_r;&c_t;&c_a;</trd>
  <trd lng="ca">retorta</trd>
  <trdgrp lng="cs">
    <trd>k&rcaron;ivule</trd>,
    <trd>retorta</trd>
  </trdgrp>
  <trd lng="de">Retorte</trd>
  <trd lng="en">retort <klr>(glass vessel used in chemistry)</klr></trd>
  <trdgrp lng="fr">
    <trd>retorte</trd>,
    <trd>cornue</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>retorta</trd>,
    <trd>p&aacute;rl&oacute;ed&eacute;ny</trd>,
    <trd>g&ouml;reb</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>distilleerkolf</trd>,
    <trd>retort</trd>
  </trdgrp>
  <trd lng="pt">retorta</trd>
  <trd lng="ru">&c_r;&c_je;&c_t;&c_o;&c_r;&c_t;&c_a;</trd>
  <trd lng="sk">retorta</trd>
</drv>

<drv mrk="retort.0a">
  <kap><tld/>a</kap>
  <snc mrk="retort.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="vid" cel="kabine.0a.FIG">Kabineta<sncref/></ref>,
      nur teoria, surpapera:
      <ekz>
        <tld/>a provo.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
