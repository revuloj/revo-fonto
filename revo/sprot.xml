<kap>
  <rad>sprot</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="sprot.0o">
  <kap><tld/>o</kap>
  <snc mrk="sprot.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Fi&scirc;eto, el kiu oni faras
      <ref tip="malprt" cel="ancxov.0o.KUI">an&ccirc;ovojn</ref>;
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">genro</ref> el familio <ref
      tip="super" cel="klupee.0oj">klupeedoj</ref>
      (<trd lng="la">Sprattus</trd>):
      <ekz>
        estas pli facile veneni&gcirc;i pro <tld/>oj, ol per brando;
        trinku!
        <fnt>
          <aut>trad. P. Mo&jcirc;aev</aut>,
          <vrk><url ref="http://filmoj.net/ivan-vasiljevicx">
          Ivan Vasiljevi&ccirc; &Scirc;an&gcirc;as la Profesion</url></vrk>,
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_ib;&c_l;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_p;&c_r;&c_o;&c_t;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>&scaron;prot</trd>,
    <trd>&scaron;prota</trd>
  </trdgrp>
  <trd lng="de">Sprotte</trd>
  <trd lng="en">sprat</trd>
  <trd lng="es">espadin</trd>
  <trdgrp lng="fr">
    <trd>harenguet</trd>,
    <trd>sprat</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>spratt</trd>,
    <trd>sprotni</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>spratto</trd>,
    <trd>papalina</trd>
  </trdgrp>
  <trd lng="ja">&#x30B9;&#x30D7;&#x30E9;&#x30C3;&#x30C8;&#x30A4;&#x30EF;&#x30B7;</trd>
  <trd lng="nl">sprot</trd>
  <trdgrp lng="pl">
    <trd>szprot</trd>,
    <trd>szprotka</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>espadilha</trd>,
    <trd>lavadilha</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_i;&c_l;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_sh;&c_p;&c_r;&c_o;&c_t;</trd>,
    <trd>&c_sh;&c_p;&c_r;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">&scaron;prota <klr>(rusk&#xE1; sardinka)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_ib;&#x43B;&#x44C;&#x43A;&c_a;</trd>,
    <trd>&c_sh;&#x43F;&c_r;&#x43E;&c_t; (&c_r;&c_i;&c_b;&c_a; &c_ib; &#x43A;&#x43E;&#x43D;&c_s;&c_je;&c_r;&c_v;&c_i; &c_z; &#x43D;&c_je;&c_ji;)</trd>
  </trdgrp>
</drv>
</art>
