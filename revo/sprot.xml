<vortaro>
<art mrk="$Id$">
<kap>
<rad>sprot</rad>/o
</kap>
<drv mrk="sprot.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="klupe.0edoj">klupeedoj</ref>
      (<trd lng="la">Sprattus</trd>), el kiu oni faras 
      <ref tip="malprt" cel="ancxov.0o.KUI">an&ccirc;ovojn</ref>.
    </dif>
  </snc>
  <trd lng="de">Sprotte</trd>
  <trd lng="en">sprat</trd>
  <trd lng="es">espadin</trd>
  <trdgrp lng="fr">
    <trd>sprat</trd>, <trd>harenguet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>spratto</trd>, <trd>papalina</trd>
  </trdgrp>
  <trd lng="nl">sprot</trd>
  <trdgrp lng="pt">
    <trd>espadilha</trd>, <trd>lavadilha</trd>
  </trdgrp>
</drv>
</art>

