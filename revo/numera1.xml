<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: numera1.xml,v 1.20 2023/10/20 05:22:00 revo Exp $">
<kap>
  <rad>numerator</rad>/o <fnt>Z</fnt>
</kap>
<drv mrk="numera1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 7</lok></fnt>
  <snc>
    <dif>
      La <ref cel="divid.0ato">dividato</ref>
      en <ref cel="frakci.kvocienta0o">kvocienta frakcio</ref>
      a&ubreve; en <ref cel="frakci.algebra0o">algebra frakcio</ref>:
      <ekz>
        en la nombro <frm><sup>4</sup>/<sub>16</sub></frm>,
        <frm>4</frm> estas la <tld/>o kaj <frm>16</frm> estas la
        <ref cel="denomi.0o">denominatoro</ref>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ib;&c_ch;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="bg">
    <trd>&c_ch;&c_i;&c_s;&c_l;&c_i;&c_t;&c_je;&c_l;</trd>,
    <trd>&c_d;&c_je;&c_l;&c_i;&c_m;&c_o;</trd>
  </trdgrp>
  <trd lng="ca">numerador</trd>
  <trd lng="cs">&#x10D;itatel</trd>
  <trd lng="de">Z&auml;hler</trd>
  <trd lng="el">&alfa;&rho;&jota;&theta;&my;&eta;&tau;&eta_ton;&sigma_fina;</trd>
  <trd lng="en">numerator</trd>
  <trd lng="es">numerador</trd>
  <trd lng="fr">num&eacute;rateur<klr tip="ind"> (d'une fraction)</klr></trd>
  <trd lng="hu">sz&aacute;ml&aacute;l&oacute;</trd>
  <trd lng="ja">&#x5206;&#x5B50;  <pr>&#x3076;&#x3093;&#x3057;</pr></trd>
  <trd lng="nl">teller</trd>
  <trd lng="pl">licznik</trd>
  <trd lng="pt">numerador</trd>
  <trd lng="ru">&c_ch;&c_i;&c_s;&c_l;&c_i;&c_t;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">&#x10D;itate&#x13E;</trd>
  <trd lng="uk">&#x447;&#x438;&#x441;&#x435;&#x43B;&#x44C;&#x43D;&#x438;&#x43A;</trd>
</drv>
</art>
