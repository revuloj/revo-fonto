<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: akusti.xml,v 1.27 2023/10/28 08:33:04 revo Exp $">
<kap>
  <rad>akustik</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="akusti.0o">
  <kap><tld/>o</kap>
  <snc mrk="akusti.0o.FIZ">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Parto de la fiziko, kiu koncernas la sonojn.
    </dif>
    <trd lng="de">Schalllehre</trd>
    <trd lng="nl">geluidsleer</trd>
  </snc>
  <snc mrk="akusti.0o.SON">
    <uzo tip="fak">TEA</uzo>
    <uzo tip="fak">KIN</uzo>
    <dif>
      Sona&ubreve;debleco, sonkvalito:
      <ekz>
        en unu loko, kie la <tld/>o estis speciala, li la&ubreve;tvo&ccirc;e deklamis fragmenton el
        islanda poezia&jcirc;o
        <fnt>
          <aut>O. Pryt</aut>
          <vrk><url ref="https://esperanto-france.org/IMG/pdf/lme603.pdf#page=6">
          Memore al  Baldur</url></vrk>
          <lok>Le monde de l&#x2019;Esp&eacute;ranto, 2019:2 (603)</lok>    
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Raumklang</trd>
    <trd lng="nl">akoestiek</trd>
  </snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>akustika</trd>,
    <trd>v&ecaron;da o zvuku</trd>
  </trdgrp>
  <trd lng="de">Akustik</trd>
  <trd lng="en">acoustics</trd>
  <trd lng="es">ac&uacute;stica</trd>
  <trd lng="fr">acoustique<klr> (subst.)</klr></trd>
  <trd lng="he">&#x5D0;&#x5B2;&#x5E7;&#x5D5;&#x5BC;&#x5E1;&#x5B0;&#x5D8;&#x5B4;&#x5D9;&#x5E7;&#x5B8;&#x5D4;</trd>
  <trd lng="hu">akusztika</trd>
  <trd lng="it">acustica</trd>
  <trdgrp lng="ja">
    <trd>&#x97F3;&#x97FF;&#x5B66;  <pr>&#x304A;&#x3093;&#x304D;&#x3087;&#x3046;&#x304C;&#x304F;</pr></trd>,
    <trd>&#x97F3;&#x97FF;&#x52B9;&#x679C;  <pr>&#x304A;&#x3093;&#x304D;&#x3087;&#x3046;&#x3053;&#x3046;&#x304B;</pr></trd>
  </trdgrp>
  <trd lng="pl">akustyka</trd>
  <trd lng="pt">ac&uacute;stica</trd>
  <trd lng="ru">&c_a;&c_k;&c_u;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>akustika</trd>,
    <trd>veda o zvuku</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&#x43A;&c_u;&c_s;&c_t;&c_i;&#x43A;&c_a;</trd>
</drv>

<drv mrk="akusti.0a">
  <kap><tld/>a</kap>
  <snc mrk="akusti.0a.SON">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Rilata al la kvalito de sono, sonkvalita, sonte&hcirc;nika.
    </dif>
  </snc>
  <snc mrk="akusti.0a.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Sonanta per natura, propra vibrado, sen amplifo:
      <ekz>
        mi ludas akompanan <tld/>an gitaron en la grupo <ctl>La 
        Kuracistoj</ctl>
        <fnt>
          <aut>G. R. Fischer</aut>,
          <vrk><url ref="http://www.muenster.de/~kunar/gitarludado.htm">
          Gitarludado</url></vrk>,
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x4F20;&#x97F3; <pr>chu&#xE1;ny&#x12B;n</pr></trd>,
    <trd>&#x50B3;&#x97F3; <pr>chu&#xE1;ny&#x12B;n</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_a;&c_k;&c_u;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="fr">acoustique<klr> (adj.)</klr></trd>
  <trd lng="he">&#x5D0;&#x5B2;&#x5E7;&#x5D5;&#x5BC;&#x5E1;&#x5B0;&#x5D8;&#x5B4;&#x5D9;</trd>
  <trdgrp lng="ja">
    <trd>&#x97F3;&#x97FF;&#x306E;  <pr>&#x304A;&#x3093;&#x304D;&#x3087;&#x3046;&#x306E;</pr></trd>,
    <trd>&#x8074;&#x899A;&#x306E;  <pr>&#x3061;&#x3087;&#x3046;&#x304B;&#x304F;&#x306E;</pr></trd>,
    <trd>&#x30A2;&#x30B3;&#x30FC;&#x30B9;&#x30C6;&#x30A3;&#x30C3;&#x30AF;&#x306E;</trd>
  </trdgrp>
  <trd lng="uk">&c_a;&#x43A;&c_u;&c_s;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>
</drv>
</art>
