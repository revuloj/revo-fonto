<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: patri.xml,v 1.20 2024/02/16 19:24:11 revo Exp $">
<kap>
  <rad>patri</rad>/o <fnt><bib>PV</bib></fnt> 
</kap>

<drv mrk="patri.0o">
  <kap><tld/>o</kap>
  <snc mrk="patri.0o.MOR">
    <dif>
      <ref tip="dif" cel="patr.0ujo">Patrujo</ref>:
      <ekz>
        <klr>[ilin]</klr> turmentas la penso pri la sorto de ilia <tld/>o
        <fnt>
          <aut>Yamasaki Seik&ocirc;</aut>
          <vrk><url ref="&Monato;/2000/006909.php">Novjara kla&ccirc;o super konjako</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 25</lok>
          </fnt>;
      </ekz>
      <ekz>
        en antikveco irlandanoj &scirc;atis nomi sian <tld/>on &leftquot;lando de sanktuloj kaj
        kleruloj&rightquot;
        <fnt><aut>IRL</aut>,
        <vrk><url ref="&Monato;/2000/006914.php">Lando de malsanktuloj</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000/04, p. 10</lok>
        </fnt>;
      </ekz>
      <ekz>
        pave kaj parade sin prezenti kiel iun savanton de la <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Gilbert Ledon</aut>
          <vrk><url ref="&Monato;/2003/008040.html">Kvina povo</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="patrio.0o">patrioto</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_c;&c_mol;&c_k;&c_a;&c_w;&c_sh;&c_ch;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_a;&c_j;&c_ch;&c_y;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_o;&c_t;&c_je;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>,
    <trd>&c_r;&c_o;&c_d;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="br">mammvro</trd>
  <trdgrp lng="cs">
    <trd>domovina</trd>,
    <trd>ot&ccaron;ina</trd>,
    <trd>vlast</trd>
  </trdgrp>
  <trd lng="de">Vaterland</trd>
  <trdgrp lng="en">
    <trd>motherland</trd>,
    <trd>fatherland</trd>
  </trdgrp>
  <trd lng="es">patria</trd>
  <trd lng="fr">patrie</trd>
  <trd lng="hu">haza</trd>
  <trd lng="it">patria</trd>
  <trd lng="nl">vaderland</trd>
  <trd lng="no">fedreland</trd>
  <trd lng="pl">ojczyzna</trd>
  <trd lng="pt">p&aacute;tria</trd>
  <trd lng="ru">&c_r;&c_o;&c_d;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>domovina</trd>,
    <trd>ot&ccaron;ina</trd>,
    <trd>vlas&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="tr">
    <trd>vatan</trd>,
    <trd>yurt</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_t;&#x44C;&#x43A;&c_ib;&c_v;&c_shch;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_t;&c_ch;&c_i;&c_z;&#x43D;&c_a;</trd>,
    <trd>&c_r;&c_ib;&c_d;&#x43D;&c_i;&c_j; &#x43A;&c_r;&c_a;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7956;&#x56FD; <pr>z&#x1D4;gu&#xF3;</pr></trd>,
    <trd>&#x7956;&#x570B; <pr>z&#x1D4;gu&#xF3;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="patri.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="patri.0ismo.MOR">
    <dif>
      <ref tip="dif" cel="patrio.0ismo.MOR">Patriotismo</ref>:
      <ekz>
        por tiaj homoj la dialektoj estas pereigindaj malamikoj de la 
        <tld/>ismo
        <fnt>
          <aut>Blanka Meduzo</aut>,
          <vrk><url ref="http://d.hatena.ne.jp/shiro-kurage/mobile?date=20100618">
          Hatena de Esperanto</url></vrk>,
          <lok>2010-06-18</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_t;&c_r;&c_y;&c_ja;&c_t;&c_y;&c_z;&c_m;</trd>
  <trdgrp lng="de">
    <trd>Patriotismus</trd>,
    <trd>Vaterlandsliebe</trd>
  </trdgrp>
  <trd lng="en">patriotism</trd>
  <trd lng="fr">patriotisme</trd>
  <trdgrp lng="hu">
    <trd>hazafis&aacute;g</trd>,
    <trd>hazaszeretet</trd>,
    <trd>patriotizmus</trd>
  </trdgrp>
  <trd lng="it">patriottismo</trd>
  <trdgrp lng="nl">
    <trd>patriotisme</trd>,
    <trd>patriottisme</trd>,
    <trd>vaderlandsliefde</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_t;&c_r;&c_i;&c_o;&c_t;&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="zh">
    <trd>&#x611B;&#x570B;&#x4E3B;&#x7FA9; <pr>&#xE0;igu&#xF3;zh&#x1D4;y&#xEC;</pr></trd>,
    <trd>&#x7231;&#x56FD;&#x4E3B;&#x4E49; <pr>&#xE0;igu&#xF3;zh&#x1D4;y&#xEC;</pr></trd>
  </trdgrp>
</drv>
</art>
