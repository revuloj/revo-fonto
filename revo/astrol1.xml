<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: astrol1.xml,v 1.24 2023/10/28 08:33:09 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>astrolog</rad>/o
</kap>
<drv mrk="astrol1.0o">
  <kap><tld/>o</kap>
  <snc mrk="astrol1.0o.astrologiisto">
    <dif>
      <ref tip="dif" cel="astrol2.0isto">Astrologiisto</ref>:
      <ekz>
        en la jaro 1186 la <tld/>oj ektimigis la popolojn per anta&ubreve;diro, ke balda&ubreve;
        fari&gcirc;os kuni&gcirc;o de &ccirc;iuj planedoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la re&gcirc;o ordonis alvoki la <tld/>ojn, magiistojn, sor&ccirc;istojn
        <fnt>
          <bib>MT</bib>
          <lok>Daniel 2:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o venis prezenti al la sinjoro raporton pri la situacio de la steloj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i petis <tld/>on difini la daton de funebro
        <fnt><bib>RugxDom</bib> <lok>&ccirc;apitro 63a, p. 377a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x5360;&#x661F;&#x5BB6; <pr>zh&#xE0;nx&#x12B;ngji&#x101;</pr></trd>
</snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trdgrp lng="cs">
    <trd>astrolog</trd>,
    <trd>hv&ecaron;zdopravec</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Astrologe</trd>,
    <trd>Sterndeuter</trd>
  </trdgrp>
  <trd lng="en">astrologist</trd>
  <trd lng="es">astr&oacute;logo</trd>
  <trdgrp lng="fa">
    <trd>&a_A;&a_kh;&a_t;&a_r;&a_b;&f_y;&a_n;</trd>,
    <trd>&a_s;&a_t;&a_A;&a_r;&a_h;&zwnj;&a_b;&f_y;&a_n;</trd>,
    <trd>&a_T;&a_A;&a_l;&a_ayn;&zwnj;&a_b;&f_y;&a_n;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>astrologue</trd>
  </trdgrp>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E1;&#x5D8;&#x5E8;&#x5D5;&#x5B9;&#x5DC;&#x5D5;&#x5B9;&#x5D2;</trd>
  <trdgrp lng="hu">
    <trd>asztrol&oacute;gus</trd>,
    <trd>csillagj&oacute;s</trd>
  </trdgrp>
  <trd lng="it">astrologo</trd>
  <trd lng="ja">&#x5360;&#x661F;&#x8853;&#x5E2B;  <pr>&#x305B;&#x3093;&#x305B;&#x3044;&#x3058;&#x3085;&#x3064;&#x3057;</pr></trd>
  <trd lng="pl">astrolog</trd>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_r;&c_o;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">astrol&oacute;g</trd>
  <trd lng="uk">&#x430;&#x441;&#x442;&#x440;&#x43E;&#x43B;&#x43E;&#x433;</trd>
</drv>
</art>
