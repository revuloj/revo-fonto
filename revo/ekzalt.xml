<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ekzalt.xml,v 1.28 2025/03/29 13:06:04 revo Exp $">
<kap>
  <rad>ekzalt</rad>/i <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="ekzalt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="ekzalt.0i.PSI">
    <dif>
      Levi <ref cel="sent.0o.emocio">sento</ref>n
      de la animo &gcirc;is alta grado de intenseco, tre
      <ref tip="super" cel="ekscit.0i.anime">eksciti</ref>:
      <ekz>
        vi estas <tld/>ita, strange naiva, mia kara, vi estas ankora&ubreve; granda infano
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        sin<tld/>a
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk><url ref="&Steloj;butiko">La Stranga Butiko</url></vrk>
          <lok>Lamentado</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis <tld/>ita &ndash; &ccirc;iuj <klr>[&#8230;]</klr> la&ubreve;dis lin, de la apla&ubreve;doj li ebrii&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Angela Tellier</aut>
          <vrk><url ref="&Monato;/2004/008306.html">La&ubreve;proverbaj fabeloj</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Livia</nom> rigardas <tld/>ate la ekzekuton
        <fnt>
          <bib>Monato</bib>
          <aut>Donald Broadribb</aut>
          <vrk><url ref="&Monato;/2005/008750.php">Novelo romantikeca kaj iom
          satira</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        spiriteco estas rezulto de <tld/>aj, sublimaj momentoj travivitaj
        <fnt>
          <bib>Monato</bib>
          <aut>W. H. Simcock</aut>
          <vrk><url ref="&Monato;/2007/009197.php">Kio estas spiriteco?</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas malfacile por kosmopolitoj kompreni la <tld/>adon de belo-bono en naciismon
        <fnt>
          <aut>Gilbert Ledon</aut>
            <vrk><url ref="&Monato;/2000/007018.php">Kulturo de milito</url></vrk>
            <bib>Monato</bib>
            <lok>2000/06, p. 5</lok>
          </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fam">famigi</ref>,
      <ref cel="pas">pasiigi</ref>,
      <ref cel="ekscit.0i">eksciti</ref>.
    </refgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>entuziasmigi</ke>, <ke>trovigligi</ke>.
  </rim>

  <trd lng="be">&c_e;&c_k;&c_z;&c_a;&c_l;&c_mol;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">exaltar</trd>
  <trdgrp lng="cs">
    <trd>nadchnout</trd>,
    <trd>opojit</trd>,
    <trd>vzru&scaron;it</trd>,
    <trd>vzru&scaron;ovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>aufregen</trd>,
    <trd>exaltieren</trd>,
    <trd>in <ind>Aufregung</ind> versetzen</trd>,
    <trd>in <ind>Hysterie</ind> versetzen</trd>,
    <trd>in exaltierte <ind>Stimmung</ind> versetzen</trd>
  </trdgrp>
  <trd lng="en">exalt</trd>
  <trd lng="es">exaltar</trd>
  <trd lng="fr">exalter</trd>
  <trdgrp lng="ja">
    <trd>&#39640;&#25562;&#12373;&#12379;&#12427;  <pr>&#12371;&#12358;&#12424;&#12358;&#12373;&#12379;&#12427;</pr></trd>,
    <trd>&#29105;&#29378;&#12373;&#12379;&#12427;  <pr>&#12397;&#12387;&#12365;&#12423;&#12358;&#12373;&#12379;&#12427;</pr></trd>,
    <trd>&#24375;&#12367;&#12377;&#12427;  <pr>&#12388;&#12424;&#12367;&#12377;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="pl">egzaltowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>exaltar</trd>,
    <trd>excitar</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_z;&c_a;&c_l;&c_mol;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>ve&lcaron;mi nadchn&uacute;&tcaron;</trd>,
    <trd>vzru&scaron;i&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_je;&#1082;&c_z;&c_a;&#1083;&#1100;&c_t;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_d;&#1086;&c_v;&#1086;&c_d;&c_i;&c_t;&c_i; &c_d;&#1086; &c_s;&c_t;&c_a;&#1085;&c_u; &#1082;&c_r;&c_a;&c_j;&#1085;&#1100;&#1086;&c_g;&#1086;
    &c_z;&c_b;&c_u;&c_d;&c_zh;&c_je;&#1085;&#1085;&#1103;</trd>,
    <trd>&c_r;&#1086;&c_z;&#1087;&c_a;&#1083;&#1102;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_b;&c_u;&c_d;&c_zh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#27985;&#36523;&#21457;&#25238; <pr>h&#250;nsh&emacron;nf&amacron;d&#466;u</pr></trd>
</drv>

<drv mrk="ekzalt.0o">
  <kap><tld/>o, <var><kap><tld/>i&gcirc;o</kap></var></kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="ekzalt.0o.PSI">
    <dif>
      Stato de granda <ref tip="super" cel="vigl.0o">vigleco</ref> de sentoj,
      de <ref tip="vid" cel="ard.0o">ardo</ref>,
      <ref tip="vid" cel="ebri.0o">ebrieco</ref>,
      <ref tip="vid" cel="fajr.0o.fervoro">fajro</ref>,
      <ref tip="vid" cel="febr.0o.fervoro">febro</ref>:
      <ekz>
        poezia <tld/>o
        <fnt>
          <aut>G. De Knibber, trad. L. De Cooman</aut>,
          <vrk><url ref="http://de-cooman.be/jan/eo/landschap.htm">
          Jan De Cooman: Pejza&gcirc;oj</url></vrk>,
          <lok>1998</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi ja atendis ion, kiel eksciti&gcirc;on, <tld/>i&gcirc;on
        <fnt>
          <aut>C. Piron</aut>,
          <vrk><url ref="http://esperanto.org.uk/eldonoj/piron/tekstoj/Tien.html">
          Tien</url></vrk>,
          <lok>1997</lok>
        </fnt>;
      </ekz>
      <ekz>
        la rakonto prognozas <klr>[&#8230;]</klr> kriman <tld/>i&gcirc;on de frenezaj knabetoj, kiuj
        pasigis horojn anta&ubreve; komputilekrano, enmergi&gcirc;intaj en virtualan violentan
        ludon
        <fnt>
          <bib>Monato</bib>
          <aut>Paulo Silas</aut>
          <vrk><url ref="&Monato;/2015/011507.php">La lastaj tagoj de Lillo</url></vrk>
          <lok>2015</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="frenez">frenezo</ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="ekstaz.0o">ekstazo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_z;&c_a;&c_l;&c_mol;&c_t;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Aufregung</trd>,
    <trd>innere <ind>Aufruhr</ind></trd>,
    <trd>&Uuml;berspanntheit</trd>
  </trdgrp>
  <trd lng="fr">exaltation</trd>
  <trdgrp lng="ja">
    <trd>&#39640;&#25562;  <pr>&#12371;&#12358;&#12424;&#12358;</pr></trd>,
    <trd>&#33288;&#22894;  <pr>&#12371;&#12358;&#12405;&#12435;</pr></trd>,
    <trd>&#29105;&#29378;  <pr>&#12397;&#12387;&#12365;&#12423;&#12358;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_je;&#1082;&c_z;&c_a;&#1083;&#1100;&c_t;&c_a;&c_c;&c_ib;&#1103;</trd>
  <trd lng="zh">&#21160;&#30442; <pr>d&#242;ngd&#224;ng</pr></trd>
</drv>
</art>
