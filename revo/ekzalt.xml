<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>ekzalt</rad>/i
  <fnt>Z</fnt>
</kap>
<drv mrk="ekzalt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Levi senton de la animo &gcirc;is alta grado de intenseco, tre
      eksciti:
      <ekz><tld/>i fervoregon, imagon de iu;</ekz>
      <ekz><tld/>ita kapo, spirito.</ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fam">famigi</ref>,
      <ref cel="pas">pasiigi</ref>,
      <ref cel="entuzi.0o">entuziasmo</ref>,
      <ref cel="frenez">frenezo</ref>,
      <ref cel="fervor.0o">fervoro</ref>,
      <ref cel="ekstaz.0o">ekstazo</ref>,
      <ref cel="ekscit.0i">eksciti</ref>.
    </refgrp>
  </snc>
  <trd
  lng="be">&c_e;&c_k;&c_z;&c_a;&c_l;&c_mol;&c_t;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
     <trd>aufregen</trd>,
     <trd>exaltieren</trd>,
     <trd>in <ind>Aufregung versetzen</ind></trd>,
     <trd>in <ind>Hysterie versetzen</ind></trd>,
     <trd>in exaltierte <ind>Stimmung versetzen</ind></trd>
  </trdgrp>
  <trd lng="en">exalt</trd>
  <trd lng="es">exaltar</trd>
  <trd lng="fr">exalter</trd>
  <trd lng="pl">egzaltowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>exaltar</trd>,
    <trd>excitar</trd>
  </trdgrp>
  <trd
  lng="ru">&c_e;&c_k;&c_z;&c_a;&c_l;&c_mol;&c_t;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
