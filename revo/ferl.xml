<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ferl</rad>/i
</kap>
<drv mrk="ferl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Volvi velon k alligi &gcirc;in sur jardon. 
    </dif>
  </snc>
  <trdgrp lng="ru">
    <trd>furl</trd>,
    <trd>roll up</trd>,
    <trd>take in</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol; &c_p;&c_a;&c_r;&c_u;&c_s;&c_a;</trd>
</drv>
<drv mrk="ferl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i k rezulto de tiu ago.  
    </dif>
  </snc>
</drv>
</art>
