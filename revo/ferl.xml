<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ferl.xml,v 1.17 2023/10/14 18:06:40 revo Exp $">
<kap>
  <rad>ferl</rad>/i
</kap>
<drv mrk="ferl.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Volvi velon kaj alligi &gcirc;in sur jardon.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>ohnout</trd>,
    <trd>podkasat</trd>,
    <trd>st&#xE1;hnout plachtu</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>festmachen</trd>,
    <trd>festzurren</trd>,
    <trd>einziehen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>furl</trd>,
    <trd>roll up</trd>,
    <trd>take in</trd>
  </trdgrp>
  <trd lng="es">recoger velas<klr tip="amb"> (at&aacute;ndolas en la
  verga)</klr></trd>
  <trd lng="fr">serrer<klr tip="ind"> (une voile)</klr></trd>
  <trdgrp lng="hu">
    <trd>bevon<klr tip="ind"> (vitorl&aacute;t)</klr></trd>,
    <trd>felteker<klr tip="ind"> (vitorl&aacute;t)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x305F;&#x305F;&#x3080;</trd>,
    <trd>&#x7E2E;&#x5E06;&#x3059;&#x308B;  <pr>&#x3061;&#x3062;&#x307B;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ferrar velas</trd>,
    <trd>colher velas</trd>,
    <trd>amarrar velas</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol; &c_p;&c_a;&c_r;&c_u;&c_s;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>podkasa&#x165;</trd>,
    <trd>stiahnu&#x165; plachtu</trd>
  </trdgrp>
  <trd lng="uk">&#x437;&#x433;&#x43E;&#x440;&#x442;&#x430;&#x442;&#x438; &#x432;&#x456;&#x442;&#x440;&#x438;&#x43B;&#x430;</trd>
</drv>

<drv mrk="ferl.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i kaj rezulto de tiu ago.
    </dif>
  </snc>
  <trd lng="cs">podkas&#xE1;n&#xED;</trd>
  <trdgrp lng="de">
    <trd>Festmachen</trd>,
    <trd>Festzurren</trd>,
    <trd>Einziehen</trd>
  </trdgrp>
  <trd lng="hu">vitorlabevon&aacute;s</trd>
  <trd lng="ja">&#x7E2E;&#x5E06;  <pr>&#x3057;&#x3085;&#x304F;&#x306F;&#x3093;</pr></trd>
  <trd lng="sk">podkasanie</trd>
  <trd lng="uk">&#x437;&#x433;&#x43E;&#x440;&#x442;&#x430;&#x43D;&#x43D;&#x44F; &#x432;&#x456;&#x442;&#x440;&#x438;&#x43B;</trd>
</drv>
</art>
