            <vrk>Mitoj kaj faktoj pri Esperanto</vrk>
            <lok>Nova Esperanta Krestomatio, 1991, p. 266.</lok>
          </fnt>.
        </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_d;&c_s;&c_t;&c_a;&c_v;&c_a; &c_d;&c_l;&c_ja; &c_ib;&c_s;&c_n;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_ja;</trd>
