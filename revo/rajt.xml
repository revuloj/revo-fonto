<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>rajt</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="rajt.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="rajt.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Povo, pravigita de le&gcirc;o a&ubreve; moro, nepre fari ion
      a&ubreve;
      postuli ion de aliuloj:
      <ekz>
        nur la devoj estas la fundamento de la <tld/>oj;
      </ekz>
      <ekz>
        la patro havas la <tld/>on puni siajn infanojn<fnt>B</fnt>;
      </ekz>
      <ekz>
        havi la <tld/>on de vo&ccirc;donado;
      </ekz>
      <ekz>
        mi havas <tld/>on sur la danan tronon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi havas plenan <tld/>on uzi tiun formon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        kiu havas forton, havas <tld/>on
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        defendi siajn <tld/>ojn;
      </ekz>
      <ekz>
        perdi siajn politikajn <tld/>ojn;
      </ekz>
      <ekz>
        Ligo pri la homaj <tld/>oj;
      </ekz>
      <ekz>
        <tld/>o de sindefendo;
      </ekz>
      <ekz>
        <tld/>oj de a&ubreve;toreco<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="privil.0o">privilegio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_v;&c_a;</trd>
  <trd lng="cs">pr&aacute;vo</trd>
  <trd lng="de">Recht</trd>
  <trd lng="en">right</trd>
  <trd lng="es">derecho<klr tip="amb"> (jur&iacute;dico)</klr></trd>
  <trd lng="fr">droit<klr> (juridique)</klr></trd>
  <trd lng="he">&zayin;&kaf;&vav;&tav;</trd>
  <trd lng="hu">jog <klr tip="amb">(jogosults&aacute;g)</klr></trd>
  <trd lng="nl">recht</trd>
  <trd lng="pl">prawo</trd>
  <trd lng="pt">direito</trd>
  <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_o;</trd>
  <trd lng="tp">ken</trd>
</drv>

<drv mrk="rajt.0e">
  <kap><tld/>e</kap>
  <snc mrk="rajt.0e.JUR">
    <dif>
      Konforme al ia <tld/>o:
      <ekz>
        <tld/>e plendi;
      </ekz>
      <ekz>
        sen<tld/>e agi;
      </ekz>
      <ekz>
        <tld/>e posedi.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">rechtens</trd>
  <trdgrp lng="fr">
    <trd>&agrave; bon droit</trd>,
    <trd>de plein doit</trd>
  </trdgrp>
  <trd lng="he">&bet;<ind>&resh;&shin;&vav;&tav;</ind></trd>
  <trdgrp lng="hu">
    <trd>joggal</trd>,
    <trd>jogosan</trd>
  </trdgrp>
  <trd lng="nl">rechtens</trd>
  <trdgrp lng="pl">
    <trd>prawnie</trd>,
    <trd>zgodnie z <ind>prawem</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o; &c_p;&c_r;&c_a;&c_v;&c_u;</trd>
</drv>

<drv mrk="rajt.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="rajt.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Havi la <tld/>on pri:
      <ekz>
        <tld/>i pri io, <tld/>i ion<fnt>Z</fnt>,
      </ekz>
      <ekz>
        <tld/>i fari;
      </ekz>
      <ekz>
        <ind>kion <tld/>as leono, ne <tld/>as azeno</ind>
        <fnt><bib>PrV</bib></fnt>;
        <trd lng="hu">amit szabad Jupiternek, nem szabad a
        hat&ouml;k&ouml;rnek</trd>
      </ekz>
      <ekz>
        vi <tld/>as ricevi kompenson pro la perdo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_je;&c_c;&c_mol;
  <ind>&c_p;&c_r;&c_a;&c_v;&c_a;</ind></trd>
  <trdgrp lng="cs">
    <trd>m&iacute;t pr&aacute;vo</trd>,
    <trd>sm&ecaron;t</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>das Recht haben</trd>,
    <trd>d&uuml;rfen</trd>
  </trdgrp>
  <trd lng="es">tener derecho</trd>
  <trd lng="fr">avoir le droit</trd>
  <trdgrp lng="he">
    <trd>&lamed;&he;&yod;&vav;&tav; <ind>&zayin;&kaf;&alef;&yod;</ind>
    &lamed;-</trd>,
    <trd>&lamed;&he;&yod;&vav;&tav; <ind>&resh;&shin;&alef;&yod;</ind>
    &lamed;-</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>joga van</trd>,
    <trd>jogosult</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>het <ind>recht</ind> hebben</trd>,
    <trd>mogen</trd>
  </trdgrp>
  <trd lng="pl">mie&cacute; <ind>prawo</ind></trd>
  <trd lng="ru">&c_i;&c_m;&c_je;&c_t;&c_mol;
  &c_p;&c_r;&c_a;&c_v;&c_o;</trd>
  <trd lng="tp">ken</trd>
</drv>

<drv mrk="rajt.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rajt.0igi.JUR_iun">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Doni al iu ian <tld/>on:
      <ekz>
        mi <tld/>igas vin vo&ccirc;doni en mia nomo<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="de">berechtigen</trd>
    <trd lng="he">&lamed;&he;<ind>&resh;&shin;&vav;&tav;</ind></trd>
    <trdgrp lng="hu">
      <trd>feljogos&iacute;t</trd>,
      <trd>felhatalmaz</trd>
    </trdgrp>
    <trd lng="nl">machtigen</trd>
    <trdgrp lng="pl">
      <trd>uprawnia&cacute;</trd>,
      <trd>dawa&cacute; <ind>prawo</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="rajt.0igi.JUR_ion">
    <uzo tip="fak">JUR</uzo>
    <dif>
      Igi ion la&ubreve;<tld/>a, oficiale konfirmi:
      <ekz>
        <tld/>iga dokumento
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="konfir.0i">konfirmi</ref>,
      <ref cel="sankci.0i">sankcii</ref>
    </refgrp>
    <trd lng="he">&lamed;<ind>&alef;&shin;&resh;</ind></trd>
    <trdgrp lng="hu">
      <trd>enged&eacute;lyez</trd>,
      <trd>elismer (jogosnak)</trd>,
    </trdgrp>
    <trd lng="nl">autoriseren</trd>
    <trdgrp lng="pl">
      <trd>uprawnia&cacute;</trd>,
      <trd>dawa&cacute; <ind>prawo</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_u;&c_p;&c_a;&c_w;&c_n;&c_a;&c_v;&c_a;&c_zh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_c;&c_mol;
    <ind>&c_p;&c_r;&c_a;&c_v;&c_a;</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>autoriser</trd>,
    <trd>permettre</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_u;&c_p;&c_o;&c_l;&c_n;&c_o;&c_m;&c_o;&c_ch;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_n;&c_a;&c_d;&c_je;&c_l;&c_i;&c_t;&c_mol; &c_p;&c_r;&c_a;&c_v;&c_o;&c_m;</trd>
  </trdgrp>
</drv>

<drv mrk="rajt.0igilo">
  <kap><tld/>igilo</kap>
  <snc mrk="rajt.0igilo.JUR">
    <uzo tip="fak">JUR</uzo>
    <ref tip="dif" cel="permes.0ilo.KOMUNE">permesilo</ref>
      <ekz>
        post montri siajn oficialajn <tld/>igilojn,
        ili diras siajn nomojn: Jefferson kaj Davis
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://fenomenonifo.wordpress.com/2009/09/21/afero-ilkley-moor/">
          Afero Ilkley Moor</url></vrk>,
          <lok>Nifo-dosieroj, 2009-09-21</lok>
        </fnt>.
      </ekz>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_z;&c_v;&c_o;&c_l; <klr tip="amb">(&c_d;&c_a;&c_k;&c_u;&c_m;&c_a;&c_n;&c_t;)</klr></trd>,
    <trd>&c_l;&c_ib;&c_c;&c_e;&c_n;&c_z;&c_ib;&c_ja;</trd>
  </trdgrp>
  <trd lng="br">aotre<klr tip="ind"> (skrid-testeni)</klr></trd>
  <trdgrp lng="en">
    <trd>permit</trd>,
    <trd>license</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_g;&a_w;&a_A;&a_h;&f_y;&a_n;&a_A;&a_m;&a_h;</trd>,
    <trd>&f_p;&a_r;&a_w;&a_A;&a_n;&a_h;<klr tip="ind"> (&a_m;&a_j;&a_w;&a_z;)</klr></trd>,
    <trd>&a_j;&a_w;&a_A;&a_z;</trd>,
    <trd>&a_m;&a_j;&a_w;&a_z;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>autorisation<klr> (document)</klr></trd>,
    <trd>brevet</trd>,
    <trd>licence<klr> (permis)</klr></trd>,
    <trd>permis<klr> (document)</klr></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Erlaubnis</trd>,
    <trd>Lizenz</trd>
  </trdgrp>
  <trd lng="he">&resh;&shin;&yod;&vav;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>enged&eacute;ly</trd>,
    <trd>igazolv&aacute;ny</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>autorizzazione<klr> (documento)</klr></trd>,
    <trd>permesso<klr> (documento)</klr></trd>,
    <trd>attestato<klr> (documento di autorizzazione)</klr></trd>,
    <trd>concessione<klr> (documento)</klr></trd>
    <trd>licenza<klr> (documento)</klr></trd>
  </trdgrp>
  <trd lng="nl">brevet</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_r;&c_je;&c_sh;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_l;&c_i;&c_c;&c_je;&c_n;&c_z;&c_i;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>tillst&aring;nd</trd>,
    <trd>licens</trd>
  </trdgrp>
  <trd lng="tr">izin</trd>
</drv>

<drv mrk="rajt.auxtor0o">
  <kap>a&ubreve;tor<tld/>o</kap>
  <snc mrk="rajt.auxtor0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      En civila juro, la ekskluzivaj <tld/>oj de a&ubreve;toro de verko
      scienca, arta a&ubreve; literatura kaj de liaj juraj posteuloj
      disponi
      dum difinita tempo pri la uzo, publikigo, reproduktado
      ktp de tiu verko:
      <ekz>
        por programaro kies a&ubreve;tor<tld/>on tenas &leftquot;Free
        Software
        Foundation&rightquot;, petu
        permeson de &leftquot;Free Software Foundation&rightquot;
        <fnt><lok><url ref="http://www.esperanto.mv.ru/Cetero/gpl.html"
        >GPL</url></lok></fnt>;
      </ekz>
      <ekz>
        se necesas, vi devas anka&ubreve; ricevi de via dunganto (se vi
        estas
        dungita programisto) <klr>...</klr> skriban malpretendon je la
        a&ubreve;tor<tld/>o
        <fnt><lok><url ref="http://www.esperanto.mv.ru/Cetero/gpl.html"
        >GPL</url></lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_r;&c_s;&c_k;&c_a;&c_je; <ind>&c_p;&c_r;&c_a;&c_v;&c_a;</ind></trd>
  <trd lng="de">Urheberrecht</trd>
  <trd lng="en">copyright</trd>
  <trdgrp lng="fr">
    <trd>copyright</trd>,
    <trd>droit d'auteur</trd>
  </trdgrp>
  <trd lng="he">&zayin;&kaf;&vav;&yod;&vav;&tav;
  &yod;&vav;&tsadi;&resh;&yod;&fmem;</trd>
  <trd lng="hu">szerz&odblac;i <ind>jog</ind></trd>
  <trd lng="nl">auteursrecht</trd>
  <trd lng="pl">prawo autorskie</trd>
  <trd lng="ru">&c_a;&c_v;&c_t;&c_o;&c_r;&c_s;&c_k;&c_o;&c_je; &c_p;&c_r;&c_a;&c_v;&c_o;</trd>
</drv>

<drv mrk="rajt.azil0o">
  <kap>azil<tld/>o, <var><kap>rifu&gcirc;<tld/>o</kap></var></kap>
  <snc mrk="rajt.azil0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <tld lit="R"/>o de persekutato al sekureco en difinita
      rifu&gcirc;loko:    
      <ekz>
        azil<tld/>o: la <tld/>o obteni azilon estas garantiita, 
        respektante la dispoziciojn de la &Gcirc;eneva Konvencio de la
        28a de
        
        Julio 1951
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://wikisource.org/wiki/Euxropa_Konstitucio_dua_parto">
          E&ubreve;ropa Konstitucio, dua parto</url></vrk>,
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        okcent romaoj submetis al Grekio peton pri rifu&gcirc;<tld/>o
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.romnet.hu/hirek/eszp0307042.html">
          Krizo pri romaaj rifu&gcirc;intoj...</url></vrk>,
          <lok>RomNet, 2003-05-20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">droit d'asile</trd>
  <trd lng="nl">asielrecht</trd>
  <trd lng="pl">prawo azylu</trd>
</drv>

<drv mrk="rajt.balot0o">
   <kap>balot<tld/>o</kap>
   <snc mrk="rajt.balot0o.POL">
    <uzo tip="fak">POL</uzo>
     <dif>
       <tld lit="R"/>o <ref tip="vid" cel="balot.0i">baloti</ref>.
     </dif>
   </snc>
   <trdgrp lng="de">
     <trd>Wahlrecht</trd>,
     <trd>Stimmrecht</trd>
   </trdgrp>
   <trd lng="fr">droit de vote</trd>
   <trdgrp lng="en">
     <trd>right to <ind>vote</ind></trd>,
     <trd>suffrage</trd>
   </trdgrp>
   <trd lng="hu">szavazati jog</trd>
   <trd lng="es">derecho al sufragio</trd>
   <trd lng="nl">stemrecht</trd>
   <trd lng="pl"><klr>(aktywne) </klr>prawo <ind>wyborcze</ind></trd>
   <trdgrp lng="ru">
     <trd>&c_p;&c_r;&c_a;&c_v;&c_o; &c_g;&c_o;&c_l;&c_o;&c_s;&c_a;
     <klr>(&c_r;&c_je;&c_sh;&c_a;&c_ju;&c_shch;&c_je;&c_g;&c_o;)</klr></trd>;
     <trd>&c_p;&c_r;&c_a;&c_v;&c_o; &c_i;&c_z;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol;</trd>,
     <trd><klr>(&c_a;&c_k;&c_t;&c_i;&c_v;&c_n;&c_o;&c_je;)</klr>
     <ind>&c_i;&c_z;&c_b;&c_i;&c_r;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_n;&c_o;&c_je; &c_p;&c_r;&c_a;&c_v;&c_o;</ind></trd>
   </trdgrp>
</drv>

<drv mrk="rajt.egal0a">
  <kap>egal<tld/>a</kap>
  <snc mrk="rajt.egal0a.JUR">
    <dif>
      Havanta egalan <tld/>on:
      <ekz>
        lingvo internacia prezentas egal<tld/>an propra&jcirc;on de la
        tuta mondo, apartenanta speciale al neniu raso
        <fnt>Z</fnt>;
      </ekz>
      <ekz>
        en multaj landoj la virinoj akiris la
        egal<tld/>econ
        <fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_r;&c_a;&c_w;&c_n;&c_a;&c_p;&c_r;&c_a;&c_w;&c_n;&c_y;</trd>
  <trd lng="fr">&eacute;gal de droit</trd>
  <trd lng="he">&shin;&vav;&vav;&he;
  &zayin;&kaf;&vav;&yod;&vav;&tav;</trd>
  <trd lng="hu">egyenjog&uacute;</trd>
  <trd lng="nl">gelijkberechtigd</trd>
  <trd lng="pl">r&oacute;wnouprawniony</trd>
  <trd
  lng="ru">&c_r;&c_a;&c_v;&c_n;&c_o;&c_p;&c_r;&c_a;&c_v;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="rajt.kopi0o">
  <kap>kopi<tld/>o</kap>
  <snc mrk="rajt.kopi0o.auxtorrajto">
    <ref tip="dif" cel="rajt.auxtor0o">a&ubreve;tor<tld/>o</ref>
  </snc> 
  <trd lng="nl">auteursrecht</trd> 
</drv>
</art>
