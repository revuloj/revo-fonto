<art mrk="$Id: ostologi.xml,v 1.4 2023/09/03 07:12:35 revo Exp $">
<kap>
  <rad>ostologi</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="ostologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="ostologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Scienco pri <ref tip="vid" cel="ost.0o.ANA">ostoj</ref>
      kaj pri kuracado de ostoj:
      <ekz>
        <tld/>a fako de <klr>...</klr> hospitalo  
        <fnt>
          <aut>Kuang Bo, Lian &Scirc;i</aut>,
          <vrk><url ref="http://www.espero.com.cn/2014-02/07/content_31394620.htm">
          Pri nova terapio de ostorompi&gcirc;o</url></vrk>,
          <lok>El Popola &Ccirc;inio, 2014-02-07</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">osteology</trd>
  <trd lng="fr">ost&eacute;ologie</trd>
  <trd lng="ja">&#x9AA8;&#x5B66;  [<ind>&#x3053;&#x3064;&#x304C;&#x304F;</ind>]</trd>
  <trdgrp lng="pl">
    <trd>osteologia</trd>,
    <trd>ostologia</trd>
  </trdgrp>
  <trd lng="ro">osteologie</trd>
  <trdgrp lng="uk">
    <trd>&#x43E;&#x441;&#x442;&#x435;&#x43E;&#x43B;&#x43E;&#x433;&#x456;&#x44F;</trd>,
    <trd>&#x432;&#x447;&#x435;&#x43D;&#x43D;&#x44F; &#x43F;&#x440;&#x43E; &#x43A;&#x456;&#x441;&#x442;&#x43A;&#x438;</trd>
  </trdgrp>
</drv>

<drv mrk="ostologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="ostologi.0isto.MED">
    <dif>
      Specialisto pri <tld/>o;
      <ref tip="sin" cel="ostolog.0o.MED">ostologo</ref>.
    </dif>
  </snc>
  <trd lng="en">osteologist</trd>
  <trd lng="fr">ost&eacute;ologiste</trd>
  <trd lng="pl">ostolog</trd>
  <trd lng="ro">ostolog</trd>
</drv>
</art>

