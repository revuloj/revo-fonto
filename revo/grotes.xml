<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>grotesk</rad>/a
</kap>
<drv mrk="grotes.0a">
  <kap><tld/>a</kap>
  <snc mrk="grotes.0a.BELA">
    <dif>Konforma al grotesko.</dif>
    <trd lng="pt">grotesco<klr tip="ind"> (adj.)</klr></trd>
    <trd lng="ru">&c_g;&c_r;&c_o;&c_t;&c_je;&c_s;&c_k;&c_o;&c_v;&c_y;&c_j;</trd>
  </snc>
  <snc mrk="grotes.0a.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Ridinde <ref tip="super" cel="strang.0a">stranga</ref>, precipe pri
      aspekto a&ubreve; formoj:
      <ekz>
        <tld/>a karikaturo, gesto, sintenado, danco, viza&gcirc;o, homo.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="burles.0a">burleska</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>grotesk</trd>,
      <trd>&uuml;bersteigert</trd>
    </trdgrp>
    <trd lng="es">grotesco</trd>
    <trdgrp lng="hu">
      <trd>groteszk</trd>,
      <trd>furcsa</trd>,
      <trd>k&uuml;l&ouml;n&ouml;s</trd>,
      <trd>torz</trd>
    </trdgrp>
    <trd lng="nl">grotesk</trd>
    <trdgrp lng="pt">
     <trd>extravagante<klr tip="ind"> (adj.)</klr></trd>, 
     <trd>caricatural</trd> 
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_g;&c_r;&c_o;&c_t;&c_je;&c_s;&c_k;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_ch;&c_u;&c_d;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
    </trdgrp>
    <trd lng="sv">grotesk</trd>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_t;&c_e;&c_s;&c_k;&c_a;&c_v;&c_y;</trd>
  <trd lng="fr">grotesque</trd>
</drv>

<drv mrk="grotes.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Murpentra ornama stilo populara en la 1a jarcento en Romio,
      imitita en e&ubreve;ropa <ref tip="vid"
      cel="manier.0ismo">manierismo</ref>,
      el fantazia kunmeto de bestoj, folioj, floroj, homoj kaj aliaj.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_r;&c_a;&c_t;&c_e;&c_s;&c_k;</trd>
  <trd lng="fr">style <ind>grotesque</ind></trd>
  <trd lng="hu">groteszk<klr tip="ind"> (st&iacute;lus)</klr></trd>
  <trd lng="ru">&c_g;&c_r;&c_o;&c_t;&c_je;&c_s;&c_k;</trd>
</drv>
</art>
