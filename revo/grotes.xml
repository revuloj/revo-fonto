<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>grotesk</rad>/a
</kap>
<drv mrk="grotes.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Ridinde stranga, precipe pri aspekto a&ubreve; formoj:
      <ekz><tld/>a karikaturo, gesto, sintenado, danco, viza&gcirc;o, homo.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="burles.0a">burleska</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>grotesk</trd>,
     <trd>&uuml;bersteigert</trd></trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_o;&c_t;&c_je;&c_s;&c_k;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_ch;&c_u;&c_d;&c_l;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sv">grotesk</trd>
</drv>
</art>
