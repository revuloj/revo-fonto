<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>fiask</rad>/o
</kap>
<drv mrk="fiask.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Plena malsukceso:
      <ekz>renkonti <tld/>on<fnt>Z</fnt>.</ekz> 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Fiasko</trd>,
     <trd>Misserfolg</trd>,
     <trd>Pleite</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;chec</trd>,
    <trd>fiasco</trd>,
    <trd>insucc&egrave;s</trd>,
    <trd>ratage</trd></trdgrp> 
  <trd lng="nl">fiasco</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_i;&c_a;&c_s;&c_k;&c_o;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_v;&c_a;&c_l;</trd>,
    <trd>&c_n;&c_je;&c_u;&c_d;&c_a;&c_ch;&c_a;</trd>
  </trdgrp>
</drv>
<drv mrk="fiask.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Plene malsukcesi. 
    </dif>
    <refgrp tip="vid">
      <ref cel="fal.0i">fali</ref>,
      <ref cel="bankro.0i">bankroti</ref>,
      <ref cel="abort.0i">aborti</ref>,
      <ref cel="tomb">entombi&gcirc;i</ref>,
      <ref cel="ruin">ruini&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>fehlschlagen</trd>,
     <trd>Schiffbruch erleiden</trd>,
     <trd>scheitern</trd></trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;chouer</trd>,
    <trd>rater<klr tip="ind"> (intr., &eacute;chouer)</klr></trd></trdgrp> 
  <trd lng="nl">falen</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol; &c_f;&c_i;&c_a;&c_s;&c_k;&c_o;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_je;&c_r;&c_p;&c_je;&c_t;&c_mol; &c_n;&c_je;&c_u;&c_d;&c_a;&c_ch;&c_u;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_v;&c_a;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
