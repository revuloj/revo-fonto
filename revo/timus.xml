  </snc>
  <trdgrp lng="cs">
    <trd>brzl&iacute;k</trd>,
    <trd>tymus</trd>
  </trdgrp>
  <trd lng="en">thymus</trd>
  <trd lng="es">timo<klr> (gl&aacute;ndula)</klr></trd>
  <trd lng="fr">thymus</trd>
  <trd lng="sk">t&yacute;mus</trd>
  <trd lng="uk">&#x432;&#x438;&#x43B;&#x43E;&#x447;&#x43A;&#x43E;&#x432;&#x430; &#x437;&#x430;&#x43B;&#x43E;&#x437;&#x430;</trd>
</drv>
