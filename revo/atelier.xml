<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: atelier.xml,v 1.11 2023/05/12 15:25:57 revo Exp $">
<kap>
  <rad>atelier</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="atelier.0o">
  <kap><tld/>o</kap>
  <snc mrk="atelier.0o.BELA">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Iom vasta, hela laborejo, kian uzas <ref cel="pentr.0isto">pentristo</ref>
      a&ubreve; <ref cel="skulpt.0isto">skulptisto</ref>:
      <ekz>
        <nom>Grit</nom> purigos la <tld/>on
        <fnt>
          <aut>trad. C.M.C.</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Perle.htm">
          La junulino kun la perlo</url></vrk>,
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> instruis private en sia <tld/>o kaj fondis privatan
        pentrado-lernejon por virinoj
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2014/011271.php">Mar&scirc;i kun mortontoj</url></vrk>
          <bib>Monato</bib>
          <lok>2014/05, p. 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia <tld/>o estas mia &gcirc;ardeno
        <fnt>
          <aut>Franz-Georg R&ouml;ssler</aut>
            <vrk><url ref="&Monato;/2001/007301.php">Mia ateliero estas mia &gcirc;ardeno</url></vrk>
            <bib>Monato</bib>
            <lok>2001/01, p. 16</lok>
          </fnt>;
      </ekz>
      <ekz>
        la albanan skulptiston <nom>Gezim Hidri</nom>, laborantan en sia <tld/>o en Londono,
        vizitis subite anoj de la brita enmigra servo
        <fnt>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2008/009530.php">Hejmeniru!</url></vrk>
          <bib>Monato</bib>
          <lok>2008/08, p. 10</lok>
        </fnt>.
      </ekz>
    </dif>  
    <trdgrp lng="cs">
      <trd>ateli&eacute;r</trd>,
      <trd>pomocn&iacute;ci</trd>,
      <trd>p&rcaron;en. &zcaron;&aacute;ci</trd>,
      <trd>um&ecaron;leck&aacute; d&iacute;lna</trd>
    </trdgrp>
    <trd lng="fr">atelier<klr> (d'artiste)</klr></trd>
    <trd lng="sk">ateli&eacute;r</trd>
  </snc>
  <snc mrk="atelier.0o.studio">
    <uzo tip="fak">KIN</uzo><uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="dif" cel="studi.0o.KOMUNE">Studio</ref>, 
      elprovejo por arta eksperimentado:
      <ekz>
        la taverno krome estos teatro konstante proponanta folk-balojn, koncertojn kaj
        muzikajn <tld/>ojn
        <fnt>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2008/009487.php">Pri paeljoj kaj pentra&jcirc;oj</url></vrk>
          <bib>Monato</bib>
          <lok>2008/06, p. 5</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni surdiskigis &gcirc;in iam anta&ubreve;e en surdiskiga <tld/>o
        <fnt>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2013/010944.php">Kanto de la montoj</url></vrk>
          <bib>Monato</bib>
          <lok>2013/03, p. 12</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Kunstwerkstatt</trd>
    <trd lng="en">art workshop</trd>
    <trd lng="fr">atelier</trd>
  </snc>
  <snc mrk="atelier.0o.metiejo">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="meti.0ejo">Metiejo</ref>, metilernejo:
      <ekz>
        en sub&ccirc;ielaj <tld/>oj la infanoj lernas antikvajn metiojn (potfaradon,
        kotbrikmuldadon, mueladon ktp)
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008467.html">Kampoj de Soria Antonio Machado, universala
          poeto</url></vrk>
          <bib>Monato</bib>
          <lok>2004/12, p. 23</lok>
        </fnt>.
      </ekz>
    </dif>
    <!-- ne traduku evitindajn:
    <trd lng="de">Werkstatt</trd>
    <trd lng="en">workshop</trd>
    -->
  </snc>
  <trd lng="he">&#x5E1;&#x5D3;&#x5E0;&#x5D4;</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_t;&c_je;&#x43B;&#x44C;&c_jeu;</trd>,
    <trd>&c_h;&c_u;&c_d;&#x43E;&c_zh;&#x43D;&#x44F; &#x43C;&c_a;&c_j;&c_s;&c_t;&c_je;&c_r;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

