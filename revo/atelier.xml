<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>atelier</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="atelier.0o">
  <kap><tld/>o</kap>
  <snc mrk="atelier.0o.BELA">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Iom vasta, hela laborejo, kian uzas pentristo
      a&ubreve; skulptisto:
      <ekz>
        Grit purigos la <tld/>on
        <fnt>
          <aut>trad. C.M.C.</aut>,
          <vrk><url ref="http://www.senlime.be/Kino/Perle.htm">
          La junulino kun la perlo</url></vrk>,
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>ateliér</trd>,
    <trd>pomocníci</trd>,
    <trd>přen. žáci</trd>,
    <trd>umělecká dílna</trd>
  </trdgrp>
  <trd lng="fr">atelier d'artiste</trd>
  <trd lng="sk">ateliér</trd>
</drv>
</art>

