<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: auxtomatik.xml,v 1.4 2024/02/02 09:23:34 revo Exp $">
<kap>
  <rad>a&ubreve;tomatik</rad>/o <fnt><bib>PIV2</bib></fnt>
</kap>
<drv mrk="auxtomatik.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxtomatik.0o.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Arto programi
      <ref tip="vid" cel="auxtom.0o">a&ubreve;tomatojn</ref>:
      <ekz>
        raporto pri liaj esploroj pri <tld/>o
        aperis en la revuo <ctl>Scientific American</ctl>
        en 1915
        <fnt>
         <aut>T. del Barrio</aut>,
         <vrk><url ref="http://groups.yahoo.com/group/per-esperanto-bazascienco/message/222">
         Leonardo Torres Quevedo</url></vrk>,
         <lok>per-esperanto-bazascienco, 2002-04-12</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="robotik.0o.MAS">robotiko</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_t;&c_y;&c_k;&c_a;</trd>,
    <trd>&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_je; &c_k;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="fr">automatique<klr> (subst.)</klr></trd>
  <trdgrp lng="zh">
    <trd>&#x81EA;&#x52A8;&#21270;&#x88C5;&#x7F6E; <pr>z&#xEC;d&#xF2;nghu&#xE0;zhu&amacron;ngzh&#xEC;</pr></trd>,
    <trd>&#x81EA;&#x52D5;&#21270;&#x88DD;&#x7F6E; <pr>z&#xEC;d&#xF2;nghu&#xE0;zhu&amacron;ngzh&#xEC;</pr></trd>,
    <trd>&#x81EA;&#x52A8;&#x63A7;&#21046; <pr>z&#xEC;d&#xF2;ngk&#xF2;ngzh&#xEC;</pr></trd>,
    <trd>&#x81EA;&#x52D5;&#x63A7;&#21046; <pr>z&#xEC;d&#xF2;ngk&#xF2;ngzh&#xEC;</pr></trd>,
    <trd>&#x81EA;&#x52A8;&#x64CD;&#x4F5C; <pr>z&#xEC;d&#xF2;ngc&amacron;ozu&#xF2;</pr></trd>,
    <trd>&#x81EA;&#x52D5;&#x64CD;&#x4F5C; <pr>z&#xEC;d&#xF2;ngc&amacron;ozu&#xF2;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="auxtomatik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="auxtomatik.0isto.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Specialisto pri
      <ref tip="vid" cel="auxtomatik.0o.MAS"><tld/>o</ref>,
      pri teorio kaj realigo de a&ubreve;tomatoj:
      <ekz>
        la amba&ubreve;flanka konverto de Laplace <klr>...</klr>
        estas uzata interalie de <tld/>istoj
        kaj anka&ubreve; uzata en statistiko
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Laplaca_konverto">
          laplaca konverto</url>, 2013-01-13</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_w;&c_t;&c_a;&c_m;&c_a;&c_t;&c_y;&c_k;</trd>
  <trd lng="fr">automaticien</trd>
</drv>
</art>

