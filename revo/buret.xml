<vortaro>
  <art mrk="$Id: buret.xml,v 1.3 2024/04/15 13:24:37 afido Exp $">
    <kap>
      <rad>buret</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="buret.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      Vitra <ref tip="super" cel="tub.0o.TEK">tubo</ref> kun malferma&jcirc;o
      kaj skalo uzata por <ref cel="titr.0i">titrado</ref>:
      <ekz>
        <nom>Fran&ccedil;ois Descroizilles</nom> <klr>[&#8230;]</klr> disvolvis la unuan <tld/>on, kiun
        <nom>Louis
        Joseph Gay-Lussac</nom> plibonigis
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=495364"
              >Fran&ccedil;ois Descroizilles</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de <nom>Mohr</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=516218"
              >Karl Friedrich Mohr</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/b/b5/Titration_with_a_Mohr_burette_(Alessandri_1895.36).png/282px-Titration_with_a_Mohr_burette_(Alessandri_1895.36).png"
      prm="PD"
      >titrado per la <tld/>o de Mohr
        <fnt>
          <aut>valeg96, Alessandri, P. E. Analisi Volumetrica Applicata Ai Prodotti Commerciali e
          Industriali; Ulrico Hoepli: Milano, 1895.</aut>
          <lok><url ref="&WCW;/File:Titration_with_a_Mohr_burette_(Alessandri_1895.36).png"
              >&WCW;/File:Titration_with_a_Mohr_burette_(Alessandri_1895.36).png</url></lok>
        </fnt>
    </bld>
    <trd lng="de">B&uuml;rette</trd>
    <trd lng="en">burette</trd>
    <trd lng="fr">burette</trd>
    <trd lng="ru">&c_b;&c_ju;&c_r;&c_je;&c_t;&c_k;&c_a;</trd>
  </snc>
<trd lng="be">&c_b;&c_ju;&c_r;&c_e;&c_t;&c_k;&c_a;</trd>
</drv>

  </art>

