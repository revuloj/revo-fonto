<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: rongx.xml,v 1.29 2025/12/26 04:47:28 revo Exp $">
<kap>
  <ofc>9</ofc><rad>ron&gcirc;</rad>/i <fnt><bib>PIV1</bib></fnt>,
  <var><kap><rad var="d">rod</rad>/i <fnt><bib>PV</bib></fnt></kap><uzo tip="stl">RAR</uzo></var>
</kap>
<drv mrk="rongx.0i">
  <kap><ofc>9</ofc><tld/>i, <var><kap><tld var="d"/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rongx.0i.KOMUNE">
    <dif>
      <ref tip="sin" cel="mord.0eti">Mordeti</ref>, man&gcirc;i per malgrandaj
      pecetoj:
      <ekz>
        li havos en si la punon, la <tld/>antan vermon, kiu rifuzos
        morti tra
        nekalkulebla tempo
        <fnt>
          <bib>Fab4</bib>
          <lok>Turgardisto Ole</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;evaloj <klr>[&#8230;]</klr> vintre, malsategaj, <klr>[&#8230;]</klr>
        <tld/>as la arbo&scirc;elojn
        <fnt>
          <bib>Monato</bib>
          <aut>Lenke Sz&aacute;sz</aut>
          <vrk><url ref="&Monato;/2011/010508.php">Sova&gcirc;a
          sorto</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        skrupulo <klr>[&#8230;]</klr>, kiu <tld/>as kaj raspas mian
        konsciencon
        <fnt>
          <bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 1a, p.
          402a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&ubreve; <fnt><bib>BL</bib></fnt>: Anka&ubreve; eblas diri <ke>mordeti</ke>.
  </rim>

  <trd lng="be">&c_g;&c_r;&c_y;&c_z;&c_mol;&c_c;&c_ib;</trd>
  <trdgrp lng="ca">
    <trd>rosegar</trd>,
    <trd>corroir</trd>
  </trdgrp>
  <trd lng="de">nagen</trd>
  <trdgrp lng="en">
    <trd>gnaw</trd>,
    <trd>nibble</trd>
  </trdgrp>
  <trd lng="es">roer</trd>
  <trd lng="fa">&a_j;&a_w;&f_y;&a_d;&a_n;</trd>
  <trd lng="fr">ronger</trd>
  <trd lng="hu">r&aacute;g</trd>
  <trdgrp lng="id">
    <trd>kerat</trd>,
    <trd><baz>kerat</baz>mengerat</trd>,
    <trd>kerip</trd>,
    <trd><baz>kerip</baz>mengerip</trd>,
    <trd>kerikit</trd>,
    <trd><baz>kerikit</baz>mengerikiti</trd>,
    <trd>kerumit</trd>,
    <trd><baz>kerumit</baz>mengerumit</trd>,
    <trd>gerogot</trd>,
    <trd><baz>gerogot</baz>menggerogoti</trd>,
    <trd>unggis</trd>,
    <trd><baz>unggis</baz>mengunggis</trd>
  </trdgrp>
  <trd lng="ja">&#12363;&#12376;&#12427;</trd>
  <trd lng="nl">knagen</trd>
  <trdgrp lng="pl">
    <trd>obgryza&cacute;</trd>,
    <trd>podgryza&cacute;</trd>,
    <trd>chrupa&cacute;</trd>,
    <trd>gry&zacute;&cacute;</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>roer</trd>,
    <trd>mordiscar</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_r;&c_y;&c_z;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_r;&c_i;&c_z;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_b;&c_g;&c_r;&c_i;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_o;&c_ch;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_o;&c_z;&#8217;&c_ji;&c_d;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_ib;&c_d;&c_t;&c_o;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21827; <pr>k&ecaron;n</pr></trd>,
    <trd>&#21870; <pr>ni&egrave;</pr></trd>,
    <trd>&#22227; <pr>ni&egrave;</pr></trd>,
    <trd>&#21643; <pr>z&eacute;</pr></trd>,
    <trd>&#40789; <pr>h&eacute;</pr></trd>,
    <trd>&#40807;&#21676; <pr>ni&egrave;y&#462;o</pr></trd>,
    <trd>&#21870; <pr>ni&egrave;</pr></trd>,
    <trd>&#22169; <pr>ni&egrave;</pr></trd>,
    <trd>&#40789; <pr>h&eacute;</pr></trd>,
    <trd>&#40833; <pr>h&eacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rongx.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="rongx.0ulo.mamulo">
    <dif>
      Iu el <ref tip="malprt" cel="rongx.0uloj"><tld/>uloj</ref>.
    </dif>
    <trd lng="be">&c_g;&c_r;&c_y;&c_z;&c_u;&c_n;</trd>
    <trd lng="pl">gryzo&nacute;</trd>
  </snc>
  <trd lng="en">rodent</trd>
  <trdgrp lng="id">
    <trd><baz>kerat</baz>pengerat</trd>,
    <trd>hewan pengerat</trd>
  </trdgrp>
  <trd lng="ja">&#40807;&#27503;&#39006;  <pr>&#12370;&#12387;&#12375;&#12427;&#12356;</pr></trd>
</drv>

<drv mrk="rongx.0uloj">
  <kap><ofc>9</ofc><tld/>uloj <fnt><bib>PIV1</bib></fnt>,
  <var><kap><tld var="d"/>uloj <fnt><bib>PV</bib></fnt></kap></var></kap>
  <snc mrk="rongx.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> de
      <ref tip="malprt" cel="mam.0uloj">mamuloj</ref>  (<trd lng="la">Rodentia</trd>),
      havanta en &ccirc;iu makzelo po du &ccirc;izilspecajn incizivojn:
      <ekz>
        fontoj de infekti&gcirc;o estas <tld/>uloj
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008278.html">Damokla
          glavo super la homaro</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        turfalkoj &ccirc;asas <tld/>uletojn, gvatante ilin dum senmova restado
        en la aero kun
        vibrantaj flugiloj
        <fnt>
          <bib>Monato</bib>
          <aut>Thierry Tailhades</aut>
          <vrk><url ref="&Monato;/2012/010805.php">Erariga
          foto (2)</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        malgrandaj <tld var="d"/>uloj faras tiujn altajn pepajn sonojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18885">Strigoformaj
          birdoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
    </snc>
  <trd lng="be">&c_g;&c_r;&c_y;&c_z;&c_u;&c_n;&c_y;</trd>
  <trd lng="ca">rosegadors</trd>
  <trd lng="de">Nagetiere</trd>
  <trdgrp lng="en">
    <trd>rodents</trd>,
    <trd>Rodentia</trd>
  </trdgrp>
  <trd lng="es">roedores<klr tip="amb"> (clase animal)</klr></trd>
  <trd lng="fa">&a_j;&a_w;&a_n;&a_d;&f_g;&a_A;&a_n;</trd>
  <trd lng="fr">rongeurs</trd>
  <trd lng="hu">r&aacute;gcs&aacute;l&oacute;k</trd>
  <trd lng="id">Rodentia<klr> (ordo)</klr></trd>
  <trd lng="nl">knaagdieren</trd>
  <trd lng="pl">gryzonie</trd>
  <trd lng="ru">&c_g;&c_r;&c_y;&c_z;&c_u;&c_n;&c_y; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="uk">&c_g;&c_r;&c_i;&c_z;&c_u;&c_n;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#21870;&#40831;&#31867; <pr>ni&egrave;ch&#464;l&egrave;i</pr></trd>,
    <trd>&#22169;&#40786;&#39006; <pr>ni&egrave;ch&#464;l&egrave;i</pr></trd>,
    <trd>&#21870;&#40831;&#30446; <pr>ni&egrave;ch&#464;m&ugrave;</pr></trd>,
    <trd>&#22169;&#40786;&#30446; <pr>ni&egrave;ch&#464;m&ugrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="rongx.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="rongx.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="R"/>i &ccirc;e la &ccirc;irka&ubreve;o:
      <ekz>
        el lia brustpo&scirc;o, kie la viroj kutime portas tuketon
        a&ubreve; fontoplumon, montri&gcirc;is
        &ccirc;irka&ubreve;<tld/>ita koka osto
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 19a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_b;&c_g;&c_r;&c_y;&c_z;&c_a;&c_c;&c_mol;</trd>
    <trd lng="pl">obgryza&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_b;&c_g;&c_r;&c_y;&c_z;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_b;&c_g;&c_r;&c_y;&c_z;&c_a;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
<trd lng="de">abnagen</trd>
  <trd lng="en"><ind>gnaw</ind> up</trd>
  <trdgrp lng="id">
    <trd><baz>kerat</baz>mengerat sekeliling</trd>,
    <trd><baz>kerip</baz>mengerip sekeliling</trd>,
    <trd><baz>kerikit</baz>mengerikiti sekeliling</trd>,
    <trd><baz>kerumit</baz>mengerumit sekeliling</trd>,
    <trd><baz>gerogot</baz>menggerogoti sekeliling</trd>,
    <trd><baz>unggis</baz>mengunggis sekeliling</trd>
  </trdgrp>
  <trd lng="zh">&#21643; <pr>z&eacute;</pr></trd>
</drv>
</art>
