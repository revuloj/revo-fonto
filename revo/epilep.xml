<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <ofc>1</ofc>
    <rad>epilepsi</rad>/o
  </kap>
  <drv mrk="epilep.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="san.mal0o" lst="voko:malsanoj">Malsano</ref>
      karakterizata de la
      perdo je l' konscio kaj de konvulsioj.  
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_l;&c_je;&c_p;&c_s;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_je;&c_p;&c_i;&c_l;&c_je;&c_p;&c_s;&c_i;&c_ja;</trd>
  <trd lng="ca">epil&egrave;psia</trd>
  <trdgrp lng="de">
     <trd>Epilepsie</trd>,
     <trd>Fallsucht<klr> [ark]</klr></trd>
  </trdgrp>
  <trd lng="el">&epsilon;&pi;&jota;&lambda;&eta;&psi;&jota_ton;&alfa;</trd>
  <trd lng="en">epilepsy</trd>
  <trd lng="es">epilepsia</trd>
  <trd lng="fr">&eacute;pilepsie</trd>
  <trd lng="nl">epilepsie</trd>
  <trdgrp lng="pl">
    <trd>padaczka</trd>,
    <trd>epilepsja</trd>
  </trdgrp>
  <trd lng="pt">epilepsia</trd>
  <trd lng="ru">&c_e;&c_p;&c_i;&c_l;&c_je;&c_p;&c_s;&c_i;&c_ja;</trd>
</drv>
</art>
