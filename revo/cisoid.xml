<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>cisoid</rad>/o
</kap>
<drv mrk="cisoid.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 34</lok></fnt>
  <snc>
    <dif>
      Speco de triagrada ebena <ref tip="super"
      cel="kurb.0o.MAT">kurbo</ref>:
      <ekz>
        la kartezia ekvacio de <tld/>o estas de la tipo
        <frm>(<k>x</k><sup>2</sup>+<k>y</k><sup>2</sup>)(<k>x</k>cos&alfa;+<k>y</k>cos&alfa;)
        =
        2<k>a</k><k>y</k><sup>2</sup></frm>.
      </ekz>
    </dif>
  </snc>
  <bld lok="../bld/k_cisoido.svg.gif" alt="300" lrg="300">
    <tld/>o
  </bld>
  <trd lng="be">&c_c;&c_y;&c_s;&c_o;&c_ib;&c_d;&c_a;</trd>
  <trd lng="ca">cissoide</trd>
  <trd lng="de">Cissoide</trd>
  <trd lng="en">cissoid</trd>
  <trd lng="fr">cisso&iuml;de</trd>
  <trd lng="hu">cisszoid</trd>
  <trd lng="pt">ciss&oacute;ide</trd>
  <trdgrp lng="pl">
    <trd>cissoida</trd>,
    <trd>cysoida</trd></trdgrp> 
  <trd lng="ru">&c_c;&c_i;&c_s;&c_s;&c_o;&c_i;&c_d;&c_a;</trd>
</drv>
</art>

