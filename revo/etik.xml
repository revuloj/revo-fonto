<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>1</ofc>
<rad>etik</rad>/o
</kap>
<drv mrk="etik.0o">
  <kap><tld/>o</kap>
  <snc mrk="etik.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      Bran&ccirc;o de <ref tip="malprt" cel="filozo.0o">filozofio</ref>, kiu
      temas pri homaj volo kaj ago, de norma vidpunkto:
      <ekz>
        beda&ubreve;rinde la tekniko progresas pli rapide ol la <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>Ethik</trd>,
       <trd>Sittenlehre</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Aro de moralaj principoj de individuo a&ubreve; grupo:
      <ekz>
        la kristana <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>Ethik</trd>,
       <trd>Sitte</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="es">&eacute;tica</trd>
  <trd lng="fr">&eacute;thique<klr tip="ind"> (subst.)</klr></trd>
  <trdgrp lng="hu">
    <trd>etika</trd>,
    <trd>erk&ouml;lcstan</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_t;&c_i;&c_k;&c_a;</trd>
</drv>
<drv mrk="etik.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Koncernanta <tld/>on:
      <ekz>
        <tld/>aj principoj.
      </ekz>
    </dif>
    <trd lng="fr">&eacute;thique<klr tip="ind"> (adj.)</klr></trd>
    <trd lng="hu">etikai</trd>
    <trdgrp lng="ru">
      <trd>&c_e;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>,
      <trd>&c_n;&c_r;&c_a;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Morale bona:
      <ekz>
        <tld/>a faro.
      </ekz>
    </dif>
    <trd lng="fr">moral<klr tip="ind"> (moralement bon)</klr></trd>
    <trd lng="hu">etikus</trd>
    <trd lng="ru">&c_e;&c_t;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </snc>
  <trd lng="be">&c_e;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trdgrp lng="de">
       <trd>ethisch</trd>,
       <trd>sittlich</trd>,
       <trd>moralisch</trd>
  </trdgrp>
  <trd lng="es">&eacute;tico</trd>
</drv>
</art>
