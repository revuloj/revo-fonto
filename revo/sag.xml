<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sag</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sag.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sag.0o.ARM">
    <dif>
      Vergeto kun &scirc;tona a&ubreve; metala pinto, kiun oni pafas per
      <ref cel="ark.paf0o">pafarko</ref>:
      <ekz>
        prenu do nun viajn armilojn, vian <ind><tld/>ujo</ind>n kaj vian
        pafarkon, kaj iru sur la kampon kaj &ccirc;asu por mi &ccirc;asa&jcirc;on
        <fnt><bib>MT</bib>, <lok>&Gen; 27:3</lok></fnt>.
        <trdgrp lng="be">
          <trd>&c_k;&c_a;&c_l;&c_ch;&c_a;&c_n;</trd>, 
          <trd>&c_t;&c_u;&c_l;</trd>
        </trdgrp>
        <trd lng="de">K&ouml;cher</trd>
        <trd lng="fr">carquois</trd>
        <trdgrp lng="hu">
          <trd>tegez</trd>,
          <trd>puzdra</trd>
        </trdgrp>
        <trd lng="it">faretra</trd>
        <trd lng="ru">&c_k;&c_o;&c_l;&c_ch;&c_a;&c_n;</trd>
        <trd lng="sv">koger</trd>
      </ekz>
    </dif>
    <trd lng="be">&c_s;&c_t;&c_r;&c_a;&c_l;&c_a;</trd>
    <trdgrp lng="ca">
      <trd>fletxa</trd>,
      <trd>sageta</trd>
    </trdgrp>
    <trd lng="hu">ny&iacute;l</trd>
    <trd lng="it">freccia</trd>      
    <trd lng="es">flecha</trd>        
    <trd lng="ru">&c_s;&c_t;&c_r;&c_je;&c_l;&c_a;</trd>
  </snc>
  <snc mrk="sag.0o.FORM">
    <dif>
      <tld lit="S"/>oforma streko kun triangula ekstremo,
      simbolo de direkto a&ubreve; de movo.
    </dif>
  </snc>
  <snc mrk="sag.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib><lok>p. 28</lok></fnt>
    <dif>
      <klr>(de <ref cel="ark.0o.segmento">arko</ref> de cirklo)</klr>
      Plej granda <ref tip="super" cel="strek.0o.segmento">streko</ref>
      orte liganta la arkon al la <ref
      cel="sxnur.0o.cirklo">&scirc;nuro</ref>
      &gcirc;in <ref cel="strecx.sub0i">substre&ccirc;anta</ref>; la longo de
      tia streko:
      <ekz>
        <tld/>o estas segmento de radiuso;
      </ekz>
      <ekz>
        la <tld/>o de arko <ref cel="trancx.de0i.MAT">detran&ccirc;ita</ref>
        de <ref cel="angul.centra0o">centra angulo</ref> kun mezuro &alfa;
        estas porporcia al <frm>1-cos(&alfa;/2)</frm>.
      </ekz>
    </dif>
    <trd lng="en">sagitta</trd>
    <trdgrp lng="hu">
      <trd>ny&iacute;l(&aacute;s)</trd>,
      <trd>szagitta</trd>
    </trdgrp>
    <trd lng="it">senoverso</trd>
    <trdgrp lng="pl">
      <trd>wysoko&sacute;&cacute;<klr tip="ind"> (odcinka ko&lstroke;a)</klr></trd>,
      <trd>strza&lstroke;ka<klr tip="ind"> (odcinka ko&lstroke;a)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_r;&c_je;&c_l;&c_k;&c_a;</trd>,
      <trd>&c_s;&c_a;&c_g;&c_i;&c_t;&c_t;&c_a;</trd>
    </trdgrp>
  </snc>
  <trd lng="bg">&c_s;&c_t;&c_r;&c_je;&c_l;&c_a;</trd>
  <trd lng="de">Pfeil</trd>
  <trd lng="en">arrow</trd>
  <trd lng="fr">fl&egrave;che</trd>
  <trd lng="nl">pijl</trd>
  <trdgrp lng="pt">
   <trd>flecha</trd>
  </trdgrp>
  <trd lng="sv">pil</trd>
</drv>
</art>
