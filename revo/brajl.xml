<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>brajl</rad>/o
  </kap>
  <drv mrk="brajl.0o">
    <kap><tld/>o</kap>
    <snc>
      <dif>
        Speciala skribo por blinduloj, elpensita de Braille (Braj).  
      </dif>
    </snc>
    <trdgrp lng="be">
      <trd>&c_sh;&c_r;&c_y;&c_f;&c_t;
      <ind>&c_B;&c_r;&c_a;&c_j;&c_l;&c_ja;</ind></trd>,
      <trd>&c_s;&c_y;&c_s;&c_t;&c_e;&c_m;&c_a;
      <ind>&c_B;&c_r;&c_a;&c_j;&c_l;&c_ja;</ind></trd>,
      <trd>&c_a;&c_z;&c_b;&c_u;&c_k;&c_a;
      <ind>&c_B;&c_r;&c_a;&c_j;&c_l;&c_ja;</ind></trd>
    </trdgrp>
    <trd lng="de">Braille-Schrift</trd>
    <trd lng="fr">braille</trd>
    <trdgrp lng="hu">
      <trd>Braille-&iacute;r&aacute;s</trd>,
      <trd>vak&iacute;r&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>braill</trd>,
      <trd lng="pl">pismo Braill'a</trd>,
      <trd>pismo wypuk&lstroke;e</trd>,
      <trd>pismo dla niewidomych</trd>
    </trdgrp>
    <trd lng="ru">&c_p;&c_i;&c_s;&c_mol;&c_m;&c_o; &c_d;&c_l;&c_ja;
    &c_s;&c_l;&c_je;&c_p;&c_y;&c_h; &c_p;&c_o;
    &c_m;&c_je;&c_t;&c_o;&c_d;&c_u;
    &c_B;&c_r;&c_a;&c_j;&c_l;&c_ja;</trd>
    <trd lng="sk">Braillovo p&iacute;smo</trd>
  </drv>
</art>
