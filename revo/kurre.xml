<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kurre.xml,v 1.5 2020/12/27 10:46:33 revo Exp $">
<kap>
  <rad>gruk-ru</rad> <fnt> <bib>ElektFab</bib></fnt>,
  <var><kap><rad var="k">kurre</rad> <fnt> <bib>Fab2</bib></fnt></kap></var>
</kap>
<drv mrk="kurre.0">
  <kap><tld/>, <var><kap><tld var="k"/></kap></var></kap>
  <gra><vspec>sonimito</vspec></gra> 
  <snc mrk="kurre.0.ZOO">
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleko</ref>
      de kolombo:
      <ekz>
        tie sidis sur la avelarbo du kolombetoj kaj vokis: <tld/>, <tld/>
        <fnt>
          <bib>ElektFab</bib>
          <lok>Cindrulino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arbaraj kolomboj diris: &leftquot;<tld var="k"/>, <tld var="k"/>&rightquot;
        <fnt>
          <bib>Fab2</bib>, <lok>Ne&gcirc;a Re&gcirc;ino</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kver.0i.SON">kveri<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="de">gurre</trd>
  <trd lng="en">cooing sound</trd>
  <trd lng="fr">crou<klr> (cri de pigeon)</klr></trd>
  <trd lng="pl">gruchu</trd>
</drv>
</art>

