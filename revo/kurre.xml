<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kurre.xml,v 1.3 2015/09/30 05:10:17 revo Exp $">
<kap>
  <rad>kurre</rad> <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="kurre.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra> 
  <snc mrk="kurre.0.ZOO">
    <dif>
      <ref tip="lst" cel="blek.0o" lst="voko:blekoj">Bleko</ref>
      de kolombo:
      <ekz>
        la arbaraj kolomboj diris: <ctl><tld/>, <tld/></ctl>
        <fnt>
          <bib>Fab2</bib>, <lok>Ne&gcirc;a Re&gcirc;ino</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="kver.0i.SON">kveri<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="en">cooing sound</trd>
  <trd lng="fr">crou<klr> (cri de pigeon)</klr></trd>
  <trd lng="pl">gruchu</trd>
</drv>
</art>

