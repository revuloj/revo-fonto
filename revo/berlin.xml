<vortaro>
  <art mrk="$Id$">
   <kap>
    <rad>Berlin</rad>/o
   </kap>
   <drv mrk="berlin.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">GEOG</uzo>
    <snc>
       <dif>
          &Ccirc;efurbo kaj plej granda <ref tip="super"
cel="urb.0o">urbo</ref> de <ref
tip="malprt" cel="german1.G0ujo">Germanujo</ref> (13&ring;25'E,
52&ring;31'N), samtempe propra federacia lando.
       </dif>
       <trd lng="de">Berlin</trd>
       <trd lng="lat">Berolinum</trd>
    </snc>
</drv>

  <drv mrk="berlin.b0ano">
    <kap><tld lit="b"/>ano</kap>
    <snc>
      <dif>
         Civitano de Berlino.
      </dif>
    </snc>
    <trd lng="de">Berliner</trd>
  </drv>

  </art>

