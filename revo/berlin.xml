<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Berlin</rad>/o
</kap>
<drv mrk="berlin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      &Ccirc;efurbo kaj plej granda <ref tip="super"
      cel="urb.0o">urbo</ref> de <ref
      tip="malprt" cel="german1.G0ujo">Germanujo</ref> (13&ring;25'E,
      52&ring;31'N), samtempe propra federacia lando:
      <ekz>
        deputitoj, registoj kaj oficistoj transloki&gcirc;is el la provizora
        &ccirc;efurbo Bonn al <tld/>o<fnt><aut>Stefan Maul</aut>: <vrk>En
        giganta truo</vrk>, <bib>Monato</bib><lok>jaro 1999a, numero 9a,
        p. 6a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Berlin</trd>
    <trd lng="en">Berlin</trd>
    <trd lng="es">Berl&iacute;n</trd>
    <trdgrp lng="fr">
      <trd>Berlin</trd>
    </trdgrp>
    <trd lng="hu">Berlin</trd>
    <trd lng="lat">Berolinum</trd>
    <trd lng="nl">Berlijn</trd>
    <trd lng="pl">Berlin</trd>
    <trd lng="ru">&c_B;&c_je;&c_r;&c_l;&c_i;&c_n;</trd>
    <trd lng="sk">Berl&iacute;n</trd>
  </snc>
</drv>

<drv mrk="berlin.b0ano">
  <kap><tld lit="b"/>ano</kap>
  <snc>
    <dif>
      Civitano de Berlino.
    </dif>
  </snc>
  <trd lng="de">Berliner</trd>
  <trd lng="en">Berliner</trd>
  <trdgrp lng="fr">
    <trd>berlinois<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">berlini</trd>
  <trd lng="nl">Berlijner</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_l;&c_i;&c_n;&c_je;&c_c;</trd>
  <trd lng="sk">Berl&iacute;n&ccaron;an</trd>
</drv>
</art>
