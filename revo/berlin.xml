<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap><ofc>* 7</ofc>
  <rad>Berlin</rad>/o
</kap>
<drv mrk="berlin.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      &Ccirc;efurbo kaj plej granda <ref tip="super" cel="urb.0o">urbo</ref>
      de <ref tip="malprt" cel="german1.G0ujo">Germanujo</ref> (13&#730;25'E,
      52&#730;31'N), samtempe propra federacia lando:
      <ekz>
        li venigis al si el <tld/>o multajn librojn
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>;
      </ekz>
      <ekz>
        deputitoj, registoj kaj oficistoj transloki&gcirc;is el la provizora
        &ccirc;efurbo Bonn al <tld/>o
        <fnt>
          <aut>Stefan Maul</aut>:
          <vrk>En giganta truo</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 1999a, numero 9a, p. 6a</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_B;&c_e;&c_r;&c_l;&c_ib;&c_n;</trd>
    <trd lng="br">Berlin</trd>
    <trd lng="ca">Berl&iacute;n</trd>
    <trd lng="de">Berlin</trd>
    <trd lng="en">Berlin</trd>
    <trd lng="es">Berl&iacute;n</trd>
    <trd lng="fr">Berlin</trd>
    <trd lng="fy">Berlyn</trd>
    <trd lng="hu">Berlin</trd>
    <trd lng="io">Berlin</trd>
    <trd lng="lat">Berolinum</trd>
    <trd lng="nl">Berlijn</trd>
    <trd lng="pl">Berlin</trd>
    <trd lng="pt">Berlim</trd>
    <trd lng="ru">&c_B;&c_je;&c_r;&c_l;&c_i;&c_n;</trd>
    <trd lng="sk">Berl&iacute;n</trd>
  </snc>
</drv>

<drv mrk="berlin.b0a">
  <kap><tld lit="b"/>a</kap>
  <snc>
    <dif>
      Rilata al Berlino.
    </dif>
  </snc>
  <trd lng="en">Berliner</trd>
  <trdgrp lng="fr">
    <trd>berlinois<klr> (adj.)</klr></trd>
    <trd>berlinoise<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="fy">Berlynsk</trd>
  <trd lng="io">Berlinana</trd>
  <trd lng="nl">Berlijns</trd>
</drv>

<drv mrk="berlin.b0ano">
  <kap><tld lit="b"/>ano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de Berlino.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_e;&c_r;&c_l;&c_ib;&c_n;&c_je;&c_c;</trd>
  <trd lng="br">Berlinad</trd>
  <trd lng="de">Berliner</trd>
  <trd lng="en">Berliner</trd>
  <trd lng="fr">Berlinois<klr> (subst.)</klr></trd>
  <trd lng="hu">berlini</trd>
  <trd lng="io">Berlinano</trd>
  <trd lng="nl">Berlijner</trd>
  <trd lng="pl">Berli&nacute;czyk</trd>
  <trd lng="ru">&c_b;&c_je;&c_r;&c_l;&c_i;&c_n;&c_je;&c_c;</trd>
  <trd lng="sk">Berl&iacute;n&ccaron;an</trd>
</drv>

<drv mrk="berlin.b0anino">
  <kap><tld lit="b"/>anino</kap>
  <snc>
    <dif>
      Inseksa <ref tip="super" cel="berlin.b0ano"><tld
      lit="b"/>ano</ref>:
      <ekz>
        &scirc;i ja estis tre &ccirc;arma <tld lit="b"/>anino
        <fnt>
          <aut>Trevor Steele</aut>
          <vrk>Kvaza&ubreve; &ccirc;io dependus de mi</vrk>
          <lok>p. 165a</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i denaske estas <tld lit="b"/>anino
        <fnt>
          <bib>Monato</bib>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url
          ref="http://www.monato.net/2011/010369.php">Tekniko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Berlinerin</trd>
  <trd lng="en">female Berliner</trd>
  <trd lng="fr">Berlinoise<klr> (subst.)</klr></trd>
  <trd lng="io">Berlinanino</trd>
  <trd lng="nl">Berlijnse</trd>
</drv>

<drv mrk="berlin.0o.muro">
  <kap><tld lit="b"/>a muro</kap>
  <snc>
    <dif>
      <ref tip="super" cel="mur.0ego">Murego</ref>, starigita de la
      registaro de la Germana Demokratia Respubliko por malebligi la
      liberan cirkuladon inter la du partoj de Berlino (1961&mdash;1989).
    </dif>
  </snc>
  <trd lng="ca">mur de Berl&iacute;n</trd>
  <trd lng="de">Berliner Mauer</trd>
  <trd lng="en">Berlin Wall</trd>
  <trd lng="es">muro de Berl&iacute;n</trd>
  <trd lng="fr">mur de Berlin</trd>
  <trd lng="fy">Berlynske Muorre</trd>
  <trd lng="nl">Berlijnse Muur</trd>
</drv>
</art>

