<art mrk="$Id: rajon.xml,v 1.14 2025/07/15 06:00:21 revo Exp $">
<kap>
  <rad>rajon</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="rajon.0o">
  <kap><tld/>o</kap>
  <snc mrk="rajon.0o.TEKS">
    <uzo tip="fak">KEM</uzo>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      &Hcirc;emia <ref tip="super" cel="sxtof.0o.TEKS">&scirc;tofo</ref> teksita
      el  
      <ref tip="vid" cel="viskoz.0o">viskozo</ref>:
      <ekz>
        mi havis nur du &ccirc;emizojn <klr>...</klr> el vera <tld/>o
        <fnt>
          <aut>J. Rejt&odblac;, trad. L. Bal&aacute;zs</aut>,
          <vrk><url ref="http://www.freeweb.hu/verkoj/02.htm">La
          anta&ubreve;en&scirc;ovita garnizono</url></vrk>,
          <lok>[vidita en 2007-03]</lok>
        </fnt>;
      </ekz>
      <ekz>
        disvasti&gcirc;is anka&ubreve; subvesta&jcirc;oj el <tld/>o: &scirc;tofo, kiun oni 
        inventis por anstata&ubreve;i silkon en para&scirc;utoj
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;subvesto&rightquot;, 
        2007-01-28</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_s;&c_k;&c_o;&c_z;&c_n;&c_a;&c_ja;
  &c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_a;</trd>
  <trd lng="ca">rai&oacute;</trd>
  <trd lng="cs">rajon</trd>
  <trd lng="de">Viskose</trd>
  <trd lng="en">rayon</trd>
  <trd lng="es">ray&oacute;n</trd>
  <trd lng="fr">rayonne</trd>
  <trd lng="hu">m&udblac;selyem</trd>
  <trd lng="it">rayon</trd>
  <trd lng="ja">&#12524;&#12540;&#12520;&#12531;</trd>
  <trdgrp lng="nl">
    <trd>rayon</trd>,
    <trd>kunstzijde</trd>
  </trdgrp>
  <trd lng="pl">wiskoza</trd>
  <trdgrp lng="pt">
    <trd>seda artificial</trd>,
    <trd>"<ind>rayon</ind>"</trd>
  </trdgrp>
  <trd lng="ro">celofibr&abreve;</trd>
  <trd lng="ru">&c_v;&c_i;&c_s;&c_k;&c_o;&c_z;&c_n;&c_a;&c_ja; &c_t;&c_k;&c_a;&c_n;&c_mol;</trd>
  <trd lng="sk">raj&oacute;n</trd>
  <trdgrp lng="uk">
    <trd>&c_r;&c_a;&c_j;&c_o;&c_n;</trd>,
    <trd>&c_sh;&c_t;&c_u;&c_ch;&c_n;&c_i;&c_j; &c_sh;&c_o;&c_v;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#20154;&#36896;&#19997; <pr>r&eacute;nz&agrave;os&#299;</pr></trd>,
    <trd>&#20154;&#36896;&#32114; <pr>r&eacute;nz&agrave;os&#299;</pr></trd>,
    <trd>&#23256;&#32264; <pr>l&eacute;iy&iacute;ng</pr></trd>,
    <trd>&#23256;&#33830; <pr>l&eacute;iy&iacute;ng</pr></trd>
  </trdgrp>
</drv>
</art>

