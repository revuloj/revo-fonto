<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>indign</rad>/i
</kap>
<drv mrk="indign.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Senti kaj ekmontri koleron kaj malestimon pro maljusta&jcirc;o a&ubreve;
      malnobla&jcirc;o:
      <ekz>
        &ccirc;iuj <tld/>is pro, pri tia konduto;
      </ekz>
      <ekz>
        Dio a&ubreve;dis kaj flami&gcirc;is kaj forte ekindignis 
        kontra&ubreve; Izrael
        <fnt><bib>M</bib>, <lok>Psalmaro 78:59</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>sich <ind>entr&uuml;sten</ind></trd>,
     <trd>sich <ind>ereifern</ind></trd>
  </trdgrp>
  <trd lng="en">indignant</trd>
</drv>
<drv mrk="indign.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sento de <tld/>anto:
      <ekz>
        esprimi sian <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Entr&uuml;stung</trd>
</drv>
<drv mrk="indign.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Esprimanta <tld/>on:
      <ekz>
        <tld/>a tono, rigardo.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="indign.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld/>ante:
      <ekz>
        li for&jcirc;etis <tld/>e tiun malhonestan proponon.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="indign.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Ka&ubreve;zi, naski <tld/>on:
      <ekz>
        lia mallojala konduto <tld/>igas &ccirc;iujn;
      </ekz>
      <ekz>
        <tld/>iga trompo, perfido
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ofend.0i">ofendi</ref>,
      <ref cel="star">harstariga</ref>.
    </refgrp>
  </snc>
</drv>
<drv mrk="indign.0igajxo0indajxo">
  <kap><tld/>iga&jcirc;o, <tld/>inda&jcirc;o<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Afero, kiu meritas <tld/>on.
    </dif>
  </snc>
</drv>
</art>

<!--
$Log$
-->
</vortaro>
