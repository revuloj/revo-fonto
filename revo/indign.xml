<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: indign.xml,v 1.28 2025/05/26 20:30:19 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>indign</rad>/i
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="indign.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Senti kaj ekmontri koleron kaj malestimon pro maljusta&jcirc;o a&ubreve;
      malnobla&jcirc;o:
      <ekz>
        la Izraelidoj forte <tld/>is, kaj ili foriris de li
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 3:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        Dio a&ubreve;dis kaj flami&gcirc;is kaj forte ekindignis
        kontra&ubreve; <nom>Izrael</nom>
        <fnt><bib>MT</bib>, <lok>&Psa; 78:59</lok></fnt>;
      </ekz>
      <ekz>
        iuj <tld/>is inter si, dirante: por kio fari&gcirc;is &ccirc;i tiu mal&scirc;paro
        <fnt>
          <bib>NT</bib>
          <lok>Marko 14:4</lok>
        </fnt>?
      </ekz>
      <ekz>
        la tuta aristokrataro <tld/>as pro la despotismo de la pastroj
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is pro la propra malforteco
        <fnt>
          <bib>PatrojFiloj</bib>
          <lok>VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        pri tiaj kalumnioj <tld/>is jam knabeto <nom>Zamenhof</nom> en <nom>Bjalistok</nom>
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxagre.0igxi">&ccirc;agreni&gcirc;i</ref>,
      <ref cel="koler.0i">koleri</ref>,
      <ref cel="ofend.0igxi">ofendi&gcirc;i</ref>,
      <ref cel="protes.0i.malkonsenti">protesti</ref>,
      <ref cel="skanda.0i">skandali</ref>,
      <ref cel="tumult.0i">tumulti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_u;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="de">
    <trd>sich <ind>emp&ouml;ren</ind></trd>,
    <trd>sich <ind>entr&uuml;sten</ind></trd>,
    <trd>sich <ind>ereifern</ind></trd>
  </trdgrp>
  <trd lng="en">be indignant</trd>
  <trd lng="es">indignar</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>indign&eacute;</ind></trd>,
    <trd>s'<ind>indigner</ind></trd>,
    <trd>se <ind>scandaliser</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&aacute;borog</trd>,
    <trd>fel van h&aacute;borodva</trd>,
    <trd>m&eacute;ltatlankodik</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24996;&#24936;&#12377;&#12427;  <pr>&#12405;&#12435;&#12364;&#12356;&#12377;&#12427;</pr></trd>,
    <trd>&#24996;&#12427;  <pr>&#12416;&#12378;&#12363;&#12427;</pr></trd>
  </trdgrp>
  <trd lng="nl">verontwaardigd zijn</trd>
  <trd lng="pt">indignar-se</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_u;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_o;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pohor&scaron;i&tcaron; sa</trd>,
    <trd>rozhor&ccaron;ova&tcaron;<klr>(sa)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&yacute;t indignov&aacute;n</trd>,
    <trd>indignovat se</trd>,
    <trd>pohor&scaron;en</trd>,
    <trd>rozho&rcaron;&ccaron;en</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>be <ind>indignant</ind></trd>,
    <trd>be <ind>resentful</ind></trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_b;&c_u;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;&c_s;&c_ja;</trd>
  <trd lng="zh">&#21972; <pr>ch&emacron;n</pr></trd>
</drv>

<drv mrk="indign.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sento de <tld/>anto:
      <ekz>
        la Eternulo el&jcirc;etis ilin el ilia lando en kolero kaj en furiozo kaj en granda
        <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Readmono 29:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi mem parolis kun <tld/>o pri la frenezaj elspezoj de la faraona kortego
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>;
      </ekz>
      <ekz>
        la princo parolis kun videbla <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        la viza&gcirc;o de Mario ekflamis per honto kaj <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ho, ho! interrompis la virino duone kun rido, duone kun koketa <tld/>o
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="koler.0o">kolero</ref>,
      <ref cel="tumult.0o">tumulto</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_u;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>hn&ecaron;v</trd>,
    <trd>indignace</trd>,
    <trd>nelibost</trd>,
    <trd>rozhorlenost</trd>,
    <trd>rozho&rcaron;&ccaron;enost</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Emp&ouml;rung</trd>,
    <trd>Entr&uuml;stung</trd>,
    <trd>Indigniertheit<klr> [geh][rar]</klr></trd>
  </trdgrp>
  <trd lng="en">indignation</trd>
  <trd lng="es">indignaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>indignation</trd>,
    <trd>r&eacute;volte</trd>
  </trdgrp>
  <trd lng="hu">felh&aacute;borod&aacute;s</trd>
  <trdgrp lng="ja">
    <trd>&#24996;&#24936;  <pr>&#12405;&#12435;&#12364;&#12356;</pr></trd>,
    <trd>&#24996;&#12426;  <pr>&#12356;&#12365;&#12393;&#12362;&#12426;</pr></trd>,
    <trd>&#32681;&#24996;  <pr>&#12366;&#12405;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">verontwaardiging</trd>
  <trd lng="pt">indigna&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_u;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_o;&c_d;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pohor&scaron;enie</trd>,
    <trd>rozhor&ccaron;enie</trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_b;&c_u;&c_r;&c_je;&c_n;&c_n;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#20041;&#24868; <pr>y&igrave;f&egrave;n</pr></trd>,
    <trd>&#24868; <pr>f&egrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="indign.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Esprimanta <tld/>on:
      <ekz>
        <tld/>a tono, rigardo;
      </ekz>
      <ekz>
        <tld/>a moralinstruo
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 5:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> ripro&ccirc;is per <tld/>aj vortoj tiun virinon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> &jcirc;etis al la filo Lin&scirc;ardo kolerajn kaj <tld/>ajn rigardojn
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deksepa</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni &jcirc;etis la korpon al hundoj, ke ili &gcirc;in dis&scirc;iru, sed anka&ubreve; la hundoj kun
        <tld/>a bojado forkuris
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a bruado traruli&gcirc;is inter la vicoj kaj tuj trankvili&gcirc;is
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_u;&c_r;&c_a;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>indignovan&yacute;</trd>,
    <trd>pohn&ecaron;van&yacute;</trd>,
    <trd>rozho&rcaron;&ccaron;en&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>emp&ouml;rt</trd>,
    <trd>entr&uuml;stet</trd>,
    <trd>indigniert<klr> [geh]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>indignant</trd>,
    <trd>resentful</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>indign&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felh&aacute;borodott</trd>,
    <trd>m&eacute;ltatlankod&oacute;</trd>
  </trdgrp>
  <trd lng="ja">&#24996;&#24936;&#12375;&#12383;  <pr>&#12405;&#12435;&#12364;&#12356;&#12375;&#12383;</pr></trd>
  <trd lng="nl">verontwaardigd</trd>
  <trd lng="pt">indignativo</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_u;&c_shch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_o;&c_d;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nahnevan&yacute;</trd>,
    <trd>rozhor&ccaron;en&yacute;</trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_b;&c_u;&c_r;&c_je;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#24868;&#24868;&#19981;&#24179; <pr>f&egrave;nf&egrave;nb&ugrave;p&iacute;ng</pr></trd>,
    <trd>&#24868;&#28608; <pr>f&egrave;nj&imacron;</pr></trd>,
    <trd>&#27668;&#24868; <pr>q&igrave;f&egrave;n</pr></trd>,
    <trd>&#21463;&#21040;&#20940;&#36785; <pr>sh&ograve;ud&agrave;ol&iacute;ngr&#468;</pr></trd>,
    <trd>&#28608;&#24594; <pr>j&imacron;n&ugrave;</pr></trd>,
    <trd>&#24575;&#25041; <pr>f&egrave;nm&egrave;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="indign.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld lit="I"/>ante:
      <ekz>
        ni <tld/>e dis&scirc;iros kaj bruligos &ccirc;ion, kion ni skribis
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Dua Kongreso Esperantista en Gen&egrave;ve en la 28a de a&ubreve;gusto 1906</vrk>
        </fnt>;
      </ekz>
      <ekz>
        Post avaraj senkoruloj // Li <tld/>e kra&ccirc;as
        <fnt>
          <bib>LSB</bib>
          <lok>L' Almozulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e ekkriis la konato: &mdash; kiel li kura&gcirc;is tion &ccirc;i fari
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>!
      </ekz>
      <ekz>
        &ccirc;u vi ne estas prudenta? &scirc;i diris <tld/>e
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_u;&c_r;&c_a;&c_n;&c_a;</trd>
  <trd lng="cs">rozho&rcaron;&ccaron;en&ecaron;</trd>
  <trdgrp lng="de">
    <trd>emp&ouml;rt</trd>,
    <trd>entr&uuml;stet</trd>,
    <trd>indigniert<klr> [geh]</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>indignantly</trd>,
    <trd>resentfully</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>avec <ind>indignation</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felh&aacute;borodottan</trd>,
    <trd>m&eacute;ltatlankodva</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#24996;&#24936;&#12375;&#12390;  <pr>&#12405;&#12435;&#12364;&#12356;&#12375;&#12390;</pr></trd>,
    <trd>&#24996;&#28982;&#12392;&#12375;&#12390;  <pr>&#12405;&#12435;&#12380;&#12435;&#12392;&#12375;&#12390;</pr></trd>
  </trdgrp>
  <trd lng="nl">verontwaardigd</trd>
  <trd lng="pt">com <ind>indigna&ccedil;&atilde;o</ind><klr tip="ind">
  (adv.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_u;&c_shch;&c_jo;&c_n;&c_n;&c_o;</trd>,
    <trd>&c_v; &c_n;&c_je;&c_g;&c_o;&c_d;&c_o;&c_v;&c_a;&c_n;&c_i;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">rozhor&ccaron;ene</trd>
</drv>

<drv mrk="indign.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Ka&ubreve;zi, naski <tld/>on:
      <ekz>
        ili <tld/>igis kontra&ubreve; si Davidon
        <fnt>
          <bib>MT</bib>
          <lok>I. Kroniko 19:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u <tld/>igas lin la mensogo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>?
      </ekz>
      <ekz>
        &ccirc;u ne <tld/>igis nin la nesatigebla postulemo de la edzino
        <fnt>
          <bib>ElektFab</bib>
          <lok>Anta&ubreve;parolo de la tradukinto</lok>
        </fnt>?
      </ekz>
      <ekz>
        via agado indignigas &ccirc;iujn kontra&ubreve; vi
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        lin <tld/>igis mia &leftquot;sed&rightquot;
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la nekredebla impertinenteco <tld/>igis min, ke mi jam emis alsalti kaj reven&gcirc;i
        la aferon
        <fnt>
          <bib>VaK</bib>
          <lok>Tria &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cxagre.0i">&ccirc;agreni</ref>,
      <ref cel="koler.0igi">kolerigi</ref>,
      <ref cel="ofend.0i">ofendi</ref>,
      <ref cel="provok.0i.kauxzi">provoki</ref>,
      <ref cel="sxok.0i">&scirc;oki</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="har.0stariga">harstariga</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_u;&c_r;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>budit pohor&scaron;en&iacute;</trd>,
    <trd>indignovat</trd>,
    <trd>pohn&ecaron;vat</trd>,
    <trd>rozho&rcaron;&ccaron;it</trd>,
    <trd>zp&uring;sobit pohor&scaron;en&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>emp&ouml;ren</trd>,
    <trd>aufregen</trd>,
    <trd>provozieren</trd>
  </trdgrp>
  <trd lng="en">cause <ind>indignation</ind></trd>
  <trdgrp lng="fr">
    <trd>indigner</trd>,
    <trd>r&eacute;volter<klr tip="ind"> (causer de l'indignation)</klr></trd>
  </trdgrp>
  <trd lng="hu">felh&aacute;bor&iacute;t</trd>
  <trd lng="ja">&#24996;&#24936;&#12373;&#12379;&#12427;  <pr>&#12405;&#12435;&#12364;&#12356;&#12373;&#12379;&#12427;</pr></trd>
  <trd lng="nl">verontwaardigen</trd>
  <trd lng="pt">indignar<klr tip="amb"> (causar indigna&ccedil;&atilde;o)</klr></trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_m;&c_u;&c_shch;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>poburova&tcaron;</trd>,
    <trd>rozhor&ccaron;i&tcaron;</trd>
  </trdgrp>
  <trd lng="uk">&c_o;&c_b;&c_u;&c_r;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="indign.0indajxo">
  <kap><tld/>inda&jcirc;o,
     <var><kap><tld/>iga&jcirc;o</kap></var>
  </kap>
  <snc>
    <dif>
      Io, kio elvokas <tld/>on:
      <ekz>
        kiel! pacienci post tiaj <tld/>inda&jcirc;oj! ne <klr>[&#8230;]</klr> tian aferon mi ne
        povas konsenti
        <fnt>
          <bib>GDn</bib>
          <lok>Akto Tria</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="skanda.0o">skandalo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_u;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_w;&c_ch;&c_y;&c_n;&c_a;&c_k;</ind></trd>,
    <trd>&c_s;&c_k;&c_a;&c_n;&c_d;&c_a;&c_l;&c_mol;&c_n;&c_y; <ind>&c_w;&c_ch;&c_y;&c_n;&c_a;&c_k;</ind></trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Affront</trd>,
    <trd>Skandal</trd>,
    <trd>Provokation</trd>
  </trdgrp>
  <trd lng="en">indignity</trd>
  <trdgrp lng="hu">
    <trd>botr&aacute;ny</trd>,
    <trd>skandalum</trd>
  </trdgrp>
  <trd lng="ja">&#21329;&#21155;&#12394;&#34892;&#28858;  <pr>&#12402;&#12428;&#12388;&#12394;&#12371;&#12358;&#12356;</pr></trd>
  <trd lng="uk">&c_o;&c_b;&c_u;&c_r;&c_l;&c_i;&c_v;&c_i;&c_j; &c_v;&c_ch;&c_i;&c_n;&c_o;&c_k;</trd>
  <trdgrp lng="zh">
    <trd>&#20844;&#24320;&#20398;&#36785; <pr>g&omacron;ngk&amacron;iw&#468;r&ugrave;</pr></trd>,
    <trd>&#20882;&#29359; <pr>m&agrave;of&agrave;n</pr></trd>,
    <trd>&#36629;&#34065; <pr>q&imacron;ngmi&egrave;</pr></trd>,
    <trd>&#19985;&#38395; <pr>ch&#466;uw&eacute;n</pr></trd>
  </trdgrp>
</drv>

</art>
