<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: indign.xml,v 1.19 2022/03/21 15:27:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>indign</rad>/i
</kap>
<drv mrk="indign.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Senti kaj ekmontri koleron kaj malestimon pro maljusta&jcirc;o a&ubreve;
      malnobla&jcirc;o:
      <ekz>
        &ccirc;iuj <tld/>is pro, pri tia konduto;
      </ekz>
      <ekz>
        Dio a&ubreve;dis kaj flami&gcirc;is kaj forte ekindignis
        kontra&ubreve; Izrael
        <fnt><bib>MT</bib>, <lok>&Psa; 78:59</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <refgrp tip="vid">
    <ref cel="cxagre.0i">&ccirc;agreni</ref>,
    <ref cel="skanda.0i">skandali</ref>,
    <ref cel="sxok.0i">&scirc;oki</ref>
  </refgrp>
  <trd lng="be">&c_a;&c_b;&c_u;&c_r;&c_a;&c_c;&c_c;&c_a;</trd>
  <trdgrp lng="de">
    <trd>sich <ind>entr&uuml;sten</ind></trd>,
    <trd>sich <ind>ereifern</ind></trd>
  </trdgrp>
  <trd lng="en">be indignant</trd>
  <trd lng="es">indignar</trd>
  <trdgrp lng="fr">
    <trd>&ecirc;tre <ind>indign&eacute;</ind></trd>,
    <trd>s'<ind>indigner</ind></trd>,
    <trd>se <ind>scandaliser</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>h&aacute;borog</trd>,
    <trd>fel van h&aacute;borodva</trd>,
    <trd>m&eacute;ltatlankodik</trd>
  </trdgrp>
  <trd lng="nl">verontwaardigd zijn</trd>
  <trd lng="pt">indignar-se</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_u;&c_shch;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_o;&c_d;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pohor&#x161;i&#x165; sa</trd>,
    <trd>rozhor&#x10D;ova&#x165;<klr>(sa)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>b&#xFD;t indignov&#xE1;n</trd>,
    <trd>indignovat se</trd>,
    <trd>pohor&#x161;en</trd>,
    <trd>rozho&#x159;&#x10D;en</trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&#x431;&#x443;&#x440;&#x44E;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>
</drv>

<drv mrk="indign.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Sento de <tld/>anto:
      <ekz>
        esprimi sian <tld/>on.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_u;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>hn&#x11B;v</trd>,
    <trd>indignace</trd>,
    <trd>nelibost</trd>,
    <trd>rozhorlenost</trd>,
    <trd>rozho&#x159;&#x10D;enost</trd>
  </trdgrp>
  <trd lng="de">Entr&uuml;stung</trd>
  <trd lng="en">indignation</trd>
  <trd lng="es">indignaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>indignation</trd>,
    <trd>r&eacute;volte</trd>
  </trdgrp>
  <trd lng="hu">felh&aacute;borod&aacute;s</trd>
  <trd lng="nl">verontwaardiging</trd>
  <trd lng="pt">indigna&ccedil;&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_u;&c_shch;&c_je;&c_n;&c_i;&c_je;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_o;&c_d;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pohor&#x161;enie</trd>,
    <trd>rozhor&#x10D;enie</trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&#x431;&#x443;&#x440;&#x435;&#x43D;&#x43D;&#x44F;</trd>
</drv>

<drv mrk="indign.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Esprimanta <tld/>on:
      <ekz>
        <tld/>a tono, rigardo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_u;&c_r;&c_a;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>indignovan&#xFD;</trd>,
    <trd>pohn&#x11B;van&#xFD;</trd>,
    <trd>rozho&#x159;&#x10D;en&#xFD;</trd>
  </trdgrp>
  <trd lng="en">indignant</trd>
  <trdgrp lng="fr">
    <trd>indign&eacute;</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felh&aacute;borodott</trd>,
    <trd>m&eacute;ltatlankod&oacute;</trd>
  </trdgrp>
  <trd lng="nl">verontwaardigd</trd>
  <trd lng="pt">indignativo</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_u;&c_shch;&c_jo;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_g;&c_o;&c_d;&c_u;&c_ju;&c_shch;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>nahnevan&#xFD;</trd>,
    <trd>rozhor&#x10D;en&#xFD;</trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&#x431;&#x443;&#x440;&#x435;&#x43D;&#x438;&#x439;</trd>
</drv>

<drv mrk="indign.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld lit="I"/>ante:
      <ekz>
        li for&jcirc;etis <tld/>e tiun malhonestan proponon.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_u;&c_r;&c_a;&c_n;&c_a;</trd>
  <trd lng="cs">rozho&#x159;&#x10D;en&#x11B;</trd>
  <trd lng="en">indignantly</trd>
  <trdgrp lng="fr">
    <trd>avec <ind>indignation</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felh&aacute;borodottan</trd>,
    <trd>m&eacute;ltatlankodva</trd>
  </trdgrp>
  <trd lng="nl">verontwaardigd</trd>
  <trd lng="pt">com <ind>indigna&ccedil;&atilde;o</ind><klr tip="ind">
  (adv.)</klr></trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_o;&c_z;&c_m;&c_u;&c_shch;&c_jo;&c_n;&c_n;&c_o;</trd>,
    <trd>&c_v; &c_n;&c_je;&c_g;&c_o;&c_d;&c_o;&c_v;&c_a;&c_n;&c_i;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">rozhor&#x10D;ene</trd>
</drv>

<drv mrk="indign.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Ka&ubreve;zi, naski <tld/>on:
      <ekz>
        lia mallojala konduto <tld/>igas &ccirc;iujn;
      </ekz>
      <ekz>
        <tld/>iga trompo, perfido.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ofend.0i">ofendi</ref>,
      <ref cel="star">harstariga</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_u;&c_r;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>budit pohor&#x161;en&#xED;</trd>,
    <trd>indignovat</trd>,
    <trd>pohn&#x11B;vat</trd>,
    <trd>rozho&#x159;&#x10D;it</trd>,
    <trd>zp&#x16F;sobit pohor&#x161;en&#xED;</trd>
  </trdgrp>
  <trd lng="en">cause indignation</trd>
  <trdgrp lng="fr">
    <trd>indigner</trd>,
    <trd>r&eacute;volter<klr tip="ind"> (causer de l'indignation)</klr></trd>
  </trdgrp>
  <trd lng="hu">felh&aacute;bor&iacute;t</trd>
  <trd lng="nl">verontwaardigen</trd>
  <trd lng="pt">indignar<klr tip="amb"> (causar indigna&ccedil;&atilde;o)</klr></trd>
  <trd lng="ru">&c_v;&c_o;&c_z;&c_m;&c_u;&c_shch;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>poburova&#x165;</trd>,
    <trd>rozhor&#x10D;i&#x165;</trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&#x431;&#x443;&#x440;&#x44E;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="indign.0indajxo">
  <kap><tld/>inda&jcirc;o<fnt>Z</fnt></kap>
  <snc>
    <ref tip="dif" cel="skanda.0o">skandalo.</ref>
  </snc>
  <trd lng="en">indignity</trd>
  <trdgrp lng="hu">
    <trd>botr&aacute;ny</trd>,
    <trd>skandalum</trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&#x431;&#x443;&#x440;&#x43B;&#x438;&#x432;&#x438;&#x439; &#x432;&#x447;&#x438;&#x43D;&#x43E;&#x43A;</trd>
</drv>

<drv mrk="indign.0igajxo">
  <kap><tld/>iga&jcirc;o<fnt>Z</fnt></kap>
  <snc>
    <ref tip="dif" cel="skanda.0o">skandalo.</ref>
  </snc>
  <trd lng="en">indignity</trd>
  <trdgrp lng="hu">
    <trd>botr&aacute;ny</trd>,
    <trd>skandalum</trd>
  </trdgrp>
</drv>
</art>
