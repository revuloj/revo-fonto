<art mrk="$Id: klauxn.xml,v 1.18 2023/12/03 09:58:50 revo Exp $">
<kap>
  <rad>kla&ubreve;n</rad>/o
</kap>
<drv mrk="klauxn.0o">
  <kap><tld/>o</kap>
  <snc mrk="klauxn.0o.KOMUNE">
    <dif>
      <ref tip="malprt" cel="cirk.0o">Cirka</ref> <ref tip="super" cel="amuz.0isto.KOMUNE">amuzisto</ref>:
      <ekz>
        la tintilo de la <tld/>o tintis
        <fnt><bib>Fab3</bib>, <lok>&scirc;tono de la sa&gcirc;uloj</lok></fnt>;
      </ekz>
      <ekz>
        estu muziko, <tld/>oj, aliaj porinfanaj distra&jcirc;oj kaj amuza&jcirc;oj
        <fnt>
        <aut>Z. Lapcionak</aut>,
        <vrk>Bangalor...</vrk>,
        <lok>soc.culture.esperanto, 2003-08-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_o;&c_w;&c_n;</trd>
  <trd lng="bg">&c_k;&c_l;&c_o;&c_u;&c_n;</trd>
  <trd lng="ca">pallasso</trd>
  <trdgrp lng="de">
    <trd>Clown</trd>,
    <trd>Spa&szlig;macher</trd>
  </trdgrp>
  <trd lng="en">clown</trd>
  <trd lng="es">payaso</trd>
  <trd lng="fa">&a_d;&a_l;&a_q;&f_k;</trd>
  <trd lng="fr">clown</trd>
  <trdgrp lng="nl">
    <trd>clown</trd>,
    <trd>pipo</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>b&lstroke;azen</trd>,
    <trd>klaun</trd>,
    <trd>pajac</trd>
  </trdgrp>   
  <trd lng="ro">clovn</trd>
  <trd lng="ru">&c_k;&c_l;&c_o;&c_u;&c_n;</trd>
  <trd lng="tr">palya&ccedil;o</trd>
  <trd lng="uk">&#x43A;&#x43B;&#x43E;&c_u;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x5C0F;&#x4E11; <pr>xi&#x1CE;och&#x1D2;u</pr></trd>,
    <trd>&#x4E11;&#x89D2; <pr>ch&#x1D2;uju&#xE9;</pr></trd>,
    <trd>&#x6D3B;&#x5B9D; <pr>hu&#xF3;b&#x1CE;o</pr></trd>,
    <trd>&#x4EBA;&#x4F86;&#x760B; <pr>r&#xE9;nl&#xE1;if&#x113;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="klauxn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="klauxn.0i.KOMUNE">
    <dif>
      Prezenti facilajn ske&ccirc;ojn, ridigajn amuza&jcirc;ojn kiel <tld/>o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        kiam li ne <tld/>as, li aspektas kiel tre serioza kaj respondeca
        politikisto
        <fnt>
        <aut>nikst</aut>,
        <vrk>Re: Fwd: Usona &gcirc;endarmo denove agresas jam en
        Jugoslavio!</vrk>,
        <lok>soc.culture.esperanto, 1999-03-30</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_a;&c_z;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_d;&c_u;&c_r;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="fr">faire le <ind>clown</ind></trd>
  <trdgrp lng="nl">
    <trd><ind>clown</ind> spelen</trd>,
    <trd>de <ind>clown</ind> uithangen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>b&lstroke;aznowa&cacute;</trd>,
    <trd>pajacowa&cacute;</trd>,
    <trd>wyg&lstroke;upia&cacute; si&eogonek;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>p&abreve;c&abreve;li</trd>,
    <trd>pierde vremea</trd>
  </trdgrp> 
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_v;&#x43E;&c_d;&c_i;&c_t;&c_i;&c_s;&#x44F; &#x44F;&#x43A; &#x43A;&#x43B;&#x43E;&c_u;&#x43D;</trd>,
    <trd>&c_b;&#x43B;&c_a;&c_z;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x8CBD;&#x7B11; <pr>y&#xED;xi&#xE0;o</pr></trd>
</drv>

<drv mrk="klauxn.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="klauxn.0ajxo.KOMUNE">
    <dif>
      Ridiga prezenta&jcirc;o:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi simple publike ridindigas vin per tre mallertaj kaj ne
        &gcirc;isfine
        pripensitaj <tld/>a&jcirc;oj
        <fnt>
        <aut>Pejno S.</aut>,
        <vrk>Chu fu&scirc;ulo au klerulo?</vrk>,
        <lok>soc.culture.esperanto, 1999-01-27</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_l;&c_a;&c_w;&c_n;&c_a;&c_d;&c_a;</trd>
  <trd lng="fa">&a_d;&a_l;&a_q;&f_k;&zwnj;&a_b;&a_A;&a_z;&f_y;</trd>
  <trd lng="fr">clownerie</trd>
  <trd lng="nl">clownerie</trd>
  <trdgrp lng="pl">
    <trd>b&lstroke;aze&nacute;stwo</trd>,
    <trd>wyg&lstroke;up</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>aiureal&abreve;</trd>,
    <trd>comportare prosteasc&abreve;</trd>,
    <trd>bufonerie</trd>
  </trdgrp> 
  <trdgrp lng="uk">
    <trd>&#x43A;&#x43B;&#x43E;&c_u;&#x43D;&c_a;&c_d;&c_a;</trd>,
    <trd>&c_b;&#x43B;&c_a;&c_z;&c_je;&#x43D;&c_s;&c_t;&c_v;&#x43E;</trd>
  </trdgrp>
  <trd lng="zh">&#x72D7;&#x5C41; <pr>g&#x1D2;up&#xEC;</pr></trd>
</drv>
</art>
