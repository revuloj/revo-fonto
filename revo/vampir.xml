<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>vampir</rad>/o
</kap>
<drv mrk="vampir.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Sudamerika <ref tip="lst" cel="vesper1.0o"
      lst="voko:vespertoj">vesperto</ref>,
      kiu nutras sin nur de sango de aliaj bestoj;
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:zoologiaj_genroj">genro</ref>
      de <ref tip="malprt" cel="vesper1.0oj">vespertoj</ref>  (<trd
      lng="la">Desmodus</trd>).
    </dif>
    <trd lng="be">&c_v;&c_a;&c_m;&c_p;&c_ib;&c_r;</trd>
    <trd lng="de">Vampir <klr tip="amb">(Fledermaus)</klr></trd>
    <trd lng="hu">v&aacute;mp&iacute;r</trd>
    <trd lng="pt">vampiro<klr tip="amb"> (morcego)</klr></trd>
    <trd lng="sv">vampyr</trd>
  </snc>
  <snc>
    <dif>
      Fabela fantomo eliranta nokte el tombo por su&ccirc;i sangon de
      dormantoj.
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_a;&c_m;&c_p;&c_ib;&c_r;</trd>,
      <trd>&c_v;&c_u;&c_p;&c_y;&c_r;</trd>,
      <trd>&c_v;&c_u;&c_r;&c_d;&c_a;&c_l;&c_a;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Vampir</trd>
    <trdgrp lng="hu">
      <trd>v&aacute;mp&iacute;r</trd>,
      <trd>v&eacute;rsz&iacute;v&oacute; (k&iacute;s&eacute;rtet)</trd>
    </trdgrp>
    <trd lng="pt">vampiro</trd>
    <trd lng="sv">vampyr</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iu ri&ccirc;i&gcirc;anta je malprofito de aliaj; ekspluatanto.
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_a;&c_m;&c_p;&c_ib;&c_r;</trd>,
      <trd>&c_k;&c_r;&c_y;&c_v;&c_a;&c_s;&c_m;&c_o;&c_k;</trd>
    </trdgrp>
    <trd lng="de">Blutsauger</trd>
    <trd lng="el">&beta;&rho;&ypsilon;&kappa;&omikron_ton;&lambda;&alfa;&kappa;&alfa;&sigma_fina;</trd>
    <trdgrp lng="hu">
      <trd>v&aacute;mp&iacute;r</trd>,
      <trd>szipolyoz&oacute;</trd>
    </trdgrp>
    <trd lng="pt">vampiro<klr tip="amb"> (explorador)</klr></trd>
    <trd lng="sv">blodsugare</trd>
  </snc>
  <trd lng="bg">&c_v;&c_a;&c_m;&c_p;&c_i;&c_r;</trd>
  <trd lng="ca">vampir</trd>
  <trd lng="en">vampire</trd>
  <trd lng="es">vampiro</trd>
  <trdgrp lng="fr">
    <trd>vampire</trd>
  </trdgrp>
  <trd lng="nl">vampier</trd>
  <trd lng="ru">&c_v;&c_a;&c_m;&c_p;&c_i;&c_r;</trd>
  <trd lng="sk">up&iacute;r</trd>
</drv>
</art>
