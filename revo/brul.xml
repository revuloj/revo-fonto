<drv mrk="brul.ek0igi">
  <kap>ek<tld/>igi</kap>
  <snc mrk="brul.ek0igi.KOMUNE">
    <dif>
      Ekflamigi ion:
      <ekz>
        ek<tld/>igi fajron, kandelon;
      </ekz>
      <ekz>
        mi ek<tld/>igis fajron, kaj tuj estos al vi pli varme kaj pli
        lume
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la pastroj ek<tld/>igis la tor&ccirc;ojn
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ek<tld/>igis la lumojn (vd <ref tip="sin" cel="lum.ek0igi">eklumigi</ref>)
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ek<tld/>igis la kristnaskan arbon <klr>(la kandelojn sur
        &gcirc;i)</klr>
        <fnt>
          <bib>Fab2</bib>
