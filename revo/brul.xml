<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>brul</rad>/i
</kap>
<drv mrk="brul.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="brul.0i.konsumigxi">
    <dif>
      Konsumi&gcirc;i per fajro:
      <ekz>
        la ligno, la kandelo <tld/>as;
      </ekz>
      <ekz>
        fajro <tld/>etas sub la cindro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>la afero ne <tld/>as</ind>
        <klr>(ne ur&gcirc;as)</klr>
        <trd lng="fr">il n'y a pas le feu</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="nl">branden</trd>
    <trdgrp lng="pt">
      <trd>queimar</trd>,
      <trd>arder</trd>,
      <trd>comburir</trd>,
      <trd>esbrasear</trd>,
      <trd>incendiar-se</trd>
    </trdgrp>
  </snc>
  <snc mrk="brul.0i.varmi">
    <dif>
      Esti varmega kiel fajro:
      <ekz>
        via mano <tld/>as de febro;
      </ekz>
      <ekz>
        <tld/>e varmega fero
        <fnt><bib>NT</bib>, <lok>&Ti1; 4:2</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="nl">gloeien</trd>
    <trdgrp lng="pt">
      <trd>queimar</trd>,
      <trd>arder</trd>,
      <trd>escaldar</trd>
    </trdgrp>
  </snc>
  <snc mrk="brul.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti en forta eksciteco:
      <ekz>
        <tld/>i de amo, de scivolo, de deziro<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i pro senpacienco<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0i">ardi</ref>,
      <ref cel="flagr.0i">flagri</ref>,
      <ref cel="flam.0i">flami</ref>.
    </refgrp>
    <trd lng="nl">branden</trd>
    <trdgrp lng="pt">
      <trd>arder</trd>,
      <trd>inflamar-se</trd>,
      <trd>incendiar-se</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_g;&c_a;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="cs">ho&rcaron;et</trd>
  <trd lng="de">brennen</trd>
  <trd lng="en">burn</trd>
  <trd lng="es">quemar</trd>
  <trd lng="fr">bruler <klr tip="ind">(intr.)</klr></trd>
  <trd lng="hu">&eacute;g</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_g;&c_o;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_y;&c_l;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_y;&c_h;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">horie&tcaron;</trd>
</drv>

<drv mrk="brul.0o">
  <kap><tld/>o</kap>
  <snc mrk="brul.0o.KOMUNE">
    <dif>
      Stato de <tld/>anta&jcirc;o:
      <ekz>
        kia granda <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="de">Brand</trd>
  <trd lng="en">combustion</trd>
  <trd lng="fr">combustion</trd>
  <trdgrp lng="hu">
    <trd>&eacute;g&eacute;s</trd>,
    <trd>t&udblac;z</trd>
  </trdgrp>
  <trd lng="nl">brand</trd>
  <trdgrp lng="pt">
    <trd>ti&ccedil;&atilde;o</trd>, <trd>brasa</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">horenie</trd>
</drv>

<drv mrk="brul.0a">
  <kap><tld/>a</kap>
  <snc mrk="brul.0a.brulanta">
    <dif>
      <tld lit="B"/>anta.
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_a;&c_ju;&c_ch;&c_y;</trd>
    <trd lng="de">brennend</trd>
    <trd lng="en">burning</trd>
    <trd lng="fr">brulant</trd>
    <trd lng="hu">&eacute;g&odblac;</trd>
    <trdgrp lng="pt">
      <trd>ardente</trd>,
      <trd>candente</trd>,
      <trd>incandescente</trd>
    </trdgrp>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_ja;&c_shch;&c_i;&c_j;</trd>
    <trd lng="sk">horiaci</trd>
  </snc>
  <snc mrk="brul.0a.bruligebla">
    <ref tip="dif" cel="brul.0igebla"><tld/>igebla</ref>
    <ekz>
      ligno estas bona <tld/>a materialo
      <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>.
    </ekz>
    <trd lng="be">&c_g;&c_a;&c_r;&c_u;&c_ch;&c_y;</trd>
    <trdgrp lng="de">
      <trd>Brenn-</trd>,
      <trd>brennbar</trd>
    </trdgrp>
    <trd lng="en">combustible</trd>
    <trd lng="fr">combustible <klr tip="ind">(adj.)</klr></trd>
    <trd lng="hu">&eacute;ghet&odblac;</trd>
    <trd lng="nl">brandbaar</trd>
    <trd lng="pt">combust&iacute;vel</trd>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_ju;&c_ch;&c_i;&c_j;</trd>
    <trd lng="sk">hor&lcaron;av&yacute;</trd>
  </snc>
</drv>

<drv mrk="brul.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="brul.0ego.KOMUNE">
    <dif>
      Granda <tld/>o, ka&ubreve;zanta difektojn kaj perdojn.
    </dif>
    <refgrp tip="sin">
      <ref cel="incend.0o">incendio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_r;</trd>
  <trdgrp lng="de">
    <trd>Feuersbrunst</trd>,
    <trd>Gro&szlig;brand</trd>
  </trdgrp>
  <trd lng="en">inferno</trd>
  <trd lng="fr">incendie</trd>
  <trdgrp lng="hu">
    <trd>t&udblac;z</trd>,
    <trd>t&udblac;zv&eacute;sz</trd>
  </trdgrp>
  <trd lng="nl">brand</trd>
  <trd lng="pt">inc&ecirc;ndio</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_r;</trd>
</drv>

<drv mrk="brul.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="brul.0ema.KOMUNE">
    <dif>
      Facile <tld/>anta.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_jo;&c_g;&c_k;&c_a;&c_w;&c_z;&c_g;&c_a;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="de">entz&uuml;ndlich</trd>
  <trdgrp lng="en">
    <trd>flammable</trd>,
    <trd>volatile</trd>
  </trdgrp>
  <trd lng="fr">inflammable</trd>
  <trd lng="hu">gy&uacute;l&eacute;kony</trd>
  <trd lng="nl">ontvlambaar</trd>
  <trd lng="pt">inflam&aacute;vel</trd>
  <trd lng="ru">&c_l;&c_je;&c_g;&c_k;&c_o; <ind>&c_v;&c_o;&c_s;&c_p;&c_l;&c_a;&c_m;&c_je;&c_n;&c_ja;&c_ju;&c_shch;&c_i;&c_j;&c_s;&c_ja;</ind></trd>
  <trd lng="sk">hor&lcaron;av&yacute;</trd>
</drv>

<drv mrk="brul.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="brul.0igi.konsumigi">
    <dif>
      Konsumigi per fajro:
      <ekz>
        <tld/>igi lignon, paperon, herezulon, domon;
      </ekz>
      <ekz>
        post infekta malsano oni ofte <tld/>igas la vestojn de la malsanulo
        <fnt><bib>F</bib>, <lok>&FE; 39</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="cindr.0igi">cindrigi</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>&eacute;get</trd>,
      <trd>el&eacute;get</trd>
    </trdgrp>
  </snc>
  <snc mrk="brul.0igi.vundi">
    <ref tip="dif" cel="brul.0difekti0vundi"><tld lit="B"/>vundi</ref>
    <ekz>
      ne estingu la fajron, kiu vin ne
      <tld/>igas<fnt><bib>PrV</bib></fnt>.
    </ekz>
    <trd lng="hu">meg&eacute;get</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_ja;&c_ch;&c_y;</trd>
  </trdgrp>
  <trd lng="de">verbrennen</trd>
  <trdgrp lng="en">
    <trd>burn</trd>
  </trdgrp>
  <trd lng="fr">bruler <klr tip="ind">(tr.)</klr></trd>
  <trd lng="nl">verbranden</trd>
  <trdgrp lng="pt">
    <trd>queimar</trd>,
    <trd>atear <klr>(fogo)</klr></trd>,
    <trd>acender <klr>(o fogo)</klr></trd>,
    <trd>incendiar</trd>,
    <trd>p&ocirc;r fogo</trd>,
    <trd>tacar <klr>(fogo)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_zh;&c_je;&c_ch;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">p&aacute;li&tcaron;</trd>
</drv>

<drv mrk="brul.0igajxo">
  <kap><tld/>iga&jcirc;o</kap>
  <snc mrk="brul.0igajxo.KOMUNE">
    <dif>
      Substanco, uzata hejme a&ubreve; industrie por produkti
      varmon per <tld/>igo.
    </dif>
    <refgrp tip="sub">
      <ref cel="gas.brul0o">brulgaso</ref>,
      <ref cel="karb.0o">karbo</ref>,
      <ref cel="naft.0o">nafto</ref>,
      <ref cel="petrol.0o">petrolo</ref>,
      <ref cel="urani.0o">uranio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;</trd>
  <trd lng="fr">combustible <klr tip="ind">(subst.)</klr></trd>
  <trdgrp lng="en">
    <trd>fuel</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>t&uuml;zel&odblac;</trd>,
    <trd>t&uuml;zel&odblac;anyag</trd>
  </trdgrp>
  <trd lng="nl">brandstof</trd>
  <trd lng="pt">combust&iacute;vel</trd>
  <trd lng="ru">&c_t;&c_o;&c_p;&c_l;&c_i;&c_v;&c_o;</trd>
</drv>

<drv mrk="brul.0igebla">
  <kap><tld/>igebla, <tld/>ebla<fnt>Z</fnt></kap>
  <snc mrk="brul.0igebla.KOMUNE">
    <dif>
      Povanta <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_u;&c_ch;&c_y;</trd>
  <trd lng="de">brennbar</trd>
  <trdgrp lng="en">
    <trd>combustible</trd>
  </trdgrp>
  <trd lng="fr">combustible <klr tip="ind">(adj.)</klr></trd>
  <trd lng="hu">&eacute;ghet&odblac;</trd>
  <trd lng="nl">brandbaar</trd>
  <trd lng="pt">combust&iacute;vel</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_ju;&c_ch;&c_i;&c_j;</trd>
  <trd lng="sk">hor&lcaron;av&yacute;</trd>
</drv>

<drv mrk="brul.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="brul.0ilo.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Parto de lumigilo a&ubreve; varmigilo, el kiu eli&gcirc;as la flamo.
    </dif>
  </snc>
  <adm>
    Kontrastigi rilate al "flamingo".
    <aut>MB</aut>
  </adm>
  <trd lng="be">&c_g;&c_a;&c_r;&c_e;&c_l;&c_k;&c_a; <klr tip="amb">(&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a;)</klr></trd>
  <trd lng="de">Brenner</trd>
  <trdgrp lng="en">
    <trd>burner</trd>
  </trdgrp>
  <trd lng="fr">bruleur</trd>
  <trd lng="hu">&eacute;g&odblac;</trd>
  <trd lng="nl">brander</trd>
  <trd lng="pt">pavio</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_je;&c_l;&c_k;&c_a;</trd>
</drv>

<drv mrk="brul.0umo">
  <kap><ofc>*</ofc><tld/>umo</kap>
  <snc mrk="brul.0umo.MED">
    <uzo tip="fak">MED</uzo>
    <ref tip="dif" cel="inflam.0o">inflamo</ref>
  <ekz>
    personoj, kiuj rigardis la eklipson sen speciala protekto 
    <klr>...</klr>, estas enhospitaligitaj kun pli malpli gravaj 
    retin<tld/>umoj
    <fnt>
      <aut>P. Levy</aut>,
      <vrk>Re: Eklipso kaj katastrofo - Transmonden!</vrk>,
      <lok><url ref="http://groups.google.be/group/soc.culture.esperanto?hl=eo">soc.culture.esperanto</url>, 1999-08-18</lok></fnt>.
  </ekz>
  </snc>
  <trd lng="be">&c_z;&c_a;&c_p;&c_a;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="en">
    <trd>inflammation</trd>
  </trdgrp>
  <trd lng="fr">inflammation</trd>
  <trd lng="hu">gyullad&aacute;s</trd>
  <trd lng="nl">ontsteking<klr tip="ind"> (ziekte)</klr></trd>
  <trd lng="pt">inflama&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_v;&c_o;&c_s;&c_p;&c_a;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>
</drv>

<drv mrk="brul.al0i">
  <kap>al<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="brul.al0i.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <tld lit="B"/>glui&gcirc;i:
      <ekz>
        la terpomoj al<tld/>is en la kaserolo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_g;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>
  <trd lng="fr">attacher<klr> (cuisine)</klr></trd>
  <trdgrp lng="hu">
    <trd>oda&eacute;g</trd>,
    <trd>odakozm&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">aanbranden</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol; <klr>(&c_k; &c_ch;&c_je;&c_m;&c_u;-&c_l;., &c_n;&c_a;&c_p;&c_r;., &c_k;
  &c_k;&c_a;&c_s;&c_t;&c_r;&c_ju;&c_l;&c_je;)</klr></trd>
</drv>

<drv mrk="brul.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="brul.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="B"/>i &ccirc;e la &ccirc;irka&ubreve;o:
      <ekz>
        miaj ostoj &ccirc;irka&ubreve;<tld/>is kiel en forno
        <fnt><bib>MT</bib><lok>&Psa; 102:3</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> komencis kolekti kaj glatigi la
        &ccirc;irka&ubreve;<tld/>intajn foliojn
        <fnt><bib>MkM</bib><lok>&ccirc;ap. 14</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_g;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>
  <trd lng="fr">bruler<klr tip="ind"> (intr., en p&eacute;riph&eacute;rie)</klr></trd>
</drv>

<drv mrk="brul.ek0igi">
  <kap>ek<tld/>igi</kap>
  <snc mrk="brul.ek0igi.KOMUNE">
    <dif>
      Ekflamigi ion:
      <ekz>
        ek<tld/>igi fajron, kandelon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anz&uuml;nden</trd>,
    <trd>entz&uuml;nden</trd>,
    <trd>entflammen</trd>,
    <trd>entfachen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>ignite</trd>,
    <trd>light<klr> up</klr></trd>,
    <trd>spark</trd>,
    <trd>strike</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>allumer <klr tip="ind">(mettre le feu &agrave;)</klr></trd>,
    <trd>mettre le <ind>feu</ind> (&agrave;)</trd>
  </trdgrp>
  <trd lng="hu">meggy&uacute;jt</trd>
  <trd lng="nl">aansteken</trd>
  <trdgrp lng="pt">
    <trd>atear <klr>(fogo)</klr></trd>,
    <trd>acender <klr>(o fogo)</klr></trd>,
    <trd>tacar <klr>(fogo)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_z;&c_a;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_zh;&c_je;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zap&aacute;li&tcaron;</trd>
</drv>

<drv mrk="brul.0difekti0vundi">
  <kap><tld/>difekti, <tld/>vundi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Vundi, difekti per fajro a&ubreve; varmego:
      <ekz>
        <ind><tld/>vundo, <tld/>difekto</ind>;
        <trd lng="fr">brulure</trd>
      </ekz>
      <ekz>
        li <tld/>vundis sian fingron &ccirc;e la forno;
      </ekz>
      <ekz>
        <klr>[donu]</klr> <tld/>difekton pro <tld/>difekto, vundon pro
        vundo, kontuzon pro kontuzo
        <fnt><bib>MT</bib>, <lok>&Eli; 21:25</lok></fnt>;
      </ekz>
      <ekz>
        la blovilo <tld/>difekti&gcirc;is
