      <ref cel="urani.0o">uranio</ref>
    </refgrp>
    <trdgrp lng="zh">
    <trd>&#x835B; <pr>r&#xE1;o</pr></trd>,
    <trd>&#x85AA; <pr>x&#x12B;n</pr></trd>,
    <trd>&#x71C3;&#26009; <pr>r&#xE1;nli&#xE0;o</pr></trd>,
    <trd>&#x71C3;&#x70E7;&#29289; <pr>r&#xE1;nsh&amacron;ow&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_v;&c_a;</trd>
