<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>brul</rad>/i
</kap>
<drv mrk="brul.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc num="1">
    <dif>
      Konsumi&gcirc;i per fajro:
      <ekz>
        la ligno, la kandelo <tld/>as;
      </ekz>
      <ekz>
        fajro <tld/>etas sub la cindro
      </ekz>
      <ekz>
        <klr>(f)</klr> la afero ne <tld/>as
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>
        <klr>(ne ur&gcirc;as)</klr>.
      </ekz> 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Esti varmega kiel fajro:
      <ekz>
        via mano <tld/>as de febro;
      </ekz>
      <ekz>
        <tld/>e varmega fero
        <fnt>(<vrk>Nova testamento</vrk>,
        <lok>I. Timoteo 4, 2</lok>)</fnt>.
      </ekz> 
    </dif>
  </snc>
  <snc num="3">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Esti en forta eksciteco:
      <ekz>
        <tld/>i de amo, de scivolo, de deziro<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i pro senpacienco<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <refgrp tip="sin">
      <ref cel="ard.0i">ardi</ref>,
      <ref cel="flam.0i">flami</ref>,
      <ref cel="flagr.0i">flagri</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">ho&rcaron;et</trd>
  <trd lng="de">brennen</trd>
  <trd lng="en">burn</trd>
</drv>
<drv mrk="brul.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Stato de <tld/>anta&jcirc;o. 
    </dif>
  </snc>
  <trd lng="de">Brand</trd>
</drv>
<drv mrk="brul.0ego">
  <kap><tld/>ego</kap>
  <snc>
    <dif>
      Granda <tld/>o, ka&ubreve;zanta difektojn k perdojn. 
    </dif>
    <refgrp tip="sin">
      <ref cel="incend.0o">incendio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Feuersbrunst</trd>,
    <trd>Gro&szlig;brand</trd>
  </trdgrp>
</drv>
<drv mrk="brul.0a">
  <kap><tld/>a</kap>
  <snc num="1">
    <dif>
      <tld/>anta. 
    </dif>
    <trd lng="de">brennend</trd>
  </snc>
  <snc num="2">
    <dif>
      <tld/>igebla:
      <ekz>
        ligno estas bona <tld/>a materialo<fnt>Z</fnt>.
      </ekz> 
    </dif>
    <trdgrp lng="de"><trd>Brenn-</trd>, <trd>brennbar</trd></trdgrp>
  </snc>
</drv>
<drv mrk="brul.0igi">
  <kap><tld/>igi</kap>
  <snc num="1">
    <dif>
      Konsumigi de fajro:
      <ekz>
        <tld/>igi lignon, paperon, herezulon, domon.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="cindr.0igi">cindrigi</ref>.
    </refgrp>
  </snc>
  <snc num="2">
    <dif>
      <tld/>vundi:
      <ekz>
        ne estingu la fajron, kiu vin ne <tld/>igas
        <fnt>(<aut>Zamenhof</aut>: <vrk>Proverbaro</vrk>)</fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="de">verbrennen</trd>
</drv>
<drv mrk="brul.ek0igi">
  <kap>ek<tld/>igi</kap>
  <snc>
    <dif>
      Ekflamigi ion:
      <ekz>
        ek<tld/>igi fajron, kandelon.
      </ekz> 
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>anz&uuml;nden</trd>,
    <trd>entz&uuml;nden</trd>,
    <trd>entflammen</trd>,
    <trd>entfachen</trd>
  </trdgrp>
</drv>
<drv mrk="brul.0igebla">
  <kap><tld/>ebla<fnt>Z</fnt>, <tld/>igebla</kap>
  <snc>
    <dif>
      Povanta <tld/>i. 
    </dif>
  </snc>
  <trd lng="de">brennbar</trd>
</drv>
<drv mrk="brul.0ema">
  <kap><tld/>ema</kap>
  <snc>
    <dif>
      Facile <tld/>anta. 
    </dif>
  </snc>
  <trd lng="de">entz&uuml;ndlich</trd>
</drv>
<drv mrk="brul.0umo">
  <kap><tld/>umo</kap>
  <snc>
    <ref tip="dif" cel="inflam.0o">Inflamo.</ref> 
  </snc>
</drv>
<drv mrk="brul.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Parto de lumigilo a&ubreve; varmigilo el kiu eli&gcirc;as la flamo. 
    </dif>
  </snc>
  <trd lng="de">Brenner</trd>
</drv>
<drv mrk="brul.0difekti0vundi">
  <kap><tld/>difekti, <tld/>vundi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Vundi, difekti per fajro a&ubreve; varmego:
      <ekz>
        li <tld/>vundis sian
        fingron &ccirc;e la forno.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">verbrennen</trd>
</drv>
</art>
