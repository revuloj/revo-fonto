<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brul.xml,v 1.143 2024/12/14 12:50:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>brul</rad>/i <fnt><bib>UV</bib></fnt>
</kap>
<drv mrk="brul.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="brul.0i.konsumigxi">
    <dif>
      <ref cel="oksid.0o">Oksid</ref>i&gcirc;i produktante <ref tip="prt" cel="varm.0o.FIZ">varmo</ref>n
      kaj <ref tip="prt" cel="lum.0o.FIZ">lumo</ref>n:
      <ekz>
        jen la arbeta&jcirc;o <tld/>as en la fajro
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 3:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        fendita ligno facile ek<tld/>as
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        sen fajro ne <tld/>as e&ccirc; pajlo
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; la trono <tld/>is sep fajraj tor&ccirc;oj
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 4:5</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>la afero ne <tld/>as</ind>
        <klr>(ne ur&gcirc;as)</klr>
        <trd lng="fr">il n'y a pas le feu</trd>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="nl">branden</trd>
    <trdgrp lng="pl">
      <trd>pali&cacute; si&eogonek;</trd>,
      <trd>p&lstroke;on&aogonek;&cacute;</trd>,
      <trd>spala&cacute; si&eogonek;</trd>,
      <trd>gorze&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>queimar</trd>,
      <trd>arder</trd>,
      <trd>comburir</trd>,
      <trd>esbrasear</trd>,
      <trd>incendiar-se</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x71C3;&#x8D77; <pr>r&#xE1;nq&#x1D0;</pr></trd>,
    <trd>&#x71C3;&#x70E7; <pr>r&#xE1;nsh&amacron;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="brul.0i.konsumi">
    <dif>
      Elmontri varmon kaj lumon de io, kio <tld/>as:
      <ekz>
        abunda fajro brulis sur la kameno
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        blue <tld/>anta flamo <klr>[&#8230;]</klr> lumigis la tutan salonon
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lampo <klr>[&#8230;]</klr> <tld/>as kaj lumas
        <fnt>
          <bib>NT</bib>
          <lok>Johano 5:35</lok>
        </fnt>;
      </ekz>
      <ekz>
        eterna fajro <tld/>u sur la altaro, ne estingi&gcirc;u
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 6:13</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x70E7;&#25481; <pr>sh&amacron;odi&#xE0;o</pr></trd>
</snc>
  <snc mrk="brul.0i.varmi">
    <dif>
      Esti varmega, doloriga kiel io <tld/>anta:
      <ekz>
        <tld/>e varmega fero
        <fnt><bib>NT</bib>, <lok>&Ti1; 4:2</lok></fnt>;
      </ekz>
      <ekz>
        via mano <tld/>as de febro;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        se kalumnio ne <tld/>as, &gcirc;i almena&ubreve; makulas
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="nl">gloeien</trd>
    <trdgrp lng="pl">
      <trd>pali&cacute; si&eogonek;</trd>,
      <trd>p&lstroke;on&aogonek;&cacute;</trd>,
      <trd>pra&zdot;y&cacute;</trd>,
      <trd>gorze&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>queimar</trd>,
      <trd>arder</trd>,
      <trd>escaldar</trd>
    </trdgrp>
  </snc>
  <snc mrk="brul.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Esti en forta eksciteco:
      <ekz>
        <tld/>i de scivolo;
      </ekz>
      <ekz>
        mi <tld/>as de amo
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        nia koro <klr>[&#8230;]</klr> <tld/>is en ni, dum li parolis
        <fnt>
          <bib>NT</bib>
          <lok>Luko 24:32</lok>
        </fnt>;
      </ekz>
      <ekz>
        la virino <tld/>is en sia volupto
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 1:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        la virineto de maro <tld/>is de deziro vidi &scirc;ian belecon
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ashundoj, kiuj <tld/>is de deziro kuri al la &ccirc;aso
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro IX</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi <tld/>as de senpacienco
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ard.0i">ardi</ref>,
      <ref cel="flagr.0i">flagri</ref>,
      <ref cel="flam.0i">flami</ref>.
    </refgrp>
    <trd lng="nl">branden</trd>
    <trdgrp lng="pl">
      <trd>pali&cacute; si&eogonek;</trd>,
      <trd>p&lstroke;on&aogonek;&cacute;</trd>,
      <trd>spala&cacute; si&eogonek;</trd>,
      <trd>gorze&cacute;</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>arder</trd>,
      <trd>inflamar-se</trd>,
      <trd>incendiar-se</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_r;&c_e;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_g;&c_a;&c_r;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_y;&c_m;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>ho&rcaron;et</trd>,
    <trd>ho&rcaron;et</trd>,
    <trd>p&aacute;lit</trd>,
    <trd>sv&iacute;tit <klr>(slunce)</klr></trd>
  </trdgrp>
  <trd lng="de">brennen</trd>
  <trd lng="en">burn</trd>
  <trd lng="es">quemar</trd>
  <trd lng="fr">bruler <klr tip="ind">(intr.)</klr></trd>
  <trd lng="he">&lamed;&shin;&resh;&vav;&fpe;</trd>
  <trd lng="hu">&eacute;g</trd>
  <trd lng="id"><baz>bakar</baz>terbakar</trd>
  <trd lng="ja">&#x71C3;&#12360;&#x308B;  <pr>&#12418;&#12360;&#x308B;</pr></trd>
  <trd lng="ro">arde</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_r;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_g;&c_o;&c_r;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_y;&c_l;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_y;&c_h;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">horie&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_o;&c_r;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_g;&c_o;&c_r;&c_ja;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_a;&c_l;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="brul.0o">
  <kap><tld/>o</kap>
  <snc mrk="brul.0o.KOMUNE">
    <dif>
      La procezo kaj rezultanta varmo kaj lumo dum io <tld/>as:
      <ekz>
        kia granda <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>!
      </ekz>
      <ekz>
        ne ekzistas fumo sen <tld/>o
        <fnt>
          <bib>Prv</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <klr>[&#8230;]</klr> rimarkas <tld/>on komenci&gcirc;antan en la
        kvartalo
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i anoncadis <klr>[&#8230;]</klr> alarmon pri <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Malnova pre&gcirc;eja sonorilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi lasus mian korpon por for<tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>I. Korintanoj 13:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        savita el <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Amos 4:11</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fra&ubreve;linoj <klr>[&#8230;]</klr> rapide estingis la <tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>Abio</lok>
        </fnt>;
      </ekz>
      <ekz>
        la gardisto fajfis, &dash; estis <tld/>o, la tuta strato estis hele
        lumita
        <fnt>
          <bib>Fab3</bib>
          <lok>Koboldo &ccirc;e la butikisto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="cs">
    <trd>ho&rcaron;en&iacute;</trd>,
    <trd>ohe&ncaron;</trd>
  </trdgrp>
  <trd lng="de">Brand</trd>
  <trd lng="en">combustion</trd>
  <trdgrp lng="es">
    <trd>quema</trd>,
    <trd>combusti&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">combustion</trd>
  <trd lng="he">&shin;&resh;&yod;&pe;&he;</trd>
  <trdgrp lng="hu">
    <trd>&eacute;g&eacute;s</trd>,
    <trd>t&udblac;z</trd>
  </trdgrp>
  <trd lng="id"><baz>bakar</baz>pembakaran</trd>
  <trdgrp lng="ja">
    <trd>&#x71C3;&#x713C;  <pr>&#x306D;&#12435;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#x706B;&#x4E8B;  <pr>&#x304B;&#12376;</pr></trd>
  </trdgrp>
  <trd lng="nl">brand</trd>
  <trdgrp lng="pl">
    <trd>spalanie</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>ti&ccedil;&atilde;o</trd>, <trd>brasa</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  <trd lng="sk">horenie</trd>
  <trd lng="uk">&c_g;&c_o;&c_r;&c_ib;&c_n;&c_n;&c_ja;</trd>
  <trdgrp lng="zh">
    <trd>&#x81EA;&#x52A8;&#x5F00;&#x706B; <pr>z&#xEC;d&#xF2;ngk&amacron;ihu&#x1D2;</pr></trd>,
    <trd>&#x706B;&#x52BF; <pr>hu&#x1D2;sh&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="brul.0a">
  <kap><tld/>a</kap>
  <snc mrk="brul.0a.brulanta">
    <dif>
      <tld lit="B"/>anta:
      <ekz>
        <tld/>a kiel lafo
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk>La Stranga Butiko</vrk>
          <lok>Nigra Tago</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>a suno
        <fnt>
          <bib>Fab2</bib>
          <lok>La ombro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_a;&c_ju;&c_ch;&c_y;</trd>
    <trd lng="de">brennend</trd>
    <trd lng="en">burning</trd>
    <trd lng="es">abrasador</trd>
    <trd lng="fr">brulant</trd>
    <trd lng="hu">&eacute;g&odblac;</trd>
    <trd lng="id"><baz>bakar</baz>terbakar</trd>
    <trdgrp lng="pl">
      <trd>pal&aogonek;cy</trd>,
      <trd>p&lstroke;on&aogonek;cy</trd>,
      <trd>spalaj&aogonek;cy si&eogonek;</trd>,
      <trd>pra&zdot;&aogonek;cy</trd>,
      <trd>gorej&aogonek;cy</trd>
    </trdgrp>
    <trdgrp lng="pt">
      <trd>ardente</trd>,
      <trd>candente</trd>,
      <trd>incandescente</trd>
    </trdgrp>
    <trd lng="ro">aprins</trd>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_ja;&c_shch;&c_i;&c_j;</trd>
    <trd lng="sk">horiaci</trd>
    <trdgrp lng="zh">
    <trd>&#x70E4; <pr>k&#x1CE;o</pr></trd>,
    <trd>&#x9A55;&#x967D;&#22914;&#x4F19; <pr>ji&amacron;oy&#xE1;ngr&#xFA;hu&#x1D2;</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="brul.0a.bruligebla">
    <dif>
      <ref tip="dif" cel="brul.0igebla"><tld lit="B"/>igebla</ref>:
      <ekz>
        ligno estas bona <tld/>a materialo
        <fnt><bib>F</bib>, <lok>&FE; 31</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_g;&c_a;&c_r;&c_u;&c_ch;&c_y;</trd>
    <trdgrp lng="de">
      <trd>Brenn-</trd>,
      <trd>brennbar</trd>
    </trdgrp>
    <trd lng="en">combustible</trd>
    <trd lng="es">combustible <klr tip="ind">(adj.)</klr></trd>
    <trd lng="fr">combustible <klr tip="ind">(adj.)</klr></trd>
    <trd lng="hu">&eacute;ghet&odblac;</trd>
    <trd lng="id"><ind>dapat</ind> dibakar</trd>
    <trd lng="nl">brandbaar</trd>
    <trdgrp lng="pl">
      <trd>palny</trd>
    </trdgrp>
    <trd lng="pt">combust&iacute;vel</trd>
    <trd lng="ru">&c_g;&c_o;&c_r;&c_ju;&c_ch;&c_i;&c_j;</trd>
    <trd lng="sk">hor&lcaron;av&yacute;</trd>
    <trd lng="zh">&#26131;&#x71C3;&#30340; <pr>y&#xEC;r&#xE1;nde</pr></trd>
</snc>
  <trdgrp lng="cs">
    <trd>o&zcaron;ehav&yacute;</trd>,
    <trd>pal&ccaron;iv&yacute;</trd>
  </trdgrp>
  <trd lng="he">&bet;&vav;&ayin;&resh;</trd>
  <trdgrp lng="ja">
    <trd>&#x71C3;&#12360;&#x308B;&#12424;&#12358;&#x306A;  <pr>&#12418;&#12360;&#x308B;&#12424;&#12358;&#x306A;</pr></trd>,
    <trd>&#x71C3;&#x713C;&#x306E;  <pr>&#x306D;&#12435;&#12375;&#12423;&#12358;&#x306E;</pr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>vznetliv&yacute;</trd>,
    <trd>z&aacute;paln&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="brul.0ego">
  <kap><tld/>ego</kap>
  <snc mrk="brul.0ego.KOMUNE">
    <dif>
      Granda <tld/>o, ka&ubreve;zanta difektojn kaj perdojn:
      <ekz>
        ve, la granda urbo, en unu horo pereis <klr>[...]</klr> kaj &ccirc;iuj
        <klr>[...]</klr>
        staris malproksime kaj kriis, vidante la fumon de &scirc;ia <tld/>ego
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 18:16-18</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="incend.0o">incendio</ref>
    </refgrp>
    <trd lng="zh">&#x71F9; <pr>xi&#x1CE;n</pr></trd>
</snc>
  <trd lng="be">&c_p;&c_a;&c_zh;&c_a;&c_r;</trd>
  <trd lng="cs">po&zcaron;&aacute;r</trd>
  <trdgrp lng="de">
    <trd>Feuersbrunst</trd>,
    <trd>Gro&szlig;brand</trd>
  </trdgrp>
  <trd lng="en">inferno</trd>
  <trd lng="es">incendio</trd>
  <trd lng="fr">incendie</trd>
  <trd lng="he">&shin;&resh;&yod;&pe;&he;</trd>
  <trdgrp lng="hu">
    <trd>t&udblac;z</trd>,
    <trd>t&udblac;zv&eacute;sz</trd>
  </trdgrp>
  <trd lng="id"><baz>bakar</baz>kebakaran</trd>
  <trdgrp lng="ja">
    <trd>&#22823;&#x706B;&#x4E8B;  <pr>&#x304A;&#x304A;&#x304B;&#12376;</pr></trd>,
    <trd>&#x706B;&#x707D;  <pr>&#x304B;&#12373;&#12356;</pr></trd>
  </trdgrp>
  <trd lng="nl">brand</trd>
  <trd lng="pl">po&zdot;ar</trd>
  <trd lng="pt">inc&ecirc;ndio</trd>
  <trd lng="ro">incendiu</trd>
  <trd lng="ru">&c_p;&c_o;&c_zh;&c_a;&c_r;</trd>
  <trd lng="sk">po&zcaron;iar</trd>
  <trd lng="uk">&c_p;&c_o;&c_zh;&c_je;&c_zh;&c_a;</trd>
</drv>

<drv mrk="brul.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="brul.0ema.KOMUNE">
    <dif>
      Facile <tld/>anta:
      <ekz>
        beda&ubreve;rinde la planko estis kovrita per <tld/>ema plasto;
      </ekz>
      <ekz>
        <klr>[li]</klr> ekzamenis la esti&gcirc;intan gason kaj realigis la
        eksperimenton
        nomatan nuntempe teston de oksihidrogeno, kiu bruligis la gason,

        li tial nomis &gcirc;in <tld/>ema aero
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1601">Hidrogeno</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <ref tip="vid" cel="brul.0igxema"><tld/>i&gcirc;ema</ref>
  <trd lng="be">&c_l;&c_jo;&c_g;&c_k;&c_a;&c_w;&c_z;&c_g;&c_a;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="de">entz&uuml;ndlich</trd>
  <trdgrp lng="en">
    <trd>combustible</trd>,
    <trd>volatile</trd>
  </trdgrp>
  <trd lng="es">inflamable</trd>
  <trd lng="fr">inflammable</trd>
  <trd lng="hu">gy&uacute;l&eacute;kony</trd>
  <trd lng="id"><ind>mudah</ind> terbakar</trd>
  <trd lng="ja">&#x71C3;&#12360;&#12420;&#12377;&#12356;  <pr>&#12418;&#12360;&#12420;&#12377;&#12356;</pr></trd>
  <trd lng="nl">ontvlambaar</trd>
  <trd lng="pl">&lstroke;atwopalny</trd>
  <trd lng="pt">inflam&aacute;vel</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_ju;&c_ch;&c_i;&c_j;</trd>
  <trd lng="sk">hor&lcaron;av&yacute;</trd>
  <trdgrp lng="sk">
