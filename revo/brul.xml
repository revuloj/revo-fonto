  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Difekti per fajro a&ubreve; varmego:
      <ekz>
        <ind><tld/>difekto</ind>;
        <trd lng="fr">brulure</trd>
      </ekz>
      <ekz>
        <klr>[donu]</klr>
        <tld/>difekton pro <tld/>difekto, vundon pro
        vundo, kontuzon pro kontuzo
        <fnt><bib>MT</bib>, <lok>&Eli; 21:25</lok></fnt>;
      </ekz>
      <ekz>
        la blovilo <tld/>difekti&gcirc;is
        <fnt><bib>MT</bib>, <lok>&Jer; 6:29</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_p;&c_a;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_p;&c_a;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_ja;&c_k;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_ja;&c_ch;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pop&aacute;lit</trd>,
    <trd>po&scaron;kodit ohn&ecaron;m</trd>,
    <trd>p&rcaron;ip&aacute;lit</trd>
  </trdgrp>
  <trd lng="de">verbrennen</trd>
  <trd lng="en">burn</trd>
  <trdgrp lng="fr">
    <trd>blesser<klr tip="amb"> (par brulure)</klr></trd>,
    <trd>bruler <klr tip="amb"> (tr.)</klr></trd>,
    <trd>se <ind>bruler</ind></trd>
  </trdgrp><trd lng="fr">bruler<klr> (abimer par le feu)</klr></trd>
  <trd lng="hu">meg&eacute;get</trd>
  <trd lng="nl">verbranden</trd>
  <trdgrp lng="pl">
    <trd>oparzy&cacute; si&eogonek;</trd>,
    <trd>sparzy&cacute; si&eogonek;</trd>,
    <trd>przypiec si&eogonek;</trd>,
    <trd>spali&cacute; si&eogonek;<klr> (podczas opalania)</klr></trd>,
    <trd>ogorze&cacute;</trd>
  </trdgrp>
  <trd lng="pt">queimar</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_b;&c_zh;&c_i;&c_g;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_zh;&c_je;&c_ch;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">po&scaron;kodi&tcaron; oh&ncaron;om</trd>
</drv>

