<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: telefo1.xml,v 1.11 2023/09/04 18:15:45 revo Exp $">
<kap>
<rad>telefotograf</rad>/i
</kap>
<drv mrk="telefo1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="fotogr.0i">Fotografi</ref> malproksimajn
      objektojn:
      <ekz>
        <tld/>ita per objektivo de 180&#x202F;mm fokusdistanco.
      </ekz>  
    </dif>
    <refgrp tip="vid">
      <ref cel="telegr.foto0i">fototelegrafi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="en">
    <trd>to transmit a wirephoto</trd>,
    <trd>to telephotograph</trd>
  </trdgrp>
  <trd lng="fr">t&eacute;l&eacute;photographier</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;vf&eacute;nyk&eacute;pez</trd>,
    <trd>t&aacute;voli <ind>felv&eacute;telt</ind> k&eacute;sz&iacute;t</trd>
  </trdgrp>
  <trd lng="ja">&#x671B;&#x9060;&#x3067;&#x5199;&#x771F;&#x64AE;&#x5F71;&#x3059;&#x308B;  [<ind>&#x307C;&#x3046;&#x3048;&#x3093;&#x3067;&#x3057;&#x3083;&#x3057;&#x3093;&#x3055;&#x3064;&#x3048;&#x3044;&#x3059;&#x308B;</ind>]</trd>
  <trdgrp lng="pt">
    <trd>telefotografar</trd>
  </trdgrp>
  <trd lng="sk">fotografova&tcaron; na dia&lcaron;ku <klr>(teleobjekt&iacute;vom)</klr></trd>
</drv>
</art>
