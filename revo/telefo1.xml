<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: telefo1.xml,v 1.15 2025/10/27 08:41:31 revo Exp $">
<kap>
<rad>telefotograf</rad>/i
</kap>
<drv mrk="telefo1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="fotogr.0i">Fotografi</ref> malproksimajn
      objektojn:
      <ekz>
        <tld/>ita per objektivo de 180&#x202F;mm fokusdistanco.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="telegr.foto0i">fototelegrafi</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_t;&c_a;&c_g;&c_r;&c_a;&c_f;&c_a;&c_v;&c_a;&c_c;&c_mol; &c_p;&c_r;&c_a;&c_z; &c_t;&c_e;&c_l;&c_je;&c_a;&c_b;&#8217;&c_je;&c_k;&c_t;&c_y;&c_w;</trd>
  <trdgrp lng="en">
    <trd>to transmit a wirephoto</trd>,
    <trd>to telephotograph</trd>
  </trdgrp>
  <trd lng="fr">t&eacute;l&eacute;photographier</trd>
  <trdgrp lng="hu">
    <trd>t&aacute;vf&eacute;nyk&eacute;pez</trd>,
    <trd>t&aacute;voli <ind>felv&eacute;telt</ind> k&eacute;sz&iacute;t</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd><baz>foto</baz>memfoto jauh</trd>,
    <trd><baz>potret</baz>memotret jauh</trd>
  </trdgrp>
  <trd lng="ja">&#x671B;&#36960;&#12391;&#20889;&#x771F;&#x64AE;&#x5F71;&#12377;&#x308B;
  <pr>&#x307C;&#12358;&#12360;&#12435;&#12391;&#12375;&#12419;&#12375;&#12435;&#12373;&#12388;&#12360;&#12356;&#12377;&#x308B;</pr></trd>
  <trd lng="pl">fotografowa&cacute; teleobiektywem</trd>
  <trd lng="pt">telefotografar</trd>
  <trd lng="sk">fotografova&tcaron; na dia&lcaron;ku <klr>(teleobjekt&iacute;vom)</klr></trd>
</drv>
</art>
