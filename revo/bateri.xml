<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bateri.xml,v 1.32 2023/10/29 19:11:00 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>bateri</rad>/o
</kap>
<drv mrk="bateri.0o">
  <kap><tld/>o</kap>
  <snc mrk="bateri.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Artileria grupo kun sia ilaro, konsistanta el kelkaj
      <ref tip="prt" cel="kanon1.0o.pafilego">kanonoj</ref>:
      <ekz>
        por artilerie plifortigi kampanjajn regimentojn 43 fortaj kaj 11 malfortaj
        <tld/>oj deloki&gcirc;is
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=380611">Batalo apud Verdun</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        dragonoj de la brigado <klr>[&#x2026;]</klr> ne kapablis bridi siajn &ccirc;evalojn kaj
        da&ubreve;rigis sian sturmon kontra&ubreve; la franca &leftquot;Dika <tld lit="B"/>o&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=6737">Batalo de Waterloo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>nenia lukso, krom tuta <tld/>o da
        komputiloj &ndash; liaj fenestroj al la mondo
        <fnt>
        <aut>Anne Tilmont</aut> <vrk><url ref="&Monato;/1998/005932.php">Kosmopolita civitano kun okulo al la universo</url></vrk>
        <bib>Monato</bib>
        <lok>1998/04, p. 9</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Batterie</trd>,
      <trd>Gesch&uuml;tzbatterie</trd>
    </trdgrp>
    <trd lng="en">battery</trd>
    <trdgrp lng="fa">
      <trd>&a_t;&a_w;&f_p;&a_kh;&a_A;&a_n;&a_h;</trd>,
      <trd>&a_A_madda;&a_t;&a_sh;&a_b;&a_A;&a_r;</trd>
    </trdgrp>
    <trd lng="fr">batterie<klr tip="ind"> (artillerie)</klr></trd>
    <trd lng="hu">&uuml;teg</trd>
    <trd lng="nl">batterij</trd>
    <trd lng="pl">bateria</trd>
    <trd lng="zh">&#x70AE;&#x81FA; <pr>p&#xE0;ot&#xE1;i</pr></trd>
</snc>
  <snc mrk="bateri.0o.ELE">
    <uzo tip="fak">ELE</uzo>
    <dif>
      Grupo da elektraj <ref tip="prt" cel="genera2.0o">generatoroj</ref>:
      <ekz>
        fiksu &gcirc;in al la <tld/>o de via di&gcirc;ita
        telefonaparato
        <fnt>
          <aut>-</aut>:
          <vrk>Nova! Signal-fortigilo de Big Planet</vrk>,
          <bib>Monato</bib>
          <lok>2002/05, p. 32</lok>
        </fnt>;
      </ekz>
      <ekz>
        la litio-feraj <tld/>oj vivos 5 jarojn
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2012/010893.php">Elektra buso protektu la medion</url></vrk>
          <lok>2012/12, p. 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        plumbo-acida <tld/>o en unu &ccirc;elo
        <fnt>
          <vrk><url ref="https://kaest2018.ikso.net/wp-content/uploads/sites/12/2018/10/Elektromobileco_Kaest2018.pdf">PDF
          Elektromobileco kaj Esperanto - kaest2018.ikso.net</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;argi la <tld/>on de elektraj veturiloj
        <fnt>
          <aut>Ivo Durwael</aut>
          <vrk><url ref="&Monato;/2011/010227.php">Magia tapi&scirc;o</url></vrk>
          <bib>Monato</bib>
          <lok>2011/01, p. 16</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum bremsado la baterioj estas &scirc;argataj
        <fnt>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2012/010893.php">Elektra buso protektu la medion</url></vrk>
          <bib>Monato</bib>
          <lok>2012/12, p. 19</lok>
        </fnt>;
      </ekz>
      <ekz>
        necesas re&scirc;argi la <tld/>on &ccirc;iutage
        <fnt>
          <aut>Pa&ubreve;l Peeraerts</aut>
          <vrk><url ref="&Monato;/2008/009612.php">Inteligenta po&scirc;amiko</url></vrk>
          <bib>Monato</bib>
          <lok>2008/10, p. 12</lok>
        </fnt>;
      </ekz>
      <ekz>
        sun<tld/>a brakhorlo&gcirc;o
        <fnt>
          <aut>Walter Klag</aut>
            <vrk><url ref="&Monato;/2000/007024.php">Pli ekzakte ne eblas: Radi-regata
            horlo&gcirc;o</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 13</lok>
          </fnt>;
      </ekz>
      <ekz>
        li havis en sia korpo du artefaritajn pumpilojn ligitajn al <tld/>oj, kiuj
        pumpadis sangon tra la vejnoj kaj arterioj
        <fnt>
          <aut>Julius Hauser</aut>
          <vrk><url ref="&Monato;/2013/010850.php">Korpo sen koro</url></vrk>
          <bib>Monato</bib>
          <lok>2013/01, p. 10</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Pilojn kaj akumulatorojn oni grupigas serie, por ricevi pli
      grandan
      elektromovan forton, a&ubreve; paralele, por ricevi pli intensan
      kurenton.
    </rim>
    <refgrp tip="vid">
      <ref cel="akumul1.0o">akumulatoro</ref>,
      <ref cel="konden.0o">kondensatoro</ref>,
      <ref cel="pil.0o">pilo</ref>
    </refgrp>
    <trd lng="ar">&a_m;&a_r;&a_A;&a_k;&a_m;</trd>
    <trd lng="bg">&c_b;&c_a;&c_t;&c_je;&c_r;&c_i;&c_ja;</trd>
    <trd lng="bn">&#x9AC;&#x9CD;&#x9AF;&#x9BE;&#x99F;&#x9BE;&#x9B0;&#x9C0;</trd>
    <trd lng="co">battaria</trd>
    <trd lng="cy">batri</trd>
    <trd lng="da">batteri</trd>
    <trdgrp lng="de">
      <trd>Batterie</trd>,
      <trd>Akku</trd>
    </trdgrp>
    <trd lng="el">&my;&pi;&alfa;&tau;&alfa;&rho;&jota_ton;&alfa;</trd>
    <trd lng="en">battery</trd>
    <trd lng="et">patarei</trd>
    <trd lng="eu">bateria</trd>
    <trd lng="fa">&a_b;&a_A;&a_t;&a_r;&f_y;</trd>
    <trd lng="fi">paristo</trd>
    <trd lng="fr">batterie<klr tip="ind"> (piles, accus,...)</klr></trd>
    <trd lng="hu">telep<klr tip="ind"> (elektromos)</klr></trd>
    <trd lng="ia">batteria</trd>
    <trd lng="id">baterai</trd>
    <trd lng="ja">&#x96FB;&#x6C60; <pr>&#x3067;&#x3093;&#x3061;</pr></trd>
    <trd lng="ka">&#x10D1;&#x10D0;&#x10E2;&#x10D0;&#x10E0;&#x10D4;&#x10D0;</trd>
    <trd lng="km">&#x1790;&#x17D2;&#x1798;&#x1796;&#x17B7;&#x179B;</trd>
    <trd lng="lt">baterija</trd>
    <trd lng="lv">baterija</trd>
    <trd lng="mk">&c_b;&c_a;&c_t;&c_je;&c_r;&c_i;&#x458;&c_a;</trd>
    <trd lng="ms">bateri</trd>
    <trd lng="mt">batterija</trd>
    <trd lng="na">battar&igrave;a</trd>
    <trd lng="nl">batterij</trd>
    <trd lng="no">batteri</trd>
    <trd lng="oc">bateria</trd>
    <trd lng="pl">bateria</trd>
    <trd lng="pt">bateria</trd>
    <trd lng="ro">baterie</trd>
    <trd lng="scn">batteria</trd>
    <trd lng="sl">baterija</trd>
    <trd lng="sv">batteri</trd>
    <trd lng="zh">&#x7535;&#x6C60; <pr>di&#xE0;nch&#xED;</pr></trd>
</snc>
  <trd lng="be">&c_b;&c_a;&c_t;&c_a;&c_r;&c_e;&c_ja;</trd>
  <trd lng="bo">&#xF66;&#xF90;&#xF58;&#xF0B;&#xF42;&#xFB3;&#xF7C;&#xF42;&#xF0B;</trd>
  <trd lng="ca">bateria</trd>
  <trd lng="cs">baterie</trd>
  <trd lng="es">bater&iacute;a</trd>
  <trd lng="he">&#x5E1;&#x5D5;&#x5B9;&#x5DC;&#x5B0;&#x5DC;&#x5B8;&#x5D4;</trd>
  <trd lng="it">batteria</trd>
  <trd lng="ru">&c_b;&c_a;&c_t;&c_a;&c_r;&c_je;&c_ja;</trd>
  <trd lng="sk">bat&eacute;ria</trd>
  <trd lng="uk">&c_b;&c_a;&c_t;&c_a;&c_r;&c_je;&#x44F;</trd>
  <adm>
    Kontroli la tradukojn post vastigo de la difino. <aut>MB</aut>
  </adm>
</drv>
</art>
