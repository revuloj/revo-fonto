      <ekz>
        li devas subteni sin <klr>...</klr> per tio, kion li povas <tld/>i
        propramane<fnt><bib>DonKihxoto</bib>, <lok>&ccirc;apitro 38a, p.
        289a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">marauder</trd>
  <trd lng="nl">stropen</trd>
  <trd lng="ru">&c_m;&c_a;&c_r;&c_o;&c_d;&c_jo;&c_r;&c_s;&c_t;&c_v;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
