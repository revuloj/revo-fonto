<drv mrk="marod.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="sxtel.0i">&Scirc;teli</ref> terproduktojn,
