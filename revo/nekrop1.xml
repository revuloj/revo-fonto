<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: nekrop1.xml,v 1.25 2024/01/29 15:21:35 revo Exp $">
<kap>
  <rad>nekropsi</rad>/o
</kap>
<drv mrk="nekrop1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Esplora dissekco de kadavro:
      <ekz>
        <tld/>o evidentigis, ke li jam estis mortinta anta&ubreve; la
        infanoj<fnt><aut>Guido Van Damme</aut>: <vrk>Tagluma
        ko&scirc;maro</vrk>, <bib>Monato</bib><lok>jaro 1996a, numero 11a, p.
        14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_o;&c_s;&c_p;&c_a;&c_l;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>nekropsie</trd>,
    <trd>ohled&aacute;n&iacute; mrtvoly</trd>,
    <trd>pitva</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Nekropsie</trd>,
    <trd>Obduktion</trd>
  </trdgrp>
  <trd lng="el">&ny;&epsilon;&kappa;&rho;&omikron;&psi;&jota_ton;&alfa;</trd>
  <trdgrp lng="en">
    <trd>autopsy</trd>,
    <trd>necropsy</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>autopsia</trd>,
    <trd>necropsia</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&f_k;&a_A;&a_l;&a_b;&a_d;&a_sh;&f_k;&a_A;&a_f;&f_y;</trd>,
    <trd>&f_k;&a_A;&a_l;&a_b;&a_d;&f_g;&a_sh;&a_A;&f_y;&f_y;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>autopsie</trd>
  </trdgrp>
  <trd lng="hu">boncol&aacute;s</trd>
  <trd lng="it">autopsia</trd>
  <trd lng="ja">&#x691C;&#x6B7B;  <pr>&#x3051;&#x3093;&#x3057;</pr></trd>
  <trdgrp lng="nl">
    <trd>sectie</trd>,
    <trd>dissectie</trd>,
    <trd>necropsie</trd>,
    <trd>lijkschouwing</trd>
  </trdgrp>
  <trd lng="pl">sekcja zw&lstroke;ok</trd>
  <trdgrp lng="pt">
    <trd>aut&oacute;psia</trd>,
    <trd>necropsia</trd>,
    <trd>necroscopia</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_s;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je;
  <klr>(&c_p;&c_o;&c_s;&c_m;&c_je;&c_r;&c_t;&c_n;&c_o;&c_je;)</klr></trd>
  <trd lng="sk">zis&tcaron;ovanie pr&iacute;&ccaron;iny smrti pitvou</trd>
  <trdgrp lng="uk">
    <trd>&c_a;&c_v;&c_t;&c_o;&c_p;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_n;&c_je;&c_k;&c_r;&c_o;&c_p;&c_s;&c_ib;&c_ja;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_t;&c_i;&c_n; (&c_t;&c_r;&c_u;&c_p;&c_a;)</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9A57;&#x5C4D; <pr>y&#xE0;nsh&#x12B;</pr></trd>,
    <trd>&#x9A8C;&#x5C38; <pr>y&#xE0;nsh&#x12B;</pr></trd>,
    <trd>&#x68C0;&#x5C38; <pr>ji&#x1CE;nsh&#x12B;</pr></trd>,
    <trd>&#x6AA2;&#x5C4D; <pr>ji&#x1CE;nsh&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="nekrop1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>aj konstatoj<fnt><aut>Wim M. A. De Smet</aut>:
        <vrk>Gigantoj
        sur strandoj</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero
        11a,
        p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd><klr>bei der </klr>Obduktion</trd>,
    <trd><klr>aus der </klr>Obduktion</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>autopsy (adj)</trd>,
    <trd>necropsy (adj)</trd>
  </trdgrp>
  <trd lng="hu">boncol&aacute;si</trd>
  <trd lng="it">autoptico</trd>
</drv>
</art>
