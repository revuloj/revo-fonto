<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>nekropsi</rad>/o
</kap>
<drv mrk="nekrop1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Esplora dissekco de kadavro:
      <ekz>
        <tld/>o evidentigis, ke li jam estis mortinta anta&ubreve; la
        infanoj<fnt><aut>Guido Van Damme</aut>: <vrk>Tagluma
        ko&scirc;maro</vrk>, <bib>Monato</bib><lok>jaro 1996a, numero 11a, p.
        14a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Nekropsie</trd>,
    <trd>Obduktion</trd>
  </trdgrp>
  <trd lng="el">&ny;&epsilon;&kappa;&rho;&omikron;&psi;&jota_ton;&alfa;</trd>
  <trdgrp lng="en">
    <trd>autopsy</trd>,
    <trd>necropsy</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>autopsia</trd>,
    <trd>necropsia</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>autopsie</trd>
  </trdgrp>
  <trd lng="hu">boncol&aacute;s</trd>
  <trdgrp lng="nl">
    <trd>sectie</trd>,
    <trd>dissectie</trd>,
    <trd>necropsie</trd>,
    <trd>lijkschouwing</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>aut&oacute;psia</trd>,
    <trd>necropsia</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_s;&c_k;&c_r;&c_y;&c_t;&c_i;&c_je; <klr>(&c_p;&c_o;&c_s;&c_m;&c_je;&c_r;&c_t;&c_n;&c_o;&c_je;)</klr></trd>
</drv>

<drv mrk="nekrop1.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <tld/>aj konstatoj<fnt><aut>Wim M. A. De Smet</aut>: <vrk>Gigantoj
        sur strandoj</vrk>, <bib>Monato</bib><lok>jaro 1998a, numero 11a,
        p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
</art>
