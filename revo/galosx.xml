<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>galo&scirc;</rad>/o</kap>
<drv mrk="galosx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Piedvesto, plej ofte ka&ubreve;&ccirc;uka, kiun oni metas sur la
      &scirc;uoj
      por &scirc;irmi kontra&ubreve; malsekeco.
    </dif>
    <trdgrp lng="hu">
      <trd>kalocsni</trd>,
      <trd>h&oacute;cip&odblac;</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>galoche</trd>,
      <trd>overschoen</trd>
    </trdgrp>
    <trd lng="pl">kalosz</trd>
  </snc>
  <snc>
    <dif>
      Speco de leda &scirc;uo kun ligna plandumo.
    </dif>
    <refgrp tip="vid">
      <ref cel="sxu.ligno0o">ligno&scirc;uo</ref>.
    </refgrp>
    <trd lng="pl">drewniak</trd>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_jo;&c_sh;&c_a;</trd>
  <trd lng="de">Galosche</trd>
  <trd
  lng="el">&gamma;&alfa;&lambda;&omikron_ton;&tau;&sigma;&alfa;</trd>
  <trd lng="fr">galoche</trd>
  <trd lng="hu">fatalp&uacute; <ind>cip&odblac;</ind></trd>
  <trd lng="pt">galocha</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_o;&c_sh;&c_a;</trd>
</drv>
</art>
