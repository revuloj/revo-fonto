<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>galo&scirc;</rad>/o
</kap>
<drv mrk="galosx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc num="1">
    <dif>
      Piedvesto, plej ofte ka&ubreve;&ccirc;uka, kiun oni metas sur la &scirc;uoj
      por &scirc;irmi kontra&ubreve; malsekeco. 
    </dif>
  </snc>
  <snc num="2">
    <dif>
      Speco de leda &scirc;uo kun ligna plandumo. 
    </dif>
    <refgrp tip="sin">
      <ref cel="sxu">ligno&scirc;uo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Galosche</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_o;&c_sh;&c_a;</trd>
</drv>
</art>
