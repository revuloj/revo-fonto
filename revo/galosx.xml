<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>*</ofc>
<rad>galo&scirc;</rad>/o</kap>
<drv mrk="galosx.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Piedvesto, plej ofte ka&ubreve;&ccirc;uka, kiun oni metas sur la &scirc;uoj
      por &scirc;irmi kontra&ubreve; malsekeco.
    </dif>
  </snc>
  <trdgrp lng="nl">
    <trd>galoche</trd>,
    <trd>overschoen</trd>
  </trdgrp>
  <snc>
    <dif>
      Speco de leda &scirc;uo kun ligna plandumo.
    </dif>
    <refgrp tip="vid">
      <ref cel="sxu.ligno0o">ligno&scirc;uo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Galosche</trd>
  <trd lng="el">&gamma;&alfa;&lambda;&omikron_ton;&tau;&sigma;&alfa;</trd>
  <trd lng="fr">galoche</trd>
  <trd lng="pt">galocha</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_o;&c_sh;&c_a;</trd>
</drv>
</art>
