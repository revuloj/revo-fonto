<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kantonment</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kanton1.0o">
  <kap><tld/>o</kap>
  <snc mrk="kanton1.0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <dif>
      Domhava loko, kie trupoj instali&gcirc;as por iome restadi;
      <ref tip="sin" cel="kvartir.0o.MIL">kvartiro</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="kamp">kampadejo</ref>.
    </refgrp>
  </snc>
  <trd lng="ca">acantonament</trd>
  <trd lng="de">Truppenunterkunft</trd>
  <trd lng="fr">cantonnement<klr tip="ind"> (de troupe)</klr></trd>
  <trdgrp lng="hu">
    <trd>kv&aacute;rt&eacute;ly <klr>(katonai)</klr></trd>,
    <trd>sz&aacute;ll&aacute;sk&ouml;rlet</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>accantonamento<klr> (mil.)</klr></trd>,
    <trd>alloggiamento<klr> (mil.)</klr></trd>,
    <trd>quartiere<klr> (mil.)</klr></trd>
  </trdgrp>
  <trd lng="nl">kantonnement</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je; <klr>(&c_v;&c_o;&c_i;&c_n;&c_s;&c_k;&c_o;&c_j; &c_ch;&c_a;&c_s;&c_t;&c_i; &c_v; &c_n;&c_a;&c_s;&c_je;&c_l;&c_jo;&c_n;&c_n;&c_o;&c_m;
  &c_p;&c_u;&c_n;&c_k;&c_t;&c_je;)</klr></trd>
  <trd lng="sk">ubytovanie vojska</trd>
</drv>

<drv mrk="kanton1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="kanton1.0i.MIL">
    <dif>
      Restadi <tld/>e.
    </dif>
  </snc>
  <trd lng="ca">acantonar</trd>
  <trd lng="de">einquartiert sein</trd>
  <trdgrp lng="fr">
    <trd>cantonner<klr tip="ind"> (intr.)</klr></trd>,
    <trd>&ecirc;tre en <ind>cantonnement</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>besz&aacute;ll&aacute;sol</trd>,
    <trd>elsz&aacute;ll&aacute;sol</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>stare accantonati<klr> (mil.)</klr></trd>,
    <trd>alloggiare<klr> (mil.)</klr></trd>,
    <trd>essere acquartierato<klr> (mil.)</klr></trd>
  </trdgrp>
  <trd lng="nl">in <ind>kantonnement</ind> gelegerd zijn</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_k;&c_v;&c_a;&c_r;&c_t;&c_i;&c_r;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_v;&c_o;&c_j;&c_s;&c_k;&c_a;</trd>
  <trd lng="sk">byť ubytovaný <klr>(vojsko)</klr></trd>
</drv>
</art>
