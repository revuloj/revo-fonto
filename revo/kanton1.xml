<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kanton1.xml,v 1.17 2022/03/21 17:51:56 revo Exp $">
<kap>
  <rad>kantonment</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="kanton1.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="kanton1.0i.MIL">
    <dif>
      (pp trupoj) Restadi en tiucela domhava loko, ekz. en kazerno:
      <ekz>
        en la jaro 1758 <tld/>is en la vila&gcirc;o imperiestraj
        regimentoj de <nom>L. Wolfebruttel</nom> kaj 
        <nom>Konigseck</nom> kun generalo <nom>Rutov</nom>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=328475">Star&eacute; Hradi&scaron;t&ecaron;</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">acantonar</trd>
  <trd lng="cs">b&yacute;t ubytov&aacute;n <klr>(vojsko)</klr></trd>
  <trdgrp lng="de">
    <trd>unterbringen (Soldaten)</trd>,
    <trd>kasernieren</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cantonner<klr tip="ind"> (intr.)</klr></trd>,
    <trd>&ecirc;tre en <ind>cantonnement</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>besz&aacute;ll&aacute;sol</trd>,
    <trd>elsz&aacute;ll&aacute;sol</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>stare accantonati<klr> (mil.)</klr></trd>,
    <trd>alloggiare<klr> (mil.)</klr></trd>,
    <trd>essere acquartierato<klr> (mil.)</klr></trd>
  </trdgrp>
  <trd lng="nl">in <ind>kantonnement</ind> gelegerd zijn</trd>
  <trdgrp lng="pl">
    <trd>kwaterowa&cacute;</trd>,
    <trd>stacjonowa&cacute;</trd>
  </trdgrp>   
  <trd lng="ro">sta&#x21B;ionate</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_k;&c_v;&c_a;&c_r;&c_t;&c_i;&c_r;&c_o;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol; &c_v;&c_o;&c_j;&c_s;&c_k;&c_a;</trd>
  <trd lng="sk">by&tcaron; ubytovan&yacute; <klr>(vojsko)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x441;&#x442;&#x43E;&#x44F;&#x442;&#x438;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x43C;&#x456;&#x449;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x440;&#x43E;&#x437;&#x43A;&#x432;&#x430;&#x440;&#x442;&#x438;&#x440;&#x43E;&#x432;&#x443;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="kanton1.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <snc mrk="kanton1.0o.ago">
    <dif>
      Ago <tld/>i:
      <ekz>
        la &leftquot;inventa&jcirc;o&rightquot; de kazerno okazis en la 17-a jarcento &dash; anta&ubreve;e la 
        soldatoj estis lo&gcirc;igitaj en la domoj de la civitanoj &dash; <klr>[&#x2026;]</klr>,
        &gcirc;i reduktis la kverelojn pro <tld/>o inter civitanoj kaj soldatoj
        kaj igis la soldatojn &ccirc;iam disponeblaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=92244">Kazerno</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        deviga <tld/>o (kvartirado), kontribucio, imposto kaj arbitreco faligis la lokan ekonomion
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=195959">Dzia&lstroke;dowo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Unterbringung <klr>(Soldaten)</klr></trd>,
      <trd>Kasernierung</trd>
    </trdgrp>
  </snc>
  <snc mrk="kanton1.0o.MIL">
    <dif>
      La domhava loko, kie trupoj estas <tld/>ataj.
    </dif>
    <refgrp tip="sin">
      <ref cel="kvartir.0o.MIL">kvartiro</ref>,
      <ref cel="garniz.0o.ejo">garnizono<sncref/></ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="kamp">kampadejo</ref>.
    </refgrp>
    <trd lng="ca">acantonament</trd>
    <trdgrp lng="cs">
      <trd>kantonnement</trd>,
      <trd>ubytov&aacute;n&iacute; vojska</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Truppenunterkunft</trd>,
      <trd>Garnison</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>billet</trd>,
      <trd>cantonment</trd>
    </trdgrp>
    <trd lng="fr">cantonnement<klr tip="ind"> (de troupe)</klr></trd>
    <trdgrp lng="hu">
      <trd>kv&aacute;rt&eacute;ly <klr>(katonai)</klr></trd>,
      <trd>sz&aacute;ll&aacute;sk&ouml;rlet</trd>
    </trdgrp>
    <trdgrp lng="it">
      <trd>accantonamento<klr> (mil.)</klr></trd>,
      <trd>alloggiamento<klr> (mil.)</klr></trd>,
      <trd>quartiere<klr> (mil.)</klr></trd>
    </trdgrp>
    <trd lng="nl">kantonnement</trd>
    <trd lng="pl">kwatera</trd>     
    <trd lng="ro">locuin&#x21B;&abreve;</trd>
    <trd lng="ru">&c_r;&c_a;&c_s;&c_p;&c_o;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je; <klr>(&c_v;&c_o;&c_i;&c_n;&c_s;&c_k;&c_o;&c_j; &c_ch;&c_a;&c_s;&c_t;&c_i; &c_v; &c_n;&c_a;&c_s;&c_je;&c_l;&c_jo;&c_n;&c_n;&c_o;&c_m;
    &c_p;&c_u;&c_n;&c_k;&c_t;&c_je;)</klr></trd>
    <trd lng="sk">ubytovanie vojska</trd>
  </snc>
  <trdgrp lng="uk">
    <trd>&#x440;&#x43E;&#x437;&#x43C;&#x456;&#x449;&#x435;&#x43D;&#x43D;&#x44F; &#x432;&#x456;&#x439;&#x441;&#x44C;&#x43A;&#x43E;&#x432;&#x43E;&#x457; &#x447;&#x430;&#x441;&#x442;&#x438;&#x43D;&#x438; &#x432; &#x43D;&#x430;&#x441;&#x435;&#x43B;&#x435;&#x43D;&#x43E;&#x43C;&#x443; &#x43F;&#x443;&#x43D;&#x43A;&#x442;&#x456;</trd>,
    <trd>&#x43C;&#x456;&#x441;&#x446;&#x435; &#x43F;&#x43E;&#x441;&#x442;&#x43E;&#x44E;</trd>
  </trdgrp>
</drv>


</art>
