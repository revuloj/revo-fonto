<vortaro>
<art mrk="$Id$">
<kap>
<rad>ekscentrik</rad>/o
</kap>
<drv mrk="ekscen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Disko, ringo a&ubreve; cilindra stango, kies rotacia akso ne okupas la centron:
      <ekz>
        <ind><tld/>a</ind> disko, ringo, stango.
        <trd lng="be">&c_e;&c_k;&c_s;&c_c;&c_e;&c_n;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
        <trdgrp lng="de">
          <trd>exzentrisch</trd>,
          <trd>unwuchtig</trd>
        </trdgrp>
        <trd lng="fr">excentrique<klr tip="ind"> (adj., m&eacute;c.)</klr></trd>
        <trd lng="es">exc&eacute;ntrico</trd>
        <trdgrp lng="hu">
          <trd>k&ouml;rhagy&oacute;</trd>,
          <trd>excentrikus</trd>
        </trdgrp>
        <trd lng="pt">exc&#234;ntrico</trd>
        <trd lng="ru">&c_e;&c_k;&c_s;&c_c;&c_je;&c_n;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_c;&c_e;&c_n;&c_t;&c_r;&c_y;&c_k;</trd>
  <trdgrp lng="de">
     <trd>Exzenter</trd>,
     <trd>Unwucht</trd>
  </trdgrp>
  <trd lng="fr">excentrique<klr tip="ind"> (subst., m&eacute;c.)</klr></trd>
  <trdgrp lng="hu">
    <trd>k&ouml;rhagy&oacute; elem</trd>,
    <trd>excenter</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_c;&c_je;&c_n;&c_t;&c_r;&c_i;&c_k;</trd>
</drv>
</art>
