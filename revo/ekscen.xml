<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>ekscentrik</rad>/o
</kap>
<drv mrk="ekscen.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Disko, ringo a&ubreve; cilindra stango, kies rotacia akso ne
      okupas la centron:
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_k;&c_s;&c_c;&c_e;&c_n;&c_t;&c_r;&c_y;&c_k;</trd>
  <trdgrp lng="de">
     <trd>Exzenter</trd>,
     <trd>Unwucht</trd>
  </trdgrp>
  <trd lng="en">eccentric<klr tip="amb"> (geometry)</klr></trd>
  <trd lng="fr">excentrique<klr tip="ind"> (subst.,
  m&eacute;c.)</klr></trd>
  <trdgrp lng="hu">
    <trd>k&ouml;rhagy&oacute; elem</trd>,
    <trd>excenter</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>momo&sacute;r&oacute;d</trd>
  </trdgrp>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_c;&c_je;&c_n;&c_t;&c_r;&c_i;&c_k;</trd>
</drv>
<drv mrk="ekscen.0a">
  <kap><tld/>a</kap>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
      Rilata al <tld/>o:
      <ekz>
        <ind><tld/>a</ind> disko, ringo, stango.
      </ekz>
    </dif>
    <refgrp tip="vid">
      Ne konfuzu kun <ref cel="ekscentr.0a">ekscentra</ref>
    </refgrp>
    <trd lng="be">&c_e;&c_k;&c_s;&c_c;&c_e;&c_n;&c_t;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>exzentrisch</trd>,
      <trd>unwuchtig</trd>
    </trdgrp>
    <trd lng="en">eccentric<klr tip="amb"> (geometry)</klr></trd>
    <trd lng="fr">excentrique<klr tip="ind"> (adj.,
    m&eacute;c.)</klr></trd>
    <trd lng="es">exc&eacute;ntrico</trd>
    <trdgrp lng="hu">
      <trd>k&ouml;rhagy&oacute;</trd>,
      <trd>excentrikus</trd>
    </trdgrp>
    <trd lng="pl">mimo&sacute;rodowy</trd>
    <trd lng="pt">exc&ecirc;ntrico</trd>
    <trd lng="ru">&c_e;&c_k;&c_s;&c_c;&c_je;&c_n;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  </snc>
</drv>
</art>
