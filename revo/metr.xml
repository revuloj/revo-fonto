<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: metr.xml,v 1.42 2019/06/30 18:21:48 revo Exp $">
<kap>
  <ofc>*</ofc><ofc>1</ofc><ofc>7</ofc>
  <rad>metr</rad>/o
</kap>
<drv mrk="metr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <mlg kod="SI">m</mlg>
  <snc mrk="metr.0o.unuo">
    <uzo tip="fak">FIZ</uzo>
    <mlg kod="SI">m</mlg>
    <dif>
      Baza <ref tip="lst" cel="unu.mezur0o"
      lst="voko:longoj">mezurunuo</ref> de
      <ref cel="long.0o.FIZ">longo</ref> en la Internacia Sistemo de
      unuoj (SI):
      <ekz>
        kvar <tld/>oj da tiu &ccirc;i &scirc;tofo kostas na&ubreve;
        frankojn
        <fnt><bib>F</bib>, <lok>&FE; 14</lok></fnt>;
      </ekz>
      <ekz>
        la drapo kostas po 2 spesmiloj por <tld/>o 
        = &ccirc;iu <tld/>o (ne la tuta drapo) kostas 2 spesmilojn
        <fnt>
          <bib>LR</bib>
          <lok>13. Prepozicioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        muro kelke da <tld/>oj alta
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        duono da metro
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekde 1983 unu <tld/>o egalas al longo trairata de lumo en vakuo
        dum 299&nbsp;792&nbsp;548-ono de <ref
        cel="sekund.0o.tempo">sekundo</ref>;
      </ekz>
      <ekz>
        anta&ubreve;e la <tld/>on oni difinis kiel dekmiliononon de
        kvarono de
        la tera <ref cel="meridi.0o.GEOG">meridiano</ref> kaj poste kiel
        onon de la ondolongo de iu radiado, emisiata de
        <ref cel="kripto1.0o">kriptono</ref>;
      </ekz>
      <ekz>
        la <ind><tld/>a sistemo</ind>.
        <trd lng="br">reizhiad <ind>metrek</ind></trd>
        <trd lng="ca">sistema <ind>m&egrave;tric</ind></trd>
        <trd lng="fr">syst&egrave;me <ind>m&eacute;trique</ind></trd>
        <trd lng="pl">system <ind>metryczny</ind></trd>
      </ekz>
    </dif>
      <refgrp tip="vid">
       <ref cel="kilome.0o">kilometro</ref>,
       <ref cel="hektom.0o">hektometro</ref>,
       <ref cel="dekame.0o">dekametro</ref>,
       <ref cel="decime.0o">decimetro</ref>,
       <ref cel="centim1.0o">centimetro</ref>,
       <ref cel="milime.0o">milimetro</ref>
     </refgrp>
    <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_r;
    <klr>(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a;
    &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
    <trd lng="bg">&c_m;&c_je;&c_t;&c_malmol;&c_r;</trd>
    <trd lng="de">Meter</trd>
    <trdgrp lng="en">
      <trd>metre</trd>,
      <trd>meter <klr>(American)</klr></trd>
    </trdgrp>
    <trd lng="fr">m&egrave;tre<klr tip="ind"> (unit&eacute; de mesure)
    </klr></trd>
    <trd lng="hu">m&eacute;ter</trd>
    <trd lng="id">meter</trd>
    <trd lng="nl">meter</trd>
    <trd lng="pl">metr</trd>
    <trd lng="ru">&c_m;&c_je;&c_t;&c_r; <klr tip="amb">
    (&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a; 
    &c_i;&c_z;&c_m;&c_je;&c_r;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  </snc>
  <snc mrk="metr.0o.mezurilo">
    <dif>
      <ref cel="mezur.0ilo">Mezurilo</ref>, havanta unumetran longon:
      <ekz>
        mezuri la&ubreve; sia <tld/>o
        <klr>(ne objektive pritaksi)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_r; 
    <klr>(&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a; &c_d;&c_l;&c_ja; 
    &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
    <trd lng="bg">&c_m;&c_je;&c_t;&c_malmol;&c_r;</trd>
    <trd lng="de">Meterma&szlig;</trd>
    <trd lng="fr">m&egrave;tre<klr tip="ind"> (instrument de mesure)
    </klr></trd>
    <trd lng="nl">meter</trd>
    <trd lng="pl">metr</trd>
    <trd lng="ru">&c_m;&c_je;&c_t;&c_r;<klr tip="amb"> 
    (&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t; 
    &c_i;&c_z;&c_m;&c_je;&c_r;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  </snc>
  <snc mrk="metr.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Aparta aran&gcirc;o de <ref cel="pied.0o.POE">piedoj</ref>,
      konsistiganta
      la ritman skeleton de <ref cel="vers.0o">verso</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="ritm.0o.POE">ritmo</ref>,
      <ref cel="prozod.0o">prozodio</ref>,
      <ref cel="metrik.0o">metriko</ref>;
    </refgrp>
    <refgrp tip="sub">
      <ref cel="amfibr.n0o">n-amfibra&hcirc;o</ref>,
      <ref cel="anapes.n0o">n-anapesto</ref>,
      <ref cel="jamb.n0o">n-jambo</ref>,
      <ref cel="troke.n0o">n-trokeo</ref>,
      <ref cel="silab.n0o.metro">n-silabo</ref>,
      <ref cel="heksam.0o">heksametro</ref>,
      <ref cel="pentam.0o">pentametro</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_e;&c_t;&c_a;&c_r; 
      <klr>(&c_u; &c_v;&c_je;&c_r;&c_sh;&c_y;)</klr></trd>,
      <trd>&c_p;&c_a;&c_m;&c_je;&c_r;<klr> 
      (&c_v;&c_je;&c_r;&c_sh;&c_a;&c_v;&c_a;&c_n;&c_y;)</klr></trd>
    </trdgrp>
    <trd
    lng="bg">&c_v;&c_je;&c_r;&c_s;&c_i;&c_f;&c_i;&c_k;&c_a;&c_c;&c_i;&c_ja;</trd>
    <trd lng="de">Metrum</trd>
    <trd lng="fr">m&egrave;tre<klr tip="ind"> (forme de vers)</klr>
    </trd>
    <trdgrp lng="hu">
      <trd>metrum</trd>,
      <trd>versm&eacute;rt&eacute;k</trd>
    </trdgrp>
    <trd lng="nl">metrum</trd>
    <trd lng="pl">metrum</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_m;&c_je;&c_r;<klr tip="amb"> 
      (&c_s;&c_t;&c_i;&c_h;&c_o;&c_t;&c_v;&c_o;&c_r;&c_n;&c_y;&c_j;)
      </klr></trd>,
      <trd>&c_m;&c_je;&c_t;&c_r;<klr tip="amb"> 
      (&c_v; &c_s;&c_t;&c_i;&c_h;&c_je;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="br">metr</trd>
  <trd lng="ca">metre</trd>
  <trdgrp lng="cs">
    <trd>metro</trd>,
    <trd>podzemn&iacute; dr&aacute;ha</trd>
  </trdgrp>
  <trd lng="en">meter</trd>
  <trd lng="es">metro</trd>
  <trd lng="pt">metro</trd>
  <trdgrp lng="sk">
    <trd>metro</trd>,
    <trd>podzemn&aacute; dr&aacute;ha</trd>
  </trdgrp>
</drv>

<drv mrk="metr.kvadrata0o">
  <kap>kvadrata <tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <mlg kod="SI">m&quadrat;</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mezur0o" lst="voko:areoj">Mezurunuo</ref>
      de
      <ref cel="are.0o.FIZ">areo</ref> en la Internacia Sistemo de
      unuoj, egala al la areo de <ref
      cel="kvadra1.0o.figuro">kvadrato</ref>,
      kies latero egalas al unu metro; simb. m<sup>2</sup>:
      <ekz>
        unu arbeta&jcirc;o de tiu &ccirc;i kreska&jcirc;o ombras la
        spacon de tri kvadrataj <tld/>oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la provoj, kiujn oni faris kun tiu &ccirc;i reto sub Quiberon, 
        sur la spaco de 800 kvadrataj <tld/>oj, donis rezulta&jcirc;ojn
        tiel bonajn, ke <klr>[&#8230;]</klr>
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        apartamentoj &ccirc;e la marbordo kun areo de 100 kvadrataj
        <tld/>oj kostas 75&nbsp;000 e&ubreve;rojn
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url
          ref="&Monato;/2008/009497.php">Anka&ubreve;
          eksterlandanoj a&ccirc;etas domojn</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        areo de cirklo kun unumetra radiuso estas &pi; kvadrataj metroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_n;&c_y; 
  &c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="bg">&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_je;&c_n;
  <ind>&c_m;&c_je;&c_t;&c_malmol;&c_r;</ind></trd>
  <trd lng="br">metr-karrez</trd>
  <trd lng="ca"><ind>metre</ind> quadrat</trd>
  <trd lng="cs">&ccaron;tvere&ccaron;n&yacute; metr</trd>
  <trd lng="de">Quadratmeter</trd>
  <trd lng="en">square <ind>metre</ind></trd>
  <trd lng="es">metro cuadrado</trd>
  <trd lng="fr"><ind>m&egrave;tre</ind> carr&eacute;</trd>
  <trd lng="hu">n&eacute;gyzetm&eacute;ter</trd>
  <trd lng="id"><ind>meter</ind> persegi</trd>
  <trd lng="nl">vierkante <ind>meter</ind></trd>
  <trd lng="pl">metr kwadratowy</trd>
  <trd lng="ru">
  &c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j; 
  &c_m;&c_je;&c_t;&c_r;</trd>
  <trd lng="sk">&scaron;tvorcov&yacute; meter</trd>
</drv>

<drv mrk="metr.kuba0o">
  <kap>kuba <tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <mlg kod="SI">m&cubic;</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:kvantoj">Mezurunuo</ref> de
      <ref cel="volume.0o.FIZ">volumeno</ref> en la Internacia Sistemo
      de unuoj, egala al la volumeno de 
      <ref cel="kub.0o.figuro">kubo</ref>,
      kies e&gcirc;o egalas al unu metro; simb. m<sup>3</sup>:
      <ekz>
        oni konstruis akvujon, kiu povis enteni &ccirc;irka&ubreve; tri
        miliardojn da kubaj <tld/>oj
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        unu kuba <tld/>o da ligno en formo de &scirc;tipoj kostas
        60&nbsp;&#8364;
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2006/008880.php">Brulanta
          
           afero</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        unu kuba <tld/>o da ne&gcirc;o pezas 300-500 kilogramojn
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo/pg</aut>
          <vrk><url ref="&Monato;/2006/008872.php">Revenas
            la ne&gcirc;o</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        kuba metro da akvo pezas proksimume unu <ref
        cel="tun.0o">tunon</ref>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>
    &c_k;&c_u;&c_b;&c_ib;&c_ch;&c_n;&c_y; &c_m;&c_e;&c_t;&c_a;&c_r;
    </trd>,
    <trd>&c_k;&c_u;&c_b;&c_a;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="bg">&c_k;&c_u;&c_b;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;
  &c_m;&c_je;&c_t;&c_malmol;&c_r;</trd>
  <trd lng="br">metr-kub</trd>
  <trd lng="ca"><ind>metre</ind> c&uacute;bic</trd>
  <trd lng="cs">kubick&yacute; metr</trd>
  <trd lng="de">Kubikmeter</trd>
  <trd lng="en">cubic metre</trd>
  <trd lng="es">metro c&uacute;bico</trd>
  <trd lng="fr"><ind>m&egrave;tre</ind> cube</trd>
  <trd lng="hu">k&ouml;bm&eacute;ter</trd>
  <trd lng="id"><ind>meter</ind> kubik</trd>
  <trd lng="nl">kubieke <ind>meter</ind></trd>
  <trdgrp lng="pl">
    <trd>metr sze&sacute;cienny</trd>,
    <trd>kubik</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>
    &c_k;&c_u;&c_b;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; 
    &c_m;&c_je;&c_t;&c_r;</trd>,
    <trd>&c_k;&c_u;&c_b;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  </trdgrp>
  <trd lng="sk">kubick&yacute; meter</trd>
</drv>

<drv mrk="metr.0oens">
  <kap><tld/>o en sekundo</kap>
  <uzo tip="fak">FIZ</uzo>
  <mlg kod="SI">m/s</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">Mezurunuo</ref> de
      <ref cel="rapid.0o.FIZ">rapido</ref> en la Internacia Sistemo de
      unuoj; simb. m.s<sup>-1</sup> a&ubreve; m/s:
      <ekz>
        unu <tld/>o en sekundo estas la rapido de korpo kun unuforma
        movado, kiu trairas unu <tld/>on dum unu
        <ref cel="sekund.0o.tempo">sekundo</ref>;
      </ekz>
      <ekz>
        la rapido de la lumo estas 3.10<sup>8</sup> <tld/>oj en sekundo
        <fnt><bib>PIV1</bib></fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> &jcirc;etas la pilkon je rapido de proksimume 40
        <tld/>oj en sekundo
        <fnt>
          <bib>Monato</bib>
          <aut>Vus-Tit Zik</aut>
          <vrk><url ref="&Monato;/2009/009769.php">Iom da
          historio kaj iom da fiziko</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;tono estas lan&ccirc;ata de ponto 12 metrojn alta,
        vertikale,
        de supre malsupren, kun rapido de 1,8 <tld/>oj en sekundo
        <klr>[&#8230;]</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url
          ref="&Monato;/2015/011597.php">&Scirc;ton&jcirc;eto
          sen cenzuro</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
&c_m;&c_e;&c_t;&c_a;&c_r; &c_z;&c_a; &c_s;&c_e;&c_k;&c_u;&c_n;&c_d;&c_u;
  </trd>
  <trd lng="bg">&c_m;&c_je;&c_t;&c_r;&c_a; &c_v;
  &c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_a;</trd>
  <trd lng="br">metr dre eilenn</trd>
  <trd lng="ca">metre[s] per segon</trd>
  <trd lng="de">Meter pro Sekunde</trd>
  <trd lng="en">metre per second</trd>
  <trd lng="es">metro por segundo</trd>
  <trd lng="fr">m&egrave;tre par seconde</trd>
  <trd lng="hu">m&eacute;ter per szekundum</trd>
  <trd lng="id"><ind>meter</ind> per sekon</trd>
  <trd lng="nl">meter per seconde</trd>
  <trd lng="pl">metr na sekund&eogonek;</trd>
  <trd lng="ru">
  &c_m;&c_je;&c_t;&c_r; &c_v; &c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_u;</trd>
</drv>

<drv mrk="metr.0oenss">
  <kap><tld/>o en kvadrata sekundo</kap>
  <uzo tip="fak">FIZ</uzo>
  <mlg kod="SI">m/s&quadrat;</mlg>
  <snc>
    <dif>
      <ref tip="lst" cel="unu.mezur0o"
      lst="voko:mezurunuoj">Mezurunuo</ref> de
      <ref cel="akcel.0o.FIZ">akcelo</ref> en la Internacia Sistemo de
      unuoj; simb. m&#8901;s<sup>-2</sup>, m/s<sup>2</sup> a&ubreve;
      m/s/s:
      <ekz>
        unu <tld/>o en kvadrata sekundo estas la akcelo de korpo kun
        unuforme
        akcelata movado, kies rapido varias je unu <tld/>o en sekundo
        dum unu <ref cel="sekund.0o.tempo">sekundo</ref>;
      </ekz>
      <ekz>
        la akcelo de la tera gravito egalas proksimume al 9,81 <tld/>oj
        en kvadrata sekundo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">
  &c_m;&c_e;&c_t;&c_a;&c_r; 
  &c_z;&c_a; &c_s;&c_e;&c_k;&c_u;&c_n;&c_d;&c_u; 
  &c_z;&c_a; &c_s;&c_e;&c_k;&c_u;&c_n;&c_d;&c_u;
  </trd>
  <trd lng="br">metr dre eilenn-garrez</trd>
  <trd lng="ca">metre[s] per segon al quadrat</trd>
  <trd lng="de">Meter pro Quadratsekunde</trd>
  <trd lng="es">metro por segundo al cuadrado</trd>
  <trd lng="fr">m&egrave;tre par seconde par seconde</trd>
  <trd lng="hu">m&eacute;ter per n&eacute;gyzetszekundum</trd>
  <trd lng="id"><ind>meter</ind> per sekon kuadrat</trd>
  <trd lng="nl">meter per seconde per seconde</trd>
  <trdgrp lng="pl">
    <trd>metr na sekund&eogonek; do kwadratu</trd>,
    <trd>metr na sekund&eogonek; kwadrat<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="ru">
  &c_m;&c_je;&c_t;&c_r; &c_v; 
  &c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_u; 
  &c_z;&c_a; &c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_u;
  </trd>
</drv>
</art>
