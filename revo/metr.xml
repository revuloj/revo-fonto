<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>metr</rad>/o
</kap>
<drv mrk="metr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="metr.0o.unuo">
    <uzo tip="fak">FIZ</uzo>
    <dif>
      Baza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de
      <ref cel="long.0o.FIZ">longo</ref> en la Internacia Sistemo de unuoj;
      simb. m:
      <ekz>
        ekde 1983 unu <tld/>o egalas al longo trairata de lumo en vakuo
        dum 299 792 548-ono de <ref cel="sekund.0o.tempo">sekundo</ref>;
      </ekz>
      <ekz>
        anta&ubreve;e la <tld/>on oni difinis kiel dekmiliononon de kvarono de
        la tera <ref cel="meridi.0o.GEOG">meridiano</ref> kaj poste kiel
        onon de la ondolongo de iu radiado, emisiata de
        <ref cel="kripto1.0o">kriptono</ref>;
      </ekz>
      <ekz>
        la <ind><tld/>a sistemo</ind>.
        <trd lng="fr">syst&egrave;me <ind>m&eacute;trique</ind></trd>
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_r; <klr>(&c_a;&c_d;&c_z;&c_ib;&c_n;&c_k;&c_a; &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
    <trd lng="de">Meter</trd>
    <trd lng="fr">m&egrave;tre<klr tip="ind"> (unit&eacute; de mesure)</klr></trd>
    <trd lng="nl">meter</trd>
    <trd lng="ru">&c_m;&c_je;&c_t;&c_r; <klr tip="amb">(&c_je;&c_d;&c_i;&c_n;&c_i;&c_c;&c_a; &c_i;&c_z;&c_m;&c_je;&c_r;&c_je;&c_n;&c_i;&c_ja;)</klr></trd>
  </snc>
  <snc mrk="metr.0o.mezurilo">
    <dif>
      <ref cel="mezur.0ilo">Mezurilo</ref>, havanta unumetran longon:
      <ekz>
        mezuri la&ubreve; sia <tld/>o
        <klr>(ne objektive pritaksi)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_r; <klr>(&c_p;&c_r;&c_y;&c_l;&c_a;&c_d;&c_a; &c_d;&c_l;&c_ja; &c_v;&c_y;&c_m;&c_ja;&c_r;&c_e;&c_n;&c_mol;&c_n;&c_ja;)</klr></trd>
    <trd lng="de">Meterma&szlig;</trd>
    <trd lng="fr">m&egrave;tre<klr tip="ind"> (instrument de mesure)</klr></trd>
    <trd lng="nl">meter</trd>
  </snc>
  <snc mrk="metr.0o.POE">
    <uzo tip="fak">POE</uzo>
    <dif>
      Aparta aran&gcirc;o de <ref cel="pied.0o.POE">piedoj</ref>, konsistiganta
      la ritman skeleton de <ref cel="vers.0o">verso</ref>.
    </dif>
    <refgrp tip="vid">
      <ref cel="ritm.0o.POE">ritmo</ref>,
      <ref cel="prozod.0o">prozodio</ref>,
      <ref cel="metrik.0o">metriko</ref>;
    </refgrp>
    <refgrp tip="sub">
      <ref cel="amfibr.n0o">n-amfibra&hcirc;o</ref>,
      <ref cel="anapes.n0o">n-anapesto</ref>,
      <ref cel="jamb.n0o">n-jambo</ref>,
      <ref cel="troke.n0o">n-trokeo</ref>,
      <ref cel="silab.n0o.metro">n-silabo</ref>,
      <ref cel="heksam.0o">heksametro</ref>,
      <ref cel="pentam.0o">pentametro</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_m;&c_e;&c_t;&c_a;&c_r; <klr>(&c_u; &c_v;&c_je;&c_r;&c_sh;&c_y;)</klr></trd>,
      <trd>&c_p;&c_a;&c_m;&c_je;&c_r; <klr>(&c_v;&c_je;&c_r;&c_sh;&c_a;&c_v;&c_a;&c_n;&c_y;)</klr></trd>
    </trdgrp>
    <trd lng="de">Metrum</trd>
    <trd lng="fr">m&egrave;tre<klr tip="ind"> (forme de vers)</klr></trd>
    <trd lng="nl">metrum</trd>
    <trdgrp lng="ru">
      <trd>&c_r;&c_a;&c_z;&c_m;&c_je;&c_r; <klr tip="amb">(&c_s;&c_t;&c_i;&c_h;&c_o;&c_t;&c_v;&c_o;&c_r;&c_n;&c_y;&c_j;)</klr></trd>,
      <trd>&c_m;&c_je;&c_t;&c_r; <klr tip="amb">(&c_v; &c_s;&c_t;&c_i;&c_h;&c_je;)</klr></trd>
    </trdgrp>
  </snc>
  <trd lng="en">meter</trd>
</drv>

<drv mrk="metr.kvadrata0o">
  <kap>kvadrata <tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="are.0o.FIZ">areo</ref> en la Internacia Sistemo de unuoj,
      egala al la areo de <ref cel="kvadra1.0o.figuro">kvadrato</ref>,
      kies latero egalas al unu metro; simb. m<sup>2</sup>:
      <ekz>
        areo de cirklo kun unumetra radiuso estas &pi; kvadrataj metroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_n;&c_y; &c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="de">Quadratmeter</trd>
  <trd lng="fr"><ind>m&egrave;tre</ind> carr&eacute;</trd>
  <trd lng="nl">vierkante <ind>meter</ind></trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_d;&c_r;&c_a;&c_t;&c_n;&c_y;&c_j; &c_m;&c_je;&c_t;&c_r;</trd>
</drv>

<drv mrk="metr.kuba0o">
  <kap>kuba <tld/>o</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="volume.0o.FIZ">volumeno</ref> en la Internacia Sistemo de
      unuoj, egala al la volumeno de <ref cel="kub.0o.figuro">kubo</ref>,
      kies e&gcirc;o egalas al unu metro; simb. m<sup>3</sup>:
      <ekz>
        kuba metro da akvo pezas proksimume unu <ref
        cel="tun.0o">tunon</ref>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_u;&c_b;&c_ib;&c_ch;&c_n;&c_y; &c_m;&c_e;&c_t;&c_a;&c_r;</trd>,
    <trd>&c_k;&c_u;&c_b;&c_a;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  </trdgrp>
  <trd lng="de">Kubikmeter</trd>
  <trd lng="fr"><ind>m&egrave;tre</ind> cube</trd>
  <trd lng="nl">kubieke <ind>meter</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_u;&c_b;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j; &c_m;&c_je;&c_t;&c_r;</trd>,
    <trd>&c_k;&c_u;&c_b;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  </trdgrp>
</drv>

<drv mrk="metr.0oens">
  <kap><tld/>o en sekundo</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="rapid.0o.FIZ">rapido</ref> en la Internacia Sistemo de
      unuoj; simb. m.s<sup>-1</sup> a&ubreve; m/s:
      <ekz>
        unu <tld/>o en sekundo estas la rapido de korpo kun unuforma
        movado, kiu trairas unu <tld/>on dum unu
        <ref cel="sekund.0o.tempo">sekundo</ref>;
      </ekz>
      <ekz>
        la rapido de la lumo estas 3.10<sup>8</sup> <tld/>oj en sekundo
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_r; &c_z;&c_a; &c_s;&c_e;&c_k;&c_u;&c_n;&c_d;&c_u;</trd>
  <trd lng="fr">m&egrave;tre par seconde</trd>
  <trd lng="nl">meter per seconde</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_r; &c_v; &c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_u;</trd>
</drv>

<drv mrk="metr.0oens2">
  <kap><tld/>o en kvadrata sekundo</kap>
  <uzo tip="fak">FIZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="unu.mezur0o">Mezurunuo</ref> de
      <ref cel="akcel.0o.FIZ">akcelo</ref> en la Internacia Sistemo de
      unuoj; simb. m.s<sup>-2</sup>, m/s<sup>2</sup> a&ubreve; m/s/s:
      <ekz>
        unu <tld/>o en kvadrata sekundo estas la akcelo de korpo kun
        unuforme
        akcelata movado, kies rapido varias je unu <tld/>o en sekundo dum
        unu
        <ref cel="sekund.0o.tempo">sekundo</ref>;
      </ekz>
      <ekz>
        la akcelo de la tera gravito egalas proksimume al 9,81 <tld/>oj en
        kvadrata sekundo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_m;&c_e;&c_t;&c_a;&c_r; &c_z;&c_a; &c_s;&c_e;&c_k;&c_u;&c_n;&c_d;&c_u; &c_z;&c_a; &c_s;&c_e;&c_k;&c_u;&c_n;&c_d;&c_u;</trd>
  <trd lng="fr">m&egrave;tre par seconde par seconde</trd>
  <trd lng="nl">meter per seconde per seconde</trd>
  <trd lng="ru">&c_m;&c_je;&c_t;&c_r; &c_v; &c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_u; &c_z;&c_a; &c_s;&c_je;&c_k;&c_u;&c_n;&c_d;&c_u;</trd>
</drv>
<adm>
  Enkonduki ien difinon por "unuforma" kaj "unuforme akcelata movado",
  konataj jam en PIV1 kaj en MatVort. <aut>MB</aut>
</adm>
</art>
