<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hidrok.xml,v 1.17 2023/11/15 17:08:00 revo Exp $">
<kap>
  <rad>hidrokarbid</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hidrok.0o">
  <kap><tld/>o</kap>
  <snc mrk="hidrok.0o.KEM">
    <uzo tip="fak">KEM</uzo>
    <dif>
      Substanco konsistanta el <ref cel="hidrog.0o">hidrogeno</ref> kaj
      <ref cel="karbon.0o">karbono</ref> 
      (hidrogena <ref tip="super" cel="karbid.0o">karbido</ref>):
      <ekz>
        acetileno kaj benzeno estas <tld/>oj.
      </ekz>
    </dif>
  </snc>
  <rim>
    Tiu vorto &scirc;ajnas malnovi&gcirc;inta. <fnt><bib>PIV3</bib></fnt> preferas &leftquot;hidrokarbono&rightquot;.
    <aut>Wolfram Diestel</aut>
  </rim>
  <trd lng="be">&c_v;&c_u;&c_g;&c_l;&c_je;&c_v;&c_a;&c_d;&c_a;&c_r;&c_o;&c_d;</trd>
  <trd lng="ca">hidrocarbur</trd>
  <trd lng="de">Kohlenwasserstoff</trd>
  <trd lng="el">&ypsilon;&delta;&rho;&alfa;&ny;&theta;&rho;&alfa;&kappa;&jota;&kappa;&omikron_ton; &epsilon;&xi;&ypsilon_ton;</trd>
  <trd lng="en">hydrocarbon</trd>
  <trd lng="es">hidrocarburo</trd>
  <trd lng="fa">&a_h;&f_y;&a_d;&a_r;&a_w;&f_k;&a_r;&a_b;&a_n;</trd>
  <trd lng="fr">hydrocarbure</trd>
  <trd lng="hu">sz&eacute;nhidrog&eacute;n</trd>
  <trd lng="it">idrocarburo</trd>
  <trd lng="nl">koolwaterstofverbinding</trd>
  <trd lng="pl">w&eogonek;glowod&oacute;r</trd>
  <trd lng="pt">hidrocarboneto</trd>
  <trd lng="ru">&c_u;&c_g;&c_l;&c_je;&c_v;&c_o;&c_d;&c_o;&c_r;&c_o;&c_d;</trd>
  <trdgrp lng="zh">
    <trd>&#x70C3; <pr>t&#x12B;ng</pr></trd>,
    <trd>&#x78B3;&#x6C22;&#x5316;&#x5408;&#x7269; <pr>t&#xE0;nq&#x12B;nghu&#xE0;h&#xE9;w&#xF9;</pr></trd>,
    <trd>&#x78B3;&#x5316;&#x6C22; <pr>t&#xE0;nhu&#xE0;q&#x12B;ng</pr></trd>
  </trdgrp>
</drv>
</art>
