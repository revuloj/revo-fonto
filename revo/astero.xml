<kap>
  <rad>asteroid</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="astero.0o">
  <kap><tld/>o</kap>
  <snc mrk="astero.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Malgranda planedo, kiel tiuj, kiuj trovi&gcirc;as inter la orbitoj de
      Marso kaj de Jupitero:
      <ekz>
        <ctl>Dawn</ctl> esploros la <tld/>on Vesto
        <fnt>
          <aut>Smash</aut>,
          <vrk>Dawn jam direktas sin al la asteroida zono</vrk>,
          <lok><url ref="http://raporto.info">Raporto.info</url>, 
          2007-10-08</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x5C0F;&#x884C;&#x661F; <pr>xi&#x1CE;ox&#xED;ngx&#x12B;ng</pr></trd>
</snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_e;&c_r;&c_o;&c_ib;&c_d;</trd>
  <trd lng="bg">&c_a;&c_s;&c_t;&c_je;&c_r;&c_o;&c_i;&c_d;</trd>
  <trdgrp lng="cs">
    <trd>asteroid</trd>,
    <trd>planetka</trd>
  </trdgrp>
  <trd lng="de">Asteroid</trd>
  <trd lng="en">asteroid</trd>
  <trd lng="es">asteroide</trd>
  <trd lng="fa">&a_s;&f_y;&a_A;&a_r;&f_k;</trd>
  <trd lng="fr">ast&eacute;ro&iuml;de</trd>
  <trd lng="he">&#x5D0;&#x5B7;&#x5E1;&#x5D8;&#x5B5;&#x5E8;&#x5D5;&#x5B9;&#x5D0;&#x5B4;&#x5D9;&#x5D3;</trd>
  <trdgrp lng="hu">
    <trd>kisbolyg&oacute;</trd>,
    <trd>aszteroida</trd>
  </trdgrp>
  <trd lng="it">asteroide</trd>
  <trd lng="ja">&#x5C0F;&#x60D1;&#x661F;  <pr>&#x3057;&#x3087;&#x3046;&#x308F;&#x304F;&#x305B;&#x3044;</pr></trd>
  <trdgrp lng="nl">
