<art mrk="$Id$">
<kap>
  <rad>asteroid</rad>/oj
  <fnt>Z</fnt>
</kap>
<drv mrk="astero.0oj">
  <kap><tld/>oj</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Malgrandaj planedoj, kiel tiuj, kiuj trovi&gcirc;as inter la orbitoj de
      Marso kaj de Jupitero.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_s;&c_t;&c_e;&c_r;&c_o;&c_ib;&c_d;&c_y;</trd>
  <trd lng="de">Asteroiden</trd>
  <trd lng="en">asteroids</trd>
  <trd lng="es">asteroides</trd>
  <trd lng="fr">ast&eacute;ro&iuml;des</trd>
  <trdgrp lng="hu">
    <trd>kisbolyg&oacute;</trd>,
    <trd>aszteroida</trd></trdgrp>
  <trd lng="ru">&c_a;&c_s;&c_t;&c_je;&c_r;&c_o;&c_i;&c_d;&c_y;</trd>
</drv>
</art>
