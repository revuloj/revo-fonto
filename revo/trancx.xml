<drv mrk="trancx.en0o">
  <kap>
    en<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="T"/>e farita <ref tip="super" cel="sulk.0o">sulko</ref>:
      <ekz>
        en<tld/>ojn pro mortinto ne faru sur via korpo
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 19:28</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> faris krucforman en<tld/>on de la umbiliko &gcirc;is la &scirc;losilosto
        <fnt>
          <bib>Kandid</bib>
          <lok>&Ccirc;apitro XXVIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iumatene mi faris en<tld/>on en malnovan laton <klr>(nombrante tagojn)</klr>
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        akraj &scirc;tonoj faris etajn, blankajn en<tld/>ojn sur la supra ledo de liaj
        mar&scirc;o&scirc;uoj
        <fnt>
          <bib>VRA</bib>
          <lok>Lars Ahlin</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fend.0o">fendo</ref>,
      <ref cel="fold.0o">foldo</ref>,
      <ref cel="kanel.0o">kanelo</ref>,
      <ref cel="krev.0o">krevo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Einschnitt</trd>,
    <trd>Kerbe</trd>
  </trdgrp>
</drv>









<drv mrk="trancx.mis0o">
  <kap>mis<tld/>o</kap>
  <snc mrk="trancx.mis0o.malbone">
    <dif>
      Malbone <tld/>i:
      <ekz>
        Kaheno &ccirc;iam mis<tld/>as oblikve
        <fnt>
          <bib>KrB</bib>
          <lok>Bonaj kalkuloj, bonaj amikoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="trancx.mis0o.LIN">
    <uzo tip="fak">LIN</uzo>
    <dif>
      <ref tip="super" cel="lud.vort0o.LIN">Vortludo</ref>
      en kiu de malsama divido de samsonaj vortoj rezultas
      malsama senco,
      ekzemple en
      <ctl>la stranga butiko</ctl> kaj
      <ctl>last-ranga butiko</ctl>;
      <ref tip="sin" cel="kalembur.0o.vortludo">kalemburo</ref>:
      <ekz>
        li ne markis per pa&ubreve;zo la limojn inter la radikoj en siaj 
        mis<tld/>oj
        <fnt>
          <aut>M.-T. Lloancy</aut>
          <vrk><url ref="http://kabareto.esperanto.cc/perhumuro.htm">
          Per humuro al maturo: Schwartz 100-jara</url></vrk>
          <lok>Franca Esperantisto, 1990-12 (416)</lok>
        </fnt>.
      </ekz>
    </dif>  
    <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_m;&c_b;&c_u;&c_r;</trd>
    <trd lng="cs">kalamb&uacute;r</trd>
    <trd lng="de">Kalauer</trd>
    <trd lng="es">calambur</trd>
    <trd lng="fr">calembour</trd>
    <trd lng="sk">kalamb&uacute;r</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x99C4;&#x3058;&#x3083;&#x308C;  <pr>&#x3060;&#x3058;&#x3083;&#x308C;</pr></trd>,
    <trd>&#x8A9E;&#x5442;&#x5408;&#x308F;&#x305B;  <pr>&#x3054;&#x308D;&#x3042;&#x308F;&#x305B;</pr></trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_a;&c_l;&c_a;&c_m;&c_b;&c_u;&c_r;</trd>
</drv>

<drv mrk="trancx.posx0ilo">
