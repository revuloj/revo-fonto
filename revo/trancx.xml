<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>tran&ccirc;</rad>/i
</kap>
<drv mrk="trancx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Dividi solidan korpon per akraranda ilo:
      <ekz>
        <tld/>anta kiel razilo;
      </ekz>
      <ekz>
        <tld/>i drapon;
      </ekz>
      <ekz>
        <tld/>i rekte en la karnon
        <klr>(f: uzi efikan, sed malmildan rimedon)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        amba&ubreve;<tld/>a glavo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        &ccirc;iu tajloro havas sian <tld/>manieron
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tond.0i">tondi</ref>,
      <ref cel="falcx.0i">fal&ccirc;i</ref>,
      <ref cel="romp.0i">rompi</ref>,
      <ref cel="hak.0i">haki</ref>,
      <ref cel="sekc.0i">sekci</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Trai&gcirc;i:
      <ekz>
        krioj <tld/>is aeron, la noktan silenton;
      </ekz>
      <ekz>
        la popolo, kies landon tra<tld/>as riveroj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la herbejon dis<tld/>is rivereto<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Akre kaj dolore frapi la sentumojn:
      <ekz>
        tia muziko <tld/>as la orelon<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la lumo <tld/>is al ni la okulojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        vi dis<tld/>as mian koron<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>a malvarmo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        fajfo;
      </ekz>
      <ekz>
        ni sentis la akran <tld/>adon de l' frosto;
      </ekz>
      <ekz>
        <tld/>anta koloro<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_e;&c_z;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>kr&aacute;jet</trd>,
    <trd>&rcaron;ezat</trd>
  </trdgrp>
  <trd lng="de">schneiden</trd>
  <trd lng="en">cut</trd>
  <trd lng="fr">couper</trd>
  <trd lng="nl">snijden</trd>
  <trd lng="ru">&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">sk&auml;ra</trd>
</drv>

<drv mrk="trancx.0o">
  <kap><tld/>o</kap>
  <snc mrk="trancx.0o.KOMUNE">
    <dif>
      Ago <tld/>i; rezulto de tiu ago:
      <ekz>
        la &hcirc;irurgiistoj faris <tld/>ojn en la karno por trovi la
        plumbon<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_r;&c_je;&c_z;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_r;&c_je;&c_z;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_r;&c_je;&c_z;</trd>
    </trdgrp>
  </snc>
  <snc mrk="trancx.0o.dedekinda">
    <uzo tip="fak">MAT</uzo>
    <ref tip="dif" cel="trancx.dedekinda0o">dedekinda <tld/>o.</ref>
    <trd lng="ru">&c_s;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
</drv>

<drv mrk="trancx.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Diversforma ilo por <tld/>i.
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_o;&c_zh;</trd>
  <trd lng="de">Messer</trd>
  <trd lng="en">knife</trd>
  <trd lng="fr">couteau</trd>
  <trd lng="nl">mes</trd>
  <trd lng="ru">&c_n;&c_o;&c_zh;</trd>
  <trd lng="sv">kniv</trd>
</drv>

<drv mrk="trancx.0iletoposx0ilo">
  <kap><tld/>ileto, po&scirc;<tld/>ilo</kap>
  <snc>
    <dif>
      Malgranda <tld/>ilo kun faldebla klingo.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_o;&c_zh;&c_y;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_z;&c_o;&c_r;&c_y;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Taschenmesser</trd>
  <trd lng="fr">canif</trd>
  <trd lng="nl">zakmes</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_o;&c_ch;&c_i;&c_n;&c_n;&c_y;&c_j; &c_n;&c_o;&c_zh;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_n;&c_o;&c_j; &c_n;&c_o;&c_zh;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>fickkniv</trd>,
    <trd>f&auml;llkniv</trd>
  </trdgrp>
</drv>

<drv mrk="trancx.al0i">
  <kap><ofc>*</ofc>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Doni &gcirc;ustan formon per <tld/>ado:
      <ekz>
        al<tld/>i drapon por vesto;
      </ekz>
      <ekz>
        al<tld/>i veston.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
  <trd lng="nl">knippen<klr> (v.kledingstuk)</klr></trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="trancx.de0ifor0i">
  <kap>de<tld/>i, for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Apartigi, forigi <tld/>ante:
      <ekz>
        dek fojojn mezuru, unu fojon de<tld/>u
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        en formo de de<tld/>ita (senpinta) piramido<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="ru">&c_o;&c_t;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Apartigi, forigi:
      <ekz>
        la armeo estas for<tld/>ita de la &ccirc;efurbo<fnt>B</fnt>;
      </ekz>
      <ekz>
        pro manko de komunikiloj tiu vasta regiono estas for<tld/>ita for
        de la mondvivo.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_ch;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="trancx.de0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      <klr>(p.p. <ref cel="angul.0o">angulo</ref> rilate al
      <ref cel="kurb.0o.MAT">kurbo</ref> a&ubreve;
      <ref cel="surfac.0o.MAT">surfaco</ref>)</klr>
      Oni diras, ke ebena angulo de<tld/>as
      <ref cel="ark.0o.segmento">arkon</ref> sur cirklo por diri, ke la
      randoj de tiu arko estas intersekcopunktoj de la cirklo kun la
      lateroj de la angulo; oni diras, ke solida angulo de<tld/>as surfacon
      sur sfero por diri, ke tiu surfaco estas intersekco de la sfero kun
      la solida angulo:
      <ekz>
        <ref cel="angul.centra0o">centra angulo</ref> estas duoblo de
        <ref cel="angul.cirkonferenca0o">cirkonferenca angulo</ref>, kiu
        de<tld/>as la saman arkon.
      </ekz>
    </dif>
    <adm>
      &Ccirc;i tiu termino logike devenas de "d&eacute;couper" kaj
      "&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;", sed mi ne certas, &ccirc;u
      &gcirc;i estas tradicia en E. <aut>MB</aut>
    </adm>
    <trdgrp lng="en">
      <trd>be subtended by</trd>,
      <trd>intercept<klr tip="ind"> (an arc of curve, a surface)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;couper</trd>,
      <trd>intercepter<klr tip="ind"> (math.)</klr></trd>
    </trdgrp>
    <trd lng="pl">by&cacute; opartym na<klr tip="ind"> (&lstroke;uku)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_ch;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
  <trd lng="fr">d&eacute;couper</trd>
  <trd lng="nl">afsnijden</trd>
</drv>

<drv mrk="trancx.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Doni formon al io, for<tld/>ante de &gcirc;i pecetojn:
      <ekz>
        en la balkono estis el<tld/>ita karikatura viza&gcirc;o<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="tajl.0i">tajli</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
  <trd lng="nl">uitsnijden</trd>
  <trd lng="ru">&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="trancx.dedekinda0o">
  <kap>dedekinda <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(en la aro de <ref cel="racion.0o">racionaloj</ref>)</klr>
      Tia duopo <frm>(<g>A</g>,<g>B</g>)</frm> el subaroj de la aro de
      racionaloj, ke:
      (1) &ccirc;iu elemento de <frm><g>A</g></frm> estas strikte malpli
      granda ol ajna elemento de <frm><g>B</g></frm>; 
      (2) por &ccirc;iu pozitiva racionalo <frm>&epsilon;</frm> ekzistas 
      tiaj <frm><k>x</k>&#8712;<g>A</g></frm> kaj 
      <frm><k>y</k>&#8712;<g>B</g></frm>, ke 
      <frm>0 &lt; <k>y</k>-<k>x</k> &lt; &epsilon;</frm>:
      <ekz>
        neracionalo estas difinebla per <klr>(dedekinda)</klr> <tld/>o
        farata en racionaloj
        <fnt><bib>MatTerm</bib><lok>p. 8</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dedeki.0o">Dedekindo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Dedekindscher <ind>Schnitt</ind></trd>
  <trd lng="en">Dedekind['s] <ind>cut</ind></trd>
  <trd lng="fr">coupure de Dedekind</trd>
  <trd lng="pl">przekr&oacute;j Dedekinda</trd>
  <trd lng="ru">&c_d;&c_je;&c_d;&c_je;&c_k;&c_i;&c_n;&c_d;&c_o;&c_v;&c_o; <ind>&c_s;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</ind></trd>
</drv>
</art>
