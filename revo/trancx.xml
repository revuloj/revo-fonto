    <trd>&c_r;&c_o;&c_z;&c_r;&c_ib;&c_z;</trd>,
    <trd>&c_r;&c_ib;&c_z;&c_k;&c_i;&c_j; &c_b;&c_ib;&c_l;&c_mol;</trd>,
    <trd>&c_k;&c_o;&c_l;&c_mol;&c_k;&c_a;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_ib;&c_z;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_i;&c_b;&c_k;&c_a; (&c_h;&c_l;&c_ib;&c_b;&c_a;)</trd>,
    <trd>&c_sh;&c_m;&c_a;&c_t;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="trancx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="trancx.0ajxo.SNC">
    <dif>
      <ref tip="super" cel="pec.0o.rompajxo">Peco</ref>
      apartigita per <tld/>o:
      <ekz>
        ridante oni kaptadis <tld/>a&jcirc;ojn da pomoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        sklavoj enportis tripiedon <klr>[&#x2026;]</klr>, sur kiun ili komencis &scirc;uti etajn
        <tld/>a&jcirc;ojn da mirho kaj nardo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        sur vespera tablo estis teo kun <tld/>a&jcirc;oj de &scirc;inko
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi donis al &scirc;i <tld/>a&jcirc;on da pano trempitan en mielo
        <fnt>
          <aut>A. L&ouml;wenstein</aut>,
          <vrk>La &scirc;tona urbo</vrk>,
          <lok>1999</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ankora&ubreve; &scirc;miras buteron sur siajn
        pan<tld/>a&jcirc;ojn
        <fnt>
          <aut>S. &Scaron;timec</aut>,
          <vrk>Zagrebaj solistoj en Bonaero</vrk>,
          <bib>Monato</bib>,
          <lok>2001:7</lok>
        </fnt>.
      </ekz>
    </dif>  
    <trd lng="be">&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a; <klr>(&c_a;&c_d;&c_r;&c_e;&c_z;&c_a;&c_n;&c_a;&c_ja;)</klr></trd>
    <trdgrp lng="de">
      <trd>Scheibe <klr>(geschnitten)</klr></trd>,
      <trd>St&uuml;ck <klr>(geschnitten)</klr></trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>filete</trd>,
      <trd>tajada</trd>,
      <trd>rebanada</trd>,
      <trd>loncha</trd>,
      <trd>rodaja</trd>,
      <trd>porci&oacute;n</trd>
    </trdgrp>
    <trd lng="fr">tranche</trd>
  </snc>
  <snc mrk="trancx.0ajxo.fendo">
    <dif>
      <tld lit="T"/>o, fendo:
      <ekz>
        la <klr>[hak]</klr>ilo falis perforte sur la kverka&jcirc;an tabulon en kiun &gcirc;i kavigis profundan
        <tld/>a&jcirc;on
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Deksepa</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_n;&c_a;&c_d;&c_r;&c_e;&c_z;</trd>,
      <trd>&c_v;&c_y;&c_r;&c_a;&c_z;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x5207;&#x308C;  <pr>&#x3072;&#x3068;&#x304D;&#x308C;</pr></trd>,
    <trd>&#x5207;&#x7247;  <pr>&#x305B;&#x3063;&#x307A;&#x3093;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_ib;&c_z;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_i;&c_b;&c_k;&c_a; (&c_h;&c_l;&c_ib;&c_b;&c_a;)</trd>,
    <trd>&c_sh;&c_m;&c_a;&c_t;&c_o;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="trancx.0ilo">
  <kap><ofc>*</ofc><tld/>ilo</kap>
  <snc mrk="trancx.0ilo.KOMUNE">
    <dif>
      Diversforma ilo por <tld/>i:
      <ekz>
        la <tld/>ilo estis tiel malakra, ke mi ne povis <tld/>i per &gcirc;i la viandon kaj mi
        devis uzi mian po&scirc;an <tld/>ilon
        <fnt>
          <bib>F</bib>
          <lok>&FE; 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni metis anta&ubreve; mi man&gcirc;ilaron, kiu konsistis el telero, kulero, <tld/>ilo,
        forko, glaseto
        <fnt>
          <bib>F</bib>
          <lok>&FE; 34</lok>
        </fnt>;
      </ekz>
      <ekz>
        faru al vi &scirc;tonajn <tld/>ilojn
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 5:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        estis al &scirc;i, <klr>[&#x2026;]</klr> kiel se &scirc;i pa&scirc;us sur akrajn pinglojn kaj <tld/>ilojn
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la fi&scirc;o estis kaptita, <klr>[&#x2026;]</klr> la servistino per granda <tld/>ilo &gcirc;in
        dis<tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Persista stana soldato</lok>
        </fnt>;
      </ekz>
      <ekz>
        en mia zono sidis <tld/>ilo kaj pistolo
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        ne timu <tld/>ilon, timu babilon
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_o;&c_zh;</trd>
  <trd lng="bg">&c_n;&c_o;&c_zh;</trd>
  <trd lng="bo">&t_ga;&t_rata;&t_gigu;&t_tsheg;</trd>
  <trd lng="de">Messer</trd>
  <trd lng="en">knife</trd>
  <trd lng="fr">couteau</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;g&oacute;eszk&ouml;z</trd>,
    <trd>k&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cuchilla</trd>,
    <trd>cuchillo</trd>,
    <trd>navaja</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5203;&#x7269;  <pr>&#x306F;&#x3082;&#x306E;</pr></trd>,
    <trd>&#x30CA;&#x30A4;&#x30D5;</trd>,
    <trd>&#x30AB;&#x30C3;&#x30BF;&#x30FC;</trd>,
    <trd>&#x5305;&#x4E01;  <pr>&#x307B;&#x3046;&#x3061;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">mes</trd>
  <trd lng="pl">n&oacute;&zdot;</trd>
  <trd lng="ru">&c_n;&c_o;&c_zh;</trd>
  <trd lng="sv">kniv</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_ib;&c_zh;</trd>,
    <trd>&c_r;&c_ib;&c_z;&c_a;&c_k;</trd>
  </trdgrp>
</drv>

<drv mrk="trancx.al0i">
  <kap><ofc>*</ofc>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trancx.al0i.KOMUNE">
    <dif>
      Doni &gcirc;ustan formon per <tld/>ado:
      <ekz>
        el la tolo oni al<tld/>is dekdu pecojn
        <fnt>
          <bib>Fab3</bib>
          <lok>Lino</lok>
        </fnt>;
      </ekz>
      <ekz>
        vestojn, supervestojn kaj e&ccirc; iom pli elegantan
        tola&jcirc;on mi al<tld/>i ne povus
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="es">cortar</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
  <trd lng="de">zuschneiden</trd>
  <trd lng="hu">kiszab <klr tip="ind">(anyagb&oacute;l)</klr></trd>
  <trd lng="nl">knippen<klr> (v.kledingstuk)</klr></trd>
  <trd lng="pl">docina&cacute;</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_r;&c_i;&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i; (&c_d;&c_o; &c_ch;&c_o;&c_g;&c_o;-&c_n;.)</trd>,
    <trd>&c_k;&c_r;&c_o;&c_ji;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_k;&c_r;&c_o;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_sh;&c_i;&c_v;&c_a;&c_t;&c_i; &c_a;&c_p;&c_l;&c_ib;&c_k;&c_a;&c_c;&c_ib;&c_jeu;&c_ju;</trd>
  </trdgrp>
</drv>

<drv mrk="trancx.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trancx.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="T"/>i &ccirc;e la &ccirc;irka&ubreve;o, por
      doni &gcirc;ustan formon:
      <ekz>
        pacience &scirc;i &ccirc;irka&ubreve;<tld/>is unu post la alia &ccirc;iujn presajn literojn, kiujn &scirc;i
        bezonis por komposti sian leteron
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        la salika bran&ccirc;o tie ekstere fari&gcirc;is belega
        arbo, libera kaj ne &ccirc;irka&ubreve;<tld/>ita &gcirc;i
        tie staris
        <fnt><bib>Fab3</bib>, <lok>&Ccirc;io sur sian &gcirc;ustan
        lokon!</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="es">recortar</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_c;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>beschneiden</trd>,
    <trd>zurecht <ind>schneiden</ind></trd>
  </trdgrp>
  <trd lng="fr">tailler</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;rbev&aacute;g</trd>,
    <trd>k&ouml;rbemetsz</trd>
  </trdgrp>
  <trd lng="pl">obcina&cacute;</trd>
</drv>

<drv mrk="trancx.de0i">
  <kap>de<tld/>i, <var><kap>for<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trancx.de0i.KOMUNE">
    <dif>
      <ref tip="super" cel="apart.0igi">Apartigi</ref>, 
      <ref tip="super" cel="for.0igi">forigi</ref> <tld/>ante:
      <ekz>
        ili de<tld/>is tie bran&ccirc;on kun unu peniko da vinberoj
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 13:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> forrazis al ili duonon de la barbo, kaj de<tld/>is iliajn
        vestojn &gcirc;is duono
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 10:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Cipora</nom> prenis akran &scirc;tonon, kaj de<tld/>is la prepucion de sia filo
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 4:25</lok>
        </fnt>;
      </ekz>
      <ekz>
        de<tld/>i la ungojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ursa Felo</lok>
        </fnt>;
      </ekz>
      <ekz>
        maldelikataj krea&jcirc;oj, ili for<tld/>is pecon de mia belega barbo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ne&gcirc;ulino kaj Rozulino</lok>
        </fnt>!
      </ekz>
      <ekz>
        li de<tld/>is buklon de siaj ar&gcirc;entkoloraj haroj
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kolo de la botelo restis nerompita, kaj oni &gcirc;in de<tld/>is per diamanto
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li prenis el sia zono malgrandan ponardon, kaj for<tld/>is la sigelon &ccirc;e la
        malsupro
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        potenca konstrua&jcirc;o <klr>[&#x2026;]</klr> de la formo de de<tld/>ita 
        <klr>(senpinta)</klr> piramido
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro V</lok>
        </fnt>;
      </ekz>
      <ekz>
        dek fojojn mezuru, unu fojon de<tld/>u
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">lev&aacute;g</trd>
    <trd lng="pl">odcina&cacute;</trd>
    <trd lng="ru">&c_o;&c_t;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="trancx.de0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="apart.0igi">Apartigi</ref>, 
      <ref tip="super" cel="for.0igi">forigi</ref> akre:
      <ekz>
        li forigis tiun paragrafon kiel nepraktikan, anka&ubreve; li for<tld/>is multon el la
        lasta punkto
        <fnt>
          <bib>VivZam</bib>
          <lok>Vivo de Zamenhof</lok>
        </fnt>;
      </ekz>
      <ekz>
        necesos pli for<tld/>i publikajn elspezojn
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2015/011508.php">Malpli malbonaj inter aliaj malbonaj</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni devas radike for<tld/>i la ribelemon
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> refutas, ke tiu revolucio vere celis egalecon: &gcirc;i for<tld/>is
        tutan aron da nefrance parolantaj minoritatoj
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2007/009079.php">Mi estas homo</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="hu">elv&aacute;g</trd>
    <trd lng="pl">odcina&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_ch;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="trancx.de0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      <klr>(p.p. <ref cel="angul.0o">angulo</ref> rilate al
      <ref cel="kurb.0o.MAT">kurbo</ref> a&ubreve;
      <ref cel="surfac.0o.MAT">surfaco</ref>)</klr>
      Diri, ke ebena angulo de<tld/>as
      <ref cel="ark.0o.segmento">arkon</ref> sur cirklo, signifas, ke la
      randoj de tiu arko estas intersekcopunktoj de la cirklo kun la
      lateroj de la angulo; diri, ke solida angulo de<tld/>as surfacon
      sur sfero, signifas, ke tiu surfaco estas intersekco de la sfero
      kun la solida angulo:
      <ekz>
        <ref cel="angul.centra0o">centra angulo</ref> estas duoblo de
        <ref cel="angul.cirkonferenca0o">cirkonferenca angulo</ref>, kiu
        de<tld/>as la saman arkon.
      </ekz>
    </dif>
    <adm>
      &Ccirc;i tiu termino logike devenas de "d&eacute;couper" kaj
      "&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;", sed mi ne certas,
      &ccirc;u &gcirc;i estas tradicia en E.
      <aut>MB</aut>
    </adm>
    <trd lng="de">einen <ind>zugeordneten</ind> Bogen haben</trd>
    <trdgrp lng="en">
      <trd>be subtended by</trd>,
      <trd>intercept<klr tip="ind"> (an arc of curve, a
      surface)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;couper</trd>,
      <trd>intercepter<klr tip="ind"> (math.)</klr></trd>
    </trdgrp>
    <trd lng="hu">kimetsz<klr tip="ind"> (&iacute;vet)</klr></trd>
    <trd lng="pl">by&cacute; opartym na<klr tip="ind">
    (&lstroke;uku)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_p;&c_i;&c_r;&c_a;&c_t;&c_s;&c_ja;<klr tip="ind">
      (&c_n;&c_a; &c_d;&c_u;&c_g;&c_u;)</klr></trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_ch;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_c;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>abschneiden</trd>,
    <trd>k&uuml;rzen</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cortar</trd>,
    <trd>amputar</trd>,
    <trd>cercenar</trd>,
    <trd>truncar</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;couper</trd>
  <trd lng="ja">&#x5207;&#x308A;&#x6368;&#x3066;&#x308B;  <pr>&#x304D;&#x308A;&#x3059;&#x3066;&#x308B;</pr></trd>
  <trd lng="nl">afsnijden</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_k;&c_r;&c_a;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="trancx.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trancx.el0i.formi">
    <dif>
      Doni formon al io, for<tld/>ante de &gcirc;i pecetojn,
      <ref tip="vid" cel="tajl.0i">tajli</ref>:
      <ekz>
        <klr>[li]</klr> el<tld/>as la komikajn amuzajn bildojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        la sapisto el&scirc;iris el la sub&scirc;tofo de sia malnova jako ru&gcirc;an &ccirc;ifonon, el<tld/>is
        el &gcirc;i dentvican kokan kreston kaj algluis &gcirc;in
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        li el<tld/>is du pecetojn da ligno, kiuj similis al malgrandaj lignaj &scirc;uoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        el<tld/>i el ha&ubreve;toj rimenojn
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
          <lok>Dua skizo:</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la balkono estis el<tld/>ita karikatura viza&gcirc;o
        <fnt><bib>Fab2</bib>, <lok>Malnova domo</lok></fnt>.
      </ekz>
    </dif>  
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_v;&c_y;&c_c;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">ausschneiden</trd>
    <trd lng="es">tallar</trd>
    <trdgrp lng="hu">
      <trd>kiv&aacute;g</trd>,
      <trd>kimetsz</trd>
    </trdgrp>
    <trd lng="nl">uitsnijden</trd>
    <trd lng="pl">wycina&cacute;</trd>
    <trd lng="ru">&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="trancx.el0i.el_io">
    <dif>
      For<tld/>i el io:
      <ekz>
        la junulo iris al la drako, el<tld/>is la sep langojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        per &ccirc;i tiu ponardo mi povus el<tld/>i tiun malnoblan koron
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XXI</lok>
        </fnt>;
      </ekz>
      <ekz>
        premma&scirc;inoj el<tld/>is la necesajn formojn per unusola premo
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_a;&c_d;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_a;&c_d;&c_c;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">heraus <ind>schneiden</ind></trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x5207;&#x308A;&#x629C;&#x304F;  <pr>&#x304D;&#x308A;&#x306C;&#x304F;</pr></trd>,
    <trd>&#x5207;&#x308A;&#x51FA;&#x3059;  <pr>&#x304D;&#x308A;&#x3060;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&c_r;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_v;&c_i;&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
