<drv mrk="trancx.0o">
  <kap><tld/>o</kap>
  <snc mrk="trancx.0o.KOMUNE">
    <dif>
      Ago <tld/>i; rezulto de tiu ago:
      <ekz>
        la &hcirc;irurgiistoj faris <tld/>ojn en la karno por trovi la
        plumbon<fnt>B</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_p;&c_a;&c_r;&c_e;&c_z;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_r;&c_e;&c_z;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_r;&c_e;&c_z;</trd>
    </trdgrp>
    <trd lng="de">Schnitt</trd>
    <trd lng="en">cut</trd>
    <trdgrp lng="es">
      <trd>cortadura</trd>,
      <trd>recorte</trd>,
      <trd>corte</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>coupe</trd>,
      <trd>d&eacute;coupe</trd>,
      <trd>incision</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>v&aacute;g&aacute;s</trd>,
      <trd>bemetsz&eacute;s</trd>,
      <trd>szelet</trd>
    </trdgrp>
    <trd lng="pl">ci&eogonek;cie</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_r;&c_je;&c_z;</trd>,
      <trd>&c_n;&c_a;&c_d;&c_r;&c_je;&c_z;</trd>,
      <trd>&c_r;&c_a;&c_z;&c_r;&c_je;&c_z;</trd>
    </trdgrp>
  </snc>
  <snc mrk="trancx.0o.dedekinda">
    <uzo tip="fak">MAT</uzo>
    <ref tip="dif" cel="trancx.dedekinda0o">dedekinda <tld/>o.</ref>
    <trd lng="be">&c_s;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    <trd lng="de">Schnitt</trd>
    <trd lng="hu">szelet</trd>
    <trd lng="ru">&c_s;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trd lng="bo">&t_ra;&t_mata;&t_tsheg;</trd>
</drv>

<drv mrk="trancx.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="trancx.0ajxo.SNC">
    <dif>
      <ref tip="super" cel="pec.0o.rompajxo">Peco</ref>
      apartigita per <tld/>o:
      <ekz>
        mi donis al &scirc;i <tld/>a&jcirc;on da pano trempitan en mielo
        <fnt>
          <aut>A. L&ouml;wenstein</aut>,
          <vrk>La &scirc;tona urbo</vrk>,
          <lok>1999</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili ankora&ubreve; &scirc;miras buteron sur siajn
        pan<tld/>a&jcirc;ojn
        <fnt>
          <aut>S. &Scaron;timec</aut>,
          <vrk>Zagrebaj solistoj en Bonaero</vrk>,
          <bib>Monato</bib>,
          <lok>2001:7</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_s;&c_t;&c_k;&c_a; <klr>(&c_a;&c_d;&c_r;&c_e;&c_z;&c_a;&c_n;&c_a;&c_ja;)</klr></trd>
  <trdgrp lng="es">
    <trd>filete</trd>,
    <trd>tajada</trd>,
    <trd>rebanada</trd>,
    <trd>loncha</trd>,
    <trd>rodaja</trd>,
    <trd>porci&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">tranche</trd>
</drv>

<drv mrk="trancx.0ilo">
  <kap><ofc>*</ofc><tld/>ilo</kap>
  <snc mrk="trancx.0ilo.KOMUNE">
    <dif>
      Diversforma ilo por <tld/>i:
      <ekz>
        la <tld/>ilo estis tiel malakra, ke mi ne povis <tld/>i per &gcirc;i la viandon kaj mi
        devis uzi mian po&scirc;an <tld/>ilon
        <fnt>
          <bib>F</bib>
          <lok>&FE; 34</lok>
        </fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_o;&c_zh;</trd>
  <trd lng="bg">&c_n;&c_o;&c_zh;</trd>
  <trd lng="bo">&t_ga;&t_rata;&t_gigu;&t_tsheg;</trd>
  <trd lng="de">Messer</trd>
  <trd lng="en">knife</trd>
  <trd lng="fr">couteau</trd>
  <trdgrp lng="hu">
    <trd>v&aacute;g&oacute;eszk&ouml;z</trd>,
    <trd>k&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cuchilla</trd>,
    <trd>cuchillo</trd>,
    <trd>navaja</trd>
  </trdgrp>
  <trd lng="nl">mes</trd>
  <trd lng="pl">n&oacute;&zdot;</trd>
  <trd lng="ru">&c_n;&c_o;&c_zh;</trd>
  <trd lng="sv">kniv</trd>
</drv>

<drv mrk="trancx.al0i">
  <kap><ofc>*</ofc>al<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trancx.al0i.KOMUNE">
    <dif>
      Doni &gcirc;ustan formon per <tld/>ado:
      <ekz>
        al<tld/>i drapon por vesto;
      </ekz>
      <ekz>
        vestojn, supervestojn kaj e&ccirc; iom pli elegantan
        tola&jcirc;on mi  al<tld/>i ne povus
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="es">cortar</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
  <trd lng="hu">kiszab <klr tip="ind">(anyagb&oacute;l)</klr></trd>
  <trd lng="nl">knippen<klr> (v.kledingstuk)</klr></trd>
  <trd lng="pl">docina&cacute;</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="trancx.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trancx.cxirkaux0i.KOMUNE">
    <dif>
      <tld lit="T"/>i &ccirc;e la &ccirc;irka&ubreve;o, por
      doni &gcirc;ustan formon:
      <ekz>
        la salika bran&ccirc;o tie ekstere fari&gcirc;is belega
        arbo, libera kaj ne &ccirc;irka&ubreve;<tld/>ita &gcirc;i
        tie staris
        <fnt><bib>Fab3</bib>, <lok>&Ccirc;io sur sian &gcirc;ustan
        lokon!</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="es">recortar</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_b;&c_c;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="fr">tailler</trd>
  <trdgrp lng="hu">
    <trd>k&ouml;rbev&aacute;g</trd>,
    <trd>k&ouml;rbemetsz</trd>
  </trdgrp>
  <trd lng="pl">obcina&cacute;</trd>
</drv>

<drv mrk="trancx.de0i">
  <kap>de<tld/>i, <var><kap>for<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trancx.de0i.KOMUNE">
    <dif>
      Apartigi, forigi <tld/>ante:
      <ekz>
        dek fojojn mezuru, unu fojon de<tld/>u
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        en formo de de<tld/>ita (senpinta) piramido<fnt>B</fnt>.
      </ekz>
    </dif>
    <trd lng="hu">lev&aacute;g</trd>
    <trd lng="pl">odcina&cacute;</trd>
    <trd lng="ru">&c_o;&c_t;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>
  </snc>
  <snc mrk="trancx.de0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Apartigi, forigi:
      <ekz>
        la armeo estas for<tld/>ita de la &ccirc;efurbo<fnt>B</fnt>;
      </ekz>
      <ekz>
        pro manko de komunikiloj tiu vasta regiono estas for<tld/>ita
        for de la mondvivo.
      </ekz>
    </dif>
    <trd lng="hu">elv&aacute;g</trd>
    <trd lng="pl">odcina&cacute;</trd>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_ch;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="trancx.de0i.MAT">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="klr">(fakula &jcirc;argono)</uzo>
    <dif>
      <klr>(p.p. <ref cel="angul.0o">angulo</ref> rilate al
      <ref cel="kurb.0o.MAT">kurbo</ref> a&ubreve;
      <ref cel="surfac.0o.MAT">surfaco</ref>)</klr>
      Diri, ke ebena angulo de<tld/>as
      <ref cel="ark.0o.segmento">arkon</ref> sur cirklo, signifas, ke la
      randoj de tiu arko estas intersekcopunktoj de la cirklo kun la
      lateroj de la angulo; diri, ke solida angulo de<tld/>as surfacon
      sur sfero, signifas, ke tiu surfaco estas intersekco de la sfero
      kun la solida angulo:
      <ekz>
        <ref cel="angul.centra0o">centra angulo</ref> estas duoblo de
        <ref cel="angul.cirkonferenca0o">cirkonferenca angulo</ref>, kiu
        de<tld/>as la saman arkon.
      </ekz>
    </dif>
    <adm>
      &Ccirc;i tiu termino logike devenas de "d&eacute;couper" kaj
      "&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;", sed mi ne certas,
      &ccirc;u &gcirc;i estas tradicia en E.
      <aut>MB</aut>
    </adm>
    <trd lng="de">einen <ind>zugeordneten</ind> Bogen haben</trd>
    <trdgrp lng="en">
      <trd>be subtended by</trd>,
      <trd>intercept<klr tip="ind"> (an arc of curve, a
      surface)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>d&eacute;couper</trd>,
      <trd>intercepter<klr tip="ind"> (math.)</klr></trd>
    </trdgrp>
    <trd lng="hu">kimetsz<klr tip="ind"> (&iacute;vet)</klr></trd>
    <trd lng="pl">by&cacute; opartym na<klr tip="ind">
    (&lstroke;uku)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_p;&c_i;&c_r;&c_a;&c_t;&c_s;&c_ja;<klr tip="ind">
      (&c_n;&c_a; &c_d;&c_u;&c_g;&c_u;)</klr></trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_k;&c_a;&c_t;&c_mol;</trd>,
      <trd>&c_o;&c_t;&c_s;&c_je;&c_ch;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_a;&c_d;&c_c;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>cortar</trd>,
    <trd>amputar</trd>,
    <trd>cercenar</trd>,
    <trd>truncar</trd>
  </trdgrp>
  <trd lng="fr">d&eacute;couper</trd>
  <trd lng="nl">afsnijden</trd>
</drv>

<drv mrk="trancx.el0i">
  <kap>el<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trancx.el0i.formi">
    <dif>
      Doni formon al io, for<tld/>ante de &gcirc;i pecetojn,
      <ref tip="vid" cel="tajl.0i">tajli</ref>:
      <ekz>
        en la balkono estis el<tld/>ita karikatura viza&gcirc;o
        <fnt><bib>Fab2</bib>, <lok>Malnova domo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_r;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_c;&c_ib;&c_n;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">tallar</trd>
  <trdgrp lng="hu">
    <trd>kiv&aacute;g</trd>,
    <trd>kimetsz</trd>
  </trdgrp>
  <trd lng="nl">uitsnijden</trd>
  <trd lng="pl">wycina&cacute;</trd>
  <trd lng="ru">&c_v;&c_y;&c_r;&c_je;&c_z;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="trancx.mis0o">
  <kap>mis<tld/>o</kap>
  <snc mrk="trancx.mis0o.LIN">   
    <dif>
      <ref tip="super" cel="lud.vort0o.LIN">Vortludo</ref>
      en kiu de malsama divido de samsonaj vortoj rezultas
      malsama senco,
      ekzemple en
      <ctl>la stranga butiko</ctl> kaj
      <ctl>last-ranga butiko</ctl>;
      <ref tip="sin" cel="kalembur.0o.vortludo">kalemburo</ref>:
      <ekz>
        li ne markis per pa&ubreve;zo la limojn inter la radikoj en siaj 
        mis<tld/>oj
        <fnt>
          <aut>M.-T. Lloancy</aut>
          <vrk><url ref="http://kabareto.esperanto.cc/perhumuro.htm">
          Per humuro al maturo: Schwartz 100-jara</url></vrk>
          <lok>Franca Esperantisto, 1990-12 (416)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_l;&c_ja;&c_m;&c_b;&c_u;&c_r;</trd>
  <trd lng="cs">kalamb&uacute;r</trd>
  <trd lng="de">Kalauer</trd>
  <trd lng="es">calambur</trd>
  <trd lng="fr">calembour</trd>
  <trd lng="sk">kalamb&uacute;r</trd>
</drv>

<drv mrk="trancx.posx0ilo">
  <kap>po&scirc;<tld/>ilo</kap>
  <snc mrk="trancx.posx0ilo.KOMUNE">
    <dif>
      Malgranda <tld/>ilo kun faldebla klingo:
      <ekz>
        sur la baskon &scirc;i elmetadis rozarion, po&scirc;<tld/>ilon,
        kelke da pankrustetoj
        <fnt>
          <aut>B. N&ecaron;mcov&aacute;, trad. V. Tobek kaj K. Proch&aacute;zka</aut>,
          <vrk><url ref="https://eo.wikisource.org/wiki/Avineto/I.">
          Avineto</url></vrk>,
          <lok>[sen dato]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_o;&c_zh;&c_y;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_a;&c_n;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_z;&c_o;&c_r;&c_y;&c_k;</trd>
  </trdgrp>
  <trd lng="de">Taschenmesser</trd>
  <trd lng="fr">canif</trd>
  <trd lng="hu">zsebk&eacute;s</trd>
  <trd lng="nl">zakmes</trd>
  <trd lng="es">navaja de bolsillo</trd>
  <trd lng="pl">scyzoryk</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_o;&c_ch;&c_i;&c_n;&c_n;&c_y;&c_j;
    &c_n;&c_o;&c_zh;&c_i;&c_k;</trd>,
    <trd>&c_s;&c_k;&c_l;&c_a;&c_d;&c_n;&c_o;&c_j;
    &c_n;&c_o;&c_zh;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>fickkniv</trd>,
    <trd>f&auml;llkniv</trd>
  </trdgrp>
</drv>

<drv mrk="trancx.dedekinda0o">
  <kap>dedekinda <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc>
    <dif>
      <klr>(en la aro de <ref cel="racion.0o">racionaloj</ref>)</klr>
      Tia duopo <frm am="(bb A,bb B)">(<g>A</g>,<g>B</g>)</frm> el
      subaroj de la aro de
      racionaloj, ke:
      (1) &ccirc;iu elemento de <frm am="bb A"><g>A</g></frm> estas strikte
      malpli
      granda ol ajna elemento de <frm am="bb B"><g>B</g></frm>;
      (2) por &ccirc;iu pozitiva racionalo <frm am="epsilon">&epsilon;</frm> ekzistas
      tiaj <frm am="x in bb A"><k>x</k>&#8712;<g>A</g></frm> kaj
      <frm am="y in bb B"><k>y</k>&#8712;<g>B</g></frm>, ke
      <frm am="0 lt y-x lt epsilon">0 &lt; <k>y</k>-<k>x</k> &lt;
      &epsilon;</frm>:
      <ekz>
        neracionalo estas difinebla per <klr>(dedekinda)</klr> <tld/>o
        farata en racionaloj
        <fnt><bib>MatTerm</bib>, <lok>p. 8</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dedeki.0o">Dedekindo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Dedekindscher <ind>Schnitt</ind></trd>
  <trd lng="en">Dedekind['s] <ind>cut</ind></trd>
  <trd lng="fr">coupure de Dedekind</trd>
  <trd lng="hu">Dedekind-szelet</trd>
  <trd lng="pl">przekr&oacute;j Dedekinda</trd>
  <trd lng="ru">&c_d;&c_je;&c_d;&c_je;&c_k;&c_i;&c_n;&c_d;&c_o;&c_v;&c_o;
  <ind>&c_s;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</ind></trd>
</drv>
