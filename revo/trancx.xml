    <trd lng="ru">&c_s;&c_je;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
  <trd lng="bo">&t_ra;&t_mata;&t_tsheg;</trd>
  <trdgrp lng="uk">
