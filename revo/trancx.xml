<drv mrk="trancx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="trancx.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="divid.0i.partigi">Dividi</ref> solida&jcirc;on per akra ilo:
      <ekz>
        la tajloreto, prenis panon el la &scirc;ranko, <tld/>is pecon tra la tuta bulo kaj
        sur&scirc;miris la marmeladon
        <fnt>
          <bib>ElektFab</bib>
          <lok>La kura&gcirc;a tajloreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili batetendis foliojn da oro kaj <tld/>is ilin en formo de fadenoj por enteksi
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 39:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        amba&ubreve;<tld/>a glavo
        <fnt>
          <bib>MT</bib>
          <lok>Sentencoj 5:4</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> kolektis de &gcirc;i kolocintojn <klr>[&#x2026;]</klr> kaj <tld/>is ilin en
        la kaldronon kun la supo
        <fnt>
          <bib>MT</bib>
          <lok>II. Re&gcirc;oj 4:39</lok>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is pecon da ligno kaj iufoje anka&ubreve; <tld/>is al si la fingron
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i tamen balda&ubreve; ree sin levis, por enver&scirc;i teon al sia edzo kaj <tld/>i panon
        kun butero
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        per sia pio&ccirc;eto, li tran&ccirc;is en la glacio kelkajn &scirc;tupojn
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Sesa</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        via lango kiel akrigita razilo &gcirc;i tran&ccirc;as malicon
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 52:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        el fremda ledo oni <tld/>as lar&gcirc;e
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i rekte en la karnon
        <klr>(uzi efikan, sed malmildan rimedon)</klr>
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        per tondiloj li <tld/>is la dekstran manikon
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu tajloro havas sian <tld/>manieron
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="falcx.0i">fal&ccirc;i</ref>,
      <ref cel="fend.0i.KOMUNE">fendi</ref>,
      <ref cel="hak.0i">haki</ref>,
      <ref cel="romp.0i">rompi</ref>,
      <ref cel="sekc.0i">sekci</ref>,
      <ref cel="tond.0i">tondi</ref>
    </refgrp>
    <trd lng="es">cortar</trd>
    <trdgrp lng="pl">
      <trd>ci&aogonek;&cacute;</trd>,
      <trd>przecina&cacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="trancx.0i.FIG_trairi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Trai&gcirc;i:
      <ekz>
        kelkdeko da kanaloj <klr>[&#x2026;]</klr> <tld/>is <klr>[la ebena&jcirc;on]</klr> en &ccirc;iuj
        direktoj, kaj &gcirc;i &scirc;ajnis kvaza&ubreve; reto
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro XXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vipeto de la dukino, svingite de kolera brako, <tld/>is siblante la aeron
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dektria</lok>
        </fnt>;
      </ekz>
      <ekz>
        kor&scirc;ira &gcirc;emo kaj ploro <tld/>is la glacian aeron de la nokto
        <fnt>
          <bib>LGA</bib>
          <lok>VI.</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>is la mallumon per blankaj okuloj a&ubreve;tomobilo de la ru&gcirc;a kruco
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="apart.0igi">apartigi</ref>,
      <ref cel="lim.dis0i">dislimi</ref>,
      <ref cel="kur.tra0i">trakuri</ref>
    </refgrp>
   <trdgrp lng="de">
     <trd>zerschneiden</trd>,
     <trd>zerteilen</trd>
   </trdgrp>
   <trd lng="es">cortar</trd>
   <trd lng="pl">przecina&cacute;</trd>
  </snc>
  <snc mrk="trancx.0i.FIG_frapi">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Akre kaj dolore <ref tip="sin" cel="frap.0i.tusxegi">frapi</ref> la sentumojn:
      <ekz>
        kruda vento <tld/>is al &ccirc;iu la viza&gcirc;on
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de unu patrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la frosto <tld/>is la maljunajn pulmojn
        <fnt>
          <bib>LGA</bib>
          <lok>V.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la lumo <klr>[&#x2026;]</klr> al la proksime starantaj <tld/>as la okulojn kaj ili
        penas estingi &gcirc;in
        <fnt>
          <bib>EE</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vido de la ri&ccirc;a tablo akre <tld/>is sin en la okulojn
        <fnt>
          <bib>LGA</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        doloro, envio, kolero <tld/>is tiam dudek virinajn brustojn
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        ha, nun vi ploras, nun kompato <tld/>as viajn korojn
        <fnt>
          <bib>Ret</bib>
          <lok>Aldono</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vortoj slavaj, <klr>[&#x2026;]</klr> enmetite en sistemon romanagermanan, <tld/>as
        la orelon
        <fnt>
          <bib>LR</bib>
          <lok>Unua Parto &ndash; Leksikologio</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agac.0i.inciti">agaci<sncref/></ref>,
      <ref cel="impres.0i">impresi</ref>,
      <ref cel="incit.0i.fizike">inciti<sncref/></ref>,
      <ref cel="pik.0i.akre_sentojn">piki<sncref/></ref>,
      <ref cel="tauxz.0i.FIG">ta&ubreve;zi<sncref/></ref>,
      <ref cel="turmen.0i">turmenti</ref>
    </refgrp>
    <trd lng="de">stechen <klr>(Gef&uuml;hl)</klr></trd>
    <trd lng="es">romper</trd>
  </snc>
