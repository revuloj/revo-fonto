<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vost</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="vost.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vost.0o.vertebraro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Libere pendanta ekstremo de la spino, &ccirc;e kelkaj
      <ref tip="malprt" cel="verteb.0uloj">vertebruloj</ref>:
      <ekz>
        &ccirc;iu vulpo sian <tld/>on la&ubreve;das
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        subpremi, subtiri la <tld/>on
        <klr>(pp frapita hundo; f. pp trompito.
        <ref tip="vid" cel="naz.0o">nazo</ref>)</klr>.
      </ekz>
    </dif>
    <trd lng="de">Schwanz</trd>
    <trdgrp lng="es">
      <trd>cola</trd>,
      <trd>rabo</trd>
    </trdgrp>
    <trd lng="hu">farok</trd>
    <trdgrp lng="id">
      <trd>buntut</trd>,
      <trd>ekor</trd>
    </trdgrp>
    <trd lng="nl">staart</trd>
    <trd lng="pl">ogon</trd>
    <trd lng="sv">svans</trd>
  </snc>
  <snc mrk="vost.0o.postajxo">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Posta parto de la korpo de birdoj a&ubreve; serpentoj:
      <ekz>
        korniko vundita propran <tld/>on timas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Schwanz</trd>
    <trd lng="es">cola</trd>
    <trd lng="hu">farok</trd>
    <trd lng="nl">staart</trd>
    <trd lng="pl">ogon</trd>
    <trd lng="sv">stj&auml;rt</trd>
  </snc>
  <snc mrk="vost.0o.fino">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fino; lasta parto:
      <ekz>
        la <tld/>o de vagonaro;
      </ekz>
      <ekz>
        la <tld/>o <klr>(trena&jcirc;o)</klr> de virina robo;
      </ekz>
      <ekz>
        <tld/>o <klr>(baskoj)</klr> de frako;
      </ekz>
      <ekz>
        tiu senutila deklaro faras impreson de alkudrita <tld/>o.
      </ekz>
    </dif>
    <trd lng="de">Ende</trd>
    <trd lng="es">cola</trd>
    <trd lng="hu">v&eacute;ge<klr tip="amb"> (vminek)</klr></trd>
    <trd lng="nl">sleep</trd>
    <trd lng="sv">sl&auml;p</trd>
  </snc>
  <snc mrk="vost.0o.vico">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Longa vico da atendantoj.
    </dif>
    <trd lng="de">Schlange</trd>
    <trd lng="es">cola</trd>
    <trd lng="hu">sor<klr tip="amb"> (v&aacute;rakoz&oacute;k&eacute;)</klr></trd>
    <trd lng="nl">rij</trd>
    <trdgrp lng="pl">
      <trd>ogonek</trd>,
      <trd>kolejka</trd>
    </trdgrp>
    <trd lng="sv">k&ouml;</trd>
  </snc>
  <trd lng="be">&c_h;&c_v;&c_o;&c_s;&c_t;</trd>
  <trd lng="br">lost</trd>
  <trd lng="ca">cua</trd>
  <trd lng="el">&omikron;&ypsilon;&rho;&alfa_ton;</trd>
  <trd lng="en">tail</trd>
  <trd lng="fr">queue</trd>
  <trd lng="it">coda</trd>
  <trdgrp lng="pt">
   <trd>cauda</trd>,
   <trd>rabo</trd>
   </trdgrp>
  <trd lng="ru">&c_h;&c_v;&c_o;&c_s;&c_t;</trd>
  <trd lng="sk">zakončenie</trd>
</drv>

<drv mrk="vost.har0o">
  <kap>har<tld/>o</kap>
  <snc mrk="vost.har0o.FRIZ">
    <dif>
      Fasko da haroj libere pendantaj el kuntena la&ccirc;o a&ubreve; banto:
      <ekz>
        Marley kun sia har<tld/>o, kutima ve&scirc;to, pantalono, kaj botoj
        <fnt>
          <aut>C. Dickens, trad. M. Westcott</aut>,
          <vrk><url ref="http://esperanto.luin.nu/eLibrejo/pdf/071-6">
          Kristnaska kanto</url></vrk>,
          <lok>1905</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>cua de cavall</trd>,
    <trd>cua <klr>(cabellera)</klr></trd>
  </trdgrp>
  <trd lng="de">Pferdeschwanz<klr> (Frisur)</klr></trd>
  <trd lng="fr">queue de cheval</trd>
  <trdgrp lng="id">
    <trd><ind>buntut</ind> kuda</trd>,
    <trd><ind>ekor</ind> kuda</trd>
  </trdgrp>
  <trd lng="it">coda di cavallo</trd>
  <trdgrp lng="pl">
    <trd>fr&eogonek;dzel</trd>,
    <trd>chwost</trd>
  </trdgrp>
  <trd lng="sk">konský chvost <klr>(účes)</klr></trd>
  <trd lng="sv">h&auml;stsvans<klr> (frisyr)</klr></trd>
</drv>

<drv mrk="vost.sen0uloj">
  <kap>sen<tld/>uloj</kap>
  <snc mrk="vost.sen0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO"
      lst="voko:zoologiaj_ordoj">Ordo</ref> 
      de <ref tip="super" cel="amfibi.0oj">amfibioj</ref>,
      kies lasta evoluformo ne havas voston;
      (<trd lng="la">Anura</trd>).
    </dif>
    <ref tip="sin" cel="anur.0oj.ZOO">anuroj</ref>
    <refgrp tip="prt">
      <ref cel="raned.0oj">ranedoj</ref>,
      <ref cel="bufed1.0oj">bufedoj</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_s;&c_h;&c_v;&c_o;&c_s;&c_t;&c_y;&c_ja; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">anurs</trd>
  <trd lng="de">Froschlurche</trd>
  <trd lng="fr">anoures</trd>
  <trdgrp lng="hu">
    <trd>b&eacute;k&aacute;k<klr tip="amb"> (rendje)</klr></trd>,
    <trd>farokn&eacute;lk&uuml;liek<klr tip="amb"> (rendje)</klr></trd>
  </trdgrp>
  <trd lng="it">anuri</trd>
  <trd lng="nl">staartlozen</trd>
  <trd lng="pl">p&lstroke;azy <ind>bezogonowe</ind></trd>
  <trd lng="pt">anuros</trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_h;&c_v;&c_o;&c_s;&c_t;&c_y;&c_je; &c_z;&c_je;&c_m;&c_n;&c_o;&c_v;&c_o;&c_d;&c_n;&c_y;&c_je; <klr
  tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">bezchvostové živočíchy</trd>
  <trd lng="sv">stj&auml;rtl&ouml;sa groddjur</trd>
</drv>
</art>
