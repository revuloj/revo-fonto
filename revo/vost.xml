<drv mrk="vost.har0o">
  <kap>har<tld/>o</kap>
  <snc mrk="vost.har0o.FRIZ">
    <dif>
      Fasko da haroj libere pendantaj el kuntena la&ccirc;o a&ubreve; banto:
      <ekz>
        <nom>Marley</nom> kun sia har<tld/>o, kutima ve&scirc;to, pantalono, kaj botoj
        <fnt>
          <aut>C. Dickens, trad. M. Westcott</aut>,
          <vrk><url ref="http://esperanto.luin.nu/eLibrejo/pdf/071-6">
          Kristnaska kanto</url></vrk>,
          <lok>1905</lok>
        </fnt>;
      </ekz>
      <ekz>
        distinga maldika viro, kiu portis junaspektan har<tld/>on
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj'</aut>
          <vrk><url ref="&Monato;/2014/011373.php">Geraldo Mattos kaj Sutte
          All&aacute;</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_o;&c_n;&c_s;&c_k;&c_ib; <ind>&c_h;&c_v;&c_o;&c_s;&c_t;</ind> <klr>(&c_p;&c_r;&c_y;&c_ch;&c_o;&c_s;&c_k;&c_a;)</klr></trd>
  <trdgrp lng="ca">
    <trd>cua de cavall</trd>,
    <trd>cua <klr>(cabellera)</klr></trd>
  </trdgrp>
  <trd lng="cs">ko&ncaron;sk&yacute; ohon <klr>(&uacute;&ccaron;es)</klr></trd>
  <trd lng="de">Pferdeschwanz<klr> (Frisur)</klr></trd>
  <trd lng="fr">queue de cheval</trd>
  <trdgrp lng="id">
    <trd><ind>buntut</ind> kuda</trd>,
    <trd><ind>ekor</ind> kuda</trd>
  </trdgrp>
  <trd lng="it">coda di cavallo</trd>
  <trd lng="ja">&#x30DD;&#x30CB;&#x30FC;&#x30C6;&#x30FC;&#x30EB;</trd>
  <trdgrp lng="pl">
    <trd>fr&eogonek;dzel</trd>,
    <trd>chwost</trd>
  </trdgrp>
  <trd lng="sk">konsk&yacute; chvost <klr>(&uacute;&ccaron;es)</klr></trd>
  <trd lng="sv">h&auml;stsvans<klr> (frisyr)</klr></trd>
  <trd lng="uk">&c_h;&c_v;&#x43E;&c_s;&c_t;&c_i;&#x43A; (&c_f;&#x43E;&c_r;&#x43C;&c_a; &c_z;&c_a;&c_ch;&c_ib;&c_s;&#x43A;&c_i;)</trd>
</drv>

<drv mrk="vost.rugx0ulo">
  <kap>
    ru&gcirc;<tld/>ulo
  </kap>
  <snc mrk="vost.rugx0ulo.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Iu el <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">genro</ref>
      de &ccirc;. 11 <ref tip="lst" lst="voko:birdoj" cel="bird.0o.ZOO">birdo</ref>-specioj 
      el familio mu&scirc;kaptuledoj, kun rustkoloraj a&ubreve; ru&gcirc;brunaj vostoj,
        (<trd lng="la">Phoenicurus</trd>):
      <ekz>
        <ind>&Gcirc;ardenru&gcirc;<tld/>ulo</ind>
        <fnt>
          <vrk><url ref="http://aztekium.pl/word.py?lang=tr&amp;xlang=eo&amp;online=fenikuro">Wiki
          Kelime fenikuro</url></vrk>
        </fnt>;
        <trd lng="de">Gartenrotschwanz</trd>
        <trd lng="fr">rouge-queue &agrave; front blanc</trd>
        <trd lng="la">Phoenicurus phoenicurus</trd>
      </ekz>
      <ekz>
        <ind>Orientazia ru&gcirc;<tld/>ulo</ind>
        <fnt>
          <vrk><url ref="&Viki;/Orientazia_ru%C4%9Dvostulo">Orientazia
          ru&gcirc;vostulo</url></vrk>
        </fnt>;
        <trd lng="de">Spiegelrotschwanz</trd>
        <trd lng="fr">rouge-queue aurore</trd>
        <trd lng="la">Phoenicurus auroreus</trd>
      </ekz>
      <ekz>
        <ind>Nigra ru&gcirc;<tld/>ulo</ind>
        <fnt>
          <vrk><url ref="https://www.naturephoto-cz.com/nigra-ru%C4%9Dvostulo-picture_eo-1493.html">Nigra
          ru&gcirc;vostulo Fotoj, Bildoj</url></vrk>
        </fnt>.
        <trd lng="de">Hausrotschwanz</trd>
        <trd lng="fr">rouge-queue noir</trd>
        <trd lng="la">Phoenicurus ochruros</trd>
      </ekz>
    </dif>
    <ref tip="sin" cel="fenikur.0o.genro">fenikuro</ref>
    <bld lok="&WCU;/thumb/4/40/Gartenrotschwanz_1.jpg/480px-Gartenrotschwanz_1.jpg" prm="CC BY-SA         3.0">&Gcirc;ardenru&gcirc;<tld/>ulo
        <fnt>
          <aut>Aconcagua, propra verko</aut>
          <lok><url ref="&WCW;/File:Gartenrotschwanz_1.jpg">&WCW;/File:Gartenrotschwanz_1.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_y;&c_h;&c_v;&c_o;&c_s;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">Rotschwanz</trd>
  <trd lng="en">Phoenicurus</trd>
  <trd lng="fr">rouge-queue</trd>
  <trd lng="ja">&#x30B8;&#x30E7;&#x30A6;&#x30D3;&#x30BF;&#x30AD;</trd>
  <trd lng="uk">&c_g;&#x43E;&c_r;&c_i;&c_h;&c_v;&c_ib;&c_s;&c_t;&#x43A;&c_a;</trd>
</drv>

<drv mrk="vost.sen0uloj">
  <kap>sen<tld/>uloj</kap>
  <snc mrk="vost.sen0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> 
      de <ref tip="super" cel="amfibi.0oj">amfibioj</ref>,
      kies lasta evoluformo ne havas voston
      (<trd lng="la">Anura</trd>).
    </dif>
    <ref tip="sin" cel="anur.0oj.ZOO">anuroj</ref>
    <refgrp tip="prt">
      <ref cel="raned.0oj">ranedoj</ref>,
      <ref cel="bufed1.0oj">bufedoj</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_b;&c_ja;&c_s;&c_h;&c_v;&c_o;&c_s;&c_t;&c_y;&c_ja; <klr tip="amb">(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  <trd lng="ca">anurs</trd>
  <trd lng="de">Froschlurche</trd>
  <trd lng="fr">anoures</trd>
  <trdgrp lng="hu">
    <trd>b&eacute;k&aacute;k<klr tip="amb"> (rendje)</klr></trd>,
    <trd>farokn&eacute;lk&uuml;liek<klr tip="amb"> (rendje)</klr></trd>
  </trdgrp>
  <trd lng="it">anuri</trd>
  <trd lng="ja">&#x7121;&#x5C3E;&#x985E;  <pr>&#x3080;&#x3073;&#x308B;&#x3044;</pr></trd>
  <trd lng="nl">staartlozen</trd>
  <trd lng="pl">p&lstroke;azy <ind>bezogonowe</ind></trd>
  <trd lng="pt">anuros</trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_h;&c_v;&c_o;&c_s;&c_t;&c_y;&c_je; &c_z;&c_je;&c_m;&c_n;&c_o;&c_v;&c_o;&c_d;&c_n;&c_y;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;)</klr></trd>
  <trd lng="sk">bezchvostov&eacute; &zcaron;ivo&ccaron;&iacute;chy</trd>
  <trd lng="sv">stj&auml;rtl&ouml;sa groddjur</trd>
  <trd lng="uk">&c_b;&c_je;&c_z;&c_h;&c_v;&#x43E;&c_s;&c_t;&c_ib; &c_z;&c_je;&#x43C;&#x43D;&#x43E;&c_v;&#x43E;&c_d;&#x43D;&c_ib;</trd>
</drv>
