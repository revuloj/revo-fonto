<drv mrk="vost.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vost.0o.vertebraro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Libere pendanta ekstremo de la spino, &ccirc;e kelkaj
      <ref tip="malprt" cel="verteb.0uloj">vertebruloj</ref>:
      <ekz>
        &ccirc;iu vulpo sian <tld/>on la&ubreve;das
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        okazon kaptu &ccirc;e l' kapo, &ccirc;ar la <tld/>o estas glita
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        prenu de la vir&scirc;afo la sebon kaj la <tld/>on
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 29:22</lok>
        </fnt>;
      </ekz>
      <ekz>
        subpremi, subtiri la <tld/>on
        <klr>(pp frapita hundo; f. pp trompito;
        vd <ref tip="vid" cel="naz.0o">nazo</ref>)</klr>.
      </ekz>
    </dif>
    <trd lng="de">Schwanz</trd>
    <trdgrp lng="es">
      <trd>cola</trd>,
      <trd>rabo</trd>
    </trdgrp>
    <trd lng="hu">farok</trd>
    <trdgrp lng="id">
      <trd>buntut</trd>,
      <trd>ekor</trd>
    </trdgrp>
    <trd lng="nl">staart</trd>
    <trd lng="pl">ogon</trd>
    <trd lng="sv">svans</trd>
  </snc>
  <snc mrk="vost.0o.postajxo">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Posta parto de la korpo de birdoj a&ubreve; serpentoj:
      <ekz>
        korniko vundita propran <tld/>on timas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        kolorri&ccirc;a kiel <tld/>o de pavo
        <fnt>
          <bib>Fab4</bib>
          <lok>Malbona princo (Legendo)</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Schwanz</trd>
    <trd lng="es">cola</trd>
    <trd lng="hu">farok</trd>
    <trd lng="nl">staart</trd>
    <trd lng="pl">ogon</trd>
    <trd lng="sv">stj&auml;rt</trd>
  </snc>
  <snc mrk="vost.0o.fino">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fino; lasta parto:
      <ekz>
        la <tld/>o de vagonaro;
      </ekz>
      <ekz>
        la <tld/>o <klr>(trena&jcirc;o)</klr> de virina robo;
      </ekz>
      <ekz>
        <tld/>o <klr>(baskoj)</klr> de frako;
      </ekz>
      <ekz>
        movi&gcirc;is okcidenten luma fenomeno, kiu i&gcirc;is ardanta pilko kun longa <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2013/010926.php">Spaco por a&jcirc;oj el la
          spaco</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>on de la unua aviadilo ornamas la blazono de la litova duko
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2013/011092.php">Litovio denove kun
          flugiloj</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Ende</trd>
    <trd lng="es">cola</trd>
    <trd lng="hu">v&eacute;ge<klr tip="amb"> (vminek)</klr></trd>
    <trd lng="nl">sleep</trd>
    <trd lng="sv">sl&auml;p</trd>
  </snc>
