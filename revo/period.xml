<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
 
<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>period</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
 
<drv mrk="period.0o">
  <kap><tld/>o</kap>
  <snc mrk="period.0o.AST">
    <uzo tip="fak">AST</uzo>
    <dif>
      Tempo, kiun bezonas astro por retrovi&gcirc;i
      en sama punkto de sia rondvojo.
    </dif>
    <refgrp tip="vid">
      <ref cel="faz.0o">fazo</ref>,
      <ref cel="stadi.0o">stadio</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>peri&oacute;dus</trd>,
      <trd>kering&eacute;si id&odblac;</trd></trdgrp>
  </snc>
  <snc mrk="period.0o.funkcio">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>EKV</bib><lok>&para;333</lok></fnt>
    <dif>
      <klr>(de reela <ref cel="funkci.0o.MAT">funkcio</ref>
        <frm><k>f</k></frm>)</klr>
      &Ccirc;iu tia reelo <frm>T</frm>, ke
      <frm><k>f</k>(<k>x</k>+T) = <k>f</k>(<k>x</k>)</frm>,
      kiu ajn estas <frm><k>x</k></frm>; pli specife: la
      plej malgranda el tiuj nombroj:
      <ekz>
        la funkcio <ref cel="sinus.0o.funkcio">sinuso</ref> havas
        <tld/>on
        <frm>2&pi;</frm>.
      </ekz>
    </dif>
    <rim>
      La difino nature vasti&gcirc;as al kompleksaj funkcioj kaj al
      &ccirc;iuj aliaj okazoj, kiam adicio estas difinita en la
      fonto-aro.
    </rim>
    <trdgrp lng="hu">
      <trd>peri&oacute;dus</trd>,
      <trd>szakasz</trd></trdgrp>
    <trd lng="pl">okres<klr tip="ind"> (funkcji)</klr></trd>
  </snc>
  <snc mrk="period.0o.dauxro">
    <dif>
      Tempa da&ubreve;ro inter du difinitaj fazoj
      de fenomeno a&ubreve; de evoluado.
    </dif>
    <refgrp tip="vid">
      <ref cel="epok.0o">epoko</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>id&odblac;szak</trd>,
      <trd>id&odblac;k&ouml;z</trd></trdgrp>
  </snc>
  <snc mrk="period.0o.BELE">
    <uzo tip="fak">BELE</uzo>
    <dif>
      Aro da frazoj prezentanta malvolvi&gcirc;on de iu unueca penso.
    </dif>
    <refgrp tip="sin">
      <ref cel="fraz.0aro">frazaro</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;</trd>
  <trd lng="ca">per&iacute;ode</trd>
  <trd lng="de">Periode</trd>
  <trd lng="en">period</trd>
  <trdgrp lng="fr">
    <trd>p&eacute;riode</trd>
  </trdgrp>
  <trd lng="hu">k&ouml;rmondat</trd>
  <trd lng="nl">periode</trd>
  <trd lng="pl">okres</trd>
  <trd lng="pt">per&iacute;odo</trd>
  <trd lng="ro">perioad&abreve;</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_i;&c_o;&c_d;</trd>
  <trdgrp lng="sk">
    <trd>fáza</trd>,
    <trd>obdobie</trd>,
    <trd>časový úsek</trd>
  </trdgrp>
</drv>
 
<drv mrk="period.0a">
  <kap><tld/>a</kap>
  <snc mrk="period.0a.tempe">
    <dif>
      Ripeti&gcirc;anta post pli malpli difinita tempo:
      <ekz>
        <tld/>a febro, eldona&jcirc;o, fenomeno.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="interm.0a">intermita</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>periodikus</trd>,
      <trd>id&odblac;szakos</trd>
    </trdgrp>
  </snc>
  <snc mrk="period.0a.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="period.0a.funkcio">
      <fnt><bib>MatVort</bib></fnt>
      <dif>
        <klr>(p.p. reela <ref
        cel="funkci.0o.MAT">funkcio</ref>)</klr>
        Akceptanta almena&ubreve; unu
        <ref cel="period.0o.funkcio"><tld/>on <sncref/></ref>.
      </dif>
      <trd lng="de">periodisch<klr tip="ind"> (-e
      Funktion)</klr></trd>
      <trd lng="en">periodic[al]<klr tip="ind">
      (function)</klr></trd>
      <trd lng="fr"><klr tip="ind">(fonction) 
          </klr>p&eacute;riodique</trd>
      <trd lng="hu">periodikus<klr tip="ind"> 
         (f&uuml;ggv&eacute;ny)</klr></trd>
      <trd lng="pl"><klr tip="amb">(funkcja) </klr>okresowa</trd>
      <trd lng="ru">&c_p;&c_je;&c_r;&c_i;&c_o;&c_d;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja;<klr 
         tip="amb"> (&c_f;&c_u;&c_n;&c_k;&c_c;&c_i;&c_ja;)</klr></trd>
    </subsnc>
    <subsnc mrk="period.0a.frakcio">
      <fnt><bib>MatTerm</bib><lok>p. 9</lok></fnt>
      <ref tip="vid" cel="frakci.pozicia0o">pozicia frakcio.</ref>
    </subsnc>
  </snc>
  <snc mrk="period.0a.BELE">
    <dif>
      Karakterizata per <tld/>oj<sncref ref="period.0o.BELE"/>:
      <ekz>
        <tld/>a stilo.
      </ekz>
    </dif>
    <trd lng="hu">k&ouml;rmondatos</trd>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">peri&ograve;dic<klr> (adj.)</klr></trd>
  <trd lng="de">periodisch</trd>
  <trd lng="fr">p&eacute;riodique<klr> (adj.)</klr></trd>
  <trd lng="nl">periodiek</trd>
  <trd lng="pl">okresowy</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_i;&c_o;&c_d;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>občasný</trd>,
    <trd>periodický</trd>
  </trdgrp>
</drv>
 
<drv mrk="period.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="period.0ajxo.TIP">
    <uzo tip="fak">TIP</uzo>
    <dif>
      Publikiga&jcirc;o, kies sinsekvaj partoj, numeritaj kaj
      datitaj, <tld/>e aperas por ne anta&ubreve;difinita tempo
      kaj ofte kun fiksaj rubrikoj
      verkitaj de diversaj a&ubreve;toroj:
    </dif>
    <ekz>
      la nomon de la <tld/>a&jcirc;o kutime sekvas la numero de la
      jar-volumo
      <fnt><bib>Dan&gcirc;Lng</bib>, <lok>Atentigo de la 2a
      eldono</lok></fnt>.
    </ekz>
    <ref tip="super" cel="seri.0ajho">seria&jcirc;o</ref>
    <refgrp tip="sub">
      <ref cel="bulten.0o">bulteno</ref>,
      <ref cel="gazet.0o">gazeto</ref>,
      <ref cel="leter.inform0o">informletero</ref>,
      <ref cel="libr.jar0o">jarlibro</ref>,
      <ref cel="jxurna.0o">&jcirc;urnalo</ref>,
      <ref cel="magazin.0o">magazino</ref>,
      <ref cel="leter.novajx0o">nova&jcirc;letero</ref>,
      <ref cel="revu.0o">revuo</ref>
    </refgrp>
    <refgrp tip="prt">
      <ref cel="kolekt.jar0o">jarkolekto</ref>,
      <ref cel="kajer.0o">kajero</ref>,
      <ref cel="numer.0o">numero</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_r;&c_y;&c_ja;&c_d;&c_y;&c_ch;&c_n;&c_a;&c_je; &c_v;&c_y;&c_d;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trd lng="ca">peri&ograve;dic<klr> (subst.)</klr></trd>
  <trd lng="de">Periodikum</trd>
  <trd lng="en">periodical</trd>
  <trd lng="fr">p&eacute;riodique<klr> (publication)</klr></trd>
  <trdgrp lng="hu">
    <trd>foly&oacute;irat</trd>,
    <trd>periodika</trd></trdgrp>
  <trd lng="nl">tijdschrift</trd>
  <trd lng="pl">periodyk</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_i;&c_o;&c_d;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_i;&c_z;&c_d;&c_a;&c_n;&c_i;&c_je;</trd>
</drv>
 
<drv mrk="period.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="period.0igi.HIS">
    <uzo tip="fak">HIS</uzo>
    <uzo tip="fak">GEOL</uzo>
    <dif>
      Dividi tempon la&ubreve; <tld/>oj:
      <ekz>
        niaj eseistoj verkas <klr>...</klr> pri <tld/>igo 
        de nia beletro 
        <fnt>
          <aut>A. Val&eacute;n</aut>,
          <vrk><url
          ref="http://www.liberafolio.org/2013/agrableta-surprizo-post-timo-de-desaponto">
          Agrableta surprizo post timo de desaponto </url></vrk>,
          <lok>2013-02-09</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">p&eacute;riodiser</trd>
  <trd lng="sk">časovo rozložiť</trd>
</drv>

<drv mrk="period.0atabelo">
  <kap><tld lit="P"/>a tabelo</kap>
  <snc>
    <uzo tip="fak">KEM</uzo>
    <dif>
      La klasifika sistemo de &hcirc;emiaj
      <ref cel="elemen.0o.KEM">elementoj<sncref/></ref>,
      kreita de Demetrio <ref cel="mendelev.M0o">Mendelevo</ref>,
      prezentanta la &leftquot;<tld lit="P"/>an le&gcirc;on&rightquot; de li
      malkovritan.
    </dif>
  </snc>
  <trd lng="be">&c_P;&c_e;&c_r;&c_y;&c_ja;&c_d;&c_y;&c_ch;&c_n;&c_a;&c_ja; &c_t;&c_a;&c_b;&c_l;&c_ib;&c_c;&c_a;
  &c_e;&c_l;&c_je;&c_m;&c_e;&c_n;&c_t;&c_a;&c_w;</trd>
  <trd lng="ca">taula <ind>peri&ograve;dic</ind>a</trd>
  <trd lng="de">Periodensystem</trd>
  <trd lng="en">Periodic Table</trd>
  <trd lng="hu">peri&oacute;dusos rendszer</trd>
  <trdgrp lng="nl">
    <trd>periodiek systeem der elementen</trd>,
    <trd>tabel van Mendeleev</trd></trdgrp>
  <trd lng="pl">Uk&lstroke;ad okresowy pierwiastk&oacute;w</trd>
  <trd lng="ru">&c_P;&c_je;&c_r;&c_i;&c_o;&c_d;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; &c_t;&c_a;&c_b;&c_l;&c_i;&c_c;&c_a;
  &c_e;&c_l;&c_je;&c_m;&c_je;&c_n;&c_t;&c_o;&c_v;</trd>
  <trd lng="sk">periodická sústava</trd>
</drv>
 
</art>
