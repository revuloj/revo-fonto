<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vang.xml,v 1.35 2021/05/29 10:57:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vang</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vang.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vang.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Flanka parto de la <ref tip="malprt" cel="vizagx.0o.ANA">viza&gcirc;o</ref>:
      <ekz>
        sulkplenaj <tld/>oj;
      </ekz>
      <ekz>
        propran <tld/>on neniu batas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        plen<tld/>a<fnt>B</fnt>,
      </ekz>
      <ekz>
        <ind>puf<tld/>a</ind><fnt>Z</fnt>
        <trd lng="hu">puf&oacute;k</trd>
      </ekz>
      <ekz>
        infano;
      </ekz>
      <ekz>
        <tld/>haroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_a;&c_k;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_b;&c_u;&c_z;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF60;&#xF42;&#xFB2;&#xF58;&#xF0B;&#xF54;&#xF0B;</trd>
  <trdgrp lng="br">
    <trd>boc'h</trd>,
    <trd>jod</trd>
  </trdgrp>
  <trd lng="ca">galta</trd>
  <trdgrp lng="cs">
    <trd>bo&ccaron;n&iacute; &ccaron;&aacute;st n&aacute;stroje</trd>,
    <trd>tv&aacute;&rcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wange</trd>,
    <trd>Backe</trd>
  </trdgrp>
  <trd lng="el">&my;&alfa_ton;&gamma;&omikron;&ypsilon;&lambda;&omikron;</trd>
  <trdgrp lng="en">
    <trd>cheek</trd>,
    <trd>jowl</trd>
  </trdgrp>
  <trd lng="es">mejilla</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_w;&a_n;&a_h;<klr tip="ind">
    (&a_S;&a_w;&a_r;&a_t;)</klr></trd>,
    <trd>&a_l;&a_damma;&f_p;</trd>
  </trdgrp>
  <trd lng="fr">joue</trd>
  <trd lng="he">&lamed;&het;&yod;</trd>
  <trd lng="hr">obraz</trd>
  <trdgrp lng="hu">
    <trd>arc</trd>,
    <trd>orca</trd>
  </trdgrp>
  <trd lng="id">pipi</trd>
  <trdgrp lng="it">
    <trd>guancia</trd>, <trd>gota</trd>
  </trdgrp>
  <trd lng="nl">wang</trd>
  <trd lng="pl">policzek</trd>
  <trd lng="pt">bochecha</trd>
  <trd lng="ru">&c_shch;&c_je;&c_k;&c_a;</trd>
  <trd lng="sk">bo&ccaron;n&aacute; &ccaron;as&tcaron; n&aacute;stroja</trd>
  <trd lng="sv">kind</trd>
</drv>

<drv mrk="vang.sur0i">
  <kap>sur<tld/>i
  <var><kap><tld/>ofrapi</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vang.sur0i.KOMUNE">
    <dif>
      Frapi sur la vangon:
      <ekz>
        li vidis, ke &scirc;i levas la brakon por sur<tld/>i la 
        junulinon
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>Kastelo de Prelongo</vrk>,
          <lok>1907</lok>
        </fnt>;
      </ekz>
      <ekz>
        la avino <tld/>ofrapis lin kaj enliti&gcirc;is sola
        <fnt>
          <aut>A. Pu&scirc;kin, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="http://www.esperanto-ondo.ru/Libroj/V_bibl01.pdf">
          La pika damo</url></vrk>,
          <lok>2003-2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_u;&c_d;&c_r;&c_ja;&c_m; &c_p;&c_l;&c_je;&c_s;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="es">abofetear</trd>
  <trd lng="fr">gifler</trd>
</drv>

<drv mrk="vang.sur0o">
  <kap>sur<tld/>o <var><kap><tld/>obato</kap></var> 
  <var><kap><tld/>ofrapo</kap></var></kap>
  <snc mrk="vang.sur0o.KOMUNE">
    <dif>
      Bato per la mano sur la <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_w;&c_h;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_l;&c_ja;&c_v;&c_u;&c_h;&c_a;</trd>
  </trdgrp>  
  <trd lng="bg">&c_p;&c_l;&c_je;&c_s;&c_n;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="br">
    <trd>flac'had</trd>,
    <trd>jodad</trd>,
    <trd>skouarnad</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>galtada</trd>,
    <trd>bufetada</trd>
  </trdgrp>
  <trd lng="de">Ohrfeige</trd>
  <trd lng="en">slap on the face</trd>
  <trd lng="es">bofetada</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&f_y;&a_l;&f_y;</trd>,
    <trd>&f_ch;&a_fatha;&f_k;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gifle</trd>,
    <trd>soufflet<klr> (gifle)</klr></trd>,
    <trd>torgnole</trd>
  </trdgrp>
  <trd lng="he">&samekh;&tet;&yod;&resh;&tav; &lamed;&het;&yod;</trd>
  <trdgrp lng="hu">
    <trd>pofon</trd>,
    <trd>arculcsap&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>ceffone</trd>, 
    <trd>sberla</trd>,
    <trd>schiaffo<klr> (sul viso)</klr></trd>
  </trdgrp>
  <trd lng="nl">oorveeg</trd>
  <trd lng="pl">policzek</trd>
  <trd lng="pt">bofetada</trd>
  <trd lng="ru">&c_p;&c_o;&c_shch;&c_jo;&c_ch;&c_i;&c_n;&c_a;</trd>
  <trd lng="sv">&ouml;rfil</trd>
</drv>

<drv mrk="vang.post0o">
  <kap>post<tld/>o</kap>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc mrk="vang.post0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <ref tip="dif" cel="vang.sid0o">sid<tld/>o</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_g;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_je;&c_d;&c_a;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>feskenn</trd>,
    <trd>pe&ntilde;s</trd>
  </trdgrp>
  <trd lng="ca">natja</trd>
  <trd lng="el">&gamma;&lambda;&omikron;&ypsilon;&tau;&omikron_ton;&sigma_fina;</trd>
  <trd lng="es">nalga</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_sh;&f_y;&a_m;&a_n;&f_g;&a_A;&a_h;</trd>,
    <trd>&f_k;&a_f;&a_l;</trd>,
    <trd>&f_k;&f_p;&a_l;</trd>,
    <trd>&a_s;&a_damma;&a_r;&f_y;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">fesse</trd>
  <trd lng="he">&ayin;&kaf;&vav;&zayin;</trd>
  <trdgrp lng="hu">
    <trd>farpofa</trd>,
    <trd>f&eacute;l <ind>fen&eacute;k</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>chiappa</trd>, <trd>natica</trd>
  </trdgrp>
  <trd lng="nl">bil</trd>
  <trd lng="pl">po&sacute;ladek</trd>
  <trd lng="pt">n&aacute;dega</trd>
  <trd lng="ru">&c_ja;&c_g;&c_o;&c_d;&c_i;&c_c;&c_a;</trd>
  <trd lng="sk">zadok <klr>(sedacia &ccaron;as&tcaron;)</klr></trd>
  <trd lng="sv">skinka<klr> (kroppsdel)</klr></trd>
</drv>

<drv mrk="vang.puf0eto">
  <kap>puf<tld/>eto</kap>
  <snc mrk="vang.puf0eto.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Pli dika, iom elstara parto de <tld/>o sub la okulo:
      <ekz>
        &scirc;iaj blondaj haroj <klr>...</klr> tremetadis super la 
        sunbrunigitaj puf<tld/>etoj kaj la grandaj okuloj 
        <fnt>
        <aut>Ivan G. &Scirc;irjaev</aut>,
        <vrk>Sen titolo</vrk>,
        <lok>&ccirc;apitro 1a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">p&ograve;mul</trd>
  <trd lng="es">p&oacute;mulo</trd>
  <trd lng="fr">pommette</trd>
  <trd lng="it">zigomo</trd>
</drv>

<drv mrk="vang.sid0o">
  <kap>sid<tld/>o</kap>
  <snc mrk="vang.sid0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      &Ccirc;iu el la du muskolaj masoj, sur kiuj homo sidas;
      simila korpoparto de iuj bestoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="glute.0o">gluteo</ref>;
    </refgrp>
    <ref tip="malprt" cel="pug.0o">pugo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_g;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_je;&c_d;&c_a;&c_w;&c_k;&c_a;</trd>
  </trdgrp>           
  <trd lng="bg">&c_b;&c_u;&c_z;&c_a; &c_n;&c_a; &c_d;&c_u;&c_p;&c_je;&c_t;&c_o;</trd>
  <trdgrp lng="br">
    <trd>feskenn</trd>,
    <trd>pe&ntilde;s</trd>
  </trdgrp>
  <trd lng="ca">natja</trd>
  <trd lng="el">&gamma;&lambda;&omikron;&ypsilon;&tau;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">buttocks</trd>
  <trd lng="es">nalga</trd>
  <trdgrp lng="fa">
    <trd>&a_n;&a_sh;&f_y;&a_m;&a_n;&f_g;&a_A;&a_h;</trd>,
    <trd>&f_k;&a_f;&a_l;</trd>,
    <trd>&f_k;&f_p;&a_l;</trd>,
    <trd>&a_s;&a_damma;&a_r;&f_y;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">fesse</trd>
  <trd lng="he">&ayin;&kaf;&vav;&zayin;</trd>
  <trdgrp lng="hu">
    <trd>farpofa</trd>,
    <trd>f&eacute;l <ind>fen&eacute;k</ind></trd>
  </trdgrp>    
  <trdgrp lng="it">
    <trd>chiappa</trd>, <trd>natica</trd>
  </trdgrp>
  <trd lng="nl">bil</trd>
  <trd lng="pl">po&sacute;ladek</trd>
  <trd lng="pt">n&aacute;dega</trd>
  <trd lng="ru">&c_ja;&c_g;&c_o;&c_d;&c_i;&c_c;&c_a;</trd>
  <trd lng="sv">skinka<klr> (kroppsdel)</klr></trd>
</drv>
</art>
