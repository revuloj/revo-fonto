<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vang.xml,v 1.43 2024/03/01 09:26:55 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vang</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vang.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="vang.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Flanka parto de la <ref tip="malprt" cel="vizagx.0o.ANA">viza&gcirc;o</ref>:
      <ekz>
        <nom>Ella</nom> parolis iom kolere, &scirc;iaj <tld/>oj ru&gcirc;i&gcirc;is
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        sur &scirc;iaj flavi&gcirc;intaj, velkintaj, kavi&gcirc;intaj vangoj aperis ru&gcirc;aj makuloj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        nejuna homo <klr>[&#8230;]</klr> kun granda viza&gcirc;o distingi&gcirc;anta per lar&gcirc;aj kaj
        &scirc;velformaj <tld/>oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i kisis liajn manojn kaj <tld/>ojn
        <fnt>
          <bib>IK</bib>
        </fnt>;
      </ekz>
      <ekz>
        vintro sin trovos sur <tld/>oj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        konstanta &scirc;vito lavadis niajn <tld/>ojn
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvara</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> pin&ccirc;is al &scirc;i la <tld/>on
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        bur&gcirc;o avidanta bongustan hejman kuira&jcirc;on kaj dekduon da <ind>puf<tld/>a</ind>j infana&ccirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
        <trd lng="hu">puf&oacute;k</trd>
      </ekz>
      <ekz>
        plen<tld/>a junulo kun senkolora lanugo sur la mentono
        <fnt>
          <bib>PatrojFiloj</bib>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> havis sufi&ccirc;e dik<tld/>an kaj afablan viza&gcirc;on
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        ru&gcirc;<tld/>a pomo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;i estis juna, roz<tld/>a kaj bele vestita
        <fnt>
          <bib>InfanTorent2</bib>
        </fnt>;
      </ekz>
      <ekz>
        oni kra&ccirc;is sur lian viza&gcirc;on kaj <tld/>ofrapis lin, kaj iuj frapis lin per la
        manplatoj
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 26:67</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> insulte batas min sur la <tld/>ojn
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 16:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        fari apla&ubreve;don sur la <tld/>on
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        propran <tld/>on neniu batas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <ind>ne ka&scirc;i&gcirc;as lia lango malanta&ubreve; la <tld/>o</ind>
        <fnt>
          <bib>PrV</bib>
        </fnt>.
        <trd lng="de">er h&auml;lt mit seiner Meinung nicht hinterm <ind>Berg</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_a;&c_k;&c_a;</trd>
  <trdgrp lng="bg">
    <trd>&c_b;&c_u;&c_z;&c_a;</trd>,
    <trd>&c_s;&c_t;&c_r;&c_a;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_-a;&t_ga;&t_rata;&t_ma;&t_tsheg;&t_pa;&t_tsheg;</trd>
  <trdgrp lng="br">
    <trd>boc'h</trd>,
    <trd>jod</trd>
  </trdgrp>
  <trd lng="ca">galta</trd>
  <trdgrp lng="cs">
    <trd>bo&ccaron;n&iacute; &ccaron;&aacute;st n&aacute;stroje</trd>,
    <trd>tv&aacute;&rcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Wange</trd>,
    <trd>Backe</trd>
  </trdgrp>
  <trd lng="el">&my;&alfa_ton;&gamma;&omikron;&ypsilon;&lambda;&omikron;</trd>
  <trdgrp lng="en">
    <trd>cheek</trd>,
    <trd>jowl</trd>
  </trdgrp>
  <trd lng="es">mejilla</trd>
  <trdgrp lng="fa">
    <trd>&f_g;&a_w;&a_n;&a_h;<klr tip="ind">
    (&a_S;&a_w;&a_r;&a_t;)</klr></trd>,
    <trd>&a_l;&a_damma;&f_p;</trd>
  </trdgrp>
  <trd lng="fr">joue</trd>
  <trd lng="he">&lamed;&het;&yod;</trd>
  <trd lng="hr">obraz</trd>
  <trdgrp lng="hu">
    <trd>arc</trd>,
    <trd>orca</trd>
  </trdgrp>
  <trd lng="id">pipi</trd>
  <trdgrp lng="it">
    <trd>guancia</trd>, <trd>gota</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x307B;&#x304A;</trd>,
    <trd>&#20596;&#x677F;  <pr>&#x304C;&#x308F;&#12356;&#x305F;</pr></trd>
  </trdgrp>
  <trd lng="nl">wang</trd>
  <trd lng="pl">policzek</trd>
  <trd lng="pt">bochecha</trd>
  <trd lng="ru">&c_shch;&c_je;&c_k;&c_a;</trd>
  <trd lng="sk">bo&ccaron;n&aacute; &ccaron;as&tcaron; n&aacute;stroja</trd>
  <trd lng="sv">kind</trd>
  <trd lng="uk">&c_shch;&#x43E;&#x43A;&c_a;</trd>
</drv>

<drv mrk="vang.sur0i">
  <kap>sur<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vang.sur0i.KOMUNE">
    <dif>
      Frapi sur la <tld/>on; <tld/>ofrapi:
      <ekz>
        li vidis, ke &scirc;i levas la brakon por sur<tld/>i la
        junulinon
        <fnt>
          <aut>H. Vallienne</aut>,
          <vrk>Kastelo de Prelongo</vrk>,
          <lok>1907</lok>
        </fnt>;
      </ekz>
      <ekz>
        la avino <tld/>ofrapis lin kaj enliti&gcirc;is sola
        <fnt>
          <aut>A. Pu&scirc;kin, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;Libroj/V_bibl01.pdf">
          La pika damo</url></vrk>,
          <lok>2003-2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_p;&c_o;&c_w;&c_h;&c_u;</ind></trd>,
    <trd>&c_a;&c_p;&c_l;&c_ja;&c_v;&c_u;&c_sh;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_v;&c_a;&c_c;&c_mol; <ind>&c_p;&c_l;&c_ju;&c_h;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="bg">&c_u;&c_d;&c_r;&c_ja;&c_m; &c_p;&c_l;&c_je;&c_s;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="es">abofetear</trd>
  <trd lng="fr">gifler</trd>
</drv>

<drv mrk="vang.sur0o">
  <kap>sur<tld/>o</kap>
  <snc mrk="vang.sur0o.KOMUNE">
    <dif>
      <ref tip="sin" cel="frap.vango0o"><tld lit="V"/>ofrapo</ref>,
      <ref tip="sin" cel="bat.vango0o"><tld/>obato</ref>:
      <ekz>
        la sinjoro kredeble ekkoleris pro tiu &ccirc;i respondo kaj diris al mi, ke li donos
        al mi sur<tld/>on
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la malfeli&ccirc;a infanino <klr>[&#8230;]</klr> senresponde suferis la sur<tld/>ojn de siaj
        gepatroj
        <fnt>
          <bib>KPr</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam mi <klr>[&#8230;]</klr> demandis mian patron pri tiu temo, li respondis per
        sur<tld/>o
        <fnt>
          <bib>ChL</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_w;&c_h;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_l;&c_ja;&c_v;&c_u;&c_h;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_l;&c_je;&c_s;&c_n;&c_i;&c_c;&c_a;</trd>
  <trdgrp lng="br">
    <trd>flac'had</trd>,
    <trd>jodad</trd>,
    <trd>skouarnad</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>galtada</trd>,
    <trd>bufetada</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Ohrfeige</trd>,
    <trd>Backpfeife</trd>
  </trdgrp>
  <trd lng="en">slap on the face</trd>
  <trd lng="es">bofetada</trd>
  <trdgrp lng="fa">
    <trd>&a_s;&f_y;&a_l;&f_y;</trd>,
    <trd>&f_ch;&a_fatha;&f_k;</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>gifle</trd>,
    <trd>soufflet<klr> (gifle)</klr></trd>,
    <trd>torgnole</trd>
  </trdgrp>
  <trd lng="he">&samekh;&tet;&yod;&resh;&tav; &lamed;&het;&yod;</trd>
  <trdgrp lng="hu">
    <trd>pofon</trd>,
    <trd>arculcsap&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>ceffone</trd>,
    <trd>sberla</trd>,
    <trd>schiaffo<klr> (sul viso)</klr></trd>
  </trdgrp>
  <trd lng="nl">oorveeg</trd>
  <trd lng="pl">policzek</trd>
  <trd lng="pt">bofetada</trd>
  <trd lng="ru">&c_p;&c_o;&c_shch;&c_jo;&c_ch;&c_i;&c_n;&c_a;</trd>
  <trd lng="sv">&ouml;rfil</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x44F;&#x43F;&c_a;&c_s;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&c_i;&c_ch;&#x43D;&c_i;&#x43A;</trd>
  </trdgrp>
</drv>

