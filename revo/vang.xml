<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>vang</rad>/o
</kap>
<drv mrk="vang.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Flanka parto de la viza&gcirc;o:
      <ekz>
        sulkplenaj <tld/>oj;
      </ekz>
      <ekz>
        propran <tld/>on neniu batas
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        plen<tld/>a<fnt>B</fnt>,
      </ekz>
      <ekz>
        <ind>puf<tld/>a</ind><fnt>Z</fnt>
        <trd lng="hu">puf&oacute;k</trd>
      </ekz>
      <ekz>
        infano;
      </ekz>
      <ekz>
        <tld/>haroj.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_ch;&c_a;&c_k;&c_a;</trd>
  <trd lng="cs">tv&aacute;&rcaron;</trd>
  <trdgrp lng="de">
    <trd>Wange</trd>,
    <trd>Backe</trd>
  </trdgrp>
  <trd lng="el">&my;&alfa_ton;&gamma;&omikron;&ypsilon;&lambda;&omikron;</trd>
  <trd lng="es">mejilla</trd>
  <trd lng="fr">joue</trd>
  <trd lng="he">&lamed;&het;&yod;</trd>
  <trd lng="hr">obraz</trd>
  <trdgrp lng="hu">
    <trd>arc</trd>,
    <trd>orca</trd>
  </trdgrp>
  <trd lng="nl">wang</trd>
  <trd lng="pl">policzek</trd>
  <trd lng="pt">bochecha</trd>
  <trd lng="ru">&c_shch;&c_je;&c_k;&c_a;</trd>
</drv>

<drv mrk="vang.0obato0ofraposur0o">
  <kap><tld/>obato, <tld/>ofrapo, sur<tld/>o</kap>
  <snc>
    <dif>
      Bato per la mano sur la <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_o;&c_w;&c_h;&c_a;</trd>,
    <trd>&c_a;&c_p;&c_l;&c_ja;&c_v;&c_u;&c_h;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Ohrfeige</trd>
  <trd lng="es">bofetada</trd>
  <trdgrp lng="fr">
    <trd>gifle</trd>,
    <trd>soufflet</trd>,
    <trd>torgnole</trd>
  </trdgrp>
  <trd lng="he">&samekh;&tet;&yod;&resh;&tav; &lamed;&het;&yod;</trd>
  <trdgrp lng="hu">
    <trd>pofon</trd>,
    <trd>arculcsap&aacute;s</trd>
  </trdgrp>
  <trd lng="nl">oorveeg</trd>
  <trd lng="pl">policzek</trd>
  <trd lng="pt">bofetada</trd>
  <trd lng="ru">&c_p;&c_o;&c_shch;&c_jo;&c_ch;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="vang.post0o">
  <kap>post<tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>PV</bib></fnt>
  <snc>
    <ref tip="dif" cel="vang.sid0o">sid<tld/>o</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_g;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_je;&c_d;&c_a;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="el">&gamma;&lambda;&omikron;&ypsilon;&tau;&omikron_ton;&sigma_fina;</trd>
  <trd lng="es">nalga</trd>
  <trd lng="fr">fesse</trd>
  <trd lng="he">&ayin;&kaf;&vav;&zayin;</trd>
  <trdgrp lng="hu">
    <trd>farpofa</trd>,
    <trd>f&eacute;l <ind>fen&eacute;k</ind></trd>
  </trdgrp>
  <trd lng="nl">bil</trd>
  <trd lng="pl">po&sacute;ladek</trd>
  <trd lng="pt">n&aacute;dega</trd>
  <trd lng="ru">&c_ja;&c_g;&c_o;&c_d;&c_i;&c_c;&c_a;</trd>
</drv>

<drv mrk="vang.sid0o">
  <kap>sid<tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      &Ccirc;iu el la du muskolaj masoj, sur kiuj homo sidas;
      analoga korpoparto de iuj bestoj.
    </dif>
    <refgrp tip="sin">
      <ref cel="glute.0o">gluteo</ref>;
    </refgrp>
    <ref tip="malprt" cel="pug.0o">pugo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ja;&c_g;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_je;&c_d;&c_a;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="el">&gamma;&lambda;&omikron;&ypsilon;&tau;&omikron_ton;&sigma_fina;</trd>
  <trd lng="es">nalga</trd>
  <trd lng="fr">fesse</trd>
  <trd lng="he">&ayin;&kaf;&vav;&zayin;</trd>
  <trdgrp lng="hu">
    <trd>farpofa</trd>,
    <trd>f&eacute;l <ind>fen&eacute;k</ind></trd>
  </trdgrp>    
  <trd lng="nl">bil</trd>
  <trd lng="pl">po&sacute;ladek</trd>
  <trd lng="pt">n&aacute;dega</trd>
  <trd lng="ru">&c_ja;&c_g;&c_o;&c_d;&c_i;&c_c;&c_a;</trd>
</drv>
</art>
