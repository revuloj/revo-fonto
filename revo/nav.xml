<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>nav</rad>/o
</kap>
<drv mrk="nav.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Parto de pre&gcirc;ejo etendi&gcirc;anta de pordego &gcirc;is &hcirc;orejo:
      <ekz>
        &ccirc;efa <tld/>o, flankaj <tld/>oj.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="transe.0o">transepto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Schiff</trd>,
     <trd>Kirchenschiff</trd>
  </trdgrp>
  <trd lng="en">nave</trd>
  <trd lng="es">nave</trd>
  <trd lng="hu">haj&oacute;</trd>
  <trd lng="nl">schip<klr tip="ind"> (v.kerk)</klr></trd>
  <trd lng="pt">nave</trd>
  <trd lng="ru">&c_n;&c_je;&c_f;</trd>
  <trd lng="sv">skepp<klr tip="ind"> (i kyrka)</klr></trd>
</drv>
</art>
