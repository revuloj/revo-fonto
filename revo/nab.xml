<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
   <art mrk="$Id$">
     <kap>
       <rad>nab</rad>/o
     </kap>
     <drv mrk="nab.0o">
       <kap><tld/>o</kap>
       <snc mrk="nab.0o.TEK">
         <uzo tip="fak">TEK</uzo>
         <dif>
           Centra parto de rado, en kiu oni enigas la radie dislokitajn
           stangojn kaj en kies mezon oni metas la akson, 
&ccirc;irka&ubreve;
kiu
           okazas la rotacio de la rado.
         </dif>
         <ref tip="super" cel="aks.0ingo">aksingo</ref>
         <trd lng="de">Nabe</trd>
         <trd lng="fr">moyeu</trd>
         <trd lng="hu">ker&eacute;kagy</trd>
         <trd lng="nl">naaf</trd>
        <trdgrp lng="ru">
           <trd>&c_s;&c_t;&c_u;&c_p;&c_i;&c_c;&c_a; <klr>(&c_v;
&c_k;&c_o;&c_l;&c_je;&c_s;&c_je;)</klr></trd>,
           <trd>&c_v;&c_t;&c_u;&c_l;&c_k;&c_a;</trd></trdgrp>
       </snc>
       <snc mrk="nab.0o.KOMP">
         <uzo tip="fak">KOMP</uzo>
         <dif>
           Aran&gcirc;a&jcirc;o interliganta plurajn aliajn
aran&gcirc;a&jcirc;ojn:
           <ekz>
             &ccirc;iuj komputiloj en oficeja &ccirc;ambro estas konektitaj
al la
             loka reto per <tld/>o fiksita sur la muro en la
             kontra&ubreve;a angulo.</ekz>
         </dif>
         <trd lng="de">Hub</trd>
         <trd lng="nl">hub</trd>
        <trdgrp lng="ru">

<trd>&c_k;&c_o;&c_n;&c_c;&c_je;&c_n;&c_t;&c_r;&c_a;&c_t;&c_o;&c_r;</trd>,
           <trd>&c_h;&c_a;&c_b;</trd></trdgrp>
       </snc>
       <trd lng="en">hub</trd>
       <trd lng="hu">hub</trd>
     </drv>
   </art>
