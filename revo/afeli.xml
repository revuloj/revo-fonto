<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: afeli.xml,v 1.23 2023/10/12 16:57:42 revo Exp $">
<kap>
  <rad>afeli</rad>/o
</kap>
<drv mrk="afeli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="punkt.0o.spaco">Punkto</ref>
      de la <ref tip="malprt" cel="orbit.0o.AST">orbito</ref>
      de sun-&ccirc;irka&ubreve;anta objekto,
      &ccirc;e kiu tiu estas plej malproksima de la suno:
      <ekz>
        la orbito de la Tero estas elipsa, la finpunktoj, nomitaj apsidoj, estas la
        perihelio, kie la Tero estas plej proksima al la suno, kaj la <tld/>o, kie la
        Tero estas <klr>[plej malproksima]</klr> de la suno
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=2025">Jaro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="perihe.0o">perihelio</ref>
    </refgrp>
    <refgrp tip="super">
      <ref cel="apsid.0o">apsido</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_f;&c_e;&c_l;&c_ib;&c_j;</trd>
  <trd lng="ca">afeli</trd>
  <trdgrp lng="cs">
    <trd>afel</trd>,
    <trd>af&eacute;lium</trd>,
    <trd>odslun&iacute;</trd>
  </trdgrp>
  <trd lng="de">Aphel</trd>
  <trd lng="en">aphelion</trd>
  <trd lng="es">afelio</trd>
  <trd lng="fr">aph&eacute;lie</trd>
  <trdgrp lng="hu">
    <trd>napt&aacute;vol</trd>,
    <trd>af&eacute;lium</trd>
  </trdgrp>
  <trd lng="it">afelio</trd>
  <trd lng="ja">&#x9060;&#x65E5;&#x70B9;  <pr>&#x3048;&#x3093;&#x3058;&#x3064;&#x3066;&#x3093;</pr></trd>
  <trd lng="pl">afelium</trd>
  <trd lng="pt">af&eacute;lio</trd>
  <trd lng="ru">&c_a;&c_f;&c_je;&c_l;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>af&eacute;lium</trd>,
    <trd>odslnie</trd>
  </trdgrp>
  <trd lng="sv">aphelium</trd>
  <trd lng="uk">&#x430;&#x444;&#x435;&#x43B;&#x456;&#x439;</trd>
</drv>
</art>
