<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>afeli</rad>/o
</kap>
<drv mrk="afeli.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Punkto de la orbito de 
      sun-&ccirc;irka&ubreve;anta objekto,
      &ccirc;e kiu tiu estas plej malproksima de la suno.
    </dif>
    <refgrp tip="vid">
      <ref cel="perihe.0o">perihelio</ref>
    </refgrp>
    <refgrp tip="super">
      <ref cel="apsid.0o">apsido</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_f;&c_e;&c_l;&c_ib;&c_j;</trd>
  <trd lng="de">Aphel</trd>
  <trd lng="en">aphelion</trd>
  <trd lng="es">afelio</trd>
  <trd lng="fr">aph&eacute;lie</trd>
  <trdgrp lng="hu">
    <trd>napt&aacute;vol</trd>,
    <trd>af&eacute;lium</trd>
  </trdgrp>
  <trd lng="pt">af&eacute;lio</trd>
  <trd lng="ru">&c_a;&c_f;&c_je;&c_l;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>af&eacute;lium</trd>,
    <trd>odslnie</trd>
  </trdgrp>
</drv>
</art>
