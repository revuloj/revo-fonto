    <trd>Offizierssch&uuml;ler</trd>
  </trdgrp>
  <trd lng="en">cadet</trd>
  <trd lng="es">cadete</trd>
  <trd lng="fr">cadet<klr tip="ind"> (&eacute;l&egrave;ve officier)</klr></trd>
  <trd lng="hu">kad&eacute;t</trd>
  <trd lng="nl">kadet</trd>
  <trd lng="ru">&c_k;&c_a;&c_d;&c_je;&c_t;</trd>
  <trd lng="sv">kadett</trd>
</drv>
</art>
