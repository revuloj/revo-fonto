<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: seps.xml,v 1.24 2024/03/24 20:00:01 revo Exp $">
<kap>
  <rad>seps</rad>/o <fnt><bib>PV</bib></fnt>,
  <var><kap><rad var="emi">sepsemi</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
</kap>
<drv mrk="seps.0o">
  <kap><tld/>o, <var><kap><tld var="emi"/>o</kap></var></kap>
  <snc mrk="seps.0o.inflamo">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kompleksa sistema inflamreago de organismo pro infekti&gcirc;o per
      bakterioj, ties venenoj a&ubreve; fungoj:
      <ekz>
        la plej seriozaj kazoj de <tld/>o inkluzivas la disigitan
        <tld/>on (<tld/>on kun akuta organo-malfunkcio) kaj <tld/>an
        &scirc;okon (<tld/>on kun refraktiva arteria hipotensio)
        <fnt><bib>Viki</bib>, <vrk><url ref="&Viki;/Sepso">Sepso</url></vrk></fnt>
      </ekz>;
      <ekz>
        lia edzino mizere mortis pro <tld/>o, kune kun la feto
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2013/011081.php">Astrobiologio kaj
          religio</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
<!-- kreu artikolon antiseps/
      <ekz>
        fondita en 1985, &gcirc;i limigas sin al la produktado de perbu&scirc;aj kuraciloj, kiel
        antibiotikoj, anti<tld/>a&jcirc;oj kaj kontra&ubreve;inflamaj medikamentoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2011/010247.php"
              >Kipro eksportas, &Ccirc;inio importas</url></vrk>
          <lok>2011</lok>
        </fnt>;
      </ekz>
-->
      <ekz>
        oni uzas antibiotikojn por malhelpi <tld var="emi"/>on en la sango
        <fnt>
          <aut>M. Sujet</aut>,
          <vrk><url ref="https://groups.yahoo.com/neo/groups/per-esperanto-politiko/conversations/messages/339">
          karbobacilo kaj televidado</url></vrk>,
          <lok>esperanto-politiko, 2001-11-06</lok>
        </fnt>;
      </ekz>
      <ekz>
        kriterioj pri diagnozo de brul-<tld var="emi"/>o
        <fnt>
          <bib>CRI</bib>,
          <vrk><url ref="&CRI;681/2012/12/11/1s143560_1.htm">Patrono
          por la vivo</url></vrk>,
          <lok>2012-12-11</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">sepsis</trd>
  <trd lng="be">&c_s;&c_e;&c_p;&c_s;&c_y;&c_s;</trd>
  <trd lng="de">Sepsis</trd>
  <trdgrp lng="fr">
    <trd>sepsis</trd>,
    <trd>septic&eacute;mie</trd>
  </trdgrp>
  <trd lng="la">Sepsis</trd>
  <trd lng="pl">sepsa</trd>
  <trd lng="uk">&c_s;&c_je;&#x43F;&c_s;&c_i;&c_s;</trd>
  <trdgrp lng="zh">
    <trd>&#x8113;&#x6BD2;&#x75C7; <pr>n&#xF3;ngd&#xFA;zh&#xE8;ng</pr></trd>,
    <trd>&#x81BF;&#x6BD2;&#x75C7; <pr>n&#xF3;ngd&#xFA;zh&#xE8;ng</pr></trd>,
    <trd>&#x8113;&#x6BD2;&#x8840;&#x75C7; <pr>n&#xF3;ngd&#xFA;xi&#x11B;zh&#xE8;ng</pr></trd>,
    <trd>&#x81BF;&#x6BD2;&#x8840;&#x75C7; <pr>n&#xF3;ngd&#xFA;xi&#x11B;zh&#xE8;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="seps.0a">
  <kap><tld/>a</kap>
  <snc mrk="seps.0a.MED">
  <uzo tip="fak">MED</uzo>
    <dif>
      Ka&ubreve;zanta sepson:
      <ekz>
        <tld/>aj mikroboj;
      </ekz>
      <ekz>
        <ind>kontra&ubreve;<tld/>a</ind> metodo de operacioj.
        <trd lng="be">&c_a;&c_n;&c_t;&c_y;&c_s;&c_e;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
        <trd lng="ca">antis&egrave;ptic</trd>
        <trd lng="fr">antiseptique</trd>
        <trd lng="hu">antiszeptikus</trd>
        <trd lng="nl">antiseptisch</trd>
        <trd lng="pl">aseptyczny</trd>
        <trd lng="ru">&c_a;&c_n;&c_t;&c_i;&c_s;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
      </ekz>
    </dif>
    <ref tip="ant" cel="steril.0a.BIO">sterila<sncref/></ref>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_p;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="ca">s&egrave;ptic</trd>
  <trdgrp lng="cs">
    <trd>hnilobn&yacute;</trd>,
    <trd>septick&yacute;</trd>
  </trdgrp>
  <trd lng="de">septisch</trd>
  <trd lng="fr">septique</trd>
  <trdgrp lng="hu">
    <trd>szeptikus</trd>,
    <trd>fert&odblac;z&eacute;ses</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x8150;&#x6557;&#x6027;&#x306E;  <pr>&#x3075;&#x306F;&#x3044;&#x305B;&#x3044;&#x306E;</pr></trd>,
    <trd>&#x6557;&#x8840;&#x306E;  <pr>&#x306F;&#x3044;&#x3061;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">bederf veroorzakend</trd>
  <trd lng="pl">septyczny</trd>
  <trd lng="ru">&c_s;&c_je;&c_p;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>hnilobn&yacute;</trd>,
    <trd>septick&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_r;&c_a;&c_z;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_je;&#x43F;&c_t;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>
