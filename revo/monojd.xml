   <uzo tip="fak">MAT</uzo>
   <snc>
      <dif>
        <ref cel="asoci.0eca.magmo">Asocieca</ref>
        <ref tip="super" cel="magm.0o.MAT">magmo</ref> kun
        <ref cel="neuxtr.0a.MAT">ne&ubreve;tra elemento</ref>.
