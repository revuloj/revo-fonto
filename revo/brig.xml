<art mrk="$Id: brig.xml,v 1.17 2023/08/31 08:48:53 revo Exp $">
<kap>
  <rad>brig</rad>/o
</kap>
<drv mrk="brig.0o">
  <kap><tld/>o</kap>
  <snc mrk="brig.0o.MAR">
  <uzo tip="fak">MAR</uzo>
  <uzo tip="fak">HIS</uzo>
    <dif>
      Dumasta <ref tip="super" cel="sxip.vel0o">vel&scirc;ipo</ref>
      kun kvadrataj veloj:
      <ekz>
        mi estas kiel maristo, naskita kaj kreskinta sur ferdeko de
        pirata <tld/>o
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_b;&c_r;&c_y;&c_g;</trd>
  <trd lng="de">Brigg</trd>
  <trd lng="en">brig</trd>
  <trd lng="fr">brick</trd>
  <trd lng="hu">brigg</trd>
  <trd lng="pl">bryg</trd>
  <trd lng="ru">&c_b;&c_r;&c_i;&c_g;</trd>
  </snc>
  <trdgrp lng="cs">
    <trd>briga</trd>,
    <trd>brigga</trd>
  </trdgrp>
  <trd lng="he">&#x5DE;&#x5E4;&#x5E8;&#x5E9;&#x5D9;&#x5EA;</trd>
  <trd lng="ja">&#x30D6;&#x30EA;&#x30C3;&#x30B0;</trd>
  <trdgrp lng="sk">
    <trd>briga</trd>,
    <trd>dvojs&#x165;a&#x17E;&#x148;ov&#xE1; lo&#x10F;</trd>
  </trdgrp>
  <trd lng="uk">&#x431;&#x440;&#x438;&#x491;</trd>
</drv>
</art>
