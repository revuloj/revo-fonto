<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>etiked</rad>/o
    </kap>
    <drv mrk="etiked.0o">
      <kap><tld/>o</kap>
      <snc mrk="etiked.0o.karteto">
        <dif>
          Karteto, fiksita sur objekto, por montri la enhavon, prezon,
          destinon, adreson ktp:
          <ekz>
            tiu konjako, kiel videblas sur la <tld/>o, estis gajninta konsiderindajn
            premiojn dum la mondekspozicio en Lie&gcirc;o kaj Barcelono
            <fnt>
              <aut>Wolfgang Hildesheimer, tr. Wolfgang Kirschstein</aut>:
              <vrk>Iom granda a&ccirc;eto</vrk>,
              <bib>LOdE</bib>, <lok>1999. N-ro 3 (53).</lok>
            </fnt>.
          </ekz>
        </dif>
        <trdgrp lng="be">
          <trd>&c_e;&c_t;&c_y;&c_k;&c_je;&c_t;&c_k;&c_a;</trd>,
          <trd>&c_ja;&c_r;&c_l;&c_y;&c_k;</trd>,
          <trd>&c_sh;&c_y;&c_l;&c_mol;&c_d;&c_a;&c_ch;&c_k;&c_a;</trd>
        </trdgrp>
        <trd lng="fr">&eacute;tiquette<klr tip="ind"> (fiche)</klr></trd>
        <trdgrp lng="ru">
          <trd>&c_e;&c_t;&c_i;&c_k;&c_je;&c_t;&c_k;&c_a;</trd>,
          <trd>&c_ja;&c_r;&c_l;&c_y;&c_k;</trd>
        </trdgrp>
        <trdgrp lng="de">
           <trd>Schild</trd>,
           <trd>Schildchen</trd>,
           <trd>Etikett</trd>
        </trdgrp>
      </snc>
      <snc mrk="etiked.0o.KOMP">
        <dif>
          Parto (<ref tip="super" cel="kamp.0o.formulara">kampo</ref>)
          de <ref tip="malprt" cel="datum.0o.KOMP">datumo</ref>
          priskribanta ties specon:
          <ekz>
            <ind>HTML-<tld/>o</ind>.
            <trd lng="be">HTML-<ind>&c_t;&c_e;&c_g;</ind></trd>
            <trdgrp lng="de">
               <trd>HTML-Sprungmarke</trd>,
               <trd>HTML-Verkn&uuml;pfung</trd>,
               <trd>HTML-Tag</trd>
            </trdgrp>
            <trd lng="en">HTML tag</trd>
          </ekz>
        </dif>
        <trd lng="be">&c_t;&c_e;&c_g;</trd>
        <trdgrp lng="de">
           <trd>Sprungmarke</trd>,
           <trd>Marke</trd>,
           <trd>Tag</trd>
        </trdgrp>
        <trd lng="en">tag</trd>
        <trdgrp lng="ru">
          <trd>&c_t;&c_je;&c_g;</trd>,
          <trd>&c_t;&c_e;&c_g;</trd>
        </trdgrp>
      </snc>
    <trd lng="es">etiqueta</trd>
  <trdgrp lng="pt">
    <trd>etiqueta</trd>,
    <trd>r&#243;tulo</trd>,
    <trd>d&#237;stico</trd>,
    <trd>marca</trd>
  </trdgrp>
    </drv>
  </art>
