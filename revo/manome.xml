<art mrk="$Id: manome.xml,v 1.20 2025/04/02 18:50:42 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>manometr</rad>/o
<fnt><bib>JED</bib></fnt></kap>
<drv mrk="manome.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="aparat.0o.TEK">Aparato</ref>
      por mezuri premon de gasoj kaj vaporoj:
      <ekz>
        <tld/>oj kiuj mezuras la diferencon inter la atmosfera premo kaj
        la premo de la
        mezurata punkto povas tiam esti uzataj kiel vakumetroj pro la
        fakto, ke ili
        povas indiki &leftquot;negativan&rightquot; premon (pli precize, negativa
        premo-diferenco
        koncerne al atmosfera premo)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=589388">Manometro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Prefere diru <ke>premmezurilo</ke>.
  </rim>
                       
  <trd lng="be">&c_m;&c_a;&c_n;&c_o;&c_m;&c_e;&c_t;&c_a;&c_r;</trd>
  <trd lng="br">manometr</trd>
  <trd lng="ca">man&ograve;metre</trd>
  <trdgrp lng="de">
    <trd>Manometer</trd>,
    <trd>Druckmesser</trd>
  </trdgrp>      
  <trd lng="fr">manom&egrave;tre</trd>
  <trdgrp lng="hu">
    <trd>manom&eacute;ter</trd>,
    <trd>nyom&aacute;sm&eacute;r&odblac;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#22311;&#21147;&#35336;  <pr>&#12354;&#12388;&#12426;&#12423;&#12367;&#12369;&#12356;</pr></trd>,
    <trd>&#12510;&#12494;&#12513;&#12540;&#12479;&#12540;</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>manometer</trd>,
    <trd>drukmeter</trd>
  </trdgrp>
  <trd lng="pl">manometr</trd>
  <trd lng="pt">man&ocirc;metro</trd>
  <trd lng="ro">manometru</trd>
  <trd lng="ru">&c_m;&c_a;&c_n;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  <trd lng="uk">&#1084;&#1072;&#1085;&#1086;&#1084;&#1077;&#1090;&#1088;</trd>
  <trdgrp lng="zh">
    <trd>&#21387;&#21147;&#34920; <pr>y&#257;l&#236;bi&#462;o</pr></trd>,
    <trd>&#22739;&#21147;&#34920; <pr>y&#257;l&#236;bi&#462;o</pr></trd>,
    <trd>&#21387;&#24378;&#35745; <pr>y&#257;qi&#225;ngj&#236;</pr></trd>,
    <trd>&#22739;&#24375;&#35336; <pr>y&#257;qi&#225;ngj&#236;</pr></trd>
  </trdgrp>
</drv>
</art>
