  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="en"><trd>to hunch</trd>, <trd>arch</trd></trdgrp>
  <trd lng="es">jorobar</trd>
  <trd lng="hu">felp&uacute;posod&iacute;</trd>
   <trdgrp lng="it">
    <trd>ingobbire</trd>, 
    <trd>aggobbirsi</trd>, 
    <trd>diventare <ind>gobbo</ind></trd>
  </trdgrp>
  <trd lng="pl">zgarbi&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_b;&c_i;&c_t;&c_mol;</trd>
  <trd lng="uk">&c_g;&c_o;&c_r;&c_b;&c_i;&c_t;&c_i;</trd>
-->
</drv>
