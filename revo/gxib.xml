<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&gcirc;ib</rad>/o
</kap>
<drv mrk="gxib.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Dorsa natura elstara&jcirc;o, ka&ubreve;zita de grasamaso:
      <ekz>
        <tld/>oj de kamelo.
      </ekz>
    </dif>
    <trd lng="de">H&ouml;cker</trd>
    <trd lng="pl">garb</trd>
  </snc>
  <snc>
    <uzo tip="fak">ANA</uzo>
    <dif>
      Dorsa a&ubreve; brusta nenormala elstara&jcirc;o, ka&ubreve;zita de
      kurbeco de la spino a&ubreve; de la brustosto (sternumo):
      <ekz>
        la du <tld/>oj de Pul&ccirc;inelo;
      </ekz>
      <ekz>
        propran <tld/>on neniu vidas
        (<refgrp tip="vid">
          <ref cel="kritik.0i">kritiki</ref>,
          <ref cel="trab.0o">trabo</ref>,
          <ref cel="naz.0o">nazo</ref>
        </refgrp>)<fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Buckel</trd>
  </snc>
  <trd lng="be">&c_g;&c_o;&c_r;&c_b;</trd>
  <trdgrp lng="en">
    <trd>hump</trd>,
    <trd>bump</trd>
  </trdgrp>
  <trd lng="es">joroba</trd>
  <trdgrp lng="fr">
    <trd>bosse<klr tip="ind"> (anatomie)</klr></trd>,
    <trd>gibbosit&eacute;<klr tip="ind"> (anatomie)</klr></trd>
  </trdgrp>
  <trd lng="hu">p&uacute;p</trd>
  <trd lng="nl">bult</trd>
  <trd lng="pl">garb</trd>
  <trdgrp lng="pt">
    <trd>corcova</trd>,
    <trd>corcunda</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_b;</trd>
  <trd lng="sv">puckel</trd>
</drv>

<drv mrk="gxib.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Konveksa&jcirc;o en objekto.
    </dif>
    <refgrp tip="vid">
      <ref cel="tuber.0o">tubero</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_u;&c_k;&c_a;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Huckel</trd>
  <trdgrp lng="en">
    <trd>hump</trd>,
    <trd>bump</trd>
  </trdgrp>
  <trd lng="es">protuberancia</trd>
  <trdgrp lng="fr">
    <trd>bosse<klr tip="ind"> (protub&eacute;rance)</klr></trd>,
    <trd>gibbosit&eacute;<klr tip="ind"> (protub&eacute;rance)</klr></trd>,
    <trd>protub&eacute;rance</trd>
  </trdgrp>
  <trd lng="hu">kidudorod&aacute;s</trd>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>

<drv mrk="gxib.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Homo havanta <tld/>on.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_b;&c_a;&c_t;&c_y; <klr tip="amb">(&c_n;&c_a;&c_z;.)</klr></trd>
  <trd lng="de">Buckliger</trd>
  <trdgrp lng="en">
    <trd>humpback</trd>
  </trdgrp>
  <trd lng="es">jorobado</trd>
  <trd lng="fr">bossu<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">p&uacute;pos<klr tip="ind"> (ember)</klr></trd>
  <trd lng="nl">bultenaar</trd>
  <trd lng="pl">garbaty</trd>
  <trd lng="pt">corcunda</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_b;&c_u;&c_n;</trd>
</drv>

<drv mrk="gxib.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Kurbigi:
      <ekz>
        <ind><tld/>igi la dorson</ind> <klr>(ekz-e p.p. timigita
        kato)</klr>.
        <trd lng="fr">faire le gros <ind>dos</ind></trd>
        <trd lng="hu">megg&ouml;rb&iacute;ti a <ind>h&aacute;t&aacute;t</ind></trd>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_g;&c_n;&c_u;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="es">jorobar</trd>
  <trd lng="hu">felp&uacute;posod&iacute;</trd>
  <trd lng="pl">zgarbi&cacute; si&eogonek;</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_b;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
