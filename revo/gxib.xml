<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gxib.xml,v 1.36 2023/11/11 11:36:38 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&gcirc;ib</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gxib.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gxib.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Dorsa natura <ref tip="super" cel="star.el0ajxo">elstara&jcirc;o</ref>, ka&ubreve;zita de grasamaso:
      <ekz>
        ili forportas sur <klr>[&#x2026;]</klr> la <tld/>o de kameloj siajn trezorojn
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 30:6</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kaj iris kameloj, la sablon levante, // Sin, kvaza&ubreve; &scirc;ipeto en mar', balancante
        // El inter la <tld/>oj sin skuis en pendo // La lar&gcirc;aj kurtenoj de tendo kaj
        tendo
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        kamelo <klr>[&#x2026;kiu]</klr> havas nur unu <tld/>on, estas dromedaro
        <fnt>
          <bib>BonaLingvo</bib>
          <lok>4. Emo Difini, Emo Elvoki</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">H&ouml;cker</trd>
     <trdgrp lng="en">
    <trd>hump</trd>,
    <trd>bump</trd>
  </trdgrp>
    <trd lng="fa">&f_k;&a_w;&a_h;&a_A;&a_n;</trd>
    <trd lng="pl">garb</trd>
  </snc>
  <snc mrk="gxib.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Dorsa a&ubreve; brusta nenormala elstara&jcirc;o, ka&ubreve;zita de
      kurbeco de la spino a&ubreve; de la brustosto (sternumo):
      <ekz>
        li havis <klr>[&#x2026;]</klr> kurbigitan dorson, tiel, ke unuavide li &scirc;ajnis esti
        <tld/>a, kaj super tiu <tld/>o trovis sin granda kapo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        propran <tld/>on neniu vidas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Buckel</trd>
    <trd lng="en">hunch</trd>
    <trdgrp lng="fa">
      <trd>&f_k;&a_w;&f_zh;&f_y;</trd>,
      <trd>&a_q;&a_w;&a_z;</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x80BF;&#x5757; <pr>zh&#x1D2;ngku&#xE0;i</pr></trd>,
    <trd>&#x7A81;&#x8D77; <pr>t&#xFA;q&#x1D0;</pr></trd>,
    <trd>&#x507B; <pr>l&#xF3;u</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_g;&c_o;&c_r;&c_b;</trd>
  <trd lng="bg">&c_g;&c_malmol;&c_r;&c_b;&c_i;&c_c;&c_a;</trd>
  <trd lng="bo">&t_sha;&t_tsheg;&t_ba;&t_tsa;&t_naro;&t_ga;&t_tsheg;</trd>
  <trd lng="es">joroba</trd>
  <trdgrp lng="fr">
    <trd>bosse<klr tip="ind"> (anatomie)</klr></trd>,
    <trd>gibbosit&eacute;<klr tip="ind"> (anatomie)</klr></trd>
  </trdgrp>
  <trd lng="hu">p&uacute;p</trd>
  <trd lng="it">gobba</trd>
  <trdgrp lng="ja">
    <trd>&#x3053;&#x3076;</trd>,
    <trd>&#x80CC;&#x3053;&#x3076;  <pr>&#x305B;&#x3053;&#x3076;</pr></trd>,
    <trd>&#x306F;&#x3068;&#x80F8;  <pr>&#x306F;&#x3068;&#x3080;&#x306D;</pr></trd>
  </trdgrp>
  <trd lng="nl">bult</trd>
  <trd lng="pl">garb</trd>
  <trdgrp lng="pt">
    <trd>corcova</trd>,
    <trd>corcunda</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_b;</trd>
  <trd lng="sv">puckel</trd>
  <trd lng="uk">&c_g;&c_o;&c_r;&c_b;</trd>
</drv>

<drv mrk="gxib.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="gxib.0ajxo.FORM">
    <dif>
      Konveksa&jcirc;o en objekto.
    </dif>
    <refgrp tip="vid">
      <ref cel="tuber.0o.sxvelajxo">tubero<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_u;&c_k;&c_a;&c_t;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">Huckel</trd>
  <trdgrp lng="en">
    <trd>hump</trd>,
    <trd>bump</trd>, <trd>protuberance</trd>
  </trdgrp>
  <trd lng="es">protuberancia</trd>
  <trdgrp lng="fr">
    <trd>bosse<klr tip="ind"> (protub&eacute;rance)</klr></trd>,
    <trd>gibbosit&eacute;<klr tip="ind"> (protub&eacute;rance)</klr></trd>,
    <trd>protub&eacute;rance</trd>
  </trdgrp>
  <trd lng="hu">kidudorod&aacute;s</trd>
  <trdgrp lng="it">
    <trd>gobba</trd>,
    <trd>gibbosit&agrave;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3053;&#x3076;</trd>,
    <trd>&#x7A81;&#x8D77;&#x7269;  <pr>&#x3068;&#x3063;&#x304D;&#x3076;&#x3064;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_u;&c_k;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trdgrp lng="uk">
    <trd>&c_o;&c_p;&c_u;&c_k;&c_l;&c_ib;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_v;&c_i;&c_p;&c_u;&c_k;&c_l;&c_ib;&c_s;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="gxib.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="gxib.0ulo.MED">
    <dif>
      Homo havanta <tld/>on:
      <ekz>
        gazeto jam eltrovis, ke mi estas <tld/>ulo, &ccirc;u mi devas liveri al &gcirc;i okazon por
        min nomi blinda
        <fnt>
          <aut>E. About, trad. G. Moch</aut>
          <vrk><url ref="&Gutenberg;files/65835/65835-0.txt">La Re&gcirc;o de la Montoj</url></vrk>
          <lok>1909</lok>
        </fnt>?
      </ekz>
      <ekz>
        al kamparano oni diras: anta&ubreve;en la okulo, se vi estas <tld/>ulo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Pa&scirc;tistino de anseroj apud puto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ulo &gcirc;is morto restos <tld/>ulo
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_r;&c_b;&c_a;&c_t;&c_y; 
  <klr tip="amb">(&c_n;&c_a;&c_z;.)</klr></trd>
  <trd lng="de">Buckliger</trd>
  <trdgrp lng="en">
    <trd>hunchback</trd>
  </trdgrp>
  <trd lng="es">jorobado</trd>
  <trd lng="fa">&f_k;&a_w;&f_zh;&f_p;&a_sh;&a_t;</trd>
  <trd lng="fr">bossu<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">p&uacute;pos<klr tip="ind"> (ember)</klr></trd>
  <trd lng="it">gobbo<klr tip="ind"> (sost.)</klr></trd>
  <trd lng="ja">&#x305B;&#x3080;&#x3057;</trd>
  <trd lng="nl">bultenaar</trd>
  <trd lng="pl">garbaty</trd>
  <trd lng="pt">corcunda</trd>
  <trd lng="ru">&c_g;&c_o;&c_r;&c_b;&c_u;&c_n;</trd>
  <trd lng="uk">&c_g;&c_o;&c_r;&c_b;&c_a;&c_t;&c_o;&c_g;&c_o; &c_m;&c_o;&c_g;&c_i;&c_l;&c_a; &c_s;&c_p;&c_r;&c_a;&c_v;&c_i;&c_t;&c_mol;</trd>
  <trdgrp lng="zh">
    <trd>&#x99DD;&#x80CC; <pr>tu&#xF3;b&#xE8;i</pr></trd>,
    <trd>&#x99DD;&#x80CC;&#x7684;&#x4EBA; <pr>tu&#xF3;b&#xE8;ider&#xE9;n</pr></trd>,
    <trd>&#x99DD; <pr>tu&#xF3;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gxib.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="gxib.0igxi.KOMUNE">
    <dif>
      <ref tip="super" cel="kurb.0igi">Kurbigi</ref> la dorson:
      <ekz>
        la kato <klr>[&#x2026;]</klr> povosciis <tld/>i&gcirc;i kaj &scirc;pini
        <fnt>
          <bib>Fab2</bib>
          <lok>Malbela anasido</lok>
        </fnt>;
      </ekz>
      <ekz>
        la homoj kuras kun ombreloj <klr>[&#x2026;a&ubreve;]</klr> <tld/>i&gcirc;as, kvaza&ubreve; malsekaj kokoj
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_g;&c_a;&c_r;&c_b;&c_a;&c_c;&c_ib;&c_c;&c_c;&c_a;</trd>,
      <trd>&c_v;&c_y;&c_g;&c_ib;&c_n;&c_a;&c_c;&c_c;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>sich <ind>b&uuml;cken</ind></trd>,
      <trd>buckeln</trd>
    </trdgrp>
    <trdgrp lng="zh">
    <trd>&#x5F2F;&#x8170; <pr>w&#x101;ny&#x101;o</pr></trd>,
    <trd>&#x5F2F; <pr>w&#x101;n</pr></trd>
  </trdgrp>
</snc>
