<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>sterkuli</rad>/acoj
</kap>
<drv mrk="sterku.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      tropikaj plantoj (<trd lng="la">Sterculiaceae</trd>).
    </dif>
    <ref tip="sub" cel="kaka.0ujo">kakaujo</ref>
  </snc>
  <trd lng="de">Sterkuliengew&auml;chse</trd>
</drv>
</art>
