  <trdgrp lng="ja">
    <trd>&#21363;&#33288;&#12391;  <pr>&#12381;&#12387;&#12365;&#12423;&#12358;&#12391;</pr></trd>,
    <trd>&#21363;&#24109;&#12391;  <pr>&#12381;&#12367;&#12379;&#12365;&#12391;</pr></trd>,
    <trd>&#12450;&#12489;&#12522;&#12502;&#12391;  <pr>&#12354;&#12393;&#12426;&#12406;&#12391;</pr></trd>
  </trdgrp>
  <trd lng="nl">voor de <ind>vuist</ind> weg</trd>
  <trd lng="pt">de <ind>improviso</ind><klr tip="ind">
  (adv.)</klr></trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_r;&c_o;&c_m;&c_t;&c_o;&c_m;</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&c_m;&c_p;&c_r;&c_o;&c_v;&c_ib;&c_z;&c_o;&c_v;&c_a;&c_n;&c_o;</trd>,
    <trd>&c_je;&c_k;&c_s;&c_p;&c_r;&c_o;&c_m;&c_t;&c_o;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21363;&#20852; <pr>j&iacute;x&igrave;ng</pr></trd>,
    <trd>&#21363;&#24109; <pr>j&iacute;x&iacute;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="improv.0ajxo">
  <kap>
    <tld/>o,
    <var><kap><tld/>a&jcirc;o</kap></var>
  </kap>
  <snc>
    <dif>
      Io verkita a&ubreve; organizita senprepare; <tld/>ado:
      <ekz>
        &jcirc;azo <klr>[&#8230;]</klr> estas la vo&ccirc;a <tld/>o surfone de <tld/>a akompano
        <fnt>
          <bib>KFl</bib>
          <lok>V. Pri unueco kaj lukto, kaj anka&ubreve; pri &rightquot;tipeco de l&#8217; disdivido&#8221;</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni trovos en la disko nek <tld/>ojn de &ccirc;efgitaro, nek tondrajn pa&scirc;ojn de baso
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#8470; 7 (105)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>(oksimore)</klr>
        tamen &ccirc;io kune pasis sufi&ccirc;e glate, kiel &leftquot;bone preparita <tld/>o&rightquot;
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_m;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>,
