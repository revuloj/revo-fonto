<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: improv.xml,v 1.15 2015/07/04 08:37:36 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>improviz</rad>/i
</kap>
<drv mrk="improv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tuj kaj senprepare verki:
      <ekz>
        <tld/>i paroladon, versa&jcirc;on, muzikon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>improvisieren</trd>,
      <trd>aus dem <ind>Stegreif</ind> dichten</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Senprepare organizi ion:
      <ekz>
        <tld/>i man&gcirc;adon por neatenditaj gastoj;
      </ekz>
      <ekz>
        <tld/>i ekskurson, balon, akcepton;
      </ekz>
      <ekz>
        <ind><tld/>ita</ind> prelego;
        <trdgrp lng="fr">
          <trd>improvis&eacute;</trd>,
          <trd>impromptu<klr tip="ind"> (adj.)</klr></trd></trdgrp> 
        <trd lng="hu">r&ouml;gt&ouml;nz&ouml;tt</trd>
      </ekz>
      <ekz>
        li estis lerta <tld/>anto de ludoj por la infanoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>improvisieren</trd>,
      <trd>auf die Schnelle <ind>organisieren</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">improvizovat</trd>
  <trd lng="en">improvise</trd>
  <trd lng="es">improvisar</trd>
  <trd lng="fr">improviser</trd>
  <trdgrp lng="hu">
    <trd>r&ouml;gt&ouml;n&ouml;z</trd>,
    <trd>improviz&aacute;l</trd>
  </trdgrp>
  <trd lng="nl">improviseren</trd>
  <trd lng="pt">improvisar</trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_r;&c_o;&c_v;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">improvizovať</trd>
</drv>

<drv mrk="improv.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Senprepare:
      <ekz>
        <tld/>e ludi rolon de la teatro;
      </ekz>
      <ekz>
        <tld/>e akompani per piano.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_m;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_z;&c_u;&c_ju;&c_ch;&c_y;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_p;&c_r;&c_o;&c_m;&c_t;&c_a;&c_m;</trd>,
    <trd>&c_b;&c_je;&c_z; &c_p;&c_a;&c_d;&c_r;&c_y;&c_h;&c_t;&c_o;&c_w;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>improvisiert</trd>,
    <trd>provisorisch</trd>,
    <trd>ohne <ind>Vorlage</ind></trd>,
    <trd>aus dem <ind>Stegreif</ind></trd>
  </trdgrp>
  <trd lng="en">extemporaneously</trd>
  <trdgrp lng="fr">
    <trd>&agrave; l'<ind>improviste</ind></trd>,
    <trd>impromptu<klr tip="ind"> (adv.)</klr></trd>
  </trdgrp> 
    <trdgrp lng="hu">
      <trd>r&ouml;gt&ouml;n&ouml;zve</trd>,
      <trd>improviz&aacute;lva</trd>,
      <trd>kap&aacute;sb&oacute;l</trd>
    </trdgrp>
  <trd lng="nl">voor de <ind>vuist</ind> weg</trd>
  <trd lng="pt">de <ind>improviso</ind><klr tip="ind">
  (adv.)</klr></trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_r;&c_o;&c_m;&c_t;&c_o;&c_m;</trd>
</drv>

<drv mrk="improv.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io verkita a&ubreve; organizita senprepare.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_m;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_p;&c_r;&c_o;&c_m;&c_t;</trd>
  </trdgrp>
  <trd lng="en">improvisation</trd>
  <trd lng="es">improvisaci&oacute;n</trd>
  <trd lng="fr">improvisation<klr tip="ind"> (chose
  improvis&eacute;e)</klr></trd>
  <trdgrp lng="hu">
    <trd>r&ouml;gt&ouml;nz&eacute;s</trd>,
    <trd>improviz&aacute;ci&oacute;</trd>
  </trdgrp>
  <trd lng="nl">improvisatie</trd>
  <trdgrp lng="pt">
   <trd>improviso<klr tip="amb"> (coisa improvisada)</klr></trd>, 
   <trd>improvisa&ccedil;&atilde;o<klr tip="amb"> (coisa improvisada)</klr></trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_m;&c_p;&c_r;&c_o;&c_v;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_p;&c_r;&c_o;&c_m;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="improv.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu kutimas verki a&ubreve; rolludi senprepare.
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_z;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="cs">improvizátor</trd>
  <trd lng="de">Improvisierer</trd>
  <trd lng="en">improviser</trd>
  <trd lng="es">improvisador</trd>
  <trd lng="fr">improvisateur</trd>
  <trdgrp lng="hu">
    <trd>r&ouml;gt&ouml;nz&odblac;</trd>,
    <trd>improviz&aacute;tor</trd>
  </trdgrp>
  <trd lng="nl">improviseur</trd>
  <trd lng="pt">improvisador</trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_r;&c_o;&c_v;&c_i;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>
  <trd lng="sk">improvizátor <klr>(z povolania)</klr></trd>
</drv>
</art>
