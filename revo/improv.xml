<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>improviz</rad>/i
</kap>
<drv mrk="improv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tuj kaj senprepare verki:
      <ekz>
        <tld/>i paroladon, versa&jcirc;on, muzikon.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>improvisieren</trd>,
      <trd>aus dem <ind>Stegreif</ind> dichten</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Senprepare organizi ion:
      <ekz>
        <tld/>i man&gcirc;adon por neatenditaj gastoj;
      </ekz>
      <ekz>
        <tld/>i ekskurson, balon, akcepton;
      </ekz>
      <ekz>
        <ind><tld/>ita</ind> prelego;
        <trdgrp lng="fr">
          <trd>improvis&eacute;</trd>,
          <trd>impromptu<klr tip="ind"> (adj.)</klr></trd></trdgrp> 
      </ekz>
      <ekz>
        li estis lerta <tld/>anto de ludoj por la infanoj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>improvisieren</trd>,
      <trd>auf die Schnelle <ind>organisieren</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="es">improvisar</trd>
  <trd lng="fr">improviser</trd>
  <trd lng="nl">improviseren</trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_r;&c_o;&c_v;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="improv.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      Senprepare:
      <ekz>
        <tld/>e ludi rolon de la teatro;
      </ekz>
      <ekz>
        <tld/>e akompani per piano.
      </ekz>
    </dif>
  </snc>
    <trdgrp lng="de">
      <trd>improvisiert</trd>,
      <trd>provisorisch</trd>,
      <trd>ohne <ind>Vorlage</ind></trd>,
      <trd>aus dem <ind>Stegreif</ind></trd>
    </trdgrp>
  <trdgrp lng="fr">
    <trd>&agrave; l'<ind>improviste</ind></trd>,
    <trd>impromptu<klr tip="ind"> (adv.)</klr></trd></trdgrp> 
  <trd lng="nl">voor de <ind>vuist</ind> weg</trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_r;&c_o;&c_m;&c_t;&c_o;&c_m;</trd>
</drv>

<drv mrk="improv.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Io verkita a&ubreve; organizita senprepare.
    </dif>
  </snc>
  <trd lng="es">improvisaci&oacute;n</trd>
  <trd lng="fr">improvisation<klr tip="ind"> (chose improvis&eacute;e)</klr></trd>
  <trd lng="nl">improvisatie</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_m;&c_p;&c_r;&c_o;&c_v;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_p;&c_r;&c_o;&c_m;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="improv.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Homo, kiu kutimas verki a&ubreve; rolludi senprepare.
    </dif>
  </snc>
  <trd lng="de">Improvisierer</trd>
  <trd lng="es">improvisador</trd>
  <trd lng="fr">improvisateur</trd>
  <trd lng="nl">improviseur</trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_r;&c_o;&c_v;&c_i;&c_z;&c_a;&c_t;&c_o;&c_r;</trd>
</drv>
</art>
