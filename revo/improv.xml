<drv mrk="improv.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tuj kaj senprepare <ref tip="super" cel="verk.0i">verki</ref>:
      <ekz>
        <tld/>i paroladon, versa&jcirc;on, muzikon;
      </ekz>
      <ekz>
        <klr>[li]</klr> frapadis la kordojn kaj <klr>[&#x2026;]</klr> komencis <tld/>i,
        ser&ccirc;ante grandajn komparojn en la spektaklo, kiu etendis sin anta&ubreve; li
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XLVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        kune kun la kolego li <tld/>as gajan dancon el la hejmaj montoj
        <fnt>
          <bib>Voja&gcirc;Imp</bib>
          <lok>El Odeso &gcirc;is Konstantinopolo</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas e&ccirc; konsilinde kunmeti plurajn variantojn de la fino, tiel ke &ndash; la&ubreve; la
        cirkonstancoj &ndash; oni povu elekti la plej ta&ubreve;gan, <klr>[&#x2026;]</klr> a&ubreve; <tld/>i
        novan finon el ilia kombino
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estis <klr>[la&ubreve;]</klr> stilo libera kaj vigla kaj de la frapantaj kastanjetoj
        ricevas iom la karakteron de rava <tld/>ado
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        certe Vi havas imponan poezian talenton, sed anka&ubreve; ne forgesu, ke e&ccirc; sur la suno
        trovi&gcirc;as makuloj, kial Vi ne provu <tld/>i
        &dash; <tld/>i? pri kio?
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 4</lok>
        </fnt>?
      </ekz>
      <ekz>
        la juna komponistogeneracio jam eksperimentis per la hazardo, utiligis naturajn
        kaj teknikajn sonojn, <tld/>is kaj enkondukis grafikajn simbolojn anstata&ubreve;
        muziknotoj
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2014/011202.php">Pura muziko por eonoj</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>improvisieren</trd>,
      <trd>aus dem <ind>Stegreif</ind> dichten</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Senprepare <ref tip="super" cel="organi2.0i">organizi</ref> ion:
      <ekz>
        <tld/>i man&gcirc;adon por neatenditaj gastoj;
      </ekz>
      <ekz>
        <ind><tld/>ita</ind> prelego;
        <trdgrp lng="fr">
          <trd>improvis&eacute;</trd>,
          <trd>impromptu<klr tip="ind"> (adj.)</klr></trd></trdgrp> 
        <trd lng="hu">r&ouml;gt&ouml;nz&ouml;tt</trd>
      </ekz>
      <ekz>
        Jen la botelo mobilizas // Kaj via glaso improvizas // Vetkuron al najbara glas'
        <fnt>
          <bib>LSB</bib>
          <lok>Vagonrestoracio</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia impreso estas, ke kiel agento de &scirc;tato ekstera, <nom>Beren&scirc;tam</nom> estis nur amatoro,
        fakte <tld/>is tiun rolon por si
        <fnt>
          <bib>CKv</bib>
          <lok>17</lok>
        </fnt>;
      </ekz>
      <ekz>
        por bremsi kaj haltigi la krizan spiralon <klr>[&#x2026;]</klr> 
        oni komence &scirc;ajne iom <tld/>is: Usono savis la bankon <nom>Bear Stearns</nom>, poste ne
        savis alian bankon <nom>Lehman Brothers</nom>, kiu bankrotis
        <fnt>
          <bib>Monato</bib>
          <aut>Marc Vanden Bempt</aut>
          <vrk><url ref="&Monato;/2008/009665.php">La nuna monda financa krizo</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        la plimulto de tiuj institucioj luas lokon &gcirc;enerale apena&ubreve; ta&ubreve;gan, kiam kreskas
        la studentaro, la respondeculoj <tld/>as, starigante kursejon e&ccirc; en
        anta&ubreve;&ccirc;ambro
        <fnt>
          <bib>Monato</bib>
          <aut>Henriel Fidilalao</aut>
          <vrk><url ref="&Monato;/2013/010877.php">Komerce en la klas&ccirc;ambro</url></vrk>
          <lok>2013</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>improvisieren</trd>,
      <trd>auf die Schnelle <ind>organisieren</ind></trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="cs">improvizovat</trd>
  <trd lng="en">improvise</trd>
  <trd lng="es">improvisar</trd>
  <trd lng="fr">improviser</trd>
  <trdgrp lng="hu">
    <trd>r&ouml;gt&ouml;n&ouml;z</trd>,
    <trd>improviz&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5373;&#x8208;&#x3067;&#x4F5C;&#x308B;  <pr>&#x305D;&#x3063;&#x304D;&#x3087;&#x3046;&#x3067;&#x3064;&#x304F;&#x308B;</pr></trd>,
    <trd>&#x5373;&#x5E2D;&#x3067;&#x6F14;&#x3058;&#x308B;  <pr>&#x305D;&#x304F;&#x305B;&#x304D;&#x3067;&#x3048;&#x3093;&#x3058;&#x308B;</pr></trd>,
    <trd>&#x305D;&#x306E;&#x5834;&#x3067;&#x7528;&#x610F;&#x3059;&#x308B;  <pr>&#x305D;&#x306E;&#x3070;&#x3067;&#x3088;&#x3046;&#x3044;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">improviseren</trd>
  <trd lng="pt">improvisar</trd>
  <trd lng="ru">&c_i;&c_m;&c_p;&c_r;&c_o;&c_v;&c_i;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">improvizova&tcaron;</trd>
  <trd lng="uk">&c_ib;&#x43C;&#x43F;&c_r;&#x43E;&c_v;&c_ib;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="improv.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <ref cel="prepar.sen0a">Senprepare</ref>:
      <ekz>
        <tld/>e ludi rolon de la teatro, akompani per piano;
      </ekz>
      <ekz>
        li <tld/>e kaj iranritme dancis
        <fnt>
          <bib>Monato</bib>
          <aut>Yves Nicolas</aut>
          <vrk><url ref="&Monato;/2015/011609.php">Ne mortas la kimono-kulturo</url></vrk>
          <lok>2015</lok>
        </fnt>;
      </ekz>
      <ekz>
        li alkutimi&gcirc;is preska&ubreve; &ccirc;iutage venigi Jozefon, por infanece &gcirc;oji pri lia lerteco
        <tld/>e elokventi la&ubreve; malsamaj abstrakta&jcirc;oj
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Konfeso de la Potifar-edzino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &gcirc;endarmoj <klr>[&#x2026;]</klr> <tld/>e ekrolis kiel flegistoj
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010813.php">Malsanulejen anstata&ubreve; malliberejen</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        armeoj, <klr>[&#x2026;]</klr> organizitaj <tld/>e, disponantaj minimuman armilaron,
        akiritan kontrabande
        <fnt>
        <aut>Michel Duc Goninaz</aut>
        <vrk><url ref="&Monato;/2000/006947.html">Eks-Jugoslavio: 
          kiu kiun mistifikas?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/06, p. 8</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="proviz1.0a">provizore</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_m;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_z;&c_u;&c_ju;&c_ch;&c_y;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_p;&c_r;&c_o;&c_m;&c_t;&c_a;&c_m;</trd>,
    <trd>&c_b;&c_je;&c_z; &c_p;&c_a;&c_d;&c_r;&c_y;&c_h;&c_t;&c_o;&c_w;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>improvisiert</trd>,
    <trd>provisorisch</trd>,
    <trd>ohne <ind>Vorlage</ind></trd>,
    <trd>aus dem <ind>Stegreif</ind></trd>,
    <trd>aus dem <ind>Stand</ind></trd>
  </trdgrp>
  
  <trd lng="en">extemporaneously</trd>
  <trdgrp lng="fr">
    <trd>&agrave; l'<ind>improviste</ind></trd>,
    <trd>impromptu<klr tip="ind"> (adv.)</klr></trd>
  </trdgrp> 
    <trdgrp lng="hu">
      <trd>r&ouml;gt&ouml;n&ouml;zve</trd>,
      <trd>improviz&aacute;lva</trd>,
      <trd>kap&aacute;sb&oacute;l</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5373;&#x8208;&#x3067;  <pr>&#x305D;&#x3063;&#x304D;&#x3087;&#x3046;&#x3067;</pr></trd>,
    <trd>&#x5373;&#x5E2D;&#x3067;  <pr>&#x305D;&#x304F;&#x305B;&#x304D;&#x3067;</pr></trd>,
    <trd>&#x30A2;&#x30C9;&#x30EA;&#x30D6;&#x3067;  <pr>&#x3042;&#x3069;&#x308A;&#x3076;&#x3067;</pr></trd>
  </trdgrp>
  <trd lng="nl">voor de <ind>vuist</ind> weg</trd>
  <trd lng="pt">de <ind>improviso</ind><klr tip="ind">
  (adv.)</klr></trd>
  <trd lng="ru">&c_e;&c_k;&c_s;&c_p;&c_r;&c_o;&c_m;&c_t;&c_o;&c_m;</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&#x43C;&#x43F;&c_r;&#x43E;&c_v;&c_ib;&c_z;&#x43E;&c_v;&c_a;&#x43D;&#x43E;</trd>,
    <trd>&c_je;&#x43A;&c_s;&#x43F;&c_r;&#x43E;&#x43C;&c_t;&#x43E;&#x43C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5373;&#x5174; <pr>j&#xED;x&#xEC;ng</pr></trd>,
    <trd>&#x5373;&#x5E2D; <pr>j&#xED;x&#xED;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="improv.0ajxo">
  <kap>
    <tld/>o,
    <var><kap><tld/>a&jcirc;o</kap></var>
  </kap>
  <snc>
    <dif>
      Io verkita a&ubreve; organizita senprepare; <tld/>ado:
      <ekz>
        &jcirc;azo <klr>[&#x2026;]</klr> estas la vo&ccirc;a <tld/>o surfone de <tld/>a akompano
        <fnt>
          <bib>KFl</bib>
          <lok>V. Pri unueco kaj lukto, kaj anka&ubreve; pri &rightquot;tipeco de l&#x2019; disdivido&#x201D;</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni trovos en la disko nek <tld/>ojn de &ccirc;efgitaro, nek tondrajn pa&scirc;ojn de baso
        <fnt>
          <bib>LOdE</bib>
          <lok>2003, &#x2116; 7 (105)</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>(oksimore)</klr>
        tamen &ccirc;io kune pasis sufi&ccirc;e glate, kiel &leftquot;bone preparita <tld/>o&rightquot;
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 10</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_m;&c_p;&c_r;&c_a;&c_v;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>,
    <trd>&c_e;&c_k;&c_s;&c_p;&c_r;&c_o;&c_m;&c_t;</trd>
  </trdgrp>
  <trd lng="de">Improvisation</trd>
  <trd lng="en">improvisation</trd>
  <trd lng="es">improvisaci&oacute;n</trd>
