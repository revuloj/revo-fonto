<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: damask.xml,v 1.31 2023/11/04 13:16:26 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>damask</rad>/o
</kap>
<drv mrk="damask.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxtof.0o.TEKS">&Scirc;tofo</ref> el silko
      kun
      brilaj enteksitaj desegnoj:
      <ekz>
        la&ubreve;dire <tld/>o estis enkondukita en E&ubreve;ropon
        de Marko-Pa&ubreve;lo.
      </ekz>
    </dif>
  </snc>
  <ref tip="hom" cel="damask2.D0o">Damasko</ref>
  <trdgrp lng="be">
    <trd>&c_d;&c_a;&c_m;&c_a;&c_s;&c_t;</trd>,
    <trd>&c_a;&c_d;&c_a;&c_m;&c_a;&c_sh;&c_a;&c_k;</trd>,
    <trd>&c_d;&c_a;&c_m;&c_a;&c_s;&c_k; <klr tip="amb">(&c_t;&c_k;&c_a;&c_n;&c_ib;&c_n;&c_a;)</klr></trd>
  </trdgrp>
  <trd lng="bg">&c_d;&c_a;&c_m;&c_a;&c_s;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>dom&agrave;s<klr> (teixit)</klr></trd>,
    <trd>dam&agrave;s<klr> (teixit)</klr></trd>
  </trdgrp>
  <trd lng="cs">dama&scaron;ek</trd>
  <trd lng="de">Damast</trd>
  <trd lng="en">damask</trd>
  <trd lng="es">damasco<klr> (tejido)</klr></trd>
  <trd lng="fr">damas<klr tip="ind"> (&eacute;toffe)</klr></trd>
  <trd lng="he">&#x5D0;&#x5E8;&#x5D9;&#x5D2; &#x5D3;&#x5DE;&#x5E9;&#x5E7;</trd>
  <trd lng="hu">damaszt</trd>
  <trdgrp lng="ja">
    <trd>&#x30C0;&#x30DE;&#x30B9;&#x30AF;&#x7E54; 
    <pr>&#x30C0;&#x30DE;&#x30B9;&#x30AF;&#x304A;</pr></trd>,
    <trd>&#x3069;&#x3093;&#x3059;</trd>
  </trdgrp>
  <trd lng="nl">damast</trd>
  <trd lng="pl">adamaszek</trd>
  <trd lng="pt">damasco</trd>
  <trd lng="ru">&c_d;&c_a;&c_m;&c_a; <klr tip="amb">(&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
  <trd lng="sv">damastv&auml;v</trd>
  <trd lng="sk">dama&scaron;ek</trd>
  <trd lng="zh">&#x7DBE; <pr>l&#xED;ng</pr></trd>
</drv>

<drv mrk="damask.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Teksi tiel, ke la rezulto similas al <tld/>o:
      <ekz>
        <tld/>i kotonan tolon..
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_k;&c_a;&c_c;&c_mol; <ind>&c_d;&c_a;&c_m;&c_a;&c_s;&c_t;</ind></trd>
  <trd lng="ca">adomassar</trd>
  <trd lng="cs">tk&aacute;t dama&scaron;ek</trd>
  <trd lng="en">damask</trd>
  <trd lng="fr">damasser</trd>
  <trd lng="hu">sz&odblac; <klr tip="amb">(damsztt&aacute;)</klr></trd>
  <trd lng="sk">tka&tcaron; damasek</trd>
  <trd lng="sv">damastv&auml;va</trd>
</drv>
</art>
