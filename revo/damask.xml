<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>damask</rad>/o
</kap>
<drv mrk="damask.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="sxtof.0o.TEKS">&Scirc;tofo</ref> el silko kun
      brilaj enteksitaj desegnoj:
      <ekz>
        la&ubreve;dire <tld/>o estis enkondukita en E&ubreve;ropon
        de Marko-Pa&ubreve;lo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="ca">
    <trd>dom&agrave;s<klr> (teixit)</klr></trd>,
    <trd>dam&agrave;s<klr> (teixit)</klr></trd>
  </trdgrp>
  <trd lng="cs">dama&scaron;ek</trd>
  <trd lng="de">Damast</trd>
  <trd lng="es">damasco<klr> (tejido)</klr></trd>
  <trd lng="fr">damas<klr tip="ind"> (&eacute;toffe)</klr></trd>
  <trd lng="hu">damaszt</trd>
  <trd lng="pt">damasco</trd>
  <trd lng="ru">&c_d;&c_a;&c_m;&c_a; <klr
tip="amb">(&c_t;&c_k;&c_a;&c_n;&c_mol;)</klr></trd>
  <trd lng="sv">damastv&auml;v</trd>
</drv>

<drv mrk="damask.D0o">
  <kap><tld lit="D"/>o</kap>
  <snc mrk="damask.D0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <tld/>o estas la &ccirc;efurbo de Sirio kaj la plej granda urbo de
      Sirio.
    </dif>
  </snc>
  <trd lng="bg">&c_D;&c_a;&c_m;&c_a;&c_s;&c_k;</trd>
  <trd lng="de">Damaskus</trd>
  <trd lng="en">Damascus</trd>
  <trd lng="sv">Damaskus</trd>
</drv>

<drv mrk="damask.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Teksi tiel, ke la rezulto similas al <tld/>o:
      <ekz>
        <tld/>i kotonan tolon..
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">tk&aacute;t dama&scaron;ek</trd>
  <trd lng="en">damask</trd>
  <trd lng="fr">damasser</trd>
  <trd lng="hu">sz&odblac; <klr tip="amb">(damsztt&aacute;)</klr></trd>
  <trd lng="sv">damastv&auml;va</trd>
</drv>
</art>
