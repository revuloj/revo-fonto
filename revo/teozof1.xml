<vortaro>
<art mrk="$Id: teozof1.xml,v 1.8 2024/01/18 01:34:30 revo Exp $">
<kap>
  <rad>teozof</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="teozof1.0o">
  <kap><tld/>o</kap>
  <snc mrk="teozof1.0o.FIL">
    <uzo tip="fak">FIL</uzo>
    <dif>
      <ref tip="vid" cel="teozof.0o.FIL">Teozofi</ref>ano:
      <ekz>
        Steiner kaj multaj aliaj germanaj <tld/>oj &ccirc;irka&ubreve; li
        malkonsentis akcepti la hindan knabon Jidu Kri&scirc;namurti kiel
        renaskita<klr>n</klr> Kristo<klr>n</klr>
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Antropozofia_Societo">
          Antropozofia Societo</url>, 2012-10-22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_a;&c_s;&c_o;&c_f;</trd>
  <trd lng="bg">&c_t;&c_je;&c_o;&c_s;&c_o;&c_f;</trd>
  <trdgrp lng="cs">
    <trd>teozof</trd>,
    <trd>teozofista</trd>
  </trdgrp>
  <trd lng="en">theosophist</trd>
  <trd lng="fr">th&eacute;osophe</trd>
  <trd lng="pl">teozof</trd>
  <trd lng="sk">teozof</trd>
  <trd lng="uk">&c_t;&c_je;&#x43E;&c_s;&#x43E;&c_f;</trd>
</drv>
</art>

