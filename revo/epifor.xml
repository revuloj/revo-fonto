<art mrk="$Id: epifor.xml,v 1.16 2023/10/14 15:49:04 revo Exp $">
<kap>
  <rad>epifor</rad>/o
</kap>
<drv mrk="epifor.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Sen&ccirc;esa nevola eli&gcirc;o de <ref cel="larm.0o">larmoj</ref>:
      <ekz>
        simptomoj estas &scirc;velo, ru&gcirc;eco, doloro kaj jukado &ccirc;e la palpebra rando, <tld/>o
        (larmelfluo)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=226302">Blefarito</url></lok>
        </fnt>.
      </ekz>  
    </dif>
    <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsano</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_ib;&c_zh;&c_v;&c_o;&c_l;&c_mol;&c_n;&c_a;&c_je; <ind>&c_s;&c_mol;&c_l;&c_jo;&c_z;&c_a;&c_v;&c_y;&c_d;&c_z;&c_ja;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</ind></trd>,
    <trd>&c_e;&c_p;&c_ib;&c_f;&c_a;&c_r;&c_a; <klr tip="amb">(&c_m;&c_e;&c_d;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>antistrofa</trd>,
    <trd>epifora</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Tr&auml;nenfluss</trd>,
     <trd>Tr&auml;nentr&auml;ufeln</trd>,
     <trd>Augentriefen</trd>
  </trdgrp>
  <trd lng="fr">&eacute;piphora</trd>
  <trd lng="id">epifora</trd>
  <trdgrp lng="ja">
    <trd>&#x6D41;&#x6D99;  <pr>&#x308A;&#x3085;&#x3046;&#x306A;&#x307F;&#x3060;</pr></trd>,
    <trd>&#x6D41;&#x6D99;&#x75C7;  <pr>&#x308A;&#x3085;&#x3046;&#x308B;&#x3044;&#x3057;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="pl">&lstroke;zawienie</trd>
  <trd lng="pt">ep&iacute;fora</trd>
  <trd lng="sk">antistrofa</trd>
  <trd lng="uk">&c_je;&#x43F;&c_ib;&c_f;&#x43E;&c_r;&c_a;</trd>
</drv>
</art>
