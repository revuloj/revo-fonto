<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: decili.xml,v 1.22 2022/03/22 18:26:34 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>decilitr</rad>/o
</kap>
<drv mrk="decili.0o">
  <kap><tld/>o</kap>
  <mlg>dl</mlg>
  <snc>
    <dif>
      Komunuza <ref tip="super" cel="unu.mezur0o">mezurunuo</ref> de volumena
      kapacito, egala al dekono de <ref tip="malprt" cel="litr.0o">litro</ref>:
      <ekz>
        li rabis preska&ubreve; 30 foje en sumo de 142 milionojn da forinto, anta&ubreve; la rabadoj
        li trinkis 1-2 <tld/>ojn da viskio
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=466020">Attila Ambrus</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_e;&c_c;&c_y;&c_l;&c_ib;&c_t;&c_r;</trd>
  <trd lng="cs">decilitr</trd>
  <trd lng="de">Deziliter</trd>
  <trdgrp lng="en">
    <trd>deciliter</trd>,
    <trd>decilitre</trd>
  </trdgrp>
  <trd lng="es">decilitro</trd>
  <trd lng="fr">d&eacute;cilitre</trd>
  <trdgrp lng="he">
    <trd>&#x5D3;&#x5B5;&#x5E6;&#x5B4;&#x5D9;&#x5DC;&#x5B4;&#x5D9;&#x5D8;&#x5E8;</trd>,
    <trd>&#x5E2;&#x5E9;&#x5D9;&#x5E8;&#x5D9;&#x5EA; &#x5D4;&#x5DC;&#x5D9;&#x5D8;&#x5E8;</trd>
  </trdgrp>
  <trd lng="hu">deciliter</trd>
  <trd lng="nl">deciliter</trd>
  <trd lng="pl">decylitr</trd>
  <trd lng="pt">decilitro</trd>
  <trd lng="ru">&c_d;&c_je;&c_c;&c_i;&c_l;&c_i;&c_t;&c_r;</trd>
  <trd lng="sk">deciliter</trd>
  <trd lng="sv">deciliter</trd>
  <trd lng="uk">&#x434;&#x435;&#x446;&#x438;&#x43B;&#x456;&#x442;&#x440;</trd>
</drv>
</art>
