<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>tribrak</rad>/o
</kap>
<drv mrk="tribra.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">POE</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <ref tip="super" cel="pied.vers0o">Verspiedo</ref> konsistanta
      el tri mallongaj a&ubreve; senakcentaj silaboj;
      simbolo /&ypsilon;&ypsilon;&ypsilon;/:
      <ekz>
        la <ref cel="sxveb.0o.licenco">&scirc;vebo</ref> anstata&ubreve;igas
        trieran piedon per <tld/>o
        <fnt><bib>PGl</bib>, <lok>p. 22</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_r;&c_y;&c_b;&c_r;&c_a;&c_h;&c_ib;&c_j;</trd>
  <trd lng="fr">tribraque</trd>
  <trd lng="hu">tribrachisz</trd>
  <trd lng="nl">tribrachys</trd>
  <trd lng="ru">&c_t;&c_r;&c_i;&c_b;&c_r;&c_a;&c_h;&c_i;&c_j;</trd>
</drv>
<adm>
  Sxajnas al mi, ke la formo "tribrahxo" devus
  ekzisti (vd amfibrahxo). <aut>MB</aut>
</adm>
</art>
