<drv mrk="kravat.bant0o">
  <kap>bant<tld/>o</kap>
  <snc mrk="kravat.bant0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="kravat.0o.TEKS">Kravato</ref> kun &scirc;vela,
      banta nodo:
      <ekz>
        mi trovis foton <klr>[&#8230;]</klr>, sur kiu <nom>Marut</nom> staras
        <klr>[&#8230;]</klr> kun bant<tld/>o kaj proleto&ccirc;apo
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro
        43a</lok></fnt>;
      </ekz>
      <ekz>
        sur &gcirc;ia kolo estis blanka fraka bant<tld/>o kaj sur la brusto &gcirc;i havis
        perlamotan sinjorinan binoklon
        <fnt>
          <bib>MkM</bib>
          <lok>22</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_a;&c_b;&c_a;&c_ch;&c_k;&c_a; <klr tip="amb">(&c_g;&c_a;&c_l;&c_mol;&c_sh;&c_t;&c_u;&c_k;)</klr></trd>
  <trdgrp lng="ca">
    <trd>corbata de llacet</trd>,
    <trd>corbat&iacute;</trd>
  </trdgrp>
  <trd lng="cs">mot&yacute;lek - kravata</trd>
  <trdgrp lng="de">
