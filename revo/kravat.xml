<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kravat</rad>/o
</kap>
<drv mrk="kravat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Eta ornama <ref tip="super" cel="tuk.kol0o">koltuko</ref>,
      vira galanteria&jcirc;o,
      kiun oni ligas &ccirc;irka&ubreve; &ccirc;emiza kolumo
      kaj kies ekstremojn oni lasas surbruste pendaj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Krawatte</trd>,
    <trd>Binder</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tie</trd>,
    <trd>necktie</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cravate</trd>
  </trdgrp>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_s;&c_t;&c_u;&c_k;</trd>
</drv>
</art>
