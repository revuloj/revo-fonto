  <trd lng="bn">&#x99F;&#x9BE;&#x987;</trd>
  <trd lng="ca">corbata</trd>
  <trd lng="co">cravatta</trd>
  <trdgrp lng="cs">
    <trd>kravata</trd>,
    <trd>v&aacute;zanka</trd>
  </trdgrp>
  <trd lng="da">slips</trd>
