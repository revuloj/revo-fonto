<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kravat</rad>/o
</kap>
<drv mrk="kravat.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kravat.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Eta <ref tip="super" cel="tuk.kol0o">koltuko</ref>, vira
      galanteria&jcirc;o, kiun oni orname nodas anta&ubreve; la kolo:
      <ekz>
        li <klr>...</klr> stari&gcirc;is anta&ubreve; la spegulo, &gcirc;ustigis la <tld/>on
        <fnt><bib>Marta</bib></fnt>;
      </ekz>
      <ekz>
        la civiluloj portas hele bluajn <tld/>ojn
        <fnt><bib>Mary</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_mol;&c_sh;&c_t;&c_u;&c_k;</trd>
  <trd lng="ca">corbata</trd>
  <trdgrp lng="de">
    <trd>Krawatte</trd>,
    <trd>Binder</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tie</trd>,
    <trd>necktie</trd>
  </trdgrp>
  <trd lng="es">corbata</trd>
  <trd lng="fr">cravate</trd>
  <trd lng="hu">nyakkend&odblac;</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_s;&c_t;&c_u;&c_k;</trd>
</drv>

<drv mrk="kravat.bant0o">
  <kap>bant<tld/>o</kap>
  <snc mrk="kravat.bant0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="kravat.0o.TEKS">Kravato</ref> kun &scirc;vela, banta nodo:
      <ekz>
        mi trovis foton <klr>...</klr>, sur kiu Marut staras
        <klr>...</klr> kun bant<tld/>o kaj proleto&ccirc;apo
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>dua libro, &ccirc;apitro 43a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">noeud papillon</trd>
</drv>
</art>
