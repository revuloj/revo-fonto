<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: epidemiologi.xml,v 1.8 2023/11/07 20:34:11 revo Exp $">
<kap>
  <rad>epidemiologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="epidemiologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="epidemiologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Studo de oftecoj kaj evoluoj de malsanoj en homgrupoj, parto de 
      <ref tip="malprt" cel="medici.0o.SCI">medicino</ref>:
      <ekz>
        timo kreskas <klr>...</klr> pro la rezultatoj de studo farita de
        la 
        Ministerio pri Sano kaj de la regiona observejo pri <tld/>o 
        rilate la industriajn zonojn de Sicilio 
        <fnt>
          <aut>-</aut>,
          <vrk>-</vrk>,
          <lok><url ref="http://www.radioarkivo.org/vidu.php/2275">
          Radio Itala Internacia, 2005-07-16</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="biostatistik.0o.BIO">biostatistiko</ref>,
      <ref cel="epidem.0o.MED">epidemio</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_d;&c_e;&c_m;&c_ib;&c_ja;&c_l;&c_jo;&c_g;&c_ib;&c_ja;</trd>
  <trd lng="de">Epidemiologie</trd>
  <trd lng="fr">&eacute;pid&eacute;miologie</trd>
  <trd lng="ja">&#x75AB;&#x5B66;  <pr>&#x3048;&#x304D;&#x304C;&#x304F;</pr></trd>
  <trd lng="pl">epidemiologia</trd>
  <trd lng="pt">epidemiologia</trd>
  <trd lng="zh">&#x6D41;&#x884C;&#x75C5;&#x5B66; <pr>li&#xFA;x&#xED;ngb&#xEC;ngxu&#xE9;</pr></trd>
</drv>

<drv mrk="epidemiologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="epidemiologi.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        kuracisto <tld/>isto
        <fnt>
          <aut>J. A. Vergara</aut>,
          <vrk><url ref="http://eo.nitobe.info/ar/prezento.php?id=80">
          Prezento...</url></vrk>,
          <lok>Nitobe-centro por lingva demokratio, [vidita en
          2007-03]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_p;&c_ib;&c_d;&c_e;&c_m;&c_ib;&c_jo;&c_l;&c_ja;&c_g;</trd>
  <trd lng="de">Epidemiologe</trd>
  <trd lng="fr">&eacute;pid&eacute;miologiste</trd>
  <trd lng="pl">epidemiolog</trd>
  <trdgrp lng="pt">
    <trd>epidemiologista</trd>,
    <trd>epidemi&oacute;logo</trd>
  </trdgrp>
</drv>
</art>

