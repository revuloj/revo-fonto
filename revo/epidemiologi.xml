<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>epidemiologi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="epidemiologi.0o">
  <kap><tld/>o</kap>
  <snc mrk="epidemiologi.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Studo de oftecoj kaj evoluoj de malsanoj en homgrupoj, parto de 
      <ref tip="malprt" cel="medici.0o.SCI">medicino</ref>:
      <ekz>
        timo kreskas <klr>...</klr> pro la rezultatoj de studo farita de la 
        Ministerio pri Sano kaj de la regiona observejo pri <tld/>o 
        rilate la industriajn zonojn de Sicilio 
        <fnt>
          <aut>-</aut>,
          <vrk>-</vrk>,
          <lok><url ref="http://www.radioarkivo.org/vidu.php/2275">
          Radio Itala Internacia, 2005-07-16</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="biostatistik.0o.BIO">biostatistiko</ref>,
      <ref cel="epidem.0o.MED">epidemio</ref>
    </refgrp>
  </snc>
  <trd lng="fr">&eacute;pid&eacute;miologie</trd>
</drv>

<drv mrk="epidemiologi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="epidemiologi.0isto.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Fakulo pri <tld/>o:
      <ekz>
        kuracisto <tld/>isto
        <fnt>
          <aut>J. A. Vergara</aut>,
          <vrk><url ref="http://eo.nitobe.info/ar/prezento.php?id=80">
          Prezento...</url></vrk>,
          <lok>Nitobe-centro por lingva demokratio, [vidita en 2007-03]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">&eacute;pid&eacute;miologiste</trd>
</drv>
</art>

<!--
$Log$
