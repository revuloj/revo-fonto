<art mrk="$Id: vulkan.xml,v 1.27 2024/06/29 21:25:14 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>vulkan</rad>/o
</kap>
<drv mrk="vulkan.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">GEOL</uzo>
  <snc>
    <dif>
      Monto, kiu dum <ref tip="vid" cel="erupci.0o">erupcioj</ref>
      eligas lafon, cindron, gasojn tra sia kratero:
      <ekz>
        &Ccirc;i-momente en la mondo estas preska&ubreve; 600 aktivaj vulkanoj,
        kiuj
        havas erupciojn a&ubreve; almena&ubreve; estis aktivaj en historia
        tempo<fnt><aut>Evgeni Georgiev</aut>: <vrk>Vulkanoj</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 2a, p. 22a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_u;&c_l;&c_k;&c_a;&c_n;</trd>
  <trd lng="bg">&c_v;&c_u;&c_l;&c_k;&c_a;&c_n;</trd>
  <trd lng="br">menez-tan</trd>
  <trd lng="ca">volc&agrave;</trd>
  <trdgrp lng="cs">
    <trd>sopka</trd>,
    <trd>vulk&#xE1;n</trd>
  </trdgrp>
  <trd lng="de">Vulkan</trd>
  <trd lng="el">&eta;&phi;&alfa;&jota_ton;&sigma;&tau;&epsilon;&jota;&omikron;</trd>
  <trd lng="en">volcano</trd>
  <trd lng="es">volc&aacute;n</trd>
  <trd lng="fr">volcan</trd>
  <trd lng="he">&he;&resh;-&gimel;&ayin;&shin;</trd>
  <trd lng="hu">vulk&aacute;n</trd>
  <trd lng="it">vulcano</trd>
  <trd lng="ja">&#x706B;&#x5C71;  <pr>&#x304B;&#12374;&#12435;</pr></trd>
  <trd lng="nl">vulkaan</trd>
  <trd lng="pl">wulkan</trd>
  <trd lng="pt">vulc&atilde;o</trd>
  <trd lng="ru">&c_v;&c_u;&c_l;&c_k;&c_a;&c_n;</trd>
  <trdgrp lng="sk">
    <trd>sopka</trd>,
    <trd>vulk&#xE1;n</trd>
  </trdgrp>
  <trd lng="sv">vulkan</trd>
  <trd lng="uk">&c_v;&c_u;&#x43B;&#x43A;&c_a;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x706B;&#x5C71; <pr>hu&#x1D2;sh&amacron;n</pr></trd>,
    <trd>&#x706B;&#x795E; <pr>hu&#x1D2;sh&#xE9;n</pr></trd>
  </trdgrp>
</drv>
</art>
