<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: cirku.xml,v 1.4 2010/04/16 16:31:31 revo Exp $">
<kap>
  <rad>cirku</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="cirku.0o">
  <kap><tld/>o</kap>
  <snc mrk="cirku.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Mezgranda rabobirdo,
      genro el familio <ref tip="super" 
      cel="akcipi1.0oj.ZOO">akcipitredoj</ref>
      (<trd lng="la">Circus</trd>):
      <ekz>
        li <klr>...</klr> trinkis teon kaj rigardis al ili per malicaj, 
        kiel de <tld/>o, okuloj
        <fnt>
          <aut>U. Karatkevi&ccirc;, trad. Z. Lapcionak</aut>,
          <vrk><url ref="http://zmila.at.tut.by/Libroportistoj.html">
          Libroportistoj</url></vrk>,
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        bluaj <tld/>oj &ccirc;asas etajn mamulojn
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;blua
        cirkuo&rightquot;, 
        2009-06-14</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="de">Weihe</trd>
    <trd lng="en">harrier</trd>
    <trd lng="fr">busard</trd>
    <trd lng="hu">r&eacute;tih&eacute;ja</trd>
    <trd lng="it">albanella</trd>
    <trd lng="nl">kiekendief</trd>
    <trd lng="pl">b&lstroke;otniak</trd>
    <trd lng="pt">tartaranh&atilde;o</trd>
    <trd lng="sv">(k&auml;rr)h&ouml;k</trd>
  </snc>
</drv>

<drv mrk="cirku.marcxa0o">
  <kap>mar&ccirc;a <tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      Specio de <ref tip="super" cel="cirku.0o.ZOO">cirkuo</ref>.
    </dif>
    <trd lng="en"><klr>western </klr>marsh-<ind>harrier</ind></trd>
    <trd lng="la">Circus aeruginosus</trd>
    <trd lng="pl">b&lstroke;otniak stawowy</trd>
    <trd lng="sv">brun <ind>k&auml;rrh&ouml;k</ind></trd>
  </snc>
</drv>

<drv mrk="cirku.blua0o">
  <kap>blua <tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      Specio de <ref tip="super" cel="cirku.0o.ZOO">cirkuo</ref>.
    </dif>
    <trd lng="en">hen <ind>harrier</ind></trd>
    <trd lng="la">Circus cyaneus</trd>
    <trd lng="pl">b&lstroke;otniak zbo&zdot;owy</trd>
    <trd lng="sv">bl&aring; <ind>k&auml;rrh&ouml;k</ind></trd>
  </snc>
</drv>

<drv mrk="cirku.griza0o">
  <kap>griza <tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <fnt>
       <aut>Wouter F. Pilger</aut>,
       <vrk>
         <url ref="http://www.xs4all.nl/~pilger/wfp/birdeou8.htm">
           Komunlingvaj nomoj de E&ubreve;ropaj birdoj</url>
       </vrk>
    </fnt>
    <dif>
      Specio de <ref tip="super" cel="cirku.0o.ZOO">cirkuo</ref>.
    </dif>
    <trd lng="en">montagu's <ind>harrier</ind></trd>
    <trd lng="la">Circus pygargus</trd>
    <trd lng="pl">b&lstroke;otniak &lstroke;&aogonek;kowy</trd>
    <trd lng="sv">&auml;ngsh&ouml;k</trd>
  </snc>
</drv>
</art>

