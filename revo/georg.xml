<kap>
  <ofc>*</ofc><ofc>7</ofc>
  <rad>Georg</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="georg.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> grekdevena:
      <ekz>
        <tld/>o Va&scirc;ington estis naskita la dudek duan de
        Februaro de la jaro mil sepcent tridek dua
        <fnt><bib>F</bib>, <lok>&FE;12.</lok></fnt>;
      </ekz>
      <ekz>
        &gcirc;in simbole estras re&gcirc;ino sub protekto de la kavalira figuro de <ind>sankta <tld/>o</ind>
        <fnt>
          <aut>Jozefo Lej&ccirc;</aut>,
          <vrk><url ref="&Monato;/2012/010644.php">Ludaj aludoj</url></vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2012a, numero 5a, p. 11a</lok>
        </fnt>.
        <trd lng="de">der heilige Georg</trd>
        <trd lng="en">Saint George</trd>
        <trd lng="fr">saint Georges</trd>
        <trdgrp lng="nl">
          <trd>Sint-Joris</trd>,
          <trd>Sint Joris</trd>
        </trdgrp>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_G;&c_je;&c_o;&c_r;&c_g;&c_ib;</trd>,
    <trd>&c_R;&c_y;&c_g;&c_o;&c_r;</trd>,
    <trd>&c_Ju;&c_r;&c_y;</trd>
  </trdgrp>   
  <trd lng="bg">&c_G;&c_je;&c_o;&c_r;&c_g;&c_i;</trd>
  <trdgrp lng="br">
    <trd>Jord</trd>,
    <trd>Jorj</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Georg</trd>,
    <trd>J&uuml;rgen</trd>
  </trdgrp>    
  <trd lng="fr">Georges</trd>
  <trd lng="grc">&Gamma;&epsilon;&omega_acute;&rho;&gamma;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="en">George</trd>
  <trd lng="es">Jorge</trd>
  <trd lng="hu">Gy&ouml;rgy</trd>
  <trd lng="it">Giorgio</trd>
  <trdgrp lng="ja">
    <trd>&#x30B2;&#x30AA;&#x30EB;&#x30B0;</trd>,
    <trd>&#x30B8;&#x30E7;&#x30FC;&#x30B8;  <pr>&#x3058;&#x3087;&#x30FC;&#x3058;</pr></trd>,
    <trd>&#x30B8;&#x30E7;&#x30EB;&#x30B8;&#x30E5;</trd>
  </trdgrp>
  <trdgrp lng="nl">
   <trd>Joris</trd>, 
   <trd>Sjors</trd>, 
   <trd>George</trd> 
  </trdgrp>
  <trd lng="pl">Grzegorz</trd>
  <trdgrp lng="pt">
   <trd>Jorge</trd>, 
   <trd>George</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_G;&c_je;&c_o;&c_r;&c_g;&c_i;&c_j;</trd>,
    <trd>&c_Je;&c_g;&c_o;&c_r;</trd>,
    <trd>&c_Ju;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_G;&c_je;&c_o;&c_r;&c_g;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_D;&c_zh;&c_o;&c_r;&c_d;&c_zh;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x413;&#x435;&#x43E;&#x440;&#x433;&#x456;&#x439;</trd>,
    <trd>&#x42E;&#x440;&#x456;&#x439;</trd>,
    <trd>&#x416;&#x43E;&#x440;&#x436;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4E54;&#x6CBB; <pr>qi&#xE1;ozh&#xEC;</pr></trd>,
    <trd>&#x4E8E;&#x5C14;&#x6839; <pr>y&#xFA;&#x11B;rg&#x113;n</pr></trd>
