<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc><ofc>7</ofc>
  <rad>Georg</rad>/o
</kap>
<drv mrk="georg.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo grekdevena:
      <ekz>
        <tld/>o Va&scirc;ington estis naskita la dudek duan de
        Februaro de la jaro mil sepcent tridek dua
        <fnt><bib>F</bib>, <lok>&FE;12.</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_G;&c_je;&c_o;&c_r;&c_g;&c_ib;</trd>,
    <trd>&c_R;&c_y;&c_g;&c_o;&c_r;</trd>,
    <trd>&c_Ju;&c_r;&c_y;</trd>
  </trdgrp>   
  <trd lng="bg">&c_G;&c_je;&c_o;&c_r;&c_g;&c_i;</trd>
  <trdgrp lng="br">
    <trd>Jord</trd>,
    <trd>Jorj</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Georg</trd>,
    <trd>J&uuml;rgen</trd>
  </trdgrp>    
  <trd lng="fr">Georges</trd>
  <trd lng="grc">&Gamma;&epsilon;&omega_acute;&rho;&gamma;&jota;&omikron;&sigma_fina;</trd>
  <trd lng="es">Jorge</trd>
  <trd lng="hu">Gy&ouml;rgy</trd>
  <trd lng="it">Giorgio</trd>
  <trdgrp lng="pt">
   <trd>Jorge</trd>, 
   <trd>George</trd> 
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_G;&c_je;&c_o;&c_r;&c_g;&c_i;&c_j;</trd>,
    <trd>&c_Je;&c_g;&c_o;&c_r;</trd>,
    <trd>&c_Ju;&c_r;&c_i;&c_j;</trd>,
    <trd>&c_G;&c_je;&c_o;&c_r;&c_g;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>,
    <trd>&c_D;&c_zh;&c_o;&c_r;&c_d;&c_zh;<klr> (&c_i;&c_n;&c_o;&c_s;&c_t;&c_r;.)</klr></trd>
  </trdgrp>
</drv>
</art>
