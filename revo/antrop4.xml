<kap>
  <rad>antropomorfism</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="antrop4.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">FIL</uzo>
  <snc mrk="antrop4.0o.KOMUNE">
    <dif>
      Atribuo al nehoma&jcirc;o de homaj ecoj.
    </dif>
    <subsnc mrk="antrop4.0o.FIL">
      <uzo tip="fak">FIL</uzo>
      <dif>
        Atribuo al dio de homaj formo, pasioj a&ubreve; sentoj.
      </dif>
    </subsnc>
    <subsnc mrk="antrop4.0o.ajxobesto">
      <dif>
        Atribuo al a&jcirc;o a&ubreve; besto de homaj sintenoj:
        <ekz>
          J. H. Fabre okupas tute apartan lokon en la etologio; li 
          estis la unua, kiu plene liberi&gcirc;is de la <tld/>o 
          <fnt>
            <aut>J. &Scirc;uvajev</aut>,
            <vrk><url ref="&STEB;zoologio/konduto_de_bestoj/f874-p30.htm">
            Pri la konduto de bestoj</url></vrk>,
            <lok>Fokuso, 1987:1, p. 2a</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <refgrp tip="vid">
      <ref cel="person.0igo.LIN">personigo<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_t;&c_r;&c_a;&c_p;&c_a;&c_m;&c_a;&c_r;&c_f;&c_ib;&c_z;&c_m;</trd>
  <trd lng="bg">&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;&c_o;&c_m;&c_o;&c_r;&c_f;&c_i;&c_z;&c_malmol;&c_m;</trd>
  <trd lng="ca">antropomorfisme</trd>
  <trdgrp lng="cs">
    <trd>antropomorfizmus</trd>,
    <trd>personifikace</trd>
  </trdgrp>
  <trd lng="de">Anthropomorfismus</trd>
  <trd lng="en">anthropomorphism</trd>
  <trd lng="es">antropomorfismo</trd>
  <trd lng="fa">&a_A;&a_n;&a_s;&a_A;&a_n;&zwnj;&a_d;&f_y;&a_s;&f_y;</trd>
  <trd lng="fr">anthropomorphisme</trd>
  <trd lng="hu">antropomorfizmus</trd>
  <trd lng="it">antropomorfismo</trd>
  <trd lng="ja">&#x795E;&#x4EBA;&#x540C;&#x5F62;&#x540C;&#x6027;&#x8AD6;  [<ind>&#x304B;&#x307F;&#x3058;&#x3093;&#x3069;&#x3046;&#x304B;&#x305F;&#x3069;&#x3046;&#x305B;&#x3044;&#x308D;&#x3093;</ind>]</trd>
  <trd lng="nl">antropomorfisme</trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;&c_o;&c_m;&c_o;&c_r;&c_f;&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>antropomorfizmus</trd>,
    <trd>personifik&#xE1;cia</trd>
  </trdgrp>
  <trd lng="uk">&#x430;&#x43D;&#x442;&#x440;&#x43E;&#x43F;&#x43E;&#x43C;&#x43E;&#x440;&#x444;&#x456;&#x437;&#x43C;</trd>
