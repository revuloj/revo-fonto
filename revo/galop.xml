<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>galop</rad>/o
</kap>
<drv mrk="galop.0o">
  <kap><tld/>o</kap>
  <snc num="1">
    <dif>
      Plej rapida kurmaniero de &ccirc;evalo. 
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre rapida kuro. 
    </dif>
  </snc>
  <trd lng="en">gallop</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_o;&c_p;</trd>
  <trd lng="sv">galopp</trd>
</drv>
<drv mrk="galop.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld/>ante: <ekz>la buboj <tld/>e forkuris<fnt>B</fnt></ekz>
    </dif>
  </snc>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_o;&c_p;&c_o;&c_m;</trd>
  <trd lng="sv">i <ind>galopp</ind></trd>
</drv>
<drv mrk="galop.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="G"/>e kuri. 
    </dif>
    <refgrp tip="sin">
      <ref cel="trot">troto</ref>,
      <ref cel="pasx.0o">pa&scirc;o</ref>,
      <ref cel="ambl.0o">amblo</ref>.
    </refgrp>
  </snc>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_k;&c_a;&c_t;&c_mol; &c_g;&c_a;&c_l;&c_o;&c_p;&c_o;&c_m;</trd>
  <trd lng="sv">galoppera</trd>
</drv>
</art>
