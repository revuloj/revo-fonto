<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>galop</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="galop.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="galop.0i.RAJ">
    <dif>
      Kuri la&ubreve; plej rapida maniero de kvarpieduloj,
      longe etendante la krurojn:
      <ekz>
        la sova&gcirc;aj &ccirc;evaloj <tld/>as tra la vastaj stepoj
        <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ambl.0i.RAJ">ambli <sncref/></ref>,
      <ref cel="pasx.0i">pa&scirc;i</ref>,
      <ref cel="trot.0i.RAJ">troti <sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="galop.0i.FIG">
    <dif>
      Tre rapide <ref tip="vid" cel="kur.0i.iu">kuri</ref>:
      <ekz>
        Kruko kuras, <tld/>as, saltas, sed vane
        <fnt><aut>L. Beaucaire</aut>, <vrk>Kruko kaj Baniko 
        el Bervalo</vrk>, <lok>1974</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_a;&c_k;&c_a;&c_c;&c_mol; <ind>&c_g;&c_a;&c_l;&c_o;&c_p;&c_a;&c_m;</ind></trd>,
    <trd>&c_s;&c_k;&c_a;&c_k;&c_a;&c_c;&c_mol; <ind>&c_n;&c_a;&c_w;&c_s;&c_k;&c_a;&c_ch;</ind></trd>
  </trdgrp>
  <trd lng="de">galoppieren</trd>
  <trd lng="en">gallop</trd>
  <trd lng="es">galopar</trd>
  <trd lng="fr">galoper</trd>
  <trdgrp lng="hu">
    <trd>galoppozik</trd>,
    <trd>v&aacute;gt&aacute;zik</trd></trdgrp>
  <trd lng="nl">galopperen</trd>
  <trd lng="el">&kappa;&alfa;&lambda;&pi;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="pt">galopar</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_k;&c_a;&c_t;&c_mol; &c_g;&c_a;&c_l;&c_o;&c_p;&c_o;&c_m;</trd>
  <trd lng="sv">galoppera</trd>
  <trd lng="tr">d&ouml;rtnala gitmek</trd>
</drv>

<drv mrk="galop.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld lit="G"/>ante:
      <ekz>
        la buboj <tld/>e forkuris<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_o;&c_p;&c_a;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_w;&c_s;&c_k;&c_a;&c_ch;</trd>
  </trdgrp>
  <trd lng="de">galoppierend</trd>
  <trd lng="en">galloping</trd>
  <trd lng="fr">au <ind>galop</ind></trd>
  <trd lng="hu">v&aacute;gt&aacute;ban</trd>
  <trd lng="nl">in <ind>galop</ind></trd>
  <trd lng="pt">a <ind>galope</ind></trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_o;&c_p;&c_o;&c_m;</trd>
  <trd lng="sv">i <ind>galopp</ind></trd>
  <trd lng="tr">d&ouml;rtnala</trd>
</drv>

<drv mrk="galop.0o">
  <kap><tld/>o</kap>
  <snc mrk="galop.0o.KOMUNE">
    <dif>
      Tre rapida kuro a&ubreve; kurmaniero.
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_o;&c_p;</trd>
  <trd lng="de">Galopp</trd>
  <trd lng="el">&kappa;&alfa;&lambda;&pi;&alfa;&sigma;&my;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">gallop</trd>
  <trd lng="es">galope</trd>
  <trd lng="fr">galop</trd>
  <trdgrp lng="hu">
    <trd>galopp</trd>,
    <trd>v&aacute;gta</trd></trdgrp>
  <trd lng="nl">galop</trd>
  <trd lng="pt">galope</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_o;&c_p;</trd>
  <trd lng="sv">galopp</trd>
</drv>
</art>
