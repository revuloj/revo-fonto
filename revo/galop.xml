<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: galop.xml,v 1.30 2023/11/11 10:00:38 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>galop</rad>/o <fnt><bib>Fab1</bib></fnt>
</kap>
<drv mrk="galop.0o">
  <kap><ofc>1</ofc><tld/>o</kap>
  <snc mrk="galop.0o.KOMUNE">
    <dif>
      Tre rapida kuro a&ubreve; kurmaniero:
      <ekz>
        eksonis &ccirc;evala <tld/>o
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro XIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        sen selo &scirc;i sidadis kiel kunkreski&gcirc;inta kun la &ccirc;evalo, kiu kuradis en plena
        <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la vulpo etendis la voston, kaj la re&gcirc;a filo sidi&gcirc;is sur &gcirc;in: <tld/>',
        <tld/>' trans &scirc;tonojn kaj akvojn
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_a;&c_l;&c_o;&c_p;</trd>
  <trd lng="ca">galop</trd>
  <trdgrp lng="cs">
    <trd>chvat</trd>,
    <trd>kalup</trd>,
    <trd>shon</trd>,
    <trd>sp&ecaron;ch</trd>
  </trdgrp>
  <trd lng="de">Galopp</trd>
  <trd lng="el">&kappa;&alfa;&lambda;&pi;&alfa;&sigma;&my;&omikron_ton;&sigma_fina;</trd>
  <trd lng="en">gallop</trd>
  <trd lng="es">galope</trd>
  <trd lng="fr">galop</trd>
  <trdgrp lng="hu">
    <trd>galopp</trd>,
    <trd>v&aacute;gta</trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#x99C6;&#x3051;&#x8DB3;  <pr>&#x304B;&#x3051;&#x3042;&#x3057;</pr></trd>,
    <trd>&#x30AE;&#x30E3;&#x30ED;&#x30C3;&#x30D7;</trd>
  </trdgrp>
  <trd lng="nl">galop</trd>
  <trd lng="pl">galop</trd>
  <trd lng="pt">galope</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_o;&c_p;</trd>
  <trdgrp lng="sk">
    <trd>cval</trd>,
    <trd>galop</trd>
  </trdgrp>
  <trd lng="sv">galopp</trd>
  <trd lng="uk">&#x491;&#x430;&#x43B;&#x43E;&#x43F;</trd>
  <trd lng="zh">&#x75BE;&#x99B3; <pr>j&#xED;ch&#xED;</pr></trd>
</drv>

<drv mrk="galop.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="galop.0i.RAJ">
    <dif>
      Kuri la&ubreve; plej rapida maniero de kvarpieduloj,
      longe etendante la krurojn:
      <ekz>
        la sova&gcirc;aj &ccirc;evaloj <tld/>as tra la vastaj stepoj
        <fnt><bib>Fab1</bib>, <lok>Sova&gcirc;aj cignoj</lok></fnt>;
      </ekz>
      <ekz>
        li ek<tld/>is anta&ubreve;en inter la montoj, tenis &scirc;in anta&ubreve; si sur la &ccirc;evalo, kaj la
        &ccirc;asistoj <tld/>is post li
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;ashundoj <tld/>is anta&ubreve;en
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        du kale&scirc;oj kun furioza rapideco preter<tld/>is
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        de la avangardo al<tld/>is rajdisto
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        subite la besto kun li for<tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        per unu salto &gcirc;i trans<tld/>is la &ccirc;evalon de la falinta malamiko
        <fnt>
          <bib>Fab4</bib>
          <lok>Sterkskarabo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la leporo <tld/>ulo forkuris kaj sin ka&scirc;is
        <fnt>
          <bib>ElektFab</bib>
          <lok>Du fratoj</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ambl.0i.RAJ">ambli <sncref/></ref>,
      <ref cel="pasx.0i">pa&scirc;i</ref>,
      <ref cel="trot.0i.RAJ">troti <sncref/></ref>
    </refgrp>
  </snc>
  <snc mrk="galop.0i.FIG">
    <dif>
      Tre rapide <ref tip="vid" cel="kur.0i.iu">kuri</ref>:
      <ekz>
        la &scirc;telistoj forkuris kaj <tld/>is kvaza&ubreve; &ccirc;asataj de la diablo
        <fnt>
          <bib>ElektFab</bib>
          <lok>Fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        Kruko kuras, <tld/>as, saltas, sed vane
        <fnt><aut>L. Beaucaire</aut>, <vrk>Kruko kaj Baniko 
        el Bervalo</vrk>, <lok>1974</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_a;&c_k;&c_a;&c_c;&c_mol; <ind>&c_g;&c_a;&c_l;&c_o;&c_p;&c_a;&c_m;</ind></trd>,
    <trd>&c_s;&c_k;&c_a;&c_k;&c_a;&c_c;&c_mol; <ind>&c_n;&c_a;&c_w;&c_s;&c_k;&c_a;&c_ch;</ind></trd>
  </trdgrp>
  <trd lng="bo">&#xF62;&#xF92;&#xFB1;&#xF74;&#xF42;&#xF0B;</trd>
  <trd lng="ca">galopar</trd>
  <trdgrp lng="cs">
    <trd>cv&aacute;lat</trd>,
    <trd>galopovat</trd>
  </trdgrp>
  <trd lng="de">galoppieren</trd>
  <trd lng="el">&kappa;&alfa;&lambda;&pi;&alfa_ton;&zeta;&omega;</trd>
  <trd lng="en">gallop</trd>
  <trd lng="es">galopar</trd>
  <trd lng="fr">galoper</trd>
  <trdgrp lng="hu">
    <trd>galoppozik</trd>,
    <trd>v&aacute;gt&aacute;zik</trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AE;&#x30E3;&#x30ED;&#x30C3;&#x30D7;&#x3067;&#x8D70;&#x308B;  <pr>&#x30AE;&#x30E3;&#x30ED;&#x30C3;&#x30D7;&#x3067;&#x306F;&#x3057;&#x308B;</pr></trd>,
    <trd/>,
    <trd>&#x5927;&#x6025;&#x304E;&#x3067;&#x3059;&#x308B;  <pr>&#x304A;&#x304A;&#x3044;&#x305D;&#x304E;&#x3067;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">galopperen</trd>
  <trd lng="pl">galopowa&cacute;</trd>
  <trd lng="pt">galopar</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_k;&c_a;&c_t;&c_mol; &c_g;&c_a;&c_l;&c_o;&c_p;&c_o;&c_m;</trd>
  <trd lng="sk">cv&aacute;la&tcaron;</trd>
  <trd lng="sv">galoppera</trd>
  <trd lng="tr">d&ouml;rtnala gitmek</trd>
  <trdgrp lng="uk">
    <trd>&#x491;&#x430;&#x43B;&#x43E;&#x43F;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>,
    <trd>&#x441;&#x43A;&#x430;&#x43A;&#x430;&#x442;&#x438; &#x491;&#x430;&#x43B;&#x43E;&#x43F;&#x43E;&#x43C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x51AF; <pr>f&#xE9;ng</pr></trd>,
    <trd>&#x8DB9; <pr>ju&#xE9;</pr></trd>,
    <trd>&#x99C6; <pr>q&#x16B;</pr></trd>,
    <trd>&#x817E; <pr>t&#xE9;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="galop.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld lit="G"/>ante:
      <ekz>
        subite venis <tld/>e du &ccirc;evaloj
        <fnt>
          <bib>Fab2</bib>
          <lok>Najbaraj familioj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>e ili &ccirc;irka&ubreve;rajdis la herboplacon
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &ccirc;asistaro <tld/>e alproksimi&gcirc;adis
        <fnt>
          <bib>Fab3</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili ekkuris <tld/>e, sin &jcirc;etis en la &ccirc;ambron kaj falis sur la patran bruston
        <fnt>
          <bib>ElektFab</bib>
          <lok>Ha&ccirc;jo kaj Grenjo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_g;&c_a;&c_l;&c_o;&c_p;&c_a;&c_m;</trd>,
    <trd>&c_n;&c_a;&c_w;&c_s;&c_k;&c_a;&c_ch;</trd>
  </trdgrp>
  <trd lng="ca">al <ind>galop</ind></trd>
  <trd lng="cs">cvalem</trd>
  <trdgrp lng="de">
    <trd>galoppierend</trd>,
    <trd>im <ind>Galopp</ind></trd>
  </trdgrp>
  <trd lng="en">galloping</trd>
  <trd lng="fr">au <ind>galop</ind></trd>
  <trd lng="hu">v&aacute;gt&aacute;ban</trd>
  <trdgrp lng="ja">
    <trd>&#x30AE;&#x30E3;&#x30ED;&#x30C3;&#x30D7;&#x3067;</trd>,
    <trd>&#x5168;&#x901F;&#x529B;&#x3067;  <pr>&#x305C;&#x3093;&#x305D;&#x304F;&#x308A;&#x3087;&#x304F;&#x3067;</pr></trd>
  </trdgrp>
  <trd lng="nl">in <ind>galop</ind></trd>
  <trd lng="pl">galopem</trd>
  <trd lng="pt">a <ind>galope</ind></trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_o;&c_p;&c_o;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>cvalom</trd>,
    <trd>galopom</trd>,
    <trd>r&yacute;chlym behom</trd>
  </trdgrp>
  <trd lng="sv">i <ind>galopp</ind></trd>
  <trd lng="tr">d&ouml;rtnala</trd>
  <trd lng="uk">&#x491;&#x430;&#x43B;&#x43E;&#x43F;&#x43E;&#x43C;</trd>
</drv>

</art>
