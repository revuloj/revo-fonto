<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><ofc>1</ofc>
<rad>galop</rad>/o</kap>
<drv mrk="galop.0o">
  <kap><tld/>o</kap>
  <snc mrk="galop.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Plej rapida kurmaniero de &ccirc;evalo.
    </dif>
    <adm>
      Koherigi la referencitajn terminojn kun ~o! &leftquot;Troto&rightquot; ne ekzistas,
      &leftquot;pasxo&rightquot; ne havas ZOO snc (cetere, cxu ne estas &leftquot;pasxado&rightquot;) kaj al
      &leftquot;amblo&rightquot; mankas vinjeto ZOO. <aut>MB</aut>
    </adm>
    <refgrp tip="vid">
      <ref cel="trot">troto</ref>,
      <ref cel="pasx.0o">pa&scirc;o</ref>,
      <ref cel="ambl.0o">amblo</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Tre rapida kuro.
    </dif>
  </snc>
  <trd lng="de">Galopp</trd>
  <trd lng="en">gallop</trd>
  <trd lng="fr">galop</trd>
  <trd lng="nl">galop</trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_o;&c_p;</trd>
  <trd lng="sv">galopp</trd>
</drv>

<drv mrk="galop.0e">
  <kap><tld/>e</kap>
  <snc>
    <dif>
      <tld lit="G"/>ante:
      <ekz>
        la buboj <tld/>e forkuris<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">galoppierend</trd>
  <trd lng="fr">au <ind>galop</ind></trd>
  <trd lng="nl">in <ind>galop</ind></trd>
  <trd lng="ru">&c_g;&c_a;&c_l;&c_o;&c_p;&c_o;&c_m;</trd>
  <trd lng="sv">i <ind>galopp</ind></trd>
</drv>

<drv mrk="galop.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <tld lit="G"/>e kuri.
    </dif>
  </snc>
  <trd lng="de">galoppieren</trd>
  <trd lng="fr">galoper</trd>
  <trd lng="nl">galopperen</trd>
  <trd lng="ru">&c_s;&c_k;&c_a;&c_k;&c_a;&c_t;&c_mol; &c_g;&c_a;&c_l;&c_o;&c_p;&c_o;&c_m;</trd>
  <trd lng="sv">galoppera</trd>
</drv>
<adm>
  Konsideri, cxu la radikkaraktero estas sxangxinda. <aut>MB</aut>
</adm>
</art>
