<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karakt2.xml,v 1.16 2023/12/03 08:00:23 revo Exp $">
<kap>
  <rad>karakteristik</rad>/o
</kap>
<drv mrk="karakt2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="karakt2.0o.logaritmo">
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <klr>(de <ref cel="logari.0o">logaritmo</ref>)</klr>
      &Gcirc;ia <ref cel="part.entjera0o">entjera parto</ref>:
      <ekz>
        la <tld/>o de la ordinara logaritmo de 
        <frm am="10^n*x">10<sup><k>n</k></sup>&middot;<k>x</k></frm> 
        egalas al tiu de <frm am="x"><k>x</k></frm> plus
        <frm am="n"><k>n</k></frm>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mantis.0o.MAT">mantiso.</ref>
    <trd lng="pl">cecha<klr tip="ind"> (logarytmu)</klr></trd>
  </snc>
  <snc mrk="karakt2.0o.ringo">
    <fnt><bib>MatStokTerm</bib><lok>p. 29</lok></fnt>
    <dif>
      <klr>(de <ref cel="ring.0o.MAT">ringo</ref>)</klr>
      Tia plej malgranda nenula entjero <frm am="n"><k>n</k></frm>, se
      &gcirc;i 
      ekzistas, ke <frm am="n*1=0"><k>n</k>&#x2219;1 = 0</frm>:
      <ekz>
        la ringo de entjeroj havas nulan <tld/>on <klr>(ne ekzistas tia
        nenula
        entjero <frm am="n"><k>n</k></frm>, ke <frm am="n*1=0"><k>n</k>&#x2219;1 = 0</frm>)</klr>;
      </ekz>
      <ekz>
        la <ref cel="ring.0o_de_nmodulaj_restoklasoj">ringo de n-modulaj        
        restoklasoj</ref> havas <tld/>on <frm am="n"><k>n</k></frm>.
      </ekz>
    </dif>
    <trd lng="pl">charakterystyka<klr tip="ind">
    (pier&sacute;cienia)</klr></trd>
  </snc>
  <ref tip="hom" cel="karakt1.0a">karakteriza</ref>
  <trd lng="be">&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_a;&c_r;&c_y;&c_s;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="cs">charakteristika</trd>
  <trd lng="de">Charakteristik</trd>
  <trd lng="en">characteristic<klr tip="ind"> (of a logarithm, of a
  ring)</klr></trd>
  <trd lng="fr">caract&eacute;ristique<klr tip="ind"> (d'un logarithme, d'un
  anneau)</klr></trd>
  <trd lng="hu">karakterisztika</trd>
  <trd lng="ru">&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;<klr tip="ind"> (&c_l;&c_o;&c_g;&c_a;&c_r;&c_i;&c_f;&c_m;&c_a;,
  &c_k;&c_o;&c_l;&c_mol;&c_c;&c_a;)</klr></trd>
  <trd lng="sk">charakteristika</trd>
  <trd lng="uk">&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x7279;&#x5FB5;&#x6570; <pr>t&#xE8;zh&#x113;ngsh&#xF9;</pr></trd>,
    <trd>&#x793A;&#x6027;&#x6570; <pr>sh&#xEC;x&#xEC;ngsh&#xF9;</pr></trd>,
    <trd>&#x5BF9;&#x6570;&#x7684;&#x9996;&#x6570; <pr>du&#xEC;sh&#xF9;desh&#x1D2;ush&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
