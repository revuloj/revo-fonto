<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
  <kap>
    <rad>karakteristik</rad>/o
  </kap>
<drv mrk="karakt2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      <klr>(de <ref cel="logari.0o">logaritmo</ref>)</klr>
      &Gcirc;ia <ref cel="part.entjera0o">entjera parto</ref>:
      <ekz>
        la <tld/>o de la ordinara logaritmo de 10<sup>n</sup>.x egalas al tiu de
        x plus n.
      </ekz>
    </dif>
    <ref tip="vid" cel="mantis.0o">mantiso.</ref>
  </snc>
  <trd lng="de">Characteristik</trd>
  <trd lng="en">characteristic<klr tip="ind"> (of logarithm)</klr></trd>
  <trd lng="fr">caract&eacute;ristique<klr tip="ind"> (d'un logarithme)</klr></trd>
  <trd lng="ru">&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;<klr tip="ind"> (&c_l;&c_o;&c_g;&c_a;&c_r;&c_i;&c_f;&c_m;&c_a;)</klr></trd>
</drv>
</art>

