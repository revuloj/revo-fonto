<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>karakteristik</rad>/o
</kap>
<drv mrk="karakt2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <snc mrk="karakt2.0o.logaritmo">
    <fnt><bib>PIV1</bib></fnt>
    <dif>
      <klr>(de <ref cel="logari.0o">logaritmo</ref>)</klr>
      &Gcirc;ia <ref cel="part.entjera0o">entjera parto</ref>:
      <ekz>
        la <tld/>o de la ordinara logaritmo de 
        <frm>10<sup><k>n</k></sup>.<k>x</k></frm> 
        egalas al tiu de <frm><k>x</k></frm> plus
        <frm><k>n</k></frm>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mantis.0o.MAT">mantiso.</ref>
    <trd lng="pl">cecha<klr tip="ind"> (logarytmu)</klr></trd>
  </snc>
  <snc mrk="karakt2.0o.ringo">
    <fnt><bib>MatStokTerm</bib><lok>p. 29</lok></fnt>
    <dif>
      <klr>(de <ref cel="ring.0o.MAT">ringo</ref>)</klr>
      Tia plej malgranda nenula entjero <frm><k>n</k></frm>, se &gcirc;i 
      ekzistas, ke <frm><k>n</k>&#8729;1 = 0</frm>:
      <ekz>
        la ringo de entjeroj havas nulan <tld/>on <klr>(ne ekzistas tia nenula
        entjero <frm><k>n</k></frm>, ke <frm><k>n</k>&#8729;1 = 0</frm>)</klr>;
      </ekz>
      <ekz>
        la ringo de restoj module <frm><k>m</k></frm> havas <tld/>on <frm><k>m</k></frm>.
      </ekz>
    </dif>
    <trd lng="pl">charakterystyka<klr tip="ind"> (pier&sacute;cienia)</klr></trd>
  </snc>
  <trd lng="de">Charakteristik</trd>
  <trd lng="en">characteristic<klr tip="ind"> (of a logarithm, of a ring)</klr></trd>
  <trd lng="fr">caract&eacute;ristique<klr tip="ind"> (d'un logarithme, d'un anneau)</klr></trd>
  <trd lng="ru">&c_h;&c_a;&c_r;&c_a;&c_k;&c_t;&c_je;&c_r;&c_i;&c_s;&c_t;&c_i;&c_k;&c_a;<klr tip="ind"> (&c_l;&c_o;&c_g;&c_a;&c_r;&c_i;&c_f;&c_m;&c_a;, &c_k;&c_o;&c_l;&c_mol;&c_c;&c_a;)</klr></trd>
</drv>
</art>
