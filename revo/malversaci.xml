<art mrk="$Id: malversaci.xml,v 1.14 2024/01/25 20:10:33 revo Exp $">
<kap>
  <rad>malversaci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="malversaci.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="malversaci.0o.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="super" cel="sxtel.0o">&Scirc;telo</ref> de komuna, kompania
      a&ubreve;
      administra mono por persona profito:
      <ekz>
        foje li a&ubreve;dis ion pri proprigo de kolektita greno,
        <klr>...</klr>
        pri <tld/>o de lokaj oficistoj
        <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 99a, volumo 3a, p.
        278a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="frauxd.0o">fra&ubreve;do</ref>,
      <ref cel="tromp.0o">trompo</ref>,
      <ref cel="korupt.0o">korupto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_t;&c_r;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_s;&c_l;&c_u;&c_zh;&c_b;&c_o;&c_v;&c_a;&c_je; &c_z;&c_l;&c_o;&c_w;&c_zh;&c_y;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="ca">malversaci&oacute;</trd>
  <trdgrp lng="cs">
    <trd>bezpr&aacute;vn&eacute; p&rcaron;ivlastn&ecaron;n&iacute;</trd>,
    <trd>malverzace</trd>,
    <trd>zpronev&ecaron;ra</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Amtsmi&szlig;brauch</trd>,
    <trd>Unterschlagung</trd>,
    <trd>Veruntreuung</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_kh;&a_t;&a_l;&a_A;&a_s;</trd>
  <trd lng="fr">malversation</trd>
  <trdgrp lng="hu">
    <trd>sikkaszt&aacute;s</trd>,
    <trd>h&udblac;tlen kezel&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>verduistering<klr tip="ind"> (v.geld)</klr></trd>,
    <trd>malversatie</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>defraudacja</trd>,
    <trd>malwersacja</trd>,
    <trd>sprzeniewierzenie</trd>
  </trdgrp>
  <trd lng="pt">malversa&ccedil;&atilde;o</trd>
  <trd lng="ro">deturnare de fonduri</trd>
  <trd lng="sk">sprenevera</trd>
  <trdgrp lng="uk">
    <trd>&c_z;&c_l;&c_o;&c_v;&c_zh;&c_i;&c_v;&c_a;&c_n;&c_n;&c_ja; &c_p;&c_o; &c_s;&c_l;&c_u;&c_zh;&c_b;&c_ib;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_v;&c_l;&c_a;&c_s;&c_n;&c_je;&c_n;&c_n;&c_ja; &c_g;&c_r;&c_o;&c_m;&c_a;&c_d;&c_s;&c_mol;&c_k;&c_i;&c_h; &c_g;&c_r;&c_o;&c_sh;&c_je;&c_j;</trd>,
    <trd>&c_r;&c_o;&c_z;&c_t;&c_r;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4FB5;&#x541E;&#x516C;&#x6B3E; <pr>q&#x12B;nt&#x16B;ng&#x14D;ngku&#x1CE;n</pr></trd>,
    <trd>&#x8CAA;&#x6C61; <pr>t&#x101;nw&#x16B;</pr></trd>,
    <trd>&#x8D2A;&#x6C61; <pr>t&#x101;nw&#x16B;</pr></trd>,
    <trd>&#x6EE5;&#x7528;&#x804C;&#x6743; <pr>l&#xE0;ny&#xF2;ngzh&#xED;qu&#xE1;n</pr></trd>,
    <trd>&#x6FEB;&#x7528;&#x8077;&#x6B0A; <pr>l&#xE0;ny&#xF2;ngzh&#xED;qu&#xE1;n</pr></trd>
  </trdgrp>
</drv>
</art>

