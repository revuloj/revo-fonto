<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>amend(ament)</rad>/o
</kap>
<drv mrk="amend.0o">
  <kap><tld/>o</kap>
    <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Parta &scirc;an&gcirc;o al le&gcirc;o a&ubreve; al diskutata projekto.
    </dif>
    <refgrp tip="sin">
      <ref cel="korekt.0o">korekto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Ab&auml;nderung</trd>,
    <trd>Anpassung</trd>,
    <trd>Berichtigung</trd>,
    <trd>Besserung</trd>,
    <trd>Korrektur</trd>,
    <trd>Nachtrag</trd>,
    <trd>Novellierung</trd>,
    <trd>Verbesserung</trd>
  </trdgrp>
  <trd lng="en">amendment</trd>
  <trd lng="es">enmienda</trd>
  <trd lng="fr">amendement</trd>
  <trd lng="nl">amendement</trd>
  <trd lng="ru">&c_p;&c_o;&c_p;&c_r;&c_a;&c_v;&c_k;&c_a;</trd>
  <trd lng="sk">novela <klr>(z&aacute;kona)</klr></trd>
</drv>
</art>
