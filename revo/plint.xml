<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: plint.xml,v 1.9 2023/10/20 16:38:17 revo Exp $">
<kap>
  <rad>plint</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="plint.0o">
  <kap><tld/>o</kap>
  <snc mrk="plint.0o.ARKI_kolona">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Plata suba&jcirc;o de
      <ref tip="malprt" cel="kolon.0o.ARKI">kolono</ref>:
      <ekz>
        la <tld/>oj de la kolonetoj
        <fnt><bib>I&scirc;tar</bib>,
        <lok>&ccirc;apitro 8a, konsterno</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="nl">zuilvoet</trd>
    <trdgrp lng="pl">
      <trd>plina</trd>,
      <trd>cok&oacute;&lstroke;</trd>,
      <trd>p&lstroke;ycie&nacute;</trd>
    </trdgrp>
  </snc>
  <snc mrk="plint.0o.ARKI_mura">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Elstara strio la&ubreve;longe de la bazo de muro a&ubreve; vando:
      <ekz>
        Restos nur mortigi tiujn malfortajn grupojn <klr>[de
        blatoj]</klr> de la kuirejo, la lavma&scirc;inejo a&ubreve; de
        sub la muraj <tld/>oj
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;blato&rightquot;,
        2008-08-30</lok></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="pl">
      <trd>cok&oacute;&lstroke;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_a;&c_z;&c_a; <klr tip="amb">(&c_k;&c_a;&c_l;&c_jo;&c_n;&c_y;, &c_p;&c_ib;&c_l;&c_ja;&c_s;&c_t;&c_r;&c_y;)</klr></trd>,
    <trd>&c_p;&c_l;&c_ib;&c_n;&c_t;&c_u;&c_s; <klr tip="amb">(&c_k;&c_a;&c_l;&c_jo;&c_n;&c_y;)</klr></trd>
    <trd>&c_a;&c_s;&c_n;&c_o;&c_v;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_n;&c_o;&c_zh;&c_zh;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>podstavec pod sochou</trd>,
    <trd>spodn&#xED; &#x10D;&#xE1;st sloupu</trd>
  </trdgrp>
  <trd lng="fr">plinthe</trd>
  <trdgrp lng="ja">
    <trd>&#x67F1;&#x790E;  <pr>&#12385;&#12421;&#12358;&#x305D;</pr></trd>,
    <trd>&#x53F0;&#x5EA7;  <pr>&#12384;&#12356;&#12374;</pr></trd>,
    <trd>&#26681;&#x77F3;  <pr>&#x306D;&#12356;&#12375;</pr></trd>
  </trdgrp>
  <trd lng="nl">plint</trd>
  <trdgrp lng="sk">
    <trd>p&#xE4;tka st&#x13A;pa</trd>,
    <trd>v&#xFD;stupok mura</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43B;&c_ib;&#x43D;&c_t;&c_u;&c_s;</trd>,
    <trd>&#x43F;&#x43B;&c_ib;&#x43D;&c_t;</trd>,
    <trd>&c_c;&#x43E;&#x43A;&#x43E;&#x43B;&#x44C; (&#x43A;&#x43E;&#x43B;&#x43E;&#x43D;&c_i;)</trd>
  </trdgrp>
</drv>
</art>

