<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: pra.xml,v 1.24 2025/02/05 08:32:32 revo Exp $">
    <kap>
      <ofc>*</ofc> <rad>pra</rad> <fnt><bib>PV</bib></fnt>
    </kap>
    <subart>
      <drv mrk="pra.0">
        <kap><tld/>-</kap>
        <gra><vspec>prefikso</vspec></gra>
        <snc mrk="pra.0.HIS">
          <uzo tip="fak">HIS</uzo>
          <dif>
            Tre primitiva, el anta&ubreve;longe:
            <ekz>
              <tld/>arbaro
              <klr>(ekzistanta de nememorebla tempo)</klr>;
            </ekz>
            <ekz>
              <tld/>hejmo
              <klr>(hejmo de la <tld/>uloj)</klr>;
            </ekz>
            <ekz>
              <tld/>historio;
            </ekz>
            <ekz>
              <ind><tld/>homaro</ind>;
              <trd lng="ru">&c_p;&c_je;&c_r;&c_v;&c_o;&c_b;&c_y;&c_t;&c_n;&c_o;&c_je;
                &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
               <trd lng="pl">lud pierwotny</trd>
            </ekz>
            <ekz>
              <ind><tld/>tempo</ind>;
              <trdgrp lng="de">
                <trd>Urzeit</trd>,
                <trd>Vorzeit</trd>
              </trdgrp>
              <trd lng="nl">oertijd</trd>
              <trd lng="pl">czasy zamierzch&lstroke;e {pl}</trd>
              <trdgrp lng="ru">
                <trd>&c_d;&c_o;&c_i;&c_s;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_a;&c_ja; &c_e;&c_p;&c_o;&c_h;&c_a;</trd>,
                <trd>&c_p;&c_je;&c_r;&c_v;&c_o;&c_b;&c_y;&c_t;&c_n;&c_o;&c_je; &c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
              </trdgrp>
            </ekz>
            <ekz>
              <refgrp tip="vid">
                <ref cel="patr.pra0o"><tld/>patro</ref>,
                <ref cel="patr.pra0ino"><tld/>patrino</ref>
              </refgrp>,
            </ekz>
            <ekz>
              <ind><tld/>tipo</ind>.
              <trdgrp lng="de">
                <trd>Urtyp</trd>,
                <trd>Vormodell</trd>,
                <trd>Prototyp</trd>
              </trdgrp>
              <trd lng="nl">prototype</trd>
              <trd lng="pl">archetyp</trd>
              <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_o;&c_t;&c_i;&c_p;</trd>
            </ekz>
          </dif>
          <trdgrp lng="es">
            <trd>pre-<klr> (antiqu&iacute;simo, primitivo)</klr></trd>,
            <trd>proto-</trd>
          </trdgrp>
          <trdgrp lng="fr">
            <trd>pr&eacute;-<klr> (ancien, primitif)</klr></trd>,
            <trd>proto-</trd>
          </trdgrp>
          <trd lng="pl">pra-</trd>
          <trd lng="vo">&uuml;l-</trd>
          <trd lng="zh">&#x539F;&#x59CB; <pr>yu&#xE1;nsh&#x1D0;</pr></trd>
</snc>
        <snc mrk="pra.0.GEN">
          <uzo tip="fak">GEN</uzo>
          <dif>
            Sekva, pli malproksima en grado de parenceco ol la
            &jcirc;us anta&ubreve;a, tuj posta ol la indikita de la radikalo:
            <ekz>
              la <ind><tld/>nepo</ind> estas la filo de la nepo;
              <trd lng="pl">prawnuk</trd>
              <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_n;&c_u;&c_k;</trd>
            </ekz>
            <ekz>
              la <ind><tld/>avo</ind> estas la patro de la avo.
              <trdgrp lng="de">
                <trd>Urgro&szlig;vater</trd>,
                <trd>Uropa</trd>
              </trdgrp>
              <trd lng="nl">overgrootvader</trd>
              <trd lng="pl">pradziadek</trd>
              <trd lng="ru">&c_p;&c_r;&c_a;&c_d;&c_je;&c_d;</trd>
            </ekz>
          </dif>
          <trd lng="es">bis-<klr> (parentesco de cuarta
          generaci&oacute;n)</klr></trd>
          <trd lng="fr">arri&egrave;re-<klr> (parent&eacute;)</klr></trd>
          <trd lng="ru">&c_p;&c_r;&c_a;-</trd>
        </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_r;&c_a;-</trd>,
        <trd>&c_p;&c_je;&c_r;&c_sh;&c_a;-</trd>
      </trdgrp>
      <trd lng="de">Ur-</trd>
      <trd lng="pt">pr&eacute;-</trd>
        <trd lng="uk">&c_shch;&#x43E; &#x43F;&#x43E;&c_z;&#x43D;&c_a;&c_ch;&c_a;&c_jeu;</trd>
</drv>
    </subart>

    <subart>
      <dif>
        Samsignifa vortero.
      </dif>

      <drv mrk="pra.0a">
        <kap><tld/>a</kap>
        <snc mrk="pra.0a.HIS">
          <dif>
            Primitiva, anta&ubreve;historia, origina.
          </dif>
        </snc>
        <trdgrp lng="be">
          <trd>&c_p;&c_je;&c_r;&c_sh;&c_a;&c_b;&c_y;&c_t;&c_n;&c_y;</trd>,
          <trd>&c_d;&c_a;&c_g;&c_ib;&c_s;&c_t;&c_a;&c_r;&c_y;&c_ch;&c_n;&c_y;</trd>
        </trdgrp>
        <trd lng="bo">&#xF55;&#xF0B;&#xF60;&#xF56;&#xF74;&#xF53;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>velmi d&aacute;vn&yacute;</trd>,
    <trd>velmi star&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
          <trd>urzeitlich</trd>,
          <trd>vorzeitlich</trd>
        </trdgrp>
  <trdgrp lng="es">
          <trd>arcaico</trd>,
          <trd>ancestral</trd>
        </trdgrp>
  <trd lng="fr">archa&iuml;que</trd>
  <trdgrp lng="ja">
    <trd>&#x539F;&#x59CB;&#x306E;  <pr>&#12370;&#12435;&#12375;&#x306E;</pr></trd>,
    <trd>&#x592A;&#x53E4;&#x306E;  <pr>&#x305F;&#12356;&#12371;&#x306E;</pr></trd>
  </trdgrp>
  <trd lng="nl">voorhistorisch</trd>
  <trd lng="pl">dawny  <klr>(prastary)</klr></trd>
  <trdgrp lng="ru">
          <trd>&c_p;&c_je;&c_r;&c_v;&c_o;&c_b;&c_y;&c_t;&c_n;&c_y;&c_j;</trd>,
          <trd>&c_d;&c_o;&c_i;&c_s;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
        </trdgrp>
  <trdgrp lng="sk">
    <trd>prad&aacute;vny</trd>,
    <trd>starod&aacute;vny</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&c_r;&c_v;&c_ib;&c_s;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&c_i;&#x43C;&c_ib;&c_t;&c_i;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&c_d;&#x43E;&c_ib;&c_s;&c_t;&#x43E;&c_r;&c_i;&c_ch;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x8FDC;&#x53E4; <pr>yu&#x1CE;ng&#x1D4;</pr></trd>,
    <trd>&#36960;&#x53E4; <pr>yu&#x1CE;ng&#x1D4;</pr></trd>,
    <trd>&#x539F;&#x59CB; <pr>yu&#xE1;nsh&#x1D0;</pr></trd>
  </trdgrp>
</drv>

      <drv mrk="pra.0ulo">
        <kap><tld/>ulo</kap>
        <snc mrk="pra.0ulo.GEN">
          <uzo tip="fak">GEN</uzo>
          <dif>
            Individuo de kiu oni devenas, precipe tia apartenanta al
            malproksima generacio:
            <ekz>
              inter liaj <tld/>uloj estis kaj pastroj, kaj militistoj
              <fnt>
                <aut>D. Cibulevskij</aut>:
                <vrk>Ukrainaj kantoj kaj peterburgaj groteskoj</vrk>.
                <bib>Monato</bib>.
              </fnt>;
            </ekz>
            <ekz>
              la <ctl>natura</ctl> amo al la loko, kie oni naski&gcirc;is,
              kie estas entombigitaj la <tld/>uloj estas limigita al
              tre malvasta regiono kaj neniel al tuta lando
              <fnt>
                <aut>E. Lanti</aut>:
                <vrk><url ref="http://home.arcor.de/gmickle/skk/92ms_2.html">Letero pri la kvaza&ubreve;aj
                &leftquot;Aksiomoj&rightquot;</url></vrk>
              </fnt>;
            </ekz>
            <ekz>
              eble temas pri atavismo, reveno al tiu epoko, kiam
              la <tld/>ulo de l' homo ne estis ankora&ubreve; animalo
              socia kaj vivis izole en sia kaverno
              <fnt>
                <aut>A. &Ccirc;e&hcirc;ov, trad. M.Luka&scirc;</aut>, <vrk>Enujulo</vrk>,
                <lok>La Nica literatura revuo, 3:1</lok>
              </fnt>.
            </ekz>
          </dif>
          <ref tip="ant" cel="post.0eulo">posteulo</ref>
        </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_sh;&c_ch;&c_u;&c_r;</trd>,
    <trd>&c_r;&c_o;&c_d;&c_a;&c_p;&c_a;&c_ch;&c_y;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trd lng="cs">d&aacute;vn&yacute; p&rcaron;edek</trd>
  <trd lng="de">Vorfahre</trd>
  <trdgrp lng="en">
          <trd>ancestor</trd>,
          <trd>forebear</trd>
        </trdgrp>
  <trdgrp lng="es">
          <trd>ancestro</trd>,
          <trd>antepasado</trd>,
          <trd>ascendiente</trd>
        </trdgrp>
  <trd lng="fr">anc&ecirc;tre</trd>
  <trdgrp lng="ja">
    <trd>&#20808;&#31062;  <pr>&#x305B;&#12435;&#x305E;</pr></trd>,
    <trd>&#31062;&#20808;  <pr>&#x305D;&#x305B;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="nl">voorvader</trd>
  <trdgrp lng="pl">
    <trd>antenat</trd>,
    <trd>protoplasta</trd>,
    <trd>przodek</trd>
  </trdgrp>
  <trdgrp lng="ru">
          <trd>&c_p;&c_r;&c_je;&c_d;&c_o;&c_k;</trd>,
          <trd>&c_p;&c_r;&c_a;&c_shch;&c_u;&c_r;</trd>
        </trdgrp>
  <trd lng="sk">d&aacute;vny predok</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_a;&c_shch;&c_u;&c_r;</trd>,
    <trd>&c_d;&c_a;&#x43B;&c_je;&#x43A;&c_i;&c_j; &#x43F;&c_r;&c_je;&c_d;&#x43E;&#x43A;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#31062;&#x5B97; <pr>z&#x1D4;z&#x14D;ng</pr></trd>,
    <trd>&#31062;&#x8F29; <pr>z&#x1D4;b&#xE8;i</pr></trd>,
    <trd>&#31062;&#x8F88; <pr>z&#x1D4;b&#xE8;i</pr></trd>,
    <trd>&#x5B97; <pr>z&#x14D;ng</pr></trd>,
    <trd>&#31062; <pr>z&#x1D4;</pr></trd>,
    <trd>&#31062;&#20808; <pr>z&#x1D4;xi&amacron;n</pr></trd>,
    <trd>&#20808;&#x4EBA; <pr>xi&amacron;nr&#xE9;n</pr></trd>,
    <trd>&#20808; <pr>xi&amacron;n</pr></trd>
  </trdgrp>
</drv>
    </subart>
