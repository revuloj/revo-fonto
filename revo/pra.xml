</drv>
<drv mrk="pra.0ulo">
<kap><tld/>ulo</kap>
<snc>
<dif>
<tld/>avo.  
</dif>
</snc>
<trd lng="de">Vorfahre</trd>
<trd lng="nl">voorvader</trd>
</drv>
</subart>
