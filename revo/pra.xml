<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc> <rad>pra</rad>
</kap>
<subart>
  <dif>
    Prefikso montranta:
  </dif>
  <snc>
    <dif>
      Grandan primitivecon, longan anta&ubreve;tempecon:
      <ekz>
        <tld/>arbaro
        <klr>(ekzistanta de nememorebla tempo)</klr>;
      </ekz>
      <ekz>
        <tld/>hejmo
        <klr>(hejmo de anta&ubreve;uloj)</klr>;
      </ekz>
      <ekz>
        <tld/>historio;
      </ekz>
      <ekz>
        <ind><tld/>homaro</ind>;
        <trd lng="ru">&c_p;&c_je;&c_r;&c_v;&c_o;&c_b;&c_y;&c_t;&c_n;&c_o;&c_je; &c_ch;&c_je;&c_l;&c_o;&c_v;&c_je;&c_ch;&c_je;&c_s;&c_t;&c_v;&c_o;</trd>
      </ekz>
      <ekz>
        <ind><tld/>tempo</ind>;
        <trdgrp lng="de">
          <trd>Urzeit</trd>,
          <trd>Vorzeit</trd>
        </trdgrp>
        <trd lng="nl">oertijd</trd>
        <trdgrp lng="ru">
          <trd>&c_d;&c_o;&c_i;&c_s;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je; &c_v;&c_r;&c_je;&c_m;&c_ja;</trd>,
          <trd>&c_p;&c_je;&c_r;&c_v;&c_o;&c_b;&c_y;&c_t;&c_n;&c_o;&c_je; &c_v;&c_r;&c_je;&c_m;&c_ja;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <ind><tld/>tipo</ind>.
        <trdgrp lng="de">
          <trd>Urtyp</trd>,
          <trd>Vormodell</trd>,
          <trd>Prototyp</trd>
        </trdgrp>
        <trd lng="nl">prototype</trd>
        <trd lng="ru">&c_p;&c_r;&c_o;&c_t;&c_o;&c_t;&c_i;&c_p;</trd>
      </ekz>
    </dif>
    <trd lng="vo">&uuml;l-</trd>
  </snc>
  <snc>
    <dif>
      Pli malproksiman gradon de parenceco &ccirc;u anta&ubreve;a &ccirc;u posta:
      <ekz>
        la <ind><tld/>nepo</ind> estas la filo de la nepo;
        <trd lng="ru">&c_p;&c_r;&c_a;&c_v;&c_n;&c_u;&c_k;</trd>
      </ekz>
      <ekz>
        la <ind><tld/>avo</ind> estas la patro de la avo;
        <trdgrp lng="de">
          <trd>Urgro&szlig;vater</trd>,
          <trd>Uropa</trd>
        </trdgrp>
        <trd lng="nl">overgrootvader</trd>
        <trd lng="ru">&c_p;&c_r;&c_a;&c_d;&c_je;&c_d;</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la <ind><tld/>patroj</ind>
        <klr>(plej malproksimaj anta&ubreve;uloj)</klr>.
        <trdgrp lng="de">
          <trd>Vorfahren</trd>,
          <trd>Vorv&auml;ter</trd>
        </trdgrp>
        <trd lng="nl">voorvaderen</trd>
        <trd lng="ru">&c_p;&c_r;&c_a;&c_o;&c_t;&c_c;&c_y;</trd>
      </ekz>
    </dif>
    <trd lng="ru">&c_p;&c_r;&c_a;-</trd>
  </snc>
  <trd lng="de">Ur-</trd>
  <trd lng="pt">pr&eacute;-</trd>
</subart>

<subart>
  <dif>
    Samsignifa vortero.
  </dif>
  <drv mrk="pra.0a">
    <kap><tld/>a</kap>
    <snc>
      <dif>
        Primitiva, anta&ubreve;historia.
      </dif>
    </snc>
    <trdgrp lng="de">
      <trd>urzeitlich</trd>,
      <trd>vorzeitlich</trd>
    </trdgrp>
    <trd lng="nl">voorhistorisch</trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_je;&c_r;&c_v;&c_o;&c_b;&c_y;&c_t;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_d;&c_o;&c_i;&c_s;&c_t;&c_o;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
    </trdgrp>
  </drv>
  <drv mrk="pra.0ulo">
    <kap><tld/>ulo</kap>
    <snc>
      <dif>
        <tld lit="P"/>avo.
      </dif>
    </snc>
    <trd lng="de">Vorfahre</trd>
    <trd lng="nl">voorvader</trd>
    <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_o;&c_k;</trd>
  </drv>
</subart>
</art>
