<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: indivi1.xml,v 1.13 2021/04/14 07:00:36 revo Exp $">
<kap>
  <rad>individual</rad>/a
</kap>

<drv mrk="indivi1.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      <ref tip="dif" cel="indivi.0a">Individua</ref>:
      <ekz>
        la kernaj valoroj de R.I. (<nom>Rotary International</nom>) estas volontulado, amikeco,
        <tld/>a integreco kaj la kapablo organizi kaj estri
        <fnt>
          <vrk><url
          ref="https://rotaryclubdeesperantobrasil.org/2017/06/12/11-a-renkonto-de-la-rotary-club-de-esperanto-brasil/"
              >11-a Renkonto de la Rotary Club de Esperanto - Brasil</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la ludo estas <tld/>a kaj teama
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Gorodki"
              >Gorodki - Vikipedio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_y;&c_v;&c_ib;&c_d;&c_u;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trd lng="de">individuell</trd>
</drv>


<drv mrk="indivi1.0ismo">
  <kap><tld/>ismo <fnt><bib>PV</bib></fnt></kap>
  <snc>
    <dif>
      <ref tip="dif" cel="indivi.0ismo">Individuismo</ref>:
      <ekz>
        kristana humanismo estas la konvinko <klr>[&#8230;]</klr>, ke la homa libero kaj
        <tld/>ismo harmonias kun la kristanaj doktrino kaj praktiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=315778"
              >Kristana humanismo</url></lok>
        </fnt>.
      </ekz> 
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_y;&c_v;&c_ib;&c_d;&c_u;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
  <trd lng="de">Individualismus</trd>
  <trd lng="es">individualismo</trd>
  <trd lng="fr">individualisme</trd>
  <trd lng="hu">individualizmus</trd>
  <trd lng="nl">individualisme</trd>
  <trd lng="pt">individualismo</trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_i;&c_v;&c_i;&c_d;&c_u;&c_a;&c_l;&c_i;&c_z;&c_m;</trd>
  <trd lng="uk">&c_ib;&c_n;&c_d;&c_i;&c_v;&c_ib;&c_d;&c_u;&c_a;&c_l;&c_ib;&c_z;&c_m;</trd>
</drv>

<drv mrk="indivi1.0isto">
  <kap><tld/>isto <fnt><bib>PV</bib></fnt></kap>
  <snc>
    <dif>
      <ref tip="dif" cel="indivi.0isto">Individuisto</ref>:
      <ekz>
        kelkaj gravaj <tld/>istoj aperis, inter kiuj precipe
        <nom>Anton Bruckner</nom> (1824&ndash;1896) elstaras
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=261145">Romantisma muziko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_y;&c_v;&c_ib;&c_d;&c_u;&c_a;&c_l;&c_ib;&c_s;&c_t;</trd>
  <trd lng="bg">&c_i;&c_n;&c_d;&c_i;&c_v;&c_i;&c_d;&c_u;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
  <trd lng="de">Individualist</trd>
  <trd lng="es">individualista</trd>
  <trd lng="fr">individualiste</trd>
  <trd lng="hu">individualista</trd>
  <trd lng="nl">individualist</trd>
  <trd lng="pl">indywidualista</trd>
  <trd lng="pt">individualista</trd>
  <trd lng="ru">&c_i;&c_n;&c_d;&c_i;&c_v;&c_i;&c_d;&c_u;&c_a;&c_l;&c_i;&c_s;&c_t;</trd>
</drv>

</art>
