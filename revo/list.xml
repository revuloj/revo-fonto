<drv mrk="list.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="list.0igi.KOMUNE">
    <dif>
      Starigi <tld/>on de iuj aferoj, objektoj:
      <ekz>
        estas bone <tld/>igi tie &ccirc;i <klr>[&#8230;]</klr> tradukitajn verkojn
        <fnt><bib>Plumamikoj</bib>, <lok>tradukoj</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="vic.atendo0o">atendovico</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_ib; <ind>&c_s;&c_mol;&c_p;&c_ib;&c_s;</ind></trd>
  <trdgrp lng="cs">
    <trd>uspo&rcaron;&aacute;dat do seznamu</trd>,
    <trd>vyjmenovat</trd>
  </trdgrp>
  <trd lng="de">auflisten</trd>
  <trdgrp lng="es">
    <trd>enumerar</trd>,
    <trd>listar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>&eacute;num&eacute;rer</trd>,
    <trd>lister</trd>
  </trdgrp>
  <trd lng="pl">robi&cacute; <ind>list&eogonek;</ind></trd>
  <trd lng="ro">face o <ind>list&abreve;</ind></trd>
  <trd lng="sk">usporiada&tcaron; do zoznamu</trd>
</drv>

<drv mrk="list.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="list.en0igi.KOMUNE">
    <dif>
      Enmeti en <tld/>on:
      <ekz>
        li iris por en<tld/>igi sin inter laborser&ccirc;antoj
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        2a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_n;&c_je;&c_s;&c_mol;&c_c;&c_ib; &c_w; <ind>&c_s;&c_mol;&c_p;&c_ib;&c_s;</ind></trd>
  <trdgrp lng="cs">
    <trd>zapsat do seznamu</trd>,
    <trd>za&rcaron;adit do seznamu</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>agregar a la <ind>lista</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>ajouter &agrave; la <ind>liste</ind></trd>
  </trdgrp>
  <trd lng="pl">wprowadzi&cacute; na <ind>list&eogonek;</ind></trd>
  <trd lng="ro">pune pe <ind>list&abreve;</ind></trd>
  <trd lng="sk">zap&iacute;sa&tcaron; do zoznamu</trd>
</drv>

<drv mrk="list.atendo0o">
  <kap>
    atendo<tld/>o
  </kap>
  <snc>
    <dif>
      <tld lit="L"/>o en kiun enskribi&gcirc;as atendantoj kaj la&ubreve; kies ordo ili estas servataj:
      <ekz>
        kiam la publika financado redukti&gcirc;as, la servoj transformi&gcirc;as al atendo<tld/>oj
        <fnt>
          <bib>MD08</bib>
          <vrk>En Rusujo, oni nun vivas malpli longe ol en 1990</vrk>
        </fnt>;
      </ekz>
      <ekz>
        li estas sur la atendo<tld/>o de pli ol dudek jaroj
        <fnt>
          <bib>MD08</bib>
          <vrk>Dek Kvar Jaroj da Demokratio kaj Longa Atendo</vrk>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; en la &ccirc;efurbo la kutima, fiksa telefonservo estas celobjekto de plendoj, &ccirc;ar
        la atendo<tld/>o de dezirantoj havi propran, hejman telefonaparaton &ccirc;iam
        kreskas
        <fnt>
          <bib>Monato</bib>
          <aut>Alberto Garc&iacute;a Fumero</aut>
          <vrk><url ref="https://www.monato.be/2009/009707.php"
              >Servo alternative fiksita</url></vrk>
          <lok>2009</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ni]</klr> aran&gcirc;is krediton por a&ccirc;eti sian novan <nom>Mazda</nom>, <klr>[&#8230;]</klr> en
        1980 oni devis registri sin sur la atendo<tld/>o por a&ccirc;eti &gcirc;in, ni havis la
        prioritaton &ccirc;ar mia avo estis malsanulo
        <fnt>
          <bib>MD14</bib>
          <vrk>La a&ubreve;tomobilo, instiganto de &leftquot;etbur&gcirc;aj deziroj&rightquot;</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Warteliste</trd>
  <trd lng="en">waiting list</trd>
</drv>


<drv mrk="list.dissendo0o">
  <kap>dissendo<tld/>o</kap>
  <snc mrk="list.dissendo0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <ref cel="forum.0o.komp">Forumo</ref> en
      <ref tip="malprt" cel="ret.komputila0o">komputila reto</ref>
      funkcianta tiel, ke la mesa&gcirc;ojn senditajn al speciala retadreso
      tiea servilo a&ubreve;tomate plusendas al la <ctl>listanoj</ctl>,
      a&ubreve; <ctl>abonantoj</ctl> (kies retadresojn &gcirc;i prenas en la
      koncerna <ctl>dissenda listo</ctl>):
      <ekz>
        informoj pri la dissendo<tld/>o de ReVo troveblas &ccirc;e
        <fnt><url>http://www.uni-leipzig.de/esperanto/voko/revo/dok/revolist.html</url></fnt>
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_mol;&c_p;&c_ib;&c_s; &c_r;&c_a;&c_s;&c_s;&c_y;&c_l;&c_k;&c_ib;</trd>
  <trd lng="cs">seznam adres k rozesl&aacute;n&iacute;</trd>
  <trdgrp lng="de">
    <trd>Mailliste</trd>,
    <trd>Versandliste</trd>
  </trdgrp>
  <trd lng="el">&lambda;&jota_ton;&sigma;&tau;&alfa; &tau;&alfa;&chi;&ypsilon;&delta;&rho;&omikron;&my;&epsilon;&jota;&omikron;&ypsilon_ton;</trd>
  <trd lng="en">mailing list</trd>
  <trd lng="es">lista de distribuci&oacute;n</trd>
  <trd lng="fr">liste de diffusion</trd>
  <trd lng="he">&resh;&shin;&yod;&mem;&tav; &tav;&pe;&vav;&tsadi;&he;</trd>
  <trd lng="hu">levelez&eacute;si lista</trd>
  <trd lng="nl">verzendlijst</trd>
  <trdgrp lng="pl">
    <trd>lista adresowa</trd>,
    <trd>lista dystrybucyjna</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>list&abreve; de expediere po&scedil;tal&abreve;</trd>,
    <trd>list&abreve; de email-uri</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_p;&c_i;&c_s;&c_o;&c_k; &c_r;&c_a;&c_s;&c_s;&c_y;&c_l;&c_k;&c_i;</trd>
  <trdgrp lng="sk">
    <trd>mailov&aacute; <ind>konferencia</ind></trd>,
    <trd>zoznam adries na rozoslanie</trd>
  </trdgrp>
</drv>


<drv mrk="list.ligil0o">
  <kap>ligil<tld/>o</kap>
  <snc mrk="list.ligil0o.KOMP">
    <uzo tip="fak">KOMP</uzo>
    <dif>
      <tld lit="L"/>o realigita tiel, ke &ccirc;iu &gcirc;ia ano entenas
      ligilon (referencon) al la tujsekva ano (unudirekta
      ligil<tld/>o), a&ubreve; du ligilojn al la tujsekva ano kaj al la
      &jcirc;usanta&ubreve;a (amba&ubreve;direkta ligil<tld/>o). La lasta listano
      povas havi nulan (vakuan) referencon, a&ubreve; referenci la
      komencon (ringa ligil<tld/>o).
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>seznam dat</trd>,
    <trd>soubor dat</trd>
  </trdgrp>
  <trd lng="de">verkettete <ind>Liste</ind></trd>
  <trd lng="en">linked list</trd>
  <trd lng="es">lista enlazada</trd>
  <trd lng="he">&resh;&shin;&yod;&mem;&he; &mem;&qof;&vav;&shin;&resh;&tav;</trd>
  <trdgrp lng="hu">
    <trd>kapcsolt lista</trd>,
    <trd>l&aacute;ncolt lista</trd>
  </trdgrp>
  <trd lng="pl">lista po&lstroke;&aogonek;czona</trd>
  <trd lng="pt">lista encadeada</trd>
  <trd lng="ro">list&abreve; legat&abreve;</trd>
  <trd lng="ru">&c_c;&c_je;&c_p;&c_n;&c_o;&c_j; &c_s;&c_p;&c_i;&c_s;&c_o;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>sp&aacute;jan&yacute; <ind>zoznam</ind></trd>,
    <trd>zoznam d&aacute;t</trd>
  </trdgrp>
</drv>

<drv mrk="list.nigra_0o">
  <kap>
    nigra <tld/>o
  </kap>
  <snc>
    <dif>
      Listo de malpermesitaj, sankciumitaj aferoj:
      <ekz>
        trafinte en nigran <tld/>on, en Germanio ne facile oni ricevas laboron
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; nigra <tld/>o de &jcirc;urnalistoj <klr>[&#8230;]</klr> taksitaj tro kritikemaj estis
        farita por malebligi al ili aliron al grandaj &jcirc;urnaloj
        <fnt>
          <bib>MD17</bib>
          <vrk>En Sud-Koreio, la fino de la dinastio Bak</vrk>
        </fnt>;
      </ekz>
