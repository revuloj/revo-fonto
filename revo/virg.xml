<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>virg</rad>/a
</kap>
<drv mrk="virg.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc>
    <dif>
      Ne ankora&ubreve; spertinta seksan kuni&gcirc;on:
      <ekz>
        <ind>la sankta <tld/>ulino</ind><fnt>Z</fnt>;
        <trd lng="ru">&c_s;&c_v;&c_ja;&c_t;&c_a;&c_ja; &c_d;&c_je;&c_v;&c_a;</trd>
      </ekz>
      <ekz>
        konservi sian <ind><tld/>econ</ind>;
        <trd lng="ru">&c_d;&c_je;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
      </ekz>
      <ekz>
        mal<tld/>igi knabinon.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Ne tu&scirc;ita, ne esplorita, nekulturita:
      <ekz>
        <tld/>a tero<fnt>B</fnt>, arbaro<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pra">pra</ref>.
    </refgrp>
    <refgrp tip="sin">
      <ref cel="krud.0a">kruda</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Sendifekta, pura:
      <ekz>
        la <tld/>a blankeco de la muroj<fnt>B</fnt>;
      </ekz>
      <ekz>
        super Betanio pendis la <tld/>a bluo de l'
        &ccirc;ielo<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Senpeka:
      <ekz>
        <tld/>a koro.
      </ekz>
    </dif>
    <trd lng="de">keusch</trd>
  </snc>
  <trd lng="de">jungfr&auml;ulich</trd>
  <trd lng="en">virginal</trd>
  <trd lng="nl">maagdelijk</trd>
  <trd lng="ru">&c_d;&c_je;&c_v;&c_s;&c_t;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
</drv>

<drv mrk="virg.V0ulino">
  <kap><tld lit="V"/>ulino <fnt>(<vrk>Plena Vortaro</vrk>,
  <lok>konstelacio</lok>)</fnt></kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro kaj signo de la <ref tip="malprt"
      cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Virgo</trd>).
    </dif>
  </snc>
  <trd lng="de">Jungfrau</trd>
  <trd lng="nl">maagd</trd>
  <trd lng="pl">panna</trd>
  <trd lng="ru">&c_D;&c_je;&c_v;&c_a;</trd>
</drv>
</art>
