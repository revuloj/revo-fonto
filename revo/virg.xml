<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>virg</rad>/a
</kap>
<drv mrk="virg.0a">
  <kap><ofc>*</ofc><tld/>a</kap>
  <snc num="1">
    <dif>
      Ne ankora&ubreve; spertinta seksan kuni&gcirc;on:
      <ekz>
        la sankta <tld/>ulino<fnt>Z</fnt>;
      </ekz>
      <ekz>
        konservi sian <tld/>econ;
      </ekz>
      <ekz>
        mal<tld/>igi knabinon.
      </ekz>
    </dif>
  </snc>
  <snc num="2">
    <uzo tip="klr">(f)</uzo>
    <dif>
      Ne tu&scirc;ita, ne esplorita, nekulturita:
      <ekz>
        <tld/>a tero<fnt>B</fnt>, arbaro<fnt>B</fnt>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="pra">pra</ref>.
    </refgrp>
    <refgrp tip="sin">
      <ref cel="krud.0a">kruda</ref>.
    </refgrp>
  </snc>
  <snc num="3">
    <dif>
      Sendifekta, pura:
      <ekz>
        la <tld/>a blankeco de la muroj<fnt>B</fnt>;
      </ekz>
      <ekz>
        super Betanio pendis la <tld/>a bluo de l'
        &ccirc;ielo<fnt>B</fnt>.
      </ekz> 
    </dif>
  </snc>
  <snc num="4">
    <dif>
      Senpeka:
      <ekz>
        <tld/>a koro.
      </ekz>
    </dif>
    <trd lng="de">keuch</trd>
  </snc>
  <trd lng="de">jungfr&auml;ulich</trd>
  <trd lng="en">virginal</trd>
</drv>
<drv mrk="virg.V0ulino">
  <kap><tld lit="V"/>ulino</kap>
  <uzo tip="fak">AST</uzo>
  <snc>
    <dif>
      Stelaro de la <ref tip="malprt" cel="zodiak.0o">zodiako</ref>
      (<trd lng="la">Virgo</trd>), tra kiu la suno pasas de la 22a de
      a&ubreve;gusto &gcirc;is la 23a de septembro.
    </dif>
  </snc>
  <trd lng="de">Jungfrau</trd>
</drv>
</art>
<!--
$Log$
-->
</vortaro>