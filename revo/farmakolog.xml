<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: farmakolog.xml,v 1.8 2023/10/14 18:06:40 revo Exp $">
<kap>
  <rad>farmakolog</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="farmakolog.0o">
  <kap><tld/>o</kap>
  <snc mrk="farmakolog.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="vid" cel="farmak.0o.MED">Farmakologi</ref>isto,
      fakulo pri <ref tip="vid" cel="medika1.0o.MED">medikamentoj</ref>:
      <ekz>
        granda kuracisto kaj <tld/>o Li Shizhen en sia
        <ctl>Kompendio de Medicinaj Materialoj</ctl>
        (1578) aldonis pli ol dek receptojn pri drogteo
        <fnt>
          <aut>Cui Zhiqin</aut>,
          <vrk><url ref="http://www.ina-asocio.info/pdf/teotrinkado.html.pdf">
          Teoterapio kaj Antisenileco</url></vrk>,
          <lok>2-a INA-konferenco &Scirc;anhajo, 2005</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_r;&c_m;&c_a;&c_k;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="ca">farmac&ograve;leg</trd>
  <trd lng="cs">farmakolog</trd>
  <trd lng="fr">pharmacologue</trd>
  <trd lng="ja">&#x85AC;&#x7406;&#x5B66;&#x8005;  <pr>&#x3084;&#x304F;&#x308A;&#x304C;&#x304F;&#x3057;&#x3083;</pr></trd>
  <trdgrp lng="sk">
    <trd>farmakol&#xF3;g</trd>,
    <trd>lek&#xE1;rnik</trd>
  </trdgrp>
  <trd lng="uk">&c_f;&c_a;&c_r;&#x43C;&c_a;&#x43A;&#x43E;&#x43B;&#x43E;&c_g;</trd>
</drv>
</art>

