<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: procion.xml,v 1.15 2024/02/17 07:52:36 revo Exp $">
<kap>
   <rad>procion</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="procion.0o">
   <kap><tld/>o</kap>
  <snc mrk="procion.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="urs.lav0o.ZOO">Lavurso</ref>:
      <ekz>
         du <tld/>aj feloj
         <fnt><bib>RugxDom</bib>, <lok>&ccirc;apitro 105a, volumo 3a, p.
         367a</lok></fnt>;
      </ekz>
      <ekz>
        en sova&gcirc;a naturo tiuj estas vulpoj, lupoj, &scirc;akaloj, <tld/>oj, skunkoj, vespertoj
        kaj aliaj
        <fnt>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2008/009550.php">Batalo kontra&ubreve; mortiga malsano</url></vrk>
              <bib>Monato</bib>
          <lok>2008/08, p. 26</lok>
        </fnt>;
      </ekz>
      <ekz>
        lordo <nom>Ailsa</nom> malsa&gcirc;e enkondukis tien kiel eksperimenton anka&ubreve; melojn kaj <tld/>ojn
        <fnt>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2004/008289.html">Mistera insulo Ailsa Craig</url></vrk>
              <bib>Monato</bib>
          <lok>2004/07, p. 22</lok>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="be">&c_ja;&c_n;&c_o;&c_t;</trd>
  <trd lng="bg">&c_m;&c_i;&c_je;&c_shch;&c_a; &c_m;&c_je;&c_ch;&c_k;&c_a;</trd>
  <trd lng="ca">&oacute;s rentador</trd>
  <trdgrp lng="cs">
    <trd>medv&iacute;dek m&yacute;val</trd>,
    <trd>m&yacute;val severn&iacute;</trd>
  </trdgrp>
  <trd lng="de">Waschb&auml;r</trd>
  <trd lng="en">raccoon</trd>
  <trd lng="es">mapache</trd>
  <trd lng="fr">raton laveur</trd>
  <trdgrp lng="he">
     <trd>&dalet;&bet;&yod;&bet;&vav;&fnun;</trd>,
     <trd>&resh;&qof;&vav;&fnun;</trd>
   </trdgrp>
  <trd lng="hu">mos&oacute;medve</trd>
  <trd lng="la">Procyon lotor</trd>
  <trd lng="nl">wasbeer</trd>
  <trdgrp lng="pt">
    <trd>racum</trd>,  
    <trd>m&atilde;o-pelada</trd>,
   </trdgrp>
  <trd lng="ru">&c_je;&c_n;&c_o;&c_t;</trd>
  <trd lng="sk">m&yacute;val severn&yacute;</trd>
  <trd lng="uk">&c_jeu;&c_n;&c_o;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x6D63;&#x718A; <pr>hu&#x1CE;nxi&#xF3;ng</pr></trd>,
    <trd>&#x6D63;&#x718A; <pr>hu&#xE0;nxi&#xF3;ng</pr></trd>
  </trdgrp>
</drv>
</art>

