      <trd><baz>singkap</baz>tersingkap</trd>,
      <trd><baz>ungkap</baz>terungkap</trd>
    </trdgrp>
    <trd lng="nl">openbaar</trd>
    <trdgrp lng="pl">
      <trd>jawny</trd>,
      <trd><klr>og&oacute;lnie, powszechnie </klr>znany</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ja;&c_v;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <trd lng="br">diguzh</trd>
  <trd lng="es">indisimulado</trd>
  <trdgrp lng="fr">
    <trd>manifeste<klr tip="ind"> (sans dissimulation)</klr></trd>,
    <trd>ouvert<klr tip="ind"> (sans dissimulation)</klr></trd>,
    <trd>patent<klr tip="ind"> (sans dissimulation)</klr></trd>,
    <trd>transparent<klr tip="ind"> (sans dissimulation)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_ja;&c_v;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_d;&c_k;&c_r;&c_i;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x723D;&#x5FEB; <pr>shu&#x1CE;ngkuai</pr></trd>,
    <trd>&#x723D;&#26391; <pr>shu&#x1CE;ngl&#x1CE;ng</pr></trd>
