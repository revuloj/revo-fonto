  </trdgrp>
  <trd lng="zh">&#20599;&#20599;&#22320; <pr>t&#x14D;ut&#x14D;ude</pr></trd>
</drv>

<drv mrk="kasx.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="kasx.0ejo.KOMUNE">
    <dif>
      Loko, kie oni <tld/>as sin a&ubreve; ion:
      <ekz>
        eksciu &ccirc;iujn <tld/>ejojn, kie li sin <tld/>as
        <fnt>
          <bib>MT</bib>
          <lok>I. Samuel 23:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu havos en la kaverno sian <tld/>ejon
        <fnt><bib>Gerda</bib>, <lok>&ccirc;apitro 22a</lok></fnt>;
      </ekz>
      <ekz>
        kiam oni ekbruligis lampon, oni metas &gcirc;in ne en <tld/>ejon <klr>[&#8230;]</klr> sed
        sur la lampingon, por ke la enirantoj povu vidi la lumon
        <klr>(t.e. oni montru sian kredon, sian enlumi&gcirc;on)</klr>
        <fnt>
          <bib>NT</bib>
          <lok>Luko 11:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        eltreni uron el &gcirc;ia <tld/>ejo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiom da fojoj li vizitis la solecajn <tld/>ejojn inter rokoj kaj arbegoj
        <fnt>
          <bib>Tor</bib>
          <lok>II.</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="embusk.0ejo">embuskejo</ref>,
      <ref cel="rifugx.0ejo">rifu&gcirc;ejo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_h;&c_o;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>koach</trd>,
    <trd>kuzhiadell</trd>,
    <trd>toull-kuzh</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Versteck</trd>,
    <trd>Unterschlupf</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hiding place</trd>,
    <trd>hide <klr>(brite)</klr></trd>
  </trdgrp>
  <trd lng="es">escondite</trd>
  <trdgrp lng="fr">
    <trd>cachette</trd>,
    <trd>cache<klr tip="amb"> (cachette)</klr></trd>,
    <trd>planque</trd>
  </trdgrp>
  <trd lng="hu">rejtekhely</trd>
  <trd lng="id"><baz>sembunyi</baz>persembunyian</trd>
  <trdgrp lng="ja">
    <trd>&#x96A0;&#x308C;&#x5BB6;  <pr>&#x304B;&#x304F;&#x308C;&#x304C;</pr></trd>,
    <trd>&#x96A0;&#12375;&#22580;&#25152;  <pr>&#x304B;&#x304F;&#12375;&#12400;&#12375;&#12423;</pr></trd>
  </trdgrp>
  <trd lng="nl">schuilplaats</trd>
  <trdgrp lng="pl">
