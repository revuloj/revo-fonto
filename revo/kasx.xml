<drv mrk="kasx.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="kasx.0ema.KOMUNE">
    <dif>
      Havanta inklinon por <tld/>i siajn pensojn;
      <ref tip="sin" cel="sekret.0ema">sekretema</ref>:
      <ekz>
        &scirc;i devis esti pli forta, pli <tld/>ema, pli sindetenema
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        montru al mi <klr>[&#8230;]</klr> homon &dash; miraklon, kiu ne konas &scirc;ajnigon, <tld/>emon,
        koketecon, flatemon
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>!
      </ekz>
      <ekz>
        <tld/>emulo, ci ne diris al mi, ke cia edzino estas graveda
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Dua</lok>
        </fnt>!
      </ekz>
      <ekz>
        amindumadis lin kvaza&ubreve; <tld/>eme la sinjorinoj, kaj li, anka&ubreve; <tld/>eme,
        malestimadis ilin
        <fnt>
          <bib>DKM</bib>
          <lok>&Ccirc;apitro LXII</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj rigardoj <tld/>eme ser&ccirc;as iun signon de sekreta interkompreno en la okuloj
        de la knabinoj
        <fnt>
          <bib>Tor</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        tie kaj tie grupetoj da junuloj <tld/>eme nestas post arbetoj
        <fnt>
          <bib>Tor</bib>
          <lok>I.</lok>
        </fnt>;
      </ekz>
      <ekz>
        Jo&ccirc;jo havis karakteron <tld/>eman kaj li &gcirc;enerale
        nevolonte lasis scii iun pri siaj sekretoj
        <fnt>
          <aut>I. G. &Scirc;irjaev</aut>,
          <vrk>Sen titolo</vrk>,
          <lok>[1898?]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_r;&c_y;&c_t;&c_n;&c_y;</trd>
  <trdgrp lng="br">
    <trd>kuzhmuzik</trd>,
    <trd>kuzhuter</trd>
  </trdgrp>
  <trd lng="de">verschwiegen</trd>
  <trd lng="en">secretive</trd>
  <trdgrp lng="es">
    <trd>disimulado</trd>,
    <trd>discreto</trd>,
    <trd>hip&oacute;crita</trd>,
    <trd>farsante</trd>,
  </trdgrp>
  <trdgrp lng="fr">
    <trd>cachottier<klr tip="ind"> (adj.)</klr></trd>,
    <trd>dissimulateur<klr tip="ind"> (adj.)</klr></trd>,
    <trd>dissimul&eacute;<klr tip="ind"> (caract&egrave;re)</klr></trd>,
    <trd>secret<klr tip="ind"> (cachottier)</klr></trd>,
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rejt&odblac;zk&ouml;d&odblac;</trd>,
    <trd>hallgatag</trd>,
    <trd>titkol&oacute;dz&oacute;</trd>,
  </trdgrp>
  <trd lng="id">penuh <ind>rahasia</ind></trd>
  <trdgrp lng="ja">
