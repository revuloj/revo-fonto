<drv mrk="kasx.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kasx.mal0i.KOMUNE">
    <dif>
      <ref tip="sin" cel="kon.dis0igi">Diskonigi</ref>, klare sciigi <tld/>ita&jcirc;on:
      <ekz>
        &scirc;i mal<tld/>os ilian perfidon
        <fnt>
          <bib>ElektFab</bib>
          <lok>La ora birdo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Johano forprenis la fojnon por mal<tld/>i la kamaradon
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni ne &ccirc;ion povis mal<tld/>i al mi, kaj el tiu malmulto, kiun mi scias, vi
        ekscios nur malmulte
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        sian intencon Li mal<tld/>as al la piuloj
        <fnt><bib>MT</bib>, <lok>&Sen; 3:32</lok></fnt>;
      </ekz>
      <ekz>
        kiu mal<tld/>as sekreton, tiu estas kiel kalumnianto
        <fnt><bib>MT</bib>, <lok>&Sen; 20:19</lok></fnt>;
      </ekz>
      <ekz>
        lia fratino el kompato jam longe iom post iom mal<tld/>is al li la veron
        <fnt>
          <bib>BdV</bib>
          <lok>La Batalo de l' Vivo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Selma</nom> <klr>[&#8230;]</klr> mal<tld/>is siajn monajn manipulojn
        <fnt>
          <bib>HsT</bib>
          <lok>Kuni&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la tempo &ccirc;iam mal<tld/>as la veron
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="aper.0igi">aperigi</ref>,
      <ref cel="kovr.mal0i">malkovri</ref>,
      <ref cel="manife.0i">manifesti</ref>,
      <ref cel="montr.0i">montri</ref>,
      <ref cel="publik">publikigi</ref>,
      <ref cel="revela.0i">revelacii</ref>,
      <ref cel="mask.sen0igi">senmaskigi</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_y;&c_ja;&c_v;&c_ib;&c_c;&c_mol;</trd>,
    <trd>&c_v;&c_y;&c_k;&c_r;&c_y;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_a;&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
