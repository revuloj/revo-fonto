<drv mrk="kasx.0ludo">
  <kap><tld/>ludo</kap>
  <snc mrk="kasx.0ludo.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Infana ludo, en kiu unu ludanto ser&ccirc;as aliajn,
      <tld/>i&gcirc;intajn ludantojn:
      <ekz>
        <klr>[ili]</klr> <tld/>ludis en mallumaj lokoj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la suno brilis <tld/>lude malanta&ubreve; bran&ccirc;oj kaj trunkoj, oblikvaj radifaskoj
        penetris sub la arbokronojn
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la celo de niaj deziroj <klr>[&#8230;]</klr> ludas <tld/>ludon kun ni
        <fnt><bib>Mortul&Scirc;ip</bib>, <lok>enkonduko 1, de la
        tradukinto</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_o;&c_v;&c_a;&c_n;&c_k;&c_ib;</trd>
