      <trd>expl&iacute;cito</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>nyilv&aacute;nos</trd>,
      <trd>nem titkolt</trd>,
      <trd>pal&aacute;stolatlan</trd>
