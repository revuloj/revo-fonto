<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>ka&scirc;</rad>/i
</kap>
<drv mrk="kasx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kasx.0i.okulo">
    <dif>
      Forigi ion de ies okuloj, metante &gcirc;in en sekretan lokon,
      a&ubreve; kovrante &gcirc;in per io: 
      <ekz>oni <tld/>is lin de Atalja<fnt>Z</fnt>;</ekz>
      <ekz>
        &scirc;i forkuris kaj <tld/>is sin en la plej proksima arbaro<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i oran ringon por ke oni &gcirc;in ne trovu<fnt>Z</fnt>;
      </ekz>
      <ekz>fremdan vundon <tld/>as vesto
        <klr>(ni ne scias pri la suferoj de proksimulo)</klr><fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>ne&gcirc;o <tld/>as nur &gcirc;is printempo
        <klr>(nenio restas longe sekreta)</klr><fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        la lumo estas <tld/>ata al mi per la kurtenoj<fnt>Z</fnt>;
      </ekz>
      <ekz><tld/>i al si la viza&gcirc;on post la manoj;</ekz>
      <ekz>&jcirc;eti <tld/>itan rigardon en libron<fnt>Z</fnt></ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mask.0i">maski</ref>,
      <ref cel="kovr.0i">kovri</ref>,
      <ref cel="sxlos.0i">&scirc;losi</ref>,
      <ref cel="vual.0i">vuali</ref>.
    </refgrp>
  <trdgrp lng="de">
     <trd>verstecken</trd>,
     <trd>verbergen</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>elrejt</trd>,
     <trd>eldug</trd>
  </trdgrp>
  </snc>
  <snc mrk="kasx.0i.scio">
    <dif>
      Forigi ion de ies scio, ne dirante a&ubreve; ne esprimante tion:
      <ekz><tld/>i aferon, siajn projektojn, siajn pensojn;</ekz>
      <ekz>sa&gcirc;ulo <tld/>as sian ofendon<fnt>Z</fnt>;</ekz>
      <ekz>sa&gcirc;a homo <tld/>as scion<fnt>Z</fnt>;</ekz>
      <ekz>tiuj belaj paroloj ne povas <tld/>i la fakton;</ekz>
      <ekz><tld/>i ion anta&ubreve; iu<fnt>Z</fnt>;</ekz>
      <ekz>sin <tld/>i de iu;</ekz>
      <ekz>
        pli bona estas ripro&ccirc;o ne<tld/>ita ol amo <tld/>ita<fnt>Z</fnt>;
      </ekz>
      <ekz>akcepti <tld/>itajn donacojn<fnt>Z</fnt>;</ekz>
      <ekz>tuso kaj amo ne estas <tld/>eblaj<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>senco<tld/>aj (enigmaj) frazoj</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="ignor.0i">ignori</ref>,
      <ref cel="sekret.0i">sekreti</ref>,
      <ref cel="mensog.0i">mensogi</ref>,
      <ref cel="sufok.0i">sufoki</ref>,
      <ref cel="silent">forsilenti</ref>,
      <ref cel="hipokr.0i">hipokriti</ref>.
    </refgrp>
  <trdgrp lng="be">
     <trd>&c_h;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
     <trd>&c_u;&c_t;&c_o;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>vertuschen</trd>,
     <trd>verbergen</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>pal&aacute;stol</trd>,
     <trd>elleplez</trd>
  </trdgrp>
  </snc>
  <trd lng="en">hide</trd>
  <trd lng="nl">verbergen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_ja;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_u;&c_t;&c_a;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sv">
    <trd>g&ouml;mma</trd>,
    <trd>d&ouml;lja</trd>
  </trdgrp>
</drv>
<drv mrk="kasx.0ite">
  <kap><tld/>(it)e</kap>
  <snc mrk="kasx.0ite.kasxante">
    <dif>
      Sin <tld/>ante: 
      <ekz>kio vendi&gcirc;as <tld/>ite, vendi&gcirc;as plej profite<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <trdgrp lng="be">
      <trd>&c_k;&c_r;&c_a;&c_d;&c_k;&c_o;&c_m;</trd>,
      <trd>&c_u;&c_k;&c_r;&c_a;&c_d;&c_k;&c_a;&c_m;</trd>,
      <trd>&c_u;&c_p;&c_o;&c_t;&c_a;&c_j;</trd>,
      <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_n;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_n;&c_o;</trd>,
      <trd>&c_t;&c_a;&c_j;&c_k;&c_o;&c_m;</trd>
    </trdgrp>
  <trdgrp lng="de">
     <trd>versteckt</trd>,
     <trd>verborgen</trd>
  </trdgrp>
  <trdgrp lng="hu">
     <trd>rejtve</trd>,
     <trd>eldugva</trd>
  </trdgrp>
  </snc>
  <snc mrk="kasx.0ite.nesprimante">
    <dif>
      Neesprimante. 
    </dif>
    <trdgrp lng="be">
      <trd>&c_u;&c_p;&c_o;&c_t;&c_a;&c_j;</trd>,
      <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_a;</trd>,
      <trd>&c_n;&c_ja;&c_ja;&c_w;&c_n;&c_a;</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_o;</trd>,
      <trd>&c_n;&c_je;&c_ja;&c_v;&c_n;&c_o;</trd>
    </trdgrp>
    <trd lng="hu">rejtve</trd>
  </snc>
</drv>
<drv mrk="kasx.0ejo">
  <kap><tld/>ejo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Loko, kie oni <tld/>as sin a&ubreve; ion. 
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_h;&c_o;&c_w;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Versteck</trd>
  <trd lng="hu">rejtekhely</trd>
  <trdgrp lng="ru">
    <trd>&c_t;&c_a;&c_j;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_n;&c_o;&c_je; &c_u;&c_b;&c_je;&c_zh;&c_i;&c_shch;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">g&ouml;mst&auml;lle</trd>
</drv>
<drv mrk="kasx.0ema">
  <kap><tld/>ema<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Havanta inklinon por <tld/>i siajn pensojn; sekretama. 
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_k;&c_r;&c_y;&c_t;&c_n;&c_y;</trd>
  <trd lng="de">verschwiegen</trd>
  <trdgrp lng="hu">
    <trd>rejt&odblac;zk&ouml;d&odblac;</trd>,
    <trd>hallgatag</trd>,
    <trd>titkol&oacute;dz&oacute;</trd>,
  </trdgrp>
  <trd lng="ru">&c_s;&c_k;&c_r;&c_y;&c_t;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="kasx.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Objekto malanta&ubreve; kiu oni <tld/>as sin a&ubreve; ion 
    </dif>
    <refgrp tip="sin">
      <ref cel="kovr.0o">kovrilo</ref>,
      <ref cel="ekran.0o">ekrano</ref>,
      <ref cel="pretek.0o">preteksto</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="hu">
    <trd>&aacute;lca</trd>,
    <trd>fedez&eacute;k</trd>
  </trdgrp>
</drv>
<drv mrk="kasx.0ludo">
  <kap><tld/>ludo</kap>
  <snc>
    <dif>
      Infana ludo, en kiu unu ludanto ser&ccirc;as aliajn, ka&scirc;i&gcirc;intajn
      ludantojn.
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_o;&c_v;&c_a;&c_n;&c_k;&c_ib;</trd>
  <trd lng="de">Verstecken</trd>
  <trd lng="hu">b&uacute;j&oacute;cska</trd>
  <trd lng="ru">&c_p;&c_r;&c_ja;&c_t;&c_k;&c_i;</trd>
</drv>
<drv mrk="kasx.mal0i">
  <kap>mal<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Diskonigi, klare sciigi <tld/>ita&jcirc;on a&ubreve; <tld/>inda&jcirc;on:
      <ekz>la tempo &ccirc;iam mal<tld/>as la veron<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>sian intencon Dio mal<tld/>as al la piuloj<fnt>Z</fnt>;</ekz>
      <ekz>kiu mal<tld/>as sekreton estas kiel kalumnianto<fnt>Z</fnt></ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kon.re0i">rekoni</ref>,
      <ref cel="konfes.0i">konfesi</ref>,
      <ref cel="montr.0i">montri</ref>,
      <ref cel="aper.0igi">aperigi</ref>,
      <ref cel="revela.0i">revelacii</ref>,
      <ref cel="publik">publikigi</ref>,
      <ref cel="kovr.mal0i">malkovri</ref>,
      <ref cel="manife.0i">manifesti</ref>,
      <ref cel="vual">senvualigi</ref>,
      <ref cel="mask.sen0igi">senmaskigi</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
     <trd>&c_v;&c_y;&c_ja;&c_v;&c_ib;&c_c;&c_mol;</trd>,
     <trd>&c_v;&c_y;&c_k;&c_r;&c_y;&c_c;&c_mol;</trd>,
     <trd>&c_p;&c_a;&c_k;&c_a;&c_z;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>aufdecken</trd>,
     <trd>enth&uuml;llen</trd>,
     <trd>enttarnen</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>felt&aacute;r</trd>,
    <trd>leleplez</trd>,
    <trd>el&aacute;rul <klr tip="ind">(titkot)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_v;&c_y;&c_ja;&c_v;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_o;&c_b;&c_n;&c_a;&c_r;&c_u;&c_zh;&c_i;&c_t;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_z;&c_o;&c_b;&c_l;&c_a;&c_ch;&c_i;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="kasx.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="kasx.mal0o.ago">
    <dif>
      Ago mal<tld/>i. 
    </dif>
    <trdgrp lng="be">
      <trd>&c_v;&c_y;&c_ja;&c_w;&c_l;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
      <trd>&c_v;&c_y;&c_k;&c_r;&c_y;&c_c;&c_mol;&c_c;&c_jo;</trd>,
      <trd>&c_v;&c_y;&c_k;&c_r;&c_y;&c_v;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>felt&aacute;r&aacute;s</trd>,
      <trd>leleplez&eacute;s</trd>,
      <trd>el&aacute;rul&aacute;s <klr tip="ind">(titok&eacute;)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_v;&c_y;&c_ja;&c_v;&c_l;&c_je;&c_n;&c_i;&c_je;</trd>,
      <trd>&c_o;&c_b;&c_n;&c_a;&c_r;&c_u;&c_zh;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kasx.mal0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Supernatura inspiro per kiu Dio konigas ion al iu;
      <ref tip="sin" cel="revela.0o">revelacio</ref>.
    </dif>
    <trd lng="be">&c_a;&c_d;&c_k;&c_r;&c_y;&c_c;&c_mol;&c_c;&c_jo;</trd>
    <trd lng="hu">kinyilatkoztat&aacute;s</trd>
    <trd lng="ru">&c_o;&c_t;&c_k;&c_r;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
  </snc>
</drv>
<drv mrk="kasx.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="kasx.mal0a.diskonita">
    <dif>
      Diskonita:
      <ekz>
        lia honto fari&gcirc;is tro mal<tld/>a, por ke mi povu
        akcepti lin hejme<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="fam.0a">fama</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_ja;&c_w;&c_n;&c_y;</trd>,
      <trd>&c_a;&c_d;&c_k;&c_r;&c_y;&c_t;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>nyilv&aacute;nos</trd>,
      <trd>nem titkolt</trd>,
      <trd>pal&aacute;stolatlan</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_ja;&c_v;&c_n;&c_y;&c_j;</trd>,
      <trd>&c_o;&c_t;&c_k;&c_r;&c_y;&c_t;&c_y;&c_j;</trd>
    </trdgrp>
  </snc>
  <snc mrk="kasx.mal0a.lojala">
    <dif>
      Sin mal<tld/>anta; lojala:
      <ekz>mal<tld/>a konduto</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="sincer.0a">sincera</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_sh;&c_ch;&c_y;&c_r;&c_y;</trd>,
      <trd>&c_a;&c_d;&c_k;&c_r;&c_y;&c_t;&c_y;</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>ny&iacute;lt</trd>,
      <trd>nem <ind>rejt&odblac;zk&ouml;d&odblac;</ind></trd>
    </trdgrp>
    <trd lng="ru">&c_o;&c_t;&c_k;&c_r;&c_o;&c_v;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>
  </snc>
</drv>
</art>
