<drv mrk="kasx.0e">
  <kap><tld/>e</kap>
  <snc mrk="kasx.0e.KOMUNE">
    <dif>
      Malmontre, <tld/>ante sin, iun agon, iun aferon:
      <ekz>
        kial vi forkuris sekrete kaj <tld/>e de mi
        <fnt>
          <bib>MT</bib>
          <lok>Genezo 31:27</lok>
        </fnt>?
      </ekz>
      <ekz>
        <klr>[&ccirc;ar]</klr> en tia okazo vi ne povas <tld/>e viziti min, vi lo&gcirc;os &ccirc;e mi
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Arne</nom> <tld/>e rigardis lin
        <fnt><bib>InfanTorent2</bib>, <lok>unua parto, &ccirc;apitro
        1a</lok></fnt>;
      </ekz>
      <ekz>
        ili <tld/>e englitigis en mian malliberejon tiom da cigaroj
        <fnt>
          <bib>IdO</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &scirc;tuparo <klr>[&#8230;]</klr> estis <tld/>e farita interne de la murego mem
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekoka</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[mi]</klr> <tld/>e kelkfoje kisis la nukon de <nom>Alice</nom>
        <fnt>
          <bib>Metrop</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="sekret.0e">sekrete</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_a;&c_d;&c_k;&c_o;&c_m;</trd>,
    <trd>&c_u;&c_k;&c_r;&c_a;&c_d;&c_k;&c_a;&c_m;</trd>,
    <trd>&c_u;&c_p;&c_o;&c_t;&c_a;&c_j;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="br">
    <trd>e koach</trd>,
    <trd>e kuzh</trd>,
    <trd>dre goach</trd>,
    <trd>dre guzh</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>versteckt</trd>,
    <trd>verborgen</trd>,
    <trd>heimlich</trd>
  </trdgrp>
  <trd lng="en">secretively</trd>
  <trd lng="es">a escondidas</trd>
  <trdgrp lng="fr">
    <trd>en <ind>cachette</ind></trd>,
    <trd>&agrave; la <ind>d&eacute;rob&eacute;e</ind></trd>,
    <trd>en <ind>tapinois</ind></trd>,
    <trd>furtivement</trd>,
    <trd>subrepticement</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>rejtve</trd>,
    <trd>eldugva</trd>
  </trdgrp>
 <trdgrp lng="id">
   <trd>secara <ind>sembunyi</ind>-sembunyi</trd>,
   <trd>secara <ind>rahasia</ind></trd>,
   <trd><ind>diam</ind>-diam</trd>
 </trdgrp>
 <trdgrp lng="ja">
    <trd>&#12371;&#12387;&#x305D;&#x308A;</trd>,
    <trd>&#x79D8;&#x5BC6;&#x88E1;&#x306B;  <pr>&#12402;&#x307F;&#12388;&#x308A;&#x306B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>heimelijk<klr tip="ind"> (bw.)</klr></trd>,
    <trd>stiekem<klr tip="ind"> (bw.)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>chy&lstroke;kiem</trd>,
    <trd>skrycie</trd>,
    <trd>po kryjomu</trd>,
    <trd>potajemnie</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_r;&c_y;&c_t;&c_n;&c_o;</trd>,
    <trd>&c_t;&c_a;&c_j;&c_k;&c_o;&c_m;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_t;&c_a;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_t;&c_a;&c_j;&c_k;&c_i;</trd>,
    <trd>&c_s;&c_k;&c_r;&c_i;&c_t;&c_o;</trd>,
