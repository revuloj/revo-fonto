<drv mrk="kasx.mal0o">
  <kap>mal<tld/>o</kap>
  <snc mrk="kasx.mal0o.KOMUNE">
    <dif>
      Ago mal<tld/>i:
      <ekz>
        min teruras tiuj mal<tld/>oj, kies cinikismon ci ne &scirc;ajnas konjekti
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        en &scirc;ia pala viza&gcirc;o batalis decidemo kaj maltrankvilo pro la mal<tld/>o, kiun &scirc;i
        volis fari
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        mal<tld/>o de Via vorto klerigas
        <fnt><bib>MT</bib>, <lok>&Psa; 119:130</lok></fnt>;
      </ekz>
      <ekz>
        la predikado de Jesuo Kristo <klr>[estas]</klr> mal<tld/>o de la mistero,
        silentigita tra eternaj tempoj
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 16:25</lok>
        </fnt>.
      </ekz>
    </dif>
