<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: karel.xml,v 1.5 2022/03/21 17:51:57 revo Exp $">
<kap>
  <rad>karel</rad>/o
</kap>
<drv mrk="karel.0o">
  <kap><tld/>o</kap>
  <snc mrk="karel.0o.devenanto">
    <dif>
      Iu, kiu devenas de <ref tip="vid" cel="karel.K0ujo"><tld lit="K"/>ujo</ref>:
      <ekz>
        pomoroj (la&ubreve;vorte &leftquot;&ccirc;emaruloj&rightquot;) &mdash; specifa etna grupo de rusoj kaj <tld/>oj &ccirc;e la
        Blanka maro
        <fnt>
          <aut>Jurij German</aut>
          <vrk><url ref="https://esperanto.mv.ru/German/rus-0.html">Rusio juna</url></vrk>
          <lok>Prologo</lok>
        </fnt>;
      </ekz>
      <ekz>
        Tverjaj <tld/>oj <klr>[&#x2026;]</klr> fu&gcirc;is el teritorio okcidente kaj norde de Ladoga
        lago, okupita de Svedio en 1617
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=128324">Tverjaj kareloj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_r;&c_e;&c_l;</trd>
  <trdgrp lng="en">
    <trd>Kareli</trd>
    <trd>Karelian</trd>
  </trdgrp>
  <trd lng="nl">Kareli&euml;r</trd>
  <trd lng="uk">&#x43A;&#x430;&#x440;&#x435;&#x43B;</trd>
</drv>

<drv mrk="karel.K0ujo">
  <kap><tld lit="K"/>ujo</kap>
  <snc mrk="karel.K0ujo.regiono">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      Historia <ref tip="lst" lst="voko:regionoj" cel="region.0o.land">regiono</ref>
      en la nordoriento de <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>,
      nun dividita inter <ref tip="malprt" cel="finn.F0lando">Finnlando</ref> kaj
      Rusujo.
    </dif>
  </snc>
  <snc mrk="karel.K0ujo.rusa_subsxtato">
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.sub0o">Sub&scirc;tato</ref> 
      en la nord-okcidenta parto de <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>,
      &ccirc;efurbo estas Petrozavodsko.
    </dif>
  </snc>
  <trd lng="be">&c_K;&c_a;&c_r;&c_e;&c_l;&c_ib;&c_ja;</trd>
  <trd lng="de">Karelien</trd>
  <trd lng="en">Karelia</trd>
  <trd lng="fr">Car&eacute;lie</trd>
  <trd lng="nl">Kareli&euml;</trd>
</drv>
</art>

