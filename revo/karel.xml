<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>karel</rad>/o
</kap>
<drv mrk="karel.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Iu de <ref tip="vid" cel="karel.K0ujo"><tld lit="K"/>ujo</ref>.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>Kareli</trd>
    <trd>Karelian</trd>
  </trdgrp>
  <trd lng="nl">Kareli&euml;r</trd>
</drv>

<drv mrk="karel.K0ujo">
  <kap><tld lit="K"/>ujo</kap>
  <snc mrk="karel.K0ujo.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      Sub&scirc;tato en la nord-okcidenta parto de Rusujo, &ccirc;efurbo estas
      Petrozavodsko.
    </dif>
  </snc>
  <trd lng="de">Karelien</trd>
  <trd lng="en">Karelia</trd>
  <trd lng="fr">Car&eacute;lie</trd>
  <trd lng="nl">Kareli&euml;</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>