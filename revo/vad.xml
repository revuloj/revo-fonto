<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>vad</rad>/i
</kap>
<drv mrk="vad.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pa&scirc;i en akvo a&ubreve; en mola substanco, koto, ne&gcirc;o, sablo ktp:
      <ekz>
        malfacile <tld/>i eni&gcirc;ante &gcirc;is la maleoloj en la sablo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i &gcirc;is la kolo en la ondoj;
      </ekz>
      <ekz>
        liaj piedoj <tld/>as tra mirtarbetoj kaj malaltaj filikoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i en la koto de la volupto.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_h;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;
    &c_u;&c_b;&c_r;&c_o;&c_d;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_b;&c_ib;&c_r;&c_a;&c_c;&c_c;&c_a;
    <klr>(&c_p;&c_a; &c_g;&c_r;&c_a;&c_z;&c_ib;,
    &c_s;&c_mol;&c_n;&c_je;&c_z;&c_je;, &c_p;&c_ja;&c_s;&c_k;&c_u;)</klr>
    </trd>
  </trdgrp>
  <trd lng="de">waten</trd>
  <trd lng="en">wade</trd>
  <trd lng="es">vadear</trd>
  <trdgrp lng="fr">
    <trd>enfoncer<klr> (en marchant)</klr></trd>,
    <trd>patauger</trd>
  </trdgrp>
  <trd lng="hu">g&aacute;zol<klr tip="ind"> (v&iacute;zben)</klr></trd>
  <trd lng="nl">waden</trd>
  <trdgrp lng="pt">
    <trd>vadear</trd>,
    <trd>patinhar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol; <ind>&c_v;&c_b;&c_r;&c_o;&c_d;</ind></trd>,
    <trd>&c_p;&c_r;&c_o;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr tip="amb">(&c_p;&c_o; &c_g;&c_r;&c_ja;&c_z;&c_i;, &c_s;&c_n;&c_je;&c_g;&c_u;,
    &c_p;&c_je;&c_s;&c_k;&c_u;)</klr></trd>,
    <trd>&c_b;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">brodi&tcaron; sa</trd>
  <trd lng="sv">vada</trd>
</drv>

<drv mrk="vad.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="vad.0ejo.travadejo">
    <ref tip="dif" cel="vad.tra0ejo">tra<tld/>ejo</ref>
    <trd lng="be">&c_b;&c_r;&c_o;&c_d;</trd>
    <trd lng="de">Furt</trd>
    <trd lng="en">ford</trd>
    <trd lng="es">vado</trd>
    <trd lng="fr">gu&eacute;</trd>
    <trd lng="hu">g&aacute;zl&oacute;</trd>
    <trd lng="pt">vau</trd>
    <trd lng="ru">&c_b;&c_r;&c_o;&c_d;</trd>
    <trd lng="sk">brod</trd>
    <trd lng="sv">vadst&auml;lle</trd>
  </snc>
</drv>

<drv mrk="vad.tra0ejo">
  <kap>tra<tld/>ejo</kap>
  <snc mrk="vad.tra0ejo.TRA">
    <dif>
      Loko malprofunda, en kiu oni povas trairi riveron <tld/>ante.
    </dif>
  </snc>
  <trd lng="be">&c_b;&c_r;&c_o;&c_d;</trd>
  <trd lng="de">Furt</trd>
  <trd lng="en">ford</trd>
  <trd lng="es">vado</trd>
  <trd lng="fr">gu&eacute;</trd>
  <trd lng="hu">g&aacute;zl&oacute;</trd>
  <trd lng="pt">vau</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_d;</trd>
  <trd lng="sk">brod</trd>
  <trd lng="sv">vadst&auml;lle</trd>
</drv>

<drv mrk="vad.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Trairi <tld/>ante.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_j;&c_s;&c_mol;&c_c;&c_ib;
  <ind>&c_w;&c_b;&c_r;&c_o;&c_d;</ind></trd>
  <trd lng="de">durchwaten</trd>
  <trd lng="en">ford</trd>
  <trd lng="fr">passer &agrave; gu&eacute;</trd>
  <trd lng="hu">&aacute;tg&aacute;zol</trd>
  <trd lng="nl">doorwaden</trd>
  <trd lng="pt">atravessar a vau</trd>
  <trd lng="es">vadear</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_j;&c_t;&c_i; <ind>&c_v;&c_b;&c_r;&c_o;&c_d;</ind></trd>
  <trd lng="sk">prebrodi&tcaron;</trd>
</drv>
</art>
