<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>vad</rad>/i
</kap>
<drv mrk="vad.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Pa&scirc;i en akvo a&ubreve; en mola substanco, koto, ne&gcirc;o, sablo ktp:
      <ekz>
        malfacile <tld/>i eni&gcirc;ante &gcirc;is la maleoloj en la sablo<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>i &gcirc;is la kolo en la ondoj;
      </ekz>
      <ekz>
        liaj piedoj <tld/>as tra mirtarbetoj kaj malaltaj filikoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>i en la koto de la volupto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="barakt.0i">barakti</ref>,
      <ref cel="plauxd.0i">pla&ubreve;di</ref>.
    </refgrp>
  </snc>
  <trd lng="de">waten</trd>
  <trd lng="en">wade</trd>
  <trd lng="es">vadear</trd>
  <trd lng="nl">waden</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_h;&c_o;&c_d;&c_i;&c_t;&c_mol; <ind>&c_v;&c_b;&c_r;&c_o;&c_d;</ind></trd>
    <trd>&c_p;&c_r;&c_o;&c_b;&c_i;&c_r;&c_a;&c_t;&c_mol;&c_s;&c_ja; <klr tip="amb">(&c_p;&c_o; &c_g;&c_r;&c_ja;&c_z;&c_i;, &c_s;&c_n;&c_je;&c_g;&c_u;, &c_p;&c_je;&c_s;&c_k;&c_u;)</klr></trd>,
    <trd>&c_b;&c_r;&c_je;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="sv">vada</trd>
</drv>

<drv mrk="vad.tra0ejo">
  <kap>(tra)<tld/>ejo</kap>
  <snc>
    <dif>
      Loko en kiu oni povas trairi riveron <tld/>ante.
    </dif>
  </snc>
  <trd lng="de">Furt</trd>
  <trd lng="en">ford</trd>
  <trd lng="es">vado</trd>
  <trd lng="ru">&c_b;&c_r;&c_o;&c_d;</trd>
  <trd lng="sv">vadst&auml;lle</trd>
</drv>

<drv mrk="vad.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Trairi <tld/>ante.
    </dif>
  </snc>
  <trd lng="de">durchwaten</trd>
  <trd lng="en">ford</trd>
  <trd lng="nl">doorwaden</trd>
  <trd lng="es">vadear</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_j;&c_t;&c_i; <ind>&c_v;&c_b;&c_r;&c_o;&c_d;</ind></trd>
</drv>
</art>
