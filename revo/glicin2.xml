<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: glicin2.xml,v 1.3 2023/12/22 08:30:40 afido Exp $">
    <kap>
      <rad>glicin</rad>/o
      <fnt><bib>PIV1</bib></fnt>
    </kap>

<drv mrk="glicin2.0o">
  <kap><tld/>o</kap>
  <snc mrk="glicin2.0o.BOT">
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="fabac.0oj">fabacoj</ref>
      (<trd lng="la">Glycine</trd>), el kiu plej gravas la specio
      <ref tip="sub" cel="soj.0o">sojo</ref>:
      <ekz>
         pasante preter oleandro a&ubreve; jasmeno, ni subite sentis aromon de
         <tld/>o, kaj apud la <tld/>o subite traflugis strieto de
         viskoza
         melaso de genisto
         <fnt>
           <aut>V. Rybakov</aut>,
           <vrk><url
           ref="https://web.archive.org/web/20120227001644/http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/ckk/ckk0003.htm">Gravitavio
           "Carido"</url></vrk>;
         </fnt>;
      </ekz>
      <ekz>
        specioj de la genro <tld/>o estas uzataj kiel man&gcirc;oplantoj <klr>[&#8230;]</klr> de
        kelkaj specioj de lepidopteroj, nome tineoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=508247"
              >Glicino (genro)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/2/26/Glycine_soja.JPG/360px-Glycine_soja.JPG"
      prm="CC BY-SA 3.0"
      >sojo (Glycine soja), Koreujo
        <fnt>
          <aut>Dalgial, propra verko</aut>
          <lok><url ref="&WCW;/File:Glycine_soja.JPG"
              >&WCW;/File:Glycine_soja.JPG</url></lok>
        </fnt>
    </bld>
  </snc>
  <ref tip="hom" cel="glicin.0o">glicino (aminacido)</ref>
  <trd lng="be">&c_s;&c_o;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>glicina</trd>,
    <trd>soia</trd>,
    <trd>g&egrave;nere Glycine</trd>
  </trdgrp>
  <trd lng="de">Glycine <klr>(Gattung)</klr></trd>
  <trd lng="fr">glycine</trd>
</drv>

  </art>

