<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: volfgang.xml,v 1.4 2020/12/06 11:10:31 revo Exp $">
<kap>
  <rad>Volfgang</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="volfgang.0o">
  <kap><tld/>o</kap>
  <snc mrk="volfgang.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> &gcirc;ermandevena:
      <ekz>
        lupo estas etimo de &gcirc;ermanaj kaj skandinavaj personaj nomoj 
        kiel <ref cel="adolf.0o">Adolfo</ref>, <ref cel="rudolf.0o">Rudolfo</ref>,
        <ref cel="olav.0o">Olavo</ref>, <tld/>o
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Lupo">
          lupo</url>, 2010-09-28</lok>
        </fnt>;
      </ekz>
      <ekz>
        Sankta <tld/>o
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Diabla_kontrakto"
              >Diabla kontrakto</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
       <tld/>o Amadeo <ref tip="ekz" cel="mozart.0o">Mozarto</ref>
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Volfgango_Amadeo_Mozarto"
              >Wolfgang Amadeus Mozart - Vikipedio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Johano <tld/>o Goeto
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Johann_Wolfgang_von_Goethe"
              >Johann Wolfgang von Goethe</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Wolfgang</trd>
  <trd lng="fr">Wolfgang</trd>
</drv>
</art>

