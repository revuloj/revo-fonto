<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>kanak</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="kanak.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ano de popolo lo&gcirc;anta en sudpacifika insularo, precipe Novgvineo,
      Salomonaj insuloj, Novhebridoj, Novkaledonio:
      <ekz>
        <ctl><tld/>o el Kananam</ctl>
        <fnt><aut>Kenneth G. Linton</aut> <vrk>Kanako el Kananam. -
        Rickmansworth: The Esperanto publishing company, 1960</vrk></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>Canaque<klr tip="ind"> (subst.)</klr></trd>,
    <trd>Kanak<klr tip="ind"> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="hu">kanak</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_a;&c_k;</trd>
</drv>

<drv mrk="kanak.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Rilata al la <tld/>oj.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>canaque<klr tip="ind"> (adj.)</klr></trd>,
    <trd>kanak<klr tip="ind"> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">kanak</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_a;&c_k;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>
</art>

