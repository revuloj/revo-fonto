<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: barat.xml,v 1.27 2023/10/29 19:11:00 revo Exp $">
<kap>
  <rad>Barat</rad>/o
</kap>

<drv mrk="barat.B0o">
  <kap><tld/>o</kap>
  <mlg kod="ISO-3166">IN</mlg>
  <snc mrk="barat.B0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> limanta norde al
      Pakistano, &Ccirc;inujo, Nepalo kaj etendi&gcirc;anta sude &gcirc;is la pinto de la
      subkontinento; <ref tip="sin" cel="hind.H0ujo.POL">Hindujo</ref>:
      <ekz>
        la grandaj ebena&jcirc;oj &ccirc;irka&ubreve; la rivero Induso en la nunaj
        Pakistano kaj okcidenta <tld/>o
        <fnt>
          <aut>A. Giridhar Rao</aut>:
          <vrk>La plej frua skribo</vrk>,
          <bib>Monato</bib>
          <lok>jaro 1999a, numero 8a, p. 12a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Graham Stuart Staines</nom> el A&ubreve;stralio laboris en <tld/>o
        inter lepruloj
        <fnt>
          <bib>Monato</bib>
          <aut>A. Giridhar Rao</aut>
          <vrk><url ref="&Monato;/2004/008204.html">Misiisto-murdisto
          al morto kondamnita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        salajroj en <tld/>o oble malsuperas tiujn en Britio
        <fnt>
          <bib>Monato</bib>
          <aut>Paul GUBBINS</aut>
          <vrk><url ref="&Monato;/2004/008223.html">&Ccirc;e-telefone ...
          kiu?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        en <tld/>o oni evoluigas malmultekostajn komputilojn kaj
        liberajn programojn uzeblajn per multaj aziaj lingvoj
        <fnt>
          <bib>Monato</bib>
          <aut>D. E. Rogers</aut>
          <vrk><url ref="&Monato;/2004/008269.html">Libera
          programaro: nova rimedo komerci</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la <tld lit="b"/>a registaro
        <fnt>
          <bib>Monato</bib>
          <aut>A. Giridhar Rao</aut>
          <vrk><url ref="&Monato;/2004/008204.html">Misiisto-murdisto
          al morto kondamnita</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        aldone estis ekspoziciataj anka&ubreve; arta&jcirc;oj de bestoj kaj
        homfiguroj el koloraj
        butonoj kaj &scirc;tofaj tuberetoj fare de <nom>Nek Chand</nom>, <tld lit="b"/>ano, kies
        &leftquot;rok&gcirc;ardeno&#x201D; sialande fari&gcirc;is furora vidinda&jcirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="&Monato;/2005/008666.php">Folklora
          festivalo en Kaustinen</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
    <trd>&#x5929;&#x7AFA; <pr>ti&#x101;nzh&#xFA;</pr></trd>,
    <trd>&#x5357;&#x4E9A;&#x6B21;&#x5927;&#x9646; <pr>n&#xE1;ny&#xE0;c&#xEC;d&#xE0;l&#xF9;</pr></trd>
  </trdgrp>
</snc>
  <trd lng="be">&c_Ib;&c_n;&c_d;&c_y;&c_ja;</trd>
  <trd lng="bg">&c_I;&c_n;&c_d;&c_i;&c_ja;</trd>
  <trd lng="bo">&#xF62;&#xF92;&#xFB1;&#xF0B;&#xF42;&#xF62;&#xF0B;</trd>
  <trd lng="cs">Indie</trd>
  <trd lng="de">Indien</trd>
  <trd lng="en">India</trd>
  <trd lng="es">India</trd>
  <trdgrp lng="fr">
    <trd>Inde</trd>,
    <trd>R&eacute;publique de l'Inde</trd>
  </trdgrp>
  <trd lng="hi">&#x92D;&#x93E;&#x930;&#x924; [<ind>bh&amacron;rat</ind>]</trd>
  <trd lng="hu">India</trd>
  <trd lng="id">India</trd>
  <trd lng="ja">&#x30D0;&#x30FC;&#x30E9;&#x30BF;&#x738B;  <pr>&#x30D0;&#x30FC;&#x30E9;&#x30BF;&#x304A;&#x3046;</pr></trd>
  <trd lng="nl">India</trd>
  <trd lng="pl">Indie</trd>
  <trd lng="pt">&Iacute;ndia</trd>
  <trd lng="ru">&c_I;&c_n;&c_d;&c_i;&c_ja;</trd>
  <trd lng="sk">India</trd>
  <trd lng="tok">ma Palata</trd>
  <trd lng="uk">&#x411;&#x445;&#x430;&#x440;&#x430;&#x442;</trd>
  <trd lng="vo">Lind&auml;n</trd>
</drv>

<drv mrk="barat.0a">
  <kap><tld lit="b"/>a</kap>
  <snc>
    <dif>
      Rilata al la &scirc;tato <tld/>o:
      <ekz>
        la <tld lit="b"/>aj kaj internaciaj investistoj profitis el &gcirc;i,
        uzante malmultekostan laboristaron socie dividitan
        <fnt>
          <aut>D. Benbabaali, trad. J.-M. Cash</aut>,
          <vrk><url ref="&Mondediplo;article2597.html">
          La el&jcirc;etitoj de la barata miraklo</url></vrk>,
          <lok>le Monde diplomatique, 2018-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_y;&c_j;&c_s;&c_k;&c_ib;</trd>
  <trd lng="de">indisch</trd>
  <trd lng="en">Indian</trd>
  <trdgrp lng="es">
    <trd>indio</trd>
    <trd>india</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>indien</trd>
    <trd>indienne<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="nl">Indiaas</trd>
  <trd lng="tok">pi ma Palata</trd>
  <trd lng="vo">Lind&auml;nik</trd>
</drv>

<drv mrk="barat.0ano">
  <kap><tld lit="b"/>ano</kap>
  <snc>
    <dif>
      Civitano de <tld/>o:
      <ekz>
        la lo&gcirc;antoj de orienta Glasgovo fartas tre bone kompare kun <tld lit="b"/>anoj
        <fnt>
          <aut>J. Brygo, trad. M. Leon</aut>,
          <vrk><url ref="&Mondediplo;article1669.html">
          Vivi ri&ccirc;ule en urbo de malri&ccirc;uloj</url></vrk>,
          <lok>le Monde diplomatique, 2010-08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_d;&c_y;&c_je;&c_c;</trd>
  <trdgrp lng="de">
    <trd>Inder</trd>
    <trd>Inderin</trd>
  </trdgrp>
  <trd lng="en">Indian</trd>
  <trdgrp lng="es">
    <trd>indio</trd>
    <trd>india</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Indien</trd>
    <trd>Indienne<klr> (subst.)</klr></trd>
  </trdgrp>
  <trd lng="nl">Indi&euml;r</trd>
  <trd lng="tok">jan pi ma Palata</trd>
  <trd lng="vo">Lind&auml;nan</trd>
</drv>
</art>
