  <trd lng="nl">middenwal</trd>
  <trd lng="pt">cortina <klr tip="amb">(constru&ccedil;&atilde;o)</klr></trd>
  <trd lng="ru">&c_k;&c_u;&c_r;&c_t;&c_i;&c_n;&c_a;</trd>
