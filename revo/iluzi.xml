<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>iluzi</rad>/o
</kap>
<drv mrk="iluzi.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Eraro de sentumoj, pro kiu ni rigardas falsajn &scirc;ajnojn kiel
      realajn:
      <ekz>
        vidaj, a&ubreve;daj <tld/>oj;
      </ekz>
      <ekz>
        teatra <tld/>o.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="miragx.0o">mira&gcirc;o</ref>.
    </refgrp>
  </snc>
  <snc>
    <dif>
      Eraro de la spirito, pro kiu ni rigardas falsajn revojn kiel
      efektiva&jcirc;ojn:
      <ekz>
        fari al si <tld/>ojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la <tld/>oj de la juneco;
      </ekz>
      <ekz>
        tio estas nur <tld/>o;
      </ekz>
      <ekz>
        tiu estis lasta mortpiko al miaj <tld/>oj;
      </ekz>
      <ekz>
        nenia <tld/>o devus plu nin flati.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fantaz.0o">fantazio</ref>,
      <ref cel="songx.0o">son&gcirc;o</ref>,
      <ref cel="delir.0o">deliro</ref>,
      <ref cel="fikci.0o">fikcio</ref>,
      <ref cel="haluci.0o">halucino</ref>,
      <ref cel="hximer.0o">&hcirc;imero</ref>,
      <ref cel="utopi.0o">utopio</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>T&auml;uschung</trd>,
    <trd>Illusion</trd>
  </trdgrp>
  <trd lng="fr">illusion</trd>
  <trd lng="nl">illusie</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_l;&c_l;&c_ju;&c_z;&c_i;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_l;&c_u;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="iluzi.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Estanta nur <tld/>o, ka&ubreve;zanta falsan &gcirc;ojon:
      <ekz>
        <tld/>a promeso, projekto, espero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>tr&uuml;gerich</trd>,
    <trd>illusorisch</trd>
  </trdgrp>
  <trd lng="fr">illusoire</trd>
  <trdgrp lng="nl">
    <trd>illusoir</trd>,
    <trd>bedrieglijk</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_i;&c_l;&c_l;&c_ju;&c_z;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_z;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_a;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
<drv mrk="iluzi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ka&ubreve;zi <tld/>on:
      <ekz>
        ne <tld/>u vin per tro granda espero<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mistif">mistifiki</ref>.
    </refgrp>
  </snc>
  <trd lng="de">vort&auml;uschen</trd>
  <trdgrp lng="fr">
    <trd>illusionner</trd>,
    <trd>abuser<klr tip="ind"> (leurrer)</klr></trd>,
    <trd>leurrer<klr tip="ind"> (illusionner)</klr></trd></trdgrp> 
  <trd lng="nl">illusies wekken</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_z;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_i;&c_l;&c_l;&c_ju;&c_z;&c_i;&c_ju;</trd>,
    <trd>&c_v;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_v; &c_z;&c_a;&c_b;&c_l;&c_u;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
<drv mrk="iluzi.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Esti en stato de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>sich t&auml;uschen</trd>,
    <trd>einer <ind>Illusion</ind> erliegen</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>s'<ind>illusionner</ind></trd>,
    <trd>s'<ind>abuser</ind></trd>,
    <trd>se <ind>leurrer</ind></trd></trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_v;&c_p;&c_a;&c_s;&c_t;&c_mol; &c_v; &c_i;&c_l;&c_l;&c_ju;&c_z;&c_i;&c_ju;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_l;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>
<drv mrk="iluzi.sen0igi">
  <kap>sen<tld/>igi</kap>
  <snc>
    <dif>
      Eltiri el <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="rev">elrevigi</ref>,
      <ref cel="rev">disrevigi</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="blov.dis0ifor0i">disblovi</ref>.
    </refgrp>
  </snc>
  <trd lng="de">entt&auml;uschen</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;sillusionner</trd>,
    <trd>enlever (&agrave; qn) ses illusions</trd>,
    <trd>d&eacute;tromper</trd></trdgrp> 
  <trd lng="nl">teleurstellen</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="iluzi.sen0igxi">
  <kap>sen<tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Perdi <tld/>on.
    </dif>
    <refgrp tip="sin">
      <ref cel="rev">elrevi&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="de">entt&auml;uscht werden</trd>
  <trdgrp lng="fr">
    <trd>perdre ses illusions</trd>,
    <trd>tomber de haut</trd>,
    <trd>&ecirc;tre <ind>d&eacute;&ccedil;u</ind></trd>,
    <trd>d&eacute;chanter</trd></trdgrp> 
  <trd lng="nl">teleurgesteld worden</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
<drv mrk="iluzi.0isto">
  <kap><tld/>isto</kap>
  <snc>
    <dif>
      Amuzisto, kiu lerte trompas la rigardantojn per falsaj &scirc;ajnoj kaj
      rapide faritaj &jcirc;ongla&jcirc;oj.
    </dif>
    <refgrp tip="vid">
      <ref cel="jxongl.0isto">&jcirc;onglisto</ref>,
      <ref cel="presti.0o">prestidigitatoro</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Zauberk&uuml;nstler</trd>,
    <trd>Illusionist</trd>
  </trdgrp>
  <trd lng="fr">illusionniste</trd>
  <trd lng="nl">illusionist</trd>
  <trd lng="ru">&c_i;&c_l;&c_l;&c_ju;&c_z;&c_i;&c_o;&c_n;&c_i;&c_s;&c_t;</trd>
</drv>
</art>
