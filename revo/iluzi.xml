<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>iluzi</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="iluzi.0o">
  <kap><tld/>o</kap>
  <snc mrk="iluzi.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="erar.0o.KOMUNE">Eraro</ref>, pro kio oni
      rigardas ion malrealan kiel realan.
    </dif>
    <subsnc mrk="iluzi.0o.senta">
      <dif>
        Eraro de sentumoj, pro kiu oni rigardas falsajn &scirc;ajnojn kiel
        realajn:
        <ekz>
          vidaj, a&ubreve;daj <tld/>oj;
        </ekz>
        <ekz>
          teatra <tld/>o.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="iluzi.0o.anima">
      <dif>
        Eraro de la spirito, pro kiu oni rigardas revojn kiel
        efektiva&jcirc;on:
        <ekz>
          fari al si <tld/>ojn<fnt>Z</fnt>;
        </ekz>
        <ekz>
          la <tld/>oj de la juneco;
        </ekz>
        <ekz>
          tio estas nur <tld/>o;
        </ekz>
        <ekz>
          tiu estis lasta mortpiko al miaj <tld/>oj;
        </ekz>
        <ekz>
          nenia <tld/>o devus plu nin flati.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="delir.0o">deliro</ref>,
        <ref cel="fantaz.0o">fantazio</ref>,
        <ref cel="fikci.0o">fikcio</ref>,
        <ref cel="haluci.0o">halucino</ref>,
        <ref cel="hximer.0o">&hcirc;imero</ref>,
        <ref cel="songx.0o">son&gcirc;o</ref>
      </refgrp>
    </subsnc>
  </snc>
  <trdgrp lng="cs">
    <trd>iluze</trd>,
    <trd>klam</trd>,
    <trd>vidina</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>T&auml;uschung</trd>,
    <trd>Illusion</trd>
  </trdgrp>
  <trd lng="es">ilusi&oacute;n</trd>
  <trd lng="fr">illusion</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rz&eacute;kcsal&oacute;d&aacute;s</trd>,
    <trd>ill&uacute;zi&oacute;</trd>
  </trdgrp>
  <trd lng="nl">illusie</trd>
  <trd lng="pt">ilus&atilde;o</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_l;&c_l;&c_ju;&c_z;&c_i;&c_ja;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_l;&c_u;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klamn&aacute; predstava</trd>,
    <trd>mam</trd>,
    <trd>vidina</trd>
  </trdgrp>
  <trd lng="vo">cit</trd>
</drv>

<drv mrk="iluzi.0a">
  <kap><tld/>a</kap>
  <snc mrk="iluzi.0a.KOMUNE">
    <dif>
      Estanta nur <tld/>o, ka&ubreve;zanta falsan &gcirc;ojon:
      <ekz>
        <tld/>a promeso, projekto, espero.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>iluzivn&iacute;</trd>,
    <trd>iluzorn&iacute;</trd>,
    <trd>neskute&ccaron;n&yacute;</trd>,
    <trd>vymy&scaron;len&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>tr&uuml;gerisch</trd>,
    <trd>illusorisch</trd>
  </trdgrp>
  <trd lng="fr">illusoire</trd>
  <trdgrp lng="hu">
    <trd>illuz&oacute;rikus</trd>,
    <trd>csal&oacute;ka</trd>,
    <trd>l&aacute;tsz&oacute;lagos</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>illusoir</trd>,
    <trd>bedrieglijk</trd>
  </trdgrp>
  <trd lng="pt">ilus&oacute;rio</trd>
  <trdgrp lng="ru">
    <trd>&c_i;&c_l;&c_l;&c_ju;&c_z;&c_o;&c_r;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_z;&c_r;&c_a;&c_ch;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_o;&c_b;&c_m;&c_a;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>iluz&oacute;rny</trd>,
    <trd>neskuto&ccaron;n&yacute;</trd>,
    <trd>vymyslen&yacute;</trd>
  </trdgrp>
  <trd lng="vo">citik</trd>
</drv>

<drv mrk="iluzi.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="iluzi.0i.kauxzi_iluzion">
    <dif>
      Ka&ubreve;zi <tld/>on:
      <ekz>
        ne <tld/>u vin per tro granda espero<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="mistifik.0i">mistifiki</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>klamat</trd>,
    <trd>mystifikovat</trd>,
    <trd>m&aacute;mit</trd>,
    <trd>p&rcaron;edst&iacute;rat</trd>,
    <trd>&scaron;&aacute;lit</trd>
  </trdgrp>
  <trd lng="de">vort&auml;uschen</trd>
  <trd lng="es">ilusionar</trd>
  <trdgrp lng="fr">
    <trd>illusionner</trd>,
    <trd>abuser<klr tip="ind"> (leurrer)</klr></trd>,
    <trd>leurrer<klr tip="ind"> (illusionner)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ill&uacute;zi&oacute;kba ringat</trd>,
    <trd>&aacute;ltat</trd>
  </trdgrp>
  <trd lng="nl">illusies wekken</trd>
  <trd lng="pt">iludir</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_o;&c_z;&c_d;&c_a;&c_v;&c_a;&c_t;&c_mol; &c_i;&c_l;&c_l;&c_ju;&c_z;&c_i;&c_ju;</trd>,
    <trd>&c_v;&c_v;&c_o;&c_d;&c_i;&c_t;&c_mol; &c_v; &c_z;&c_a;&c_b;&c_l;&c_u;&c_zh;&c_d;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>klama&tcaron;</trd>,
    <trd>mystifikova&tcaron;</trd>,
    <trd>predstiera&tcaron;</trd>,
    <trd>vyvola&tcaron; il&uacute;ziu</trd>
  </trdgrp>
  <trd lng="vo">cit&ouml;n</trd>
</drv>

<drv mrk="iluzi.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc mrk="iluzi.0igxi.KOMUNE">
    <dif>
      Esti en stato de <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd><klr>sich </klr>t&auml;uschen</trd>,
    <trd><klr>einer </klr>Illusion <ind>erliegen</ind></trd>
  </trdgrp>
  <trd lng="es">ilusionarse</trd>
  <trdgrp lng="fr">
    <trd>s'<ind>illusionner</ind></trd>,
    <trd>s'<ind>abuser</ind></trd>,
    <trd>se <ind>leurrer</ind></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ill&uacute;zi&oacute;kba ringatja mag&aacute;t</trd>,
    <trd>&aacute;ltat mag&aacute;t</trd>
  </trdgrp>
  <trd lng="pt">iludir-se</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_p;&c_a;&c_s;&c_t;&c_mol; &c_v; &c_i;&c_l;&c_l;&c_ju;&c_z;&c_i;&c_ju;</trd>,
    <trd>&c_z;&c_a;&c_b;&c_l;&c_u;&c_zh;&c_d;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="iluzi.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="iluzi.0isto.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      Amuzisto, kiu lerte trompas la rigardantojn per falsaj &scirc;ajnoj kaj
      rapide faritaj &jcirc;ongla&jcirc;oj.
      </dif>
      <ref tip="sin" cel="presti.0o">prestidigitatoro</ref>
  </snc>
  <trdgrp lng="cs">
    <trd>iluzionista</trd>,
    <trd>kouzeln&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Zauberk&uuml;nstler</trd>,
    <trd>Illusionist</trd>
  </trdgrp>
  <trd lng="es">ilusionista</trd>
  <trd lng="fr">illusionniste</trd>
  <trdgrp lng="hu">
    <trd>b&udblac;v&eacute;sz</trd>,
    <trd>illuzionista</trd>
  </trdgrp>
  <trd lng="nl">illusionist</trd>
  <trd lng="pt">ilusionista</trd>
  <trd lng="ru">&c_i;&c_l;&c_l;&c_ju;&c_z;&c_i;&c_o;&c_n;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>iluzionista</trd>,
    <trd>k&uacute;zeln&iacute;k</trd>
  </trdgrp>
</drv>

<drv mrk="iluzi.mem0o">
  <kap>mem<tld/>o</kap>
  <snc mrk="iluzi.mem0o.PSI">
    <dif>
      <tld lit="I"/>o, kiun oni mem kreas al si:
      <ekz>
        en la unua numero ni skribis, ke la vivo de la Nica Revuo estas 
        certigita por unu jaro; ni povas nun, sen troa mem<tld/>o, 
        diri, ke la ricevitaj rezultoj ebligos konstantan aperadon
        <fnt>
          <aut>-</aut>,
          <vrk><url
          ref="http://donh.best.vwh.net/Esperanto/Literaturo/Revuoj/nlr/nlr16/enkondukou.html">
          Al la legantoj!</url></vrk>,
          <lok>La Nica Literatura Revuo, 1956-07 &gcirc;is 08 (1/6), p. 
          201a-202a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">autoillusion</trd>
</drv>

<drv mrk="iluzi.sen0igi">
  <kap>sen<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="iluzi.sen0igi.KOMUNE">
    <dif>
      Eltiri el <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="rev">elrevigi</ref>,
      <ref cel="rev">disrevigi</ref>;
    </refgrp>
    <refgrp tip="vid">
      <ref cel="blov.dis0i">disblovi</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">zbavit <klr>(n&ecaron;koho)</klr> iluz&iacute;</trd>
  <trd lng="de">entt&auml;uschen</trd>
  <trd lng="es">desilusionar</trd>
  <trdgrp lng="fr">
    <trd>d&eacute;sillusionner</trd>,
    <trd>enlever (&agrave; qn) ses illusions</trd>,
    <trd>d&eacute;tromper</trd>
  </trdgrp>
  <trd lng="hu">ki&aacute;br&aacute;nd&iacute;t</trd>
  <trd lng="nl">teleurstellen</trd>
  <trd lng="pt">desiludir</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">zbavi&tcaron; il&uacute;zi&iacute;</trd>
</drv>

<drv mrk="iluzi.sen0igxi">
  <kap>sen<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="iluzi.sen0igxi.KOMUNE">
    <dif>
      Perdi <tld/>on.
    </dif>
    <refgrp tip="sin">
      <ref cel="rev">elrevi&gcirc;i</ref>.
    </refgrp>
  </snc>
  <trd lng="de">entt&auml;uscht werden</trd>
  <trd lng="es">desilusionarse</trd>
  <trdgrp lng="fr">
    <trd>perdre ses illusions</trd>,
    <trd>tomber de haut</trd>,
    <trd>&ecirc;tre <ind>d&eacute;&ccedil;u</ind></trd>,
    <trd>d&eacute;chanter</trd>
  </trdgrp>
  <trd lng="hu">ki&aacute;br&aacute;ndul</trd>
  <trd lng="nl">teleurgesteld worden</trd>
  <trd lng="pt">desiludir-se</trd>
  <trd lng="ru">&c_r;&c_a;&c_z;&c_o;&c_ch;&c_a;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>
</drv>
</art>
