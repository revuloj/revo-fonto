<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>Valenten</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="valenten.0o">
  <kap><tld/>o</kap>
  <snc mrk="valenten.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira anta&ubreve;nomo latindevena.
    </dif>
  </snc>
  <trd lng="fr">Valentin</trd>
</drv>

<drv mrk="valenten.tagode0o">
  <kap>tago de <tld/>o</kap>
  <snc mrk="valenten.tagode0o.KAL">
    <uzo tip="fak">KAL</uzo>
    <dif>
      Festo de geamantoj kaj amdeklaroj, en la 14a tago de februaro:
      <ekz>
        en la tago de <tld/>o
        <fnt><bib>HomojMan&gcirc;antaj</bib>, <lok>&leftquot;solidareco de stomakoj&rightquot;, p.
        47a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">la <ind>Saint Valentin</ind></trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>