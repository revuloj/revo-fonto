<drv mrk="plekt.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plekt.cxirkaux0i.klasika">
    <dif>
      <tld lit="P"/>i ion &ccirc;irka&ubreve;:
      <ekz>
        li &ccirc;irka&ubreve;<tld/>is min per siaj brakoj
        <klr>(<tld/>is siajn brakojn &ccirc;irka&ubreve; min)</klr>
        <fnt><bib>Fab4</bib><lok>Filino de la mar&ccirc;a re&gcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        el la amaso eliris dekkelko da junaj virinoj kaj
        &ccirc;irka&ubreve;<tld/>is lian veturilon per floroj
        <fnt><bib>Far1</bib><lok>&ccirc;ap. 24</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[fajro]</klr> pasie &ccirc;irka&ubreve;<tld/>is
        la Teron per flamantaj brakoj
        <fnt>
          <aut>J. Baghy</aut>
          <vrk>Son&gcirc;o de la morto</vrk>
          <lok><url ref="http://www.tekstoj.nl/lm/lm22-3/songho.html">http://www.tekstoj.nl/lm/lm22-3/songho.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
<!-- lau mi la sekva senco estas jam kovrita de la supra...
  <snc mrk="plekt.cxirkaux0i.PIVa">
    <dif>
      Volvi kun <tld/>oj &ccirc;irka&ubreve; io:
      <ekz>
        &ccirc;irka&ubreve;<tld/>i izolan strion &ccirc;irka&ubreve; kablo
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
    <rim>
      Mankas ekzemploj por &ccirc;i tiu senco. Povas temi pri eraro de
      PIV, kiu cetere ne konas la unuan sencon, tamen ofte renkonteblan.
    </rim>
  </snc>
-->
  <trd lng="pl">oplata&cacute;</trd>
  <trd lng="ro">se &icirc;mpletesc</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_l;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_p;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43E;&c_b;&#x43F;&#x43B;&c_ib;&c_t;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43E;&c_b;&c_v;&c_i;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="plekt.inter0i">
  <kap>inter<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provizore a&ubreve; nekomplete <tld/>i:
      <ekz>
        la mulo trafis sub inter<tld/>itajn bran&ccirc;ojn de granda kverko, kaj lia kapo
        alkro&ccirc;i&gcirc;is
        <fnt>
          <bib>MT</bib>
          <lok>II. Samuel 18:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter<tld/>i&gcirc;inte kiel dornoj
        <fnt>
          <bib>MT</bib>
          <lok>Na&hcirc;um 1:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili rigardis sin reciproke kaj tenis la manojn inter<tld/>ite
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter<tld/>inte la brakojn reciproke post la &scirc;ultroj, ili sin levis en longa
        linio
        <fnt>
          <bib>Fab1</bib>
          <lok>La virineto de maro</lok>
        </fnt>;
      </ekz>
      <ekz>
        la patrino inter<tld/>is la manojn, kantis kaj ploris
        <fnt>
          <bib>Fab3</bib>
          <lok>Historio de unu patrino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la arboj sur &gcirc;iaj bordoj inter<tld/>is siajn foliri&ccirc;ajn suprojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="plekt.inter0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      <ref tip="super" cel="implik.0i.aferojn">Impliki</ref>,
      inter<ref tip="super" cel="lig.0i.interrilatigi">ligi</ref>:
      <ekz>
        mi intencis impliki &gcirc;in en inter<tld/>on 
        <klr>(<ref cel="implik.0ajxo">implika&jcirc;o</ref>)</klr> da intrigoj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        vi lerte inter<tld/>as la decidojn
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        eksonis akre kaj inter<tld/>ite kun rido la vo&ccirc;o de Karolino
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_l;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_l;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_p;&c_l;&c_i;&c_t;&c_a;&c_m;</trd>
  <trd lng="cs">muchlat</trd>
  <trdgrp lng="de">
    <trd>verschr&auml;nken</trd>,
    <trd>verflechten</trd>,
    <trd>verweben</trd>
  </trdgrp>
  <trd lng="en">interlace</trd>
  <trd lng="es">entrelazar</trd>
  <trd lng="fr">entrelacer</trd>
  <trd lng="hu">&ouml;sszefon</trd>
  <trdgrp lng="ja">
    <trd>&#x304B;&#x3089;&#x307F;&#x5408;&#x308F;&#x305B;&#x308B;  <pr>&#x304B;&#x3089;&#x307F;&#x3042;&#x308F;&#x305B;&#x308B;</pr></trd>,
    <trd>&#x7D44;&#x307F;&#x5408;&#x308F;&#x305B;&#x308B;  <pr>&#x304F;&#x307F;&#x3042;&#x308F;&#x305B;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="nl">dooreenstrengelen</trd>
  <trd lng="pl">przeplata&cacute;</trd>
  <trd lng="ro">&icirc;ntre&#x21B;ese</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_l;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_l;&c_je;&c_s;&c_t;&c_i;</trd>
