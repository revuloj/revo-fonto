<kap>
  <ofc>*</ofc>
  <rad>plekt</rad>/i
</kap>
<drv mrk="plekt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="kun.0igi">Kunigi</ref> plurajn fadenformajn objektojn, multfoje kaj regule
      interkrucigante ilin:
      <ekz>
        &scirc;i denove sin vestis kaj <tld/>is siajn longajn harojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        el elasta &scirc;elo de saliko kaj el junko ili <tld/>is fortikan reton
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia lango <tld/>as <klr>(kombinas)</klr> falsa&jcirc;on
        <klr>(kp <ref cel="intrig.0i">intrigi</ref>)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 50:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> <tld/>is el fojno grandan belan tapi&scirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia patrino <tld/>is el ledo kaj basto multe da ingoj por boteloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        du &ccirc;enetojn el pura oro <klr>[&#8230;]</klr> kuni&gcirc;antaj per siaj finoj, <tld/>itaj
        <klr>[&#8230;]</klr> alfortikigu <klr>[&#8230;]</klr> al la kadretoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        retoj <tld/>itaj <klr>[&#8230;]</klr> estis &ccirc;e la kapiteloj
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> plantis vinber&gcirc;ardenon kaj &ccirc;irka&ubreve;metis <tld/>obarilon
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 21:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is kronon el dornoj kaj metis &gcirc;in sur lian kapon
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 27:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[ili]</klr> <tld/>is mensogon pri mi
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 119:69</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="nod.0i">nodi</ref>,
       <ref cel="trik.0i">triki</ref>,
       <ref cel="teks.0i">teksi</ref>
    </refgrp>
    <bld
      lok="&WCU;/thumb/9/96/Eugenie_Bouchard_(14462328742).jpg/360px-Eugenie_Bouchard_(14462328742).jpg"
      prm="CC BY-SA 2.0"
      >sportistino kun har<tld/>a&jcirc;o
        <fnt>
          <aut>Tatiana el Moskvo, la foto montras: Eugenie Bouchard</aut>
          <lok><url ref="&WCW;/File:Eugenie_Bouchard_(14462328742).jpg"
              >https://commons.wikimedia.org/wiki/File:Eugenie_Bouchard_(14462328742).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_l;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_l;&c_je;&c_t;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>pl&eacute;st <klr>(cop, ko&scaron;&iacute;k)</klr></trd>,
    <trd>spl&eacute;tat</trd>,
    <trd>v&iacute;t</trd>
  </trdgrp>
  <trd lng="de">flechten</trd>
  <trd lng="en">braid</trd>
  <trd lng="es">trenzar</trd>
  <trd lng="fr">tresser</trd>
  <trdgrp lng="hu">
    <trd>fon</trd>,
    <trd>befon</trd>
  </trdgrp>
  <trd lng="nl">vlechten</trd>
  <trdgrp lng="pt">
    <trd>entran&ccedil;ar</trd>,
    <trd>tran&ccedil;ar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>plies&tcaron;</trd>,
    <trd>uvi&tcaron;</trd>,
    <trd>zaplies&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="plekt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Objekto farita el <tld/>itaj fadenoj a&ubreve; vergetoj:
      <ekz>
        pajlo<tld/>a&jcirc;o de se&gcirc;o;
      </ekz>
      <ekz>
        metalfadena <tld/>a&jcirc;o de la ka&gcirc;o<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tiuj ideoj penetras tamen tra la densa <tld/>a&jcirc;o de la homaj
        anta&ubreve;ju&gcirc;oj
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="krad.0o">krado</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_ja;&c_c;&c_jo;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_l;&c_ja;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trd lng="bg">&c_p;&c_l;&c_i;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="de">
    <trd>Geflochtenes</trd>,
    <trd>Geflecht</trd>,
    <trd>Flechtwerk</trd>
  </trdgrp>
  <trd lng="en">plait</trd>
  <trdgrp lng="fr">
    <trd>tresse</trd>,
    <trd>treillis</trd>,
    <trd>lacis</trd>
  </trdgrp>
  <trd lng="hu">fonat</trd>
  <trd lng="es">trenza</trd>
  <trdgrp lng="nl">
    <trd>vlecht</trd>,
    <trd>vlechtwerk</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_l;&c_je;&c_t;&c_jo;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_l;&c_je;&c_t;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="plekt.cxirkaux0i">
  <kap>&ccirc;irka&ubreve;<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="plekt.cxirkaux0i.klasika">
    <dif>
      <tld lit="P"/>i ion &ccirc;irka&ubreve;:
      <ekz>
        li &ccirc;irka&ubreve;<tld/>is min per siaj brakoj
        <klr>(<tld/>is siajn brakojn &ccirc;irka&ubreve; min)</klr>
        <fnt><bib>Fab4</bib><lok>Filino de la mar&ccirc;a re&gcirc;o</lok></fnt>;
      </ekz>
      <ekz>
        el la amaso eliris dekkelko da junaj virinoj kaj
        &ccirc;irka&ubreve;<tld/>is lian veturilon per floroj
        <fnt><bib>Far1</bib><lok>&ccirc;ap. 24</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[fajro,]</klr> kiu pasie &ccirc;irka&ubreve;<tld/>is
        la Teron per flamantaj brakoj
        <fnt>
        <aut>J. Baghy</aut>
        <vrk>Son&gcirc;o de la morto</vrk>
        <lok><url
        ref="http://www.tekstoj.nl/lm/lm22-3/songho.html">http://www.tekstoj.nl/lm/lm22-3/songho.html</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="plekt.cxirkaux0i.PIVa">
    <dif>
      Volvi kun <tld/>oj &ccirc;irka&ubreve; io:
      <ekz>
        &ccirc;irka&ubreve;<tld/>i izolan strion &ccirc;irka&ubreve; kablo
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
    <rim>
      Mankas ekzemploj por &ccirc;i tiu senco. Povas temi pri eraro de
      PIV, kiu cetere ne konas la unuan sencon, tamen ofte renkonteblan.
    </rim>
  </snc>
  <trdgrp lng="ru">
    <trd>&c_o;&c_p;&c_l;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_o;&c_p;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="plekt.inter0i">
  <kap>inter<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Provizore a&ubreve; nekomplete <tld/>i:
      <ekz>
        inter<tld/>i la manojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la arboj sur la bordoj inter<tld/>is siajn foliri&ccirc;ajn
        suprojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        inter<tld/>i la vortojn en ritmojn<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        inter<tld/>o <klr>(implika&jcirc;o)</klr> da intrigoj.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_l;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_l;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_v;&c_p;&c_l;&c_i;&c_t;&c_a;&c_m;</trd>
  <trd lng="cs">muchlat</trd>
  <trd lng="en">interlace</trd>
  <trd lng="es">entrelazar</trd>
  <trd lng="fr">entrelacer</trd>
  <trd lng="hu">&ouml;sszefon</trd>
  <trd lng="nl">dooreenstrengelen</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_p;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_p;&c_l;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_l;&c_je;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>h&uacute;&zcaron;va&tcaron;</trd>,
    <trd>krkva&tcaron;</trd>,
    <trd>kr&ccaron;i&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="plekt.mal0idis0i">
  <kap>mal<tld/>i, <var><kap>dis<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disigi la <tld/>itajn erojn:
      <ekz>
        mal<tld/>i trika&jcirc;on, nodon, intrigon.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_l;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="bg">&c_r;&c_a;&c_z;&c_p;&c_l;&c_i;&c_t;&c_a;&c_m;</trd>
  <trd lng="cs">rozpl&eacute;st</trd>
  <trdgrp lng="de">
    <trd>aufflechten</trd>,
    <trd>entflechten</trd>,
    <trd>entwirren</trd>
  </trdgrp>
  <trd lng="es">destrenzar</trd>
  <trdgrp lng="hu">
    <trd>felfejt</trd>,
    <trd>sz&eacute;tfejt</trd>,
    <trd>kibont</trd>
  </trdgrp>
  <trd lng="nl">ontvlechten</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_l;&c_je;&c_s;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_a;&c_s;&c_p;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>rozuzli&tcaron; (uzol</trd>,
    <trd>vyp&aacute;ra&tcaron; <klr>(pleten&eacute;)</klr></trd>,
    <trd>z&aacute;pletku)</trd>
  </trdgrp>
</drv>

<drv mrk="plekt.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Krucige trapasigi tra <tld/>a&jcirc;o specialan elementon:
      <ekz>
        tra<tld/>i bluan &scirc;tofon per flavaj fadenoj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        argumentoj tra<tld/>itaj per diversaj cita&jcirc;oj kaj misteraj
        terminoj teknikaj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        tra<tld/>i siajn argumentojn per multo da la&ubreve;taj a&ubreve;toritataj
        nomoj<fnt>Z</fnt>,
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        da kvaza&ubreve;sciencaj frazoj<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_a;&c_p;&c_l;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>
  </trdgrp>
  <trd lng="cs">propl&eacute;st</trd>
  <trdgrp lng="de">
    <trd>hindurchflechten</trd>,
    <trd>durchflechten</trd>,
  </trdgrp>
  <trd lng="en">intertwine</trd>
  <trdgrp lng="hu">
    <trd>&aacute;tfon</trd>,
    <trd>&aacute;tsz&odblac;</trd>
  </trdgrp>
  <trd lng="nl">doorvlechten</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_l;&c_je;&c_t;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_p;&c_l;&c_je;&c_s;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="sk">preplies&tcaron;</trd>
</drv>
</art>
