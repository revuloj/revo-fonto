<drv mrk="plekt.mal0idis0i">
  <kap>mal<tld/>i, <var><kap>dis<tld/>i</kap></var></kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Disigi la <tld/>itajn erojn:
      <ekz>
        la virina figuro dis<tld/>is la manojn kaj kovris per ili sian viza&gcirc;on
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        la virino ku&scirc;is sur kuseno, kiun &scirc;iaj dis<tld/>itaj, ru&gcirc;ebrunaj haroj abunde
        kovris
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        viaj demandoj konfuzas min, &ndash; mi volas nur mal<tld/>i interkonfuza&jcirc;ojn
        <fnt>
          <bib>I&scirc;tar</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        mi suferas pro la nemal<tld/>ebla situacio, en kiu ni amba&ubreve; trovi&gcirc;as
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi klopodas mal<tld/>i la sencon de tiu duonma&scirc;ina kodo
        <fnt>
          <aut>Sergeo DEREVIANNYKH</aut>
          <vrk><url ref="&Monato;/2002/pdf/monato200204.pdf#page=18">&Ccirc;u vi tradukas
          france?</url></vrk>
          <bib>Monato</bib>
          <lok>2002/04, p. 18</lok>
         </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="implik.mal0i">malimpliki</ref>,
      <ref cel="konfuz.mal0i">malkonfuzi</ref>,
      <ref cel="nod.mal0i">malnodi</ref>,
      <ref cel="solv.0i.KOMUNE">solvi<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_a;&c_s;&c_p;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
