<drv mrk="plekt.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      Objekto farita per <tld/>ado:
      <ekz>
        la kompatinda birdo la&ubreve;te plendis pri sia perdita libereco kaj frapadis per siaj
        flugiloj la metalfadenan <tld/>a&jcirc;on de la ka&gcirc;o
        <fnt>
          <bib>Fab1</bib>
          <lok>Lekanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        virinoj sin vestu per decaj vestoj, kun modesteco kaj sobreco, ne kun
        har<tld/>a&jcirc;oj, a&ubreve; oro, a&ubreve; perloj
        <fnt>
          <bib>NT</bib>
          <lok>I. Timoteo 2:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni vidis <klr>[&#x2026;]</klr> kelke da ridetantaj infanaj kapetoj, elklini&gcirc;antaj el
        la bran&ccirc;aj <tld/>a&jcirc;oj
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve;ita de dika <tld/>a&jcirc;o de nigraj harligoj, la viza&gcirc;o de la &ccirc;etable sidanta
        virino bele kaj severtrajte distingi&gcirc;is
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="krad.0o">krado</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_ja;&c_c;&c_jo;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_l;&c_ja;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
