<drv mrk="plekt.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="kun.0igi">Kunigi</ref> plurajn fadenformajn objektojn, multfoje kaj
      regule
      interkrucigante ilin:
      <ekz>
        &scirc;i denove sin vestis kaj <tld/>is siajn longajn harojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        el elasta &scirc;elo de saliko kaj el junko ili <tld/>is fortikan reton
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        lia lango <tld/>as <klr>(kombinas)</klr> falsa&jcirc;on
        <klr>(kp <ref cel="intrig.0i">intrigi</ref>)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 50:19</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[&scirc;i]</klr> <tld/>is el fojno grandan belan tapi&scirc;on
        <fnt>
          <bib>Fab1</bib>
          <lok>Elinjo-fingreto</lok>
        </fnt>;
      </ekz>
      <ekz>
        mia patrino <tld/>is el ledo kaj basto multe da ingoj por boteloj
        <fnt>
          <bib>Fab1</bib>
          <lok>Ligo de amikeco</lok>
        </fnt>;
      </ekz>
      <ekz>
        du &ccirc;enetojn el pura oro <klr>[&#x2026;]</klr> kuni&gcirc;antaj per siaj finoj, <tld/>itaj
        <klr>[&#x2026;]</klr> alfortikigu <klr>[&#x2026;]</klr> al la kadretoj
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 28:14</lok>
        </fnt>;
      </ekz>
      <ekz>
        retoj <tld/>itaj <klr>[&#x2026;]</klr> estis &ccirc;e la kapiteloj
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:17</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> plantis vinber&gcirc;ardenon kaj &ccirc;irka&ubreve;metis <tld/>obarilon
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 21:33</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili <tld/>is kronon el dornoj kaj metis &gcirc;in sur lian kapon
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 27:29</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <klr>[ili]</klr> <tld/>is mensogon pri mi
        <fnt>
          <bib>MT</bib>
          <lok>Psalmaro 119:69</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="nod.0i">nodi</ref>,
       <ref cel="trik.0i">triki</ref>,
       <ref cel="teks.0i">teksi</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/9/96/Eugenie_Bouchard_(14462328742).jpg/360px-Eugenie_Bouchard_(14462328742).jpg" prm="CC BY-SA 2.0">sportistino kun har<tld/>a&jcirc;o
        <fnt>
          <aut>Tatiana el Moskvo, la foto montras: Eugenie Bouchard</aut>
          <lok><url ref="&WCW;/File:Eugenie_Bouchard_(14462328742).jpg">&WCW;/File:Eugenie_Bouchard_(14462328742).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_l;&c_je;&c_s;&c_mol;&c_c;&c_ib;</trd>,
    <trd>&c_z;&c_a;&c_p;&c_l;&c_ja;&c_t;&c_a;&c_c;&c_mol;</trd>,
