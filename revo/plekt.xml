<drv mrk="plekt.tra0i">
  <kap>tra<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Krucige trapasigi tra <tld/>a&jcirc;o specialan elementon:
      <ekz>
        mirindaj grimpokreska&jcirc;oj, tiaj <klr>[&#x2026;]</klr> kiaj ofte estas tra<tld/>itaj en
        la &ccirc;apitrokomencaj literoj
        <fnt>
          <bib>Fab1</bib>
          <lok>&Gcirc;ardeno de la paradizo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> ankroj estis tegitaj per brilanta oro, kaj &ccirc;iu &scirc;nurego estis
        tra<tld/>ita per silko
        <fnt>
          <bib>Fab4</bib>
          <lok>Historio el la dunoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        instruitaj paroloj tre imponas, precipe se ili en sufi&ccirc;a mezuro estas
        tra<tld/>itaj per diversaj citatoj
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        vi kutimis rilati kun estimo nur al tiaj argumentoj, kiuj estas plenigitaj per
        multo da citatoj, tra<tld/>itaj per multo da la&ubreve;taj a&ubreve;toritataj nomoj
        <fnt>
          <bib>EE</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="sxov.en0i.FIG">en&scirc;ovi<sncref/></ref>
