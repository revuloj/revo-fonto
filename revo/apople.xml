<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>apopleksi</rad>/o
</kap>
<drv mrk="apople.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Cerba sangalfluo, kiu senigas la malsanulon je la sentoj kaj movoj,
      sed ne
      de la spirado.
    </dif>
    <refgrp tip="sin">
      <ref cel="parali.0o">paralizo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>apoplexie</trd>,
    <trd>ictus apoplectique</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_p;&c_o;&c_p;&c_l;&c_je;&c_k;&c_s;&c_i;&c_ja;</trd>,
    <trd>&c_i;&c_n;&c_s;&c_u;&c_l;&c_mol;&c_t;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_r; <klr tip="ind">(&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;)</klr></trd>
  </trdgrp>
</drv>
</art>
