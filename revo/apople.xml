<art mrk="$Id: apople.xml,v 1.25 2023/10/13 16:46:48 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>apopleksi</rad>/o
</kap>
<drv mrk="apople.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Cerba sangalfluo, kiu senigas la <ref tip="lst" cel="san.mal0o.MED" lst="voko:malsanoj">malsan</ref>ulon je la
      sentoj kaj movoj,
      sed ne de la spirado:
      <ekz>
        la <tld/>o trafis &scirc;in tiun posttagmezon kaj embarasigis la
        kunvoja&gcirc;antojn<fnt><aut>Spomenka &Scirc;timec</aut>: <vrk>Morto en
        Melburna tramo</vrk>, <bib>Monato</bib><lok>jaro 1997a, numero
        8a,
        p. 24a</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="parali.0o">paralizo</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_a;&c_p;&c_a;&c_p;&c_l;&c_je;&c_k;&c_s;&c_ib;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>apoplexie</trd>,
    <trd>mrtvice</trd>,
    <trd>&scaron;lak</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Apoplexie</trd>,
    <trd>Schlaganfall</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>apoplexy</trd>,
    <trd>stroke</trd>
  </trdgrp>
  <trd lng="es">apopleg&iacute;a</trd>
  <trdgrp lng="fr">
    <trd>apoplexie</trd>,
    <trd>ictus apoplectique</trd>
  </trdgrp>
  <trd lng="he">&#x5E9;&#x5D1;&#x5E5;</trd>
  <trdgrp lng="hu">
    <trd>sz&eacute;l&uuml;t&eacute;s</trd>,
    <trd>agyv&eacute;rz&eacute;s</trd>,
    <trd>sz&eacute;lh&udblac;d&eacute;s</trd>
  </trdgrp>
  <trdgrp lng="id">
    <trd>apopleksi</trd>,
    <trd>strok</trd>
  </trdgrp>
   <trdgrp lng="ja">
    <trd>&#x5352;&#x4E2D;  <pr>&#x305D;&#x3063;&#x3061;&#x3085;&#x3046;</pr></trd>,
    <trd>&#x8133;&#x5352;&#x4E2D;  <pr>&#x306E;&#x3046;&#x305D;&#x3063;&#x3061;&#x3085;&#x3046;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>apopleksja</trd>,
    <trd>udar</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_a;&c_p;&c_o;&c_p;&c_l;&c_je;&c_k;&c_s;&c_i;&c_ja;</trd>,
    <trd>&c_i;&c_n;&c_s;&c_u;&c_l;&c_mol;&c_t;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_r;<klr tip="ind"> (&c_b;&c_o;&c_l;&c_je;&c_z;&c_n;&c_mol;)</klr></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>apoplexia</trd>,
    <trd>mozgov&aacute; <ind>por&aacute;&zcaron;ka</ind></trd>,
    <trd>mozgov&aacute; por&aacute;&zcaron;ka</trd>,
    <trd>m&racute;tvica</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&#x43F;&#x43E;&#x43F;&#x43B;&c_je;&#x43A;&c_s;&c_ib;&#x44F;</trd>,
    <trd>&c_ib;&#x43D;&c_s;&c_u;&#x43B;&#x44C;&c_t;</trd>,
    <trd>&c_u;&c_d;&c_a;&c_r;</trd>
  </trdgrp>
</drv>
</art>
