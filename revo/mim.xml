<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: mim.xml,v 1.5 2023/05/05 17:57:59 afido Exp $">
    <kap>
      <rad>mim</rad>/o
    </kap>

<drv mrk="mim.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <snc mrk="mim.0o.pantomimisto">  
    <dif>
      <ref tip="dif" cel="pantom.0isto">Pantomimisto</ref>:
      <ekz>
        praktika edukado fare de aktoro, <tld/>o, magiisto
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2004/008451.html">Ride resani&gcirc;i</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>-arto estas la esprimformo, kiu forte enradiki&gcirc;is en &Ccirc;e&hcirc;io kaj Slovakio
        <fnt>
          <bib>Monato</bib>
          <aut>Brigitte Faverial</aut>
          <vrk><url ref="&Monato;/2003/007994.html">Datreveno por ridi</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>   
    <ref tip="vid" cel="mimik.0o">mimiko</ref>
    <trd lng="de">Mime</trd>
  </snc>
  <trd lng="en">mime</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_ib;&c_m;</trd>,
    <trd>&c_m;&c_ib;&c_m;&c_ib;&c_s;&c_t;</trd>
  </trdgrp>
</drv>

<drv mrk="mim.0i">
  <kap>
    <tld/>i
  </kap>
  <uzo tip="fak">TEA</uzo>
  <snc mrk="mim.0i.ludi">
    <dif>
      Ludi pantomimon;
      geste <ref tip="super" cel="imit.0i.agi">imiti</ref>:
      <ekz>
        la roluloj nur pupece gestas kaj provas <tld/>i vivan kantadon
        <fnt>
          <bib>Monato</bib>
          <aut>Franz-Georg R&ouml;ssler</aut>
          <vrk><url ref="&Monato;/2013/010944.php">Kanto de la montoj</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis perfekta komedianto, kiel parole, tiel anka&ubreve; <tld/>e
        <fnt>
          <aut>Joh&aacute;n Valano</aut>
          <vrk><url ref="&Tekstaro;?nomo=chu-li-bremsis-sufiche">&Ccirc;u li bremsis
          sufi&ccirc;e?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sekvas muziko, &jcirc;onglado, <tld/>ado, dancado
        <fnt>
          <bib>Monato</bib>
          <aut>Walter Klag</aut>
          <vrk><url ref="&Monato;/2004/008451.html">Ride resani&gcirc;i</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        kelkaj signoj ja estas universalaj, &ccirc;ar ili <tld/>as agon, kiun &ccirc;iuj priskribas
        sammaniere, a&ubreve; &ccirc;ar temas pri gestoj pruntitaj de unu signolingvo al la alia
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2014/011278.php">Vortaro intuicia, horizonto
          internacia</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">mimen</trd>
  <trdgrp lng="en">
    <trd>mime</trd>,
    <trd>pantomime</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_a;&c_v;&c_a;&c_t;&c_i; &c_m;&c_ib;&c_m;&c_ib;&c_k;&c_o;&c_ju;</trd>,
    <trd>&c_zh;&c_je;&c_s;&c_t;&c_a;&c_m;&c_i;</trd>
  </trdgrp>
</drv>


  </art>

