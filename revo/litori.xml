<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>litorin</rad>/o
</kap>
<drv mrk="litori.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el la familio <ref tip="super" cel="litori.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Littorina</trd>),  mara gastropodo kun konusa konko,
      vivanta en tajdaj regionoj.
    </dif>
  </snc>
  <trd lng="de">Strandschnecke</trd>
  <trd lng="en">periwinkle</trd>
</drv>
<drv mrk="litori.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
     <dif>
       Familio el la klaso <ref tip="super" cel="gastro2.0oj">gastropodoj</ref>
       (<trd lng="la">Littorinidae</trd>).
     </dif>
  </snc>
  <trd lng="de">Strandschnecke <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
