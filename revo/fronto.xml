<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>fronton</rad>/o
</kap>
<drv mrk="fronto.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Triangula a&ubreve; duonronda ornama&jcirc;o, &ccirc;e la anta&ubreve;a parto de konstrua&jcirc;o,
      super pordo, portiko a&ubreve; fenestro. 
    </dif>
    <refgrp tip="vid">
      <ref cel="fasad.0o">fasado</ref>,
      <ref cel="front.0o">fronto</ref>,
      <ref cel="frunt.0o">frunto</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Giebeldreieck</trd>
  <trd lng="fr">fronton</trd>
  <trd lng="ru">&c_f;&c_r;&c_o;&c_n;&c_t;&c_o;&c_n;</trd>
</drv>
</art>
