<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>fronton</rad>/o
</kap>
<drv mrk="fronto.0o">
  <kap><tld/>o</kap>
  <snc mrk="fronto.0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Triangula a&ubreve; duonronda ornama&jcirc;o, &ccirc;e la anta&ubreve;a parto de
      konstrua&jcirc;o,
      super pordo, portiko a&ubreve; fenestro:
      <ekz>
        li povis <klr>...</klr> prezenti al si <klr>...</klr>, kiel la
        strato anta&ubreve;e kredeble aspektis kun siaj &scirc;tuparoj, balkona&jcirc;oj
        kaj
        pintaj <tld/>oj
        <fnt><bib>Fab2</bib>, <lok>Malnova domo</lok></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="fasad.0o">fasado</ref>,
      <ref cel="front.0o">fronto</ref>,
      <ref cel="frunt.0o">frunto</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_z;&c_a;&c_k;&c_o;&c_t;</trd>,
    <trd>&c_f;&c_r;&c_a;&c_n;&c_t;&c_o;&c_n;</trd>
  </trdgrp>
  <trd lng="ca">front&oacute;</trd>
  <trd lng="de">Giebeldreieck</trd>
  <trd lng="en">pediment</trd>
  <trd lng="es">front&oacute;n</trd> 
  <trd lng="fa">&a_s;&a_n;&a_t;&a_w;&a_r;&f_y;</trd>
  <trd lng="fr">fronton</trd>
  <trd lng="hu">oromzat</trd>
  <trd lng="pl">fronton</trd>
  <trd lng="pt">front&atilde;o</trd>
  <trd lng="ru">&c_f;&c_r;&c_o;&c_n;&c_t;&c_o;&c_n;</trd>
</drv>
</art>
