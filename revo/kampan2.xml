<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kampan2.xml,v 1.25 2019/06/30 07:42:48 revo Exp $">
<kap>
  <rad>kampanul</rad>/o
  <fnt><bib>Fab1</bib></fnt>
</kap>
<drv mrk="kampan2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio de
      <ref tip="malprt" cel="kampan3.0oj"><tld/>acoj</ref>
      (<trd lng="la">Campanula</trd>), kun <ref cel="sonor.0ilo">sonorilo</ref>formaj plej ofte bluaj
      floroj:
      <ekz>
        bluaj <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>;
      </ekz>
      <ekz>
        kvindeko da diversaj specioj de <tld/>oj kaj aliaj klo&scirc;formaj floroj
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007297.html">Kolekti sonorilojn</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas japane nomata <nac>hotaru-bukuro</nac> (sako por lampiroj),
        &ccirc;ar anta&ubreve;e oni metis lampirojn en tiun &ccirc;i blankan floron kaj &gcirc;uis delikatan lumon
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Yasuo</aut>
          <vrk><url ref="https://www.monato.be/2003/007854.html">&Ccirc;u &gcirc;oja plorpluvo?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <bld
     lok="&WCU;/thumb/1/14/0_Campanula_rapunculoides_-_Yvoire.jpg/504px-0_Campanula_rapunculoides_-_Yvoire.jpg"
     prm="CC BY-SA 3.0"
     ><ind>kampa <tld/>o</ind> (Campanula rapunculoides)
       <fnt>
         <aut>Jean-Pol Grandmont, propra verko</aut>
         <lok><url ref="&WCW;/File:0_Campanula_rapunculoides_-_Yvoire.jpg"
             >&WCW;/File:0_Campanula_rapunculoides_-_Yvoire.jpg</url></lok>
       </fnt>
    <trd lng="la">Campanula rapunculoides</trd>
    <trd lng="pl">dzwonek jednostronny</trd>
  </bld>
  <trd lng="be">&c_z;&c_v;&c_a;&c_n;&c_o;&c_ch;&c_a;&c_k; <klr tip="amb">(&c_k;&c_v;&c_je;&c_t;&c_k;&c_a;)</klr></trd>
  <trd lng="bg">&c_k;&c_a;&c_m;&c_b;&c_a;&c_n;&c_k;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>g&egrave;nere Campanula</trd>,
    <trd>campaneta</trd>
  </trdgrp>
  <trd lng="de">Glockenblume</trd>
  <trd lng="en">bellflower</trd>
  <trd lng="fr">campanule</trd>
  <trdgrp lng="hu">
    <trd>harangvir&aacute;g</trd>,
    <trd>csengetty&udblac;ke</trd></trdgrp>
  <trd lng="nl">klokjesbloem</trd>
  <trd lng="pl">dzwonek</trd>
  <trd lng="ru">&c_k;&c_o;&c_l;&c_o;&c_k;&c_o;&c_l;&c_mol;&c_ch;&c_i;&c_k;</trd>
  <trd lng="sk">zvon&ccaron;ek</trd>
  <trd lng="cs">zvonek</trd>
</drv>

</art>
