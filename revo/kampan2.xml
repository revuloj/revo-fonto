    <dif>
      Familio de dukotiledonaj vegeta&jcirc;oj
      (<trd lng="la">Campanulaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Glockenblumengew&auml;chse</trd>
</drv>
