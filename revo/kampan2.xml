<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>kampanul</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="kampan2.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Genro el familio de <ref tip="super" cel="kampan2.0acoj"><tld/>acoj</ref>
      (<trd lng="la">Campanula</trd>), kun sonorilformaj plej ofte bluaj
      floroj.
    </dif>
  </snc>
  <trd lng="de">Glockenblume</trd>
  <trd lng="fr">campanule</trd>
  <trd lng="en">bellflower</trd>
</drv>
<drv mrk="kampan2.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Campanulaceae</trd>).
    </dif>
  </snc>
  <trd lng="de">Glockenblumengew&auml;chse</trd>
</drv>
</art>
