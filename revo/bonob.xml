<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bonob.xml,v 1.9 2023/10/29 19:11:02 revo Exp $">
<kap>
    <rad>bonob</rad>/o
</kap>
<drv mrk="bonob.0o">
  <kap><tld/>o</kap>
  <snc mrk="bonob.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="dif" cel="cximpa.malgranda0o.ZOO">Malgranda &ccirc;impanzo</ref>:
      <ekz>
        nun la specio <tld lit="B"/>o estas minacata, restas nur kelkaj
        dekmiloj da individuoj en <klr>[&#x2026;]</klr> la sova&gcirc;a arbaro en
        Kongo
        <fnt>
          <aut>Gerardo</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/33130/150148/comment/5627303">
          Ni kaj nia kuzeto &leftquot;bonobo&rightquot;</url></vrk>,
          <lok>2009-05-10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj &scirc;ajne preferas dividi sian man&gcirc;a&jcirc;on
        <fnt>
          <aut>-</aut>,
          <vrk><url ref="http://www.eventeo.net/web/index.php/nordameriko/sciencoj/1844-impanzoj-bonoboj-libervole-dividas-manaon">
          &Ccirc;impanzoj bonoboj...</url></vrk>,
          <lok>eventeo, 2010-03-09</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_b;&c_a;&c_n;&c_o;&c_b;&c_a;</trd>
    <trd lng="bg">&c_b;&c_o;&c_n;&c_o;&c_b;&c_o;</trd>
    <trdgrp lng="de">
      <trd>Bonobo</trd>,
      <trd>Zwergschimpanse</trd>
    </trdgrp>
    <trd lng="fr">bonobo</trd>
    <trdgrp lng="hu">
      <trd>bonob&oacute;</trd>,
      <trd>t&ouml;rpecsimp&aacute;nz</trd>
    </trdgrp>
    <trd lng="it">bonobo</trd>
    <trd lng="zh">&#x502D;&#x9ED1;&#x7329;&#x7329; <pr>w&#x14D;h&#x113;ix&#x12B;ngxing</pr></trd>
</snc>
</drv>
</art>

