<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>Volg</rad>/o
    </kap>
    <drv mrk="volg.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">GEOG</uzo>
      <fnt><bib>ZR</bib></fnt>
      <snc>
	<dif>
          Rivero en <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>,
          la plej longa en
          <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>,
          enfluanta <ref cel="kaspi.0o">Kaspion</ref>.
	</dif>
      </snc>
      <trd lng="be">&c_V;&c_o;&c_l;&c_g;&c_a;</trd>
      <trd lng="de">Wolga</trd>
      <trd lng="en">Volga</trd>
      <trd lng="es">Volga</trd>
      <trd lng="hu">Volga</trd>
      <trd lng="nl">Wolga</trd>
      <trd lng="ru">&c_V;&c_o;&c_l;&c_g;&c_a;</trd>
    </drv>
  </art>

