<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: volg.xml,v 1.18 2022/03/28 11:08:55 revo Exp $">
  <kap>
    <rad>Volg</rad>/o
    <fnt><bib>ZR</bib></fnt>
  </kap>
<drv mrk="volg.V0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="lst" lst="voko:riveroj" cel="river.0o.GEOG">Rivero</ref>
      en <ref tip="malprt" cel="rus.R0ujo">Rusujo</ref>,
      la plej longa en <ref tip="malprt" cel="euxrop1.E0o">E&ubreve;ropo</ref>,
      enfluanta <ref cel="kaspi.K0o">Kaspion</ref>:
      <ekz>
        <tld/>o komenci&gcirc;as sur la Valdaja Alta&jcirc;o kaj enfluas la Kaspian maron &ccirc;e &gcirc;ia nordokcidenta bordo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=12593">Volgo</url></lok>
        </fnt>.
      </ekz>
     </dif>
  </snc>
  <trd lng="be">&c_V;&c_o;&c_l;&c_g;&c_a;</trd>
  <trd lng="bg">&c_V;&c_o;&c_l;&c_g;&c_a;</trd>
  <trd lng="ca">Volga</trd>
  <trd lng="cs">Volha</trd>
  <trd lng="de">Wolga</trd>
  <trd lng="en">Volga</trd>
  <trd lng="es">Volga</trd>
  <trd lng="fa">&a_w;&a_l;&f_g;&a_A;</trd>
  <trd lng="fr">Volga</trd>
  <trd lng="hu">Volga</trd>
  <trd lng="nl">Wolga</trd>
  <trd lng="pl">Wo&lstroke;ga</trd>
  <trdgrp lng="pt">
     <trd>Volga</trd>
  </trdgrp>
  <trd lng="ru">&c_V;&c_o;&c_l;&c_g;&c_a;</trd>
  <trd lng="sk">Volga</trd>
  <trd lng="uk">&#x412;&#x43E;&#x43B;&#x433;&#x430;</trd>
</drv>
  </art>

