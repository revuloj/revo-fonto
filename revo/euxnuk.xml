<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: euxnuk.xml,v 1.20 2023/11/07 20:34:12 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>e&ubreve;nuk</rad>/o
</kap>
<drv mrk="euxnuk.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="vid" cel="kastr.0i">Kastrita</ref> viro, servanta kiel
      <ref tip="vid" cel="gard.0isto">gardisto</ref> de <ref tip="vid" cel="harem.0o">haremoj</ref>, oficisto, kantisto: 
      <ekz>
        la sep <tld/>oj, kiuj servadis anta&ubreve; la re&gcirc;o <nom>A&hcirc;a&scirc;vero&scirc;</nom>
        <fnt>
          <bib>MT</bib>
          <lok>Ester 1:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Hegaj</nom>, la re&gcirc;a <tld/>o, gardisto de la virinoj
        <fnt>
          <bib>MT</bib>
          <lok>Ester 2:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        estas <tld/>oj, kiuj estas naskitaj tiaj el la patrina ventro, kaj 
        estas <tld/>oj, kiuj estis <tld/>igitaj de homoj, kaj estas
        <tld/>oj, kiuj <tld/>igis sin pro la regno de la &ccirc;ielo
        <fnt>
          <bib>NT</bib>
          <lok>Mateo 19:12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_je;&c_w;&c_n;&c_u;&c_h;</trd>
  <trd lng="bg">&c_je;&c_v;&c_n;&c_u;&c_h;</trd>
  <trdgrp lng="ca">
    <trd>eunuc</trd>,
    <trd>espad&oacute;</trd>
  </trdgrp>
  <trd lng="cs">eunuch</trd>
  <trd lng="de">Eunuch</trd>
  <trd lng="en">eunuch</trd>
  <trd lng="es">eunuco</trd>
  <trdgrp lng="fa">
    <trd>&a_kh;&a_w;&a_A;&a_j;&a_h;</trd>,
    <trd>&a_A;&a_kh;&a_t;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">eunuque</trd>
  <trd lng="hu">eunuch</trd>
  <trd lng="io">eunuko</trd>
  <trd lng="nl">eunuch</trd>
  <trd lng="pl">eunuch</trd>
  <trd lng="pt">eunuco</trd>
  <trd lng="ru">&c_je;&c_v;&c_n;&c_u;&c_h;</trd>
  <trd lng="uk">&#x454;&#x432;&#x43D;&#x443;&#x445;</trd>
  <trdgrp lng="zh">
    <trd>&#x5944;&#x4EBA; <pr>y&#x1CE;nr&#xE9;n</pr></trd>,
    <trd>&#x592A;&#x76D1; <pr>t&#xE0;iji&#xE0;n</pr></trd>,
    <trd>&#x73F0; <pr>d&#x101;ng</pr></trd>,
    <trd>&#x95B9; <pr>y&#x101;n</pr></trd>
  </trdgrp>
</drv>
</art>
