<drv mrk="civit.0o">
  <kap><tld/>o</kap>
  <snc mrk="civit.0o.urbo">
    <uzo tip="fak">HIS</uzo>
    <dif>
      A&ubreve;tonoma <ref tip="super" cel="urb.0o">urbo</ref>, konsiderata kiel politika tuto:
      <ekz>
        <klr>(pri Jerusalemo)</klr> Mi restarigos viajn ju&gcirc;istojn, kiel anta&ubreve;e, kaj viajn
        konsilistojn, kiel en la
        komenco, post tio oni nomos vin urbo de virto, <tld/>o fidela
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 1:26</lok>
        </fnt>;
      </ekz>
      <ekz>
        la &gcirc;eneva <tld/>o <klr>[&#x2026;]</klr> en la 16a jarcento bruligis Serveton sur la
        &scirc;tiparo de maltoleremo
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Steloj;monato2/006847.html">Opinio: Sobraj pensoj de sobrulo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="respub.0o">respubliko</ref>,
      <ref cel="sxtat.0o">&scirc;tato</ref>
    </refgrp>
    <ref tip="vid" cel="civili1.0o">civilizacio</ref>
    <trdgrp lng="be">
      <trd>&c_g;&c_o;&c_r;&c_a;&c_d;-&c_d;&c_z;&c_ja;&c_r;&c_zh;&c_a;&c_v;&c_a;</trd>,
      <trd>&c_p;&c_o;&c_l;&c_ib;&c_s;</trd>
    </trdgrp>
   <trdgrp lng="de">
     <trd>Stadtstaat</trd>,
     <trd>Polis</trd>
   </trdgrp>
   <trd lng="en">polity</trd>
   <trdgrp lng="he">
     <trd>&mem;&shin;&tet;&resh;</trd>,
     <trd>&mem;&dalet;&yod;&nun;&he;</trd>,
     <trd>&ayin;&yod;&resh;&yod;&yod;&he;</trd>
   </trdgrp>
   <trd lng="fr">cit&eacute;</trd>
   <trdgrp lng="it">
   <trd>citt&agrave;<klr> (pol.)</klr></trd>, 
   <trd>comune<klr> (pol., stor.)</klr></trd>, 
   <trd>polis<klr> (pol., stor.)</klr></trd>
   </trdgrp>
   <trd lng="pl">pa&nacute;stwo-miasto</trd>
   <trdgrp lng="ru">
     <trd>&c_p;&c_o;&c_l;&c_i;&c_s;</trd>,
     <trd>&c_g;&c_o;&c_r;&c_o;&c_d;-&c_g;&c_o;&c_s;&c_u;&c_d;&c_a;&c_r;&c_s;&c_t;&c_v;&c_o;</trd>
   </trdgrp>
  </snc>
  <snc mrk="civit.0o.ano">
    <uzo tip="stl">EVI</uzo>
    <dif>
      Civitano:
      <ekz>
        mi vidis vin, <tld/>o, en Parizo, sur Triumfa Arko
        <fnt>
          <bib>FdO</bib>
        </fnt>;
      </ekz>
    </dif>
  </snc>

