        novaj sociaj kaj <tld/>ismaj movadoj
        <fnt>
          <bib>MD02</bib>
          <vrk>Gravegaj balotoj por la sudamerika giganto</vrk>
        </fnt>.
      </ekz>
     </dif>
   </snc>
  <trd lng="cs">ob&ccaron;ansk&eacute; uv&ecaron;dom&ecaron;n&iacute;</trd>
