<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: progno.xml,v 1.22 2020/11/14 22:32:22 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>prognoz</rad>/o
</kap>
<drv mrk="progno.0o">
  <kap><tld/>o</kap>
  <snc mrk="progno.0o.konjekto">
    <dif>
      <ref tip="lst" lst="voko:komunikoj" cel="konjek.0o">Konjekto</ref>, bazita sur difinitaj signoj, pri okazonta&jcirc;o:
      <ekz>
        la&ubreve; <tld/>oj de la Monda Organiza&jcirc;o pri Sano la
        nombro de la kancermalsani&gcirc;oj kreskos &gcirc;is la jaro 2020<fnt><aut>Evgeni
        Georgiev</aut>: <vrk>Novaj kuracmetodoj kaj prevento</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 4a, p. 22a</lok></fnt>;
      </ekz>
      <ekz>
        tri jardekojn poste, evidenti&gcirc;as ke tiuj <tld/>oj estis tro pesimismaj
        <fnt>
          <bib>Monato</bib>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007184.html">Petrolo: Nehaltigeblaj prezalti&gcirc;oj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        favoraj konjunktur-<tld/>oj
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006835.html">Germanio: Pekoj de patriarko</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_n;&c_o;&c_z;</trd>
  <trdgrp lng="cs">
    <trd>predikce</trd>,
    <trd>progn&oacute;za</trd>,
    <trd>p&rcaron;edpov&ecaron;&dcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Prognose</trd>,
    <trd>Vorhersage</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>prognosis</trd>,
    <trd>prediction</trd>
  </trdgrp>
  <trd lng="es">pron&oacute;stico</trd>
  <trd lng="fa">&f_p;&f_y;&a_sh;&zwnj;&a_b;&f_y;&a_n;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>pr&eacute;vision</trd>,
    <trd>pronostic</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&odblac;rejelz&eacute;s</trd>,
    <trd>progn&oacute;zis</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voorspelling</trd>,
    <trd>prognose</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>prognoza</trd>,
    <trd>przewidywanie</trd>,
    <trd>rokowanie</trd>,
    <trd>spodziewany przebieg</trd>
  </trdgrp>
  <trd lng="pt">pogn&oacute;stico</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_n;&c_o;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>predpove&dcaron;</trd>,
    <trd>progn&oacute;za</trd>
  </trdgrp>
  <trd lng="sv">prognos</trd>
</drv>

<drv mrk="progno.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="progno.0i.antauxdiri">
    <dif>
      <ref tip="super" cel="dir.antaux0i">Anta&ubreve;diri</ref>
      surbaze de enketo, kalkuloj a&ubreve; takso de situacio:
      <ekz>
        dum en la lasta balotado la diversaj sonda&jcirc;oj
        &gcirc;uste <tld/>is la rezulton, &ccirc;i-foje ili maltrafis
        <fnt><aut>Karlo Juh&aacute;sz</aut>:
        <vrk>Apena&ubreve; venko</vrk>, <bib>Monato</bib><lok>jaro 2002a,
        numero 6a, p. 12a</lok></fnt>;
      </ekz>
      <ekz>
        kun enkonduko de komputiloj en redakcioj oni <tld/>is drastan redukton de
        foruzado de papero
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <lok><url ref="http://steloj.de/esperanto/monato2/007176.html">http://steloj.de/esperanto/monato2/007176.html</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        ne<tld/>ebla <klr>(kp <ref cel="arbitr.0a">arbitra</ref>)</klr>
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006884.html">A&ubreve;strio: &Ccirc;u veneno en Vieno?</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="diven.0i">diveni</ref>,
       <ref cel="konjek.0i">konjekti</ref>,
       <ref cel="supoz.0i">supozi</ref>,
       <ref cel="suspek.0i.supozi">suspekti<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_n;&c_a;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>predikovat</trd>,
    <trd>progn&oacute;zovat</trd>,
    <trd>p&rcaron;edpov&iacute;dat</trd>,
    <trd>tipovat</trd>
  </trdgrp>
  <trd lng="de">vorhersagen</trd>
  <trd lng="en">predict</trd>
  <trd lng="es">pronosticar</trd>
  <trd lng="fa">&f_p;&f_y;&a_sh;&zwnj;&a_b;&f_y;&a_n;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>annoncer<klr> (pr&eacute;voir)</klr></trd>,
    <trd>pr&eacute;voir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&odblac;rejelez</trd>,
    <trd>prognosztiz&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voorspellen</trd>,
    <trd>een <ind>prognose</ind> stellen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>prognozowa&cacute;</trd>,
    <trd>opracowywa&cacute; <ind>prognoz&eogonek;</ind></trd>,
    <trd>przewidywa&cacute;</trd>,
    <trd>rokowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_n;&c_o;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>predpoveda&tcaron;</trd>,
    <trd>tipova&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="progno.veter0o">
  <kap>veter<tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Publika <ref tip="super" cel="anonc.0o">anonco</ref> pri meteologie <tld/>ita <ref cel="veter.0o">vetero</ref> en la sekvaj tagoj:
      <ekz>
        e&ccirc; en seriozaj radioprogramoj hodia&ubreve;
        informelsendoj da&ubreve;ras ofte nur tri minutojn, inkluzive de veter<tld/>o<fnt><aut>Stefan
        Maul</aut>: <vrk>Supra&jcirc;eco super &ccirc;io</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 5a, p. 5a</lok></fnt>;
      </ekz>
      <ekz>
        la veter<tld/>aj mapoj de famaj amaskomunikiloj
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="https://www.monato.be/2004/008096.html">Vilnius ne situas en Estonio!</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        rano estas simbolo de veter<tld/>o en la germana, tamen ne en la franca
        <fnt>
          <bib>Monato</bib>
          <aut>Marteno Ecott</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007201.html">Invadas la angla</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_n;&c_o;&c_z;
&c_n;&c_a;&c_d;&c_v;&c_o;&c_r;'&c_ja;</trd>
  <trd lng="cs">p&rcaron;edpov&ecaron;&dcaron; po&ccaron;as&iacute;</trd>
  <trd lng="de">Wettervorhersage</trd>
  <trd lng="en">weather forecast</trd>
  <trd lng="es">pron&oacute;stico meteorol&oacute;gico</trd>
  <trd lng="fa">&f_p;&f_y;&a_sh;&zwnj;&a_b;&f_y;&a_n;&f_y; &a_w;&a_D;&a_ayn; &a_h;&a_w;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;t&eacute;o <klr> (pr&eacute;visions)</klr></trd>,
    <trd>pr&eacute;vision m&eacute;t&eacute;orologiques</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>id&odblac;j&aacute;r&aacute;s-el&odblac;rejelz&eacute;s</trd>,
    <trd>id&odblac;j&aacute;r&aacute;s-jelent&eacute;s</trd>
  </trdgrp>
  <trd lng="nl">weersvoorspelling</trd>
  <trd lng="pl">prognoza pogody</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_n;&c_o;&c_z; &c_p;&c_o;&c_g;&c_o;&c_d;&c_y;</trd>
  <trd lng="sk">predpove&dcaron; po&ccaron;asia</trd>
  <trd lng="sv">v&auml;derprognos</trd>
</drv>
</art>
