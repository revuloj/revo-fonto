<vortaro>
<art mrk="$Id: progno.xml,v 1.32 2023/10/20 16:38:17 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>prognoz</rad>/o
</kap>
<drv mrk="progno.0o">
  <kap><tld/>o</kap>
  <snc mrk="progno.0o.konjekto">
    <dif>
      <ref tip="lst" lst="voko:komunikoj" cel="konjek.0o">Konjekto</ref>, bazita sur difinitaj
      signoj, pri okazonta&jcirc;o:
      <ekz>
        la&ubreve; <tld/>oj de la Monda Organiza&jcirc;o pri Sano la
        nombro de la kancermalsani&gcirc;oj kreskos &gcirc;is la jaro 2020<fnt><aut>Evgeni
        Georgiev</aut>: <vrk>Novaj kuracmetodoj kaj prevento</vrk>,
        <bib>Monato</bib><lok>jaro 2001a, numero 4a, p. 22a</lok></fnt>;
      </ekz>
      <ekz>
        tri jardekojn poste, evidenti&gcirc;as ke tiuj <tld/>oj estis tro pesimismaj
        <fnt>
          <aut>Roland Rotsaert</aut>
          <vrk><url ref="&Monato;/2000/007184.html">Nehaltigeblaj prezalti&gcirc;oj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 8</lok>
          </fnt>;
      </ekz>
      <ekz>
        favoraj konjunktur-<tld/>oj
        <fnt>
       <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2000/006835.php">Pekoj de patriarko</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 9</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_n;&c_o;&c_z;</trd>
  <trdgrp lng="cs">
    <trd>predikce</trd>,
    <trd>progn&oacute;za</trd>,
    <trd>p&rcaron;edpov&ecaron;&dcaron;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Prognose</trd>,
    <trd>Vorhersage</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>prognosis</trd>,
    <trd>prediction</trd>
  </trdgrp>
  <trd lng="es">pron&oacute;stico</trd>
  <trd lng="fa">&f_p;&f_y;&a_sh;&#x200C;&a_b;&f_y;&a_n;&f_y;</trd>
  <trdgrp lng="fr">
    <trd>pr&eacute;vision</trd>,
    <trd>pronostic</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&odblac;rejelz&eacute;s</trd>,
    <trd>progn&oacute;zis</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E88;&#x60F3;  <pr>&#x3088;&#x305D;&#x3046;</pr></trd>,
    <trd>&#x4E88;&#x6E2C;  <pr>&#x3088;&#x305D;&#x304F;</pr></trd>,
    <trd>&#x4E88;&#x5831;  <pr>&#x3088;&#x307B;&#x3046;</pr></trd>,
    <trd>&#x4E88;&#x5F8C;  <pr>&#x3088;&#x3054;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voorspelling</trd>,
    <trd>prognose</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>prognoza</trd>,
    <trd>przewidywanie</trd>,
    <trd>rokowanie</trd>,
    <trd>spodziewany przebieg</trd>
  </trdgrp>
  <trd lng="pt">pogn&oacute;stico</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_n;&c_o;&c_z;</trd>
  <trdgrp lng="sk">
    <trd>predpove&dcaron;</trd>,
    <trd>progn&oacute;za</trd>
  </trdgrp>
  <trd lng="sv">prognos</trd>
  <trd lng="uk">&#x43F;&c_r;&#x43E;&c_g;&#x43D;&#x43E;&c_z;</trd>
</drv>

<drv mrk="progno.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="progno.0i.antauxdiri">
    <dif>
      <ref tip="super" cel="dir.antaux0i">Anta&ubreve;diri</ref>
      surbaze de enketo, kalkuloj a&ubreve; takso de situacio:
      <ekz>
        dum en la lasta balotado la diversaj sonda&jcirc;oj
        &gcirc;uste <tld/>is la rezulton, &ccirc;i-foje ili maltrafis
        <fnt><aut>Karlo Juh&aacute;sz</aut>:
        <vrk>Apena&ubreve; venko</vrk>, 
        <bib>Monato</bib>
        <lok>2002/06, p. 12</lok></fnt>;
      </ekz>
      <ekz>
        kun enkonduko de komputiloj en redakcioj oni <tld/>is drastan redukton de
        foruzado de papero
        <fnt>
          <aut>Stefan Maul</aut>
            <vrk><url ref="&Monato;/2000/007176.php">Estimataj gelegantoj,</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 3</lok>
          </fnt>;
      </ekz>
      <ekz>
        ne<tld/>ebla <klr>(kp <ref cel="arbitr.0a">arbitra</ref>)</klr>
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/006884.php">&Ccirc;u veneno en Vieno?</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 6</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="diven.0i">diveni</ref>,
       <ref cel="konjek.0i">konjekti</ref>,
       <ref cel="supoz.0i">supozi</ref>,
       <ref cel="suspek.0i.supozi">suspekti<sncref/></ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_n;&c_a;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>predikovat</trd>,
    <trd>progn&oacute;zovat</trd>,
    <trd>p&rcaron;edpov&iacute;dat</trd>,
    <trd>tipovat</trd>
  </trdgrp>
  <trd lng="de">vorhersagen</trd>
  <trd lng="en">predict</trd>
  <trd lng="es">pronosticar</trd>
  <trd lng="fa">&f_p;&f_y;&a_sh;&#x200C;&a_b;&f_y;&a_n;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>annoncer<klr> (pr&eacute;voir)</klr></trd>,
    <trd>pr&eacute;voir</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>el&odblac;rejelez</trd>,
    <trd>prognosztiz&aacute;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x4E88;&#x60F3;&#x3059;&#x308B;  <pr>&#x3088;&#x305D;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x4E88;&#x6E2C;&#x3059;&#x308B;  <pr>&#x3088;&#x305D;&#x304F;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x4E88;&#x5831;&#x3059;&#x308B;  <pr>&#x3088;&#x307B;&#x3046;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x4E88;&#x5F8C;&#x3092;&#x5224;&#x5B9A;&#x3059;&#x308B;  <pr>&#x3088;&#x3054;&#x3092;&#x306F;&#x3093;&#x3066;&#x3044;&#x3059;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>voorspellen</trd>,
    <trd>een <ind>prognose</ind> stellen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>prognozowa&cacute;</trd>,
    <trd>opracowywa&cacute; <ind>prognoz&eogonek;</ind></trd>,
    <trd>przewidywa&cacute;</trd>,
    <trd>rokowa&cacute;</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_n;&c_o;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>predpoveda&tcaron;</trd>,
    <trd>tipova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&#x43E;&c_g;&#x43D;&#x43E;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&#x43E;&c_b;&c_i;&c_t;&c_i; &#x43F;&c_r;&#x43E;&c_g;&#x43D;&#x43E;&c_z;&c_i;</trd>,
    <trd>&c_s;&c_t;&c_a;&c_v;&c_i;&c_t;&c_i; &#x43F;&c_r;&#x43E;&c_g;&#x43D;&#x43E;&c_z;</trd>
  </trdgrp>
</drv>

<drv mrk="progno.veter0o">
  <kap>veter<tld/>o</kap>
  <uzo tip="fak">MET</uzo>
  <snc>
    <dif>
      Publika <ref tip="super" cel="anonc.0o">anonco</ref> pri meteologie <tld/>ita <ref cel="veter.0o">vetero</ref> en la sekvaj tagoj:
      <ekz>
        e&ccirc; en seriozaj radioprogramoj hodia&ubreve;
        informelsendoj da&ubreve;ras ofte nur tri minutojn, inkluzive de veter<tld/>o
        <fnt><aut>Stefan
        Maul</aut>: <vrk>Supra&jcirc;eco super &ccirc;io</vrk>,
        <bib>Monato</bib>
        <lok>2001/05, p. 5</lok></fnt>;
      </ekz>
      <ekz>
        la veter<tld/>aj mapoj de famaj amaskomunikiloj
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="&Monato;/2004/008096.html">Vilnius ne situas en Estonio!</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        rano estas simbolo de veter<tld/>o en la germana, tamen ne en la franca
        <fnt>
          
          <aut>Marteno Ecott</aut>
          <vrk><url ref="&Monato;/2001/007201.php">Invadas la
          angla</url></vrk>
          <bib>Monato</bib>
          <lok>2001/02, p. 15</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_a;&c_g;&c_n;&c_o;&c_z;
&c_n;&c_a;&c_d;&c_v;&c_o;&c_r;'&c_ja;</trd>
  <trd lng="cs">p&rcaron;edpov&ecaron;&dcaron; po&ccaron;as&iacute;</trd>
  <trd lng="de">Wettervorhersage</trd>
  <trd lng="en">weather forecast</trd>
  <trd lng="es">pron&oacute;stico meteorol&oacute;gico</trd>
  <trd lng="fa">&f_p;&f_y;&a_sh;&#x200C;&a_b;&f_y;&a_n;&f_y; &a_w;&a_D;&a_ayn; &a_h;&a_w;&a_A;</trd>
  <trdgrp lng="fr">
    <trd>m&eacute;t&eacute;o <klr> (pr&eacute;visions)</klr></trd>,
    <trd>pr&eacute;vision m&eacute;t&eacute;orologiques</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>id&odblac;j&aacute;r&aacute;s-el&odblac;rejelz&eacute;s</trd>,
    <trd>id&odblac;j&aacute;r&aacute;s-jelent&eacute;s</trd>
  </trdgrp>
  <trd lng="ja">&#x5929;&#x6C17;&#x4E88;&#x5831;  <pr>&#x3066;&#x3093;&#x304D;&#x3088;&#x307B;&#x3046;</pr></trd>
  <trd lng="nl">weersvoorspelling</trd>
  <trd lng="pl">prognoza pogody</trd>
  <trd lng="ru">&c_p;&c_r;&c_o;&c_g;&c_n;&c_o;&c_z; &c_p;&c_o;&c_g;&c_o;&c_d;&c_y;</trd>
  <trd lng="sk">predpove&dcaron; po&ccaron;asia</trd>
  <trd lng="sv">v&auml;derprognos</trd>
  <trd lng="uk">&#x43F;&c_r;&#x43E;&c_g;&#x43D;&#x43E;&c_z; &#x43F;&#x43E;&c_g;&#x43E;&c_d;&c_i;</trd>
</drv>
