<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>man&gcirc;</rad>/i
</kap>
<drv mrk="mangx.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ma&ccirc;i kaj gluti nutra&jcirc;on;  sin nutri:
      <ekz>
        resti du tagojn ne
        <tld/>inte;
      </ekz>
      <ekz>
        doni por <tld/>i al birdoj;
      </ekz>
      <ekz>
        &ccirc;iu <tld/>as, kiel li aran&gcirc;as<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Konsumi: (f)
      <ekz>
        rusto <tld/>as feron, &ccirc;agreno la koron<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Avide karesi:
      <ekz>
        <tld/>i iun per la rigardo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>i amatinon per kisoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">j&iacute;st</trd>
  <trd lng="en">eat</trd>
</drv>
<drv mrk="mangx.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago <tld/>i:
      <ekz>
        fru<tld/>o, maten<tld/>o, tag<tld/>o, vesper<tld/>o,
        inter<tld/>eto.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="mangx.0ajxo">
  <kap><tld/>(a&jcirc;)o</kap>
  <snc>
    <dif>
      Tio, kion oni <tld/>as:
      <ekz>
        vegeta&jcirc;aj <tld/>a&jcirc;oj estas la plej sanigaj.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="mangx.0egi">
  <kap><tld/>egi</kap>
  <snc>
    <dif>
      Avide kaj multe <tld/>i: (f)
      <ekz>
        &jcirc;aluzo <tld/>egis (dolorigis kaj konsumis) lin<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="mangx.0igi">
  <kap><tld/>igi</kap>
  <snc>
    <dif>
      Igi <tld/>anta:
      <ekz>
        <tld/>igi infanon.
      </ekz>
    </dif>
  </snc>
  <snc>
    <dif>
      Igi <tld/>ata:
      <ekz>
        <tld/>igi supon al infano.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="mangx.0ilaro">
  <kap><tld/>ilaro</kap>
  <snc>
    <dif>
      Tuto de la iloj kaj vazoj, kiujn oni uzas por <tld/>i.
    </dif>
  </snc>
</drv>
<drv mrk="mangx.for0i">
  <kap>for<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Tute konsumi <tld/>ante:
      <ekz>
        limakoj for<tld/>is la laktukojn;
      </ekz>
      <ekz>
        kiam kato jam for<tld/>is, forpelado ne helpos<fnt>Z</fnt>
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="mord.0i">mordi</ref>.
    </refgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Konsumi, detrui:
      <ekz>
        la bordon iom post iom for<tld/>as la maro;
      </ekz>
      <ekz>
        tio for<tld/>is multan tempon;
      </ekz>
      <ekz>
        li for<tld/>as (forglutas) duonon de la vortoj.
      </ekz>
    </dif>
  </snc>
</drv>
<drv mrk="mangx.0okarto">
  <kap><tld/>okarto</kap>
  <snc>
    <dif>
      Karto 3
    </dif>
    <refgrp tip="sin">
      <ref cel="menu.0o">menuo</ref>.
    </refgrp>
  </snc>
</drv>
<drv mrk="mangx.insekto0uloj">
  <kap>insekto<tld/>uloj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="mam.0uloj">mamuloj</ref>
      (<trd lng="la">Insectivora</trd>).
    </dif>
  </snc>
  <trd lng="de">Insektenfresser</trd>
</drv>
</art>

