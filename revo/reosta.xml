<art mrk="$Id$">
<kap>
  <rad>reostat</rad>/o
</kap>
<drv mrk="reosta.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ELE</uzo>
  <uzo tip="fak">TEK</uzo>
  <snc>
    <dif>
