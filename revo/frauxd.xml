<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fra&ubreve;d</rad>/i <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="frauxd.0i">
  <kap><tld/>i</kap>
  <snc>
    <uzo tip="fak">JUR</uzo>
    <dif>
      Friponi, trompante pri kvantoj, datoj, kontoj en negoco a&ubreve; alia
      afero.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>frauder</trd>,
    <trd>tricher</trd>
  </trdgrp>
</drv>

<drv mrk="frauxd.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Trompado pri kvantoj por &scirc;telakiri.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>fraude</trd>,
    <trd>tricherie</trd>
  </trdgrp>
</drv>

<drv mrk="frauxd.de0i">
  <kap>de<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="sxtel.0i">&Scirc;teli</ref>, pri&scirc;teli per <tld/>o,
      per trompo en kalkuloj:
      <ekz>
        mi donos al &ccirc;iu ties respektivan parton <klr>...</klr>, sen
        de<tld/>i vin e&ccirc; per unu gro&scirc;o
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 39a, p.
        294a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>escroquer</trd>,
    <trd>l&eacute;ser<klr> (faire du tort)</klr></trd>
  </trdgrp>
</drv>
</art>

<!--
$Log$
