<drv mrk="vigl.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vigl.0igxi.KOMUNE">
    <dif>
      Ekagi iom vive, verve, rapide:
      <ekz>
        la popolo vigli&gcirc;is de la vortoj de <klr>[la]</klr> re&gcirc;o
        <fnt>
          <bib>MT</bib>
          <lok>II. Kroniko 32:8</lok>
        </fnt>;
      </ekz>
      <ekz>
        nun la bovinoj <tld/>i&gcirc;is, mi a&ubreve;dis ilin rapide trotantajn
        malanta&ubreve; mi
        <fnt><bib>Kiso</bib>, <lok>la bovinoj de la
        paro&hcirc;estro</lok></fnt>;
      </ekz>
      <ekz>
        lastatempe intereso pri la lingvo sufi&ccirc;e <tld/>i&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan MacAoidh</aut>
          <vrk><url ref="&Monato;/2007/009227.php">Lingva diverseco</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia konscienco <klr>[&#x2026;]</klr> nun subite <tld/>i&gcirc;is
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>La Sen&ccirc;esa Rakonto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la komence trankvila protesto pli<tld/>i&gcirc;is, kiam grupo de junaj artistoj
        prezentis sian proteston
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2009/009826.php">Artistoj krucumitaj</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="agit.0igxi">agiti&gcirc;i</ref>,
      <ref cel="vek.0igxi.plivigligxi">veki&gcirc;i<sncref/></ref>
    </refgrp>
    <trdgrp lng="de">
      <trd><ind>munter</ind> werden</trd>,
      <trd>erwachen</trd>,
      <trd>aufwachen</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_zh;&c_y;&c_w;&c_l;&c_ja;&c_c;&c_c;&c_a;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_b;&c_a;&c_d;&c_z;&c_jo;&c_r;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="fr">
