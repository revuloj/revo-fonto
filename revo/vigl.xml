<drv mrk="vigl.mal0igi">
  <kap>mal<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="vigl.mal0igi.KOMUNE">
    <dif>
      Senigi je emo al agado, malgajigi, malfortigi:
      <ekz>
        <klr>[la hundojn]</klr> mal<tld/>igis la varmego kaj ili ku&scirc;is eliginte la
        langon, brue spiris kaj neniel atentis la verddorsajn lacertojn
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la recesio mal<tld/>igis alti&gcirc;on de la prezoj, kaj dum 1999 nur trifoje oni
        registris inflacion
        <fnt>
       <aut>LAST</aut>
          <vrk><url ref="&Monato;/2000/006854.php">Eta recesio</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 7</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="brems.0i">bremsi</ref>,
       <ref cel="brid.0i.FIG">bridi</ref>
    </refgrp>
  </snc>
  <trd lng="cs">otupit pozornost</trd>
  <trdgrp lng="de">
    <trd>erm&uuml;den</trd>,
    <trd>einschl&auml;fern</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>abattre<klr tip="amb"> (rendre abattu)</klr></trd>,
    <trd>refroidir<klr tip="amb"> (l'ambiance etc.)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>ellanyh&iacute;t</trd>,
    <trd>tesped&eacute;sre k&eacute;sztet</trd>,
    <trd>elkedvetlen&iacute;t</trd>
  </trdgrp>
  <trd lng="ja">&#x5143;&#x6C17;&#x3092;&#x5931;&#x308F;&#x305B;&#x308B;  <pr>&#x3052;&#x3093;&#x304D;&#x3092;&#x3046;&#x3057;&#x306A;&#x308F;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_m;&c_i;&c_t;&c_mol;</trd>,
