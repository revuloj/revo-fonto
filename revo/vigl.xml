<drv mrk="vigl.mal0igxi">
  <kap>mal<tld/>i&gcirc;i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="vigl.mal0igxi.KOMUNE">
    <dif>
      I&gcirc;i dormema, malatenta, ne agema:
      <ekz>
        la malfeli&ccirc;ulino pli kaj pli malforti&gcirc;is, mal<tld/>i&gcirc;ante, malgrasi&gcirc;ante, iom
        post iom sin klinante al la tombo
        <fnt>
          <bib>ChL</bib>
        </fnt>;
      </ekz>
      <ekz>
        kaptante la paseman ombron tie enuis la &ccirc;evalgardantoj kun la mal<tld/>i&gcirc;intaj
        &ccirc;evaloj
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        jen mi a&ubreve;dis: <klr>[&#x2026;multajn tedajn vortojn]</klr>, feli&ccirc;e &scirc;i iomete
        mal<tld/>i&gcirc;as <klr>[nun]</klr>
        <fnt>
          <aut>Michel Marko</aut>
            <vrk><url ref="&Monato;/2000/007051.php">La stranga grupo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/10, p. 29</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  
  <trdgrp lng="fr">
    <trd>faiblir<klr tip="amb"> (activit&eacute; etc.)</klr></trd>,
    <trd>ralentir<klr tip="amb"> (activit&eacute; etc.)</klr></trd>
