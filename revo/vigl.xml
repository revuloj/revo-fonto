    <trd>felpezsd&uuml;l</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x6D3B;&#x767A;&#x306B;&#x306A;&#x308B;  <pr>&#x304B;&#x3063;&#x3071;&#x3064;&#x306B;&#x306A;&#x308B;</pr></trd>,
    <trd>&#x5143;&#x6C17;&#x3065;&#x304F;  <pr>&#x3052;&#x3093;&#x304D;&#x3065;&#x304F;</pr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_zh;&c_i;&c_v;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_b;&c_o;&c_d;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_p;&c_r;&c_i;&c_o;&c_b;&c_o;&c_d;&c_r;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>
  </trdgrp>
  <trdgrp lng="uk">
