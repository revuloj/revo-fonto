<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>hura</rad>
</kap>
<drv mrk="hura.0">
  <kap><tld/></kap>
  <snc>
    <dif>
      Interjekcio de &gcirc;oja a&ubreve; aproba aklamo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>hurra</trd>,
    <trd>juhu</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hurrah</trd>,
    <trd>hooray</trd>
  </trdgrp>
  <trd lng="fr">hourra<klr tip="ind"> (interj.)</klr></trd>
  <trd lng="hu">hurr&aacute;</trd>
  <trd lng="nl">hoera</trd>
  <trd lng="ru">&c_u;&c_r;&c_a;</trd>
</drv>

<drv mrk="hura.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="H"/>krio.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Hurraruf</trd>,
    <trd>Hurra</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hurrah</trd>,
    <trd>hooray</trd>
  </trdgrp>
  <trd lng="fr">hourra<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">hurr&aacute;z&aacute;s</trd>
  <trd lng="nl">hoerageroep</trd>
</drv>

<drv mrk="hura.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc>
    <dif>
      <tld lit="H"/>krii.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Hurra rufen</trd>,
    <trd>bejubeln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>exult</trd>,
    <trd>rejoice</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>crier des <ind>hourras</ind></trd>,
    <trd>acclamer</trd></trdgrp>
  <trd lng="hu">hurr&aacute;z&aacute;zik</trd>
  <trd lng="nl">hoera roepen</trd>
</drv>
</art>
