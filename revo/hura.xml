<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>hura</rad></kap>
<drv mrk="hura.0">
  <kap><tld/></kap>
  <snc>
    <dif>
      Interj. de &gcirc;oja a&ubreve; aproba aklamo. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>hurra</trd>,
     <trd>juhu</trd></trdgrp>
  <trd lng="ru">&c_u;&c_r;&c_a;</trd>
</drv>
<drv mrk="hura.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      <tld lit="H"/>krio. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Hurraruf</trd>,
     <trd>Hurra</trd></trdgrp>
</drv>
<drv mrk="hura.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
    <snc>
      <dif>
        <tld lit="H"/>krii.
      </dif>
    </snc>
  <trdgrp lng="de">
     <trd>Hurra rufen</trd>,
     <trd>bejubeln</trd></trdgrp>
</drv>
</art>
