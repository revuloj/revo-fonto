<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hura.xml,v 1.25 2023/08/31 15:46:19 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>hura</rad> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="hura.0">
  <kap><tld/></kap>
  <snc mrk="hura.0.SON">
    <gra><vspec>ekkrio</vspec></gra>
    <dif>
      Bonege, gratulinde, pri&gcirc;ojinde!:
      <ekz>
        la re&gcirc;idino preterrajdis, kaj &scirc;i efektive estis tiel bela, ke &ccirc;iuj forgesis,
        kiel kruela &scirc;i estas, kaj tial ili kriis <tld/>
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        kun kantado kaj <tld/>krioj oni trasegadis la glacion, la &scirc;ipoj estis gudrataj
        kaj &scirc;nuregataj, kaj veturis al la fremdaj landoj
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>! subite la maljuna gnomo donis al la nejuna elfknabino la&ubreve;tan
        kison
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>! <tld/>! oni kriis en la domoj, en kiuj la homoj festis la Silvestran
        vesperon
        <fnt>
          <bib>Fab4</bib>
          <lok>Dekdu per la po&scirc;to</lok>
        </fnt>;
      </ekz>
      <ekz>
        bruege eksonis la muziko, kaj &ccirc;iuj homoj kriis <tld/>
        <fnt>
          <bib>Fab3</bib>
          <lok>Kolo de botelo</lok>
        </fnt>!
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>hurra!</trd>,
    <trd>visca!</trd>
  </trdgrp>
   <trdgrp lng="de">
    <trd>hurra</trd>,
    <trd>juhu</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hurrah</trd>,
    <trd>hooray</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>&#xA1;hurra!<klr tip="ind"> (interj.)</klr></trd>,
    <trd>&#xA1;viva!</trd>,
    <trd>&#xA1;bravo!</trd>
  </trdgrp>
  <trd lng="fr">hourra<klr tip="ind"> (interj.)</klr></trd>
  <trd lng="hu">hurr&aacute;</trd>
  <trd lng="it">urr&aacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x4E07;&#x6B73;&#xFF01;  [<ind>&#x3070;&#x3093;&#x3056;&#x3044;&#xFF01;</ind>]</trd>,
    <trd>&#x30D5;&#x30E9;&#x30FC;&#xFF01;</trd>
  </trdgrp>
  <trd lng="nl">hoera</trd>
  <trd lng="pl">hura</trd>
  <trd lng="pt">viva<klr tip="ind"> (interj.)</klr></trd>
  <trd lng="ru">&c_u;&c_r;&c_a;</trd>
</drv>

<drv mrk="hura.0o">
  <kap><tld/>o</kap>
  <snc mrk="hura.0o.SON">
    <dif>
      <tld lit="H"/>-krio.
    </dif>
  </snc>
  <trd lng="be">&c_u;&c_r;&c_a;</trd>
  <trdgrp lng="ca">
    <trd>v&iacute;ctor</trd>,
    <trd>visca</trd>,
    <trd>ovaci&oacute;</trd>
  </trdgrp>
  <trd lng="cs">vol&aacute;n&iacute; hur&aacute;</trd>
  <trdgrp lng="de">
    <trd>Hurraruf</trd>,
    <trd>Hurra</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>hurrah</trd>,
    <trd>hooray</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>hurra<klr tip="ind"> (subst.)</klr></trd>,
    <trd>ovaci&oacute;n</trd>
  </trdgrp>
  <trd lng="fr">hourra<klr tip="ind"> (subst.)</klr></trd>
  <trd lng="hu">hurr&aacute;z&aacute;s</trd>
  <trd lng="it">urr&aacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x6B53;&#x547C;&#x306E;&#x58F0;  [<ind>&#x304B;&#x3093;&#x3053;&#x306E;&#x3053;&#x3048;</ind>]</trd>,
    <trd>&#x4E07;&#x6B73;  [<ind>&#x3070;&#x3093;&#x3056;&#x3044;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">hoerageroep</trd>
  <trd lng="pt">grito de <ind>viva</ind></trd>
  <trd lng="sk">volanie hur&aacute;</trd>
</drv>

<drv mrk="hura.0i">
  <kap><tld/>i</kap>
  <gra><vspec>x</vspec></gra>
  <snc mrk="hura.0i.SON">
    <dif>
      <tld lit="H"/>-krii:
      <ekz>
        dum preterpasos la flamo, <tld/>os, jubilos, flagojn flirtigos geknaboj el lokaj
        elementaj lernejoj
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2012/010675.php">La&ubreve;du lernejanojn!</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; <tld/>i kaj jubili el la sekureco de niaj fejsbuk-pa&gcirc;oj, ni e&ubreve;ropanoj
        kunlaboru
        <fnt>
          <bib>Monato</bib>
          <aut>Paul Gubbins</aut>
          <vrk><url ref="&Monato;/2015/011544.php">Popolo 1, E&ubreve;ropa Unio 0</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_ch;&c_a;&c_c;&c_mol; <ind>&c_u;&c_r;&c_a;</ind></trd>
  <trdgrp lng="ca">
    <trd>victorejar</trd>,
    <trd>aclamar</trd>
  </trdgrp>
  <trd lng="cs">volat hur&aacute;</trd>
  <trdgrp lng="de">
    <trd>Hurra rufen</trd>,
    <trd>bejubeln</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>exult</trd>,
    <trd>rejoice</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>crier des <ind>hourras</ind></trd>,
    <trd>acclamer</trd></trdgrp>
  <trd lng="hu">hurr&aacute;z&aacute;zik</trd>
  <trd lng="it">esultare<klr> (gridare "urr&aacute;")</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x6B53;&#x547C;&#x3059;&#x308B;  [<ind>&#x304B;&#x3093;&#x3053;&#x3059;&#x308B;</ind>]</trd>,
    <trd>&#x4E07;&#x6B73;&#x3092;&#x53EB;&#x3076;  [<ind>&#x3070;&#x3093;&#x3056;&#x3044;&#x3092;&#x3055;&#x3051;&#x3076;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">hoera roepen</trd>
  <trdgrp lng="pl">
    <trd>krzycze&cacute; <ind>hura</ind></trd>,
    <trd>wiwatowa&cacute;</trd>
  </trdgrp>
  <trd lng="pt">gritar <ind>viva</ind></trd>
  <trd lng="sk">vola&tcaron; hur&aacute;!</trd>
  <trd lng="uk">&#x43A;&#x440;&#x438;&#x447;&#x430;&#x442;&#x438; &#x443;&#x440;&#x430;!</trd>
</drv>
</art>
