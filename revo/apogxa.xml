<vortaro>
<art mrk="$Id$">
<kap>
  <rad>apo&gcirc;atur</rad>/o
</kap>
<drv mrk="apogxa.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.melodi0o">Melodiornamo</ref>
      uzata en klasika e&ubreve;ropa muziko,
      kiu anta&ubreve;metas anta&ubreve; la tono
      unu a&ubreve; du komencajn tonojn,
      konsumante parton de &gcirc;ia da&ubreve;ro.
    </dif>
    <refgrp tip="vid">
      <ref cel="acxakatur.0o">a&ccirc;akaturo</ref>
    </refgrp>
  </snc>
  <trd lng="en">appoggiature</trd>
  <trd lng="fr">appoggiature</trd>
</drv>
</art>
