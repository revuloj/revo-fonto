<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>apo&gcirc;atur</rad>/o
</kap>
<drv mrk="apogxa.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.melodi0o">Melodiornamo</ref>
      uzata en klasika e&ubreve;ropa muziko,
      kiu anta&ubreve;metas anta&ubreve; la tono
      unu a&ubreve; du komencajn tonojn,
      konsumante parton de &gcirc;ia da&ubreve;ro.
    </dif>
    <refgrp tip="vid">
      <ref cel="acxakatur.0o">a&ccirc;akaturo</ref>
    </refgrp>
  </snc>
  <trd lng="ca">appoggiatura</trd>
  <trdgrp lng="en">
    <trd>appoggiatura</trd>,
    <trd>long <ind>grace note</ind></trd>,
  </trdgrp>
  <trd lng="es">apoyatura</trd>
  <trd lng="fr">appoggiature</trd>
  <trd lng="sk">predr&aacute;&zcaron;ka</trd>
</drv>
</art>
