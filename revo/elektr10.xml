<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: elektr10.xml,v 1.19 2022/03/17 13:47:18 revo Exp $">
<kap>
    <rad>elektroliz</rad>/i
    <fnt>Z</fnt>
</kap>
<drv mrk="elektr10.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      Apliki <ref tip="vid" cel="elektr10.0o">elektrolizon</ref>:
      <ekz>
        katodo estas negativa elektrodo en pilo, <tld/>a aparato, de kiu ekas la elektra
        fluo al anodo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=37946">Katodo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="galvan1.0i">galvanizi</ref>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="ca">electrolitzar</trd>
  <trd lng="de">elektrolysieren</trd>
  <trd lng="fa">&a_A;&a_l;&f_k;&a_t;&a_r;&a_w;&a_l;&f_y;&a_z; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">&eacute;lectrolyser</trd>
  <trd lng="en">electrolyze</trd>
  <trd lng="es">electrolizar</trd>
  <trdgrp lng="hu">
    <trd>elektroliz&aacute;l</trd>,
    <trd>elektrolitikusan elbont</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>poddawa&cacute; <ind>elektrolizie</ind></trd>,
    <trd>rozk&lstroke;ada&cacute; <ind>elektrolitycznie</ind></trd>
  </trdgrp>
  <trd lng="pt">eletrolisar</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_a;&c_z;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;
    <klr>(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_m;
    &c_t;&c_o;&c_k;&c_o;&c_m;)</klr></trd>,
    <trd>&c_p;&c_o;&c_d;&c_v;&c_je;&c_r;&c_g;&c_a;&c_t;&c_mol;
    <ind>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_l;&c_i;&c_z;&c_u;</ind></trd>
  </trdgrp>
  <trd lng="uk">&#x435;&#x43B;&#x435;&#x43A;&#x442;&#x440;&#x43E;&#x43B;&#x456;&#x437;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="elektr10.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ELE</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      <ref tip="vid" cel="procez.0o">Procezo</ref>, &ccirc;e kiu
      elektro ka&ubreve;zas redukti&gcirc;-oksid-reakcion, aplikata ekz.
      por izoli hidrogenon, aluminion, kloron el kombina&jcirc;oj:
      <ekz>
        produktado de natria hidroksido eblas per <tld/>o de natria klorido
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=440825">Natria hidroksido</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="farad.0o">Farado</ref>,
       <ref cel="elektr9.0o">elektrolito</ref>,
       <ref cel="elektr3.0o">elektrodo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_l;&c_ib;&c_z;</trd>
  <trd lng="bg">&c_je;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_l;&c_i;&c_z;&c_a;</trd>
  <trd lng="ca">electr&ograve;lisi</trd>
  <trdgrp lng="cs">
    <trd>elektrol&yacute;za</trd>,
    <trd>rozklad</trd>
  </trdgrp>
  <trd lng="de">Elektrolyse</trd>
  <trdgrp lng="es">
        <trd>electrolisis</trd>,
        <trd>electr&oacute;lisis</trd>
    </trdgrp>
  <trdgrp lng="fa">
      <trd>&a_A;&a_l;&f_k;&a_t;&a_r;&a_w;&a_l;&f_y;&a_z;</trd>,
      <trd>&a_b;&a_r;&a_q;&f_k;&a_A;&a_f;&a_t;</trd>
    </trdgrp>
  <trd lng="fr">&eacute;lectrolyse</trd>
  <trd lng="hu">elektrol&iacute;zis</trd>
  <trd lng="pl">elektroliza</trd>
  <trd lng="pt">eletr&oacute;lise</trd>
  <trd lng="sk">elektrol&yacute;za</trd>
  <trd lng="uk">&#x435;&#x43B;&#x435;&#x43A;&#x442;&#x440;&#x43E;&#x43B;&#x456;&#x437;</trd>
</drv>

</art>
