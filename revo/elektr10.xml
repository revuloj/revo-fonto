<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>elektroliz</rad>/i
<fnt>Z</fnt>
</kap>
<drv mrk="elektr10.0i">
<kap><tld/>i</kap>
<uzo tip="fak">ELE</uzo>
<uzo tip="fak">KEM</uzo>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Malkomponi substancon per kurento en &gcirc;iajn konsistigajn
elementojn a&ubreve; almena&ubreve; en du a&ubreve; plurajn elemento-grupojn.  
</dif>
</snc>
<adm>
Difino modernigita, sed plu dubinda. <aut>MB</aut>
</adm>
<trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_l;&c_ib;&c_z;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
<trd lng="ca">electrolitzar</trd>
<trd lng="de">elektrolysieren</trd>
<trd lng="fr">&eacute;lectrolyser</trd>
<trd lng="en">electrolyze</trd>
<trd lng="es">electrolizar</trd>
<trdgrp lng="hu">
  <trd>elektroliz&aacute;l</trd>,
  <trd>elektrolitikusan elbont</trd>
</trdgrp>
<trdgrp lng="ru">
  <trd>&c_r;&c_a;&c_z;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol; <klr>(&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_m; &c_t;&c_o;&c_k;&c_o;&c_m;)</klr></trd>,
  <trd>&c_p;&c_o;&c_d;&c_v;&c_je;&c_r;&c_g;&c_a;&c_t;&c_mol; <ind>&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_l;&c_i;&c_z;&c_u;</ind></trd>
</trdgrp>
</drv>

<drv mrk="elektr10.0o">
 <kap><tld/>o</kap>
 <uzo tip="fak">ELE</uzo>
 <uzo tip="fak">KEM</uzo>
 <snc>
  <dif>
   Rezulto, efiko de elektrolizado.
  </dif>
 </snc>
 <trd lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_l;&c_ib;&c_z;</trd>
 <trd lng="ca">electr&ograve;lisi</trd>
 <trd lng="de">Elektrolyse</trd>
  <trdgrp lng="es">
    <trd>electrolisis</trd>,
    <trd>electr&oacute;lisis</trd>
  </trdgrp>
 <trd lng="hu">elektrol&iacute;zis</trd>
</drv>

</art>
