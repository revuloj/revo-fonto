<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
   <kap>
    <rad>alpar</rad>/o
   </kap>
   <drv mrk="alpar.0o">
    <kap><tld/>o</kap>
    <uzo tip="fak">EKON</uzo>
    <snc>
       <dif>
          Egaleco de valoro, ofte: kurza alparo, t.e. kiam akcia a&ubreve;
obligacia kurzo atingas sian nominalan valoron.
       </dif>
        <trdgrp lng="de">
          <trd>Pariwert</trd>,
          <trd>Parit&auml;t</trd>
       </trdgrp>
       <trd lng="es">paridad</trd>
    </snc>
    <snc>
       <dif>
          Nominala valoro de akcio a&ubreve; obligacio.
       </dif>
       <trd lng="de">Nominalwert</trd>
       <trd lng="es">valor nominal</trd>
    </snc>
  </drv>

<drv mrk="alpar.0e">
  <kap><tld/>e</kap>
  <snc>
       <dif>
          Samvalore.
       </dif>
       <trd lng="de">pari</trd>
  </snc>
  <snc>
       <dif>
          Nominalvalore.
       </dif>
       <trd lng="de">zum <ind>Nominalwert</ind></trd>
  </snc>
</drv>

</art>

