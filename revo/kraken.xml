<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kraken</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kraken.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="kraken.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Malgranda seka nesukerita <ref tip="super" 
      cel="kuk.0o.KUI">kuko</ref>, kiu krakas sub la dentoj:
      <ekz>
        poste venis teo kun kremo, kun butero kaj <tld/>oj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p.
        123a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_r;&c_e;&c_k;&c_je;&c_r;</trd>,
    <trd>&c_s;&c_u;&c_h;&c_o;&c_je; &c_p;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Hartgeb&auml;ck</trd>,
    <trd>Cracker</trd>,
    <trd>Kracker</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>cracker</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>croquet</trd>
    <trd>g&acirc;teau sec (sal&eacute;)</trd>
  </trdgrp>
  <trd lng="hu">sz&aacute;razs&uuml;tem&eacute;ny</trd>
  <trd lng="nl">cracker</trd>
  <trd lng="pt">cream-cracker</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_h;&c_o;&c_je; &c_p;&c_je;&c_ch;&c_je;&c_n;&c_mol;&c_je;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_k;&c_je;&c_r;</trd>
  </trdgrp>
</drv>
</art>
