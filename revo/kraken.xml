<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kraken.xml,v 1.29 2024/04/20 05:24:43 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kraken</rad>/o <fnt><bib>JED</bib></fnt>
</kap>
<drv mrk="kraken.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc mrk="kraken.0o.nodoforma">
    <dif>
      <ref tip="lst" lst="voko:man&gcirc;a&jcirc;oj" cel="bak.0ajxo.KUI">Baka&jcirc;o</ref>
      en formo de ok a&ubreve; de noditaj brakoj,
      <ref tip="super" cel="kring.0o">kringo</ref>,
      ofte sukerita (a&ubreve; salita):
      <ekz>
        <tld/>o estas kun sukero, kringo estas sen sukero
        <fnt>
          <bib>LR</bib>
          <lok>2. Vortuzo</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni a&ccirc;etis &ccirc;e la bulkisto spesdekan <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
        poste venis teo kun kremo, kun butero kaj <tld/>oj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 20a, p.
        123a</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="brec.0o">breco</ref>
    <bld lok="&WCU;/thumb/8/8e/Craquelin_boulonnais.jpg/360px-Craquelin_boulonnais.jpg" prm="CC
    BY-SA 4.0">Bulonja <tld/>o
        <fnt>
          <aut>FR, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Craquelin_boulonnais.jpg">&WCW;/File:Craquelin_boulonnais.jpg</url></lok>
        </fnt>
    </bld>
    <bld lok="&WCU;/thumb/2/25/Kringle_2.JPG/320px-Kringle_2.JPG" prm="CC BY-SA 4.0"><tld/>o el dana
    bakejo
        <fnt>
          <aut>RhinoMind</aut>
          <lok><url ref="&WCW;/Category:Kringle">&WCW;/Category:Kringle</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_k;&c_r;&c_e;&c_n;&c_mol;&c_d;&c_z;&c_je;&c_l;&c_mol;</trd>
    <trd lng="de"><ofc>*</ofc>Bretzel</trd>
    <trd lng="en"><ofc>*</ofc>cracknel</trd>
    <trd lng="fr"><ofc>*</ofc>craquelin</trd>
    <trd lng="pl"><ofc>*</ofc>ciastko</trd>
    <trd lng="ru"><ofc>*</ofc>&c_k;&c_r;&c_je;&c_n;&c_d;&c_je;&c_l;&c_mol;</trd>
  </snc>
  <snc mrk="kraken.0o.kraka">
    <fnt><bib>PV</bib></fnt>
    <dif>
      Malgranda seka nesukerita <ref tip="super" cel="kuk.0o.KUI">kuko</ref>, kiu krakas sub la
      dentoj:
      <ekz>
        la bretona <tld/>o estas bretona biskvito kies paston oni unue brogis
        <klr>[&#8230;]</klr>, &gcirc;i similas proksimuman diskon kun suprenlevitaj bordoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=462475">Bretona Biskvito</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="super" cel="biskvi.0o">biskvito</ref>
    <trdgrp lng="be">
      <trd>&c_k;&c_r;&c_e;&c_k;&c_je;&c_r;</trd>,
      <trd>&c_s;&c_u;&c_h;&c_o;&c_je; &c_p;&c_ja;&c_ch;&c_e;&c_n;&c_mol;&c_n;&c_je;</trd>
    </trdgrp>
    <trd lng="bg">&c_s;&c_u;&c_h;&c_a; &c_t;&c_v;&c_malmol;&c_r;&c_d;&c_a; <ind>&c_b;&c_i;&c_s;&c_k;&c_v;&c_i;&c_t;&c_a;</ind></trd>
    <trdgrp lng="cs">
      <trd>obvykle do tvaru osmy</trd>,
      <trd>such&yacute; slan&yacute; prcl&iacute;k</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Knabbergeb&auml;ck</trd>,
      <trd>Hartgeb&auml;ck</trd>,
      <trd>Cracker</trd>,
      <trd>Kracker</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>cracker</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>croquet</trd>
      <trd>g&acirc;teau sec (sal&eacute;)</trd>
    </trdgrp>
    <trd lng="hu">sz&aacute;razs&uuml;tem&eacute;ny</trd>
    <trd lng="nl">cracker</trd>
    <trdgrp lng="pl">
      <trd>ciastko <klr>(suche)</klr></trd>,
      <trd>suchar</trd>,
      <trd>herbatnik</trd>,
      <trd>krakers</trd>
    </trdgrp>
    <trd lng="pt">cream-cracker</trd>
    <trd lng="ro">biscuit</trd>
    <trdgrp lng="ru">
      <trd>&c_g;&c_a;&c_l;&c_je;&c_t;&c_a;</trd>,
      <trd>&c_s;&c_u;&c_h;&c_o;&c_je; &c_p;&c_je;&c_ch;&c_je;&c_n;&c_mol;&c_je;</trd>,
      <trd>&c_k;&c_r;&c_je;&c_k;&c_je;&c_r;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>pracl&iacute;k</trd>,
      <trd>such&aacute;r</trd>,
      <trd>such&eacute; sladk&eacute; pe&ccaron;ivo</trd>
    </trdgrp>
  </snc>
  <trd lng="ja">&#x30AF;&#x30E9;&#x30C3;&#x30AB;&#x30FC;</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_u;&c_h;&c_a;&c_r;&c_i;&#x43A;</trd>,
    <trd>&c_s;&c_u;&c_h;&c_je; &#x43F;&c_je;&c_ch;&c_i;&c_v;&#x43E;</trd>,
    <trd>&#x43A;&c_r;&c_je;&#x43A;&c_je;&c_r;</trd>,
    <trd>&c_h;&c_r;&c_u;&c_s;&c_t;&#x43A;&c_i;&c_j; &c_h;&#x43B;&c_ib;&c_b;&c_je;&c_c;&#x44C;</trd>
  </trdgrp>
</drv>
</art>
