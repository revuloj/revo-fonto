<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kraken</rad>/o
</kap>
<drv mrk="kraken.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Malgranda seka nesukerita kuko, kiu krakas sub la dento.
    </dif>
    <refgrp tip="sin">
      <ref cel="kring.0o">kringo</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
     <trd>Hartgeb&auml;ck</trd>,
     <trd>Cracker</trd>,
     <trd>Kracker</trd>
  </trdgrp>
  <trd lng="hu">perec</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_a;&c_l;&c_je;&c_t;&c_a;</trd>,
    <trd>&c_s;&c_u;&c_h;&c_o;&c_je; &c_p;&c_je;&c_ch;&c_je;&c_n;&c_mol;&c_je;</trd>,
    <trd>&c_k;&c_r;&c_je;&c_k;&c_je;&c_r;</trd>
  </trdgrp>
</drv>
</art>
