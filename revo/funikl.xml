<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>funikl</rad>/o
</kap>
<drv mrk="funikl.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="funikl.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Membrana tubo kun arterio kaj du vejnoj, kiuj kunligas la feton al la
placento.
    </dif>
    <refgrp tip="sin">
      <ref cel="umbili.0asxnuro">umbilika &scirc;nuro</ref>
    </refgrp>
    <trd lng="de">Nabelschnur</trd>
    <trd lng="fr">cordon ombilical</trd>
    <trd lng="nl">navelstreng</trd>
    <trd lng="pl">sznur p&eogonek;pkowy</trd>
    <trd lng="ru">&c_p;&c_u;&c_p;&c_o;&c_v;&c_i;&c_n;&c_a;</trd>
  </snc>
  <snc mrk="funikl.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Organo, kiu kunligas la ovolojn al la placento.
    </dif>
    <trd lng="de">Funiculus</trd>
    <trd lng="fr">funicule<klr tip="ind"> (bot.)</klr></trd>
    <trd lng="nl">funiculus</trd>
    <trd lng="pl">p&eogonek;powina</trd>
    <trd lng="ru">&c_s;&c_je;&c_m;&c_ja;&c_n;&c_o;&c_zh;&c_k;&c_a;</trd>
  </snc>
    <trd lng="hu">k&ouml;ld&ouml;kzsin&oacute;r<klr tip="amb"> (magban
is)</klr></trd>
</drv>
</art>
