<art mrk="$Id: rome.xml,v 1.9 2022/03/26 19:19:20 revo Exp $">
<kap>
  <rad>Rome</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="rome.0o">
  <kap><tld/>o</kap>
  <snc mrk="rome.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> italdevena:
      <ekz>
        tio ne estas la rozoj de la &ccirc;erko de <tld/>o kaj Julia
        <fnt><bib>Fab3</bib>, <lok>plej bela rozo de la
        mondo</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_R;&c_a;&c_m;&c_e;&c_a;</trd>
  <trd lng="bg">&c_R;&c_o;&c_m;&c_je;&c_o;</trd>
  <trd lng="de">Romeo</trd>
  <trd lng="fr">Rom&eacute;o</trd>
  <trd lng="hu">R&oacute;me&oacute;</trd>
  <trd lng="it">Romeo</trd>
  <trd lng="pt">Romeu</trd>
  <trd lng="ru">&c_R;&c_o;&c_m;&c_je;&c_o;</trd>
  <trd lng="uk">&#x420;&#x43E;&#x43C;&#x435;&#x43E;</trd>
</drv>
</art>

