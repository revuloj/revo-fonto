<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: herkul.xml,v 1.15 2021/01/31 20:42:51 revo Exp $">
<kap>
  <rad>Herkul</rad>/o <fnt><bib>PrV</bib></fnt>,
  <var><kap><rad var="es">Herkules</rad>/o <fnt><bib>Rabistoj</bib></fnt></kap></var>
</kap>
<drv mrk="herkul.0o">
  <kap><tld/>o, <var><kap><tld var="es"/>o</kap></var></kap>
  <snc mrk="herkul.0o.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Latinigita nomo de la helena <ref tip="super" cel="hero.0o.MIT">heroo</ref>
      <ref tip="vid" cel="herakl1.0o">Heraklo</ref>,
      <trd lng="lat">Herkules</trd>,
      eble devenanta de la etruska formo &leftquot;<nom>Herkles</nom>&rightquot;,
      pri kiu ekzistas romia-itala mito, la&ubreve; kiu li estis en Italujo:
      <ekz>
        foriru! kiel iam <tld var="es"/>o, mi devas preni morton izolite
        <fnt>
          <bib>Ifigenio</bib>
        </fnt>;
      </ekz>
      <ekz>
        se <tld var="es"/>o mem kontra&ubreve;batalus <klr>[&#8230;]</klr>
        <fnt>
          <bib>Hamlet</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel ratoj sur la bastonego de <tld var="es"/>o
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Unua</lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; tuta kohorto e&ccirc; <tld/>o estas malforta
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u riskas vi erari, sekvante mem <tld/>on
        <fnt>
          <aut>J. Racine, trad. G. Lagrange</aut>,
          <vrk>Fedra</vrk>,
          <lok>1991</lok>
        </fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_je;&c_r;&c_k;&c_u;&c_l;&c_je;&c_s;</trd>
  <trd lng="bg">&c_H;&c_je;&c_r;&c_k;&c_u;&c_l;&c_je;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>Hercules</trd>,
    <trd>Herkules</trd>,
    <trd>H&eacute;raakl&eacute;s</trd>
  </trdgrp>
  <trd lng="en">Hercules</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_r;&f_k;&a_w;&a_l;</trd>,
    <trd>&a_h;&a_r;&a_A;&f_k;&a_l;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">Hercule</trd>
  <trd lng="hr">Herkul</trd>
  <trd lng="it">Ercole</trd>
  <trd lng="nl">Hercules</trd>
  <trd lng="ru">&c_G;&c_je;&c_r;&c_k;&c_u;&c_l;&c_je;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>Hercules</trd>,
    <trd>Herkules</trd>,
    <trd>H&eacute;raakl&eacute;s</trd>
  </trdgrp>
  <trd lng="uk">&c_G;&c_je;&c_r;&c_k;&c_u;&c_l;&c_je;&c_s;</trd>
</drv>

<drv mrk="herkul.H0o">
  <kap><tld/>o</kap>
  <snc mrk="herkul.H0o.nomo">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena.
    </dif>
  </snc>
  <snc mrk="herkul.H0o.AST">
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
    <mlg>Her</mlg>
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera <ref tip="lst" cel="stel.0figuro"
      lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Hercules</trd>):
      <ekz>
        NGC 6141 estas galaksio en konstelacio <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=119240"
              >NGC 6083</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_je;&c_r;&c_k;&c_u;&c_l;&c_je;&c_s;</trd>
  <trd lng="bg">&c_H;&c_je;&c_r;&c_k;&c_u;&c_l;&c_je;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>Hercules</trd>,
    <trd>Herkules</trd>,
    <trd>H&eacute;raakl&eacute;s</trd>
  </trdgrp>
  <trd lng="en">Hercules</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_r;&f_k;&a_w;&a_l;</trd>,
    <trd>&a_h;&a_r;&a_A;&f_k;&a_l;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">Hercule</trd>
  <trd lng="hr">Herkul</trd>
  <trd lng="it">Ercole</trd>
  <trd lng="nl">Hercules</trd>
  <trd lng="ru">&c_G;&c_je;&c_r;&c_k;&c_u;&c_l;&c_je;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>Hercules</trd>,
    <trd>Herkules</trd>,
    <trd>H&eacute;raakl&eacute;s</trd>
  </trdgrp>
  <trd lng="uk">&c_G;&c_je;&c_r;&c_k;&c_u;&c_l;&c_je;&c_s;</trd>
</drv>

<drv mrk="herkul.h0o">
  <kap><tld lit="h"/>o</kap>
  <snc mrk="herkul.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fortulo.
    </dif>
    <trd lng="fr">hercule</trd>
  </snc>
</drv>
</art>

