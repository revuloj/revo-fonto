<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: herkul.xml,v 1.13 2017/04/26 12:10:24 revo Exp $">
<kap>
  <rad>Herkul</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="herkul.0o">
  <kap><tld/>o</kap>
  <snc mrk="herkul.0o.mita">
    <uzo tip="fak">MIT</uzo>
    <dif>
      Latinigita nomo de la helena heroo
      <ref tip="vid" cel="herakl1.0o">Heraklo</ref>,
      (<trd lng="lat">Herkules</trd>)
      eble devenanta de la etruska formo
      Herklo (Herkles),
      pri kiu ekzistas romia-itala mito,
      la&ubreve; kiu li estis en Italujo:
      <ekz>
        &ccirc;u riskas vi erari, sekvante mem <tld/>on?
        <fnt>
          <aut>J. Racine, trad. G. Lagrange</aut>,
          <vrk>Fedra</vrk>,
          <lok>1991</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="herkul.0o.nomo">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o"
      lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena.
    </dif>
  </snc>
  <snc mrk="herkul.0o.AST">
    <fnt>
      <aut>Galad&iacute;-Enr&iacute;quez, David; Wandel, Amri</aut>:
      <vrk>La kosmo kaj ni. Galaksioj, planedoj kaj vivo en la
      universo</vrk>.
      <lok>FEL, Antverpeno, 2005</lok>
    </fnt>
    <mlg>Her</mlg>
    <uzo tip="fak">AST</uzo>
    <dif>
      Nord-hemisfera <ref tip="lst" cel="stel.0figuro"
      lst="voko:stelfiguroj">stelfiguro</ref>
      (<trd lng="la">Hercules</trd>).
    </dif>
  </snc>
  <trd lng="be">&c_G;&c_je;&c_r;&c_k;&c_u;&c_l;&c_je;&c_s;</trd>
  <trd lng="bg">&c_H;&c_je;&c_r;&c_k;&c_u;&c_l;&c_je;&c_s;</trd>
  <trdgrp lng="cs">
    <trd>Hercules</trd>,
    <trd>Herkules</trd>,
    <trd>H&eacute;raakl&eacute;s</trd>
  </trdgrp>
  <trd lng="en">Hercules</trd>
  <trdgrp lng="fa">
    <trd>&a_h;&a_r;&f_k;&a_w;&a_l;</trd>,
    <trd>&a_h;&a_r;&a_A;&f_k;&a_l;&a_s;</trd>
  </trdgrp>
  <trd lng="fr">Hercule</trd>
  <trd lng="hr">Herkul</trd>
  <trd lng="it">Ercole</trd>
  <trd lng="nl">Hercules</trd>
  <trd lng="ru">&c_G;&c_je;&c_r;&c_k;&c_u;&c_l;&c_je;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>Hercules</trd>,
    <trd>Herkules</trd>,
    <trd>H&eacute;raakl&eacute;s</trd>
  </trdgrp>
  <trd lng="uk">&c_G;&c_je;&c_r;&c_k;&c_u;&c_l;&c_je;&c_s;</trd>
</drv>

<drv mrk="herkul.h0o">
  <kap><tld lit="h"/>o</kap>
  <snc mrk="herkul.0o.fig">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Fortulo.
    </dif>
    <trd lng="fr">hercule</trd>
  </snc>
</drv>
</art>

