<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: teac.xml,v 1.13 2022/03/28 12:03:04 revo Exp $">
<kap>
  <rad>teac</rad>/oj
</kap>
<drv mrk="teac.0oj">
  <kap><tld/>oj</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="famili.0o.BIO" lst="voko:botanikaj_familioj">Familio</ref>
      de <ref tip="malprt" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Theaceae</trd>):
      <ekz>
        <tld/>oj estas lignecaj, &ccirc;iamverdaj plantoj kun disaj folioj, la familio enhavas
        &ccirc;irka&ubreve; 600 tropikajn kaj subtropikajn speciojn en &ccirc;irka&ubreve; 35 genroj, ili
        kreskas en la montaraj arbaroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=34033">Teacoj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_ch;&c_a;&c_je;&c_v;&c_i;</trd>
  <trd lng="ca">te&agrave;cies</trd>
  <trd lng="cs">&ccaron;ajovn&iacute;kov&eacute; <klr>(&ccaron;ele&dcaron;)</klr></trd>
  <trd lng="de">Teegew&auml;chse</trd>
  <trd lng="fr">Th&eacute;ac&eacute;es</trd>
  <trdgrp lng="hu">
    <trd>teacserjef&eacute;l&eacute;k</trd>,
    <trd>teaf&eacute;l&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">theeboomfamilie</trd>
  <trdgrp lng="pt">
   <trd>te&aacute;ceas</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_ch;&c_a;&c_j;&c_n;&c_y;&c_je;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_ch;&c_a;&c_je;&c_v;&c_y;&c_je;<klr tip="ind">
(&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>,
    <trd>&c_k;&c_a;&c_m;&c_je;&c_l;&c_i;&c_je;&c_v;&c_y;&c_je;<klr tip="amb"> (&c_s;&c_je;&c_m;&c_je;&c_j;&c_s;&c_t;&c_v;&c_o;)</klr></trd>
  </trdgrp>
  <trd lng="sk">&ccaron;ajovn&iacute;kov&eacute;</trd>
  <trd lng="uk">&#x447;&#x430;&#x439;&#x43D;&#x456;</trd>
</drv>
</art>
