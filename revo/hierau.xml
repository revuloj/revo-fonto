<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: hierau.xml,v 1.45 2024/08/25 12:45:28 revo Exp $">
<kap>
   <ofc>*</ofc>
   <rad>hiera&ubreve;</rad>
   <fnt> <bib>UV</bib></fnt>
</kap>
<drv mrk="hierau.0">
  <kap><ofc>*</ofc><tld/></kap>
  <gra><vspec>adverbo</vspec></gra>
  <snc mrk="hierau.0.KOMUNE">
    <dif>
      En la &jcirc;us anta&ubreve;a <ref tip="super" cel="tag.0o.diurn">tago</ref>, unu tagon
      anta&ubreve; la nuna:
      <ekz>
        <tld/> estis vendredo, kaj postmorga&ubreve; estos lundo
        <fnt>
          <bib>F</bib>
          <lok>&FE; 20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> je la sepa horo la febro lin forlasis
        <fnt>
          <bib>NT</bib>
          <lok>Johano 4:52</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/> vespere ili estis ankora&ubreve; tiel belaj, kaj nun &ccirc;iuj floroj pendas
        seki&gcirc;intaj
        <fnt>
          <bib>Fab1</bib>
          <lok>Floroj de la malgranda Ida</lok>
        </fnt>!
      </ekz>
      <ekz>
        <ind><tld/> nokte</ind> <klr>(<ref cel="nokt.last0e">lastnokte</ref>)</klr>
        ni pasigis tre zorgoplenajn horojn
        <fnt>
          <bib>MrA</bib>
          <lok>&Ccirc;apitro XIV</lok>
        </fnt>.
        <trd lng="zh">&#26152;&#22812; <pr>zu&oacute; y&egrave;</pr></trd>
      </ekz>
     </dif>
     <ref tip="vid" cel="hodiau.0">hodia&ubreve;</ref>
     <ref tip="ant" cel="morgau.0">morga&ubreve;</ref>
     <trd lng="th">&#3648;&#3617;&#3639;&#3656;&#3629;&#3623;&#3634;&#3609;</trd>
   </snc>
   <trd lng="be">&c_u;&c_ch;&c_o;&c_r;&c_a;</trd>
   <trd lng="bg">&c_v;&c_ch;&c_je;&c_r;&c_a;</trd>
   <trd lng="bo">&t_kha;&t_tsheg;&t_sa;&t_nga;&t_tsheg;</trd>
  <trd lng="br">dec'h</trd>
   <trd lng="ca">ahir</trd>
   <trd lng="cs">v&ccaron;era</trd>
   <trd lng="de">gestern</trd>
   <trdgrp lng="el">
     <trd>&chi;&theta;&epsilon;&sigma_fina;</trd>,
     <trd>&chi;&tau;&epsilon;&sigma_fina;</trd>
   </trdgrp>
   <trd lng="en">yesterday</trd>
   <trd lng="es">ayer</trd>
   <trd lng="fr">hier</trd>
   <trd lng="he">&alef;&tav;&mem;&vav;&lamed;</trd>
   <trd lng="fi">eilen</trd>
   <trd lng="hu">tegnap</trd>
   <trd lng="id">kemarin</trd>
   <trd lng="ils" kod="M569x533S2ff00482x483S10002510x516S2b705540x503S10034554x503"></trd>
   <trd lng="it">ieri</trd>
   <trd lng="ja">&#26152;&#26085;
   <pr>&#12365;&#12398;&#12358;</pr></trd>
   <trd lng="nl">gisteren</trd>
   <trd lng="pl">wczoraj</trd>
   <trd lng="pt">ontem</trd>
   <trd lng="ro">ieri</trd>
   <trd lng="ru">&c_v;&c_ch;&c_je;&c_r;&c_a;</trd>
   <trd lng="sgn" kod="M537x538S31500483x483S1a000517x509S36d00479x526">h@03</trd>
   <trd lng="sv">i <ind>g&aring;r</ind></trd>
   <trd lng="sw">jana</trd>
  <trd lng="tr">d&uuml;n</trd>
   <trd lng="uk">&c_u;&c_ch;&c_o;&c_r;&c_a;</trd>
  <trd lng="vo">yesdel</trd>
   <trdgrp lng="zh">
     <trd>&#26152;&#26085; <pr>zu&oacute; r&igrave;</pr></trd>,
     <trd>&#26152;&#22825; <pr>zu&oacute; ti&amacron;n</pr></trd>
   </trdgrp>
</drv>

<drv mrk="hierau.0a">
   <kap><ofc>*</ofc><tld/>a</kap>
   <snc mrk="hierau.0a.KOMUNE">
     <dif>
       Okazinta <tld/>:
       <ekz>
         lia <tld/>a parolo estis tre bela
         <fnt><bib>F</bib>, <lok>&FE; 40</lok></fnt>;
       </ekz>
      <ekz>
        la vespero pasis kiel la <tld/>a
        <fnt>
          <bib>Fab1</bib>
          <lok>Vojkamarado</lok>
        </fnt>;
      </ekz>
      <ekz>
         ne beda&ubreve;ru <tld/>an <klr>(pasintan tagon a&ubreve; tempon)</klr>, ne atendu
         morga&ubreve;an, ne forlasu hodia&ubreve;an
         <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="hodiau.0a">hodia&ubreve;a</ref>,
      <ref cel="morgau.0a">morga&ubreve;a</ref>
    </refgrp>
  </snc>
   <trd lng="be">&c_u;&c_ch;&c_o;&c_r;&c_a;&c_sh;&c_n;&c_ib;</trd>
   <trd lng="de">gestriger</trd>
   <trd lng="fi">eilinen</trd>
   <trdgrp lng="fr">
     <trd>d'<ind>hier</ind></trd>,
     <trd>pass&eacute;<klr> (d'hier)</klr></trd>
   </trdgrp>
   <trd lng="it">di <ind>ieri</ind></trd>
   <trd lng="pl">wczorajszy</trd>
  <trd lng="uk">&c_u;&c_ch;&c_o;&c_r;&c_a;&c_sh;&c_n;&c_ib;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#26152; <pr>zu&oacute;</pr></trd>,
    <trd>&#26152;&#22825; <pr>zu&oacute;ti&amacron;n</pr></trd>,
    <trd>&#26152;&#26085; <pr>zu&oacute;r&igrave;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hierau.0o">
  <kap><tld/>o</kap>
  <snc mrk="hierau.0o.KOMUNE">
    <dif>
      <ref tip="dif" cel="tag.antaux0o">Anta&ubreve;tago, anta&ubreve;a
      tago</ref>
      <ekz>
        vi demandas, kia estas mia opinio rilate al la <tld/>o? la simpla! &gcirc;i devis okazi
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        la hiera&ubreve;o estis rekonstrui&gcirc;anta el malgrandaj pecetoj, sed maltrankvilo ne
        forlasis la direktoron
        <fnt>
          <bib>MkM</bib>
          <lok>7</lok>
        </fnt>;
      </ekz>
      <ekz>
        kio alia estas la vivo, ol diferenco inter la <tld/>o kaj hodia&ubreve;o, kiam oni
        atendas ion de la morga&ubreve;o
        <fnt>
          <bib>VaK</bib>
          <lok>Sesa &Ccirc;apitro</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="morgau.0o">morga&ubreve;o</ref>
  </snc>
   <trdgrp lng="be">
     <trd>&c_u;&c_ch;&c_o;&c_r;&c_a;&c_sh;&c_n;&c_ib; &c_d;&c_z;&c_je;&c_n;&c_mol;</trd>,
     <trd>&c_u;&c_ch;&c_o;&c_r;&c_a;</trd>
   </trdgrp>
   <trd lng="bg">&c_v;&c_ch;&c_je;&c_r;&c_a;&c_sh;&c_n;&c_i;&c_ja;&c_t;
&c_d;&c_je;&c_n;</trd>
   <trd lng="br">derc'hent</trd>
   <trd lng="de">Gestern</trd>
   <trdgrp lng="fi">
     <trd>eilinen <klr tip="amb"> (subst.)</klr></trd>,
     <trd>eilisp&auml;iv&auml;</trd>,
     <trd>edellinen p&auml;iv&auml;</trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>veille<klr tip="ind"> (jour pr&eacute;c&eacute;dent)</klr>
     </trd>,
     <trd>jour pr&eacute;c&eacute;dent</trd>
   </trdgrp>
   <trd lng="it">ieri<klr> (sost.)</klr></trd>
   <trdgrp lng="pl">
     <trd>wczoraj</trd>,
     <trd>dzie&nacute; <ind>wczorajszy</ind></trd>
   </trdgrp>
   <trd lng="pt">o dia de <ind>ontem</ind></trd>
  <trd lng="uk">&c_u;&c_ch;&c_o;&c_r;&c_a;&c_sh;&c_n;&c_ib;&c_j; &c_d;&c_je;&c_n;&c_mol;</trd>
  <trd lng="zh">&#21069;&#19968;&#22825; <pr>qi&aacute;ny&imacron;ti&amacron;n</pr></trd>
</drv>

<drv mrk="hierau.antaux0">
  <kap>anta&ubreve;<tld/></kap>
  <snc mrk="hierau.antaux0.KOMUNE">
    <dif>
      Du tagojn anta&ubreve; la nuna:
      <ekz>
        mi komencis labori anta&ubreve;<tld/>
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        male de la <tld/>aj kaj anta&ubreve;<tld/>aj kutimoj, la domo estis &ccirc;ie lumigata
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Na&ubreve;a</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la anta&ubreve;hiera&ubreve;a fulmotondro tondro derompis angula&jcirc;on de la templo
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#3623;&#3634;&#3609;&#3595;&#3639;&#3609;</trd>
  </snc>
   <trd lng="be">&c_p;&c_a;&c_z;&c_a;&c_w;&c_ch;&c_o;&c_r;&c_a;</trd>
   <trd lng="bg">&c_z;&c_a;&c_v;&c_ch;&c_je;&c_r;&c_a;</trd>
   <trd lng="bo">&t_kha;&t_-a;&t_gigu;&t_tsheg;&t_nya;&t_gigu;&t_na;&t_tsheg;&t_ma;&t_tsheg;</trd>
  <trd lng="br">derc'hent-dec'h</trd>
   <trd lng="de">vorgestern</trd>
   <trdgrp lng="el">
     <trd>&pi;&rho;&omikron;&chi;&theta;&epsilon_ton;&sigma_fina;</trd>,
     <trd>&pi;&rho;&omikron;&chi;&tau;&epsilon_ton;&sigma_fina;</trd>
   </trdgrp>
   <trdgrp lng="es">
     <trd>anteayer</trd>,
     <trd>antes de ayer</trd>
   </trdgrp>
   <trdgrp lng="fi">
     <trd>toissa p&auml;iv&auml;n&auml;</trd>
   </trdgrp>
   <trd lng="fr">avant-hier</trd>
   <trd lng="he">&shin;&lamed;&shin;&vav;&fmem;</trd>
   <trd lng="hu">tegnapel&odblac;tt</trd>
   <trdgrp lng="it">
     <trd>avantieri</trd>,
     <trd>l'altro <ind>ieri</ind></trd>
   </trdgrp>
   <trd lng="ja">&#19968;&#26152;&#26085;
<pr>&#12362;&#12392;&#12392;&#12356;</pr></trd>
   <trd lng="nl">eergisteren</trd>
   <trd lng="pl">przedwczoraj</trd>
   <trd lng="pt">anteontem</trd>
   <trd lng="ro">alalt&abreve;ieri</trd>
   <trd lng="ru">&c_p;&c_o;&c_z;&c_a;&c_v;&c_ch;&c_je;&c_r;&c_a;</trd>
   <trd lng="sv">i <ind>f&ouml;rrg&aring;r</ind></trd>
   <trd lng="tr">evvelki g&uuml;n</trd>
  <trdgrp lng="uk">
    <trd>&c_p;&c_o;&c_z;&c_a;&c_v;&c_ch;&c_o;&c_r;&c_a;</trd>,
    <trd>&c_p;&c_je;&c_r;&c_je;&c_d;&c_u;&c_ch;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#21069;&#26085; <pr>qi&aacute;nr&igrave;</pr></trd>,
    <trd>&#21069;&#22825; <pr>qi&aacute;nti&amacron;n</pr></trd>
  </trdgrp>
</drv>
</art>
