  <trd lng="en">lasso<klr> (noun)</klr></trd>
  <trdgrp lng="id">
    <trd>laso</trd>,
    <trd><baz>tali</baz>tali laso</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25237;&#12370;&#12394;&#12431;  <pr>&#12394;&#12370;&#12394;&#12431;</pr></trd>,
    <trd>&#36650;&#12394;&#12431;  <pr>&#12426;&#12435;&#12394;&#12431;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>arkan</trd>,
    <trd>lasso</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_a;&c_r;&c_k;&c_a;&c_n;</trd>,
    <trd>&c_l;&c_a;&c_s;&c_o;</trd>
  </trdgrp>
</drv>

