<drv mrk="sxnur.0i">
  <kap><tld/>i</kap>
  <snc mrk="sxnur.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="lig.0i.malhelpimovon">Ligi</ref> per <tld/>o:
      <ekz>
        liaj triplandumaj botoj estis <tld/>itaj &gcirc;is la genuo
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la veturilo balanci&gcirc;is en la kavoj <klr>[&#x2026;]</klr> &scirc;i &ccirc;irka&ubreve;<tld/>is &gcirc;in malbone,
        fojon post fojo &scirc;i re<tld/>is
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi kun<tld/>os miajn manojn kaj piedojn kaj tuj saltos en la maron
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_n;&c_u;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_sh;&c_n;&c_u;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">schn&uuml;ren</trd>
  <trd lng="en">tie<klr> (vt)</klr></trd>
  <trdgrp lng="es">
    <trd>atar con cuerda</trd>,
    <trd>encordar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>attacher par une corde</trd>,
    <trd>encorder</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x3057;&#x3070;&#x308B;</trd>,
    <trd>&#x304F;&#x304F;&#x308B;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>zwi&aogonek;za&cacute;  <ind>lin&aogonek;</ind></trd>,
    <trd>zasznurowa&cacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&#x2019;&c_ja;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_v;&#x2019;&c_ja;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i; &c_m;&c_o;&c_t;&c_u;&c_z;&c_k;&c_o;&c_m;</trd>,
    <trd>&c_z;&c_a;&c_v;&#x2019;&c_ja;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x675F; <pr>sh&#xF9;</pr></trd>,
