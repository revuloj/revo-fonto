<art mrk="$Id: sxnur.xml,v 1.115 2022/03/09 12:47:45 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;nur</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="sxnur.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="sxnur.0o.KOMUNE">
    <dif>
      Kuntorditaj fadenoj uzataj por ligi:
      <ekz>
        &scirc;i malsuprenigis ilin per <tld/>o tra la fenestro
        <fnt>
          <bib>MT</bib>
          <lok>Josuo 2:15</lok>
        </fnt>;
      </ekz>
      <ekz>
        alligi bubalon per <tld/>o
        <fnt>
          <bib>MT</bib>
          <lok>Ijob 39:10</lok>
        </fnt>;
      </ekz>
      <ekz>
        dum la embuskuloj sidis &ccirc;e &scirc;i en alia &ccirc;ambro, <klr>[&#8230;]</klr> li dis&scirc;iris la
        &scirc;nurojn
        <fnt>
          <bib>MT</bib>
          <lok>Ju&gcirc;istoj 16:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili lasis ilin en la maro, kaj samtempe malligis la <tld/>ojn de la direktiloj
        <fnt>
          <bib>NT</bib>
          <lok>La agoj 27:40</lok>
        </fnt>;
      </ekz>
      <ekz>
        fortikaj ankro<tld/>oj
        <fnt>
          <bib>Fab3</bib>
          <lok>Koboldo &ccirc;e la butikisto</lok>
        </fnt>;
      </ekz>
      <ekz>
        gardistoj, armitaj per vipoj, finitaj per pecoj da plumbo kaj fero, anstata&ubreve; la
        klak<tld/>oj <klr>(led<ref cel="rimen.0o">rimeno</ref>)</klr>
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <ind>mezur<tld/>o</ind> 
        <klr>(vd <ref cel="mezur.0ilo">mezurilo</ref>)</klr>
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 28:17</lok>
        </fnt>;
        <trd lng="de">Richtschnur</trd>
      </ekz>
      <ekz>
        <tld/>o de tridek ulnoj prezentis &gcirc;ian mezuron
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 7:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        inter la arboj sur <tld/>oj pendas longa tabulo, tio estas balancilo
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la veturigisto <klr>[&#8230;]</klr> kuntiris per la mano la longajn
        konduk<tld/>ojn <klr>(vd <ref cel="rimen.konduk0o">kondukrimeno</ref>)</klr>,
        kaj per vo&ccirc;o ra&ubreve;ka de teruro li kriis al la &ccirc;evaloj, ke ili
        haltu
        <fnt>
          <bib>Marta</bib>
        </fnt>;
      </ekz>
      <ekz>
        bruo de interparoloj miksis sin kun pla&ubreve;dado de fontano, kies guto<tld/>oj
        <klr>(vd <ref cel="radi.0o.discelanta">radio<sncref/></ref>)</klr>
        <klr>[&#8230;]</klr> disbatis sin sur <klr>[la marmoro]</klr>
        <fnt>
          <bib>QuV</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiam oni volis meti la <tld/>on &ccirc;irka&ubreve; lian kolon
        <klr>(vd <ref cel="pend.0umi">pendumi</ref>)</klr>, li diris, ke anta&ubreve; ol mizera
        pekinto estas ekzekutata, oni ja &ccirc;iam plenumas al li ian senkulpan deziron
        <fnt>
          <bib>Fab1</bib>
          <lok>Fajrilo</lok>
        </fnt>;
      </ekz>
      <ekz>
        en dom' de pendigito pri <tld/>o ne parolu
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>tordi <tld/>ojn el sablo</ind>
        <klr>(<ref tip="vid" cel="van.0e">vane</ref> peni)</klr>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">pisser dans un violon</trd>
      </ekz>
<!--
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        <ind>trostre&ccirc;i la <tld/>on</ind>
        <klr>(tro ofte uzi unu saman rimedon)</klr>.
        <trd lng="fr">tirer sur la corde</trd>
      </ekz>
-->
    </dif>
    <refgrp tip="vid">
       <ref cel="cxen.0o.TEK">&ccirc;eno</ref>,
       <ref cel="drat.0o.kablo">drato</ref>,
       <ref cel="faden.0o.TEKS">fadeno</ref>,
       <ref cel="katen.0o.JUR">kateno</ref>,
       <ref cel="line.0o">lineo</ref>
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_v;&c_ja;&c_r;&c_o;&c_w;&c_k;&c_a;</trd>,
      <trd>&c_sh;&c_p;&c_a;&c_g;&c_a;&c_t;</trd>,
      <trd>&c_sh;&c_n;&c_u;&c_r;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Schnur</trd>,
