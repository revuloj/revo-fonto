    <trd>jarcia<klr> (t&eacute;rmino marino)</klr></trd>,
    <trd>cable<klr tip="ind"> (arn&eacute;s de cables)</klr></trd>,
    <trd>cabo</trd>,
    <trd>cordaje<klr> (t&eacute;rmino marino)</klr></trd>
