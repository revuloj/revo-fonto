<drv mrk="sxnur.0i">
  <kap><tld/>i</kap>
  <snc mrk="sxnur.0i.KOMUNE">
    <dif>
      <ref tip="super" cel="lig.0i.malhelpimovon">Ligi</ref> per <tld/>o:
      <ekz>
        liaj triplandumaj botoj estis <tld/>itaj &gcirc;is la genuo
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        la veturilo balanci&gcirc;is en la kavoj <klr>[&#8230;]</klr> &scirc;i &ccirc;irka&ubreve;<tld/>is &gcirc;in malbone,
        fojon post fojo &scirc;i re<tld/>is
        <fnt>
          <bib>VRA</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi kun<tld/>os miajn manojn kaj piedojn kaj tuj saltos en la maron
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_sh;&c_n;&c_u;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_p;&c_r;&c_a;&c_sh;&c_n;&c_u;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">schn&uuml;ren</trd>
  <trd lng="en">tie</trd>
  <trdgrp lng="es">
    <trd>atar con cuerda</trd>,
    <trd>encordar</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>attacher par une corde</trd>,
    <trd>encorder</trd>
  </trdgrp>
</drv>

<drv mrk="sxnur.0ego">
  <kap><tld/>ego</kap>
  <snc>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">MAR</uzo>
    <dif>
      Aparte dika
      <ref tip="super" cel="sxnur.0o.KOMUNE"><tld/>o</ref> el vegeta&jcirc;aj
      a&ubreve; metalaj fadenoj, uzata por tiri a&ubreve; movi pezajn objektojn:
      <ekz>
        subite la <tld/>egoj, kiuj <klr>[la balonon]</klr> tenis,
        estis distran&ccirc;itaj, kaj nun &gcirc;i ekflugis supren
        kun la aerveturisto
        <fnt><bib>Fab3</bib><lok>Kolo de botelo</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i malsekigis <klr>[la fadenon]</klr> per siaj larmoj,
        kaj tial &gcirc;i estis fortika kiel ankra <tld/>ego
        <fnt><bib>Fab3</bib><lok>&Scirc;tono de sa&gcirc;uloj</lok></fnt>;
      </ekz>
      <ekz>
        sur la <tld/>egoj <klr>[kunligantaj du turojn]</klr> glitas
        super la akvo de bordo al bordo vagonetoj
        <fnt><bib>Metrop</bib><lok>&ccirc;ap. 11</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kabl.0o.sxnurego">kablo <sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_o;&c_s;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Seil</trd>,
    <trd>Tau</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>painter</trd>,
    <trd>hawser</trd>,
    <trd>cable</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>maroma</trd>,
    <trd>jarcia<klr> (t&eacute;rmino marino)</klr></trd>,
    <trd>cable<klr tip="ind"> (arn&eacute;s de cables)</klr></trd>,
    <trd>cabo</trd>,
    <trd>cordaje<klr> (t&eacute;rmino marino)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>c&acirc;ble<klr tip="ind"> (faisceau de fils)</klr></trd>,
    <trd>filin</trd>,
    <trd>cordage<klr tip="ind"> (c&acirc;ble)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fune</trd>,
    <trd>gomena</trd>,
    <trd>cordone</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_n;&c_a;&c_t;</trd>,
    <trd>&c_t;&c_r;&c_o;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="sxnur.0estro">
  <kap><tld/>estro <fnt><bib>ItEsp</bib></fnt></kap>
  <snc mrk="sxnur.0estro.SPO">
    <uzo tip="fak">SPO</uzo>
    <dif>
      Tiu, kiu gvidas vicon de montogrimpistoj
      kunligitaj per sekur&scirc;nuro.
    </dif>
  </snc>
  <trd lng="de">Seilf&uuml;hrer</trd>
  <trd lng="en">climb leader</trd>
  <trd lng="fr">premier de cord&eacute;e</trd>
  <trd lng="es">gu&iacute;a de cordada<klr> (alpinismo)</klr></trd>
  <trd lng="it">capocordata</trd>
</drv>

<drv mrk="sxnur.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc>
    <dif>
      Faristo de <tld/>oj:
      <ekz>
        la <tld/>isto tordas siajn <tld/>ojn sur la ligna vojeto, kiu iras la&ubreve;longe de
        la murego
        <fnt>
          <bib>Fab3</bib>
          <lok>Sub la saliko</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ruzulo prenis lin per la piedoj <klr>[&#8230;]</klr>, turniginta lin rapide kiel
        <tld/>isto
        <fnt>
          <vrk><url ref="https://www.gutenberg.org/files/38240/38240-h/vol2/e204.htm">Bal de la
          enmaskigitoj</url></vrk>
          <lok>The Esperantist, Vol. 2, No. 4 | </lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Seiler</trd>
</drv>


<drv mrk="sxnur.kapto0o">
  <kap>kapto<tld/>o</kap>
  <snc mrk="sxnur.kapto0o.jxeta">
    <dif>
      <tld lit="&Scirc;"/>o kun granda ma&scirc;o, kiun oni &jcirc;etas &ccirc;irka&ubreve; ion por kapti &gcirc;in:
      <ekz>
        memfida svingo de la majstra mano... la fatala kapto<tld/>o falis sur la kornojn
        de juna belega cervido
        <fnt>
          <bib>&Ccirc;uk&ccirc;oj</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u pro la kirlaj ondoj de preterveturanta giganta kesto, a&ubreve; tial, ke oni ne
        tenis sufi&ccirc;e flekseble la kapto<tld/>egojn dum la tajdo&scirc;an&gcirc;o &ndash; la farbanto
        ekflugas sur sia mortobalancilo for de la &scirc;elo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>      
    </dif>
    <ref tip="sub" cel="laz.0o">lazo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_l;&c_a;&c_s;&c_o;</trd>,
    <trd>&c_a;&c_r;&c_k;&c_a;&c_n;</trd>
  </trdgrp>
<trd lng="de">Fangleine</trd>
</drv>


<drv mrk="sxnur.rekto0o">
  <kap>rekto<tld/>o</kap>
  <snc>
  <uzo tip="fak">TEK</uzo>
    <dif>
      &Scirc;nuro servanta por desegni rektajn liniojn, plejofte
      <ref cel="horizo1.0o.FIZ">horizontalojn</ref>, por vicigi a&jcirc;ojn
      la&ubreve; unu sama rekta linio:
      <ekz>
        aleo desegnita per la rekto<tld/>o
        <fnt>
          <bib>PIV1</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiu etendis super &gcirc;i rekto<tld/>on
        <fnt><bib>MT</bib>, <lok>&Ijo; 38:5</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Richtschnur</trd>
  <trd lng="es">tendel</trd>
  <trd lng="fr">cordeau</trd>
  <trdgrp lng="hu">
    <trd>m&eacute;r&odblac;zsin&oacute;r</trd>,
    <trd>zsin&oacute;rm&eacute;rt&eacute;k</trd>
  </trdgrp>
  <trd lng="it">guida<klr> (corda o cordino)</klr></trd>
