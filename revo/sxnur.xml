<drv mrk="sxnur.0ego">
  <kap><tld/>ego</kap>
  <snc>
  <uzo tip="fak">TEK</uzo>
  <uzo tip="fak">MAR</uzo>
    <dif>
      Aparte dika
      <ref tip="super" cel="sxnur.0o.KOMUNE"><tld/>o</ref> el vegeta&jcirc;aj
      a&ubreve; metalaj fadenoj, uzata por tiri a&ubreve; movi pezajn objektojn:
      <ekz>
        subite la <tld/>egoj, kiuj <klr>[la balonon]</klr> tenis,
        estis distran&ccirc;itaj, kaj nun &gcirc;i ekflugis supren
        kun la aerveturisto
        <fnt><bib>Fab3</bib><lok>Kolo de botelo</lok></fnt>;
      </ekz>
      <ekz>
        &scirc;i malsekigis <klr>[la fadenon]</klr> per siaj larmoj,
        kaj tial &gcirc;i estis fortika kiel ankra <tld/>ego
        <fnt><bib>Fab3</bib><lok>&Scirc;tono de sa&gcirc;uloj</lok></fnt>;
      </ekz>
      <ekz>
        sur la <tld/>egoj <klr>[kunligantaj du turojn]</klr> glitas
        super la akvo de bordo al bordo vagonetoj
        <fnt><bib>Metrop</bib><lok>&ccirc;ap. 11</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="kabl.0o.sxnurego">kablo <sncref/></ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_r;&c_o;&c_s;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Seil</trd>,
    <trd>Tau</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>painter<klr> (noun, nautical)</klr></trd>,
    <trd>hawser<klr> (noun, nautical)</klr></trd>,
    <trd>cable<klr> (noun, rope)</klr></trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>maroma</trd>,
    <trd>jarcia<klr> (t&eacute;rmino marino)</klr></trd>,
    <trd>cable<klr tip="ind"> (arn&eacute;s de cables)</klr></trd>,
    <trd>cabo</trd>,
    <trd>cordaje<klr> (t&eacute;rmino marino)</klr></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>c&acirc;ble<klr tip="ind"> (faisceau de fils)</klr></trd>,
    <trd>filin</trd>,
    <trd>cordage<klr tip="ind"> (c&acirc;ble)</klr></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>fune</trd>,
    <trd>gomena</trd>,
    <trd>cordone</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5927;&#x7DB1;  <pr>&#x304A;&#x304A;&#x3065;&#x306A;</pr></trd>,
    <trd>&#x30B1;&#x30FC;&#x30D6;&#x30EB;  <pr>&#x3051;&#x30FC;&#x3076;&#x308B;</pr></trd>
  </trdgrp>
  <trd lng="pl">lina</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_a;&c_n;&c_a;&c_t;</trd>,
    <trd>&c_t;&c_r;&c_o;&c_s;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_m;&c_o;&c_t;&c_u;&c_z;</trd>,
    <trd>&c_k;&c_a;&c_n;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7E9C; <pr>l&#x1CE;n</pr></trd>,
    <trd>&#x7F06; <pr>l&#x1CE;n</pr></trd>,
    <trd>&#x7E9C;&#x7D22; <pr>l&#x1CE;nsu&#x1D2;</pr></trd>,
    <trd>&#x7F06;&#x7D22; <pr>l&#x1CE;nsu&#x1D2;</pr></trd>,
    <trd>&#x7E9C;&#x7E69; <pr>l&#x1CE;nsh&#xE9;ng</pr></trd>,
    <trd>&#x7F06;&#x7EF3; <pr>l&#x1CE;nsh&#xE9;ng</pr></trd>,
    <trd>&#x7B70; <pr>zu&#xF3;</pr></trd>,
    <trd>&#x7E69; <pr>sh&#xE9;ng</pr></trd>,
    <trd>&#x7EF3; <pr>sh&#xE9;ng</pr></trd>
  </trdgrp>
