    <trd>&c_k;&c_a;&c_n;&c_a;&c_t;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#32412; <pr>l&#462;n</pr></trd>,
    <trd>&#32518; <pr>l&#462;n</pr></trd>,
