<art mrk="$Id: bute.xml,v 1.19 2023/08/31 08:48:54 revo Exp $">
<kap>
<rad>bute</rad>/o
</kap>
<drv mrk="bute.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj" cel="genr.0o.BIO">Genro</ref>
      el la familio <ref tip="malprt" cel="akcipi1.0oj">akcipitredoj</ref>
      (<trd lng="la">Buteo</trd>), mezgranda 
      <ref tip="lst" lst="voko:birdoj" cel="bird.rabo0oj">rabobirdo</ref>
      kun lar&gcirc;aj flugiloj kaj mallonga vosto:
      <ekz>
        la <tld/>oj ofte &scirc;vebas sur termikoj tagmeze super malfermejoj kaj vidi&gcirc;as plej
        ofte farante tion
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16426">Buteo (genro)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/a/af/Steppe_buzzard_(Buteo_buteo_vulpinus).jpg/400px-Steppe_buzzard_(Buteo_buteo_vulpinus).jpg" prm="CC BY-SA 4.0">Mus<tld/>o (Buteo buteo), Nacia Parko Etosha, Namibio
        <fnt>
          <aut>Charles J Sharp, propra verko</aut>
          <lok><url ref="&WCW;/File:Steppe_buzzard_(Buteo_buteo_vulpinus).jpg">&WCW;/File:Steppe_buzzard_(Buteo_buteo_vulpinus).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_k;&c_a;&c_n;&c_ju;&c_k;</trd>
  <trd lng="bg">&c_o;&c_b;&c_i;&c_k;&c_n;&c_o;&c_v;&c_je;&c_n; &c_m;&c_i;&c_sh;&c_je;&c_l;&c_o;&c_v;</trd>
  <trd lng="ca">aligot</trd>
  <trdgrp lng="cs">
    <trd>k&aacute;n&ecaron;</trd>,
    <trd>pojistka</trd>,
    <trd>zar&aacute;&zcaron;ka</trd>,
    <trd>z&aacute;vla&ccaron;ka</trd>
  </trdgrp>
  <trd lng="de">Bussard</trd>
  <trd lng="en">buzzard</trd>
  <trd lng="fr">buse</trd>
  <trd lng="he">&alef;&yod;&he;</trd>
  <trd lng="hu">&ouml;lyv</trd>
  <trd lng="it">poiana</trd>
  <trd lng="ja">&#x30CE;&#x30B9;&#x30EA;</trd>
  <trd lng="pl">myszo&lstroke;&oacute;w</trd>
  <trd lng="ru">&c_k;&c_a;&c_n;&c_ju;&c_k;</trd>
  <trdgrp lng="sk">
    <trd>poistka</trd>,
    <trd>zar&aacute;&zcaron;ka</trd>,
    <trd>z&aacute;vla&ccaron;ka</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43A;&#x430;&#x43D;&#x44E;&#x43A;</trd>,
    <trd>&#x43A;&#x430;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>
</art>
