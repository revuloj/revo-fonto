  <trd lng="br">gwareg<klr tip="ind"> (sonerezh)</klr></trd>
  <trd lng="en">bow</trd>
  <trd lng="es">arco <klr tip="amb">(de instrumentos de
  cuerda)</klr></trd> 
  <trd lng="fr">archet</trd>
  <trd lng="hu">von&oacute;</trd>
  <trd lng="pl">smyczek</trd>
  <trd lng="ru">&c_s;&c_m;&c_y;&c_ch;&c_o;&c_k;</trd>
  <trd lng="sk">sl&aacute;k</trd>
  <trd lng="sv">str&aring;ke</trd>
</drv>
