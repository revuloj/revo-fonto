<kap>
  <ofc>8</ofc>
  <rad>vit</rad>/o
</kap>
<drv mrk="vit.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="dif" cel="vin.0berarbo">Vinberujo</ref>:
      <ekz>
        pro la malvarma klimato <tld/>o ne vegetas en Irlando
        <fnt>
          <aut>M&aacute;ire NicAoidh</aut>
          <vrk><url ref="&Monato;/2000/006878.php">&Gcirc;ardenumi</url></vrk>
          <bib>Monato</bib>
          <lok>2000/04, p. 20</lok>
          </fnt>;
      </ekz>
      <ekz>
        ankora&ubreve; trovi&gcirc;as resova&gcirc;i&gcirc;intaj <tld/>oj sur la sunaj deklivoj de la 
        &ccirc;irka&ubreve;aj montetoj kaj anka&ubreve; en&gcirc;ardene zorgeme rastritaj, alligitaj <tld/>o-linioj
        <fnt>
          <bib>Monato</bib>
          <aut>Judit Felszeghy</aut>
          <vrk><url ref="&Monato;/2007/009351.php">Je via sano!</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <rim>
    <ref tip="hom" cel="vit1.0o">Vito</ref> estas anka&ubreve; vira nomo.
  </rim>
  <trd lng="be">&c_v;&c_ib;&c_n;&c_a;&c_g;&c_r;&c_a;&c_d; 
    <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trdgrp lng="ca">
    <trd>cep</trd>,
    <trd>vinya</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Rebe</trd>,
    <trd>Weinstock</trd>
  </trdgrp>
  <trd lng="el">&kappa;&lambda;&eta;&my;&alfa;&tau;&alfa;&rho;&jota;&alfa_ton;</trd>
  <trdgrp lng="en">
    <trd>grapevine</trd>,
    <trd>grape</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd><klr tip="amb">(&a_d;&a_r;&a_kh;&a_t;) </klr>&a_m;&a_w;</trd>,
    <trd>&a_t;&a_A;&f_k;</trd>
  </trdgrp>
  <trd lng="fr">vigne</trd>
  <trd lng="hu">sz&odblac;l&odblac;<klr tip="ind">
(n&ouml;v&eacute;ny)</klr></trd>
  <trd lng="ja">&#x30D6;&#x30C9;&#x30A6;  <pr>&#x3076;&#x3069;&#x3046;</pr></trd>
  <trd lng="nl">wijnstok</trd>
  <trd lng="pt">parreira</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_n;&c_a;&c_ja;
&c_l;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>r&eacute;va</trd>,
    <trd>vini&ccaron;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>r&eacute;va</trd>,
    <trd>vinn&aacute; r&eacute;va</trd>,
    <trd>v&iacute;no</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_i;&#x43D;&#x43E;&c_g;&c_r;&c_a;&c_d; (&c_r;&#x43E;&c_s;&#x43B;&c_i;&#x43D;&c_a;)</trd>,
    <trd>&c_v;&c_i;&#x43D;&#x43E;&c_g;&c_r;&c_a;&c_d;&#x43D;&c_i;&c_j; &#x43A;&c_u;&c_shch;</trd>
  </trdgrp>
</drv>
</art>
