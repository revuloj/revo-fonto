<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>vit</rad>/o
</kap>
<drv mrk="vit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="vin.0berarbo0berujo">vinberujo</ref>.
    </dif>
  </snc>
</drv>
<drv mrk="vit.0acoj">
  <kap><tld/>acoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Vitaceae</trd>), al kiu apartenas la <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Rebengew&auml;chse</trd>
</drv>
</art>
