<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>vit</rad>/o
</kap>
<drv mrk="vit.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="vin.0berarbo0berujo">vinberujo</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_n;&c_a;&c_g;&c_r;&c_a;&c_d; 
    <klr tip="amb">(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="de">Rebe</trd>
  <trd lng="el">&kappa;&lambda;&eta;&my;&alfa;&tau;&alfa;&rho;&jota;&alfa_ton;</trd>
  <trdgrp lng="en">
    <trd>grapevine</trd>,
    <trd>grape</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd><klr tip="amb">(&a_d;&a_r;&a_kh;&a_t;) </klr>&a_m;&a_w;</trd>,
    <trd>&a_t;&a_A;&f_k;</trd>
  </trdgrp>
  <trd lng="hu">sz&odblac;l&odblac;<klr tip="ind">
(n&ouml;v&eacute;ny)</klr></trd>
  <trd lng="nl">wijnstok</trd>
  <trd lng="pt">parreira</trd>
  <trdgrp lng="ru">
    <trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d; <klr
tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>,
    <trd>&c_v;&c_i;&c_n;&c_o;&c_g;&c_r;&c_a;&c_d;&c_n;&c_a;&c_ja;
&c_l;&c_o;&c_z;&c_a;</trd>
  </trdgrp>
</drv>
</art>
