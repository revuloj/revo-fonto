<vortaro>
<art mrk="$Id$">
<kap>
   <rad>na&ubreve;til</rad>/o
</kap>
<drv mrk="nauxti.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">ZOO</uzo>
   <snc>
     <dif>
       Genro el la klaso <ref tip="super" cel="cefalo.0oj">cefalopodoj</ref>
       (<trd lng="la">Nautilus</trd>), kun spirala &scirc;elo 
plur&ccirc;ambra,
       kies plej granda entenas la molan korpon; vivanta en varmaj maroj.
     </dif>
   </snc>
   <trdgrp lng="de">
      <trd>Perlboot</trd>,
      <trd>Schiffsboot</trd>,
      <trd>Nautilus</trd>
   </trdgrp>
   <trd 
lng="el">&ny;&alfa;&ypsilon;&tau;&jota_ton;&lambda;&omikron;&sigma_fina;</trd>
   <trd lng="en">nautilus</trd>
   <trd lng="hu">nautilusz</trd>
   <trd lng="nl">nautilus</trd>
   <trd lng="fr">nautile</trd>
   <trd lng="pt">n&aacute;tilo</trd>
   <trdgrp lng="ru">
     <trd>&c_n;&c_a;&c_u;&c_t;&c_i;&c_l;&c_u;&c_s;</trd>,
     <trd>&c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_i;&c_k; <klr 
tip="amb">(&c_m;&c_o;&c_l;&c_l;&c_ju;&c_s;&c_k;)</klr></trd>
   </trdgrp>
</drv>
</art>
