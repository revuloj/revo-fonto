<kap>
  <ofc>3</ofc>
  <rad>probabl</rad>/a
</kap>
<drv mrk="probab.0a">
  <kap><tld/>a</kap>
  <snc mrk="probab.0a.KOMUNE">
    <dif>
      Havanta plimulto da &scirc;ancoj por efektivi&gcirc;i, a&ubreve;
      plimulto da motivoj por esti vera:
      <ekz>
        ke viro, kia li, havas malamikojn, tio estas ebla
        &#x2010; e&ccirc; <tld/>a
        <fnt><bib>UrdHadda</bib>, <lok>p. 13</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_m;&c_a;&c_v;&c_je;&c_r;&c_n;&c_y;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_ch;&c_y;&c_m;&c_y;</trd>
  </trdgrp>
  <trd lng="bo">&#xF61;&#xF72;&#xF53;&#xF0B;&#xF54;&#xF0B;&#xF60;&#xF51;&#xFB2;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>potenci&aacute;ln&iacute;</trd>,
    <trd>pravd&ecaron;podobn&yacute;</trd>,
    <trd>probabiln&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>wahrscheinlich</trd>,
    <trd>voraussichtlich</trd>
  </trdgrp>
  <trd lng="es">probable</trd>
  <trd lng="fr">probable</trd>
  <trd lng="hu">val&oacute;sz&iacute;n&udblac;</trd>
  <trdgrp lng="ja">
    <trd>&#x3042;&#x308A;&#x305D;&#x3046;&#x306A;</trd>,
    <trd>&#x78BA;&#x304B;&#x3089;&#x3057;&#x3044;  <pr>&#x305F;&#x3057;&#x304B;&#x3089;&#x3057;&#x3044;</pr></trd>,
    <trd>&#x3082;&#x3063;&#x3068;&#x3082;&#x3089;&#x3057;&#x3044;</trd>,
    <trd>&#x516C;&#x7B97;&#x306E;&#x9AD8;&#x3044;  <pr>&#x3053;&#x3046;&#x3055;&#x3093;&#x306E;&#x305F;&#x304B;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">waarschijnlijk</trd>
  <trdgrp lng="pl">
    <trd>prawdopodobny</trd>,
    <trd>domniemany</trd>
  </trdgrp>
  <trd lng="pt">prov&aacute;vel</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_o;&c_ja;&c_t;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>dok&aacute;zate&lcaron;n&yacute;</trd>,
    <trd>pravdepodobn&yacute;</trd>,
    <trd>uverite&lcaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_j;&#x43C;&#x43E;&c_v;&c_ib;&c_r;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x43F;&c_r;&c_a;&c_v;&c_d;&#x43E;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5F88;&#x53EF;&#x80FD; <pr>h&#x11B;nk&#x11B;n&#xE9;ng</pr></trd>,
    <trd>&#x5927;&#x7D04; <pr>d&#xE0;yu&#x113;</pr></trd>,
    <trd>&#x5927;&#x7EA6; <pr>d&#xE0;yu&#x113;</pr></trd>,
    <trd>&#x9810;&#x6599; <pr>y&#xF9;li&#xE0;o</pr></trd>,
    <trd>&#x9884;&#x6599; <pr>y&#xF9;li&#xE0;o</pr></trd>,
    <trd>&#x83AB;&#x4E0D; <pr>m&#xF2;b&#xF9;</pr></trd>,
    <trd>&#x6216;&#x7136; <pr>hu&#xF2;r&#xE1;n</pr></trd>,
    <trd>&#x83AB;&#x4E0D;&#x662F; <pr>m&#xF2;b&#xF9;sh&#xEC;</pr></trd>,
    <trd>&#x53EF;&#x80FD;&#x5730; <pr>k&#x11B;n&#xE9;ngde</pr></trd>,
    <trd>&#x51E0;&#x4E4E;&#x80FD;&#x80AF;&#x5B9A; <pr>j&#x12B;h&#x16B;n&#xE9;ngk&#x11B;nd&#xEC;ng</pr></trd>,
    <trd>&#x5E7E;&#x4E4E;&#x80FD;&#x80AF;&#x5B9A; <pr>j&#x12B;h&#x16B;n&#xE9;ngk&#x11B;nd&#xEC;ng</pr></trd>,
    <trd>&#x6781;&#x4E3A;&#x53EF;&#x80FD; <pr>j&#xED;w&#xE9;ik&#x11B;n&#xE9;ng</pr></trd>,
    <trd>&#x6975;&#x70BA;&#x53EF;&#x80FD; <pr>j&#xED;w&#xE9;ik&#x11B;n&#xE9;ng</pr></trd>,
    <trd>&#x60F3;&#x5FC5; <pr>xi&#x1CE;ngb&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="probab.0e">
  <kap><tld/>e</kap>
  <snc mrk="probab.0e.KOMUNE">
    <dif>
      Kun grandaj &scirc;ancoj de reali&gcirc;o,
      kvankam ne <ref tip="vid" cel="cert.0e">certe</ref>:
      <ekz>
        la pli maljuna <klr>[el la infanoj]</klr> estas <tld/>e sepjara
        <fnt><bib>Revadoj</bib>, <lok>na&ubreve;a promenado</lok></fnt>;
      </ekz>
      <ekz>
        la bebo fartas sufi&ccirc;e bone kaj <tld/>e pluvivos
        <fnt>
        <aut>Is</aut>
        <vrk>Vegetanta homo naskas mem</vrk>
        <bib>Monato</bib>
        <lok>1996:5, p. 26a</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="ebl.0e">eble</ref>,
      <ref cel="kred.0eble">kredeble</ref>,
      <ref cel="dub.sen0e">sendube</ref>,
      <ref cel="supoz.0eble">supozeble</ref>,
      <ref cel="sxajn.ver0e">ver&scirc;ajne</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_ib;&c_m;&c_a;&c_v;&c_je;&c_r;&c_n;&c_a;</trd>,
    <trd>&c_m;&c_a;&c_g;&c_ch;&c_y;&c_m;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">pravd&ecaron;podobn&ecaron;</trd>
  <trd lng="es">probablemente</trd>
  <trdgrp lng="fr">
    <trd>probablement</trd>,
    <trd>sans doute</trd>
  </trdgrp>
  <trd lng="hu">val&oacute;sz&iacute;n&udblac;leg</trd>
  <trdgrp lng="ja">
    <trd>&#x304A;&#x305D;&#x3089;&#x304F;</trd>,
    <trd>&#x591A;&#x5206;  <pr>&#x305F;&#x3076;&#x3093;</pr></trd>,
    <trd>&#x4E94;&#x5272;&#x4EE5;&#x4E0A;&#x306E;&#x78BA;&#x304B;&#x3055;&#x3067;  <pr>&#x3044;&#x3064;&#x308F;&#x308A;&#x3044;&#x3058;&#x3087;&#x3046;&#x306E;&#x305F;&#x3057;&#x304B;&#x3055;&#x3067;</pr></trd>
  </trdgrp>
  <trd lng="pl">prawdopodobnie</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_o;&c_ja;&c_t;&c_n;&c_o;</trd>
  <trd lng="sk">pravdepodobne</trd>
  <trdgrp lng="zh">
    <trd>&#x5F88;&#x53EF;&#x80FD; <pr>h&#x11B;nk&#x11B;n&#xE9;ng</pr></trd>,
    <trd>&#x5F53;&#x7136; <pr>d&#x101;ngr&#xE1;n</pr></trd>,
    <trd>&#x7576;&#x7136; <pr>d&#x101;ngr&#xE1;n</pr></trd>,
    <trd>&#x516B;&#x6210; <pr>b&#x101;ch&#xE9;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="probab.0o">
  <kap><tld/>o</kap>
  <snc mrk="probab.0o.KOMUNE">
    <ref tip="dif" cel="probab.0eco"><tld/>eco</ref>
  </snc>
  <snc mrk="probab.0o.MAT">
    <uzo tip="fak">MAT</uzo>
    <subsnc mrk="probab.0o.mezuro">
      <fnt><bib>EKV</bib>, <lok>&para;306</lok></fnt>
      <dif>
        <klr>(super <ref cel="algebr.sigma0o">&sigma;-algebro</ref> super aro
        <frm am="bb Omega"><g>&Omega;</g></frm>)</klr>
        Tia <ref tip="super" cel="mezur.0o.MAT">mezuro</ref>
        <frm am="P">P</frm>,
        ke
        &gcirc;iaj valoroj estas pozitivaj, kaj ke
        <frm am="P(bb Omega)=1">P(<g>&Omega;</g>) = 1</frm>.
      </dif>
      <trd lng="de">Wahrscheinlichkeitsma&szlig;</trd>
      <trd lng="en">probability <ind>measure</ind></trd>
      <trdgrp lng="es">
        <trd>probabilidad</trd>,
        <trd>ley de probabilidad</trd>,
        <trd>medida de probabilidad</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>probabilit&eacute;</trd>,
        <trd>loi de probabilit&eacute;</trd>,
        <trd>mesure de probabilit&eacute;</trd>
      </trdgrp>
      <trd lng="hu">val&oacute;sz&iacute;n&udblac;s&eacute;gi m&eacute;rt&eacute;k</trd>
      <trd lng="pl">prawdopodobie&nacute;stwo<klr tip="ind">
      (funkcja)</klr></trd>
      <trd lng="ru">&c_v;&c_je;&c_r;&c_o;&c_ja;&c_t;&c_n;&c_o;&c_s;&c_t;&c_n;&c_a;&c_ja; <ind>&c_m;&c_je;&c_r;&c_a;</ind></trd>
    </subsnc>
    <subsnc mrk="probab.0o.de_okazo">
      <fnt><bib>MatTerm</bib>, <lok>p. 24</lok></fnt>
      <dif>
        <klr>(de <ref cel="okaz.0o.MAT">okazo</ref>, kadre de iu
        <ref cel="spac.probablo0o">probablospaco</ref>)</klr>
        La bildo per la koncerna <tld/>o <sncref ref="probab.0o.mezuro"/>
        de
        tiu okazo.
      </dif>
      <trd lng="de">Wahrscheinlichkeit<klr tip="ind"> (eines
      Ereignisses)</klr></trd>
      <trd lng="en">probability<klr tip="ind"> (of an event)</klr></trd>
      <trd lng="es">probabilidad<klr tip="ind"> (de un
      evento)</klr></trd>
      <trd lng="fr">probabilit&eacute;<klr tip="ind"> (d'un
      &eacute;v&eacute;nement)</klr></trd>
      <trd lng="hu">val&oacute;sz&iacute;n&udblac;<klr tip="ind"> (esem&eacute;ny&eacute;)</klr></trd>
      <trd lng="pl">prawdopodobie&nacute;stwo<klr tip="ind">
      (zdarzenia)</klr></trd>
      <trd lng="ru">&c_v;&c_je;&c_r;&c_o;&c_ja;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;<klr tip="ind"> (&c_s;&c_o;&c_b;&c_y;&c_t;&c_i;&c_ja;
      )</klr></trd>
    </subsnc>
    <ref tip="vid" cel="kalkul.probablo0o"><tld/>okalkulo.</ref>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_a;&c_v;&c_je;&c_r;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trd lng="cs">pravd&ecaron;podobnost</trd>
  <trd lng="ja">&#x78BA;&#x7387;  <pr>&#x304B;&#x304F;&#x308A;&#x3064;</pr></trd>
  <trd lng="pl">prawdopodobie&nacute;stwo</trd>
  <trd lng="sk">pravdepodobnos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&#x43C;&#x43E;&c_v;&c_ib;&c_r;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43F;&c_r;&c_a;&c_v;&c_d;&#x43E;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>

<drv mrk="probab.0eco">
  <kap><tld/>eco</kap>
  <fnt><bib>VdE</bib></fnt>
  <snc mrk="probab.0eco.KOMUNE">
    <dif>
      Eco de io <tld/>a; grado de tiu eco:
      <ekz>
        &ccirc;e manko de certeco oni devas kontenti&gcirc;i je <tld/>eco
        <fnt><bib>PV</bib></fnt>;
      </ekz>
      <ekz>
        la <tld/>eco de lia vizito &ccirc;e tia horo estas tre malgranda
        <fnt><bib>PIV1</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_a;&c_v;&c_je;&c_r;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>pravd&ecaron;podobnost</trd>,
    <trd>probabilita</trd>
  </trdgrp>
  <trd lng="es">probabilidad</trd>
  <trd lng="fr">probabilit&eacute;</trd>
  <trd lng="hu">val&oacute;sz&iacute;n&udblac;s&eacute;g</trd>
  <trdgrp lng="ja">
    <trd>&#x78BA;&#x304B;&#x3089;&#x3057;&#x3055;  <pr>&#x305F;&#x3057;&#x304B;&#x3089;&#x3057;&#x3055;</pr></trd>,
    <trd>&#x84CB;&#x7136;&#x6027;  <pr>&#x304C;&#x3044;&#x305C;&#x3093;&#x305B;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="pl">prawdopodobie&nacute;stwo</trd>
  <trd lng="ru">&c_v;&c_je;&c_r;&c_o;&c_ja;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
  <trd lng="sk">pravdepodobnos&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_ib;&#x43C;&#x43E;&c_v;&c_ib;&c_r;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x43F;&c_r;&c_a;&c_v;&c_d;&#x43E;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x53EF;&#x80FD;&#x6027; <pr>k&#x11B;n&#xE9;ngx&#xEC;ng</pr></trd>,
    <trd>&#x6216;&#x7136;&#x7387; <pr>hu&#xF2;r&#xE1;nl&#xF9;:</pr></trd>,
    <trd>&#x673A;&#x7387; <pr>j&#x12B;l&#xF9;:</pr></trd>,
    <trd>&#x6A5F;&#x7387; <pr>j&#x12B;l&#xF9;:</pr></trd>,
    <trd>&#x6982;&#x7387; <pr>g&#xE0;il&#xF9;:</pr></trd>
  </trdgrp>
</drv>

<drv mrk="probab.kondicxa0o">
  <kap>kondi&ccirc;a <tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>EKV</bib>, <lok>&para;215</lok></fnt>
  <snc>
    <dif>
      <klr>(rilate al <ref cel="okaz.0o.MAT">okazo</ref>
      <frm am="bb a"><g>a</g></frm>,
      kadre de <ref cel="spac.probablo0o">probablospaco</ref>
      <frm am="(bb Omega,bb A,P)">(<g>&Omega;</g>,<g>A</g>,P)</frm>) </klr>
      <ref tip="super" cel="probab.0o.mezuro"><tld lit="P"/>o</ref>
      <frm am="P_(bb a)">P<sub><g>a</g></sub></frm>, difinita per
      <frm am="P_(bb a)(bb b)=P(bb a nn bb b)//P(bb a)">P<sub><g>a</g></sub>(<g>b</g>) =
      P(<g>a</g>&#x2229;<g>b</g>)/P(<g>a</g>)</frm>;
      pli ofte: la bildo per la koncerna mezuro de iu okazo
      <frm am="bb b"><g>b</g></frm>:
      <ekz>
        la kondi&ccirc;an <tld/>on <frm am="P_(bb a)(bb b)">P<sub><g>a</g></sub>(<g>b</g>)</frm>
        oni anka&ubreve; signas per <frm am="P(bb b|bb a)">P(<g>b</g>|<g>a</g>)</frm>
        <klr> (legu: <tld/>o de bo, sciante (ke okazis) a)</klr>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_m;&c_a;&c_v;&c_je;&c_r;&c_n;&c_a;&c_s;&c_mol;&c_c;&c_mol; &c_u;&c_m;&c_o;&c_w;&c_n;&c_a;&c_ja;</trd>
  <trd lng="de">bedingte <ind>Wahrscheinlichkeit</ind></trd>
  <trd lng="en">conditional <ind>probability</ind></trd>
  <trd lng="es">probabilidad condicional</trd>
  <trd lng="fr">probabilit&eacute; conditionnelle</trd>
  <trd lng="hu">felt&eacute;teles <ind>val&oacute;sz&iacute;n&udblac;s&eacute;g</ind></trd>
  <trd lng="pl">prawdopodobie&nacute;stwo warunkowe</trd>
  <trd lng="ru">&c_u;&c_s;&c_l;&c_o;&c_v;&c_n;&c_a;&c_ja; <ind>&c_v;&c_je;&c_r;&c_o;&c_ja;&c_t;&c_n;&c_o;&c_s;&c_t;&c_mol;</ind></trd>
  <trdgrp lng="zh">
    <trd>&#x6761;&#x4EF6;&#x6982;&#x7387; <pr>ti&#xE1;oji&#xE0;ng&#xE0;il&#xF9;:</pr></trd>,
    <trd>&#x689D;&#x4EF6;&#x6982;&#x7387; <pr>ti&#xE1;oji&#xE0;ng&#xE0;il&#xF9;:</pr></trd>,
    <trd>&#x6761;&#x4EF6;&#x6982;&#x7387; <pr>ti&#xE1;oji&#xE0;ng&#xE0;il&#x1DC;</pr></trd>,
    <trd>&#x689D;&#x4EF6;&#x6982;&#x7387; <pr>ti&#xE1;oji&#xE0;ng&#xE0;il&#x1DC;</pr></trd>,
    <trd>&#x6761;&#x4EF6;&#x673A;&#x7387; <pr>ti&#xE1;oji&#xE0;nj&#x12B;l&#x1DC;</pr></trd>,
    <trd>&#x689D;&#x4EF6;&#x6A5F;&#x7387; <pr>ti&#xE1;oji&#xE0;nj&#x12B;l&#x1DC;</pr></trd>
  </trdgrp>
</drv>
