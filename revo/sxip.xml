<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ip</rad>/o
</kap>
<drv mrk="sxip.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      &Ccirc;iaspeca veturilo por veturi sur akvo:
      <ekz>
        vel<tld/>o, komerc<tld/>o, milit<tld/>o, kiras<tld/>o;
      </ekz>
      <ekz>
        <tld/>on rompitan &ccirc;iuj ventoj atakas
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="jahxt.0o">ja&hcirc;to</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="pru.0o">pruo</ref>,
      <ref cel="pouxp.0o">po&ubreve;po</ref>,
      <ref cel="kil.0o">kilo</ref>.
    </refgrp>
  </snc>
  <trd lng="cs">lo&dcaron;</trd>
  <trd lng="de">Schiff</trd>
  <trd lng="en">ship</trd>
  <trdgrp lng="fr">
    <trd>navire</trd>,
    <trd>bateau</trd>
  </trdgrp>
  <trd lng="nl">schip</trd>
  <trd lng="pl">statek</trd>
  <trdgrp lng="ru">
    <trd>&c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_mol;</trd>,
    <trd>&c_s;&c_u;&c_d;&c_n;&c_o;</trd>
  </trdgrp>
  <trd lng="tr">gemi</trd>
</drv>

<drv mrk="sxip.0i">
  <kap><tld/>i<fnt>Z</fnt></kap>
  <uzo tip="fak">MAR</uzo>
  <snc mrk="sxip.0i.ntr">
    <gra><vspec>ntr</vspec></gra>
    <ref tip="dif" cel="ir.sxip0i">&scirc;ipiri.</ref>
  </snc>
  <snc mrk="sxip.0i.tr">
    <gra><vspec>tr</vspec></gra>
    <dif>
      Transporti per <tld/>o:
      <ekz>
        <tld/>adi brullignon<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
</drv>

<drv mrk="sxip.0ano">
  <kap><tld/>ano</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Homo, kies metio estas servi sur <tld/>oj:
      <ekz>
        la <tld/>anoj devas obei la <tld/>estron<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Matrose</trd>,
    <trd>Seefahrer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>marin</trd>,
    <trd>matelot</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_m;&c_o;&c_r;&c_ja;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_t;&c_r;&c_o;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="sxip.0aro">
  <kap><tld/>aro</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Aro da <tld/>oj kolekti&gcirc;intaj por kune veturi.
    </dif>
  </snc>
  <snc>
    <dif>
      Organizita grupo da diversspecaj milit<tld/>oj, estrita de admiralo.
    </dif>
  </snc>
  <snc>
    <dif>
      Tuto de la komerc- a&ubreve; milit<tld/>oj apartenantaj al iu regno.
    </dif>
  </snc>
  <trd lng="de">Flotte</trd>
  <trd lng="en">fleet</trd>
  <trd lng="fr">flotte<klr> (ensemble de navires)</klr></trd>
  <trd lng="nl">vloot</trd>
  <trd lng="ru">&c_f;&c_l;&c_o;&c_t;</trd>
</drv>

<drv mrk="sxip.0eto">
  <kap><tld/>eto</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Malgranda kunferdeka <tld/>o
    </dif>
    <refgrp tip="vid">
      <ref cel="bark.0o">barko</ref>,
      <ref cel="boat.0o">boato</ref>,
      <ref cel="sxalup.0o">&scirc;alupo</ref>.
    </refgrp>
  </snc>
</drv>

<drv mrk="sxip.el0ejo">
  <kap>el<tld/>ejo</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Loko aran&gcirc;ita por faciligi la surteri&gcirc;on de la
<tld/>veturantoj;
      kajo.
    </dif>
  </snc>
  <trd lng="de">Anlegestelle</trd>
  <trd lng="fr">d&eacute;barcad&egrave;re</trd>
</drv>

<drv mrk="sxip.0konstruejo">
  <kap><tld/>konstruejo</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      Loko por konstrui &scirc;ipojn.
    </dif>
  </snc>
  <trd lng="de">Werft</trd>
  <trd lng="en">shipyard</trd>
</drv>

<drv mrk="sxip.kroz0o">
  <kap>kroz<tld/>o</kap>
  <snc>
    <dif>
      Speciala rapida milit<tld/>o por <tld/>i.
    </dif>
  </snc>
  <refgrp tip="sin">
    <ref cel="kroz.0anto">krozanto</ref>,
    <ref cel="kroz.0isto">krozisto</ref>
  </refgrp>
  <trd lng="de">Kreuzer</trd>
  <trd lng="en">crusier</trd>
  <trd lng="ru">&c_k;&c_r;&c_je;&c_j;&c_s;&c_je;&c_r;</trd>
</drv>

<drv mrk="sxip.vel0o">
  <kap>vel<tld/>o</kap>
  <uzo tip="fak">MAR</uzo>
  <snc>
    <dif>
      <tld/>o movata fare de venta premo en veloj.
    </dif>
  </snc>
  <trd lng="de">Segelschiff</trd>
  <trdgrp lng="es">
    <trd>velero</trd>,
    <trd>barco de vela</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>bateau &agrave; <ind>voile</ind></trd>,
    <trd>voilier</trd>
  </trdgrp>
  <trd lng="nl">zeilschip</trd>
  <trd lng="ru">&c_p;&c_a;&c_r;&c_u;&c_s;&c_n;&c_i;&c_k;</trd>
</drv>
</art>
