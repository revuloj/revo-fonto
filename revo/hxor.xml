<drv mrk="hxor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="hxor.0o.kantistaro">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="grup.0o.personoj">Grupo</ref> da homoj, unusone a&ubreve; plurvo&ccirc;e kantantaj:
      <ekz>
        virina, knaba <tld/>o;
      </ekz>
      <ekz>
        la miksita <tld/>o alkantadis a&ubreve; respondadis &ccirc;e la destinitaj lokoj
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        la orgeno ludis, kaj la <tld/>o de la infanaj vo&ccirc;oj sonis milde kaj &ccirc;arme
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi starigis du grandajn <tld/>ojn kaj procesiojn
        <fnt>
          <bib>MT</bib>
          <lok>Ne&hcirc;emja 12:31</lok>
        </fnt>;
      </ekz>
      <ekz>
        ektondris ie alte, de sub la &ccirc;ielo, kvaza&ubreve; e&hcirc;o de an&gcirc;ela <tld/>o
        <fnt>
          <bib>Lasta</bib>
          <lok>La Lasta</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        neniam anta&ubreve;e &scirc;i a&ubreve;dis tian ravan <tld/>on de birdoj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro XVII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        senkura&gcirc;aj vo&ccirc;oj perdi&gcirc;as en la la&ubreve;tega <tld/>o de la kriistoj kaj mokistoj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        milvo&ccirc;a <tld/>o de susurantaj kaj trumpetantaj a&ubreve;toj kantadas sian himnon al la
        veki&gcirc;o de nova tago
        <fnt>
          <bib>Tor</bib>
          <lok>VI.</lok>
        </fnt>;
      </ekz>
      <ekz>
        membroj de mia <tld/>o
        <fnt>
          <aut>David Curtis</aut>
          <vrk><url ref="&Monato;/2002/pdf/monato200201.pdf#page=28">Koratako</url></vrk>
          <bib>Monato</bib>
          <lok>2002/01, p. 28-34</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="korus.0o">koruso</ref>
    </refgrp>
  </snc>
  <snc mrk="hxor.0o.MUZverko">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="dif" cel="hxor.0ajxo">&Hcirc;ora&jcirc;o</ref>:
      <ekz>
        sinjoro kapelestro, mi devas diri al vi, ke la <tld/>oj estis hodia&ubreve; ekstreme
        malbonaj
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <nom>Beethoven</nom> enigis &scirc;an&gcirc;on &ndash; en lia 9-a simfonio sonas <tld/>o
        <fnt>
          <bib>KFl</bib>
          <lok>III. Atomoj de evoluo</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <tld/>o de vo&ccirc;oj subtenis lian vortojn
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro VII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &ccirc;iuj gratulas per brua <tld/>o
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="hxor.0o.TEA">
    <uzo tip="fak">TEA</uzo>
    <dif>
      En la antikvaj teatra&jcirc;oj, kolektiva <ref tip="super" cel="rol.0ulo">rolulo</ref> kiu prezentas la
      publikan opinion a&ubreve; emocion:
      <ekz>
        <tld/>oj de nimfoj;
      </ekz>
      <ekz>
        <tld/>o de pastrinoj plenumis solenan dancon
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj estis necesa parto de la rolantaro en la klasikaj dramoj
        <fnt>
          <bib>Rsp</bib>
          <lok>Parto Du</lok>
        </fnt>;
      </ekz>
      <ekz>
        vi prenis sur vin la rolon de la antikva <tld/>o
        <fnt><aut>&Z;</aut> <lok>citita la&ubreve; PIV2, p. 455</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_o;&c_r;</trd>
  <trd lng="ca">cor<klr tip="ind"> (m&uacute;s)</klr></trd>
  <trd lng="bg">&c_h;&c_o;&c_r;</trd>
  <trd lng="de">Chor</trd>
  <trdgrp lng="en">
    <trd>choir</trd>,
    <trd>chorus</trd>
  </trdgrp>
  <trd lng="es">coro</trd>
  <trd lng="fi">kuoro</trd>
  <trd lng="fr">ch&oelig;ur<klr tip="ind"> (troupe)</klr></trd>
  <trd lng="he">&mem;<ind>&qof;&he;&lamed;</ind>&he;</trd>
  <trd lng="hu">k&oacute;rus</trd>
  <trd lng="id">koor</trd>
  <trdgrp lng="ja">
    <trd>&#21512;&#21809;&#22243;  <pr>&#12364;&#12387;&#12375;&#12423;&#12358;&#12384;&#12435;</pr></trd>,
    <trd>&#21512;&#21809;&#38538;  <pr>&#12364;&#12387;&#12375;&#12423;&#12358;&#12383;&#12356;</pr></trd>,
    <trd>&#21512;&#21809;  <pr>&#12364;&#12387;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#21512;&#21809;&#26354;  <pr>&#12364;&#12387;&#12375;&#12423;&#12358;&#12365;&#12423;&#12367;</pr></trd>,
    <trd>&#12467;&#12525;&#12473;</trd>
  </trdgrp>
  <trd lng="nl">koor</trd>
  <trd lng="pl">ch&oacute;r</trd>
  <trd lng="pt">coro<klr tip="amb"> (m&uacute;sica)</klr></trd>
  <trd lng="ru">&c_h;&c_o;&c_r;</trd>
  <trd lng="sv">k&ouml;r<klr tip="ind"> (mus.)</klr></trd>
  <trd lng="it">coro</trd>
  <trd lng="uk">&c_h;&c_o;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#21512;&#21809;&#22242; <pr>h&eacute;ch&agrave;ngtu&aacute;n</pr></trd>,
    <trd>&#21512;&#21809; <pr>h&eacute;ch&agrave;ng</pr></trd>,
    <trd>&#21809;&#35433;&#29677; <pr>ch&agrave;ngsh&imacron;b&amacron;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hxor.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="hxor.0ajxo.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="muzik.0ajxo">Muzika&jcirc;o</ref> verkita
      por esti kantata de <tld/>o:
      <ekz>
        la <ind>enira <tld/>a&jcirc;o</ind> de la Mateo-Pasiono
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=250148"
              >Pedaltono</url></lok>
        </fnt>.
        <trd lng="de">Eingangschor</trd>
      </ekz>
    </dif>
    <ref tip="sub" cel="hxoral.0o">&hcirc;oralo</ref>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_r;&c_a;&c_l;</trd>
  <trd lng="ca">coral<klr tip="ind"> (composici&oacute;)</klr></trd>
  <trd lng="de">Chor <klr>(St&uuml;ck)</klr></trd>
  <trdgrp lng="en">
    <trd>choral</trd>,
    <trd>carol</trd>
  </trdgrp>
  <trdgrp lng="fi">
    <trd>kuoroteos</trd>,
    <trd>kuoros&auml;vellys</trd>
  </trdgrp>
  <trd lng="fr">ch&oelig;ur<klr tip="ind">
    (pi&egrave;ce musicale)</klr></trd>
  <trd lng="hu">k&oacute;rusm&udblac;</trd>
  <trd lng="ja">&#21512;&#21809;&#26354;  <pr>&#12364;&#12387;&#12375;&#12423;&#12358;&#12365;&#12423;&#12367;</pr></trd>
  <trd lng="nl">koorlied</trd>
  <trd lng="pl">ch&oacute;ra&lstroke;</trd>
  <trd lng="pt">coro
    <klr tip="amb"> (composi&ccedil;&atilde;o musical)</klr></trd>
   <trd lng="it">corale <klr tip="ind"> (componimento)</klr></trd>
  <trd lng="uk">&c_h;&c_o;&c_r;&c_o;&c_v;&c_i;&c_j; &c_t;&c_v;&c_ib;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#38924;&#27468; <pr>s&ograve;ngg&emacron;</pr></trd>,
    <trd>&#20247;&#36190;&#27468; <pr>zh&ograve;ngz&agrave;ng&emacron;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hxor.0ano">
  <kap><tld/>ano</kap>
  <snc mrk="hxor.0ano.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="kant.0isto.profesio">Kantisto</ref>, kiu
      partoprenas en <tld/>o:
      <ekz>
        la direktanto, alestante &ccirc;e la provo de la <tld/>anoj, diras: &mdash; vi kriegis
        hiera&ubreve; kiel brutaro
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="korus.0ano">korusano</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_r;&c_y;&c_s;&c_t;</trd>
  <trd lng="ca">corista</trd>
  <trd lng="de">Chors&auml;nger</trd>
  <trdgrp lng="en">
    <trd>choir singer</trd>,
    <trd>choirboy</trd>,
    <trd>choirgirl</trd>
  </trdgrp>
  <trd lng="es">corista</trd>
  <trd lng="fi">kuorolainen</trd>
  <trdgrp lng="fr">
    <trd>choriste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kar&eacute;nekes</trd>,
    <trd>k&oacute;rustag</trd>,
    <trd>k&oacute;rista</trd>
  </trdgrp>
  <trd lng="pl">ch&oacute;rzysta</trd>
  <trd lng="pt">corista</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_je;&c_v;&c_ch;&c_i;&c_j;
    <klr tip="amb"> (en pre&gcirc;ejo)</klr></trd>
  </trdgrp>
  <trd lng="sv">k&ouml;rs&aring;ngare</trd>
  <trd lng="it">corista</trd>
  <trd lng="uk">&c_h;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#27468;&#21809;&#23478; <pr>g&emacron;ch&agrave;ngji&amacron;</pr></trd>,
    <trd>&#27468;&#25163; <pr>g&emacron;sh&#466;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hxor.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="hxor.0ejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Loko de <ref tip="malprt" cel="pregx.0ejo">pre&gcirc;ejo</ref> kie kantas
      ordinare la pre&gcirc;eja <tld/>o:
      <ekz>
        trans la sojlon de la pre&gcirc;ejo &scirc;i eniris al la <tld/>ejo
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        restis nur en la <tld/>ejo la paro&hcirc;estro, kiu sin senvestigis kaj la filo de
        Valentino, kiu da&ubreve;rigis sian nevekeblan dormadon
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekunua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ejo, &scirc;tuparoj, partero, amfiteatro, muroj, &ccirc;io, kio estis kun mi, super
        mi kaj sub mi, &ccirc;io ektondris en potenca kriego
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        en malnovaj kasteloj oni starigis sur <tld/>ejo orgenon
        <fnt>
          <bib>SrT</bib>
          <lok>Libro II.</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      <fnt><bib>PV</bib></fnt>, la&ubreve; la franca kutimo, lokas <tld/>ejon en la
      <ref cel="altar.0ejo">altarejon</ref> (<ctl>malanta&ubreve; la altaro</ctl>).
      Tio ne &ccirc;iam &gcirc;ustas e&ccirc; por la romkatolikaj kirkoj (kie la <tld/>ejo
      povas esti, ekz-e, en la mezo de la navo), kaj &ccirc;iam mal&gcirc;ustas por la
      ortodoksaj kirkoj, kie la <tld/>ejo nepre estas ekster la altarejo
      (anta&ubreve; la ikonostazo kaj en okcidenta galerio).  Cetere,
      <tld/>ejon povas havi anka&ubreve; senaltaraj pre&gcirc;ejoj (kapeloj).
      <aut>Sergio Pokrovskij</aut>
    </rim>
    </snc>
    <trd lng="be">&c_h;&c_o;&c_r;&c_y; <klr>(&c_g;&c_a;&c_l;&c_je;&c_r;&c_e;&c_ja; &c_c;&c_a;&c_r;&c_k;&c_v;&c_y;)</klr></trd>
    <trd lng="ca">cor
      <klr tip="ind"> (d'una esgl&eacute;sia)</klr></trd>
    <trd lng="de">Chor <klr>(Kirchenraum)</klr></trd>
    <trdgrp lng="en">
      <trd>choir loft</trd>
    </trdgrp>
    <trd lng="es">coro <klr>(lugar en una iglesia)</klr></trd>
    <trd lng="fi">kuoron paikka <klr tip="amb">
    (kirkossa)</klr></trd>
    <trd lng="fr">ch&oelig;ur
      <klr tip="ind"> (d'une &eacute;glise)</klr></trd>
    <trd lng="hu">k&oacute;rus
      <klr tip="ind"> (olt&aacute;r m&ouml;g&ouml;tt)</klr></trd>
    <trd lng="ja">&#32854;&#27468;&#38538;&#24109;  <pr>&#12379;&#12356;&#12363;&#12383;&#12356;&#12379;&#12365;</pr></trd>
  <trd lng="nl">koor<klr tip="ind"> (deel v.kerk)</klr></trd>
    <trd lng="pl">ch&oacute;r</trd>
    <trd lng="pt">coro<klr tip="amb"> (galeria)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_l;&c_i;&c_r;&c_o;&c_s;<klr tip="amb">
        (anta&ubreve;altareja)</klr></trd>;
      <trd>&c_h;&c_o;&c_r;&c_y;<klr tip="amb"> (galerio)</klr></trd>
    </trdgrp>
    <trd lng="it">coro
      <klr tip="ind"> (parte d'una chiesa)</klr></trd>
    <trd lng="uk">&c_h;&c_o;&c_r;&c_i;</trd>
</drv>

<drv mrk="hxor.0estro">
  <kap><tld/>estro
    <fnt><bib>MT</bib> <lok>Psalmaro 4</lok></fnt>
  </kap>
  <snc mrk="hxor.0estro.MUZ">
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="muzik.0isto">Muzikisto</ref>, kiu trejnas kaj
      gvidas <tld/>on:
      <ekz>
        citristoj kaj atenaj kantistoj atendis la signon de sia <tld/>estro
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsanis la <tld/>estro, kaj la ekzercado ne okazis
        <fnt>
          <bib>ChB</bib>
          <lok>23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_o;&c_r;&c_m;&c_a;&c_j;&c_s;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="en">
    <trd>choirmaster</trd>,
    <trd>choir director</trd>
  </trdgrp>
  <trd lng="fi">kuoronjohtaja</trd>
  <trdgrp lng="fr">
    <trd>ma&icirc;tre de chapelle</trd>,
    <trd>chef de ch&oelig;ur</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#21512;&#21809;&#22243;&#38263;  <pr>&#12364;&#12387;&#12375;&#12423;&#12358;&#12384;&#12435;&#12385;&#12423;&#12358;</pr></trd>,
    <trd>&#32854;&#27468;&#38538;&#25351;&#25582;&#32773;  <pr>&#12379;&#12356;&#12363;&#12383;&#12356;&#12375;&#12365;&#12375;&#12419;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>koordirigent</trd>,
    <trd>dirigent<klr tip="ind"> (v.koor)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>karnagy</trd>,
    <trd>k&oacute;rusvezet&odblac;</trd>
  </trdgrp>
  <trd lng="de">Chorleiter</trd>
  <trd lng="pl">dyrygent</trd>
  <trd lng="pt">regente de <ind>coro</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_m;&c_je;&c_j;&c_s;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_r;&c_je;&c_g;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
 <trdgrp lng="it">
    <trd>direttore del coro</trd>,
    <trd>maestro del coro</trd>,
    <trd>maestro di cappella</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_h;&c_o;&c_r;&c_m;&c_je;&c_j;&c_s;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_r;&c_je;&c_gu;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="zh">&#38936;&#21809; <pr>l&#464;ngch&agrave;ng</pr></trd>
</drv>

<drv mrk="hxor.0isto">
  <kap>
    <tld/>isto
  </kap>
  <snc mrk="hxor.0isto.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Profesia <tld/>ano:
      <ekz>
        <tld/>istoj kune kantis 4-vo&ccirc;ajn melodiojn
        <fnt>
          <bib>Kon11</bib>
          <lok>Gr&eacute;sillon, nobela kastelo por nobla lingvo</lok>
        </fnt>;
      </ekz>
      <ekz>
        temas ne pri <tld/>istino, sed pri virino kiu dancas duonnude kaj burleske
        <fnt>
          <bib>LOdE</bib>
          <lok>2001, &#8470;12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Chors&auml;nger</trd>
</drv>

