<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>&hcirc;or</rad>/o
</kap>
<drv mrk="hxor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <uzo tip="fak">TEA</uzo>
  <snc>
    <dif>
      Grupo de homoj, kiuj akorde, samtakte dancas,
      kantas a&ubreve; parolas:
      <ekz>
        <tld/>oj de nimfoj;
      </ekz>
      <ekz>
        la <tld/>oj de la opero, de la greka tragedio;
      </ekz>
      <ekz>
        <tld/>aj popolaj societoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Chor</trd>
  <trd lng="en">choir</trd>
  <trd lng="es">coro</trd>
  <trd lng="fr">ch&oelig;ur<klr tip="ind"> (groupe de chanteurs)</klr></trd>
  <trd lng="hu">k&oacute;rus</trd>
  <trd lng="nl">koor</trd>
  <trd lng="ru">&c_h;&c_o;&c_r;</trd>
  <trd lng="sv">k&ouml;r<klr tip="ind"> (mus.)</klr></trd>
</drv>

<drv mrk="hxor.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="muzik.0ajxo">Muzika&jcirc;o</ref> verkita por esti
      kantata de <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Choral</trd>
  <trd lng="fr">ch&oelig;ur<klr tip="ind"> (pi&egrave;ce musicale)</klr></trd>
  <trd lng="hu">k&oacute;rusm&udblac;</trd>
  <trd lng="nl">koorlied</trd>
</drv>

<drv mrk="hxor.0ano">
  <kap><tld/>ano</kap>
  <snc>
    <dif>
      <ref tip="super" cel="kant.0isto.profesio">Kantisto</ref>,
      kiu partoprenas en <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>choriste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kar&eacute;nekes</trd>,
    <trd>k&oacute;rustag</trd>,
    <trd>k&oacute;rista</trd>
  </trdgrp>

</drv>

<drv mrk="hxor.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Loko de <ref tip="malprt" cel="pregx.0ejo">pre&gcirc;ejo</ref>
      malanta&ubreve; la altaro, kie kantas ordinare la pre&gcirc;eja <tld/>o.
    </dif>
  </snc>
  <trd lng="de">Chor</trd>
  <trd lng="fr">ch&oelig;ur<klr tip="ind"> (d'une &eacute;glise)</klr></trd>
  <trd lng="hu">k&oacute;rus<klr tip="ind"> (olt&aacute;r m&ouml;g&ouml;tt)</klr></trd>
  <trd lng="nl">koor<klr tip="ind"> (deel v.kerk)</klr></trd>
</drv>

<drv mrk="hxor.0estro">
  <kap><tld/>estro</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="muzik.0isto">Muzikisto</ref>,
      kiu trejnas kaj gvidas <tld/>on.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>maitre de chapelle</trd>,
    <trd>chef de choeur</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>koordirigent</trd>,
    <trd>dirigent<klr tip="ind"> (v.koor)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>karnagy</trd>,
    <trd>k&oacute;rusvezet&odblac;</trd>
  </trdgrp>
  <trd lng="de">Chorleiter</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_m;&c_je;&c_j;&c_s;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_r;&c_je;&c_g;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
</art>
