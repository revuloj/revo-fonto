<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>&hcirc;or</rad>/o
</kap>
<drv mrk="hxor.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Aro da personoj, kiuj akorde kaj samtakte dancas, movas la membrojn,
      kantas a&ubreve; parolas:
      <ekz>
        <tld/>oj de nimfoj;
      </ekz>
      <ekz>
        la <tld/>oj de la opero, de la greka tragedio;
      </ekz>
      <ekz>
        <tld/>aj popolaj societoj.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Chor</trd>
  <trd lng="en">choir</trd>
  <trd lng="fr">ch&oelig;ur<klr tip="ind"> (groupe de chanteurs)</klr></trd>
  <trd lng="nl">koor</trd>
  <trd lng="ru">&c_h;&c_o;&c_r;</trd>
  <trd lng="sv">k&ouml;r<klr tip="ind"> (mus.)</klr></trd>
</drv>

<drv mrk="hxor.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Kanto speciale verkita por esti kantata de <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">ch&oelig;ur<klr tip="ind"> (pi&egrave;ce musicale)</klr></trd>
  <trd lng="nl">koorlied</trd>
</drv>

<drv mrk="hxor.0ejo">
  <kap><tld/>ejo</kap>
  <uzo tip="fak">ARKI</uzo>
  <snc>
    <dif>
      Loko de la pre&gcirc;ejo, kie kantas ordinare la pre&gcirc;eja <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">ch&oelig;ur<klr tip="ind"> (d'une &eacute;glise)</klr></trd>
  <trd lng="nl">koor<klr tip="ind"> (deel v.kerk)</klr></trd>
</drv>

<drv mrk="hxor.0estro">
  <kap><tld/>estro</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Gvidanto kaj direktanto de <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">directeur de ch&oelig;ur</trd>
  <trdgrp lng="nl">
    <trd>koordirigent</trd>,
    <trd>dirigent<klr tip="ind"> (v.koor)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_m;&c_je;&c_j;&c_s;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_r;&c_je;&c_g;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
</drv>
</art>
