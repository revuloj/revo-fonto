<drv mrk="hxor.0ano">
  <kap><tld/>ano</kap>
  <snc>
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="kant.0isto.profesio">Kantisto</ref>, kiu
      partoprenas en <tld/>o:
      <ekz>
        la direktanto, alestante &ccirc;e la provo de la <tld/>anoj, diras: &mdash; vi kriegis
        hiera&ubreve; kiel brutaro
        <fnt>
          <bib>FK</bib>
          <lok>Fundamenta Krestomatio de la lingvo Esperanto</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="korus.0ano">korusano</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_h;&c_a;&c_r;&c_y;&c_s;&c_t;</trd>
  <trd lng="ca">corista</trd>
  <trd lng="de">Chors&auml;nger</trd>
  <trdgrp lng="en">
    <trd>choir singer</trd>,
    <trd>choirboy</trd>,
    <trd>choirgirl</trd>
  </trdgrp>
  <trd lng="es">corista</trd>
  <trd lng="fi">kuorolainen</trd>
  <trdgrp lng="fr">
    <trd>choriste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>kar&eacute;nekes</trd>,
    <trd>k&oacute;rustag</trd>,
    <trd>k&oacute;rista</trd>
  </trdgrp>
  <trd lng="pl">ch&oacute;rzysta</trd>
  <trd lng="pt">corista</trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>,
    <trd>&c_p;&c_je;&c_v;&c_ch;&c_i;&c_j;
    <klr tip="amb"> (en pre&gcirc;ejo)</klr></trd>
  </trdgrp>
  <trd lng="sv">k&ouml;rs&aring;ngare</trd>
  <trd lng="it">corista</trd>
  <trd lng="uk">&c_h;&c_o;&c_r;&c_i;&c_s;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x6B4C;&#x5531;&#x5BB6; <pr>g&#x113;ch&#xE0;ngji&#x101;</pr></trd>,
    <trd>&#x6B4C;&#x624B; <pr>g&#x113;sh&#x1D2;u</pr></trd>
  </trdgrp>
</drv>

<drv mrk="hxor.0ejo">
  <kap><tld/>ejo</kap>
  <snc mrk="hxor.0ejo.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <uzo tip="fak">KRI</uzo>
    <dif>
      Loko de <ref tip="malprt" cel="pregx.0ejo">pre&gcirc;ejo</ref> kie kantas 
      ordinare la pre&gcirc;eja <tld/>o:
      <ekz>
        trans la sojlon de la pre&gcirc;ejo &scirc;i eniris al la <tld/>ejo
        <fnt>
          <bib>Fab2</bib>
          <lok>Ru&gcirc;aj &scirc;uoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        restis nur en la <tld/>ejo la paro&hcirc;estro, kiu sin senvestigis kaj la filo de
        Valentino, kiu da&ubreve;rigis sian nevekeblan dormadon
        <fnt>
          <bib>KPr</bib>
          <lok>&Ccirc;apitro Dekunua</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>ejo, &scirc;tuparoj, partero, amfiteatro, muroj, &ccirc;io, kio estis kun mi, super
        mi kaj sub mi, &ccirc;io ektondris en potenca kriego
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        en malnovaj kasteloj oni starigis sur <tld/>ejo orgenon
        <fnt>
          <bib>SrT</bib>
          <lok>Libro II.</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      <fnt><bib>PV</bib></fnt>, la&ubreve; la franca kutimo, lokas <tld/>ejon en la 
      <ref cel="altar.0ejo">altarejon</ref> (<ctl>malanta&ubreve; la altaro</ctl>).  
      Tio ne &ccirc;iam &gcirc;ustas e&ccirc; por la romkatolikaj kirkoj (kie la <tld/>ejo 
      povas esti, ekz-e, en la mezo de la navo), kaj &ccirc;iam mal&gcirc;ustas por la 
      ortodoksaj kirkoj, kie la <tld/>ejo nepre estas ekster la altarejo 
      (anta&ubreve; la ikonostazo kaj en okcidenta galerio).  Cetere, 
      <tld/>ejon povas havi anka&ubreve; senaltaraj pre&gcirc;ejoj (kapeloj).
      <aut>Sergio Pokrovskij</aut>
    </rim>
    </snc>
    <trd lng="be">&c_h;&c_o;&c_r;&c_y; <klr>(&c_g;&c_a;&c_l;&c_je;&c_r;&c_e;&c_ja; &c_c;&c_a;&c_r;&c_k;&c_v;&c_y;)</klr></trd>
    <trd lng="ca">cor
      <klr tip="ind"> (d'una esgl&eacute;sia)</klr></trd>
    <trd lng="de">Chor</trd>
    <trdgrp lng="en">
      <trd>choir loft</trd>
    </trdgrp>
    <trd lng="es">coro <klr>(lugar en una iglesia)</klr></trd>
    <trd lng="fi">kuoron paikka <klr tip="amb">
    (kirkossa)</klr></trd>
    <trd lng="fr">ch&oelig;ur
      <klr tip="ind"> (d'une &eacute;glise)</klr></trd>
    <trd lng="hu">k&oacute;rus
      <klr tip="ind"> (olt&aacute;r m&ouml;g&ouml;tt)</klr></trd>
    <trd lng="ja">&#x8056;&#x6B4C;&#x968A;&#x5E2D;  <pr>&#x305B;&#x3044;&#x304B;&#x305F;&#x3044;&#x305B;&#x304D;</pr></trd>
  <trd lng="nl">koor<klr tip="ind"> (deel v.kerk)</klr></trd>
    <trd lng="pl">ch&oacute;r</trd>
    <trd lng="pt">coro<klr tip="amb"> (galeria)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_k;&c_l;&c_i;&c_r;&c_o;&c_s;<klr tip="amb"> 
        (anta&ubreve;altareja)</klr></trd>;
      <trd>&c_h;&c_o;&c_r;&c_y;<klr tip="amb"> (galerio)</klr></trd>
    </trdgrp>
    <trd lng="it">coro
      <klr tip="ind"> (parte d'una chiesa)</klr></trd>
    <trd lng="uk">&c_h;&c_o;&c_r;&c_i;</trd>
</drv>

<drv mrk="hxor.0estro">
  <kap><tld/>estro
    <fnt><bib>MT</bib> <lok>Psalmaro 4</lok></fnt>
  </kap>
  <snc>
  <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="super" cel="muzik.0isto">Muzikisto</ref>, kiu trejnas kaj
      gvidas <tld/>on:
      <ekz>
        citristoj kaj atenaj kantistoj atendis la signon de sia <tld/>estro
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro LXXIV</lok>
        </fnt>;
      </ekz>
      <ekz>
        malsanis la <tld/>estro, kaj la ekzercado ne okazis
        <fnt>
          <bib>ChB</bib>
          <lok>23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_h;&c_o;&c_r;&c_m;&c_a;&c_j;&c_s;&c_t;&c_a;&c_r;</trd>
  <trdgrp lng="en">
    <trd>choirmaster</trd>,
    <trd>choir director</trd>
  </trdgrp>
  <trd lng="fi">kuoronjohtaja</trd>
  <trdgrp lng="fr">
    <trd>ma&icirc;tre de chapelle</trd>,
    <trd>chef de ch&oelig;ur</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5408;&#x5531;&#x56E3;&#x9577;  <pr>&#x304C;&#x3063;&#x3057;&#x3087;&#x3046;&#x3060;&#x3093;&#x3061;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x8056;&#x6B4C;&#x968A;&#x6307;&#x63EE;&#x8005;  <pr>&#x305B;&#x3044;&#x304B;&#x305F;&#x3044;&#x3057;&#x304D;&#x3057;&#x3083;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>koordirigent</trd>,
    <trd>dirigent<klr tip="ind"> (v.koor)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>karnagy</trd>,
    <trd>k&oacute;rusvezet&odblac;</trd>
  </trdgrp>
  <trd lng="de">Chorleiter</trd>
  <trd lng="pl">dyrygent</trd>
  <trd lng="pt">regente de <ind>coro</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_h;&c_o;&c_r;&c_m;&c_je;&c_j;&c_s;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_r;&c_je;&c_g;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
 <trdgrp lng="it">
    <trd>direttore del coro</trd>,
    <trd>maestro del coro</trd>,
    <trd>maestro di cappella</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_h;&c_o;&c_r;&c_m;&c_je;&c_j;&c_s;&c_t;&c_je;&c_r;</trd>,
    <trd>&c_r;&c_je;&c_gu;&c_je;&c_n;&c_t;</trd>
  </trdgrp>
  <trd lng="zh">&#x9818;&#x5531; <pr>l&#x1D0;ngch&#xE0;ng</pr></trd>
</drv>
