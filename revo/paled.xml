<kap>
  <rad>paled</rad>/o
</kap>
<drv mrk="paled.0o">
  <kap><tld/>o</kap>
  <fnt><bib>KrDE</bib></fnt>
  <fnt><bib>KrED</bib></fnt>
  <fnt><bib>TK</bib></fnt>
  <snc mrk="paled.0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      Plata framo, plej ofte el ligno, por stakado kaj transporto de
      komerca&jcirc;oj, subveturebla per forka &ccirc;arelo:
      <ekz>
        la dimensioj de normitaj <tld/>oj estas 1000 mm x 1200 mm (ISO) 
        kaj 800 mm x 1200 mm (CEN);        
      </ekz>
      <ekz>
        halego el normaj <tld/>oj
        <fnt>
          <aut>R. R&ouml;ssler-Buckel</aut>,
          <vrk>Metio kaj Formo...</vrk>,
          <bib>Monato</bib>,
          <lok>jaro 2007a, numero 6a, p. 19-21a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_d;&c_o;&c_n;</trd>
  <trdgrp lng="cs">
    <trd>manipula&ccaron;n&iacute; paleta</trd>,
    <trd>ploch&aacute; paleta</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Palette</trd>,
    <trd>Transportpalette</trd>
  </trdgrp>
  <trd lng="en">pallet</trd>
  <trd lng="es">pal&eacute;</trd>
  <trd lng="fr">palette</trd>
  <trd lng="hu">raklap</trd>
  <trdgrp lng="ja">
    <trd>&#x30D1;&#x30EC;&#x30C3;&#x30C8;</trd>,
    <trd>&#x8377;&#x904B;&#x3073;&#x53F0;  <pr>&#x306B;&#x306F;&#x3053;&#x3073;&#x3060;&#x3044;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>pallet</trd>,
    <trd>laadbord</trd>
  </trdgrp>
  <trd lng="pl">paleta</trd>
  <trd lng="sk">manipula&ccaron;n&aacute; paleta</trd>
  <trd lng="uk">&#x43F;&c_ib;&c_d;&c_d;&#x43E;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x6258;&#x76D8; <pr>tu&#x14D;p&#xE1;n</pr></trd>,
    <trd>&#x6258;&#x76E4; <pr>tu&#x14D;p&#xE1;n</pr></trd>,
    <trd>&#x6808;&#x677F; <pr>zh&#xE0;nb&#x1CE;n</pr></trd>,
    <trd>&#x68E7;&#x677F; <pr>zh&#xE0;nb&#x1CE;n</pr></trd>,
    <trd>&#x8CA8;&#x76E4; <pr>hu&#xF2;p&#xE1;n</pr></trd>,
    <trd>&#x8D27;&#x76D8; <pr>hu&#xF2;p&#xE1;n</pr></trd>,
    <trd>&#x6258;&#x677F; <pr>tu&#x14D;b&#x1CE;n</pr></trd>,
    <trd>&#x8A17;&#x677F; <pr>tu&#x14D;b&#x1CE;n</pr></trd>
  </trdgrp>
</drv>
