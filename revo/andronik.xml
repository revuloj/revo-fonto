<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: andronik.xml,v 1.1 2020/12/28 22:54:14 afido Exp $">
    <kap>
      <rad>Andronik</rad>/o <fnt><bib>NT</bib></fnt>
    </kap>

<drv mrk="andronik.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">PERS</uzo>
  <snc>
    <dif>
      Greka <ref tip="lst" lst="voko:personaj_nomoj" cel="nom.persona0o.GEN">persona nomo</ref>
      de diversaj viroj en la historio:
      <ekz>
        salutu <tld/>on kaj Junion, miajn parencojn kaj miajn kunkaptitojn
        <fnt>
          <bib>NT</bib>
          <lok>Romanoj 16:7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o la 1-a <klr>[&#8230;]</klr> regis de 1183 &gcirc;is 1185 kiel imperiestro de Bizanco
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=23830"
              >Androniko la 1-a</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o la 2-a, heredonto de Mikaelo la 8-a
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=348914"
              >Dua koncilio de Liono</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Andronikos</trd>
    <trd lng="en">Andronicus</trd>
    <trd lng="fr">Andronicus</trd>
    <trd lng="grc">&Alfa_psili;&ny;&delta;&rho;&omikron_ton;&ny;&jota;&kappa;&omikron;&sigma_fina;</trd>
  </snc>
</drv>

  </art>

