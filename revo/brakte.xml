<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: brakte.xml,v 1.16 2022/03/13 11:53:49 revo Exp $">
<kap>
<rad>brakte</rad>/o
</kap>
<drv mrk="brakte.0o">
  <kap><tld/>o</kap>
  <snc mrk="brakte.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="super" cel="foli.0o.BOT">Folio</ref> en la regiono
      de <ref tip="malprt" cel="inflor.0o">infloresko</ref>, ordinare
      eta, skvamforma, foje kolora kaj kunformanta la floron,
      en kies angulo elkreskas la floroj a&ubreve; flortigoj:
      <ekz>
        <ref tip="sub" cel="kupul.0o">kupulo</ref>
        estas malgranda folieca a&ubreve; skvameca <tld/>o, kiu formas
        la bazon de kelkaj fruktoj, ekzemple &ccirc;e kverkoj kaj karpenoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=19037">Kupulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="malprt" cel="involu.0o">involukro</ref>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_k;&c_v;&c_je;&c_t;&c_a;&c_k;</trd>
  <trd lng="ca">br&agrave;ctea</trd>
  <trdgrp lng="de">
    <trd>Tragblatt</trd>,
    <trd>Deckblatt</trd>,
    <trd>H&uuml;llblatt</trd>
  </trdgrp>
  <trd lng="en">bract</trd>
  <trd lng="es">br&aacute;ctea</trd>
  <trd lng="fr">bract&eacute;e</trd>
  <trdgrp lng="hu">
    <trd>murvalev&eacute;l</trd>,
    <trd>fellev&eacute;l</trd>,
    <trd>braktea</trd>
  </trdgrp>
  <trd lng="nl">schutblad<klr tip="ind"> (v.plant)</klr></trd>
  <trd lng="no">h&oslash;yblad</trd>
  <trd lng="pl">podsadka</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_c;&c_v;&c_je;&c_t;&c_n;&c_i;&c_k;</trd>
  <trd lng="uk">&#x43F;&#x440;&#x438;&#x43A;&#x432;&#x456;&#x442;&#x43E;&#x43A;</trd>
</drv>
</art>
