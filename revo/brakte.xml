<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>brakte</rad>/o
</kap>
<drv mrk="brakte.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="foli.0o.BOT">Folio</ref> en la regiono
      de <ref tip="malprt" cel="inflor.0o">infloresko</ref>, ordinare 
      simpla, skvamforma, foje kolora kaj kunformanta la floron,
      en kies angulo elkreskas la floroj a&ubreve; flortigoj.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Tragblatt</trd>,
    <trd>Deckblatt</trd>,
    <trd>H&uuml;llblatt</trd>
  </trdgrp>
</drv>
</art>

