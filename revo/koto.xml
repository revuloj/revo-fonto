<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>koto</rad>/o <fnt><bib>EncJap</bib></fnt>
</kap>
<drv mrk="koto.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Japana klasika <ref tip="super"
      cel="instru2.kord0o">kordinstrumento</ref>
      konsista el longa sonkesto (190, 25, 4,5 cm) el pa&ubreve;lovnio,
      super kiu 13 silkaj kordoj la&ubreve;longe estas stre&ccirc;itaj sur 13 pontoj,
      ludata per plektroj fiksitaj &ccirc;e la unuaj tri fingroj de la dekstra mano.
    </dif>
    <refgrp tip="vid">
      <ref cel="citr.0o">citro</ref>
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>W&ouml;bbrettzither</trd>,
    <trd>Koto</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>koto</trd>
  </trdgrp>
  <trd lng="ru">&c_k;&c_o;&c_t;&c_o;</trd>
</drv>
</art>
