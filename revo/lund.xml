<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>lund</rad>/o
</kap>
<drv mrk="lund.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc>
    <dif>
      Dua <ref tip="super" cel="tag.0o.diurn">tago</ref> de la semajno,
      inter diman&ccirc;o kaj mardo:
      <ekz>
        hiera&ubreve; estis vendredo, kaj postmorga&ubreve; estos <tld/>o
        <fnt><bib>F</bib>, <lok>&FE; 20</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_n;&c_ja;&c_d;&c_z;&c_je;&c_l;&c_a;&c_k;</trd>
  <trdgrp lng="br">
    <trd>Lun</trd>,
    <trd>dilun</trd>
  </trdgrp>
  <trd lng="cs">pond&ecaron;l&iacute;</trd>
  <trd lng="de">Montag</trd>
  <trd lng="el">&delta;&epsilon;&ypsilon;&tau;&epsilon_ton;&rho;&alfa;</trd>
  <trd lng="en">Monday</trd>
  <trd lng="es">lunes</trd>
  <trd lng="fr">lundi</trd>
  <trd lng="he">&yod;&vav;&fmem; &shin;&nun;&yod;</trd>
  <trd lng="hu">h&eacute;tf&odblac;</trd>
  <trd lng="it">luned&igrave;</trd>
  <trd lng="nl">maandag</trd>
  <trd lng="pl">poniedzia&lstroke;ek</trd>
  <trd lng="pt">segunda-feira</trd>
  <trd lng="ro">luni</trd>
  <trd 
lng="ru">&c_p;&c_o;&c_n;&c_je;&c_d;&c_je;&c_l;&c_mol;&c_n;&c_i;&c_k;</trd>
  <trd lng="tr">pazartesi</trd>
  <trd lng="sv">m&aring;ndag</trd>
  <trd lng="vo">mundel</trd>
</drv>
</art>
