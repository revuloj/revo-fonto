  </trdgrp>
  <trd lng="et">kujutavad</trd>
  <trdgrp lng="fr">
    <trd>attitude physique</trd>,
    <trd>pose</trd>
  </trdgrp>
  <trd lng="fy">pose</trd>
  <trd lng="ga">&uacute;dar</trd>
  <trd lng="gd">nan seasamh</trd>
  <trd lng="gl">representar</trd>
