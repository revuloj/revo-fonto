<drv mrk="poz.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="poz.0i.KOMUNE">
    <dif>
      Preni, plej ofte konscie, specialan teni&gcirc;on:
    </dif>
    <subsnc mrk="poz.0i.BELA">
      <uzo tip="fak">BELA</uzo>
      <dif>
        Preni kaj konservi teni&gcirc;on por esti desegnata, pentrata, fotata:
        <ekz>
          ili <tld/>as la nimfojn.
        </ekz>
      </dif>
      <trd lng="de">posieren</trd>
      <trd lng="hu">modellt &uuml;l</trd>
      <trd lng="ru">&c_p;&c_o;&c_z;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
    </subsnc>
    <subsnc mrk="poz.0i.afekti">
      <dif>
        Afekti gravan mienon; &scirc;ajnigi sin pla&ccirc;ulo, eminentulo:
        <ekz>
          &Hcirc;lestakov &ndash; <tld/>ante &ndash; &leftquot;Mi petas vin, sinjorino, al mi estas
          tre agrable
          <fnt><bib>Revizoro</bib>, <lok>akto 4a, sceno
          12a</lok></fnt>...
        </ekz>
        <ekz>
          <tld/>i spritulon, scienculon.
        </ekz>
      </dif>
      <trd lng="de">eine <ind>Pose</ind> einnehmen</trd>
      <trdgrp lng="hu">
        <trd>p&oacute;zol</trd>,
        <trd>nagyk&eacute;p&udblac;sk&ouml;dik</trd>
      </trdgrp>
      <trdgrp lng="ru">
        <trd>&c_r;&c_i;&c_s;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
        <trd>&c_i;&c_z;&c_o;&c_b;&c_r;&c_a;&c_zh;&c_a;&c_t;&c_mol; &c_i;&c_z; &c_s;&c_je;&c_b;&c_ja;</trd>
      </trdgrp>
    </subsnc>
    <subsnc mrk="poz.0i.3">
      <uzo tip="stl">EVI</uzo>
      <dif>
        Teni sin iamaniere.
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_z;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>p&#xF3;zovat</trd>,
    <trd>sed&#x11B;t modelem</trd>,
    <trd>st&#xE1;t modelem</trd>
  </trdgrp>
  <trd lng="en">pose</trd>
  <trdgrp lng="fr">
    <trd>frimer</trd>,
    <trd>poser<klr tip="amb"> (prendre une pose)</klr></trd>
  </trdgrp>
  <trd lng="nl">poseren</trd>
  <trd lng="pl">pozowa&cacute;</trd>
  <trdgrp lng="pt">
    <trd>posar</trd>,
    <trd>fazer <ind>pose</ind></trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>postavi&tcaron; sa do p&#xF3;zy</trd>,
    <trd>p&#xF3;zova&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_z;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_i;&c_s;&c_u;&c_v;&c_a;&c_t;&c_i;&c_s;&#x44F;</trd>
  </trdgrp>
</drv>

