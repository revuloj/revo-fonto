  <trdgrp lng="cs">
    <trd>dr&zcaron;en&iacute; t&ecaron;la</trd>,
    <trd>postoj</trd>,
    <trd>p&oacute;za</trd>
  </trdgrp>
  <trd lng="cy">peri</trd>
  <trd lng="da">udg&oslash;re</trd>
  <trd lng="de">Pose</trd>
  <trdgrp lng="en">
