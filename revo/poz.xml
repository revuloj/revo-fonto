  </snc>
  <trdgrp lng="be">
    <trd>&c_n;&c_a;&c_t;&c_u;&c_r;&c_sh;&c_ch;&c_y;&c_k;</trd>,
    <trd>&c_m;&c_a;&c_d;&c_e;&c_l;&c_mol;</trd>
