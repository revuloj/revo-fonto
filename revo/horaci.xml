<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: horaci.xml,v 1.13 2023/08/31 15:46:18 revo Exp $">
<kap>
  <rad>Horaci</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="horaci.0o">
  <kap><tld/>o</kap>
  <snc mrk="horaci.0o.NOM">
    <uzo tip="fak">NOM</uzo>
    <dif>
      Vira <ref tip="lst" cel="nom.antaux0o" lst="voko:personaj_nomoj">anta&ubreve;nomo</ref> latindevena.
    </dif>
    <trd lng="lat">Horatius</trd>
    <trd lng="pl">Horacy</trd>
  </snc>
  <snc mrk="horaci.0o.PERS">
    <uzo tip="fak">BELE</uzo>
    <uzo tip="fak">PERS</uzo>
    <dif>
      Latina poeto (65 a. K. &ndash; 8 a. K.), a&ubreve;toro de satiroj,
      odoj, epistoloj, rigardata kiel modele klasika:
      <ekz>
        liaj konversacioj temas nur pri la libroj <klr>...</klr>
        de <tld/>o, Perso, Juvenalo kaj Tibulo
        <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 16a,
        p. 479a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="lat">Quintus <ind>Horatius</ind> Flaccus</trd>
    <trd lng="pl">Horacjusz</trd>
  </snc>                      
  <trd lng="br">Horas</trd>
  <trd lng="be">&c_G;&c_a;&c_r;&c_a;&c_c;&c_y;&c_j;</trd>
  <trd lng="de">Horatius</trd>
  <trdgrp lng="en">
    <trd>Horace</trd>,
    <trd>Horatio</trd>
  </trdgrp>
  <trd lng="fr">Horace</trd>
  <trd lng="hu">Horatius</trd>
  <trd lng="it">Orazio</trd>
  <trd lng="ja">&#x30DB;&#x30E9;&#x30C6;&#x30A3;&#x30A6;&#x30B9;</trd>
  <trd lng="pt">Hor&aacute;cio</trd>
  <trd lng="ru">&c_G;&c_o;&c_r;&c_a;&c_c;&c_i;&c_j;</trd>
  <trd lng="uk">&#x413;&#x43E;&#x440;&#x430;&#x446;&#x456;&#x439;</trd>
</drv>
</art>

