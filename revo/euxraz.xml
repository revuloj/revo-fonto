<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>E&ubreve;razi</rad>/o
</kap>
<drv mrk="euxraz.E0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      La plej granda el la
      <ref tip="lst" cel="kontin.0o.terkuno" 
      lst="voko:kontinentoj">kontinentoj</ref>,
      norde de Afriko kaj ampleksanta
      <ref tip="prt" cel="euxrop1.E0o">E&ubreve;ropon</ref>
      kaj <ref tip="prt" cel="azi.A0o">Azion</ref>.
    </dif>
    <rim>
      Britujo, Japanujo kaj la indoneziaj insuloj 
      ne situas sur la kontinento <tld/>o.
    </rim>
  </snc>
  <trd lng="be">&c_E;&c_w;&c_r;&c_a;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_Je;&c_v;&c_r;&c_a;&c_z;&c_i;&c_ja;</trd>
  <trd lng="ca">Eur&agrave;sia</trd>
  <trd lng="de">Eurasien</trd>
  <trd lng="fr">Eurasie</trd>
  <trd lng="hu">Eur&aacute;zsia</trd>
  <trd lng="id">Eurasia</trd>
  <trd lng="nl">Eurazi&euml;</trd>
  <trd lng="pl">Eurazja</trd>
  <trd lng="pt">Eur&aacute;sia</trd>
  <trd lng="ru">&c_Je;&c_v;&c_r;&c_a;&c_z;&c_i;&c_ja;</trd>
</drv>
</art>

