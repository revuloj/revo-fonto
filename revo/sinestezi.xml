  <trd lng="no">synestesi</trd>
  <trd lng="pl">synestezja</trd>
  <trd lng="pt">sinestesia</trd>
  <trd lng="ru">&c_s;&c_i;&c_n;&c_je;&c_s;&c_t;&c_je;&c_z;&c_i;&c_ja;</trd>
  <trd lng="sk">synest&eacute;zia</trd>
