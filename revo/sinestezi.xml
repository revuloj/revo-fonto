<vortaro>
<art mrk="$Id: sinestezi.xml,v 1.7 2023/10/21 07:46:47 revo Exp $">
<kap>
  <rad>sinestezi</rad>/o,
  <var><kap><rad var="z">sinestez</rad>/o
  <fnt>
    <aut>-</aut>
    <vrk><url ref="https://www.scienca-asocio.com/Fakvortaro/Sinestezo">
    Glosaro de ISAE, 1a Versio</url></vrk>
    <lok>2017</lok>
  </fnt></kap></var>
</kap>
<drv mrk="sinestezi.0o">
  <kap><tld/>o,
  <var><kap>sinestezo</kap></var></kap>
  <snc mrk="sinestezi.0o.PSI">
    <uzo tip="fak">PSI</uzo>
    <dif>
      Subjektiva influo de <ref cel="sens.0o">senso</ref> al alia senso,
      ekzemple kiam odoro rekte peras koloran <ref cel="impres.0o">impreso</ref>n:
      <ekz>
        se ni ankora&ubreve; a&ubreve;dus certan tonon, dum ke ni imagas
        ru&gcirc;an rozon, tiam ni travivas <tld/>an eventon
        <fnt>
          <aut>R. Hauger </aut>,
          <vrk><url ref="&STEB;ceteraj/parfumo/de-incenso-al-parfumo.htm">
          De incenso antikva al moderna parfumo</url></vrk>,
          <lok>Scienca Revuo, 1991:158, p. 61a-92a</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a sperto fari&gcirc;is la inspiro de miaj multaj artaj kaj
        muzikaj krea&jcirc;oj
        <fnt>
          <aut>Tigro</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/billmak/285431">
          &Ccirc;u eblas vidi koloron en muziko?</url></vrk>,
          <lok>2010-10-27</lok>
        </fnt>;
      </ekz>
      <ekz>
        gravan rolon ludas la <tld var="z"/>o, t. e. spiritimpreso ricevita per kuni&gcirc;o de
        sensimpresoj, ekzemple koloroj farante impreson muzikan
        <fnt>
          <aut>L. C. Deij</aut>
          <vrk><url ref="http://www.tekstoj.nl/mr/MR473.pdf"
              >Enkonduko al Beletro</url></vrk>
          <lok>Malgranda Revuo, 1947:3</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ar">&a_m;&a_H;&a_A;&a_s;&a_t_marbuta;</trd>
  <trd lng="be">&c_s;&c_ib;&c_n;&c_e;&c_s;&c_t;&c_e;&c_z;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_s;&c_i;&c_n;&c_je;&c_s;&c_t;&c_je;&c_z;&c_i;&c_ja;</trd>
  <trd lng="ca">sinest&egrave;sia</trd>
  <trd lng="cs">synestezie</trd>
  <trd lng="da">syn&aelig;stesi</trd>
  <trd lng="de">Syn&auml;sthesie</trd>
  <trd lng="el">&sigma;&ypsilon;&ny;&alfa;&jota;&sigma;&theta;&eta;&sigma;&jota_ton;&alfa;</trd>
  <trdgrp lng="en">
    <trd>synaesthesia</trd>,
    <trd>synesthesia</trd>
  </trdgrp>
  <trd lng="es">sinestesia</trd>
  <trd lng="fa">&a_h;&a_m;&#8204;&a_r;&a_n;&a_j;&f_y;</trd>
  <trd lng="fi">synestesia</trd>
  <trd lng="fr">synesth&eacute;sie</trd>
  <trd lng="gl">sinestesia</trd>
