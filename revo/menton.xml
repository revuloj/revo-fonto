<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>menton</rad>/o
</kap>
<drv mrk="menton.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Parto de la viza&gcirc;o sub la bu&scirc;o:
        <ekz>
	&scirc;i rektigas la kapon, krucigas la fingrojn kaj sur ili apogas 
	la <tld/>on
        <fnt><aut>Machado de Assis, trad. Paulo Viana</aut>
        <vrk>La divenistino kaj aliaj rakontoj</vrk><lok>Oportuno, jaro
        2005a, p.
        31a</lok></fnt>.
	</ekz>
    </dif>
  </snc>                       
  <trd lng="be">&c_p;&c_a;&c_d;&c_b;&c_a;&c_r;&c_o;&c_d;&c_a;&c_k;</trd>
  <trd lng="bg">&c_b;&c_r;&c_a;&c_d;&c_a;</trd>
  <trdgrp lng="br">        
    <trd>elgezh</trd>,
    <trd>chik<klr tip="ind"> (elgezh)</klr></trd>,
    <trd>gro&ntilde;ch</trd>
  </trdgrp>
  <trd lng="ca">ment&oacute;</trd>
  <trd lng="de">Kinn</trd>
  <trd lng="en">chin</trd>
  <trd lng="fa">&f_ch;&a_A;&a_n;&a_h;</trd>
  <trd lng="fr">menton</trd>
  <trd lng="hu">&aacute;ll<klr tip="ind"> (testr&eacute;sz)</klr></trd>
  <trd lng="id">dagu</trd>
  <trd lng="it">mento</trd>
  <trd lng="nl">kin</trd>
  <trd lng="pl">podbr&oacute;dek</trd>
  <trdgrp lng="pt">
    <trd>mento</trd>,
    <trd>queixo</trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_b;&c_o;&c_r;&c_o;&c_d;&c_o;&c_k;</trd>
  <trd lng="sv">haka</trd>
</drv>
</art>
