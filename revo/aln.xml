<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aln.xml,v 1.28 2021/02/28 15:34:27 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>aln</rad>/o
</kap>
<drv mrk="aln.0o">
   <kap><ofc>*</ofc><tld/>o</kap>
   <uzo tip="fak">BOT</uzo>
   <snc>
     <dif>
       <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref>
       el familio <ref tip="malprt" cel="betula.0oj">betulacoj</ref>
       (<trd lng="la">Alnus</trd>), monoikaj arboj
       a&ubreve; arbedoj, kies viraj floraroj formas amentojn de flortriopoj sidantaj en
       la akseloj de brakteoj kaj kies virinaj floraroj formas strobiletojn
       kun flordupoj sidantaj en brakteakseloj. La fruktoj estas unusemaj nuksetoj, foje
       flugilhavaj:
      <ekz>
        maljunaj <tld/>oj, kiujn la fluo depu&scirc;is de la dekliva bordo, per siaj radikoj
        forte teni&gcirc;is je la fundo
        <fnt>
          <bib>Fab3</bib>
          <lok>Ib kaj malgranda kristino</lok>
        </fnt>;
      </ekz>
      <ekz>
        apud la kanaro ku&scirc;is trunko de <tld/>o
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        mordetas la arbaraj skaraboj niajn betulojn, <tld/>ojn kaj pinojn
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
     </dif>
    <refgrp tip="vid">
       <ref cel="betul.0o">betulo</ref>,
       <ref cel="karpen.0o">karpeno</ref>,
       <ref cel="avel.0ujo">avelujo</ref>
    </refgrp>
   </snc>
   <bld lok="../bld/aln.jpg">strobiletoj kaj kelkaj amentoj de <tld/>o</bld>
   <trdgrp lng="be">
     <trd>&c_a;&c_l;&c_je;&c_sh;&c_y;&c_n;&c_a;</trd>, 
     <trd>&c_v;&c_o;&c_l;&c_mol;&c_h;&c_a;</trd>
   </trdgrp>
   <trdgrp lng="br">
     <trd>gwernenn</trd>,
     <trd>gwezenn-wern</trd>
   </trdgrp>
   <trdgrp lng="ca">
     <trd>vern</trd>,
     <trd>g&egrave;nere Alnus</trd>
   </trdgrp>
   <trd lng="de">Erle</trd>
   <trdgrp lng="en">
     <trd>alder</trd>,
     <trd>elder</trd>
   </trdgrp>
   <trd lng="es">aliso</trd> 
   <trd lng="fr">aulne</trd>
   <trd lng="he">&#x5D0;&#x5DC;&#x5DE;&#x5D5;&#x5DF;</trd>
  <trd lng="hu">&eacute;ger(fa)</trd>
   <trd lng="nl">els<klr tip="ind"> (boom)</klr></trd>
   <trd lng="pt">amieiro</trd>
   <trd lng="ru">&c_o;&c_l;&c_mol;&c_h;&c_a;</trd>
   <trd lng="sk">jel&scaron;a</trd>
   <trd lng="sv">al</trd>
  <trd lng="cs">ol&scaron;e</trd>
</drv>
</art>
