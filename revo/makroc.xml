<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>makrocefal</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="makroc.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="makroc.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Physeter</trd>), kun nur unu specio kun dika, potsimila
      kapo.
    </dif>
    <ref tip="sin" cel="kacxal">ka&ccirc;aloto</ref>
  </snc>
  <trd lng="de">Pottwal</trd>
  <trd lng="nl">potvis</trd>
</drv>
<drv mrk="makroc.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="cetac.0oj">cetacoj</ref>
      (<trd lng="la">Physeteridae</trd>). 
    </dif>
  </snc>
  <trd lng="de">Pottwale <klr tip="ind">(Familie)</klr></trd>
  <trd lng="nl">potvissen</trd>
</drv>
</art>

