<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: presti.xml,v 1.17 2022/05/25 22:32:57 revo Exp $">
<kap>
  <rad>prestidigitator</rad>/o
</kap>
<drv mrk="presti.0o">
  <kap><tld/>o</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="presti.0o.KOMUNE">
    <uzo tip="fak">TEA</uzo>
    <dif>
      <ref tip="dif" cel="iluzi.0isto">Iluziisto</ref>:
      <ekz>
        en la cirko mi vidis &hcirc;aldean <tld/>on, kiu levi&gcirc;is en la aeron
        <fnt><bib>Far2</bib>, <lok>&ccirc;apitro 14a</lok></fnt>;
      </ekz>
      <ekz>
        do &ccirc;i tiu <tld/>o, pensis la kronprinco, e&ccirc; ne anta&ubreve;sentas en
        sia animo, ke mi konas &ccirc;iujn iliajn fripona&jcirc;ojn
        <fnt>
          <bib>Far2</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>?
      </ekz>
      <ekz>
        tiu amuzisto, kiu lerte trompas la rigardantojn per falsaj
        &scirc;ajnoj kaj (a&ubreve;)
        rapide faritaj &jcirc;ongla&jcirc;oj (<ref cel="jxongl.0isto">&jcirc;onglisto</ref>, <tld/>o)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=57816"
              >Iluzio</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_e;&c_s;&c_t;&c_y;&c_d;&c_y;&c_zh;&c_y;&c_t;&c_a;&c_t;&c_a;&c_r;</trd>
  <trd lng="ca">prestidigitador</trd>
  <trdgrp lng="cs">
    <trd>eskamot&eacute;r</trd>,
    <trd>iluzionista</trd>,
    <trd>kouzeln&iacute;k</trd>,
    <trd>&zcaron;ongl&eacute;r</trd>
  </trdgrp>
  <trd lng="de">Taschenspieler</trd>
  <trdgrp lng="fr">
    <trd>illusionniste</trd>,
    <trd>prestidigitateur</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>b&udblac;v&eacute;sz</trd>,
    <trd>szemf&eacute;nyveszt&odblac;</trd>
  </trdgrp>
  <trd lng="nl">goochelaar</trd>
  <trdgrp lng="pl">
    <trd>kuglarz</trd>,
    <trd>magik</trd>,
    <trd>prestidigitator</trd>
  </trdgrp>
  <trd lng="pt">prestidigitador</trd>
  <trdgrp lng="ro">
    <trd>scamator</trd>,
    <trd>vr&abreve;jitor</trd>,
    <trd>prestidigitator</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_je;&c_s;&c_t;&c_i;&c_d;&c_i;&c_zh;&c_i;&c_t;&c_a;&c_t;&c_o;&c_r;</trd>,
    <trd>&c_f;&c_o;&c_k;&c_u;&c_s;&c_n;&c_i;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>eskamot&eacute;r</trd>,
    <trd>kaukliar</trd>,
    <trd>k&uacute;zeln&iacute;k</trd>
  </trdgrp>
</drv>
</art>
