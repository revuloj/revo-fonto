<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: impregn.xml,v 1.9 2023/10/31 09:27:17 revo Exp $">
<kap>
  <rad>impregn</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="impregn.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="impregn.0i.KOMUNE">
    <uzo tip="fak">TEK</uzo><uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="vid" cel="sorb.0igi.KOMUNE">Sorbigi</ref>
      materialon per flua&jcirc;o por protekti &gcirc;in (kontra&ubreve; malsekeco, putrado, insektoj ktp):
      <ekz>
        per la pretigita infuza&jcirc;o ni bone <tld/>u la vatopecon
        <fnt>
          <aut>A. P&aacute;sztor</aut>,
          <vrk>Viza&gcirc;a refre&scirc;igila&jcirc;o</vrk>,
          <lok><url
          ref="https://adoc.pub/informilo-de-kiraly-lajos-esperanto-amika-rondo-miskolc5ef7640b75025cc350dfa8f57c58555161626.html">
          Nordhungaria Informo, 2007-01</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve; pedikoj, la soldatoj estis pudritaj kaj ricevis DDT-<tld/>itajn &ccirc;emizojn
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=42763"
              >DDT</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        por malhelpi galvanan korodon inter la kuproha&ubreve;to kaj la fera subtenstrukturo,
        <nom>Eiffel</nom> izolis la ha&ubreve;ton per asbesto <tld/>ita el &scirc;elako
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=18983"
              >Statuo de Libereco</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        fatala aromo <tld/>is la aeron
        <fnt><aut>G. Garc&iacute;a M&aacute;rquez, trad. F. de Diego</aut>, <vrk>Cent
        Jaroj da Soleco</vrk>, <lok>1992</lok></fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="balzam.0i.PAR">balzami</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_r;&c_a;&c_m;&c_o;&c_ch;&c_v;&c_a;&c_c;&c_mol; <klr>(&c_m;&c_a;&c_t;&c_a;&c_r;&#8217;&c_ja;&c_l;)</klr></trd>,
    <trd>&c_n;&c_a;&c_s;&c_y;&c_ch;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>impregnovat</trd>,
    <trd>napou&scaron;t&ecaron;t</trd>
  </trdgrp>
  <trd lng="de">impr&auml;gnieren</trd>
  <trd lng="fr">impr&eacute;gner</trd>
  <trdgrp lng="pl">
    <trd>impregnowa&cacute;</trd>,
    <trd>przesyca&cacute;</trd>,
    <trd>przepaja&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>impregnova&tcaron;</trd>,
    <trd>mori&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_p;&c_l;&c_ib;&c_d;&c_n;&c_ju;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_p;&c_r;&c_o;&c_s;&c_o;&c_ch;&c_u;&c_v;&c_a;&c_t;&c_i; (&c_t;&c_k;&c_a;&c_n;&c_i;&c_n;&c_u;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_je;&c_v;&c_i;&c_n;&c_u; &c_ib; &c_t;.&c_p;.)</trd>,
    <trd>&c_ib;&c_m;&c_p;&c_r;&c_je;&c_gu;&c_n;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>
</art>

