<kap>
  <ofc>7</ofc>
  <rad>antropolog</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="antrop1.0o">
  <kap><tld/>o</kap>
  <snc mrk="antrop1.0o.antropologiisto">
    <dif>
      <ref tip="dif" cel="antrop2.0isto">Antropologiisto</ref>:
      <ekz>
        kiom da rasismo, kaj tro senkritika kredo al diversaj rasismaj
        <tld/>oj, karakterizas la vidpunkton de nia a&ubreve;toro<fnt><aut>Donald
        Broadribb</aut>: <vrk>Afrikanoj vidataj deekstere</vrk>,
        <bib>Monato</bib><lok>jaro 2002a, numero 3a, p. 24a</lok></fnt>?
      </ekz>
      <ekz>
        usonaj <tld/>oj prognozas, ke post 200&#8239;-&#8239;300 jaroj la meza terlo&gcirc;anto estos
        aziano, trifingra kaj kun granda kranio, meza lo&gcirc;anto de nuna Ukrainio estas
        urba virino, edzini&gcirc;inta, kun unu infano kaj kolegia klereco
        <fnt>
          <bib>Monato</bib>
          <aut>Viktor Pajuk</aut>
          <vrk><url ref="&Monato;/2003/008173.html">Ankora&ubreve; ne trifingraj ...</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        famaj <tld/>oj pruvis en sciencaj verkoj, kiom miksite jam kuni&gcirc;is &ccirc;iuj rasoj en
        E&ubreve;ropo
        <fnt>
          <bib>VivZam</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="zh">
      <trd>&#20154;&#31867;&#23398;&#23478; <pr>r&#233;nl&#232;ixu&#233;ji&#257;</pr></trd>,
      <trd>&#20154;&#39006;&#23416;&#23478; <pr>r&#233;nl&#232;ixu&#233;ji&#257;</pr></trd>
    </trdgrp>
  </snc>
  <rim>
    La&#365; <fnt><bib>BL</bib></fnt>: Anka&#365; eblas diri <ke>antropologiisto</ke>, <ke>homsciencisto</ke>, <ke>homstudanto</ke>.
  </rim>

  <trd lng="be">&c_a;&c_n;&c_t;&c_r;&c_a;&c_p;&c_o;&c_l;&c_ja;&c_g;</trd>
  <trd lng="ca">antrop&ograve;leg</trd>
  <trd lng="cs">antropolog</trd>
  <trd lng="de">Anthropologe</trd>
  <trd lng="en">anthropologist</trd>
  <trd lng="fr">anthropologue</trd>
  <trd lng="he">&#1488;&#1463;&#1504;&#1514;&#1512;&#1493;&#1465;&#1508;&#1468;&#1493;&#1465;&#1500;&#1493;&#1465;&#1490;</trd>
  <trd lng="hu">antropol&oacute;gus</trd>
  <trd lng="ja">&#20154;&#39006;&#23398;&#32773;  <pr>&#12376;&#12435;&#12427;&#12356;&#12364;&#12367;&#12375;&#12419;</pr></trd>
  <trd lng="ru">&c_a;&c_n;&c_t;&c_r;&c_o;&c_p;&c_o;&c_l;&c_o;&c_g;</trd>
  <trdgrp lng="sk">
    <trd>antropol&oacute;g</trd>,
    <trd>antropol&oacute;g</trd>
  </trdgrp>
  <trd lng="uk">&#1072;&#1085;&#1090;&#1088;&#1086;&#1087;&#1086;&#1083;&#1086;&#1075;</trd>
</drv>
