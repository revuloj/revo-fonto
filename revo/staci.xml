<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>staci</rad>/o
</kap>
<drv mrk="staci.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="staci.0o.haltejo">
  <uzo tip="fak">TRA</uzo>
    <dif>
      Loko, kie haltas la publikaj trafikaj veturiloj, vagonoj, &scirc;ipoj, por
      preni a&ubreve; lasi la pasa&gcirc;erojn kaj varojn.
    </dif>
    <trdgrp lng="be">
      <trd>&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>, 
      <trd>&c_p;&c_r;&c_y;&c_p;&c_y;&c_n;&c_a;&c_k;</trd>, 
      <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>
    </trdgrp>
    <trd lng="nl">station</trd>
    <trdgrp lng="ru">
      <trd>&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>,
      <trd>&c_o;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>,
      <trd>&c_s;&c_t;&c_o;&c_ja;&c_n;&c_k;&c_a;</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Loko, kie multaj voja&gcirc;antoj kelktempe restadas:
      <ekz>
        marbordaj ban<tld/>oj;
      </ekz>
      <ekz>
        montaraj somer<tld/>oj.
      </ekz>
    </dif>
    <trdgrp lng="fr">
      <trd>station<klr> (touristique)</klr></trd>
    </trdgrp>
    <trd lng="he">&mem;&het;&nun;&he;</trd>
    <trdgrp lng="nl">
      <trd>vakantieoord</trd>,
      <trd>kuuroord</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Nomo de kelkaj sciencaj establejoj:
      <ekz>
        <ind>radio<tld/>o</ind>.
        <trd lng="hu">r&aacute;di&oacute;&aacute;llom&aacute;s</trd>
      </ekz>
    </dif>
    <trd lng="nl">station</trd>
    <trd lng="ru">&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
  </snc>
  <snc mrk="staci.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Provizora haltejo:
      <ekz>
        tio estis nur <tld/>o sur la vojo al io pli alta
      </ekz>
      <sup>D</sup>.
    </dif>
    <trdgrp lng="fr">
      <trd>&eacute;tape<klr> (fig.)</klr></trd>
    </trdgrp>
    <trd lng="nl">statie</trd>
    <trd lng="ru">&c_o;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="de">
    <trd>Station</trd>,
    <trd>Haltestelle</trd>
  </trdgrp>
  <trd lng="en">station</trd>
  <trdgrp lng="fr">
    <trd>station</trd>
  </trdgrp>
  <trd lng="he">&tav;&het;&nun;&he;</trd>
  <trd lng="hu">&aacute;llom&aacute;s</trd>  
</drv>
</art>
