<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>staci</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="staci.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="staci.0o.KOMUNE">
    <dif>
      <ref tip="super" cel="halt.0ejo.KOMUNE">Haltejo</ref> kun iaj
      aran&gcirc;a&jcirc;oj te&hcirc;nikaj a&ubreve; komfortigaj:
      <ekz>
        en la antikveco la gubernio servis kiel grava trans&scirc;ar&gcirc;a <tld/>o &ccirc;e la
        sud-okcidenta Silka Vojo
        <fnt>
          <bib>Monato</bib>
          <aut>Alice Liu</aut>
          <vrk><url ref="https://www.monato.be/2007/009268.php">Favore al turismo</url></vrk>
          <lok>2007</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni gloris la <tld/>on, kiam <klr>[&#8230;]</klr> Rusio pafis la unuan <nom>ISS</nom>-modulon
        <nom>Sarja</nom> kiel komponanton de la estonta laboratorio
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2009/009681.php">Politika sukceso, scienca fiasko</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
    <subsnc mrk="staci.0o.haltejo">
      <uzo tip="fak">TRA</uzo>
      <dif>
        Loko, kie haltas publikaj trafikaj veturiloj, vagonoj,
        &scirc;ipoj, por preni a&ubreve; lasi la pasa&gcirc;erojn kaj varojn:
        <ekz>
          trafi tramon &ccirc;e l' <tld/>o
          <fnt>
            <aut>Raymond Schwartz</aut>
            <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga Butiko</url></vrk>
            <lok>Nigra Tago</lok>
          </fnt>;
        </ekz>
        <ekz>
          a&ubreve;tobusa <tld/>o
          <fnt>
            <bib>Monato</bib>
            <aut>Saliko</aut>
            <vrk><url ref="https://www.monato.be/2009/009824.php">&Ccirc;arma urbo &ccirc;e la Finna Golfo</url></vrk>
            <lok>2009</lok>
          </fnt>;
        </ekz>
        <ekz>
          <ref cel="dom.staci0o">stacidomo</ref>
          <fnt>
            <bib>FdO</bib>
          </fnt>.
        </ekz>
      </dif>
      <trdgrp lng="be">
        <trd>&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>,
        <trd>&c_p;&c_r;&c_y;&c_p;&c_y;&c_n;&c_a;&c_k;</trd>,
        <trd>&c_p;&c_r;&c_y;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>
      </trdgrp>
      <trd lng="nl">station</trd>
      <trd lng="pl">stacja</trd>
      <trd lng="ro">sta&tcedil;ie</trd>
      <trdgrp lng="ru">
        <trd>&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>,
        <trd>&c_o;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>,
        <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_a;&c_n;&c_mol;</trd>,
        <trd>&c_s;&c_t;&c_o;&c_ja;&c_n;&c_k;&c_a;</trd>
      </trdgrp>
    </subsnc>
<!-- &ccirc;u fakte uzata senco? mi ne trovis pruvon...
    <subsnc mrk="staci.0o.TUR">
      <dif>
        Plezurloko, kie multaj voja&gcirc;antoj kelktempe restadas:
        <ekz>
          marbordaj ban<tld/>oj;
        </ekz>
        <ekz>
          montaraj somer<tld/>oj.
        </ekz>
      </dif>
      <trd lng="he">&mem;&het;&nun;&he;</trd>
      <trdgrp lng="nl">
        <trd>vakantieoord</trd>,
        <trd>kuuroord</trd>
      </trdgrp>
      <trd lng="pl">stacja</trd>
    </subsnc>
-->
    <subsnc mrk="staci.0o.TEK">
      <uzo tip="fak">ELE</uzo>
      <dif>
        Te&hcirc;nika establejo por generado, konservo a&ubreve; plusendo de
        energio:
        <ekz>
          li atentigis pri troa centrado de la elektra energio de Brazilo en malmultaj
          <tld/>oj
          <fnt>
            <bib>Monato</bib>
            <aut>Jozefo Lej&ccirc;</aut>
            <vrk><url ref="https://www.monato.be/2010/009949.php">Kuranta kurento-krizo</url></vrk>
            <lok>2010</lok>
          </fnt>.
        </ekz>
      </dif>
      <refgrp tip="vid">
        <ref cel="centra1.0o">centralo</ref>
      </refgrp>
      <trd lng="nl">station</trd>
      <trd lng="ru">&c_s;&c_t;&c_a;&c_n;&c_c;&c_i;&c_ja;</trd>
    </subsnc>
    <subsnc mrk="staci.0o.elsendejo">
      <uzo tip="fak">RAD</uzo>
      <dif>
        Elsendejo:
        <ekz>
          ekkriis la konduktoro, elirante el la telegrafa <tld/>o,
          portante paperon en la mano
          <fnt><bib>FK</bib>, <lok>4, la nigra virino</lok></fnt>;
        </ekz>
        <ekz>
          radioparolis <tld/>o <nom>Semarang</nom>
          <fnt>
            <aut>Raymond Schwartz</aut>
            <vrk><url ref="http://steloj.de/esperanto/butiko">La Stranga Butiko</url></vrk>
            <lok>Telegramo</lok>
          </fnt>;
        </ekz>
        <ekz>
          pluraj <tld/>oj, kiuj dissendas samhore per la sama frekvenco
          <fnt>
            <bib>Monato</bib>
            <aut>Pejno Simono</aut>
            <vrk><url ref="http://steloj.de/esperanto/monato2/007219.html">Prefere pakete</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          televida <tld/>o
          <fnt>
            <bib>Monato</bib>
            <aut>Zofia Banet-Fornalowa</aut>
            <vrk><url ref="https://www.monato.be/2004/008391.html">Listo de Sendler</url></vrk>
            <lok>2004</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="staci.0o.kalvaria_vojo">
      <uzo tip="fak">KRI</uzo>
      <dif>
        Haltejo en <ref tip="vid" cel="kalvar.k0avojo.KRI">kalvaria
        vojo</ref>:
        <ekz>
          &scirc;i staris <klr>[&#8230;]</klr> &ccirc;e la komenco de sia Kalvaria vojo,
          &scirc;i pasis ankora&ubreve; ne pli ol du &gcirc;iajn <tld/>ojn
          <fnt><bib>Marta</bib></fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <snc mrk="staci.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Provizora haltejo en agado, en progresado:
      <ekz>
        oni povis vidi en li homon, por kiu la magazeno de la juvelisto
        estis nur <tld/>o sur la vojo al lokoj pli altaj kaj pli
        profitaj
        <fnt><bib>Marta</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="etap.0o">etapo</ref>,
      <ref cel="faz.0o.KOMUNE">fazo <sncref/></ref>,
      <ref cel="sxtup.0o.FIG">&scirc;tupo <sncref/></ref>
    </refgrp>
    <trd lng="nl">statie</trd>
    <trd lng="pl">stacja</trd>
    <trd lng="ru">&c_o;&c_s;&c_t;&c_a;&c_n;&c_o;&c_v;&c_k;&c_a;</trd>
  </snc>
  <trd lng="be">&c_s;&c_t;&c_a;&c_n;&c_c;&c_y;&c_ja;</trd>
  <trd lng="cs">stanice</trd>
  <trdgrp lng="de">
    <trd>Station</trd>,
    <trd>Haltestelle</trd>
  </trdgrp>
  <trd lng="en">station</trd>
  <trd lng="es">estaci&oacute;n</trd>
  <trdgrp lng="fr">
    <trd>&eacute;tape<klr> (fig.)</klr></trd>,
    <trd>station</trd>
  </trdgrp>
  <trd lng="he">&tav;&het;&nun;&he;</trd>
  <trd lng="hu">&aacute;llom&aacute;s</trd>
  <trd lng="pl">stacja</trd>
  <trd lng="pt">esta&ccedil;&atilde;o</trd>
  <trd lng="sk">stanica</trd>
</drv>

<drv mrk="staci.akvo0o">
  <kap>akvo<tld/>o</kap>
  <snc mrk="staci.akvo0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="staci.0o.TEK"><tld lit="S"/>o</ref> kiu
      konservas akvon kaj &gcirc;in provizas en iu loko:
      <ekz>
        <nom>Jim</nom> esploris la lanternon apud la akvo<tld/>o
        <fnt><bib>FK</bib>, <lok>4, la nigra virino</lok></fnt>;
      </ekz>
      <ekz>
        &ccirc;e la akvo<tld/>o de la 36km-punkto mi kalkulis la restantan tempon
        <fnt>
          <vrk><url ref="http://www.hokkajda-esp-ligo.jp/jp/HEROLDO_PDF/Heroldo_N-ro161.pdf">Heroldo de HEL</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Zapfstelle</trd>
  <trd lng="fr">ch&acirc;teau d'eau</trd>
</drv>

<drv mrk="staci.bus0o">
  <kap>bus<tld/>o,
  <var><kap>a&ubreve;tobus<tld/>o</kap></var>,
  <var><kap>busa <tld/>o</kap></var>,
  <var><kap>a&ubreve;tobusa <tld/>o</kap></var></kap>
  <snc mrk="staci.bus0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="staci.0o.haltejo"><tld lit="S"/>o</ref>,
      kie buso haltas por preni a&ubreve; lasi la pasa&gcirc;erojn:
      <ekz>
        en bus<tld/>o, lernejo a&ubreve; plej luksa restoracio
        la necesejoj a&ccirc;as
        <fnt>
        <aut>B. Hekland</aut>,
        <vrk>Kompreni &ccirc;i-landon? Neniam!</vrk>,
        <bib>LOdE</bib>,
        <lok>1999:3</lok>
        </fnt>;
      </ekz>
      <ekz>
        iam sur iu a&ubreve;tobusa <tld/>o okazas neplanita
        renkonti&gcirc;o
        <fnt>
          <aut>V. O&scirc;lak</aut>,
          <vrk>Kiam la silento krias</vrk>,
          <lok><url ref="http://www.esperanto.net/literaturo/lf/krisilentrec.html">
          Literatura Foiro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        nove konstruita busa <tld/>o en <nom>Rondonpolis</nom>, Brazilo, ricevis
        indikilojn en Esperanto
        <fnt>
          <aut>Universala Esperanto-Asocio</aut>,
          <vrk>Elstaraj agadoj 2001</vrk>,
          <lok><url ref="http://www.uea.org/dokumentoj/komunikoj/gaz145.html">Gazetaraj
          Komunikoj</url>, jaro 2002a, numero 145a</lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Temas pri busoj, kiuj veturas inter foraj urboj. La busoj, kiuj
      veturas inter proksimaj urboj a&ubreve; ene de unu sama urbo, ne uzas
      bus<tld/>on sed <ref cel="halt.bus0ejo.TRA">bushaltejon
      <sncref/></ref>.
    </rim>
  </snc>
  <trd lng="de">Bushaltestelle</trd>
  <trd lng="fr">station de bus</trd>
  <trd lng="pl">dworzec autobusowy</trd>
  <trd lng="pt">esta&ccedil;&atilde;o rodovi&aacute;ria</trd>
</drv>

<drv mrk="staci.elektro0o">
  <kap>elektro<tld/>o, <var><kap>elektra <tld/>o</kap></var></kap>
  <snc mrk="staci.elektro0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="dif" cel="elektr.0ejo">Elektrejo</ref>:
      <ekz>
        preterpasante, ni vizitas la elektro<tld/>on, kie laboras <nom>Rudolf</nom>
        <fnt><bib>Metrop</bib></fnt>;
      </ekz>
      <ekz>
        &ccirc;irka&ubreve; la jaro 1890 estis instalita elektro<tld/>o,
        kiu produktis elektron por lumigi la palacon kaj
        Vintran &Gcirc;ardenon
        <fnt>
          <aut>A. &Ccaron;i&zcaron;ikait&#279;</aut>,
          <vrk>Kretinga, kongresurbo</vrk>
          <lok>57a Kongreso de Internacia Katolika Unui&gcirc;o
          Esperantista</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;ina konstruado de nuklea elektro<tld/>o komenci&gcirc;is en la
        80-jaroj de la lasta jarcento
        <fnt>
        <vrk><url ref="http://es.chinabroadcast.cn/1/2005/06/14/1@25723.htm">&Ccirc;ina konstruado de nuklea elektrostacio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">elektr&aacute;rna</trd>
  <trd lng="de">Kraftwerk</trd>
  <trd lng="fr">centrale &eacute;lectrique</trd>
  <trd lng="sk">elektr&aacute;re&ncaron;</trd>
</drv>

<drv mrk="staci.kosmo0o">
  <kap>kosmo<tld/>o, <var><kap>kosma <tld/>o</kap></var></kap>
  <snc mrk="staci.kosmo0o.AVI">
    <uzo tip="fak">AVI</uzo>
    <dif>
      Iom granda, longe da&ubreve;ra konstrua&jcirc;o en la kosmo,
      eventuale lo&gcirc;ebla:
      <ekz>
        anstata&ubreve; en metalaj kosmo<tld/>oj astrona&ubreve;toj povus orbiti la 
        teron en balonoj la&ubreve; novaj planoj anoncitaj de usona 
        kosmagentejo NASA 
        <fnt>
          <aut>Arono kaj Karlina</aut>,
          <vrk><url ref="http://radioverda.com/programoj/2010/3/14/rv161-20100314.html">
          Radio Verda</url></vrk>,
          <lok>RV161, 2010-03-14</lok>
        </fnt>;
      </ekz>
      <ekz>
        la internacia kosmo<tld/>o dum dek jaroj devis jam ok fojojn &scirc;an&gcirc;i sian itineron
        por eviti kolizion
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2009/009929.php">Du mil na&ubreve;</url></vrk>
          <lok>2009</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Raumstation</trd>
  <trd lng="fr">station spatiale</trd>
</drv>

<drv mrk="staci.metro0o">
  <kap>metro<tld/>o,
  <var><kap>metroa <tld/>o</kap></var></kap>
  <snc mrk="staci.metro0o.TRA">
    <uzo tip="fak">TRA</uzo>
    <dif>
      <ref tip="super" cel="staci.0o.haltejo"><tld lit="S"/>o</ref>,
      kie metroo haltas por preni a&ubreve; lasi la
      pasa&gcirc;erojn:
      <ekz>
        iu ordenano verkis romanon <ctl>Rapsodio pri metro<tld/>o</ctl>,
        kiun <nom>Pi&ccirc;</nom> priskribas
        <fnt>
          <aut>C. Declerck</aut>,
          <vrk>Pliaj novaj vortoj poiradas!</vrk>,
          <bib>Monato</bib>,
          <lok><url ref="http://bertilow.com/tekstaro/montri.html?nomo=artikoloj_el_monato_3">
          Tekstaro de Esperanto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;efe en la metro<tld/>oj, pasa&gcirc;erojn sie&gcirc;as
        distribuantoj de varbfolioj
        <fnt>
          <aut>R. Dobrzy&nacute;ski</aut>,
          <vrk>La herooj de la lingva imperiismo</vrk>,
          <bib>LOdE</bib>,
          <lok>jaro 2003a, numero 12a</lok>
        </fnt>;
      </ekz>
    <ekz>
      kristnaska arbo en la metroa <tld/>o <nom>Seokkye</nom>
      <fnt>
        <aut>B. Wennergren</aut>,
        <vrk>Kristnaska&jcirc;oj</vrk>,
        <lok><url ref="http://bertilow.com/blogo/index.php?p=38">BBBlogo
        </url></lok>
      </fnt>.
    </ekz>      
    </dif>
  </snc>
  <trd lng="cs">stanice metra</trd>
  <trdgrp lng="de">
    <trd>Metrostation</trd>,
    <trd>U-Bahnstation</trd>
  </trdgrp>
  <trd lng="fr">station de m&eacute;tro</trd>
  <trd lng="pl">stacja metra</trd>
  <trd lng="pt">esta&ccedil;&atilde;o de metr&ocirc;</trd>
  <trd lng="sk">stanica metra</trd>
</drv>

<drv mrk="staci.posxto0o">
  <kap>po&scirc;to<tld/>o</kap>
  <snc mrk="staci.posxto0o.POSX">
    <uzo tip="fak">POSX</uzo>
    <dif>
      Stacio de po&scirc;taj veturiloj:
      <ekz>
        preninte &ccirc;evalojn, ni veturis al la po&scirc;to<tld/>o <nom>Koori</nom>
        <fnt>
          <aut>Basyoo, trad. Sasaki T.</aut>,
          <vrk><url ref="http://members.jcom.home.ne.jp/verda/sasaki%20okunohosomiti.pdf">
          Streta vojo al fora interno</url></vrk>,
          <lok>[vidita en 2011]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Poststation</trd>
  <trd lng="fr">relai de poste</trd>
</drv>

<drv mrk="staci.radi0o">
  <kap>radi<tld/>o, <var><kap>radio<tld/>o</kap></var></kap>
  <snc mrk="staci.radi0o.TEK">
    <uzo tip="fak">RAD</uzo>
    <dif>
      <ref tip="super" cel="staci.0o.TEK"><tld lit="S"/>o</ref> kiu
      elsendas radiofonie:
      <ekz>
        informi la veterinaron <klr>[&#8230;]</klr> ili faris tion
        multe pli rapide ol &ccirc;iuj radio<tld/>oj de la Nordo
        <fnt><bib>&Ccirc;uk&ccirc;oj</bib>, <lok>la trimova &scirc;akproblemo, 1.
        Kakume</lok></fnt>;
      </ekz>
      <ekz>
        la radi<tld/>o elsendas Esperanto-muzikon dum 24 horoj en &ccirc;iu
        tago
        <fnt>
          <aut>L. Poshivana</aut>,
          <vrk>eroj</vrk>,
          <lok>fido7.esperanto.rus, 2003-04-26</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Rundfunkstation</trd>,
    <trd>Radiosender</trd>
  </trdgrp>
  <trd lng="fr">station radiophonique</trd>
  <trd lng="hu">r&aacute;di&oacute;&aacute;llom&aacute;s</trd>
</drv>

<drv mrk="staci.televid0o">
  <kap>televid<tld/>o</kap>
  <snc mrk="staci.televid0o.TEK">
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="staci.0o.TEK"><tld lit="S"/>o</ref> kiu
      dissendas televidprogramojn:
      <ekz>
        ekde 1996 ekzistas televid<tld/>o irlandlingva
        <fnt><bib>Viki</bib>, <lok>artikolo &leftquot;irlanda lingvo&rightquot;,
        2005-07-21</lok></fnt>;
      </ekz>
      <ekz>
        la franc-germana televid<tld/>o Arte bon&scirc;ance programis mirindan filmon de la
        universale koramata <nom>Charles Chaplin</nom> &leftquot;La Diktatoro&rightquot;
        <fnt>
          <bib>Monato</bib>
          <aut>J.M. Parisot-Delourmel</aut>
          <vrk><url ref="https://www.monato.be/2004/008286.html">Charles Chaplin kaj Esperanto</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Fernsehsender</trd>
  </trdgrp>
  <trd lng="fr">station de t&eacute;l&eacute;vision</trd>
</drv>
</art>
