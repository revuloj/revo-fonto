<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: risort.xml,v 1.26 2023/10/21 05:38:06 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>risort</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="risort.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="risort.0o.MAS">
    <uzo tip="fak">MAS</uzo>
    <dif>
      Elasta lameno a&ubreve; fadeno el metalo a&ubreve; alia materialo, kapabla
      repreni la anta&ubreve;an formon, post kiam &gcirc;i estis fleksita,
      premita a&ubreve; stre&ccirc;ita:
      <ekz>
        <tld/>o de horlo&gcirc;o, de kanapo, de kale&scirc;o<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>a matraco<fnt>B</fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_p;&c_r;&c_u;&c_zh;&c_y;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_y;&c_s;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="ca">
    <trd>ressort</trd>,
    <trd>molla</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>pero</trd>,
    <trd>pru&zcaron;ina</trd>,
    <trd>p&eacute;ro</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Feder</trd>,
    <trd>Sprungfeder</trd>
  </trdgrp>
  <trd lng="en">spring</trd>
  <trd lng="es">resorte</trd>
  <trd lng="fa">&a_f;&a_n;&a_r;</trd>
  <trd lng="fr">ressort</trd>
  <trd lng="hu">rug&oacute;</trd>
  <trdgrp lng="ja">
    <trd>&#x3070;&#x306D;</trd>,
    <trd>&#x305C;&#x3093;&#x307E;&#x3044;</trd>,
    <trd>&#x30B9;&#x30D7;&#x30EA;&#x30F3;&#x30B0;</trd>
  </trdgrp>
  <trd lng="nl">veer</trd>
  <trdgrp lng="pl">
    <trd>resor</trd>,
    <trd>spr&eogonek;&zdot;yna</trd>
  </trdgrp>
  <trd lng="pt">mola</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_r;&c_u;&c_zh;&c_i;&c_n;&c_a;</trd>,
    <trd>&c_r;&c_je;&c_s;&c_s;&c_o;&c_r;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>pero</trd>,
    <trd>pru&zcaron;ina</trd>,
    <trd>struna</trd>
  </trdgrp>
  <trd lng="sv">fj&auml;der</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_r;&c_u;&c_zh;&c_i;&#x43D;&c_a;</trd>,
    <trd>&c_r;&c_je;&c_s;&#x43E;&c_r;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="risort.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="risort.0aro.AUT">
    <uzo tip="fak">AUT</uzo>
    <dif>
      Tuto de <tld/>oj a&ubreve; similaj rimedoj,
      kiuj mildigas la skuojn de veturilo
      a&ubreve; alia ma&scirc;ino:
      <ekz>
        &ccirc;e nia kariolo, vi sciu, <tld/>aro difekti&gcirc;is
        <fnt>
          <aut>A. &Ccirc;e&hcirc;ov, trad. A. Kor&jcirc;enkov</aut>,
          <vrk><url ref="&LOdE;Libroj/V_bibl02.pdf">
          Sveda alumeto</url></vrk>,
          <lok>2004-2010</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_d;&c_v;&c_je;&c_s;&c_k;&c_a; <klr>(&c_t;&c_r;&c_a;&c_n;&c_s;&c_p;&c_a;&c_r;&c_t;&c_n;&c_a;&c_g;&c_a; &c_s;&c_r;&c_o;&c_d;&c_k;&c_u;)</klr></trd>
  <trd lng="cs">p&eacute;rov&aacute;n&iacute; <klr>(vozidla)</klr></trd>
  <trd lng="fr">syst&egrave;me de <ind>suspension</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x30B5;&#x30B9;&#x30DA;&#x30F3;&#x30B7;&#x30E7;&#x30F3;</trd>,
    <trd>&#x61F8;&#x67B6;&#x88C5;&#x7F6E;  <pr>&#x304B;&#x304B;&#x304B;&#x304B;&#x305D;&#x3046;&#x3061;</pr></trd>
  </trdgrp>
  <trd lng="pl">resorowanie</trd>
  <trdgrp lng="sk">
    <trd>pru&zcaron;enie <klr>(auta)</klr></trd>,
    <trd>p&eacute;rovanie</trd>
  </trdgrp>
  <trd lng="uk">&c_r;&c_je;&c_s;&#x43E;&c_r;&c_i;</trd>
</drv>

<drv mrk="risort.0igxi">
  <kap><tld/>i&gcirc;i</kap>
  <snc>
    <dif>
      Reagi kiel <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="salt.re0i">resalti</ref>
    </refgrp>
    <refgrp tip="vid">
      <ref cel="kauxcx.0o">ka&ubreve;&ccirc;uko</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_s;&c_p;&c_r;&c_u;&c_zh;&c_y;&c_n;&c_ib;&c_c;&c_mol;</trd>
  <trd lng="ca">rebotar</trd>
  <trdgrp lng="de">
    <trd>federn</trd>,
    <trd>abfedern</trd>,
    <trd>zur&uuml;ckfedern</trd>
  </trdgrp>
  <trd lng="hu">rug&oacute;zik</trd>
  <trdgrp lng="ja">
    <trd>&#x8DF3;&#x306D;&#x8FD4;&#x308B;  <pr>&#x306F;&#x306D;&#x304B;&#x3048;&#x308B;</pr></trd>,
    <trd>&#x306F;&#x305A;&#x3080;</trd>
  </trdgrp>
  <trd lng="pl">spr&eogonek;&zdot;ynowa&cacute;</trd>
  <trd lng="ru">&c_p;&c_r;&c_u;&c_zh;&c_i;&c_n;&c_i;&c_t;&c_mol;</trd>
</drv>
</art>
