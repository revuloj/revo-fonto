<art mrk="$Id: elitr.xml,v 1.20 2023/10/14 15:49:04 revo Exp $">
<kap>
  <rad>elitr</rad>/o
</kap>
<drv mrk="elitr.0o">
<kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      &Ccirc;iu el la eksteraj sur<ref tip="super" cel="flug.0ilo.birda">flugiloj</ref>, 
      kiuj kovras la alian, veran paron da flugiloj &ccirc;e multaj insektoj:
      <ekz>
        koleopteroj<klr>[n&#x2026;]</klr> karakterizas malmolaj anta&ubreve;aj flugiloj
        (<tld/>oj), ne ta&ubreve;gaj por flugado, nur la posta paro, kiun la <tld/>oj &scirc;irmas, estas uzata por flugado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=9458">Koleopteroj</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_d;&c_k;&c_r;&c_y;&c_l;&c_mol;&c_l;&c_je;</trd>
  <trd lng="bg">&c_je;&c_l;&c_i;&c_t;&c_r;&c_a;</trd>
  <trd lng="ca">&egrave;litre</trd>
  <trd lng="cs">krovka</trd>
  <trdgrp lng="de">
   <trd>Deckfl&uuml;gel</trd>,
   <trd>Fl&uuml;geldecke</trd>
  </trdgrp>
  <trd lng="es">&eacute;litro</trd>
  <trd lng="fa">&a_b;&a_A;&a_l;&zwnj;&f_p;&a_w;&a_sh;</trd>
  <trd lng="fr">&eacute;lytre</trd>
  <trd lng="hu">kem&eacute;ny <ind>sz&aacute;rnyfed&eacute;l</ind></trd>
  <trdgrp lng="ja">
    <trd>&#x7FC5;&#x9798;  <pr>&#x3057;&#x3057;&#x3087;&#x3046;</pr></trd>,
    <trd>&#x3055;&#x3084;&#x3070;&#x306D;</trd>
  </trdgrp>
  <trd lng="nl">dekschild</trd>
  <trd lng="pl">pokrywa<klr tip="amb"> skrzyd&lstroke;owa</klr></trd>
  <trd lng="pt">&eacute;litro</trd>
  <trd lng="ru">&c_n;&c_a;&c_d;&c_k;&c_r;&c_y;&c_l;&c_mol;&c_je;</trd>
  <trd lng="sk">krovka <klr>(hmyzu)</klr></trd>
  <trd lng="uk">&#x43D;&#x430;&#x434;&#x43A;&#x440;&#x438;&#x43B;&#x430; (&#x443; &#x436;&#x443;&#x43A;&#x456;&#x432;)</trd>
</drv>
</art>
