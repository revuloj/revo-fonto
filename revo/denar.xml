<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>denar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="denar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="denar.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Antikva romia <ref tip="lst" cel="unu.mon0o"
      lst="voko:monunuoj">monunuo</ref>:
      <ekz>
        kial oni ne vendis &ccirc;i tiun &scirc;mira&jcirc;on por tricent
        <tld/>oj kaj donis al malri&ccirc;uloj?
        <fnt><bib>NT</bib>, <lok>&Joh; 12:5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_d;&c_y;&c_n;&c_a;&c_r;</trd>
  <trd lng="bg">&c_d;&c_je;&c_n;&c_a;&c_r;&c_i;&c_j;</trd>
  <trd lng="ca">denari</trd>
  <trd lng="de">Dinar</trd>
  <trd lng="en">denarius</trd>
  <trd lng="es">denario</trd>
  <trd lng="fr">denier</trd>
  <trd lng="hu">d&eacute;n&aacute;r</trd>
  <trd lng="lat">denarius</trd>
  <trd lng="pl">denar</trd>
  <trd lng="pt">den&aacute;rio</trd>
  <trd lng="ru">&c_d;&c_i;&c_n;&c_a;&c_r;&c_i;&c_j;</trd>
</drv>
</art>
