  <trd lng="en">denarius</trd>
  <trd lng="es">denario</trd>
  <trd lng="fr">denier</trd>
  <trd lng="hu">d&eacute;n&aacute;r</trd>
  <trd lng="id">dinar</trd>
  <trd lng="pl">denar</trd>
  <trd lng="pt">den&aacute;rio</trd>
  <trd lng="ru">&c_d;&c_i;&c_n;&c_a;&c_r;&c_i;&c_j;</trd>
