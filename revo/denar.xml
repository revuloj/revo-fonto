<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>denar</rad>/o
</kap>
<drv mrk="denar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      Speco de antikva roma <ref tip="super" cel="unu.mon0o">monunuo</ref>:
      <ekz>
        kial oni ne vendis &ccirc;i tiun &scirc;mira&jcirc;on por tricent
        <tld/>oj kaj donis al malri&ccirc;uloj?
        <fnt><bib>X</bib>, <lok>&Joh; 12:5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">denier</trd>
  <trd lng="ru">&c_d;&c_i;&c_n;&c_a;&c_r;&c_i;&c_j;</trd>
</drv>
</art>
