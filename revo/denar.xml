  <ofc>*</ofc>
  <rad>denar</rad>/o <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="denar.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="denar.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Antikva romia <ref tip="lst" cel="unu.mon0o" lst="voko:monunuoj">monunuo</ref>:
      <ekz>
        kial oni ne vendis &ccirc;i tiun &scirc;mira&jcirc;on por tricent
        <tld/>oj kaj donis al malri&ccirc;uloj?
        <fnt><bib>NT</bib>, <lok>&Joh; 12:5</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="he">
    <trd>&#x5DB;&#x5BC;&#x5B6;&#x5E1;&#x5B6;&#x5E3;</trd>,
    <trd>&#x5DE;&#x5D8;&#x5D1;&#x5E2; &#x5D9;&#x5E9;&#x5DF;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30C7;&#x30CA;&#x30EA;&#x30A6;&#x30B9;&#x9280;&#x8CA8;  <pr>&#x30C7;&#x30CA;&#x30EA;&#x30A6;&#x30B9;&#x304E;&#x3093;&#x304B;</pr></trd>,
    <trd>&#x30C7;&#x30CB;&#x30FC;&#x30EB;</trd>
  </trdgrp>
  <trd lng="lat">denarius</trd>
  <trd lng="be">&c_d;&c_y;&c_n;&c_a;&c_r;</trd>
  <trd lng="bg">&c_d;&c_je;&c_n;&c_a;&c_r;&c_i;&c_j;</trd>
  <trd lng="ca">denari</trd>
  <trd lng="cs">den&#xE1;r</trd>
