<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>6</ofc>
  <rad>aplomb</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="aplomb.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Tre granda memfido, nekonsternebleco:
      <ekz>
        sin prezenti kun <tld/>o;
      </ekz>
      <ekz>
        <tld/>e aserti malvera&jcirc;on.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="arogan">aroganteco</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="fr">
    <trd>aplomb</trd>,
    <trd>assurance</trd>,
    <trd>toupet</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_a;&c_m;&c_o;&c_u;&c_v;&c_je;&c_r;&c_je;&c_n;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>,
    <trd>&c_a;&c_p;&c_l;&c_o;&c_m;&c_b;</trd>
  </trdgrp>
</drv>
</art>
