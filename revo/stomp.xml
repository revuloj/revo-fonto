<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>stomp</rad>/i
  <fnt>V</fnt>
</kap>
<drv mrk="stomp.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">BELA</uzo>
    <dif>
      Iom post iom malpliintensigi la ombrumadon a&ubreve; la helecon de desegno.
    </dif>
    <trdgrp lng="hu">
      <trd>eld&ouml;rzs&ouml;l</trd>,
      <trd>&aacute;rnyal</trd>,
      <trd>ki&aacute;rny&eacute;kol</trd>,
      <trd>&ouml;sszemos</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iom post iom malpliintensigi, mildigi.
    </dif>
  </snc>
  <trd lng="fr">estomper</trd>
  <trd lng="hu">csillap&iacute;t</trd>  
  <trdgrp lng="nl">
    <trd>doezelen</trd>,
    <trd>verdoezelen</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>estompieren</trd>,
    <trd>wischen</trd>
  </trdgrp>  
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_u;&c_sh;&c_jo;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_sh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_u;&c_sh;&c_jo;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_u;&c_sh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>

<drv mrk="stomp.0ilo">
  <kap><tld/>ilo</kap>
  <snc>
    <dif>
      Speciala ilo por <tld/>i.
    </dif>
  </snc>
  <trd lng="fr">estompe</trd>
  <trdgrp lng="hu">
    <trd>d&ouml;rzs&ouml;l&odblac;</trd>,
    <trd>viser</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_u;&c_sh;&c_jo;&c_v;&c_k;&c_a; <klr tip="amb">(&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
</drv>
</art>
