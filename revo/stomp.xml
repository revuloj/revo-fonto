<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stomp.xml,v 1.22 2022/11/11 13:03:00 revo Exp $">
<kap>
  <rad>stomp</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="stomp.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="stomp.0i.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      Iom post iom malpliintensigi la ombrumadon a&ubreve; la helecon de
      desegno.
    </dif>
    <refgrp tip="vid">
      <ref cel="damp.0i.MUZ">dampi<sncref/></ref>,
      <ref cel="akr.mal0igi">malakrigi</ref>,
      <ref cel="nebul.0igi.FIG">nebuligi<sncref/></ref>,
      <ref cel="ombr.0umi.BELA">ombrumi<sncref/></ref>,
      <ref cel="pal.0igi">paligi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>estompieren</trd>,
      <trd>lavieren</trd>,
      <trd>wischen</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>eld&ouml;rzs&ouml;l</trd>,
      <trd>&aacute;rnyal</trd>,
      <trd>ki&aacute;rny&eacute;kol</trd>,
      <trd>&ouml;sszemos</trd>
    </trdgrp>
  </snc>
  <snc mrk="stomp.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Iom post iom malpliintensigi, <ref tip="vid" cel="mild.0igi">mildigi</ref>:
      <ekz>
        la griza feliso ridetis por <tld/>i sian hele verdan grimacon
        <fnt>
          <bib>MMa</bib>
        </fnt>;
      </ekz>
      <ekz>
        la ebeno, pli malpli strikte kvadratita, estas <tld/>ita 
        de la miksa&jcirc;o de sta&ubreve;doj kaj unujaraj plantoj 
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=295106">&Gcirc;ardeno de parokestro</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam <nom>Georg Silber</nom>, unu matenon, veki&gcirc;is el <tld/>a <klr>(obtuza)</klr> son&gcirc;o, li
        retrovis sin en sia lito transformi&gcirc;inta en nenian monstran insekton
        <fnt>
          <bib>MMa</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="kasx.0i">ka&scirc;i</ref>
    </refgrp>    <trdgrp lng="de">
      <trd>abmildern</trd>,
      <trd>verwischen</trd>
    </trdgrp>
  </snc>
  <refgrp tip="ant">
    <ref cel="kontra8.0igi.KOMUNE">kontrastigi</ref>
  </refgrp>
  <trdgrp lng="en">
    <trd>dampen<klr tip="amb"> (drawing)</klr></trd>,
    <trd>soften<klr tip="amb"> (drawing)</klr></trd>,
    <trd>blend<klr tip="amb"> (drawing)</klr></trd>,
    <trd>smudge<klr tip="amb"> (drawing)</klr></trd>
  </trdgrp>
  <trd lng="fr">estomper</trd>
  <trd lng="hu">csillap&iacute;t</trd>
  <trdgrp lng="nl">
    <trd>doezelen</trd>,
    <trd>verdoezelen</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_u;&c_sh;&c_jo;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_s;&c_t;&c_u;&c_sh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_u;&c_sh;&c_jo;&c_v;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_u;&c_sh;&c_je;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">zoslabova&tcaron; tienenie v&nbsp;kresbe</trd>
  <trdgrp lng="cs">
    <trd>st&iacute;novat</trd>,
    <trd>t&ecaron;rkovat</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_r;&#x43E;&c_z;&c_t;&c_u;&c_sh;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&c_a;&c_t;&c_u;&c_sh;&#x43E;&c_v;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_z;&#x43C;&#8217;&#x44F;&#x43A;&c_sh;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
</drv>

<drv mrk="stomp.0igxi">
  <kap>
    <tld/>i&gcirc;i
  </kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="stomp.0igxi.paligxi">
    <dif>
      <ref tip="dif" cel="pal.0igxi">Pali&gcirc;i</ref>, ombrumi&gcirc;i, 
      malneti&gcirc;i, ricevi neprecizan formon:
      <ekz>
        &ccirc;iumomente la sono &scirc;an&gcirc;i&gcirc;is, <tld/>i&gcirc;is por indiki ombron, kaj reneti&gcirc;is por sekvi
        linion pli precizan
        <fnt>
          <aut>M. Proust, trad. J.-L. Tortel</aut>
          <vrk>Unu amo de Swann</vrk>
          <lok>2019</lok>
        </fnt>;
      </ekz>
      <ekz>
        pasis kelkaj jaroj, <tld/>i&gcirc;is la eventoj <klr>[&#8230;]</klr> kaj poste forvi&scirc;i&gcirc;is el
        la memoro
        <fnt>
          <bib>MkM</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>verwischen</trd>,
    <trd>verblassen</trd>
  </trdgrp>
  <trd lng="fr">s'<ind>estomper</ind></trd>
</drv>

<drv mrk="stomp.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="stomp.0ilo.BELA">
    <dif>
      Ilo por <tld/>i.
    </dif>
    <refgrp tip="vid">
              <ref cel="skrap.0ilo">skrapilo</ref>
    </refgrp>
    <bld lok="&WCU;/thumb/f/f8/Estompe.JPG/640px-Estompe.JPG" prm="CC BY-SA 3.0"><tld/>ilo
        <fnt>
          <aut>Stephhzz, propra verko</aut>
          <lok><url ref="&WCW;/File:Estompe.JPG">&WCW;/File:Estompe.JPG</url></lok>
        </fnt>
    </bld>
    <trdgrp lng="de">
      <trd>Papierwischer</trd>,
      <trd>Estompe</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="en">
    <trd>tortillon</trd>,
    <trd>blending stick</trd>,
  </trdgrp>
  <trd lng="fr">estompe</trd>
  <trdgrp lng="hu">
    <trd>d&ouml;rzs&ouml;l&odblac;</trd>,
    <trd>viser</trd>
  </trdgrp>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_t;&c_u;&c_sh;&c_jo;&c_v;&c_k;&c_a; <klr tip="amb">(&c_i;&c_n;&c_s;&c_t;&c_r;&c_u;&c_m;&c_je;&c_n;&c_t;)</klr></trd>
</drv>
</art>
