<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>abrikot</rad>/o
</kap>
<drv mrk="abriko.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Flavoran&gcirc;a frukto de <ref tip="malprt"
      cel="abriko.0ujo0arbo"><tld/>ujo</ref>,
      <ref tip="super" cel="drup.0o">drupo</ref>.
    </dif>
  </snc>
  <trd lng="de">Aprikose</trd>
  <trd lng="en">apricot</trd>
  <trd lng="es">albaricoque</trd>
  <trd lng="fr">abricot</trd>
  <trdgrp lng="hu">
    <trd>s&aacute;rgabarack</trd>,
    <trd>kajszibarack</trd>
  </trdgrp>
  <trd lng="it">albicocca</trd>
  <trd lng="tr">kay&inodot;s&inodot;</trd>
  <trd lng="nl">abrikoos</trd>
  <trd lng="pl">morela</trd>
  <trdgrp lng="pt">
    <trd>abric&oacute;</trd>,
    <trd>abricote</trd>
  </trdgrp>
  <trd lng="ru">&c_a;&c_b;&c_r;&c_i;&c_k;&c_o;&c_s;</trd>
  <trd lng="sv">aprikos</trd>
</drv>

<drv mrk="abriko.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="prunus.0o">prunuso</ref>
      (<trd lng="la">Prunus armeniaca</trd>).
    </dif>
  </snc>
  <trd lng="nl">abrikozenboom</trd>
  <trd lng="en">apricot (tree)</trd>
  <trd lng="de">Aprikosenbaum</trd>
  <trd lng="es">albaricoquero</trd>
  <trd lng="fr">abricotier</trd>
  <trdgrp lng="hu">
    <trd>s&aacute;rgabarackfa</trd>,
    <trd>kajszibarackfa</trd>
  </trdgrp>
  <trd lng="pt">abricoteiro</trd>
  <trd lng="tr">kay&inodot;s&inodot;</trd>
  <trd lng="ru">&c_a;&c_b;&c_r;&c_i;&c_k;&c_o;&c_s;&c_o;&c_v;&c_o;&c_je;
&c_d;&c_je;&c_r;&c_je;&c_v;&c_o;</trd>
  <trd lng="sv">aprikostr&auml;d</trd>
</drv>
</art>
