<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>abrikot</rad>/o
</kap>
<drv mrk="abriko.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Frukto de <ref tip="malprt" cel="abriko.0ujo0arbo"><tld/>ujo</ref>.
    </dif>
  </snc>
  <trd lng="de">Aprikose</trd>
  <trd lng="en">apricot</trd>
  <trd lng="fr">abricot</trd>
  <trd lng="it">albicocca</trd>
  <trd lng="tr">kay&inodot;s&inodot;</trd>
  <trd lng="nl">abrikoos</trd>
  <trd lng="pl">morela</trd>
  <trd lng="ru">&#1072;&#1073;&#1088;&#1080;&#1082;&#1086;&#1089;</trd>
</drv>
<drv mrk="abriko.0ujo0arbo">
  <kap><tld/>ujo, <tld/>arbo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="prunus.0o">prunuso</ref>
      (<trd lng="la">Prunus  armeniaca</trd>).
    </dif>
  </snc>
  <trd lng="en">apricot (tree)</trd>
  <trd lng="de">Aprikosenbaum</trd>
  <trd lng="fr">abricotier</trd>
  <trd lng="tr">kay&inodot;s&inodot;</trd>
  <trd

  
lng="ru">&#1072;&#1073;&#1088;&#1080;&#1082;&#1086;&#1089;&#1086;&#1074;&#1086;&#1077;
  &#1076;&#1077;&#1088;&#1077;&#1074;&#1086;</trd>
</drv>
</art>
