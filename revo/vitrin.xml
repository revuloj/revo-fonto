<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>vitrin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vitrin.0o">
  <kap><tld/>o</kap>
  <snc mrk="vitrin.0o.KOMUNE">
    <dif>
      Vitra instala&jcirc;o por elmontri objektojn.
    </dif>
    <subsnc mrk="vitrin.0o.MEBL">
      <dif>
        Elmontra, parte vitra
        <ref tip="lst" cel="mebl.0o.MEBL" lst="voko:mebloj">meblo</ref>:
        <ekz>
          scienca kurioza&jcirc;o gardata en luksa <tld/>o de l'
          Antropologia Muzeo de Madrid
          <fnt>
            <aut>J. el Coronel Ignotus, trad. J. Fernando
            Berenguer</aut>,
            <vrk>la amo en la jaro dekmil</vrk>,
            <lok>Heroldo de Esperanto, 1932:2 (655)</lok>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="vitrin.0o.montrofenestro">
      <ref tip="dif" cel="fenest.montro0o">montrofenestro</ref>
      <ekz>
        ili drivis de magazeno al magazeno babilante kun gaja 
        senrespondeco pri frivola&jcirc;oj dum &scirc;i rigardis de tempo al tempo 
        la <tld/>ojn
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </subsnc>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_t;&c_r;&c_y;&c_n;&c_a;</trd>
  <trd lng="bg">&c_v;&c_i;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>prosklen&aacute; sk&rcaron;&iacute;&ncaron;</trd>,
    <trd>vitr&iacute;na</trd>,
    <trd>zasklen&aacute; n&aacute;st&ecaron;nka</trd>
  </trdgrp>
  <trd lng="de">Vitrine</trd>
  <trdgrp lng="el">
    <trd>&beta;&jota;&tau;&rho;&jota_ton;&ny;&alfa;</trd>,
    <trd>&pi;&rho;&omikron;&theta;&eta_ton;&kappa;&eta;</trd>
  </trdgrp>
  <trd lng="en">glass show-case</trd>
  <trd lng="fr">vitrine</trd>
  <trd lng="hu">vitrin</trd>
  <trd lng="nl">vitrine</trd>
  <trdgrp lng="pl">
    <trd>witryna</trd>,
    <trd>gablota</trd>,
    <trd>gablotka</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>vitrina</trd>,
    <trd>vitrine</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">vitr&iacute;na</trd>
  <trd lng="sv">vitrinsk&aring;p</trd>
</drv>
</art>
