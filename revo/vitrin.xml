<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: vitrin.xml,v 1.20 2022/05/06 12:17:22 revo Exp $">
<kap>
  <rad>vitrin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="vitrin.0o">
  <kap><tld/>o</kap>
  <snc mrk="vitrin.0o.KOMUNE">
    <dif>
      Vitra instala&jcirc;o por elmontri objektojn.
    </dif>
    <subsnc mrk="vitrin.0o.MEBL">
      <dif>
        Elmontra, parte vitra
        <ref tip="lst" cel="mebl.0o.MEBL" lst="voko:mebloj">meblo</ref>:
        <ekz>
          scienca kurioza&jcirc;o gardata en luksa <tld/>o de l'
          Antropologia Muzeo de <nom>Madrid</nom>
          <fnt>
            <aut>J. el Coronel Ignotus, trad. J. Fernando
            Berenguer</aut>,
            <vrk>la amo en la jaro dekmil</vrk>,
            <lok>Heroldo de Esperanto, 1932:2 (655)</lok>
          </fnt>;
        </ekz>
        <ekz>
          13 surmuraj informpaneloj, 5 tablo<tld/>oj kaj vico de vitro&scirc;rankoj
          <fnt>
            <bib>Monato</bib>
            <aut>Petro Chrdle</aut>
            <vrk><url ref="&Monato;/2009/009712.php">Nova Esperanto-muzeo</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          la libron oni gardas sub apartaj kondi&ccirc;oj, en ejo kun speciale prilaborita
          <tld/>o
          <fnt>
            <bib>Monato</bib>
            <aut>Dimitrije Jani&ccirc;i&ccirc;</aut>
            <vrk><url ref="&Monato;/2012/010763.php">Hagado denove anta&ubreve; publiko</url></vrk>
          </fnt>;
        </ekz>
        <ekz>
          mi ekadmiris la lo&gcirc;ejon de veraj ri&ccirc;uloj <klr>[&#x2026;]</klr>
          plenplena de persaj tapi&scirc;oj, en la <tld/>oj brilis la kristaloj, sur la
          muroj pendis grandegaj kaj belegaj pentra&jcirc;oj
          <fnt>
            <bib>Monato</bib>
            <aut>J&uacute;lia Sigmond</aut>
            <vrk><url ref="&Monato;/2009/009693.php">La mistera nokto</url></vrk>
          </fnt>;
        </ekz>
      </dif>
    </subsnc>
    <subsnc mrk="vitrin.0o.montrofenestro">
      <dif>
        <ref tip="dif" cel="fenest.montro0o">Montrofenestro</ref>:
        <ekz>
          ili drivis de magazeno al magazeno babilante kun gaja 
          senrespondeco pri frivola&jcirc;oj dum &scirc;i rigardis de tempo al tempo 
          la <tld/>ojn
          <fnt>
            <aut>J. Francis</aut>,
            <vrk>La Granda Kaldrono</vrk>,
            <lok>1978</lok>
          </fnt>;
        </ekz>
        <ekz>
          sur la muroj de la konstrua&jcirc;oj, sur la <tld/>oj de la vendejoj kaj sur la
          fenestroj ludis kuprokoloraj briloj
          <fnt>
            <bib>Monato</bib>
            <aut>Julian Modest</aut>
            <vrk><url ref="&Monato;/2014/011227.php">La piano</url></vrk>
          </fnt>.
        </ekz>
      </dif>
    </subsnc>
  </snc>
  <trd lng="be">&c_v;&c_ib;&c_t;&c_r;&c_y;&c_n;&c_a;</trd>
  <trd lng="bg">&c_v;&c_i;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>prosklen&aacute; sk&rcaron;&iacute;&ncaron;</trd>,
    <trd>vitr&iacute;na</trd>,
    <trd>zasklen&aacute; n&aacute;st&ecaron;nka</trd>
  </trdgrp>
  <trd lng="de">Vitrine</trd>
  <trdgrp lng="el">
    <trd>&beta;&jota;&tau;&rho;&jota_ton;&ny;&alfa;</trd>,
    <trd>&pi;&rho;&omikron;&theta;&eta_ton;&kappa;&eta;</trd>
  </trdgrp>
  <trd lng="en">glass show-case</trd>
  <trd lng="fr">vitrine</trd>
  <trd lng="hu">vitrin</trd>
  <trd lng="nl">vitrine</trd>
  <trdgrp lng="pl">
    <trd>witryna</trd>,
    <trd>gablota</trd>,
    <trd>gablotka</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>vitrina</trd>,
    <trd>vitrine</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_i;&c_t;&c_r;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">vitr&iacute;na</trd>
  <trd lng="sv">vitrinsk&aring;p</trd>
  <trd lng="uk">&#x432;&#x456;&#x442;&#x440;&#x438;&#x43D;&#x430;</trd>
</drv>
</art>
