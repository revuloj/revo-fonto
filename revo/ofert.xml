<drv mrk="ofert.0o">
  <kap><tld/>o</kap>
  <snc mrk="ofert.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="propon.0o">Propono</ref> por a&ccirc;eti varon a&ubreve; ricevi servon
      kontra&ubreve; difinita prezo:
      <ekz>
        malmultekosta <tld/>o
        <fnt>
          <aut>Franck Arnaud</aut>
            <vrk><url ref="&Monato;/2000/007114.html">Intel alfrontas konkurencon</url></vrk>
           <bib>Monato</bib>
            <lok>2000/10, p. 16</lok>
          </fnt>;
      </ekz>
      <ekz>
        la&ubreve; merkata enketo inter la <tld/>o kaj bezono de laktaj produktoj jam estas
        ekvilibro
        <fnt>
       <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2000/007169.php">Lakto ne &scirc;atata</url></vrk>
          <bib>Monato</bib>
          <lok>2000/12, p. 11</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> faris multajn <tld/>ojn, por ke oni akiru iliajn produktojn
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="&Monato;/2008/009416.php">Libereco, &gcirc;ojiga ne por
          &ccirc;iuj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro la malpli granda <tld/>o la prezoj de porkoviando levi&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="&Monato;/2007/009328.php">Registaro asertas, ke man&gcirc;a&jcirc;oj estas
          sekuraj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
