<drv mrk="ofert.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ofert.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="propon.0i">Proponi</ref> al iu varon
      a&ubreve; servon a&ccirc;etotan:
      <ekz>
        vendejoj komencis <tld/>i fre&scirc;ajn man&gcirc;a&jcirc;ojn kontra&ubreve; 100 enoj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="https://www.monato.be/2006/008730.php">Vendejen!</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu restoracio devas <tld/>i minimume unu germanan speciala&jcirc;on, ekzemple la
        tipan porkpiedon kun terpomka&ccirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007174.html">Kiel savi la germanan
          kulturon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Albanio <tld/>as por turistoj tre bonajn naturajn kondi&ccirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2003/007925.html">Voja&gcirc;impresoj, somero 2002: Kosovo
          anta&ubreve;eniras</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        bankoj <klr>[&#8230;]</klr> krediton <tld/>as nur kontra&ubreve; alta interezo
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2003/008199.html">Novaj direktoj al la
          estonto</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ofert.0i.proponi">
    <dif>
      <klr>(pli &gcirc;enerale)</klr> <ref tip="dif" cel="propon.0i.por_dispono">Proponi<sncref/></ref>:
      <ekz>
        Usono ne havas pli bonan amikon ol Litovio, kiu <tld/>is grandan subtenon por
        Usono en la batalo kontra&ubreve; terorismo
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="https://www.monato.be/2004/008251.html">Amikeco longa kaj firma</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
