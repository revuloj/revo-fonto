<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: ofert.xml,v 1.30 2021/06/01 17:36:57 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>ofert</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ofert.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="ofert.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="propon.0i">Proponi</ref> al iu varon
      a&ubreve; servon a&ccirc;etotan:
      <ekz>
        vendejoj komencis <tld/>i fre&scirc;ajn man&gcirc;a&jcirc;ojn kontra&ubreve; 100 enoj
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="https://www.monato.be/2006/008730.php">Vendejen!</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iu restoracio devas <tld/>i minimume unu germanan speciala&jcirc;on, ekzemple la
        tipan porkpiedon kun terpomka&ccirc;o
        <fnt>
          <bib>Monato</bib>
          <aut>S. Maul</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007174.html">Kiel savi la germanan kulturon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Albanio <tld/>as por turistoj tre bonajn naturajn kondi&ccirc;ojn
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2003/007925.html">Voja&gcirc;impresoj, somero 2002: Kosovo anta&ubreve;eniras</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        bankoj <klr>[&#x2026;]</klr> krediton <tld/>as nur kontra&ubreve; alta interezo
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="https://www.monato.be/2003/008199.html">Novaj direktoj al la estonto</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="ofert.0i.proponi">
    <dif>
      <klr>(pli &gcirc;enerale)</klr> <ref tip="dif" cel="propon.0i.por_dispono">Proponi<sncref/></ref>:
      <ekz>
        Usono ne havas pli bonan amikon ol Litovio, kiu <tld/>is grandan subtenon por
        Usono en la batalo kontra&ubreve; terorismo
        <fnt>
          <bib>Monato</bib>
          <aut>LAST</aut>
          <vrk><url ref="https://www.monato.be/2004/008251.html">Amikeco longa kaj firma</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="af">aanbod</trd>
  <trd lng="am">&#x12A0;&#x1245;&#x122D;&#x1265;</trd>
  <trd lng="ar">&#x627;&#x644;&#x639;&#x631;&#x636;</trd>
  <trd lng="az">t&#x259;klif</trd>
  <trd lng="be">&c_p;&c_r;&c_a;&c_p;&c_a;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;
    <klr>(&c_t;&c_a;&c_v;&c_a;&c_r;,
    &c_p;&c_a;&c_s;&c_l;&c_u;&c_g;&c_u;)</klr></trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_l;&c_a;&c_g;&c_a;&c_m; (&c_s;&c_t;&c_o;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_s;&c_l;&c_u;&c_g;&c_o; &c_i; &c_t;.&c_n;.)</trd>
  </trdgrp>
  <trd lng="bo">&#xF51;&#xF56;&#xF74;&#xF63; &#xF60;&#xF56;&#xF74;&#xF63; &#xF51;&#xF56;&#xF74;&#xF63; &#xF55;&#xF74;&#xF63;</trd>
  <trd lng="ca">oferir</trd>
  <trdgrp lng="cs">
    <trd>nab&iacute;dnout <klr>(zbo&zcaron;&iacute;)</klr></trd>,
    <trd>navrhnout</trd>,
    <trd>oferovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anbieten</trd>,
    <trd>offerieren</trd>
  </trdgrp>
  <trd lng="en">offer</trd>
  <trdgrp lng="es">
    <trd>ofertar</trd>,
    <trd>ofrecer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>offrir</trd>,
    <trd>proposer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>aj&aacute;nlatot tesz</trd>,
    <trd>megaj&aacute;nl</trd>
  </trdgrp>
  <trd lng="hy">&#x561;&#x57C;&#x561;&#x57B;&#x561;&#x580;&#x56F;</trd>
  <trd lng="ne">&#x92A;&#x94D;&#x930;&#x938;&#x94D;&#x924;&#x93E;&#x935;</trd>
  <trdgrp lng="nl">
    <trd>een <ind>offerte</ind> maken voor</trd>,
    <trd>te koop <ind>aanbieden</ind></trd>
  </trdgrp>
  <trd lng="pt">ofertar</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;
  <klr>(&c_t;&c_o;&c_v;&c_a;&c_r;, &c_u;&c_s;&c_l;&c_u;&c_g;&c_u; &c_i;
  &c_t;.&c_p;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>navrhn&uacute;&tcaron;</trd>,
    <trd>pon&uacute;knu&tcaron;</trd>
  </trdgrp>
  <trd lng="sq">oferta</trd>
</drv>

<drv mrk="ofert.0o">
  <kap><tld/>o</kap>
  <snc mrk="ofert.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="propon.0o">Propono</ref> por a&ccirc;eti varon a&ubreve; ricevi servon
      kontra&ubreve; difinita prezo:
      <ekz>
        malmultekosta <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Franck Arnaud</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007114.html">Intel alfrontas konkurencon</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        la&ubreve; merkata enketo inter la <tld/>o kaj bezono de laktaj produktoj jam estas
        ekvilibro
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007169.html">Lakto ne &scirc;atata</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> faris multajn <tld/>ojn, por ke oni akiru iliajn produktojn
        <fnt>
          <bib>Monato</bib>
          <aut>last</aut>
          <vrk><url ref="https://www.monato.be/2008/009416.php">Libereco, &gcirc;ojiga ne por &ccirc;iuj</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro la malpli granda <tld/>o la prezoj de porkoviando levi&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Mu Binghua</aut>
          <vrk><url ref="https://www.monato.be/2007/009328.php">Registaro asertas, ke man&gcirc;a&jcirc;oj estas sekuraj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">(&c_p;&c_i;&c_s;&c_m;&c_je;&c_n;&c_o;) &c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je; &c_z;&c_a; &c_k;&c_u;&c_p;&c_u;&c_v;&c_a;&c_n;&c_je; &c_n;&c_a; &c_s;&c_t;&c_o;&c_k;&c_a; &c_i;&c_l;&c_i;
  &c_u;&c_s;&c_l;&c_u;&c_g;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>nab&iacute;dka <klr>(obchodn&iacute;, obliga&ccaron;n&iacute;)</klr></trd>,
    <trd>oferta</trd>,
    <trd>pod&aacute;n&iacute; (nab&iacute;dka)</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Angebot</trd>,
    <trd>Offerte</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>offre</trd>,
    <trd>proposition</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>offerte</trd>,
    <trd>prijsopgave</trd>
  </trdgrp>
  <trd lng="sk">ponuka</trd>
</drv>
</art>
