<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: ofert.xml,v 1.20 2018/08/06 22:11:00 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>ofert</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="ofert.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
      <ref tip="super" cel="propon.0i">Proponi</ref> al iu varon
      a&ubreve; 
      servon a&ccirc;etotan:
      <ekz>
        Vobis unua kura&gcirc;is, ekde &ccirc;i marto, <tld/>i
        komputilojn kun la
        nova
        Superdisk-aparato anstata&ubreve; la diskingo por tradiciaj
        disketoj
        <fnt><aut>Evgeni Georgiev</aut>:
        <vrk><url ref="&Monato;/006042.html">Zipo minacas la
        mikrodisketon</url></vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 07/1998</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trd
  lng="be">&c_p;&c_r;&c_a;&c_p;&c_a;&c_n;&c_o;&c_w;&c_v;&c_a;&c_c;&c_mol;
  
    <klr>(&c_t;&c_a;&c_v;&c_a;&c_r;,
    &c_p;&c_a;&c_s;&c_l;&c_u;&c_g;&c_u;)</klr></trd>
  <trdgrp lng="bg">
    <trd>&c_p;&c_r;&c_je;&c_d;&c_l;&c_a;&c_g;&c_a;&c_m; (&c_s;&c_t;&c_o;&c_k;&c_a;</trd>,
    <trd>&c_u;&c_s;&c_l;&c_u;&c_g;&c_o; &c_i; &c_t;.&c_n;.)</trd>
  </trdgrp>
  <trd lng="ca">oferir</trd>
  <trdgrp lng="cs">
    <trd>nab&iacute;dnout <klr>(zbo&zcaron;&iacute;)</klr></trd>,
    <trd>navrhnout</trd>,
    <trd>oferovat</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>anbieten</trd>,
    <trd>offerieren</trd>
  </trdgrp>
  <trd lng="en">offer</trd>
  <trdgrp lng="es">
    <trd>ofertar</trd>,
    <trd>ofrecer</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>offrir</trd>,
    <trd>proposer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>aj&aacute;nlatot tesz</trd>,
    <trd>megaj&aacute;nl</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>een <ind>offerte</ind> maken voor</trd>,
    <trd>te koop <ind>aanbieden</ind></trd>
  </trdgrp>
  <trd lng="pt">ofertar</trd>
  <trd lng="ru">&c_p;&c_r;&c_je;&c_d;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;
  <klr>(&c_t;&c_o;&c_v;&c_a;&c_r;, &c_u;&c_s;&c_l;&c_u;&c_g;&c_u; &c_i;
  &c_t;.&c_p;.)</klr></trd>
  <trdgrp lng="sk">
    <trd>navrhn&uacute;&tcaron;</trd>,
    <trd>pon&uacute;knu&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="ofert.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">EKON</uzo>
    <dif>
       Skriba propono por a&ccirc;eti varon a&ubreve; ricevi servon
       kontra&ubreve;
       difinita prezo:
       <ekz>
         IKEF senpage aperigas komercajn <tld/>ojn.
       </ekz>
    </dif>
  </snc>
  <trd lng="bg">(&c_p;&c_i;&c_s;&c_m;&c_je;&c_n;&c_o;) &c_p;&c_r;&c_je;&c_d;&c_l;&c_o;&c_zh;&c_je;&c_n;&c_i;&c_je; &c_z;&c_a; &c_k;&c_u;&c_p;&c_u;&c_v;&c_a;&c_n;&c_je; &c_n;&c_a; &c_s;&c_t;&c_o;&c_k;&c_a; &c_i;&c_l;&c_i;
  &c_u;&c_s;&c_l;&c_u;&c_g;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>nab&iacute;dka <klr>(obchodn&iacute;, obliga&ccaron;n&iacute;)</klr></trd>,
    <trd>oferta</trd>,
    <trd>pod&aacute;n&iacute; (nab&iacute;dka)</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Angebot</trd>,
    <trd>Offerte</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>offre</trd>,
    <trd>proposition</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>offerte</trd>,
    <trd>prijsopgave</trd>
  </trdgrp>
  <trd lng="sk">ponuka</trd>
</drv>
</art>
