<drv mrk="civili1.0o">
  <kap><tld/>o</kap>
  <snc>
    <ref tip="dif" cel="civili.0eco">civilizo</ref>
    <ekz>
      la azteka <tld/>o priskribita estas pli-malpli simila al tiu, pri
      kiu oni legas en antropologiaj studoj<fnt><aut>Donald
      Broadribb</aut>: <vrk>Vi ne hontos transdoni vian ekzempleron
      (Harry
      Harrison, La kaptita universo)</vrk>, <bib>Monato</bib><lok>jaro
      2002a, numero 3a, p. 25-26a</lok></fnt>.
    </ekz>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_v;&c_ib;&c_l;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>civilizace</trd>,
    <trd>kultura</trd>
  </trdgrp>
  <trd lng="de">Zivilisation</trd>
  <trd lng="en">civilization</trd>
  <trd lng="es">civilizaci&oacute;n</trd>
  <trd lng="fr">civilisation</trd>
  <trd lng="hu">civiliz&aacute;ci&oacute;</trd>
  <trd lng="it">civilit&agrave;<klr> (civilizzazione)</klr></trd>
  <trd lng="nl">beschaving</trd>
  <trd lng="pl">cywilizacja</trd>
  <trd lng="pt">civiliza&ccedil;&atilde;o</trd>
  <trd lng="ru">&c_c;&c_i;&c_v;&c_i;&c_l;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>
  <trdgrp lng="sk">
    <trd>civilizácia</trd>,
    <trd>kultúra</trd>
  </trdgrp>
</drv>
</art>
