<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id: mongoled.xml,v 1.9 2021/05/11 14:52:34 revo Exp $">
    <kap>
      <rad>mongoled</rad>/a
    </kap>

    <drv mrk="mongoled.0a">
      <kap>
        <tld/>a
      </kap>
      <snc>
        <uzo tip="fak">LIN</uzo>
        <dif>
          Rilata al tiu bran&ccirc;o de la
          <ref tip="malprt" cel="altaj.0a_lingvofamilio">Altaja
lingvofamilio</ref>,
          kies &ccirc;efreprezentanto estas la
          <ref tip="prt" cel="mongol.0a">mongola</ref> lingvo:
          <ekz>
            la burjata, kalmuka kaj mongola lingvoj estas la plej
            grandaj <ind>lingvoj <tld/>aj</ind>.
            <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_ib;&c_ja; &c_m;&c_o;&c_v;&c_y;</trd>
            <trd lng="bg">&c_m;&c_o;&c_n;&c_g;&c_o;&c_l;&c_o;&c_i;&c_d;&c_n;&c_i; &c_je;&c_z;&c_i;&c_c;&c_i;</trd>
            <trd lng="hu">mongol nyelvek</trd>            
            <trd lng="nl">mongolo&iuml;de talen</trd>
            <trd lng="pl">mongolskie j&eogonek;zyki</trd>
            <trd lng="pt">l&iacute;nguas mong&oacute;licas</trd>
            <trd lng="ro">mongole limbi</trd>
            <trd lng="ru">&c_m;&c_o;&c_n;&c_g;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_i;&c_je; &c_ja;&c_z;&c_y;&c_k;&c_i;</trd>
          </ekz>
        </dif>
      </snc>
      <trd lng="be">&c_m;&c_a;&c_n;&c_g;&c_o;&c_l;&c_mol;&c_s;&c_k;&c_ib;</trd>
      <trd lng="bg">&c_m;&c_o;&c_n;&c_g;&c_o;&c_l;&c_o;&c_i;&c_d;&c_n;&c_i;</trd>
      <trd lng="en">Mongolian</trd>
      <trd lng="fr">mongolo&iuml;de</trd>
      <trdgrp lng="hu">
        <trd>mongoloid</trd>,
        <trd>mongolos</trd>
      </trdgrp>
    <trd lng="nl">mongolo&iuml;de</trd>
    <trd lng="pl">mongolski <klr>(j&eogonek;zyk)</klr></trd>
    <trd lng="pt">mong&oacute;lico</trd>
    <trd lng="ro">mongol&abreve;</trd>
   </drv>
  </art>

