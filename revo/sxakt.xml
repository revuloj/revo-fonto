<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>&scirc;akt</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="sxakt.0o">
  <kap><tld/>o</kap>
  <snc mrk="sxakt.0o.mineja">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Parto de
      <ref tip="malprt" cel="min.0ejo">minejo</ref>,
      vertikala puto en la tero kondukanta al la
      <ref tip="vid" cel="galeri.0o.mineja">galerioj</ref>.
    </dif>
    <trdgrp lng="be">
      <trd>&c_sh;&c_a;&c_h;&c_t;&c_a;</trd>,
      <trd>&c_k;&c_a;&c_p;&c_a;&c_l;&c_mol;&c_n;&c_ja;</trd>
    </trdgrp>
    <trd lng="hu">akna</trd>
    <trd lng="pt">po&ccedil;o</trd>
    <trd lng="ru">&c_sh;&c_a;&c_h;&c_t;&c_a;</trd>
  </snc>
  <snc mrk="sxakt.0o.bortruo">
    <uzo tip="fak">TEK</uzo>
    <dif>
      Vertikala puto, kiun oni boras en la tero por akiri
      nafton, akvon ktp.
    </dif>
    <trd lng="be">&c_s;&c_mol;&c_v;&c_ib;&c_d;&c_r;&c_a;&c_v;&c_ib;&c_n;&c_a;</trd>
    <trd lng="hu">f&uacute;r&oacute;lyuk</trd>
    <trd lng="ru">&c_s;&c_k;&c_v;&c_a;&c_zh;&c_i;&c_n;&c_a;</trd>
  </snc>
  <trd lng="de">Schacht</trd>
  <trdgrp lng="nl">
    <trd>schacht</trd>,
    <trd>mijnschacht</trd>
  </trdgrp>
  <trd lng="sv"><klr>(gruv)</klr>schakt</trd>
</drv>

<drv mrk="sxakt.sxtupar0o">
  <kap>&scirc;tupar<tld/>o</kap>
  <snc mrk="sxakt.sxtupar0o.ARKI">
    <uzo tip="fak">ARKI</uzo>
    <dif>
      Vertikala parto de <ref tip="malprt" cel="konstr.0ajxo.domo">konstrua&jcirc;o</ref> tra la eta&gcirc;oj, kiu entenas &scirc;tuparon:
      <ekz>
        ili trovis ke la pordoj de la &scirc;tupar<tld/>o malfermeblas nur de ekstere kaj ne havas klinkojn.
        <fnt>
        <aut>Ralph Harry</aut>,
        <vrk><url ref="http://www.luin.se/inko/X24-6.pdf">La Diplomato Kiu Ridis</url></vrk>,
        <lok>1997</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">cage d'escalier</trd>
</drv>
<adm>
  Mi disigis la sencojn "sxakt.0o.mineja" kaj "sxakt.0o.bortruo";
  eble endos simile disigi la tradukojn de, nl, sv.
  <aut>Sergio</aut>
</adm>
</art>
