<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>aniz</rad>/o
</kap>
<drv mrk="aniz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Frukto de <ref tip="malprt" cel="aniz.0ujo"><tld/>ujo</ref>, uzata
      kiel spica&jcirc;o kaj por fabriki likvorojn.
    </dif>
  </snc>
  <trd lng="de">Anis</trd>
  <trd lng="en">anise</trd>
  <trd lng="es">an&iacute;s</trd>
  <trd lng="hu">&aacute;nizs</trd>
  <trd lng="nl">anijs</trd>
  <trd lng="ru">&c_a;&c_n;&c_i;&c_s;</trd>
  <trd lng="sv">anis</trd>
</drv>

<drv mrk="aniz.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="pimpin.0o">pimpinelo</ref>
      (<trd lng="la">Pimpinella anisum</trd>).
    </dif>
  </snc>
  <trd lng="de">Anis</trd>
  <trd lng="en">anise</trd>
  <trd lng="fr">anis</trd>
  <trdgrp lng="hu">
    <trd>&aacute;nizs <ind>(n&ouml;v&eacute;ny)</ind></trd>,
    <trd>illatos <ind>angyalgy&ouml;k&eacute;r</ind></trd>,
    <trd>k&ouml;z&ouml;ns&eacute;ges
<ind>angyalgy&ouml;k&eacute;r</ind></trd>,
    <trd>f&udblac;szerangyalgy&ouml;k&eacute;r</trd>
  </trdgrp>
  <trd lng="nl">anijs</trd>
  <trd lng="ru">&c_a;&c_n;&c_i;&c_s; <klr
tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="sv">anis</trd>
</drv>

<drv mrk="aniz.stel0o">
  <kap>stel<tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Stelforma frukto de unu specio de <ref tip="malprt"
      cel="ilici.0o">ilicio</ref>
      uzata kiel spica&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Sternanis</trd>
  <trd lng="en">star anise</trd>
  <trdgrp lng="fr">
    <trd>anis &eacute;toil&eacute;</trd>,
    <trd>badiane</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csillag&aacute;nizs</trd>,
    <trd>csillagos <ind>&aacute;nizs</ind></trd>
  </trdgrp>
  <trd lng="nl">steranijs</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_v;&c_jo;&c_z;&c_d;&c_ch;&c_a;&c_t;&c_y;&c_j;
<ind>&c_a;&c_n;&c_i;&c_s;</ind></trd>,
    <trd>&c_b;&c_a;&c_d;&c_mol;&c_ja;&c_n;</trd>
  </trdgrp>
</drv>
</art>
