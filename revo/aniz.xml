<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: aniz.xml,v 1.33 2022/04/25 15:10:59 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>aniz</rad>/o
</kap>
<drv mrk="aniz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="aniz.0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Frukto de <ref tip="malprt" cel="aniz.0ujo"><tld/>ujo</ref>, uzata
      kiel <ref tip="lst" cel="spic.0o" lst="voko:spicoj">spico</ref>
      kaj por fari likvorojn:
      <ekz>
        vi pagas dekona&jcirc;ojn de mento kaj de <tld/>o kaj de kumino
        <fnt>
          <bib>NT</bib>
          <lok>&Mat; 23:23</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili vendadis plej diversajn spicojn, kiel ekzemple safranon, <tld/>on, zingibron kaj precipe pipron
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        pro la gusto &ccirc;iu urbo havis sian preferon rilate proprajn herbomiksa&jcirc;ojn 
        a&ubreve; &leftquot;grion&rightquot; (en kiu enestis i.a. miriko, rosmareno, koriandro, junipero,
        cinamo, <tld/>o, kariofilo, salvio, la&ubreve;ro)
        <fnt>
          <bib>Monato</bib>
          <aut>L. van de Velde</aut>
          <vrk><url ref="&Steloj;monato2/007144.html">Bierlando Belgio</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_ib;&c_s; <klr>(&c_p;&c_l;&c_o;&c_d;)</klr></trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_s;&c_o;&c_n;</trd>
  <trd lng="cs">an&yacute;z <klr>(strom)</klr></trd>
  <trd lng="de">Anis</trd>
  <trd lng="en">anise</trd>
  <trd lng="es">an&iacute;s</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_A;&a_z;&f_y;&a_A;&a_n;&a_h;&a_hamza_sure; &a_r;&a_w;&a_m;&f_y;</trd>&a_komo;
    <trd>&a_A;&a_n;&f_y;&a_s;&a_w;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">anis<klr> (graine)</klr></trd>
  <trd lng="he">&alef;&nun;&yod;&samekh;</trd>
  <trd lng="hu">&aacute;nizs</trd>
  <trd lng="nl">anijs</trd>
  <trd lng="pt">anis</trd>
  <trd lng="ru">&c_a;&c_n;&c_i;&c_s;</trd>
  <trdgrp lng="sk">
    <trd>an&iacute;z <klr>(plod)</klr></trd>,
    <trd>an&iacute;z <klr>(rastlina, semen&aacute;)</klr></trd>
  </trdgrp>
  <trd lng="sv">anis</trd>
  <trdgrp lng="uk">
    <trd>&c_g;&c_a;&c_n;&c_u;&c_s;</trd>,
    <trd>&c_a;&c_n;&c_ib;&c_s;</trd>
  </trdgrp>
</drv>

<drv mrk="aniz.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="aniz.0ujo.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Specio de <ref tip="super" cel="pimpin.0o">pimpinelo</ref>
      (<trd lng="la">Pimpinella anisum</trd>), aroma herbo similanta
      al <ref cel="anet.0o">aneto</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_n;&c_ib;&c_s; <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>
  <trd lng="bg">&c_a;&c_n;&c_a;&c_s;&c_o;&c_n;</trd>
  <trd lng="cs">an&yacute;z <klr>(strom)</klr></trd>
  <trd lng="de">Anis</trd>
  <trd lng="en">anise</trd>
  <trdgrp lng="fa">
    <trd>&a_r;&a_A;&a_z;&f_y;&a_A;&a_n;&a_h;&a_hamza_sure;&zwnj; &a_r;&a_w;&a_m;&f_y;</trd>&a_komo;
    <trd>&a_A;&a_n;&f_y;&a_s;&a_w;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">anis<klr> (plante)</klr></trd>
  <trd lng="he">&kaf;&mem;&nun;&vav;&fnun;</trd>
  <trdgrp lng="hu">
    <trd>&aacute;nizs <klr>(n&ouml;v&eacute;ny)</klr></trd>,
    <trd>illatos <ind>angyalgy&ouml;k&eacute;r</ind></trd>,
    <trd>k&ouml;z&ouml;ns&eacute;ges
    <ind>angyalgy&ouml;k&eacute;r</ind></trd>,
    <trd>f&udblac;szerangyalgy&ouml;k&eacute;r</trd>
  </trdgrp>
  <trd lng="nl">anijs</trd>
  <trd lng="ru">&c_a;&c_n;&c_i;&c_s; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trdgrp lng="sk">
    <trd>an&iacute;z <klr>(rastlina)</klr></trd>,
    <trd>an&iacute;z <klr>(strom)</klr></trd>
  </trdgrp>
  <trd lng="sv">anis</trd>
</drv>

<drv mrk="aniz.stel0o">
  <kap>stel<tld/>o</kap>
  <snc mrk="aniz.stel0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Stelforma <ref tip="super" cel="frukt.0o.BOT">frukto</ref> de unu specio de
      <ref tip="malprt" cel="ilici.0o">ilicio</ref>
      uzata kiel <ref tip="super" cel="spic.0o">spico</ref>.
      <ekz>
        safrolo nature trov<klr>[i&gcirc;]</klr>as anka&ubreve; en zingibro <klr>[&#8230;]</klr> a&ubreve; stel<tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=592220">Safrolo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_z;&c_v;&c_je;&c_z;&c_d;&c_je;&c_n; <ind>&c_a;&c_n;&c_a;&c_s;&c_o;&c_n;</ind></trd>
  <trd lng="cs">padi&aacute;n</trd>
  <trd lng="de">Sternanis</trd>
  <trd lng="en">star anise</trd>
  <trd lng="fa">&a_b;&a_A;&a_d;&f_y;&a_A;&a_n;</trd>
  <trdgrp lng="fr">
    <trd>anis &eacute;toil&eacute;</trd>,
    <trd>badiane chinoise</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>csillag&aacute;nizs</trd>,
    <trd>csillagos <ind>&aacute;nizs</ind></trd>
  </trdgrp>
  <trd lng="nl">steranijs</trd>
  <trdgrp lng="ru">
    <trd>&c_z;&c_v;&c_jo;&c_z;&c_d;&c_ch;&c_a;&c_t;&c_y;&c_j;
    <ind>&c_a;&c_n;&c_i;&c_s;</ind></trd>,
    <trd>&c_b;&c_a;&c_d;&c_mol;&c_ja;&c_n;</trd>
  </trdgrp>
  <trd lng="sk">padi&aacute;n</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_o;&c_d;&c_ja;&c_n;</trd>,
    <trd>&c_z;&c_ib;&c_r;&c_ch;&c_a;&c_s;&c_t;&c_i;&c_j; &c_a;&c_n;&c_ib;&c_s;</trd>
  </trdgrp>
</drv>
</art>
