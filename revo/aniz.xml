<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>aniz</rad>/o
</kap>
<drv mrk="aniz.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Frukto de <ref tip="malprt" cel="aniz.0ujo"><tld/>ujo</ref>, uzata
      kiel spica&jcirc;o kaj por fabriki likvorojn.
    </dif>
  </snc>
  <trd lng="de">Anis</trd>
  <trd lng="ru">&#1072;&#1085;&#1080;&#1089;</trd>
</drv>
<drv mrk="aniz.0ujo">
  <kap><tld/>ujo</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Specio de <ref tip="super" cel="pimpin.0o">pimpinelo</ref>
      (<trd lng="la">Pimpinella anisum</trd>).
    </dif>
  </snc>
  <trd lng="de">Anis</trd>
</drv>
<drv mrk="aniz.stel0o">
  <kap>stel<tld/>o</kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Stelforma frukto de unu specio de <ref tip="malprt" cel="ilici.0o">ilicio</ref>
      uzata kiel spica&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Sternanis</trd>
</drv>
</art>
