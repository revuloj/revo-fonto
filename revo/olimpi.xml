<vortaro>
  <art mrk="$Id: olimpi.xml,v 1.9 2024/01/30 16:09:26 afido Exp $">
    <kap>
      <rad>Olimpi</rad>/o
    </kap>

<drv mrk="olimpi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc mrk="olimpi.0o.sanktejo">
    <dif>
      <ref tip="super" cel="sankt.0ejo">Sanktejo</ref> de <ref cel="zeuxs.0o">Ze&ubreve;so</ref> 
      en la nordokcidento de la peloponeza duoninsulo:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estis la loko de la antikvaj <tld lit="o"/>aj ludoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=228713">Olimpio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_A;&c_l;&c_ib;&c_m;&c_p;&c_ib;&c_ja;</trd>
    <trd lng="de">Olimpia</trd>
    <trd lng="en">Olimpia</trd>
    <trd lng="fr">Olimpie</trd>
    <trd lng="grc"> &Omikron_psili;&lambda;&ypsilon;&my;&pi;&jota_ton;&alfa;</trd>
    <trd lng="pl">Olimpia</trd>
    <trd lng="ro">Olympia</trd>
  </snc>
  <trd lng="ja">&#x30AA;&#x30EA;&#x30F3;&#x30D4;&#x30A2;</trd>
  <trd lng="uk">&#x41E;&#x43B;&c_ib;&#x43C;&#x43F;&c_ib;&#x44F;</trd>
  <trdgrp lng="zh">
    <trd>&#x5965;&#x8FD0; <pr>&#xE0;oy&#xF9;n</pr></trd>,
    <trd>&#x5967;&#x904B; <pr>&#xE0;oy&#xF9;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="olimpi.0aj_Ludoj">
  <kap>
    <tld/>aj Ludoj
  </kap>
  <uzo tip="fak">SPO</uzo>
  <snc mrk="olimpi.0aj_Ludoj.SPO">
    <dif>
      <ref tip="dif" cel="olimpik.0o">Olimpiko</ref>:
      <ekz>
        someraj <tld lit="o"/>aj ludoj
        <fnt>
          <bib>Monato</bib>
          <aut>Sergei Pa&hcirc;omov</aut>
          <vrk><url ref="&Monato;/2006/008761.php">Al kiuj la olimpiaj ludoj justus?</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam estis malfermataj la 27aj <tld/>aj Ludoj en Sidnejo, A&ubreve;stralio, <nom>Cathy
        Freeman</nom> <klr>[&#x2026;]</klr> fari&gcirc;is monde konata: estis &scirc;i, kiu metis fajron en la
        <tld lit="o"/>an kalikon
        <fnt>
          <aut>IGARASI Takeo</aut>
            <vrk><url ref="&Monato;/2000/007204.html">Tor&ccirc;o de hipokriteco</url></vrk>
            <bib>Monato</bib>
            <lok>2000/12, p. 15</lok>
          </fnt>;
      </ekz>
      <ekz>
        Internacia Olimpika Komitato (IOK) <klr>[&#x2026;]</klr> akceptis la decidon aran&gcirc;i la
        <tld/>ajn Ludojn en &Ccirc;inio
        <fnt>
          <bib>Monato</bib>
          <aut>Mikaelo Bron&scirc;tejn</aut>
          <vrk><url ref="&Monato;/2008/009484.php">&Ccirc;u bojkoto? (3)</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_l;&c_ib;&c_m;&c_p;&c_ib;&c_ja;&c_d;&c_a;</trd>,
    <trd>&c_A;&c_l;&c_ib;&c_m;&c_p;&c_ib;&c_j;&c_s;&c_k;&c_ib;&c_ja; &c_g;&c_u;&c_l;&c_mol;&c_n;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">Olympische Spiele</trd>
  <trd lng="en">olympic games</trd>
  <trd lng="fr">jeux olympiques</trd>
  <trd lng="pl">Igrzyska Olimpijskie</trd>
  <trd lng="ro">Jocuri Olimpice</trd>
  <trd lng="uk">&#x41E;&#x43B;&c_ib;&#x43C;&#x43F;&c_ib;&c_j;&c_s;&#x44C;&#x43A;&c_ib; &c_ib;&c_g;&c_r;&c_i;</trd>
  <trdgrp lng="zh">
    <trd>&#x5965;&#x8FD0;&#x4F1A; <pr>&#xC0;oy&#xF9;nhu&#xEC;</pr></trd>,
    <trd>&#x5967;&#x904B;&#x6703; <pr>&#xC0;oy&#xF9;nhu&#xEC;</pr></trd>,
    <trd>&#x5965;&#x8FD0; <pr>&#xC0;oy&#xF9;n</pr></trd>,
    <trd>&#x5967;&#x904B; <pr>&#xC0;oy&#xF9;n</pr></trd>,
    <trd>&#x5965;&#x8FD0; <pr>&#xE0;oy&#xF9;n</pr></trd>,
    <trd>&#x5967;&#x904B; <pr>&#xE0;oy&#xF9;n</pr></trd>,
    <trd>&#x5965;&#x8FD0;&#x4F1A; <pr>&#xE0;oy&#xF9;nhu&#xEC;</pr></trd>,
    <trd>&#x5967;&#x904B;&#x6703; <pr>&#xE0;oy&#xF9;nhu&#xEC;</pr></trd>,
    <trd>&#x5965;&#x6797;&#x5339;&#x514B; <pr>&#xE0;ol&#xED;np&#x1D0;k&#xE8;</pr></trd>,
    <trd>&#x5967;&#x6797;&#x5339;&#x514B; <pr>&#xE0;ol&#xED;np&#x1D0;k&#xE8;</pr></trd>,
    <trd>&#x5965;&#x6797;&#x5339;&#x514B;&#x8FD0;&#x52A8;&#x4F1A; <pr>&#xE0;ol&#xED;np&#x1D0;k&#xE8;y&#xF9;nd&#xF2;nghu&#xEC;</pr></trd>,
    <trd>&#x5967;&#x6797;&#x5339;&#x514B;&#x904B;&#x52D5;&#x6703; <pr>&#xE0;ol&#xED;np&#x1D0;k&#xE8;y&#xF9;nd&#xF2;nghu&#xEC;</pr></trd>,
    <trd>&#x5965;&#x6797;&#x5339;&#x514B;&#x8FD0;&#x52A8;&#x4F1A; <pr>&#xC0;ol&#xED;np&#x1D0;k&#xE8;Y&#xF9;nd&#xF2;nghu&#xEC;</pr></trd>,
