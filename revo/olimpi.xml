<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: olimpi.xml,v 1.6 2022/05/01 17:17:00 afido Exp $">
    <kap>
      <rad>Olimpi</rad>/o
    </kap>

<drv mrk="olimpi.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">HIS</uzo>
  <snc mrk="olimpi.0o.sanktejo">
    <dif>
      <ref tip="super" cel="sankt.0ejo">Sanktejo</ref> de <ref cel="zeuxs.0o">Ze&ubreve;so</ref> 
      en la nordokcidento de la peloponeza duoninsulo:
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estis la loko de la antikvaj <tld lit="o"/>aj ludoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=228713">Olimpio</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_A;&c_l;&c_ib;&c_m;&c_p;&c_ib;&c_ja;</trd>
    <trd lng="de">Olimpia</trd>
    <trd lng="en">Olimpia</trd>
    <trd lng="fr">Olimpie</trd>
    <trd lng="grc"> &Omikron_psili;&lambda;&ypsilon;&my;&pi;&jota_ton;&alfa;</trd>
    <trd lng="pl">Olimpia</trd>
    <trd lng="ro">Olympia</trd>
  </snc>
  <trd lng="uk">&#x41E;&#x43B;&#x456;&#x43C;&#x43F;&#x456;&#x44F;</trd>
</drv>

<drv mrk="olimpi.0aj_Ludoj">
  <kap>
    <tld/>aj Ludoj
  </kap>
  <uzo tip="fak">SPO</uzo>
  <snc mrk="olimpi.0aj_Ludoj.SPO">
    <dif>
      <ref tip="dif" cel="olimpik.0o">Olimpiko</ref>:
      <ekz>
        someraj <tld lit="o"/>aj ludoj
        <fnt>
          <bib>Monato</bib>
          <aut>Sergei Pa&hcirc;omov</aut>
          <vrk><url ref="&Monato;/2006/008761.php">Al kiuj la olimpiaj ludoj justus?</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        kiam estis malfermataj la 27aj <tld/>aj Ludoj en Sidnejo, A&ubreve;stralio, <nom>Cathy
        Freeman</nom> <klr>[&#x2026;]</klr> fari&gcirc;is monde konata: estis &scirc;i, kiu metis fajron en la
        <tld lit="o"/>an kalikon
        <fnt>
          <bib>Monato</bib>
          <aut>Igarasi Takeo</aut>
          <vrk><url ref="&Steloj;monato2/007204.html">Tor&ccirc;o de hipokriteco</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Internacia Olimpika Komitato (IOK) <klr>[&#x2026;]</klr> akceptis la decidon aran&gcirc;i la
        <tld/>ajn Ludojn en &Ccirc;inio
        <fnt>
          <bib>Monato</bib>
          <aut>Mikaelo Bron&scirc;tejn</aut>
          <vrk><url ref="&Monato;/2008/009484.php">&Ccirc;u bojkoto? (3)</url></vrk>
          <lok>2008</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_A;&c_l;&c_ib;&c_m;&c_p;&c_ib;&c_ja;&c_d;&c_a;</trd>,
    <trd>&c_A;&c_l;&c_ib;&c_m;&c_p;&c_ib;&c_j;&c_s;&c_k;&c_ib;&c_ja; &c_g;&c_u;&c_l;&c_mol;&c_n;&c_ib;</trd>
  </trdgrp>
  <trd lng="de">Olympische Spiele</trd>
  <trd lng="en">olympic games</trd>
  <trd lng="fr">jeux olympiques</trd>
  <trd lng="pl">Igrzyska Olimpijskie</trd>
  <trd lng="ro">Jocuri Olimpice</trd>
  <trd lng="uk">&#x41E;&#x43B;&#x456;&#x43C;&#x43F;&#x456;&#x439;&#x441;&#x44C;&#x43A;&#x456; &#x456;&#x433;&#x440;&#x438;</trd>
</drv>

  </art>

