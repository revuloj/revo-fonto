<vortaro>
<art mrk="$Id: tizan.xml,v 1.22 2023/09/04 19:01:52 revo Exp $">
<kap>
<rad>tizan</rad>/o
</kap>
<drv mrk="tizan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="infuz.0ajxo">Infuza&jcirc;o</ref> a&ubreve;
      <ref tip="super" cel="dekokt.0ajxo">dekokta&jcirc;o</ref> de
      (kurac)herboj:
      <ekz>
        oni diras ke la eltrovinto de la korea fama <tld/>o <nom>&Gcirc;insen</nom> 
        son&gcirc;e informi&gcirc;is pri la loko de tiu planto en iu monto kaj poste iris tien kaj trovis &gcirc;in
        <fnt>
          <aut>Reza Kheir-Khah</aut>
            <vrk><url ref="&Monato;/2000/007087.php">Mirinda dimensio de nia vivo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/09, p. 16</lok>
          </fnt>;
      </ekz>
      <ekz>
        amiko mia spertis <klr>[&#x2026;]</klr> gravegan dormoproblemon,
        li provis multajn rimedojn, komencante per la plej naturaj
        kaj sendan&gcirc;eraj (precipe <tld/>oj a&ubreve; tablojdoj de valeriano,
        pasifloro kaj aliaj herboj <klr>[&#x2026;]</klr>)
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2012/010764.php">Dormu bone, vivu bone (per
          melatonino)</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="te.0o.trinkajxo">teo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_v;&c_a;&c_r;</trd>,
    <trd>&c_u;&c_z;&c_v;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;</trd>
  </trdgrp>
  <trd lng="ca">tisana</trd>
  <trdgrp lng="de">
    <trd><klr tip="amb">(Kr&auml;uter-)</klr>Tee</trd>,
    <trd>Aufguss</trd>
  </trdgrp>
  <trdgrp lng="fa">
    <trd>&a_j;&a_w;&a_sh;&a_A;&a_n;&a_d;&a_h;</trd>,
    <trd>&a_d;&a_m;&#x200C;&f_k;&a_r;&a_d;&a_h;</trd>
  </trdgrp>
  <trd lng="fr">tisane</trd>
  <trdgrp lng="hu">
    <trd>gy&oacute;gytea</trd>,
    <trd>herbatea</trd>,
    <trd>teaf&odblac;zet</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x714E;&#x3058;&#x85AC;  [<ind>&#x305B;&#x3093;&#x3058;&#x3050;&#x3059;&#x308A;</ind>]</trd>,
    <trd>&#x85AC;&#x6E6F;  [<ind>&#x3084;&#x304F;&#x3068;&#x3046;</ind>]</trd>
  </trdgrp>
  <trd lng="nl">kruidendrank</trd>
  <trdgrp lng="pt">
    <trd>tisana</trd>,
    <trd>xaropada</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_v;&c_a;&c_r;</trd>,
    <trd>&c_n;&c_a;&c_s;&c_t;&c_o;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>lie&ccaron;iv&yacute; odvar</trd>,
    <trd>n&aacute;lev</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>n&aacute;lev</trd>,
    <trd>odvar <klr>(z bylin)</klr></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_v;&c_ib;&c_d;&c_v;&c_a;&c_r;</trd>,
    <trd>(&c_c;&c_ib;&#x43B;&#x44E;&c_shch;&c_i;&c_j;) &#x43D;&c_a;&c_s;&c_t;&c_ib;&c_j; (&c_ib;&c_z; &c_t;&c_r;&c_a;&c_v;)</trd>
  </trdgrp>
</drv>
</art>
