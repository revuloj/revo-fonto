<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: flanel.xml,v 1.24 2023/11/09 20:48:33 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>flanel</rad>/o
</kap>
<drv mrk="flanel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="teks.0ajxo.TEKS">Teksa&jcirc;o</ref> (iam lana,
      nun ofte kotona) maldika, mola kaj malrigida, &ccirc;e tu&scirc;o
      impresanta kiel lanugeca:
      <ekz>
        ili vizitis lin kaj trovis lin sidanta en la lito, vestita
        per kamizolo el verda <tld/>o kaj kun Toleda ru&gcirc;a &ccirc;apo
        <fnt><bib>DonKihxoto</bib>.
          <lok>Anta&ubreve;parolo al la leganto</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_l;&c_ja;&c_n;&c_e;&c_l;&c_mol;</trd>
  <trd lng="cs">flanel</trd>
  <trd lng="de">Flanell</trd>
  <trd lng="en">flannel</trd>
  <trd lng="es">franela</trd>
  <trd lng="fa">&a_f;&a_l;&a_A;&a_n;&a_l;</trd>
  <trd lng="fr">flanelle</trd>
  <trd lng="hu">flanell</trd>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30E9;&#x30F3;&#x30CD;&#x30EB;</trd>,
    <trd>&#x30CD;&#x30EB;</trd>,
    <trd>&#x30D5;&#x30E9;&#x30CE;</trd>
  </trdgrp>
  <trd lng="nl">flanel</trd>
  <trd lng="pl">flanela</trd>
  <trd lng="pt">flanela</trd>
  <trd lng="ru">&c_f;&c_l;&c_a;&c_n;&c_je;&c_l;&c_mol;</trd>
  <trd lng="sk">flanel</trd>
  <trd lng="uk">&#x444;&#x43B;&#x430;&#x43D;&#x435;&#x43B;&#x44C;</trd>
  <trd lng="zh">&#x7D68;&#x5E03; <pr>r&#xF3;ngb&#xF9;</pr></trd>
</drv>
</art>
