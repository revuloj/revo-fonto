<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>medal</rad>/o
</kap>
<drv mrk="medal.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Metala disketo surhavanta amba&ubreve;flanke bildon, signojn a&ubreve;
      surskribojn, stampita kiel memora&jcirc;o pri grava okazinta&jcirc;o a&ubreve;
      kiel
      honora&jcirc;o al merita homo:
      <ekz>
        la ora <tld/>o de la Geografio-societo de
        Antverpeno<fnt><aut>Wim
        M. A. De Smet</aut>: <vrk>Anta&ubreve; cent jaroj</vrk>,
        <bib>Monato</bib><lok>jaro 1998a, numero 5a, p. 25a</lok></fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FRAZ</uzo>
        &ccirc;iu <tld/>o du flankojn posedas.
        <klr>(&ccirc;io havas bona&jcirc;ojn kaj malbona&jcirc;ojn)</klr>
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="malprt">
      <ref cel="orden.0o">ordeno</ref>.
    </refgrp>
  </snc>                       
  <trd lng="be">&c_m;&c_e;&c_d;&c_a;&c_l;&c_mol;</trd>
  <trd lng="bg">&c_m;&c_je;&c_d;&c_a;&c_l;</trd>
  <trd lng="br">medalenn</trd>
  <trd lng="de">Medaille</trd>
  <trd lng="en">medal</trd>
  <trdgrp lng="fa">
    <trd>&a_m;&a_d;&a_A;&a_l;</trd>
    <trd>&a_n;&a_sh;&a_A;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">m&eacute;daille</trd>
  <trdgrp lng="hu">
    <trd>&eacute;rem</trd>,
    <trd>&eacute;rdem&eacute;rem</trd>,
    <trd>med&aacute;lia</trd>
  </trdgrp>
  <trd lng="nl">medaille</trd>
  <trd lng="pt">medalha</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_a;&c_l;&c_mol;</trd>
  <trd lng="sk">medaila</trd>
</drv>
</art>
