<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>medal</rad>/o
</kap>
<drv mrk="medal.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Metala disketo surhavanta amba&ubreve;flanke bildon, signojn a&ubreve;
      surskribojn, stampita kiel memora&jcirc;o pri grava okazinta&jcirc;o a&ubreve; kiel
      honora&jcirc;o al merita homo
    </dif>
    <refgrp tip="sin">
      <ref cel="orden.0o">ordeno</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="flank.0o">flanko 1</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Medaille</trd>
  <trd lng="en">medal</trd>
  <trd lng="nl">medaille</trd>
  <trd lng="ru">&c_m;&c_je;&c_d;&c_a;&c_l;&c_mol;</trd>
</drv>
</art>
