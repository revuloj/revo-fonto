<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>1</ofc>
      <rad>kolonel</rad>/o
    </kap>
    <drv mrk="kolone.0o">
      <kap><tld/>o</kap>
      <snc>
        <uzo tip="fak">MIL</uzo>
        <dif>
          Oficira <ref tip="super" cel="rang.0o.militista">rango</ref>
          en la terarmeo (kaj en la aera floto), inter sub<tld/>o kaj
          <ref cel="genera1.0o">generalo</ref>:
          <ekz>
            regimentestro estas <tld/>a ofico. 
          </ekz>
        </dif>
      </snc>
      <trd lng="de">Oberst</trd>
      <trd lng="en">colonel</trd>
      <trd lng="nl">kolonel</trd>
      <trd lng="ru">&c_p;&c_o;&c_l;&c_k;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
    </drv>
    <drv mrk="kolone.sub0o">
      <kap>sub<tld/>o</kap>
      <snc>
        <uzo tip="fak">MIL</uzo>
        <dif>
          Oficira <ref tip="super" cel="rang.0o.militista">rango</ref>
          en la terarmeo (kaj en la aera floto), inter
          <ref cel="major.0o">majoro</ref> kaj
          <ref cel="kolone.0o"><tld/>o</ref>.
        </dif>
      </snc>
      <trd lng="en">lieutenant-colonel</trd>
      <trd lng="nl">luitenant-kolonel</trd>
<trd lng="ru">&c_p;&c_o;&c_d;&c_p;&c_o;&c_l;&c_k;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
    </drv>
  </art>
