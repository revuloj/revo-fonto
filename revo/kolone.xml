<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kolone.xml,v 1.24 2023/12/03 09:58:52 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>kolonel</rad>/o
</kap>
<drv mrk="kolone.0o">
  <kap><tld/>o</kap>
    <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      En terarmeo kaj aerarmeo, 
      <ref tip="super" cel="oficir.0o.MIL">oficiro</ref>, kiu rangas
      inter
      <ref cel="kolone.sub0o">sub<tld/>o</ref> kaj 
      <ref cel="genera1.0o">generalo</ref>:
      <ekz>
        regimentestro estas <tld/>a ofico.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_k;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trdgrp lng="cs">
    <trd>kolonel</trd>,
    <trd>plukovn&iacute;k</trd>
  </trdgrp>
  <trd lng="de">Oberst</trd>
  <trd lng="en">colonel</trd>
  <trd lng="es">coronel</trd>
  <trd lng="fr">colonel</trd>
  <trd lng="hu">ezredes</trd>
  <trd lng="it">colonnello</trd>
  <trdgrp lng="ja">
    <trd>&#x5927;&#x4F50;  <pr>&#x305F;&#x3044;&#x3055;</pr></trd>,
    <trd>&#x9023;&#x968A;&#x9577;  <pr>&#x308C;&#x3093;&#x305F;&#x3044;&#x3061;&#x3087;&#x3046;</pr></trd>
  </trdgrp>
  <trd lng="nl">kolonel</trd>
  <trd lng="pl">pu&lstroke;kownik</trd>
  <trd lng="pt">coronel</trd>
  <trd lng="ro">colonel</trd>
  <trd lng="ru">&c_p;&c_o;&c_l;&c_k;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">plukovn&iacute;k</trd>
  <trd lng="uk">&#x43F;&#x43E;&#x43B;&#x43A;&#x43E;&#x432;&#x43D;&#x438;&#x43A;</trd>
  <trdgrp lng="zh">
    <trd>&#x9646;&#x519B;&#x4E0A;&#x6821; <pr>l&#xF9;j&#x16B;nsh&#xE0;ngxi&#xE0;o</pr></trd>,
    <trd>&#x4E0A;&#x6821; <pr>sh&#xE0;ngxi&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kolone.sub0o">
  <kap>sub<tld/>o</kap>
    <uzo tip="fak">MIL</uzo>
  <snc>
    <dif>
      En terarmeo kaj aerarmeo, 
      <ref tip="super" cel="oficir.0o.MIL">oficiro</ref>, kiu rangas
      inter
      <ref cel="major.0o">majoro</ref> kaj
      <ref cel="kolone.0o"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="be">
&c_p;&c_a;&c_d;&c_p;&c_a;&c_l;&c_k;&c_o;&c_w;&c_n;&c_ib;&c_k;</trd>
  <trd lng="cs">podplukovn&iacute;k</trd>
  <trd lng="en">lieutenant-colonel</trd>
  <trd lng="es">teniente-coronel</trd>
  <trd lng="fr">lieutenant-colonel</trd>
  <trd lng="hu">alezredes</trd>
  <trd lng="it">tenente colonnello</trd>
  <trd lng="ja">&#x4E2D;&#x4F50;  <pr>&#x3061;&#x3085;&#x3046;&#x3055;</pr></trd>
  <trd lng="nl">luitenant-kolonel</trd>
  <trd lng="pl">podpu&lstroke;kownik</trd>     
  <trd lng="ro">locotenent-colonel</trd>
  <trd lng="ru">
&c_p;&c_o;&c_d;&c_p;&c_o;&c_l;&c_k;&c_o;&c_v;&c_n;&c_i;&c_k;</trd>
  <trd lng="sk">podplukovn&iacute;k</trd>
  <trd lng="uk">&#x43F;&#x456;&#x434;&#x43F;&#x43E;&#x43B;&#x43A;&#x43E;&#x432;&#x43D;&#x438;&#x43A;</trd>
  <trd lng="zh">&#x4E2D;&#x6821; <pr>zh&#x14D;ngxi&#xE0;o</pr></trd>
</drv>
</art>
