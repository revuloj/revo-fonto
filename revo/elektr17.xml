<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: elektr17.xml,v 1.18 2021/05/16 13:56:02 revo Exp $">
  <kap>
    <rad>elektroterapi</rad>/o
    <fnt>Z</fnt>
  </kap>
  <drv mrk="elektr17.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="kurac.0o">Kuracado</ref>
      de malsanecaj afekcioj per elektro:
      <ekz>
        diversaj fizikoterapiaj te&hcirc;nikoj: <tld/>o: aplikado de elektraj kurentoj
        sendolorigaj k stimulaj <klr>[&#8230;]</klr> uzado de elektromagnetaj ondoj
        (longaj, mallongaj, infraru&gcirc;aj, ultraviolaj)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=251239"
              >Fizioterapio</url></lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trd
  lng="be">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_a;&c_t;&c_e;&c_r;&c_a;&c_p;&c_ib;&c_ja;</trd>
  <trd lng="ca">electroter&agrave;pia</trd>
  <trd lng="cs">elektroterapie</trd>
  <trd lng="de">Elektrotherapie</trd>
  <trd lng="es">electroterapia</trd>
  <trdgrp lng="fa">
    <trd>&a_b;&a_r;&a_q;&zwnj;&a_d;&a_r;&a_m;&a_A;&a_n;&f_y;</trd>,
    <trd>&a_A;&a_l;&f_k;&a_t;&a_r;&a_w;&a_t;&a_r;&a_A;&f_p;&f_y;</trd>
  </trdgrp>
  <trd lng="fr">&eacute;lectroth&eacute;rapie</trd>
  <trdgrp lng="hu">
    <trd>elektroter&aacute;pia</trd>,
    <trd>villamos <ind>gy&oacute;gykezel&eacute;s</ind></trd>
  </trdgrp>
  <trd lng="it">elettroterapia</trd>
  <trd lng="pl">elektroterapia</trd>
  <trd lng="pt">eletroterapia</trd>
  <trd lng="ru">&c_e;&c_l;&c_je;&c_k;&c_t;&c_r;&c_o;&c_t;&c_je;&c_r;&c_a;&c_p;&c_i;&c_ja;</trd>
  <trd lng="sk">elektrolie&ccaron;ba</trd>
</drv>
</art>
