<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>gilo&scirc;</rad>/i <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gilosx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gilosx.0i.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      Ornami per reta&jcirc;e interplektitaj strekoj surmetalan a&ubreve; surpaperan
      desegna&jcirc;on.
    </dif>
  </snc>
  <trd lng="de">guillochieren</trd>
  <trd lng="fr">guillocher</trd>
  <trd lng="nl">guillocheren</trd>
  <trd lng="pt">fazer <ind>guilhoch&ecirc;s</ind></trd>
  <trd lng="ru">&c_g;&c_i;&c_l;&c_mol;&c_o;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>

<drv mrk="gilosx.0ado">
  <kap><tld/>ado, <var><kap><tld/>o</kap></var></kap>
  <snc>
    <dif>
      Ago a&ubreve; arto tiel ornami.
    </dif>
  </snc>
  <trd lng="de">Guillochieren</trd>
  <trd lng="fr">guillochage<klr tip="ind"> (action)</klr></trd>
  <trd lng="nl">het <ind>guillocheren</ind></trd>
  <trd lng="pt">a&ccedil;&atilde;o de fazer <ind>guilhoch&ecirc;s</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_l;&c_mol;&c_o;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_i;&c_l;&c_mol;&c_o;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="gilosx.0ajxo">
  <kap><tld/>a&jcirc;o, <var><kap><tld/>o</kap></var></kap>
  <snc>
    <dif>
      Tia ornamo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Guilloche</trd>,
    <trd>verschlungene <ind>Linienzeichnung</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>guillochage<klr tip="ind"> (guillochis)</klr></trd>,
    <trd>guilloch&eacute;</trd>,
    <trd>guillochis</trd>
  </trdgrp>
  <trd lng="nl">guilloches</trd>
  <trd lng="pt">guilhoch&ecirc;</trd>
  <trd lng="ru">&c_g;&c_i;&c_l;&c_mol;&c_o;&c_sh;</trd>
</drv>
</art>
