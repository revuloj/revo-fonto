<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gilosx.xml,v 1.15 2022/04/30 08:42:40 revo Exp $">
<kap>
  <rad>gilo&scirc;</rad>/i
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="gilosx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gilosx.0i.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="ornam.0i.iu">Ornami</ref>
      per reta&jcirc;e interplektitaj strekoj surmetalan
      a&ubreve; surpaperan desegna&jcirc;on.
    </dif>
  </snc>
  <trd lng="ca">guilloixar</trd>
  <trd lng="de">guillochieren</trd>
  <trd lng="fr">guillocher</trd>
  <trd lng="nl">guillocheren</trd>
  <trd lng="pt">fazer <ind>guilhoch&ecirc;s</ind></trd>
  <trd lng="ru">&c_g;&c_i;&c_l;&c_mol;&c_o;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="uk">&#x491;&#x456;&#x43B;&#x44C;&#x439;&#x43E;&#x448;&#x438;&#x440;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>

<drv mrk="gilosx.0ado">
  <kap>
    <tld/>ado,
    <var><kap><tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      Ago a&ubreve; arto tiel ornami.
    </dif>
  </snc>
  <trd lng="ca">art de guilloixar, t&egrave;cnica de guilloixar</trd>
  <trd lng="de">Guillochieren</trd>
  <trd lng="fr">guillochage<klr tip="ind"> (action)</klr></trd>
  <trd lng="nl">het <ind>guillocheren</ind></trd>
  <trd lng="pt">a&ccedil;&atilde;o de fazer
  <ind>guilhoch&ecirc;s</ind></trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_i;&c_l;&c_mol;&c_o;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_i;&c_l;&c_mol;&c_o;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>

<drv mrk="gilosx.0ajxo">
  <kap>
    <tld/>a&jcirc;o,
    <var><kap><tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      Tia <ref tip="super" cel="ornam.0o.ajxo">ornamo</ref>:
      <ekz>
        krom dekori juvelojn kaj aliajn arta&jcirc;etojn, <tld/>oj estis gravaj elementoj dum
        la presado de monbiletoj, akcioj, pasportoj kaj aliaj gravaj oficialaj
        dokumentoj, kiuj celis malfaciligi ties falsadon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=271729">Gilo&scirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/23/Guilloche.jpg/209px-Guilloche.jpg" prm="CC-BY-SA-3.0">parto de <tld/>o sur malnova bileto de 5 germanaj markoj (1963)
        <fnt>
          <aut>Ralf Pfeifer</aut>
          <lok><url ref="&WCW;/File:Guilloche.jpg">&WCW;/File:Guilloche.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="ca">guilloixat</trd>
  <trdgrp lng="de">
    <trd>Guilloche</trd>,
    <trd>verschlungene <ind>Linienzeichnung</ind></trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>guillochage<klr tip="ind"> (guillochis)</klr></trd>,
    <trd>guilloch&eacute;</trd>,
    <trd>guillochis</trd>
  </trdgrp>
  <trd lng="nl">guilloches</trd>
  <trd lng="pl">gilosz</trd>
  <trd lng="pt">guilhoch&ecirc;</trd>
  <trd lng="ru">&c_g;&c_i;&c_l;&c_mol;&c_o;&c_sh;</trd>
</drv>
</art>
