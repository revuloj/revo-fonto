<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gilosx.xml,v 1.21 2026/02/22 03:17:05 revo Exp $">
<kap>
  <rad>gilo&scirc;</rad>/i
  <fnt><bib>PVEF</bib></fnt>
</kap>
<drv mrk="gilosx.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gilosx.0i.BELA">
    <uzo tip="fak">BELA</uzo>
    <uzo tip="fak">TEK</uzo>
    <dif>
      <ref tip="super" cel="ornam.0i.iu">Ornami</ref>
      per reta&jcirc;e interplektitaj strekoj surmetalan
      a&ubreve; surpaperan desegna&jcirc;on:
      <ekz>
        horlo&gcirc;oj <klr>[&#8230;]</klr> el blanka ar&gcirc;ento, polurita, a&ubreve; <tld/>ita
        <fnt>
          <vrk><url ref="https://www.bibliotekomolera.org/imatges/horlogxo.pdf"
              >La bona horlo&gcirc;o, prezaro</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        simpla stelo sen akraj pintoj, el herboverda diafana emajlo sur <tld/>ita fundo
        <fnt>
          <vrk><url
          ref="https://upload.wikimedia.org/wikipedia/commons/d/d7/EO_Hector_Hodler_Alphonse_Carl_-_Konsuloj_kaj_Esperanto-oficejoj.pdf"
              >KOnsuloj kaj Esperanto-Oficejoj</url></vrk>
          <lok>La esperantistaj signoj en tri formatoj </lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">guilloixar</trd>
  <trd lng="de">guillochieren</trd>
  <trd lng="fr">guillocher</trd>
  <trd lng="ja">&#x7DE8;&#x307F;&#x7E04;&#x6A21;&#x69D8;&#12434;&#x5F6B;&#x308B; 
  <pr>&#12354;&#x307F;&#x306A;&#x308F;&#12418;&#12424;&#12358;&#12434;&#x307B;&#x308B;</pr></trd>
  <trd lng="nl">guillocheren</trd>
  <trd lng="pl">giloszowa&cacute;</trd>
  <trd lng="pt">fazer <ind>guilhoch&ecirc;s</ind></trd>
  <trd lng="ru">&c_g;&c_i;&c_l;&c_mol;&c_o;&c_sh;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="uk">&c_gu;&c_ib;&c_l;&c_mol;&c_j;&c_o;&c_sh;&c_i;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
  <trd lng="zh">&#x626D;&#x7D22;&#x98FE;&#x82B1;&#x7D0B; <pr>ni&#x1D4;su&#x1D2;sh&igrave;hu&amacron;w&eacute;n</pr></trd>
</drv>

<drv mrk="gilosx.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Ago <tld/>i; ties rezulto, t.e. <tld/>a&jcirc;o:
      <ekz>
        krom dekori juvelojn kaj aliajn arta&jcirc;etojn, <tld/>oj estis gravaj elementoj dum
        la presado de monbiletoj, akcioj, pasportoj kaj aliaj gravaj oficialaj
        dokumentoj, kiuj celis malfaciligi ties falsadon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=271729">Gilo&scirc;o</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/2/23/Guilloche.jpg/209px-Guilloche.jpg" prm="CC-BY-SA-3.0">parto de
    <tld/>o sur malnova bileto de 5 germanaj markoj (1963)
        <fnt>
          <aut>Ralf Pfeifer</aut>
          <lok><url ref="&WCW;/File:Guilloche.jpg">&WCW;/File:Guilloche.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="ca">guilloixat</trd>
    <trdgrp lng="de">
      <trd>Guilloche</trd>,
      <trd>verschlungene <ind>Linienzeichnung</ind></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>guillochage<klr tip="ind"> (guillochis)</klr></trd>,
      <trd>guilloch&eacute;</trd>,
      <trd>guillochis</trd>
    </trdgrp>
    <trdgrp lng="ja">
      <trd>&#x7DE8;&#x307F;&#x7E04;&#x6A21;&#x69D8;  <pr>&#12354;&#x307F;&#x306A;&#x308F;&#12418;&#12424;&#12358;</pr></trd>,
      <trd>&#x7D44;&#x307F;&#x7D10;&#25991;&#x69D8;  <pr>&#x304F;&#x307F;&#12402;&#12418;&#12418;&#12435;&#12424;&#12358;</pr></trd>,
      <trd>&#x30AE;&#x30ED;&#x30FC;&#x30B7;&#x30E5;</trd>
    </trdgrp>
    <trd lng="nl">guilloches</trd>
    <trd lng="pl">gilosz</trd>
    <trd lng="pt">guilhoch&ecirc;</trd>
    <trd lng="ru">&c_g;&c_i;&c_l;&c_mol;&c_o;&c_sh;</trd>
    <trd lng="zh">&#x626D;&#x7D22;&#x98FE; <pr>ni&#x1D4;su&#x1D2;sh&igrave;</pr></trd>
  </snc>
</drv>

