<art mrk="$Id: pimpin.xml,v 1.16 2023/05/11 08:30:47 revo Exp $">
<kap>
  <rad>pimpinel</rad>/o
</kap>
<drv mrk="pimpin.0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt" cel="apiac.0oj">apiacoj</ref>
      (<trd lng="la">Pimpinella</trd>):
      <ekz>
        al la frankfurta verda sa&ubreve;co tradicie apartenas almena&ubreve; sep herboj: 
        borago, cerefolio, kreso, petroselo, <tld/>o, okzalo kaj &scirc;enoprazo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=97120">Verda sa&ubreve;co</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sub" cel="aniz.0ujo">anizujo</ref>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_ja;&c_d;&c_r;&c_y;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_g;&c_a;&c_n;&c_y;&c_zh;</trd>
  </trdgrp>
  <trd lng="cs">bedrn&iacute;k</trd>
  <trd lng="de">Bibernelle</trd>
  <trd lng="fa">&a_j;&a_ayn;&a_f;&a_r;&f_y; &f_k;&a_w;&a_h;&f_y;</trd>
  <trd lng="fr">pimpinelle</trd>
  <trd lng="hu">f&ouml;ldit&ouml;mj&eacute;n</trd>
  <trd lng="nl">bevernel</trd>
  <trd lng="pl">biedrzeniec any&zdot;</trd>
  <trd lng="pt">pimpinela</trd>
  <trd lng="ro">anason</trd>
  <trd lng="ru">&c_b;&c_je;&c_d;&c_r;&c_je;&c_n;&c_je;&c_c;</trd>
  <trd lng="sk">bedrovn&iacute;k</trd>
  <trd lng="sv">bockrot</trd>
  <trd lng="uk">&c_b;&c_je;&c_d;&c_r;&c_i;&#x43D;&c_je;&c_c;&#x44C;</trd>
</drv>
</art>
