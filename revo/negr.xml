<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>negr</rad>/o
</kap>
<drv mrk="negr.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Homo de nigra raso; nigrulo.
    </dif>
  </snc>                     
  <trd lng="be">&c_n;&c_e;&c_g;&c_a;&c_r;</trd>
  <trd lng="br">morian</trd>
  <trdgrp lng="de">
    <trd>Schwarzer</trd>,
    <trd>Neger</trd>
  </trdgrp>
  <trdgrp lng="he">
    <trd>&kaf;&vav;&shin;&yod;</trd>,
    <trd>&shin;&het;&vav;&resh;<klr tip="ind"> (&kaf;&vav;&shin;&yod;)</klr></trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>n&eacute;ger</trd>,
    <trd>fekete</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>zwarte</trd>,
    <trd>neger</trd>
  </trdgrp>
  <trd lng="pt">negro</trd>
  <trd lng="ru">&c_n;&c_je;&c_g;&c_r;</trd>
  <trd lng="sv">neger</trd>
</drv>
</art>
