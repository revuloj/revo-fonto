<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pelika.xml,v 1.31 2024/02/17 06:43:05 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pelikan</rad>/o
</kap>

<drv mrk="pelika.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pelika.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO" lst="voko:zoologiaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="pelika1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Pelecanus</trd>), granda rempieda marbirdo havanta
      la&ubreve;longe sub la beko grandan po&scirc;on, en kiu &gcirc;i
      konservas la kaptitajn fi&scirc;ojn:
      <ekz>
        Oni foje uzas bildon de <tld/>o kiel simbolon de la
        sindona patra a&ubreve; patrina amo.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_e;&c_l;&c_ib;&c_k;&c_a;&c_n;</trd>
  <trd lng="bg">&c_p;&c_je;&c_l;&c_i;&c_k;&c_a;&c_n;</trd>
  <trd lng="br">pilikant</trd>
  <trd lng="cs">pelik&aacute;n</trd>
  <trd lng="de">Pelikan</trd>
  <trd lng="en">pelican</trd>
  <trd lng="fr">p&eacute;lican</trd>
  <trdgrp lng="hu">
    <trd>g&ouml;d&eacute;ny</trd>,
    <trd>pelik&aacute;n</trd></trdgrp>
  <trd lng="ja">&#x30DA;&#x30EA;&#x30AB;&#x30F3;</trd>
  <trd lng="nl">pelikaan</trd>
  <trd lng="pl">pelikan</trd>
  <trd lng="pt">pelicano</trd>
  <trd lng="ru">&c_p;&c_je;&c_l;&c_i;&c_k;&c_a;&c_n;</trd>
  <trd lng="sk">pelik&aacute;n</trd>
  <trd lng="uk">&#x43F;&c_je;&#x43B;&c_ib;&#x43A;&c_a;&#x43D;</trd>
  <trdgrp lng="zh">
    <trd>&#x9D5C;&#x9D98; <pr>t&#xED;h&#xFA;</pr></trd>,
    <trd>&#x9E48;&#x9E55; <pr>t&#xED;h&#xFA;</pr></trd>,
    <trd>&#x6DD8;&#x6CB3; <pr>t&#xE1;oh&#xE9;</pr></trd>,
    <trd>&#x9D98; <pr>h&#xFA;</pr></trd>,
    <trd>&#x9E55; <pr>h&#xFA;</pr></trd>,
    <trd>&#x9DA6; <pr>h&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pelika.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="pelika.0oformaj.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="ord.0o.BIO" lst="voko:zoologiaj_ordoj">Ordo</ref> el la klaso <ref tip="malprt" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Pelecaniformes</trd>).
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_e;&c_l;&c_ib;&c_k;&c_a;&c_n;&c_a;&c_p;&c_a;&c_d;&c_o;&c_b;&c_n;&c_y;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>,
    <trd>&c_v;&c_je;&c_s;&c_l;&c_a;&c_n;&c_o;&c_g;&c_ib;&c_ja; <klr>(&c_a;&c_t;&c_r;&c_a;&c_d;)</klr></trd>
  </trdgrp>
  <trd lng="de">Ruderf&uuml;&szlig;er</trd>
  <trd lng="en">pelecaniformes</trd>
  <trd lng="hu">g&ouml;d&eacute;nyalak&uacute;ak</trd>
  <trd lng="nl">roeivoetigen</trd>
  <trdgrp lng="pl">
    <trd>pe&lstroke;nop&lstroke;etwe</trd>,
    <trd>wios&lstroke;onogie</trd>
  </trdgrp>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_l;&c_o;&c_n;&c_o;&c_g;&c_i;&c_je;
  <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d;
  &c_p;&c_t;&c_i;&c_c;)</klr></trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_je;&#x43B;&c_ib;&#x43A;&c_a;&#x43D;&#x43E;&#x43F;&#x43E;&c_d;&c_ib;&c_b;&#x43D;&c_ib;</trd>,
    <trd>&c_v;&c_je;&c_s;&#x43B;&#x43E;&#x43D;&#x43E;&c_g;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x9D5C;&#x5F62;&#x76EE; <pr>t&#xED;x&#xED;ngm&#xF9;</pr></trd>,
    <trd>&#x9E48;&#x5F62;&#x76EE; <pr>t&#xED;x&#xED;ngm&#xF9;</pr></trd>
  </trdgrp>
</drv>
</art>
