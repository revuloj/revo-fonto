<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pelikan</rad>/o
</kap>
<drv mrk="pelika.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="pelika.0o.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="pelika1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Pelecanus</trd>), granda rempieda marbirdo havanta
      la&ubreve;longe sub la beko grandan po&scirc;on, en kiu &gcirc;i konservas la kaptitajn
      fi&scirc;ojn:
      <ekz>
        Oni foje uzas bildon de <tld/>o kiel simbolon de la
        sindona patra a&ubreve; patrina amo.
      </ekz>
    </dif>
  </snc>                        
  <trd lng="be">&c_p;&c_e;&c_l;&c_ib;&c_k;&c_a;&c_n;</trd>
  <trd lng="br">pilikant</trd>
  <trd lng="de">Pelikan</trd>
  <trd lng="fr">p&eacute;lican</trd>
  <trdgrp lng="hu">
    <trd>g&ouml;d&eacute;ny</trd>,
    <trd>pelik&aacute;n</trd></trdgrp>
  <trd lng="nl">pelikaan</trd>
  <trd lng="pl">pelikan</trd>
  <trd lng="pt">pelicano</trd>
  <trd lng="ru">&c_p;&c_je;&c_l;&c_i;&c_k;&c_a;&c_n;</trd>
</drv>

<drv mrk="pelika.0oformaj">
  <kap><tld/>oformaj</kap>
  <snc mrk="pelika.0oformaj.ZOO">
  <uzo tip="fak">ZOO</uzo>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Pelecaniformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Ruderf&uuml;&szlig;er</trd>
  <trd lng="hu">g&ouml;d&eacute;nyalak&uacute;ak</trd>
  <trd lng="nl">roeivoetigen</trd>
  <trd lng="ru">&c_v;&c_je;&c_s;&c_l;&c_o;&c_n;&c_o;&c_g;&c_i;&c_je; <klr tip="amb">(&c_o;&c_t;&c_r;&c_ja;&c_d; &c_p;&c_t;&c_i;&c_c;)</klr></trd>
</drv>
</art>
