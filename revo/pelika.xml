<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>pelikan</rad>/o
</kap>
<drv mrk="pelika.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="pelika.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Pelecanus</trd>), granda rempieda marbirdo havanta
      la&ubreve;longe sub la beko grandan
      po&scirc;on, en kiu &gcirc;i konservas la kaptitajn
      fi&scirc;ojn:
      <ekz>
        Oni foje uzas bildon de <tld/>o kiel simbolon de la
        sindona patr(in)a amo.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Pelikan</trd>
</drv>
<drv mrk="pelika.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="pelika.0oformaj"><tld/>oformaj
      birdoj</ref> (<trd lng="la">Pelecanidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Pelikane <klr tip="ind">(Familie)</klr></trd>
</drv>
<drv mrk="pelika.0oformaj">
  <kap><tld/>oformaj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Ordo el la klaso <ref tip="super" cel="bird.0oj">birdoj</ref>
      (<trd lng="la">Pelecaniformes</trd>).
    </dif>
  </snc>
  <trd lng="de">Ruderf&uuml;&szlig;er</trd>
</drv>
</art>
