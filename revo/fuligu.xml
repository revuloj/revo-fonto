<art mrk="$Id: fuligu.xml,v 1.22 2023/11/25 18:29:25 revo Exp $">
<kap><rad>fuligul</rad>/o</kap>
<drv mrk="fuligu.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO" lst="voko:zoologiaj_specioj">Specio</ref>
      el familio <ref tip="malprt" cel="anased.0oj">anasedoj</ref>
      (<trd lng="la">Aythya fuligula</trd>),
      <ref tip="lst" cel="bird.nagxo0o" lst="voko:birdoj">na&gcirc;birdo</ref>
      kun <ref tip="prt" cel="tuf.0o">tufo </ref> sur la malanta&ubreve;a
      kapo:
      <ekz>
        la tufanaso a&ubreve; <tld/>o estas <klr>[&#8230;]</klr> mezgranda specio de la grupo de la
        merganasoj, kun populacio de preska&ubreve; unu miliono da birdoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=127033">Tufanaso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="anas.tuf0o">tufanaso</ref>
    <bld
    lok="&WCU;/thumb/4/4a/Aythya_fuligula_1002648781.JPEG/480px-Aythya_fuligula_1002648781.JPEG"
    prm="CC BY-SA 3.0"><tld/>o (Aythya fuligula) &ccirc;e Stokholmo, Svedujo
        <fnt>
          <aut>Bengt Nyman, propra verko</aut>
          <lok><url
          ref="&WCW;/File:Aythya_fuligula_1002648781.JPEG">&WCW;/File:Aythya_fuligula_1002648781.JPEG</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_r;&c_n;&c_je;&c_c; &c_ch;&c_u;&c_b;&c_a;&c_t;&c_y;</trd>
  <trd lng="ca">morell de plomall</trd>
  <trdgrp lng="cs">
    <trd>kachna chocholat&aacute;</trd>,
    <trd>pol&aacute;k chochola&ccaron;ka</trd>
  </trdgrp>
  <trd lng="de">Reiherente</trd>
  <trd lng="en">tufted duck</trd>
  <trd lng="fr">fuligule</trd>
  <trd lng="hu">kontyos r&eacute;ce</trd>
  <trdgrp lng="id">
    <trd><ind>itik</ind> rumbai</trd>,
    <trd><ind>kambangan</ind> hitam</trd>
  </trdgrp>
  <trd lng="ja">&#x30AD;&#x30F3;&#x30AF;&#x30ED;&#x30CF;&#x30B8;&#x30ED;</trd>
  <trd lng="nl">kuifeend</trd>
  <trd lng="pl">czernica</trd>
  <trdgrp lng="pt">
    <trd>ful&iacute;gula</trd>,
    <trd>negrina</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_o;&c_h;&c_l;&c_a;&c_t;&c_a;&c_ja; &c_ch;&c_je;&c_r;&c_n;&c_je;&c_t;&c_mol;</trd>
  <trd lng="sk">chochla&ccaron;ka vrko&ccaron;at&aacute;</trd>
  <trd lng="uk">&#x43D;&c_i;&c_r;&#x43E;&#x43A; &c_ch;&c_u;&c_b;&c_a;&c_t;&c_i;&c_j;</trd>
  <trd lng="zh">&#x51E4;&#22836;&#x6F5C;&#x9E2D; <pr>f&#xE8;ngt&#xF3;uqi&#xE1;ny&amacron;</pr></trd>
</drv>
