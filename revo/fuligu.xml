<vortaro>
<art mrk="$Id$">
<kap><rad>fuligul</rad>/o</kap>
<drv mrk="fuligu.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">ZOO</uzo>
    <dif>
      <ref tip="lst" cel="speci.0o.BIO"
      lst="voko:zoologiaj_specioj">Specio</ref> 
      el familio <ref tip="malprt" cel="anased.0oj">anasedoj</ref>
      (<trd lng="la">Aythya fuligula</trd>), 
      <ref tip="lst" cel="bird.nagx0o" lst="voko:birdoj">na&gcirc;birdo</ref>
      kun <ref tip="prt" cel="tuf.0o">tufo </ref> sur la malanta&ubreve;a
      kapo.
    </dif>
    <ref tip="sin" cel="anas.tuf0o">tufanaso</ref>
  </snc>
  <trd lng="ca">morell de plomall</trd>
  <trd lng="cs">kachna chocholat&aacute;</trd>
  <trd lng="de">Reiherente</trd>
  <trd lng="en">tufted duck</trd>
  <trd lng="fr">fuligule</trd>
  <trd lng="nl">kuifeend</trd>
  <trd lng="hu">kontyos r&eacute;ce</trd>
  <trd lng="pl">czernica</trd>
  <trdgrp lng="pt">
    <trd>ful&iacute;gula</trd>,
    <trd>negrina</trd>
  </trdgrp>
  <trd lng="ru">&c_h;&c_o;&c_h;&c_l;&c_a;&c_t;&c_a;&c_ja; &c_ch;&c_je;&c_r;&c_n;&c_je;&c_t;&c_mol;</trd>
</drv>
</art>
