<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>falset</rad>/o
</kap>
<drv mrk="falset.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="tembr.0o.MUZ">Tembro</ref>
      de vira <ref tip="vid" cel="vocx.kap0o">kapvo&ccirc;o</ref>
      uzata por kanti en alda a&ubreve; soprana registro.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Falsett</trd>,
    <trd>Kopfstimme</trd>,
    <trd>Fistelstimme</trd>
  </trdgrp>
  <trd lng="fr">fausset<klr> (musique)</klr></trd>
  <trdgrp lng="hu">
    <trd>fejhang</trd>,
    <trd>falzett</trd>,
    <trd>falsetto</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>falset</trd>,
    <trd>kopstem</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_l;&c_mol;&c_c;&c_je;&c_t;</trd>
</drv>

<drv mrk="falset.0ulo">
  <kap><tld/>ulo</kap>
  <snc>
    <dif>
      Vira <ref tip="super" cel="kant.0isto.profesio">kantisto</ref>
      per falseto kantanta kiel aldulo a&ubreve; sopranulo.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>falsettiste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>falzettista</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_l;&c_mol;&c_c;&c_je;&c_t;<klr tip="amb"> (&c_p;&c_je;&c_v;&c_je;&c_c;)</klr></trd>
</drv>
</art>
