<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>falset</rad>/o
</kap>
<drv mrk="falset.0o">
  <kap><tld/>o</kap>
  <snc mrk="falset.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kapvo&ccirc;a <ref tip="super" cel="regist.0o.MUZ">registro</ref>,
      kiun viroj povas uzi por kanti same altatone kiel inoj:
      <ekz>
        viroj pene kantis per <tld/>o, dum virinoj imitis viran bason
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 30a, p. 413a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_c;&c_e;&c_t; <klr>(&c_g;&c_o;&c_l;&c_a;&c_s;)</klr></trd>
  <trdgrp lng="de">
    <trd>Falsett</trd>,
    <trd>Kopfstimme</trd>,
    <trd>Fistelstimme</trd>
  </trdgrp>
  <trd lng="fr">fausset<klr> (musique)</klr></trd>
  <trdgrp lng="hu">
    <trd>fejhang</trd>,
    <trd>falzett</trd>,
    <trd>falsetto</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>falset</trd>,
    <trd>kopstem</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_l;&c_mol;&c_c;&c_je;&c_t;</trd>
</drv>

<drv mrk="falset.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="falset.0ulo.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Vira <ref tip="super" cel="kant.0isto.profesio">kantisto</ref>
      kantanta per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_c;&c_e;&c_t; <klr>(&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_k;)</klr></trd>
  <trdgrp lng="fr">
    <trd>falsettiste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>falzettista</trd>
  </trdgrp>
  <trd lng="ru">&c_f;&c_a;&c_l;&c_mol;&c_c;&c_je;&c_t;<klr tip="amb"> (&c_p;&c_je;&c_v;&c_je;&c_c;)</klr></trd>
</drv>
</art>
