<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: falset.xml,v 1.22 2023/11/09 19:13:15 revo Exp $">
<kap>
  <rad>falset</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="falset.0o">
  <kap><tld/>o</kap>
  <snc mrk="falset.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Kapvo&ccirc;a <ref tip="super" cel="regist1.0o.vocxo">registro</ref>,
      kiun viroj povas uzi por kanti same altatone kiel virinoj:
      <ekz>
        viroj pene kantis per <tld/>o, dum virinoj imitis viran bason
        <fnt><bib>LaFamilio</bib>, <lok>&ccirc;apitro 30a, p.
        413a</lok></fnt>;
      </ekz>
      <ekz>
        la majoro atingis la plej gravan frazon en sia raporto kaj lia akra vo&ccirc;o nun
        alti&gcirc;is en <tld/>on
        <fnt>
          <bib>KKr</bib>
          <lok>I</lok>
        </fnt>;
      </ekz>
      <ekz>
        pli kaj pli klare a&ubreve;di&gcirc;is proksimi&gcirc;antaj tamburbatoj kaj <tld/>aj trumpetsonoj
        <fnt>
          <bib>MkM</bib>
          <lok>19</lok>
        </fnt>;
      </ekz>
      <ekz>
        malofte, sed tamen <tld/>anta, la vo&ccirc;o de l&#x2019; kantisto postlasas agrablan impreson
        <fnt>
          <bib>LOdE</bib>
          <lok>2000, No 12</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_c;&c_e;&c_t;
  <klr>(&c_g;&c_o;&c_l;&c_a;&c_s;)</klr></trd>
  <trdgrp lng="cs">
    <trd>falzet</trd>,
    <trd>fistule</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Falsett</trd>,
    <trd>Kopfstimme</trd>,
    <trd>Fistelstimme</trd>
  </trdgrp>
  <trd lng="fr">fausset<klr> (musique)</klr></trd>
  <trdgrp lng="hu">
    <trd>fejhang</trd>,
    <trd>falzett</trd>,
    <trd>falsetto</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30D5;&#x30A1;&#x30EB;&#x30BB;&#x30C3;&#x30C8;</trd>,
    <trd>&#x88CF;&#x58F0;  <pr>&#x3046;&#x3089;&#x3054;&#x3048;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>falset</trd>,
    <trd>kopstem</trd>
  </trdgrp>
  <trd lng="pl">falset</trd>
  <trd lng="pt">falsete</trd>
  <trd lng="ru">&c_f;&c_a;&c_l;&c_mol;&c_c;&c_je;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>falzet</trd>,
    <trd>fistula</trd>
  </trdgrp>
  <trd lng="uk">&c_f;&c_a;&#x43B;&#x44C;&c_c;&c_je;&c_t;</trd>
  <trdgrp lng="zh">
    <trd>&#x5047;&#x97F3; <pr>ji&#x1CE;y&#x12B;n</pr></trd>,
    <trd>&#x5047;&#x58F0; <pr>ji&#x1CE;sh&#x113;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="falset.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="falset.0ulo.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Vira <ref tip="super" cel="kant.0isto.profesio">kantisto</ref>
      kantanta per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_l;&c_mol;&c_c;&c_e;&c_t;
  <klr>(&c_s;&c_mol;&c_p;&c_ja;&c_v;&c_a;&c_k;)</klr></trd>
  <trdgrp lng="fr">
    <trd>falsettiste</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>falzettista</trd>
  </trdgrp>
  <trd lng="pl">falset</trd>
  <trd lng="ru">&c_f;&c_a;&c_l;&c_mol;&c_c;&c_je;&c_t;<klr tip="amb">
  (&c_p;&c_je;&c_v;&c_je;&c_c;)</klr></trd>
</drv>
</art>
