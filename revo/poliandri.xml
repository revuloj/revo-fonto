<art mrk="$Id: poliandri.xml,v 1.11 2023/10/20 16:38:17 revo Exp $">
<kap>
  <rad>poliandri</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="poliandri.0o">
  <kap><tld/>o</kap>
  <snc mrk="poliandri.0o.GEN">
    <uzo tip="fak">GEN</uzo>
    <dif>
      <ref tip="dif" cel="edz.plur0eco.GEN">Pluredzeco</ref>:
      <ekz>
        virinoj la&ubreve;le&gcirc;e havas kelkajn edzojn kaj tiun &ccirc;i manieron oni 
        nomas <klr>[&#x2026;]</klr> esperante <tld/>o
        <fnt>
          <aut>S. Rafat Nami</aut>,
          <vrk><url ref="http://www.ipernity.com/blog/saidnamigs2/180089">
          Tibeto</url></vrk>,
          <lok>2009-09-03</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <snc mrk="poliandri.0o.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Ordinara pari&gcirc;o de ino kun pluraj viroj:
      <ekz>
        femaloj en Lepidoptera povas kopulacii pli ol unu 
        fojo<klr>n</klr> 
        (<tld/>o)
        <fnt>
          <aut>W. J. Teles Pontes</aut>,
          <vrk><url ref="http://www.teleskopo.com/Poliandrio%20kaj%20Monandrio%20en%20Lepidoptera.pdf">
          Poliandrio kaj Monandrio en Lepidoptera</url></vrk>,
          <lok>Teleskopo, 2009 (1)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_a;&c_n;&c_d;&c_r;&c_y;&c_ja;</trd>
  <trdgrp lng="cs">
    <trd>mnohomu&zcaron;stv&iacute;</trd>,
    <trd>polyandrie</trd>
  </trdgrp>
  <trd lng="de">Polyandrie</trd>
  <trd lng="en">polyandry</trd>
  <trd lng="es">poliandria</trd>
  <trd lng="fr">polyandrie</trd>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x59BB;&#x591A;&#x592B;  <pr>&#x3044;&#x3063;&#x3055;&#x3044;&#x305F;&#x3075;</pr></trd>,
    <trd>&#x4E00;&#x96CC;&#x591A;&#x96C4;  <pr>&#x3072;&#x3068;&#x3081;&#x3059;&#x305F;&#x304A;&#x3059;</pr></trd>
  </trdgrp>
  <trd lng="pl">poliandria</trd>
  <trd lng="ro">poliandria</trd>
  <trd lng="sk">mnohomu&zcaron;stvo</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_g;&c_a;&c_t;&#x43E;&#x43C;&c_u;&c_zh;&c_zh;&#x44F;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&c_ib;&c_a;&#x43D;&c_d;&c_r;&c_ib;&#x44F;</trd>
  </trdgrp>
</drv>
</art>

