<art mrk="$Id: kruel.xml,v 1.20 2023/12/03 09:59:02 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kruel</rad>/a
</kap>
<drv mrk="kruel.0a">
  <kap>
    <ofc>*</ofc><tld/>a
  </kap>
  <snc>
    <uzo tip="klr">(iu)</uzo>
    <dif>
      Inklina suferigi, malhumana, turmentema:
      <ekz>
        <tld/>a besto, koro, tirano, dio;
      </ekz>
      <ekz>
        <tld/>a kiel tigro;
      </ekz>
      <ekz>
        <tld/>e frapi.
      </ekz>
    </dif>
    <trdgrp lng="de">
     <trd>grausam</trd>,
     <trd>herzlos</trd>,
     <trd>gemein</trd>,
     <trd>unbarmherzig</trd>,
     <trd>unmenschlich</trd>
    </trdgrp>
    <trd lng="hu">kegyetlen</trd>
  </snc>
  <snc>
    <uzo tip="klr">(io)</uzo>
    <dif>
      Suferiga, turmenta, doloriga, mal&gcirc;ojiga, aflikta:
      <ekz>
        <tld/>a vundo, doloro, ordono, sorto, insulto, perdo, morto,
        seniluzii&gcirc;o;
      </ekz>
      <ekz>
        la honoro estas ja bela, sed la laboro <tld/>a<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>e elrevigita;
      </ekz>
      <ekz>
        la <tld/>eco de l' fatalo.
      </ekz>
    </dif>
    <trdgrp lng="de">
     <trd>schlimm</trd>,
     <trd>hart</trd>
    </trdgrp>
    <trd lng="hu">borzalmas</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_zh;&c_o;&c_r;&c_s;&c_t;&c_k;&c_ib;</trd>,
    <trd>&c_l;&c_ju;&c_t;&c_y;</trd>,
    <trd>&c_b;&c_ja;&c_z;&c_mol;&c_l;&c_ib;&c_t;&c_a;&c_s;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="ca">cruel</trd>
  <trd lng="en">cruel</trd>
  <trd lng="es">cruel</trd>
  <trd lng="fr">cruel</trd>
  <trdgrp lng="ja">
    <trd>&#x6B8B;&#x9177;&#x306A;  <pr>&#x3056;&#x3093;&#x3053;&#x304F;&#x306A;</pr></trd>,
    <trd>&#x3080;&#x3054;&#x3044;</trd>,
    <trd>&#x53B3;&#x3057;&#x3044;  <pr>&#x304D;&#x3073;&#x3057;&#x3044;</pr></trd>,
    <trd>&#x60B2;&#x60E8;&#x306A;  <pr>&#x3072;&#x3055;&#x3093;&#x306A;</pr></trd>
  </trdgrp>
  <trd lng="nl">wreed</trd>
  <trd lng="pl">okrutny</trd>
  <trdgrp lng="pt">
    <trd>cruel</trd>,
    <trd>desalmado</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>atroce</trd>,
    <trd>barbar</trd>,
    <trd>cruzime</trd>
  </trdgrp> 
  <trd lng="ru">&c_zh;&c_je;&c_s;&c_t;&c_o;&c_k;&c_i;&c_j;</trd>
  <trd lng="th">&#xE42;&#xE2B;&#xE14;&#xE23;&#xE49;&#xE32;&#xE22;</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&#x43E;&c_r;&c_s;&c_t;&#x43E;&#x43A;&c_i;&c_j;</trd>,
    <trd>&#x43B;&#x44E;&c_t;&c_i;&c_j;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6B8B;&#x9177; <pr>c&#xE1;nk&#xF9;</pr></trd>,
    <trd>&#x6B98;&#x9177; <pr>c&#xE1;nk&#xF9;</pr></trd>,
    <trd>&#x6B8B; <pr>c&#xE1;n</pr></trd>,
    <trd>&#x6B98; <pr>c&#xE1;n</pr></trd>,
    <trd>&#x6B8B;&#x5FCD; <pr>c&#xE1;nr&#x11B;n</pr></trd>,
    <trd>&#x6B98;&#x5FCD; <pr>c&#xE1;nr&#x11B;n</pr></trd>,
    <trd>&#x51F6;&#x72E0; <pr>xi&#x14D;ngh&#x11B;n</pr></trd>,
    <trd>&#x66B4; <pr>b&#xE0;o</pr></trd>,
