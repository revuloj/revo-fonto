<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>miozot</rad>/o
</kap>
<drv mrk="miozot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Genro el familio <ref tip="super"
      cel="boraga.0oj">boragacoj</ref>,
      (<trd lng="la">Myosotis</trd>),
      kun bluaj floretoj, simbolo de fidela neforgeso.
    </dif>
  </snc>                            
  <trd lng="be">&c_n;&c_je;&c_z;&c_a;&c_b;&c_u;&c_d;&c_k;&c_a;</trd>
  <trd lng="br">daoulagad-ar-Werc'hez</trd>
  <trd lng="de">Vergissmeinnicht</trd>
  <trd lng="en">forget-me-not</trd>   
  <trd lng="fa">&a_f;&a_r;&a_A;&a_m;&a_w;&a_sh;&a_m;&zwnj;&a_m;&f_k;&a_n;</trd>
  <trd lng="fr">myosotis</trd>
  <trd lng="hu">nefelejcs</trd>
  <trd lng="nl">vergeet-mij-niet</trd>
  <trd lng="ru">&c_n;&c_je;&c_z;&c_a;&c_b;&c_u;&c_d;&c_k;&c_a;</trd>
  <trd lng="uk">&c_n;&c_je;&c_z;&c_a;&c_b;&c_u;&c_d;&c_k;&c_a;</trd>
</drv>
</art>
