<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: miozot.xml,v 1.23 2020/08/29 21:10:34 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>miozot</rad>/o
</kap>
<drv mrk="miozot.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      <ref tip="lst" cel="genr.0o.BIO"
      lst="voko:botanikaj_genroj">Genro</ref> el familio <ref
      tip="malprt"
      cel="boraga.0oj">boragacoj</ref>
      (<trd lng="la">Myosotis</trd>),
      <ref tip="lst" cel="plant.floro0o" 
      lst="voko:boragoparencoj">floroplanto</ref>
      kun bluaj floretoj, simbolo de fidela 
      <ref cel="memor.0o">neforgeso</ref>:
      <ekz>
        jen estas rosmareno, &gcirc;i estas por memoro: mi petas vin,
        kara mia koro, memoru min, kaj jen estas <tld/>o, pro la fideleco
        <fnt>
          <bib>Hamlet</bib>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/7/75/Myosotis_sylvatica%2802%29.jpg/520px-Myosotis_sylvatica%2802%29.jpg"
      prm="CC-BY-SA-3.0"
      ><tld/>oj (Myosotis sylvatica)
        <fnt>
          <lok><url ref="&WCW;/File:Myosotis_sylvatica(02).jpg"
              >&WCW;/File:Myosotis_sylvatica(02).jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_z;&c_a;&c_b;&c_u;&c_d;&c_k;&c_a;</trd>
  <trd lng="bg">&c_n;&c_je;&c_z;&c_a;&c_b;&c_r;&c_a;&c_v;&c_k;&c_a;</trd>
  <trd lng="br">daoulagad-ar-Werc'hez</trd>
  <trd lng="ca">miosotis</trd>
  <trd lng="cs">pomn&ecaron;nka</trd>
  <trd lng="de">Vergissmeinnicht</trd>
  <trd lng="en">forget-me-not</trd>
  <trd lng="fa">&a_f;&a_r;&a_A;&a_m;&a_w;&a_sh;&a_m;&zwnj;&a_m;&f_k;&a_n;</trd>
  <trd lng="fr">myosotis</trd>
  <trd lng="hu">nefelejcs</trd>
  <trd lng="nl">vergeet-mij-niet</trd>
  <trdgrp lng="pl">
    <trd>niezapominajka</trd>,
    <trd>niezabudka</trd>
  </trdgrp>
  <trd lng="ro">nu-m&abreve;-uita</trd>
  <trd lng="ru">&c_n;&c_je;&c_z;&c_a;&c_b;&c_u;&c_d;&c_k;&c_a;</trd>
  <trd lng="sk">nez&aacute;budka</trd>
  <trd lng="uk">&c_n;&c_je;&c_z;&c_a;&c_b;&c_u;&c_d;&c_k;&c_a;</trd>
</drv>
</art>
