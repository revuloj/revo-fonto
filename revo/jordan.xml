<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>Jordani</rad>/o
</kap>
<drv mrk="jordan.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">GEOG</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta en
      okcidenta <ref tip="malprt" cel="azi.A0o">Azio</ref>
      kaj limanta okcidente al
      <ref tip="vid" cel="israel.0o">Israelo</ref> &ccirc;e
      <ref tip="vid" cel="jordan1.0o">Jordano</ref>, norde al
      <ref tip="vid" cel="siri.0o">Sirio</ref>, oriente
      al <ref tip="vid" cel="irak.0o">Irako</ref>, sud-oriente
      kaj sude al
      <ref tip="vid" cel="arab.SaudaA0ujo">Sauda Arabujo</ref>.
      <ref tip="prt" cel="aman.0o">Amano</ref> estas la
      <ref tip="vid" cel="urb.cxef0o">&ccirc;efurbo</ref> kaj
      plej granda urbo.
    </dif>
  </snc>
  <trd lng="ca">Jordània</trd>
  <trd lng="en">Jordan <klr tip="ind">(country)</klr></trd>
  <trd lng="es">Jordania</trd>
  <trd lng="he">&yod;&resh;&dalet;&fnun;
    <klr tip="ind">(&mem;&dalet;&yod;&nun;&he;)</klr>
  </trd>
  <trd lng="ru">&c_I;&c_o;&c_r;&c_d;&c_a;&c_n;&c_i;&c_ja;</trd>
</drv>

<drv mrk="jordan.j0a">
  <kap><tld lit="j"/>a</kap>
  <snc>
    <dif>
      Rilata al <tld lit="j"/>anoj, ilia nacieco
      a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="ca">jordà</trd>
  <trd lng="en">Jordanian</trd>
  <trd lng="es">jordano</trd>
  <trd lng="he">&yod;&resh;&dalet;&nun;&yod;</trd>
  <trd lng="ru">&c_i;&c_o;&c_r;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
</drv>

<drv mrk="jordan.j0ano">
  <kap><tld lit="j"/>ano</kap>
  <snc>
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="jordan.0o"><tld/>o</ref>.
    </dif>
  </snc>
  <trd lng="ca">jordà</trd>
  <trd lng="en">Jordanian</trd>
  <trd lng="es">jordano</trd>
  <trd lng="he">&yod;&resh;&dalet;&nun;&yod;</trd>
  <trd lng="ru">&c_i;&c_o;&c_r;&c_d;&c_a;&c_n;&c_je;&c_c;</trd>
</drv>
</art>

