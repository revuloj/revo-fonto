<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: jordan.xml,v 1.28 2023/12/03 07:09:18 revo Exp $">
<kap>
  <rad>Jordani</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="jordan.J0o">
  <kap><tld/>o</kap>
  <snc mrk="jordan.J0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <dif>
      <ref tip="super" cel="sxtat.0o">&Scirc;tato</ref> situanta en
      okcidenta <ref tip="malprt" cel="azi.A0o">Azio</ref>
      kaj limanta okcidente al
      <ref tip="vid" cel="israel.I0o">Israelo</ref> &ccirc;e
      <ref tip="vid" cel="jordan1.J0o">Jordano</ref>, norde al
      <ref tip="vid" cel="siri.S0o">Sirio</ref>, oriente
      al <ref tip="vid" cel="irak.I0o">Irako</ref>, sud-oriente
      kaj sude al
      <ref tip="vid" cel="arab.SaudaA0ujo">Sauda Arabujo</ref>;
      Amano estas &gcirc;ia
      <ref tip="vid" cel="urb.cxef0o">&ccirc;efurbo</ref> kaj
      plej granda urbo:
      <ekz>
        Israelo <klr>[&#x2026;]</klr> proponis redoni tiujn teritoriojn al <tld/>o kaj Egiptio
        kiel parto de pactraktato
        <fnt>
          <bib>Monato</bib>
          <aut>Aleksandro Gofen</aut>
          <vrk><url ref="&Monato;/2003/008132.html">Misinforma komento</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        anka&ubreve; Egiptio, Arabio, <tld/>o, Sirio proklamis la samajn principojn kaj poste,
        pere de paciencaj, longaj, kompromisaj traktadoj, ili &leftquot;rekonis&#x201D; Israelon
        <fnt>
          <bib>Monato</bib>
          <aut>Sen Rodin</aut>
          <vrk><url ref="&Monato;/2009/009725.php">Nu, karaj fratoj ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Ja;&c_r;&c_d;&c_a;&c_n;&c_ib;&c_ja;</trd>
  <trd lng="bg">&c_J;&c_o;&c_r;&c_d;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trd lng="ca">Jord&agrave;nia</trd>
  <trd lng="cs">Jord&aacute;nsko</trd>
  <trd lng="de">Jordanien</trd>
  <trd lng="en">Jordan <klr tip="ind">(country)</klr></trd>
  <trd lng="es">Jordania</trd>
  <trd lng="fr">Jordanie</trd>
  <trd lng="he">&yod;&resh;&dalet;&fnun; <klr tip="ind">(&mem;&dalet;&yod;&nun;&he;)</klr></trd>
  <trd lng="hu">Jord&aacute;nia</trd>
  <trd lng="id">Yordania</trd>
  <trd lng="it">Giordania</trd>
  <trd lng="ja">&#x30E8;&#x30EB;&#x30C0;&#x30F3;  <pr>&#x3088;&#x308B;&#x3060;&#x3093;</pr></trd>
  <trd lng="nl">Jordani&euml;</trd>
  <trd lng="pl">Jordania</trd>
  <trd lng="pt">Jord&acirc;nia</trd>
  <trd lng="ru">&c_I;&c_o;&c_r;&c_d;&c_a;&c_n;&c_i;&c_ja;</trd>
  <trd lng="sk">Jord&aacute;nsko</trd>
  <trd lng="uk">&c_J;&#x43E;&c_r;&c_d;&c_a;&#x43D;&c_ib;&#x44F;</trd>
  <trd lng="zh">&#x7D04;&#x65E6; <pr>yu&#x113;d&#xE0;n</pr></trd>
</drv>

<drv mrk="jordan.j0a">
  <kap><tld lit="j"/>a</kap>
  <snc mrk="jordan.j0a.GEOG">
    <dif>
      Rilata al <tld lit="j"/>anoj, ilia nacieco
      a&ubreve; &scirc;tato.
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_r;&c_d;&c_a;&c_n;&c_s;&c_k;&c_ib;</trd>
  <trd lng="ca">jord&agrave;</trd>
  <trd lng="bg">&c_j;&c_o;&c_r;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;</trd>
  <trd lng="de">jordanisch</trd>
  <trd lng="en">jordanian</trd>
  <trd lng="es">jordano</trd>
  <trd lng="fr">jordanien<klr> (adj.)</klr></trd>
  <trd lng="he">&yod;&resh;&dalet;&nun;&yod;</trd>
  <trd lng="hu">jord&aacute;niai</trd>
  <trd lng="it">giordano<klr> (agg.)</klr></trd>
  <trdgrp lng="nl">
    <trd>Jordaans</trd>,
    <trd>Jordanisch</trd>
  </trdgrp>
  <trd lng="pl">jorda&nacute;ski</trd>
  <trd lng="ru">&c_i;&c_o;&c_r;&c_d;&c_a;&c_n;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="zh">&#x7D04;&#x65E6;&#x4EBA; <pr>yu&#x113;d&#xE0;nr&#xE9;n</pr></trd>
</drv>

<drv mrk="jordan.j0ano">
  <kap><tld lit="j"/>ano</kap>
  <snc mrk="jordan.j0ano.GEOG">
    <dif>
      <ref tip="super" cel="sxtat.0ano">&Scirc;tatano</ref> de
      <ref tip="malprt" cel="jordan.J0o"><tld/>o</ref>:
      <ekz>
        en la vaste kondamnita masakro de <nom>Qibya</nom>, <klr>[&#x2026;]</klr> pli ol 60 <tld lit="j"/>anoj estis mortigitaj en atako
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=31406">Ariel &Scirc;aron</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ja;&c_r;&c_d;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="bg">&c_j;&c_o;&c_r;&c_d;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="ca">jord&agrave;</trd>
  <trd lng="de">Jordanier</trd>
  <trd lng="en">Jordanian</trd>
  <trd lng="es">jordano</trd>
  <trd lng="fr">Jordanien<klr> (subst.)</klr></trd>
  <trd lng="he">&yod;&resh;&dalet;&nun;&yod;</trd>
  <trd lng="hu">jord&aacute;niai <klr>(lakos)</klr></trd>
  <trd lng="it">giordano</trd>
  <trd lng="nl">Jordani&euml;r</trd>
  <trd lng="pl">Jorda&nacute;czyk</trd>
  <trd lng="ru">&c_i;&c_o;&c_r;&c_d;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="zh">&#x7D04;&#x65E6;&#x4EBA; <pr>yu&#x113;d&#xE0;nr&#xE9;n</pr></trd>
</drv>

<drv mrk="jordan.Okcidenta_0o">
  <kap>
    Okcidenta <tld/>o
  </kap>
  <snc>
    <dif>
      Regiono en Israelo limanta kun <tld/>o norde de Malviva Maro:
      <ekz>
        Israelo ricevas grandan parton de sia akvokvanto el la montoj de okcidenta
        <tld/>o kaj pro tio neniakaze volas redoni tiujn regionojn al Sirio
        <fnt>
       <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2000/006866.php">La blua &leftquot;oro&#x201D; &ccirc;iam malsufi&ccirc;os</url></vrk>
          <bib>Monato</bib>
          <lok>2000/03, p. 20</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_Z;&c_a;&c_h;&c_o;&c_d;&c_n;&c_ja;&c_ja; <ind>&c_Ja;&c_r;&c_d;&c_a;&c_n;&c_ib;&c_ja;</ind></trd>
</drv>

</art>
