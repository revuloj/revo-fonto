<vortaro>
<art mrk="$Id: grizajl.xml,v 1.4 2020/12/01 21:07:17 revo Exp $">
<kap>
  <rad>grizajl</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="grizajl.0o">
  <kap><tld/>o</kap>
  <snc mrk="grizajl.0o.BELA">
    <uzo tip="fak">BELA</uzo>
    <dif>
      <ref tip="super" cel="art.pentr0o">Pentra</ref> kaj ornama procedo uzanta nur 
      nuancojn de <ref tip="prt" cel="griz.0o">grizo</ref>, blanko, nigro
      kontrastigante &gcirc;iajn malsamajn helecojn:
      <ekz>
        temis <klr>[&#8230;]</klr> pri <tld/>a akvarelo
        <fnt>
          <aut>S. Larbar</aut>,
          <vrk>Karuseloj</vrk>,
          <lok>1987</lok>
        </fnt>;
      </ekz>
      <ekz>
        sanktuloj, pentritaj en <tld/>o, efikas kiel skulpta&jcirc;oj en muroni&ccirc;oj
        <fnt>
          <vrk><url ref="https://eo.wikipedia.org/wiki/Grizajlo"
              >Grizajlo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">grisaille</trd>
  <trd lng="de">Grisaille</trd>
  <trd lng="en">grisaille</trd>
  <trd lng="fr">grisaille<klr> (beaux-arts)</klr></trd>
  <trd lng="sk">grisaille</trd>
</drv>
</art>

