<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>paliativ</rad>/o<fnt>Z</fnt>
</kap>
<drv mrk="paliat.0o">
  <kap><tld/>o</kap>
  <snc mrk="paliat.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Kuracilo, kiu provizore mildigas malsanon, ne resanigante la
      malsanulon.
    </dif>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_ja;&c_t;&c_y;&c_w;</trd>
    <trd lng="de">Palliativum</trd>
    <trdgrp lng="hu">
      <trd>t&uuml;netkezel&odblac; gy&oacute;gyszer</trd>,
      <trd>palliat&iacute;vum</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>pijnstillend middel</trd>,
      <trd>palliatief</trd>
    </trdgrp>
  </snc>
  <snc mrk="paliat.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Duonefika kaj provizora rebonigilo:
      <ekz>
        neniaj kompromisaj <tld/>oj donos pacon al la homaro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_w;&c_m;&c_je;&c_r;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Notbehelf</trd>,
      <trd>Flickwerk</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>exp&eacute;dient<klr> (subst.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>t&uuml;netkezel&odblac;</trd>,
      <trd>sz&uuml;ks&eacute;gmegold&aacute;s</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>palliatif<klr> (subst.)</klr></trd>
    </trdgrp>
    <trd lng="nl">halve <ind>maatregel</ind></trd>
    <trd lng="pt">paliativo</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_m;&c_je;&c_r;&c_a;</trd>
  </snc>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_l;&c_i;&c_a;&c_t;&c_i;&c_v;</trd>
</drv>

<drv mrk="paliat.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Helpa sed ne solva:
      <ekz>
        &ccirc;io &ccirc;i estas <em><tld/>aj</em> rimedoj
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 21a, p. 128a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_ja;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  <trdgrp lng="fr">
    <trd>palliatif<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="hu">t&uuml;netkezel&odblac;</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_l;&c_l;&c_i;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_d;&c_a;&c_ju;&c_shch;&c_i;&c_j; <ind>&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_o;&c_je;</ind> &c_o;&c_b;&c_l;&c_je;&c_g;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
</drv>
</art>
