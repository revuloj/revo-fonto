<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap><rad>paliativ</rad>/o<fnt>Z</fnt></kap>
<drv mrk="paliat.0o">
  <kap><tld/>o</kap>
  <snc>
    <uzo tip="fak">MED</uzo>
    <dif>
      Kuracilo, kiu provizore malakrigas malsanon, ne resanigante la
      malsanulon.
    </dif>
    <trd lng="de">Palliativum</trd>
    <trdgrp lng="hu">
      <trd>t&uuml;netkezel&odblac; gy&oacute;gyszer</trd>,
      <trd>palliat&iacute;vum</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>pijnstillend middel</trd>,
      <trd>palliatief</trd>
    </trdgrp>
  </snc>
  <snc>
    <uzo tip="stl">FIG</uzo>
    <dif>
      Duonefika kaj provizora rebonigilo:
      <ekz>
        neniaj kompromisaj <tld/>oj
        donos pacon al la homaro<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Notbehelf</trd>,
      <trd>Flickwerk</trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>t&uuml;netkezel&odblac;</trd>,
      <trd>sz&uuml;ks&eacute;gmegold&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">halve <ind>maatregel</ind></trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_m;&c_je;&c_r;&c_a;</trd>
  </snc>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_l;&c_i;&c_a;&c_t;&c_i;&c_v;</trd>
</drv>
</art>
