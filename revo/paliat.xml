<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: paliat.xml,v 1.32 2024/02/16 19:24:10 revo Exp $">
<kap>
  <rad>paliativ</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="paliat.0o">
  <kap><tld/>o</kap>
  <snc mrk="paliat.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      <ref tip="super" cel="kurac.0ilo">Kuracilo</ref>, kiu
      provizore <ref cel="mild.0igi">mildig</ref>as malsanon, ne resanigante la
      malsanulon.
    </dif>
    <refgrp tip="vid">
      <ref cel="suroga.0o.KOMUNE">surogato</ref>
    </refgrp>
    <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_ja;&c_t;&c_y;&c_w;</trd>
    <trd lng="de">Palliativum</trd>
    <trdgrp lng="hu">
      <trd>t&uuml;netkezel&odblac; gy&oacute;gyszer</trd>,
      <trd>palliat&iacute;vum</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>pijnstillend middel</trd>,
      <trd>palliatief<klr tip="ind"> (zn.)</klr></trd>
    </trdgrp>
    <trdgrp lng="pl">
      <trd>paliatyw</trd>,
      <trd>p&oacute;&lstroke;&sacute;rodek</trd>
    </trdgrp>
    <trd lng="sv">palliativ</trd>
    <trdgrp lng="zh">
    <trd>&#x5B89;&#x5B81;&#x7F13;&#x548C;&#x533B;&#x7597; <pr>&#x101;nn&#xED;nghu&#x1CE;nh&#xE9;y&#x12B;li&#xE1;o</pr></trd>,
    <trd>&#x5B89;&#x5BE7;&#x7DE9;&#x548C;&#x91AB;&#x7642; <pr>&#x101;nn&#xED;nghu&#x1CE;nh&#xE9;y&#x12B;li&#xE1;o</pr></trd>
  </trdgrp>
</snc>
  <snc mrk="paliat.0o.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Duonefika kaj provizora rebonigilo:
      <ekz>
        neniaj kompromisaj <tld/>oj, neniaj sagacaj politikaj kontraktoj donos pacon al
        la homaro
        <fnt>
          <bib>GLI</bib>
        </fnt>;
      </ekz>
      <ekz>
        impostaj <tld/>oj
        <fnt>
          <vrk><url ref="https://marx.libcom.org/files/Sennaciulo_1925_n47_aug20.pdf">Valuta agonio
          de Francio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        multlingvismo ne devas ka&scirc;i la &scirc;telpa&scirc;an trudadon de la tutangla <tld/>o
        <fnt>
          <vrk><url ref="https://lernu.net/ca/forumo/temo/25329/2">Prognozo pri la E&ubreve;ropa parlamenta
          elekto je la 26a Majo ...</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_p;&c_a;&c_w;&c_m;&c_je;&c_r;&c_a;</trd>
    <trdgrp lng="de">
      <trd>Notbehelf</trd>,
      <trd>Flickwerk</trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>exp&eacute;dient<klr> (subst.)</klr></trd>,
      <trd>palliatif<klr> (subst.)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>t&uuml;netkezel&odblac;</trd>,
      <trd>sz&uuml;ks&eacute;gmegold&aacute;s</trd>
    </trdgrp>
    <trd lng="nl">halve <ind>maatregel</ind></trd>
    <trdgrp lng="pl">
      <trd>p&oacute;&lstroke;&sacute;rodek</trd>,
      <trd>&sacute;rodek ma&lstroke;o skuteczny</trd>
    </trdgrp>
    <trd lng="pt">paliativo</trd>
    <trd lng="ru">&c_p;&c_o;&c_l;&c_u;&c_m;&c_je;&c_r;&c_a;</trd>
  </snc>
  <trdgrp lng="ca">
      <trd>paliatiu</trd>,
      <trd>mitigant <klr>(subst.)</klr></trd>
    </trdgrp>
  <trdgrp lng="cs">
    <trd>paliativ</trd>,
    <trd>paliativum</trd>
  </trdgrp>
  <trd lng="en">palliative</trd>
  <trdgrp lng="ja">
    <trd>&#x4E00;&#x6642;&#x7684;&#x7DE9;&#x548C;&#x5264;  <pr>&#x3044;&#x3061;&#x3058;&#x3066;&#x304D;&#x304B;&#x3093;&#x308F;&#x3056;&#x3044;</pr></trd>,
    <trd>&#x4E00;&#x6642;&#x3057;&#x306E;&#x304E;  <pr>&#x3044;&#x3061;&#x3058;&#x3057;&#x306E;&#x304E;</pr></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_a;&c_l;&c_l;&c_i;&c_a;&c_t;&c_i;&c_v;</trd>
  <trd lng="sk">zmier&ncaron;ovac&iacute; prostriedok</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&#x43B;&c_ib;&c_a;&c_t;&c_i;&c_v;</trd>,
    <trd>&#x43F;&c_a;&#x43B;&c_ib;&c_a;&c_t;&c_i;&c_v;</trd>,
    <trd>&#x43C;&c_a;&#x43B;&#x43E;&c_d;&c_ib;&c_j;&#x43E;&c_v;&c_i;&c_j; &c_z;&c_a;&c_h;&c_ib;&c_d;</trd>,
    <trd>&#x43F;&c_ib;&c_v;&c_z;&c_a;&c_h;&c_ib;&c_d;</trd>
  </trdgrp>
</drv>

<drv mrk="paliat.0a">
  <kap><tld/>a</kap>
  <snc mrk="paliat.0a.KOMUNE">
    <dif>
      <ref tip="super" cel="help.0a.saniga">Helpa</ref> sed ne solva:
      <ekz>
        mi konsilis al li man&gcirc;i karotojn, preni sodon,
        sed &ccirc;io &ccirc;i estas <tld/>aj rimedoj; necesa estas io pli energia
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 21a, p.
        128a</lok></fnt>;
      </ekz>
      <ekz>
        celata estu bona <tld/>a flegado por mildigi la doloron de la &ccirc;evivfinaj
        pacientoj, ne ilin mortigi
        <fnt>
          <bib>Monato</bib>
          <aut>D. E. Rogers</aut>
          <vrk><url ref="&Monato;/2003/008188.html">Jes kaj ne</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        anstata&ubreve; iuj klaraj kaj certaj kuraciloj por forigi la malbona&jcirc;ojn de la
        nuntempa socio &mdash; li povis fari nur kelkajn sugestojn <tld/>ajn
        <fnt>
          <bib>Zam</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">palliativ</trd>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_ib;&c_ja;&c_t;&c_y;&c_w;&c_n;&c_y;</trd>
  <trdgrp lng="ca">
    <trd>paliatiu</trd>,
    <trd>atenuant <klr>(adj.)</klr></trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>bolest uti&scaron;uj&iacute;c&iacute;</trd>,
    <trd>paliativn&iacute;</trd>
  </trdgrp>
  <trd lng="en">palliative</trd>
  <trd lng="fr">palliatif<klr> (adj.)</klr></trd>
  <trd lng="hu">t&uuml;netkezel&odblac;</trd>
  <trd lng="nl">palliatief<klr tip="ind"> (bn.)</klr></trd>
  <trdgrp lng="pl">
    <trd>paliatywny</trd>,
    <trd>objawowy</trd>,
    <trd>&lstroke;agodz&aogonek;cy</trd>,
    <trd>u&sacute;mierzaj&aogonek;cy <klr>np. b&oacute;l</klr></trd>,
    <trd>ma&lstroke;o skuteczny</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_a;&c_l;&c_l;&c_i;&c_a;&c_t;&c_i;&c_v;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_d;&c_a;&c_ju;&c_shch;&c_i;&c_j; <ind>&c_v;&c_r;&c_je;&c_m;&c_je;&c_n;&c_n;&c_o;&c_je;</ind>
    &c_o;&c_b;&c_l;&c_je;&c_g;&c_ch;&c_je;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sk">uti&scaron;uj&uacute;ci boles&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&#x43F;&c_a;&#x43B;&c_ib;&c_a;&c_t;&c_i;&c_v;&#x43D;&c_i;&c_j;</trd>,
    <trd>&#x44F;&#x43A;&c_i;&c_j; &c_d;&c_a;&c_jeu; &c_t;&c_i;&#x43C;&c_ch;&c_a;&c_s;&#x43E;&c_v;&c_je;
    &#x43F;&#x43E;&#x43B;&c_je;&c_g;&c_sh;&c_je;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>

<drv mrk="paliat.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="paliat.0i.KOMUNE">
    <dif>
      Parte a&ubreve; malperfekte <ref tip="super" cel="kurac.0i">kuraci</ref>,
      <ref cel="kompen1.0i">kompensi</ref>,
      <ref cel="bon.re0igi">rebonigi</ref>:
      <ekz>
        dua letero de Jano ne multe <tld/>is &scirc;ian senton de
        senefikeco, &ccirc;ar &gcirc;i estis same lakona kiel la unua
        <fnt>
          <aut>J. Francis</aut>,
          <vrk>La Granda Kaldrono</vrk>,
          <lok>1978</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="rehabilit.0i.MED">rehabiliti<sncref/></ref>
    </refgrp>
  </snc>
    <trdgrp lng="be">
      <trd>&c_s;&c_u;&c_p;&c_a;&c_k;&c_o;&c_j;&c_v;&c_a;&c_c;&c_mol;</trd>,
      <trd>&c_s;&c_u;&c_c;&c_ib;&c_sh;&c_a;&c_c;&c_mol;</trd>
    </trdgrp>
    <trd lng="de">notd&uuml;rftig <ind>versorgen</ind></trd>
    <trd lng="fr">pallier</trd>
    <trdgrp lng="pl">
      <trd>&lstroke;agodzi&cacute;</trd>,
      <trd>u&sacute;mierza&cacute;</trd>
    </trdgrp>
</drv>
</art>
