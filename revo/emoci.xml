<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>3</ofc>
<rad>emoci</rad>/i
</kap>
<drv mrk="emoci.0i">
<kap><tld/>i</kap>
<gra><vspec>tr</vspec></gra>
<snc>
<dif>
Forte tu&scirc;i, movi la animon:
<ekz><tld/>i iun &gcirc;is larmoj;</ekz>
<ekz>&ccirc;iuj estis <tld/>itaj<fnt>Z</fnt></ekz>
</dif>
<refgrp tip="sin">
<ref cel="tusx.kor0i">kortu&scirc;i</ref>;
</refgrp>
<refgrp tip="vid">
<ref cel="sku.0i">skui</ref>.
</refgrp>
</snc>
  <trdgrp lng="de">
     <trd>ergreifen</trd>,
     <trd>r&uuml;hren</trd>,
     <trd>bewegen</trd>,
     <trd>ersch&uuml;ttern</trd>
  </trdgrp>
<trdgrp lng="fr">
  <trd>&eacute;mouvoir</trd>,
  <trd>remuer<klr tip="ind"> (&eacute;mouvoir)</klr></trd></trdgrp> 
<trd lng="ru">&c_v;&c_o;&c_l;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
<drv mrk="emoci.0o">
<kap><tld/>o</kap>
<snc num="1">
<dif>
Kortu&scirc;o:
<ekz>tiuj paroloj estigis grandan <tld/>on en la &ccirc;eestantaro;</ekz>
<ekz>dolora <tld/>o montri&gcirc;is sur &scirc;ia viza&gcirc;o.</ekz>
</dif>
  <trdgrp lng="de">
     <trd>Bewegtheit</trd>,
     <trd>Gem&uuml;tsbewegung</trd>,
     <trd>R&uuml;hrung</trd>,
     <trd>seelische Erregung</trd>
  </trdgrp>
</snc>
<snc num="2">
<uzo tip="fak">FIL</uzo>
<dif>
&Ccirc;iu ajn afekcia stato. 
</dif>
  <trdgrp lng="de">
     <trd>Emotion</trd>,
     <trd>Gem&uuml;tszustand</trd>
  </trdgrp>
</snc>
<trd lng="fr">&eacute;motion</trd>
<trdgrp lng="ru">
  <trd>&c_e;&c_m;&c_o;&c_c;&c_i;&c_ja;</trd>,
  <trd>&c_v;&c_o;&c_l;&c_n;&c_je;&c_n;&c_i;&c_je;</trd>,
  <trd>&c_p;&c_je;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
</trdgrp>
</drv>
<drv mrk="emoci.0a">
<kap><tld/>a</kap>
<snc>
<dif>
Kortu&scirc;anta: <ekz><tld/>a rakonto, situacio, gesto.</ekz>
</dif>
</snc>
  <trdgrp lng="de">
     <trd>bewegt</trd>,
     <trd>emotional</trd>,
     <trd>emotionell</trd>,
     <trd>ergreifend</trd>,
     <trd>ger&uuml;hrt</trd>,
     <trd>seelisch erregt</trd>
  </trdgrp>
<trd lng="ru">&c_e;&c_m;&c_o;&c_c;&c_i;&c_o;&c_n;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
</drv>
<drv mrk="emoci.0igxi">
<kap><tld/>i&gcirc;i</kap>
<snc>
<dif>
Esti movita, kortu&scirc;ita de io: <ekz>li <tld/>i&gcirc;is anta&ubreve; tia mizero.</ekz>
</dif>
<refgrp tip="vid">
<ref cel="agit.0i">agiti</ref>,
<ref cel="ekscit.0i">eksciti</ref>,
<ref cel="afekci.0o">afekcio</ref>.
</refgrp>
</snc>
  <trdgrp lng="de">
     <trd>bewegt sein</trd>,
     <trd>ergriffen werden</trd>,
     <trd>ger&uuml;hrt sein</trd>,
     <trd>ger&uuml;hrt werden</trd>
  </trdgrp>
<trd lng="fr">s'<ind>&eacute;mouvoir</ind></trd>
<trdgrp lng="ru">
  <trd>&c_v;&c_o;&c_l;&c_n;&c_o;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
  <trd>&c_p;&c_je;&c_r;&c_je;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;</trd>
</trdgrp>
</drv>
</art>
