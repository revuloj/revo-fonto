<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>1</ofc>
  <rad>prism</rad>/o
</kap>
<drv mrk="prism.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt>
  <snc mrk="prism.0o.figuro">
    <dif>
      <ref tip="super" cel="edr.plur0o">Pluredro</ref>, el kiu du facoj
      estas paralelaj kaj samdimensiaj
      <ref cel="later.plur0o">plurlateroj</ref> kaj &ccirc;iuj aliaj
      facoj estas <ref cel="parale2.0o">paralelogramoj</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_z;&c_m;&c_a;</trd>
  <trd lng="de">Prisma</trd>
  <trd lng="en">prism</trd>
  <trd lng="fr">prisme</trd>
  <trdgrp lng="hu">
    <trd>has&aacute;b</trd>,
    <trd>prizma</trd>
  </trdgrp>
  <trd lng="nl">prisma</trd>
  <trd lng="pl">graniastos&lstroke;up</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_z;&c_m;&c_a;</trd>
  <trd lng="sv">prisma</trd>
</drv>
</art>
