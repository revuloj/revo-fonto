<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prism.xml,v 1.28 2023/10/20 16:38:17 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>prism</rad>/o
</kap>
<drv mrk="prism.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt>
  <snc mrk="prism.0o.figuro">
    <dif>
      <ref tip="super" cel="edr.plur0o">Pluredro</ref>, el kiu du facoj
      estas egalaj <ref cel="later.plur0o">plurlateroj</ref>
      kun duope paralelaj lateroj, sur kiuj estas konstruitaj
      <ref cel="parale2.0o">paralelogramoj</ref>, konsistigantaj la
      ceterajn facojn:
      <ekz>
        la volumeno de &ccirc;iu <tld/>o estas produto de areo de la bazo kaj distanco inter
        la du bazaj edroj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=205321">Triangula prismo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        oni ricevas la impreson, ke Duli&ccirc;enko vidas tiujn lingvajn procezojn tra
        rozkolora <tld/>o (ekzemple, li nur preterpase mencias le&gcirc;ojn pri
        lingvoprotektado)
        <fnt>
          <bib>Monato</bib>
          <aut>Reinaldo Marcelo Ferreira</aut>
          <vrk><url ref="&Monato;/2007/009078.php">Interlingvistiko por &ccirc;iuj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        du studoj pri <nom>Braun</nom>, de <nom>Kalocsay</nom> kaj <nom>Auld</nom>
        respektive, aperis en Norda <tld lit="P"/>o en 1959
        <fnt>
          <bib>Monato</bib>
          <aut>Carlo Minnaja</aut>
          <vrk><url ref="&Monato;/2003/008065.html">Finaj agordoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_r;&c_y;&c_z;&c_m;&c_a;</trd>
  <trd lng="ca">prisma</trd>
  <trdgrp lng="cs">
    <trd>hranol</trd>,
    <trd>prisma</trd>,
    <trd>prizma</trd>
  </trdgrp>
  <trd lng="de">Prisma</trd>
  <trd lng="en">prism</trd>
  <trd lng="es">prisma</trd>
  <trd lng="fa">&a_m;&a_n;&a_sh;&a_w;&a_r;<klr tip="ind"> (&a_h;&a_n;&a_d;&a_s;&a_h;)</klr></trd>
  <trd lng="fr">prisme</trd>
  <trdgrp lng="hu">
    <trd>has&aacute;b</trd>,
    <trd>prizma</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x89D2;&#x67F1;  <pr>&#x3059;&#x307F;&#x3070;&#x3057;&#x3089;</pr></trd>,
    <trd>&#x30D7;&#x30EA;&#x30BA;&#x30E0;</trd>
  </trdgrp>
  <trd lng="nl">prisma</trd>
  <trd lng="pl">graniastos&lstroke;up</trd>
  <trd lng="pt">prisma</trd>
  <trd lng="ru">&c_p;&c_r;&c_i;&c_z;&c_m;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>hranol</trd>,
    <trd>prizma</trd>
  </trdgrp>
  <trd lng="sv">prisma</trd>
  <trd lng="uk">&#x43F;&#x440;&#x438;&#x437;&#x43C;&#x430;</trd>
</drv>
</art>
