<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>demonstr</rad>/i
</kap>
<drv mrk="demons.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Plene kaj nepre pruvi per dedukto:
      <ekz>
        la e&ubreve;klida postulato ne estas <tld/>ebla.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;montrer</trd>,
    <trd>&eacute;tablir<klr tip="ind"> (d&eacute;montrer)</klr></trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_m;&c_o;&c_n;&c_s;&c_t;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_a;&c_z;&c_y;&c_v;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="demons.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nepra pruvo per dedukto.
    </dif>
    <ref tip="sin" cel="demons1.0o.dedukto">demonstracio 1.</ref>
  </snc>
  <trdgrp lng="fr">
    <trd>d&eacute;monstration<klr tip="ind"> (raisonnement)</klr></trd>,
    <trd>raisonnement</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_d;&c_je;&c_m;&c_o;&c_n;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_p;&c_o;&c_k;&c_a;&c_z;</trd>
  </trdgrp>
</drv>
</art>
