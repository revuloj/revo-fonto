<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>demonstr</rad>/i
    </kap>

    <drv mrk="demons.0i">
      <kap><tld/>i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc mrk="demons.0i.prezenti">
        <dif>
          Detale kaj klare
          <refgrp>
            <ref cel="montr.el0i">elmontri</ref>,
            <ref cel="prezen1.0i.proponi">prezenti</ref>
          </refgrp>
          ion (aparaton, programon, kinofilmon):
          <ekz>
            <tld/>i la funkciadon de motoro, de lavma&scirc;ino
            <fnt><bib>PIV2</bib></fnt>;
          </ekz><ekz>
            ni devis prepari nin por ekspozicio de niaj
            labora&jcirc;oj, en kiu ni ilin <tld/>os kaj vendos
            <fnt>
              <aut>Berta Lenard</aut>: <vrk>Ne eblas</vrk>.
              <bib>Monato</bib>
            </fnt>;
          </ekz><ekz>
            li anka&ubreve; mirinde <tld/>is tutan ludon unuope
            <fnt><aut>V. Hordijenko</aut>:
              <vrk>Ludoj cxe Skota Studrondo 2003</vrk>.
              <bib>LOdE</bib>, <lok>2003:12 (110)</lok>
            </fnt>.
          </ekz>
        </dif>
        <trd
lng="ru">&c_d;&c_je;&c_m;&c_o;&c_n;&c_s;&c_t;&c_r;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
      </snc>

      <snc mrk="demons.0i.MAT">
        <uzo tip="fak">MAT</uzo>
        <fnt><bib>PV</bib></fnt>
        <dif>
          Matematike <ref tip="dif" cel="pruv.0i.MAT">pruvi</ref>:
          <ekz>
            <tld/>u, ke la produto de du diagonalaj matricoj ne dependas
            de la ordo de ties faktoroj;
          </ekz><ekz>
            la e&ubreve;klida postulato ne estas <ind><tld/>ebla</ind>.
            <trd lng="de">beweisbar</trd>
            <trd lng="en">provable</trd>
            <trdgrp lng="fr">
              <trd>d&eacute;montrable</trd>,
              <trd>prouvable</trd>
            </trdgrp>
            <trd lng="hu">bizony&iacute;that&oacute;</trd>
            <trd lng="pl">mo&zdot;liwy do udowodnienia</trd>
            <trd lng="ru">&c_d;&c_o;&c_k;&c_a;&c_z;&c_u;&c_je;&c_m;&c_y;&c_j;</trd>
          </ekz>
          <trd lng="ru">&c_d;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
        </dif>
        <rim>
          La senco <tld/>i<sncref ref="demons.0i.MAT"/> &scirc;ajnas jam
          eksmoda kaj malnecesa (precipe &ccirc;ar &gcirc;i malbone akordas kun la
          pli viva senco <tld/>i<sncref ref="demons.0i.prezenti"/>);
          anka&ubreve; en matematiko tute sufi&ccirc;as
          <ref tip="sin" cel="pruv.0i.MAT">pruvi<sncref/></ref>.
          <aut>Sergio Pokrovskij</aut>
        </rim>
        <trd lng="de">beweisen</trd>
        <trd lng="en">prove</trd>
        <trd lng="pl">dowodzi&cacute;</trd>
        <trdgrp lng="ru">
          <trd>&c_d;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>,
          <trd>&c_p;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_mol;</trd>
        </trdgrp>
      </snc>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_d;&c_e;&c_m;&c_a;&c_n;&c_s;&c_t;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_d;&c_a;&c_k;&c_a;&c_z;&c_v;&c_a;&c_c;&c_mol;</trd>,
        <trd>&c_d;&c_a;&c_v;&c_o;&c_d;&c_z;&c_ib;&c_c;&c_mol;</trd>
      </trdgrp>
      <trd lng="es">demostrar</trd> 
      <trdgrp lng="fr">
        <trd>d&eacute;montrer</trd>,
        <trd>&eacute;tablir<klr tip="ind"> (d&eacute;montrer)</klr></trd>
      </trdgrp>
      <trdgrp lng="hu">
        <trd>igazol</trd>,
        <trd>bizony&iacute;t</trd>
        <trd>bebizony&iacute;t</trd>
      </trdgrp>
    </drv>

    <drv mrk="demons.0o">
      <kap><tld/>o</kap>
      <snc mrk="demons.0o.prezentado">
        <dif>
          Elmeto kaj klarigo per specimenoj a&ubreve; provoj, la ago
          <tld/>i<sncref ref="demons.0i.prezenti"/>:
          <ekz>
            <tld/>ado estas esenca parto de la vendarto
            <fnt><bib>PIV2</bib></fnt>.
          </ekz>
        </dif>
        <trdgrp lng="ru">
          <trd>&c_d;&c_je;&c_m;&c_o;&c_n;&c_s;&c_t;&c_r;&c_a;&c_c;&c_i;&c_ja;</trd>,
          <trd>&c_p;&c_o;&c_k;&c_a;&c_z;</trd>
        </trdgrp>
      </snc>
      <snc mrk="demons.0o.MAT">
        <uzo tip="fak">MAT</uzo>
        <fnt><bib>PIV2</bib></fnt>
        <dif>
          Matematika <ref tip="dif" cel="pruv.0o.MAT">pruvo</ref>:
          <ekz>
            eraro englitis en vian <tld/>on.
          </ekz>
        </dif>
        <trd lng="de">Beweis</trd>
        <trd lng="en">proof</trd>
        <trdgrp lng="pl">
          <trd>dow&oacute;d</trd>,
          <trd>dowodzenie</trd>
        </trdgrp> 
        <trd
lng="ru">&c_d;&c_o;&c_k;&c_a;&c_z;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_s;&c_t;&c_v;&c_o;</trd>
      </snc>
      <ref tip="sin" cel="demons1.0o.dedukto">demonstracio.</ref>
      <trdgrp lng="be">
        <trd>&c_p;&c_a;&c_k;&c_a;&c_z;</trd>,
        <trd>&c_d;&c_e;&c_m;&c_a;&c_n;&c_s;&c_t;&c_r;&c_a;&c_c;&c_y;&c_ja;</trd>,
        <trd>&c_d;&c_o;&c_k;&c_a;&c_z;</trd>,
        <trd>&c_d;&c_o;&c_v;&c_a;&c_d;</trd>
      </trdgrp>
      <trd lng="es">demostraci&oacute;n</trd>
      <trdgrp lng="fr">
        <trd>d&eacute;monstration<klr tip="ind"> (preuve)</klr></trd>,
        <trd>preuve<klr tip="ind"> (d&eacute;monstration)</klr></trd>
      </trdgrp>
      <trd lng="hu">bizony&iacute;t&aacute;s</trd>
    </drv>
  </art>
