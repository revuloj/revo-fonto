<drv mrk="batu1.0o">
  <kap><tld/>o</kap> 
  <snc mrk="batu1.0o.pelcxaso">
    <dif>
      <ref tip="dif" cel="cxas.pel0o">Pel&ccirc;aso</ref>:
      <ekz>
        okazis la <tld/>o, kaj la reto pleni&gcirc;is per diversaj
        birdetoj
        <fnt><bib>DonKihxoto</bib>, <lok>parto 2a, &ccirc;apitro 58a, p.
        723a</lok></fnt>;
      </ekz>
      <ekz>
        tio ne estis <tld/>o sed terura batalo: la monto restis kovrita per kadavroj
        <fnt>
          <vrk><url ref="https://groups.google.com/d/topic/soc.culture.esperanto/c2jmEc4Ngyo">La Monto de la Animoj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x653F;&#x6CBB;&#x8FEB;&#x5BB3; <pr>zh&#xE8;ngzh&#xEC;p&#xF2;h&#xE0;i</pr></trd>
</snc>
  <rim>
    <ref tip="hom" cel="batu.0o">Batuo</ref> estas anka&ubreve; &Ccirc;ingisido.
  </rim>
  <trdgrp lng="ca">
    <trd>batuda</trd>,
    <trd>cacera</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hon</trd>,
    <trd>hromadn&yacute; lov</trd>,
    <trd>le&ccaron;</trd>
  </trdgrp>
  <trd lng="de">Treibjagd</trd>
  <trd lng="en">battue</trd>
  <trd lng="es">batida<klr> (caza)</klr></trd>
  <trd lng="fr">battue<klr> (chasse)</klr></trd>
  <trd lng="he">&#x5E6;&#x5D9;&#x5D3; &#x5D4;&#x5DE;&#x5D5;&#x5E0;&#x5D9;</trd>
  <trdgrp lng="pl">
    <trd>nagonka</trd>,
    <trd>polowanie z nagonk&aogonek;</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_b;&c_l;&c_a;&c_v;&c_a;<klr>
  (&c_o;&c_h;&c_o;&c_t;&c_a;)</klr></trd>
  <trdgrp lng="sk">
    <trd>hon</trd>,
    <trd>hromadn&yacute; lov</trd>
  </trdgrp>
  <trd lng="uk">&#x431;&#x43B;&#x430;&#x432;&#x430;&#xA0;(&#x43D;&#x430; &#x43F;&#x43E;&#x43B;&#x44E;&#x432;&#x430;&#x43D;&#x43D;&#x456;)</trd>
</drv>
</art>

