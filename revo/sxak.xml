<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: sxak.xml,v 1.41 2022/05/05 14:43:49 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>&scirc;ak</rad>/o
</kap>
<drv mrk="sxak.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc mrk="sxak.0o.ludo">
    <dif>
      <ref tip="dif" cel="sxak.0ludo"><tld lit="&Scirc;"/>ludo</ref>:
      <ekz>
        la nomoj de <tld/>o: <klr>[&#x2026;]</klr> <ref tip="prt" cel="regx.0o.sxaka">re&gcirc;o</ref>,
        soldato <klr>(<ref tip="prt" cel="peon.0o">peono</ref>)</klr>,
        <ref tip="prt" cel="dam.0o.sxaka">damo</ref> <klr>[&#x2026;]</klr>, 
        <ref tip="prt" cel="kurier.0o.sxaka">kuriero</ref>,
        <klr>[&#x2026;]</klr> <ref tip="prt" cel="cxeval.0o.sxaka">&ccirc;evalo</ref>,
        <ref tip="prt" cel="tur.0o.sxaka">turo</ref>
        <fnt>
          <bib>LR</bib>
          <vrk>Letero al Th. Thorsteinsson, 1910, O.V. p. 522</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[iuj]</klr> ludis <tld/>ojn kaj damojn, aliaj en societo de dancistinoj
        trinkis
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro VIII</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi enuas kaj la <tld/>o min nun ne kontentigis
        <fnt>
          <bib>Rabistoj</bib>
          <lok>Akto Kvina</lok>
        </fnt>;
      </ekz>
      <ekz>
        la usona futbalo iom similas <tld/>on, ludatan sur vasta kampo
        <fnt>
          <bib>Monato</bib>
          <aut>Garbhan Macaoidh</aut>
          <vrk><url ref="&Monato;/2004/008319.html">Multviza&gcirc;a futbalo</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        unika <tld/>a kompleto (kun &ccirc;iuj <tld/>figuroj!) el eburo
        <fnt>
          <bib>Monato</bib>
          <aut>Anatoli Ionesov</aut>
          <vrk><url ref="&Monato;/2006/008920.php">Perlo &ccirc;e la Silka Vojo</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        por liaj filinoj komence ne estis tute facile konkursi kiel <tld/>istoj kaj ne
        nur kiel <tld/>istinoj
        <fnt>
          <bib>Monato</bib>
          <aut>Edmund Grimley Evans</aut>
          <vrk><url ref="&Monato;/2007/009012.php">Pri edukado de geniuloj</url></vrk>
          <lok>2007</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_a;&c_h;&c_m;&c_a;&c_t;&c_y;</trd>
    <trd lng="ca">escacs</trd>
    <trd lng="de">Schachspiel</trd>
    <trd lng="en">chess</trd>
    <trd lng="fa">&a_sh;&a_T;&a_r;&a_n;&a_j;</trd>
    <trd lng="hu">sakkj&aacute;t&eacute;k</trd>
    <trd lng="id">catur</trd>
    <trd lng="it">scacchi<klr> (gioco)</klr></trd>
    <trd lng="pl">szachy</trd>
    <trd lng="pt">xadrez</trd>
    <trd lng="ru">&c_sh;&c_a;&c_h;&c_m;&c_a;&c_t;&c_y;</trd>
  </snc>
  <snc mrk="sxak.0o.minaco">
    <dif>
      <ref tip="super" cel="situac.0o.cirkonstancaro">Situacio</ref> de la
      <ref tip="vid" cel="regx.0o.sxaka">re&gcirc;o</ref>
      (kaj vulgare, de la damo) atakita de
      malamika &scirc;akpeco; la vorta deklaro pri tia atako:
      <ekz>
        <tld/>on al la nigra re&gcirc;o!
      </ekz>
      <ekz>
        eterna <tld/>o;
      </ekz>
      <ekz>
        <tld/>o (a&ubreve; <tld/>!) <klr>[&#x2026;]</klr> mato (a&ubreve; mat!)
        <fnt>
          <bib>LR</bib>
          <lok>2. Vortuzo</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni signas <tld/>on per 
        <ref tip="vid" cel="plus.0o">pluso</ref>:
        <frm>T:g7+</frm>.
      </ekz>
    </dif>
    <ref tip="vid" cel="mat1.0o.SPO">mato</ref>
    <trd lng="be">&c_sh;&c_a;&c_h; <klr>(&c_u;
    &c_g;&c_u;&c_l;&c_mol;&c_n;&c_je;)</klr></trd>
    <trd lng="ca">escac</trd>
    <trd lng="de">Schach</trd>
    <trd lng="en">check</trd>
    <trd lng="es">jaque</trd>
    <trd lng="fa">&f_k;&f_y;&a_sh;<klr tip="ind">
    (&a_sh;&a_T;&a_r;&a_n;&a_j;)</klr></trd>
    <trd lng="hu">sakk</trd>
    <trd lng="id">skak</trd>
    <trd lng="it">scacco<klr tip="amb"> (mossa)</klr></trd>
    <trd lng="pl">szach</trd>
    <trd lng="pt">xeque</trd>
    <trd lng="ru">&c_sh;&c_a;&c_h;<klr tip="amb"> (&c_v;
    &c_sh;&c_a;&c_h;&c_m;&c_a;&c_t;&c_a;&c_h;)</klr></trd>
  </snc>

  <trd lng="nl">schaak</trd>
  <trd lng="sv">schack</trd>
  <trd lng="uk">&#x448;&#x430;&#x445;</trd>
</drv>

<drv mrk="sxak.0ludo">
  <kap><tld/>ludo</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Kombinacia surtabla <ref tip="lst" cel="lud.0o.ago" lst="voko:ludiloj">ludo</ref>, devenanta ver&scirc;ajne el Hindujo,
      por du ludantoj (nomataj la blankulo kaj la nigrulo),
      havantaj po 16 siakolorajn diversspecajn figuretojn
      (<ref tip="prt" cel="sxak.0peco"><tld/>pecojn</ref>),
      kiujn ili la&ubreve;vice movas sur
      <ref tip="prt" cel="sxak.0tabulo"><tld/>tabulo</ref>
      celante mati la re&gcirc;on kontra&ubreve;ulan:
      <ekz>
        en la albana &ccirc;efurbo Tirano okazis la finala monda <tld/>luda mat&ccirc;o por virinoj
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2012/010578.php">&Scirc;ak-&scirc;oko por Gorba&ccirc;ov</url></vrk>
          <lok>2012</lok>
        </fnt>;
      </ekz>
      <ekz>
        ekde septembro <tld/>ludo fari&gcirc;os studobjekto en minimume dek elementaj lernejoj
        en Albanio
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2014/011288.php">Projekto Kasparov</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_sh;&c_a;&c_h;&c_m;&c_a;&c_t;&c_y;</trd>
    <trd lng="ca">escacs</trd>
    <trd lng="cs">&scaron;achy</trd>
    <trd lng="de">Schachspiel</trd>
    <trd lng="en">chess</trd>
    <trd lng="es">ajedrez</trd>
    <trd lng="fa">&a_sh;&a_T;&a_r;&a_n;&a_j;</trd>
    <trd lng="fr">&eacute;checs</trd>
    <trdgrp lng="hu">
      <trd>sakk</trd>,
      <trd>sakkj&aacute;t&eacute;k</trd>
    </trdgrp>
    <trd lng="id">catur</trd>
   <trd lng="it">scacchi<klr> (gioco)</klr></trd>
    <trdgrp lng="nl">
      <trd>schaken</trd>,
      <trd>schaakspel</trd>
    </trdgrp>
    <trd lng="kk">&c_sh;&c_a;&c_h;&c_m;&c_a;&c_t;</trd>
    <trd lng="pl">szachy</trd>
    <trd lng="pt">xadrez</trd>
    <trd lng="ru">&c_sh;&c_a;&c_h;&c_m;&c_a;&c_t;&c_y;</trd>
    <trd lng="sv">schack</trd>
    <trd lng="tr">satran&ccedil;</trd>
  </snc>
  <trd lng="uk">&#x433;&#x440;&#x430; &#x432; &#x448;&#x430;&#x445;&#x438;</trd>
</drv>

<drv mrk="sxak.0peco">
  <kap><tld/>peco</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      &Ccirc;iu el la 16 blankaj a&ubreve; 16 nigraj pecoj, uzataj en
      <tld/>ludo:
      <frm>8+8</frm> <ref tip="sub" cel="peon.0o">peonoj</ref> kaj
      <frm>8+8</frm> <ref tip="sub" cel="figur.0o.sxaka">figuroj</ref>:
      <ekz>
        &ccirc;iu ludanto havas sian aron de 16 <tld/>pecoj de 6 specoj
        <fnt>
          <bib>Viki</bib>
          <vrk><url ref="&Viki;/%C5%9Cakpeco">&Scirc;akpeco</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_h;&c_m;&c_a;&c_t;&c_n;&c_a;&c_ja;
  &c_f;&c_ib;&c_g;&c_u;&c_r;&c_a;</trd>
  <trd lng="ca">pe&ccedil;a d'escacs</trd>
  <trdgrp lng="de">
    <trd>Stein</trd>,
    <trd>Schachstein</trd>,
    <trd>Schachfigur</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>chess piece</trd>,
    <trd>chesspiece</trd>
  </trdgrp>
  <trd lng="es">pieza <klr>(de ajedrez)</klr></trd>
  <trdgrp lng="en">
    <trd>chessman</trd>,
    <trd>chess piece</trd>
  </trdgrp>
  <trd lng="fa">&a_m;&a_h;&a_r;&a_h;&a_hamza_sure;
  &a_sh;&a_T;&a_r;&a_n;&a_j;</trd>
  <trd lng="nl">schaakstuk</trd>
  <trdgrp lng="hu">
    <trd>sakkb&aacute;bu</trd>,
    <trd>sakkfigura</trd>
  </trdgrp>
  <trd lng="id">bidak</trd>
  <trdgrp lng="it">
  <trd>pezzo<klr> (degli scacchi)</klr></trd>, 
  <trd>scacco<klr tip="amb"> (pezzo)</klr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>figura <ind>szachowa</ind></trd>,
    <trd>pion<klr> szachowy</klr></trd>
  </trdgrp>
  <trd lng="pt">pe&ccedil;a de xadrez</trd>
  <trd lng="ru">&c_sh;&c_a;&c_h;&c_m;&c_a;&c_t;&c_n;&c_a;&c_ja;
  &c_f;&c_i;&c_g;&c_u;&c_r;&c_a;</trd>
  <trd lng="sv">schackpj&auml;s</trd>
  <trd lng="uk">&#x448;&#x430;&#x445;&#x43E;&#x432;&#x430; &#x444;&#x456;&#x491;&#x443;&#x440;&#x430;</trd>
</drv>

<drv mrk="sxak.0tabulo">
  <kap><tld/>tabulo</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Kvadrata <ref tip="super" cel="tabul.0o">tabulo</ref> por ludi <tld/>on, dispartigita en
      8 <ref tip="prt" cel="horizo1.0o.linio">horizontalojn</ref> kaj
      8 <ref tip="prt" cel="vertik.0o.kolumno">vertikalojn</ref>,
      kies interkrucado formas 64 kvadratajn
      <ref tip="prt" cel="fak.0o.ujo">fakojn</ref>:
      <ekz>
        la lando <nom>Gosen</nom> havis aspekton de grandega <tld/>a tabulo, kies verdaj kaj flavaj
        kvadratoj estis desegnitaj per la koloro de l' grenoj kaj de l' palmoj,
        kreskantaj sur la limoj
        <fnt>
          <bib>Far1</bib>
          <lok>&Ccirc;apitro I</lok>
        </fnt>;
      </ekz>
      <ekz>
        rafinitaj iroj sur la <tld/>a tabulo
        <fnt>
          <bib>BdV</bib>
        </fnt>;
      </ekz>
      <ekz>
        ili estas bagatelaj <tld/>soldatoj, sur la granda <tld/>tabulo de la homaro
        <fnt>
          <bib>Marta</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_sh;&c_a;&c_h;&c_m;&c_a;&c_t;&c_n;&c_a;&c_ja;
  &c_d;&c_o;&c_sh;&c_k;&c_a;</trd>
  <trd lng="ca">escaquer</trd>
  <trd lng="de">Schachbrett</trd>
  <trdgrp lng="en"><trd>chess board</trd>,
  <trd>chessboard</trd></trdgrp>
  <trd lng="es">tablero de ajedrez</trd>
  <trd lng="fa">&a_S;&a_f;&a_H;&a_h;&a_hamza_sure;
  &a_sh;&a_T;&a_r;&a_n;&a_j;</trd>
  <trd lng="fr">&eacute;chiquier</trd>
  <trd lng="hu">sakkt&aacute;bla</trd>
  <trd lng="id"><ind>papan</ind> catur</trd>
  <trd lng="it">scacchiera</trd>
  <trd lng="nl">schaakbord</trd>
  <trd lng="pl">szachownica</trd>
  <trd lng="pt">tabuleiro de xadrez</trd>
  <trd lng="ru">&c_sh;&c_a;&c_h;&c_m;&c_a;&c_t;&c_n;&c_a;&c_ja;
  &c_d;&c_o;&c_s;&c_k;&c_a;</trd>
  <trd lng="sv">schackbr&auml;de</trd>
  <trd lng="uk">&#x448;&#x430;&#x445;&#x456;&#x432;&#x43D;&#x438;&#x446;&#x44F;</trd>
</drv>
</art>
