<kap>
  <rad>telegrafi</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="telegrafi.0o">
  <kap><tld/>o</kap>
  <snc mrk="telegrafi.0o.TEL">
    <uzo tip="fak">TEL</uzo>
    <dif>
      Telegrafado; telegrafarto;
      komunikado per
      <ref tip="vid" cel="telegr.0o.TEL">telegrafo</ref>:
      <ekz>
        nova franca registaro en septembro 1924 ne kontra&ubreve;staris
        proponon
        pri Esperanto kiel klara helplingvo en <tld/>o
        <fnt>
          <aut>M. Sims</aut>,
          <vrk><url ref="http://www.esperanto-gb.org/lbe/arkivo/966/11.html">
          Esperanto kaj Ligo de Nacioj 1922&ndash;1924</url></vrk>,
          <lok>La Brita Esperantisto, 2009, printempo</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_e;&c_l;&c_je;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>
  <trd lng="de">Telegrafie</trd>
  <trd lng="en">telegraphy</trd>
  <trd lng="fr">t&eacute;l&eacute;graphie</trd>
  <trd lng="ja">&#x96FB;&#x4FE1;&#34899;  <pr>&#12391;&#12435;&#12375;&#12435;&#12376;&#12421;&#12388;</pr></trd>
  <trd lng="nl">telegrafie</trd>
  <trd lng="pl">telegrafia</trd>
  <trdgrp lng="uk">
    <trd>&c_t;&c_je;&c_l;&c_je;&c_g;&c_r;&c_a;&c_f;&c_ib;&c_ja;</trd>,
    <trd>&c_t;&c_je;&c_l;&c_je;&c_g;&c_r;&c_a;&c_f;&c_n;&c_i;&c_j; &c_z;&c_v;&#8217;&c_ja;&c_z;&c_o;&c_k;</trd>
  </trdgrp>
</drv>
