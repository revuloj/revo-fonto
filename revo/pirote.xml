<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pirote.xml,v 1.20 2022/05/01 17:51:19 revo Exp $">
<kap>
  <rad>piroteknik</rad>/o
  <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="pirote.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Arto prepari artajn <ref cel="fajr.0ajxo">fajra&jcirc;o</ref>jn:
      <ekz>
        <tld/>o-lernantoj, kiuj sin opinias kvitaj, eksplodiginte la konstrua&jcirc;on, &ccirc;ar
        ili pagis la pulvoron
        <fnt>
          <bib>MD11</bib>
          <vrk>Ununura valuto, la sensolva ekvacio</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        liaj plej bonaj verkoj estas vere <tld/>aj virtuoza&jcirc;oj
        <fnt>
          <bib>Plumamikoj</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_ib;&c_r;&c_a;&c_t;&c_e;&c_h;&c_n;&c_ib;&c_k;&c_a;</trd>
  <trd lng="ca">pirot&egrave;cnia</trd>
  <trd lng="cs">pyrotechnika</trd>
  <trd lng="de">Pyrotechnik</trd>
  <trd lng="el">&pi;&ypsilon;&rho;&omikron;&tau;&epsilon;&chi;&ny;&jota_ton;&alfa;</trd>
  <trd lng="en">pyrotechnics</trd>
  <trd lng="es">pirotecnia</trd>
  <trdgrp lng="fr">
    <trd>pyrotechnie</trd>
  </trdgrp>
  <trd lng="hu">pirotechnika</trd>
  <trd lng="nl">pyrotechniek</trd>
  <trd lng="pl">pirotechnika</trd>
  <trdgrp lng="pt">
    <trd>pirotecnia</trd>,
    <trd>pirobologia</trd>
  </trdgrp>
  <trd lng="ro">pirotehnie</trd>
  <trd lng="ru">&c_p;&c_i;&c_r;&c_o;&c_t;&c_je;&c_h;&c_n;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">pyrotechnika</trd>
  <trd lng="uk">&#x43F;&#x456;&#x440;&#x43E;&#x442;&#x435;&#x445;&#x43D;&#x456;&#x43A;&#x430;</trd>
</drv>

<drv mrk="pirote.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc>
    <dif>
      <ref tip="dif" cel="fajr.art0ajxo">Artfajra&jcirc;o</ref>:
      <ekz>
        ina&ubreve;guris la festperiodon koncertoj kaj <tld/>a&jcirc;oj
        <fnt>
          <bib>Monato</bib>
          <aut>Lode Van de Velde</aut>
          <vrk><url ref="&Monato;/2008/009517.php">Feli&ccirc;an datrevenon!</url></vrk>
          <lok>2008</lok>
        </fnt>;
      </ekz>
      <ekz>
        kantisto neatendite ekflamigis <tld/>a&jcirc;on, kies fajreroj atingis la plafonon
        <fnt>
          <bib>Monato</bib>
          <aut>Jozefo Lej&ccirc;</aut>
          <vrk><url ref="&Monato;/2013/010971.php">Kiso mortiga</url></vrk>
          <lok>2013</lok>
        </fnt>;
      </ekz>
      <ekz>
        en la lasta semajnfino de a&ubreve;gusto kelkaj lumigas korton kaj
        strandon per lanternoj kaj <tld/>a&jcirc;oj, tiel adia&ubreve;ante la
        someron
        <fnt><aut>Saliko</aut>: <vrk>&Gcirc;ojoj kaj &gcirc;uoj de finna
        somero</vrk>, <bib>Monato</bib><lok>jaro 2000a, numero 12a, p.
        12a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_je;&c_r;&c_v;&c_e;&c_r;&c_k;</trd>
  <trd lng="bo">&t_sha;&t_naro;&t_ga;&t_tsheg;&t_-a;&t_ba;&t_ra;&t_tsheg;</trd>
  <trdgrp lng="ca">
    <trd>focs d'artifici</trd>,
    <trd>focs artificials</trd>
  </trdgrp>
  <trd lng="de">Feuerwerk</trd>
  <trd lng="en">fireworks</trd>
  <trd lng="es">fuegos artificiales</trd>
  <trd lng="fr">feu d'artifice</trd>
  <trd lng="hu">t&udblac;zij&aacute;t&eacute;k</trd>
  <trd lng="nl">vuurwerk</trd>
  <trd lng="pl">sztuczne ognie  <klr>(fajerwerki)</klr>  {pl}</trd>
  <trd lng="ro">artificii</trd>
  <trd lng="ru">&c_f;&c_je;&c_j;&c_je;&c_r;&c_v;&c_je;&c_r;&c_k;</trd>
</drv>
</art>
