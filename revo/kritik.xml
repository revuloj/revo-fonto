<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: kritik.xml,v 1.26 2020/02/07 20:10:18 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>kritik</rad>/i
</kap>
<drv mrk="kritik.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kritik.0i.ART">
    <dif>
      <ref tip="super" cel="ekzame.0i.esplori">Ekzameni</ref>
      la kvalitojn kaj mankojn de io, de verko ktp:
      <ekz>
        <tld/>anta la pensojn kaj celojn de la koro
        <fnt>
          <bib>NT</bib>
          <lok>Hebreoj 4:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        anstata&ubreve; trankvile <tld/>i la aferon, <klr>[ili]</klr> komencis akran atakadon
        kontra&ubreve; &gcirc;i
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Kion Zamenhof ne povis diri en &Gcirc;enevo.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        Esperanton ja <tld/>is jam multaj miloj da homoj, kaj certe neniu e&ccirc; plej
        malgranda el &gcirc;iaj mankoj restis ka&scirc;ita
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto 1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>i estas facile, fari malfacile</ind>
        <fnt><bib>PrV</bib></fnt>.
        <trd lng="fr">la <ind>critique</ind> est facile, l'art est difficile</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="recenz.0i">recenzi</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>kritisieren</trd>,
      <trd>pr&uuml;fen</trd>,
      <trd>beurteilen</trd>,
      <trd>besprechen</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>to critique</trd>,
      <trd>criticize</trd>
    </trdgrp>
    <trd lng="nl">beoordelen</trd>
  </snc>
  <snc mrk="kritik.0i.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      <ref tip="dif" cel="lauxd.mal0i">Malla&ubreve;di</ref>:
      <ekz>
        tiu grumblemulo &ccirc;iam kaj &ccirc;ion <tld/>as;
      </ekz>
      <ekz>
        sensence kritikanta kaj grimacanta rutina mondo
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Kion Zamenhof ne povis diri en &Gcirc;enevo.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;io &scirc;ajnas al ni <tld/>inda, &ccirc;iu bagatelo, kiu ne estas konforma al nia gusto
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Oka Kongreso Esperantista en Krakow en la 11a de a&ubreve;gusto 1912</vrk>
        </fnt>;
      </ekz>
      <ekz>
        kiu <tld/>as kura&gcirc;e, mem agas malsa&gcirc;e
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxib.0o">&gcirc;ibo</ref>,
      <ref cel="lign">lignero</ref>,
      <ref cel="makul.0o">makulo</ref>,
      <ref cel="trab.0o">trabo</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>kritisieren</trd>,
      <trd>tadeln</trd>,
      <trd>negativ beurteilen</trd>,
      <trd>ung&uuml;nstig besprechen</trd>
    </trdgrp>
    <trd lng="en">to criticize</trd>
    <trd lng="nl">bekritiseren</trd>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>hodnotit</trd>,
    <trd>kritizovat</trd>,
    <trd>posuzovat</trd>
  </trdgrp>
  <trd lng="es">criticar</trd>
  <trd lng="fr">critiquer</trd>
  <trdgrp lng="hu">
    <trd>b&iacute;r&aacute;l</trd>,
    <trd>kritiz&aacute;l</trd>
  </trdgrp>
  <trd lng="pl">krytykowa&cacute;</trd>
  <trd lng="pt">criticar</trd>
  <trd lng="ro">critica</trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_t;&c_i;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trdgrp lng="sk">
    <trd>kritizova&tcaron;</trd>,
    <trd>podrobi&tcaron; kritike</trd>,
    <trd>pos&uacute;di&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="kritik.0a">
  <kap><tld/>a</kap>
  <snc mrk="kritik.0a.ART">
    <dif>
      Ekzamena:
      <ekz>
        <tld/>a esploro;
      </ekz>
      <ekz>
        traduko, eldono kun <tld/>aj notoj;
      </ekz>
      <ekz>
        se tiuj &ccirc;i homoj havus tiom da propra <tld/>a kapablo, por <klr>[&#8230;]</klr>
        <fnt>
          <bib>EE</bib>
        </fnt>;
      </ekz>
      <ekz>
        bonege skribita <tld/>a polemiko
        <fnt>
          <bib>FK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_r;&c_y;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>kritisch</trd>,
      <trd>pr&uuml;fend</trd>
    </trdgrp>
    <trd lng="en">critical</trd>
    <trd lng="es">cr&iacute;tico</trd>
    <trd lng="fr">critique<klr> (adj.)</klr></trd>
    <trdgrp lng="hu">
      <trd>b&iacute;r&aacute;l&oacute;</trd>,
      <trd>kritikus</trd>,
      <trd>kritikai</trd>
    </trdgrp>
    <trd lng="nl">kritisch</trd>
    <trd lng="ru">&c_k;&c_r;&c_i;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
    <trd lng="sk">kritick&yacute;</trd>
  </snc>
  <snc mrk="kritik.0a.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Malla&ubreve;da:
      <ekz>
        <tld/>a intenco;
      </ekz>
      <ekz>
        li rigardis &scirc;in malame kaj <tld/>e.
      </ekz>
    </dif>
    <trd lng="be">&c_k;&c_r;&c_y;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
    <trdgrp lng="de">
      <trd>kritisch</trd>,
      <trd>ung&uuml;nstig</trd>,
      <trd>negativ</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>critical</trd>,
      <trd>censorious</trd>
    </trdgrp>
    <trd lng="es">cr&iacute;tico</trd>
    <trd lng="fr">critique<klr> (adj.)</klr></trd>
    <trdgrp lng="hu">
      <trd>b&iacute;r&aacute;l&oacute;</trd>,
      <trd>kritikus</trd>,
      <trd>kritikai</trd>
    </trdgrp>
    <trd lng="nl">bekritiserend</trd>
    <trd lng="ru">&c_k;&c_r;&c_i;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
    <trd lng="sk">kritick&yacute;</trd>
  </snc>
  <snc mrk="kritik.0a.kriza">
    <uzo tip="stl">EVI</uzo>
    <dif>
      <ref tip="dif" cel="kriz.0a.KOMUNE">Kriza</ref>:
      <ekz>
        necesis fali sub la <tld/>an limon de 5000 membroj 
        por veki&gcirc;i
        <fnt>
          <aut>komitatano Z</aut>,
          <vrk><url ref="http://sezonoj.ru/2014/10/kz-8/">
          Fieron sentu ni interne</url></vrk>,
          <lok>2014-10-13</lok>
        </fnt>;
      </ekz>
      <ekz>
        tio estas &ccirc;io, <klr>[&#8230;]</klr> kion mi povas diri al vi en tiu sufi&ccirc;e <tld/>a
        momento
        <fnt>
          <bib>Monato</bib>
          <aut>CZAR</aut>
          <vrk><url ref="https://www.monato.be/2003/008152.html">La fino de Vincent van Gogh</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
<!-- ne traduku evitindajn:
    <trdgrp lng="en">
      <trd>critical</trd>,
      <trd>crucial</trd>
    </trdgrp -->
  </snc>
  <trd lng="cs">kritick&yacute;</trd>
  <trd lng="pl">krytyczny</trd>
  <trdgrp lng="ro">
    <trd>crucial</trd>,
    <trd>decisiv</trd>,
    <trd>critic</trd>
  </trdgrp>
</drv>

<drv mrk="kritik.0o">
  <kap><tld/>o</kap>
  <snc mrk="kritik.0o.ART">
    <dif>
      Ekzameno pri la bona&jcirc;oj kaj malbona&jcirc;oj iu afero, ekz. de verko:
      <ekz>
        historia, filologia <tld/>o;
      </ekz>
      <ekz>
        senpartia trafa <tld/>o;
      </ekz>
      <ekz>
        <tld/>ojn ni akceptas kun danko, &ccirc;ar ili <klr>[&#8230;]</klr> helpas al nia perfekti&gcirc;o
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kritik</trd>,
      <trd>Pr&uuml;fung</trd>,
      <trd>Beurteilung</trd>,
      <trd>Besprechung</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>critique</trd>,
      <trd>criticism</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>beoordeling</trd>,
      <trd>kritiek</trd>
    </trdgrp>
  </snc>
  <snc mrk="kritik.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Malla&ubreve;do, malaprobo pri la misoj de persono a&ubreve; afero:
      <ekz>
        el &ccirc;iuj flankoj oficialaj venis nur <tld/>oj a&ubreve; suspektoj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; kontra&ubreve; pastra prediko trovi&gcirc;as <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ju laboro pli publika, des pli granda la <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kritik</trd>,
      <trd>Tadel</trd>,
      <trd>negative <ind>Beurteilung</ind></trd>,
      <trd>ung&uuml;nstige <ind>Besprechung</ind></trd>
    </trdgrp>
    <trd lng="en">criticism</trd>
    <trd lng="nl">kritiek</trd>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_t;&c_y;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>kritika</trd>,
    <trd>posudek</trd>
  </trdgrp>
  <trd lng="es">cr&iacute;tica</trd>
  <trd lng="fr">critique</trd>
  <trdgrp lng="hu">
    <trd>b&iacute;r&aacute;lat</trd>,
    <trd>kritika</trd>
  </trdgrp>
  <trd lng="pl">krytyka</trd>     
  <trd lng="ro">critic&abreve;</trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_t;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>kritika</trd>,
    <trd>posudok</trd>
  </trdgrp>
</drv>

<drv mrk="kritik.0acxi">
  <kap><tld/>a&ccirc;i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kritik.0acxi.KOMUNE">
    <dif>
      Insiste, malice <tld/>i, &ccirc;iam emfazante samajn misojn:
      <ekz>
        mi kredis la aforismon: iru vian vojon kaj lasu al aliaj
        <tld/>a&ccirc;i
        <fnt>
          <aut>Wang Chongfang</aut>,
          <vrk>Mia revo &ccirc;e la sojlo de la 1a jarcento</vrk>,
          <lok><url ref="http://www.elerno.net/penseo/pen101.htm">Penseo,
          numero 101a, 2000-03</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> <tld/>a&ccirc;as la lingvon kaj blasfemas kontra&ubreve; &gcirc;i
        <fnt>
          <vrk><url ref="http://daziwon.net/freeboard/201129">Esperanto kaj Socia Movado</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>schm&auml;hen</trd>,
      <trd>m&auml;keln</trd>,
      <trd>verrei&szlig;en</trd>,
      <trd>auf etwas herum<ind>hacken</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="en">
    <trd>to censure</trd>,
    <trd>disparage</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;nigrer</trd>,
    <trd>d&eacute;pr&eacute;cier</trd>,
    <trd>d&eacute;tracter</trd>
  </trdgrp>
  <trd lng="nl">bekritiseren</trd>
  <trd lng="pl">krytykowa&cacute; podle</trd>     
  <trd lng="ro">critic&abreve; &icirc;n mod josnic</trd>
</drv>

<drv mrk="kritik.0ema">
  <kap><tld/>ema</kap>
  <snc mrk="kritik.0ema.KOMUNE">
    <dif>
      Volonte <tld/>anta, trovanta temojn por <tld/>o:
      <ekz>
        <nom>Kalocsay</nom> kiel &ccirc;efredaktoro de Literatura Mondo iniciatis la 
        <tld/>eman recenzon
        <fnt>
          <aut>C. kaj A. Gacond</aut>,
          <vrk><url ref="http://www.esperanto-gacond.ch/Claude_Gacond_Radioprelego_0425.pdf">
          Juan R&eacute;gulo-P&eacute;rez kaj K&aacute;lm&aacute;n Kalocsay</url></vrk>,
          <lok>radioprelego, 1971-12-01 kaj 04</lok>
        </fnt>;
      </ekz>
      <ekz>
        la specialaj bierspecoj &gcirc;uas restari&gcirc;on sub influo de la pli <tld/>ema konsumanto
        <fnt>
          <bib>Monato</bib>
          <aut>Lode van de Velde</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/007144.html">Bierlando Belgio</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        &jcirc;urnalistaro estas <tld/>ema pri &ccirc;io
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="https://www.monato.be/2010/009966.php">Retaksante la bu&gcirc;eton</url></vrk>
          <lok>2010</lok>
        </fnt>.
      </ekz>
    </dif>
    
  </snc>
  <trdgrp lng="cs">
    <trd>kritick&yacute;</trd>,
    <trd>posuzova&ccaron;n&yacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>m&auml;kelig</trd>,
    <trd>kritikw&uuml;tig</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>critical</trd>,
    <trd>analytical</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>caustique<klr> (critique)</klr></trd>,
    <trd>critique<klr> (adj.)</klr></trd>
  </trdgrp>
  <trd lng="pl">krytyczny</trd>
  <trdgrp lng="ro">
    <trd>crucial</trd>,
    <trd>decisiv</trd>,
    <trd>critic</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>kritick&yacute;</trd>,
    <trd>posudzova&ccaron;n&yacute;</trd>
  </trdgrp>
</drv>

<drv mrk="kritik.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="kritik.0isto.KOMUNE">
    <dif>
      Homo, kiu publike <tld/>as, sciigas sian opinion pri verkoj:
      <ekz>
        kompetenta, severa, justa <ind><tld/>isto</ind>;
      </ekz>
      <ekz>
        &leftquot;tion mi povus esprimi pli bone!&rightquot; pensis la granda
        <tld/>isto
        <fnt><bib>Fab3</bib>, <lok>Io</lok></fnt>;
      </ekz>
      <ekz>
        por igi la <tld/>istojn tute apartigi la aferon de la a&ubreve;toro, mi publike diras
        mem, ke mi <klr>[&#8230;]</klr> estas tute senmerita kaj ne konata
        <fnt>
          <bib>DL</bib>
        </fnt>.
      </ekz>
    </dif>
    
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_t;&c_y;&c_k;</trd>
  <trd lng="cs">kritik</trd>
  <trd lng="de">Kritiker</trd>
  <trd lng="en">critic</trd>
  <trd lng="fr">critique<klr tip="amb"> (personne)</klr></trd>
  <trdgrp lng="nl">
    <trd>recensent</trd>,
    <trd>criticus</trd>
  </trdgrp>
  <trd lng="pl">krytyk</trd>     
  <trd lng="ro">critic</trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_t;&c_i;&c_k;</trd>
  <trd lng="sk">kritik</trd>
</drv>

<drv mrk="kritik.mem0o">
  <kap>mem<tld/>o</kap>
  <snc mrk="kritik.mem0o.mallauxdo">
    <dif>
      Malla&ubreve;do de si mem, agnosko de propraj misoj kaj misagoj:
      <ekz>
        mi ne povas &ccirc;esi miri pri la nesuperebla aroganteco de kelkaj nord-amerikanoj
        kaj ilia &scirc;ajne nula kapablo pri mem<tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="https://www.monato.be/2004/008237.html">&Ccirc;u kompatindaj irakanoj?</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        mem<tld/>o, same kiel sinmokado, ofte rezultas el klara memkonscio
        <fnt>
          <bib>Monato</bib>
          <aut>Reinaldo Marcelo Ferreira</aut>
          <vrk><url ref="https://www.monato.be/2006/008937.php">Kronikoj de niaj frustri&gcirc;oj</url></vrk>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        anta&ubreve; &ccirc;io necesas mem<tld/>o de la koncernatoj
        <fnt>
          <bib>Monato</bib>
          <aut>Isikawa Takasi</aut>
          <vrk><url ref="https://www.monato.be/2012/010733.php">Mankas elektro, mankas memkritiko</url></vrk>
          <lok>2012</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="cs">sebekritika</trd>
  <trd lng="de">Selbstkritik</trd>
  <trd lng="en">self-criticism</trd>
  <trd lng="es">autocr&iacute;tica</trd>
  <trd lng="fr">autocritique</trd>
  <trd lng="hu">&ouml;nkritika</trd>
  <trd lng="nl">zelfkritiek</trd>
  <trd lng="pl">samokrytyka</trd>     
  <trd lng="ro">autocritic&abreve;</trd>
  <trd lng="sk">sebakritika</trd>
</drv>
</art>
