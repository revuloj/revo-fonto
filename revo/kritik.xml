  </trdgrp>
  <trd lng="uk">&c_k;&c_r;&c_i;&c_t;&c_i;&c_ch;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#20005;&#26684; <pr>y&#225;ng&#233;</pr></trd>,
    <trd>&#22196;&#26684; <pr>y&#225;ng&#233;</pr></trd>,
    <trd>&#25209;&#35413;&#24615; <pr>p&#299;p&#237;ngx&#236;ng</pr></trd>,
    <trd>&#25209;&#35780;&#24615; <pr>p&#299;p&#237;ngx&#236;ng</pr></trd>,
    <trd>&#35413;&#21028;&#24615; <pr>p&#237;ngp&#224;nx&#236;ng</pr></trd>,
    <trd>&#35780;&#21028;&#24615; <pr>p&#237;ngp&#224;nx&#236;ng</pr></trd>,
    <trd>&#35413;&#35542;&#24615; <pr>p&#237;ngl&#249;nx&#236;ng</pr></trd>,
    <trd>&#35780;&#35770;&#24615; <pr>p&#237;ngl&#249;nx&#236;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kritik.0o">
  <kap><tld/>o</kap>
  <snc mrk="kritik.0o.ART">
    <dif>
      <ref tip="super" cel="ekzame.0o.esploro">Ekzameno</ref> pri la bona&jcirc;oj kaj
      malbona&jcirc;oj iu afero, ekz. de verko:
      <ekz>
        historia, filologia <tld/>o;
      </ekz>
      <ekz>
        literatura <tld/>o
        <fnt>
          <bib>MMa</bib>
          <lok>&Ccirc;apitro 6</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o de la sofista retorika skolo
        <fnt>
          <bib>Ret</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o estis tre favora, la recenzoj akcentis la introspektivajn bela&jcirc;ojn de
        la poemo
        <fnt>
          <bib>SatirRak</bib>
          <lok>Liriko</lok>
        </fnt>;
      </ekz>
      <ekz>
        ju la <tld/>oj de mia lingvo estas pli severaj, des pli danke mi ilin alprenas
        <fnt>
          <bib>DL</bib>
          <lok>Dua Libro de l' Lingvo Internacia</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>ojn ni akceptas kun danko, &ccirc;ar ili <klr>[&#8230;]</klr> helpas al nia perfekti&gcirc;o
        <fnt>
          <bib>Homaranismo</bib>
          <vrk>Nefermita letero al s-ro de Beaufront.</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o egalas al atako nur por iu, kiu sentas sin perfekta
        <fnt>
          <bib>BonaLingvo</bib>
          <lok>Postparolo Al La Dua Eldono</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estas pli konata pro siaj <klr>[&#8230;]</klr> prilingvaj artikoloj, <tld/>oj kaj
        recenzoj en La Suda Stelo
        <fnt>
          <bib>EeP</bib>
          <lok>Dua Parto</lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kritik</trd>,
      <trd>Pr&uuml;fung</trd>,
      <trd>Beurteilung</trd>,
      <trd>Besprechung</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>critique</trd>,
      <trd>criticism</trd>
    </trdgrp>
    <trdgrp lng="nl">
      <trd>beoordeling</trd>,
      <trd>kritiek</trd>
    </trdgrp>
  </snc>
  <snc mrk="kritik.0o.KOMUNE">
    <uzo tip="stl">KOMUNE</uzo>
    <dif>
      Malla&ubreve;do, mal<ref tip="ant" cel="aprob.0o">aprobo</ref> pri la misoj de
      persono a&ubreve; afero:
      <ekz>
        el &ccirc;iuj flankoj oficialaj venis nur <tld/>oj a&ubreve; suspektoj
        <fnt>
          <bib>VivZam</bib>
        </fnt>;
      </ekz>
      <ekz>
        estas tre malfortaj <klr>[&#8230;]</klr> la <tld/>oj, kiujn &scirc;i ankora&ubreve; intencis esprimi
        <fnt>
          <bib>ChL</bib>
          <lok>&Ccirc;apitro Deka</lok>
        </fnt>;
      </ekz>
      <ekz>
        lia rigardo kaj lia nura &ccirc;eesto efikis al li kiel silenta <tld/>o
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Unua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        e&ccirc; kontra&ubreve; pastra prediko trovi&gcirc;as <tld/>o
        <fnt><bib>PrV</bib></fnt>;
      </ekz>
      <ekz>
        ju laboro pli publika, des pli granda la <tld/>o
        <fnt><bib>PrV</bib></fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>Kritik</trd>,
      <trd>Tadel</trd>,
      <trd>negative <ind>Beurteilung</ind></trd>,
      <trd>ung&uuml;nstige <ind>Besprechung</ind></trd>
    </trdgrp>
    <trd lng="en">criticism</trd>
    <trd lng="nl">kritiek</trd>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_t;&c_y;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>kritika</trd>,
    <trd>posudek</trd>
  </trdgrp>
  <trd lng="es">cr&iacute;tica</trd>
  <trd lng="fr">critique</trd>
  <trdgrp lng="hu">
    <trd>b&iacute;r&aacute;lat</trd>,
    <trd>kritika</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#25209;&#35413;  <pr>&#12402;&#12402;&#12423;&#12358;</pr></trd>,
    <trd>&#32771;&#35388;  <pr>&#12371;&#12358;&#12375;&#12423;&#12358;</pr></trd>,
    <trd>&#25209;&#21028;  <pr>&#12402;&#12399;&#12435;</pr></trd>
  </trdgrp>
  <trd lng="pl">krytyka</trd>
  <trd lng="ro">critic&abreve;</trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_t;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>kritika</trd>,
    <trd>posudok</trd>
  </trdgrp>
  <trd lng="uk">&c_k;&c_r;&c_i;&c_t;&c_i;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#25209;&#35413; <pr>p&#299;p&#237;ng</pr></trd>,
    <trd>&#25209;&#35780; <pr>p&#299;p&#237;ng</pr></trd>,
    <trd>&#25209;&#21028; <pr>p&#299;p&#224;n</pr></trd>,
    <trd>&#20070;&#35780; <pr>sh&#363;p&#237;ng</pr></trd>,
    <trd>&#26360;&#35413; <pr>sh&#363;p&#237;ng</pr></trd>,
    <trd>&#24433;&#35413; <pr>y&#464;ngp&#237;ng</pr></trd>,
    <trd>&#24433;&#35780; <pr>y&#464;ngp&#237;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kritik.0acxi">
  <kap><tld/>a&ccirc;i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="kritik.0acxi.KOMUNE">
    <dif>
      Insiste, malice <tld/>i, &ccirc;iam emfazante samajn misojn:
      <ekz>
        mi kredis la aforismon: iru vian vojon kaj lasu al aliaj
        <tld/>a&ccirc;i
        <fnt>
          <aut>Wang Chongfang</aut>,
          <vrk>Mia revo &ccirc;e la sojlo de la 1a jarcento</vrk>,
          <lok><url ref="http://www.elerno.net/penseo/pen101.htm">Penseo,
          numero 101a, 2000-03</url></lok>
        </fnt>;
      </ekz>
      <ekz>
