<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kritik</rad>/i
</kap>
<drv mrk="kritik.0i">
  <kap>
    <ofc>*</ofc><tld/>i
  </kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ekzameni la kvalitojn kaj mankojn de scienca,
      literatura a&ubreve; belarta verko:
      <ekz><tld/>i estas facile, fari malfacile<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        kompetenta, severa, justa <ind><tld/>isto</ind>.
        <trd lng="be">&c_k;&c_r;&c_y;&c_t;&c_y;&c_k;</trd>
        <trd lng="ru">&c_k;&c_r;&c_i;&c_t;&c_i;&c_k;</trd>
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>kritisieren</trd>,
       <trd>pr&uuml;fen</trd>,
       <trd>beurteilen</trd>,
       <trd>besprechen</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Montri la mankojn de persono a&ubreve; afero:
      <ekz>
        tiu grumblemulo &ccirc;iam kaj &ccirc;ion <tld/>as;
      </ekz>
      <ekz>kiu <tld/>as kura&gcirc;e, mem agas
malsa&gcirc;e<fnt><bib>PrV</bib></fnt></ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="lauxd.mal0i">malla&ubreve;di</ref>,
      <ref cel="vip.0i">vipi</ref>.
    </refgrp>
    <refgrp tip="vid">
      <ref cel="trab.0o">trabo</ref>,
      <ref cel="gxib.0o">&gcirc;ibo</ref>,
      <ref cel="nagx">na&gcirc;o</ref>,
      <ref cel="makul.0o">makulo</ref>,
      <ref cel="lign">lignero</ref>.
    </refgrp>
    <trdgrp lng="de">
       <trd>kritisieren</trd>,
       <trd>tadeln</trd>,
       <trd>negativ beurteilen</trd>,
       <trd>ung&uuml;nstig besprechen</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_t;&c_y;&c_k;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="en">criticize</trd>
  <trd lng="fr">critiquer</trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_t;&c_i;&c_k;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">kritizova&tcaron;</trd>
</drv>

<drv mrk="kritik.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
    <dif>
      Ekzameno pri la bona&jcirc;oj kaj malbona&jcirc;oj de verko:
      <ekz>
        historia, filologia <tld/>o;
      </ekz>
      <ekz>
        senpartia trafa <tld/>o.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>Kritik</trd>,
       <trd>Pr&uuml;fung</trd>,
       <trd>Beurteilung</trd>,
       <trd>Besprechung</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Malaprobo pri la malbona&jcirc;oj de persono a&ubreve; afero:
      <ekz>
        fari <tld/>ojn kontra&ubreve; io<fnt>Z</fnt>;
      </ekz>
      <ekz>e&ccirc; kontra&ubreve; pastra prediko trovi&gcirc;as
<tld/>o<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>ju laboro pli publika, des pli granda la
<tld/>o<fnt><bib>PrV</bib></fnt>.</ekz>
    </dif>
    <trdgrp lng="de">
       <trd>Kritik</trd>,
       <trd>Tadel</trd>,
       <trd>negative Beurteilung</trd>,
       <trd>ung&uuml;nstige Besprechung</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_t;&c_y;&c_k;&c_a;</trd>
  <trd lng="fr">critique</trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_t;&c_i;&c_k;&c_a;</trd>
  <trd lng="sk">kritika</trd>
</drv>

<drv mrk="kritik.0a">
  <kap>
    <tld/>a
  </kap>
  <snc>
    <dif>
      Ekzamena:
      <ekz>
        <tld/>a esploro;
      </ekz>
      <ekz>
        traduko, eldono kun <tld/>aj notoj;
      </ekz>
      <ekz>
        sen<tld/>ema entuziasmo;
      </ekz>
      <ekz>
        homo prudenta rilatos al tiuj asertoj tre <tld/>e<fnt>Z</fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>kritisch</trd>,
       <trd>pr&uuml;fend</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Malla&ubreve;da:
      <ekz>
        <tld/>a intenco;
      </ekz>
      <ekz>
        li rigardis &scirc;in malame kaj <tld/>e.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>kritisch</trd>,
       <trd>ung&uuml;nstig</trd>,
       <trd>negativ</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_k;&c_r;&c_y;&c_t;&c_y;&c_ch;&c_n;&c_y;</trd>
  <trd lng="fr">critique</trd>
  <trd lng="ru">&c_k;&c_r;&c_i;&c_t;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_i;&c_j;</trd>
  <trd lng="sk">kritick&yacute;</trd>
</drv>
</art>

