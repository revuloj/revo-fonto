<drv mrk="lok.mal0a">
  <kap>mal<tld/>a</kap>
  <snc mrk="lok.mal0a.KOMUNE">
    <dif>
      &Ccirc;iea, rilata al la tuto, al diversaj a&ubreve; &ccirc;iuj <tld/>oj:
      <ekz>
        mal<tld/>a radioaktiva infektado pro testadoj de nukleaj armiloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=279362">Joniga
          fona radiado</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">KOMP</uzo>
        mal<tld/>a variablo, metodo, komputila reto;
      </ekz>
      <ekz>
        en mal<tld/>a reto, la komputiloj estas malproksimaj geografie,
        Interreto estas la plej fama mal<tld/>a reto
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=62617">Komputila
          reto</url></lok>
        </fnt>;
      </ekz>
      <ekz>
      <uzo tip="fak">MAT</uzo>
        estas facile trovi <tld/>ajn minimumojn &mdash; aldonaj
        faktoj pri la problemo (ekzemple scio de tio ke la funkcio
        estas konveksa) estas postulitaj por certi&gcirc;i, ke la solvo
        fundamente estas mal<tld/>a minimumo
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=104280">Optimumigo
          (matematiko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="ant" cel="lok.0a.parta"><tld/>a<sncref/></ref>
  </snc>
  <trd lng="be">&c_g;&c_l;&c_ja;&c_b;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  <trdgrp lng="cs">
    <trd>celkov&yacute;</trd>,
    <trd>glob&aacute;ln&iacute;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>global</trd>,
    <trd>weit</trd>,
    <trd>allgemein</trd>
  </trdgrp>
  <trd lng="en">global</trd>
  <trd lng="es">global</trd>
  <trd lng="fr">global</trd>
  <trd lng="hu">glob&aacute;lis</trd>
  <trd lng="id">global</trd>
  <trd lng="nl">globaal</trd>
  <trd lng="ru">&c_g;&c_l;&c_o;&c_b;&c_a;&c_l;&c_mol;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>celkov&yacute;</trd>,
    <trd>glob&aacute;lny</trd>
  </trdgrp>
  <trd lng="sv">global</trd>
</drv>

<drv mrk="lok.mis0a">
  <kap>mis<tld/>a</kap>
  <snc mrk="lok.mis0a.KOMUNE">
    <dif>
      Kiu estas en mal&gcirc;usta loko,
      en mal&gcirc;usta funkcio, a&ubreve; eble
      <ref tip="vid" cel="embara.0a.embarasanta">embarasa</ref>:
      <ekz>
        nuanc' mistere mis<tld/>a en la lumospektro
        <fnt>
          <aut>L. N. Newell</aut>,
          <vrk>La Astronomo</vrk>,
          <lok><url
          ref="http://literaturamondo.org/PDF/1947/LM%201947-1112c.pdf">
          Literatura Mondo, 1947 (11-12)</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        meze de tiu terur<klr>[o]</klr> ekfloras unua amo, kvaza&ubreve;
        mis<tld/>a rozo 
        rekte apud la sterkejo
        <fnt>
          <bib>Monato</bib>
          <aut>Boris Kolker</aut>
          <vrk><url
          ref="http://steloj.de/esperanto/monato2/007110.html">Atentokapta
          
           sed ru&gcirc;krajoninda</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>falsch platziert</trd>,
    <trd>deplatziert</trd>,
    <trd>st&ouml;rend</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>movido<klr> (fuera de su lugar)</klr></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>misplaced</trd>,
    <trd>out of place</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>d&eacute;plac&eacute;<klr> (&agrave; la mauvaise place)</klr></trd>,
    <trd>pas &agrave; sa place</trd>
  </trdgrp>
</drv>

<drv mrk="lok.trans0i">
  <kap>trans<tld/>i</kap>
