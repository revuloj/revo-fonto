<drv mrk="lok.0aro">
  <kap><tld/>aro</kap>
  <snc mrk="lok.0aro.KOMUNE">
    <dif>
      Aro de lokoj:
      <ekz>
        paleontologia <tld/>aro
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=319179">Ameriko
          (kalko&scirc;tonaj minejoj)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="es">ubicaciones</trd>
    <trd lng="fr">emplacements</trd>
  </snc>
  <snc mrk="lok.0aro.MAT">
    <uzo tip="fak">MAT</uzo>
    <fnt><bib>MatTerm</bib>, <lok>p. 32</lok></fnt>
    <uzo tip="stl">ARK</uzo>
    <dif>
      <ref tip="super" cel="ar.0o.MAT">Aro</ref> de punktoj,
      verigantaj iun econ:
      <ekz>
        la <tld/>aro de
        <ref cel="centr.kurbeco0o.de_ebena_kurbo">kurbecocentroj</ref>
        de
        iu kurbo nomi&gcirc;as ties <ref cel="volv.el0ato">elvolvato</ref>;
      </ekz>
      <ekz>
        parabolo<klr>[n oni]</klr> povas difini kiel la <tld/>aro de 
        punktoj, kiuj estas egaldistancaj de donita punkto (la fokuso) 
        kaj donita linio (la direktanto)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=30468">Parabolo
          (matematiko)</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <rim>
      Kvankam tiu &ccirc;i termino estas a&ubreve;toritata, bone formita kaj
      ar&hcirc;aika, &ccirc;ar &ccirc;iam anstata&ubreve;igebla
      per <ctl>aro</ctl>, oni ial enkondukis por &gcirc;i novan,
      nerekomendindan formon <ctl>lokuso</ctl>
      <fnt><bib>PIV1</bib></fnt>.
    </rim>
    <trd lng="de">geometrischer <ind>Ort</ind></trd>
    <trd lng="en">geometric <ind>locus</ind></trd>
    <trd lng="es">espacio geom&eacute;trico</trd>
    <trd lng="fr">lieu g&eacute;om&eacute;trique</trd>
    <trd lng="hu">ponthalmaz</trd>
    <trd lng="pl">miejsce geometryczne</trd>
    <trd lng="ru">&c_g;&c_je;&c_o;&c_m;&c_je;&c_t;&c_r;&c_i;&c_ch;&c_je;&c_s;&c_k;&c_o;&c_je;
    <ind>&c_m;&c_je;&c_s;&c_t;&c_o;</ind></trd>
  </snc>
  <trd lng="cs">geometrick&eacute; m&iacute;sto</trd>
  <trd lng="sk">geometrick&eacute; miesto</trd>
</drv>

<drv mrk="lok.0umi">
  <kap><tld/>umi<fnt>Z</fnt></kap>
  <snc mrk="lok.0umi.arangxi">
    <dif>
      <ref tip="dif" cel="lok.0i.arangxi"><tld
      lit="L"/>i<sncref/></ref>:
      <ekz>
        la bebon vi povas <tld/>umi en infanvartejon
        <fnt>
        <aut>I. Ilf, E. Petrov, trad. V. Okc</aut>,
        <vrk><url
        ref="http://www.esperanto.mv.ru/Usono/truu9.html">Unueta&gcirc;a
        Usono</url></vrk>,
        <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        sin <tld/>umi (dungigi) kiel motoriston;
      </ekz>
      <ekz>
        <uzo tip="fak">EKON</uzo>
        <tld/>umi (procente pruntedoni) monon en bankon
        <klr>(vd <ref tip="sin"
        cel="invest.0i.EKON">investi<sncref/></ref>)</klr>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>parken</trd>,
      <trd>stellen</trd>,
      <trd>platzieren</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>place (in school or in office)</trd>,
      <trd>lend (at interest), invest</trd>
    </trdgrp>
    <trd lng="es">colocar<klr> (en un empleo, un cargo)</klr></trd>
    <trd lng="fr">placer<klr> (dans un emploi, une
    fonction)</klr></trd>
    <trd lng="hu">elhelyez <klr tip="ind">(&aacute;tv.)</klr></trd>
    <trdgrp lng="ru">
      <trd>&c_p;&c_o;&c_m;&c_je;&c_s;&c_t;&c_i;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_r;&c_i;&c_s;&c_t;&c_r;&c_o;&c_i;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <snc mrk="lok.0umi.trovi">
    <dif>
      <ref tip="sin" cel="trov.0i.elsercxi">Trovi<sncref/></ref>; 
      <ref tip="sin" cel="sond.0i.TEK">sondi<sncref/></ref>,
      malkovri la ekzaktan <tld/>on de io:
      <ekz>
        la fi&scirc;kaptaj &scirc;ipoj uzas la sonaron anka&ubreve; por <tld/>umo de 
        fi&scirc;aro, <klr>[&#8230;]</klr> e&ccirc; anka&ubreve; por <tld/>umi unuopajn fi&scirc;ojn
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=42170">E&hcirc;osondilo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">lokalisieren</trd>
    <trd lng="en">locate</trd>
  </snc>
  <trd lng="cs">um&iacute;stit</trd>
  <trd lng="sk">umiestni&tcaron;</trd>
