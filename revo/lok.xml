<drv mrk="lok.du0a">
  <kap>du<tld/>a</kap>
  <fnt><bib>KompLeks</bib></fnt>
  <snc mrk="lok.du0a.KOMUNE">
    <dif>
      Hanvanta du <ref tip="vid"
      cel="lok.0o.spaca">lokojn<sncref/></ref>:
      <ekz>
        li &scirc;in sidigas sen demand' &ccirc;e tablo nur du<tld/>a
        <fnt>
          <aut>Raymond Schwartz</aut>
          <vrk>La Stranga Butiko</vrk>
          <lok>Vagonrestoracio</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">zweisitzig</trd>
  </snc>
  <snc mrk="lok.du0a.operacio">
    <uzo tip="fak">MAT</uzo>
    <uzo tip="fak">KOMP</uzo>
    <dif>
      Kies <ref cel="lok.0nombro.argumenta">loknombro</ref> estas du:
      <ekz>
        subtraho estas du<tld/>a operacio;
      </ekz>
      <ekz>
        <ctl>superi</ctl> estas du<tld/>a rilato;
      </ekz>
      <ekz>
        ofte temas pri operacioj kaj rilatoj du<tld/>aj 
        kaj unu<tld/>aj, en la programlingvo C la kondi&ccirc;a 
        esprimo <frm am="x?y:z">x&nbsp;?&nbsp;y&nbsp;:&nbsp;z</frm> formas operacion
        tri<tld/>an
        <fnt>
          <bib>Viki</bib>
          <lok><url
          ref="https://eo.wikipedia.org/?curid=42878">Loknombro</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="en">
      <trd>binary</trd>,
      <trd>dyadic</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>dyadisch</trd>,
      <trd>zweistellig</trd>,
      <trd>zweiwertig</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>de dos <ind>argumentos</ind></trd>,
      <trd>binario<klr> (de dos argumentos)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>&agrave; deux <ind>arguments</ind></trd>,
      <trd>binaire<klr> (&agrave; deux arguments)</klr></trd>
    </trdgrp>
    <trdgrp lng="hu">
      <trd>diadikus</trd>,
      <trd>bin&eacute;r</trd>,
      <trd>k&eacute;t operandus&uacute;</trd>
    </trdgrp>
    <trd lng="id">biner</trd>
    <trdgrp lng="ru">
