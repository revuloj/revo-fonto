<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: hin1.xml,v 1.2 2022/03/20 15:36:35 revo Exp $">
    <kap>
      <rad>hin</rad>/o
    </kap>

<drv mrk="hin1.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Antikva <ref tip="lst" lst="voko:kvantoj" cel="unu.mezur0o">mezurunuo</ref>
      por likva&jcirc;oj, sesono de bat'o, &ccirc; 3,7 &ndash; 6,5 litroj:
      <ekz>
        <klr>[donu]</klr> kvaronon de <tld/>o da vino por ver&scirc;oferi
        <fnt>
          <bib>MT</bib>
          <lok>Eliro 29:40</lok>
        </fnt>;
      </ekz>
      <ekz>
        peziloj &gcirc;ustaj, efo &gcirc;usta, kaj <tld/>o &gcirc;usta estu &ccirc;e vi
        <fnt>
          <bib>MT</bib>
          <lok>Levidoj 19:36</lok>
        </fnt>;
      </ekz>
      <ekz>
        tri dekonoj de efo da delikata faruno, miksita kun duono de <tld/>o da oleo
        <fnt>
          <bib>MT</bib>
          <lok>Nombroj 15:9</lok>
        </fnt>;
      </ekz>
      <ekz>
        akvon trinku la&ubreve;mezure, po sesono de <tld/>o trinku de tempo al tempo
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 4:11</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Hin</trd>
  </snc>
  <rim>Ne konfuzu kun <ref tip="hom" cel="hin.0o">hino</ref>
    <klr>(ido de &ccirc;evalo kun azenino)</klr>.
  </rim>
  <trd lng="de">Hin</trd>
  <trd lng="uk">&#x43E;&#x441;&#x43B;&#x44E;&#x43A;</trd>
</drv>

  </art>

<!--
$Log$
-->
</vortaro>
