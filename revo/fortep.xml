<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>fortepian</rad>/o
</kap>
<drv mrk="fortep.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Granda <ref tip="super" cel="instru2.kord0o">kordinstrumento</ref> kun horizontalaj kordoj kaj klavaro. 
    </dif>
    <refgrp tip="vid">
      <ref cel="pian.0o">piano</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Fl&uuml;gel</trd>
  <trd lng="fr">piano &agrave; queue</trd>
  <trd lng="nl">vleugelpiano</trd>
  <trdgrp lng="ru">
    <trd>&c_f;&c_o;&c_r;&c_t;&c_je;&c_p;&c_i;&c_a;&c_n;&c_o;</trd>,
    <trd>&c_r;&c_o;&c_ja;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
</art>
