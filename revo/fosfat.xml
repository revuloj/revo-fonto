<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>fosfat</rad>/o
</kap>
<drv mrk="fosfat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sal.0o.KEM">Salo</ref> de <tld/>a acido.  
    </dif>
  </snc>
  <trd lng="de">Phosphat</trd>
  <trd lng="fr">phosphate</trd>
  <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_a;&c_t;</trd>
</drv>
<drv mrk="fosfat.0aacido">
  <kap><tld/>a acido</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="acid.0o">Acido</ref> el
      <ref cel="fosfor.0o">fosforo</ref>.
      H<sub>3</sub>PO<sub>4</sub>.
    </dif>
  </snc>
  <trd lng="fr">acide phosphorique</trd>
</drv>
</art>
