<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: fosfat.xml,v 1.26 2023/08/31 13:11:31 revo Exp $">
    <kap>
      <rad>fosfat</rad>/o
    </kap>
<drv mrk="fosfat.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="sal.0o.KEM">Salo</ref> de <ref tip="prt" cel="fosfor.0o">fosfora</ref>
      acido:
      <ekz>
        la britoj tie produktas <tld/>on, kiu abundis sur Nauro kaj nun preska&ubreve; el&ccirc;erpi&gcirc;is
        <fnt>
          <bib>Monato</bib>
          <aut>Garvan Makaj</aut>
          <vrk><url ref="&Monato;/2008/009581.php">Viziti mikro&scirc;tatojn</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        ili postulas malpermeson de <tld/>oj en la lavo-prepara&jcirc;oj por plibonigi la akvokvaliton
        <fnt>
          <bib>Monato</bib>
          <aut>Evgeni Georgiev</aut>
          <vrk><url ref="&Monato;/2010/010038.php">Priatenti la naturprotekton</url></vrk>
        </fnt>.
      </ekz>        
    </dif>
  </snc>
  <trd lng="be">&c_f;&c_a;&c_s;&c_f;&c_a;&c_t;</trd>
  <trd lng="bg">&c_f;&c_o;&c_s;&c_f;&c_a;&c_t;</trd>
  <trd lng="ca">fosfat</trd>
  <trd lng="de">Phosphat</trd>
  <trd lng="en">phosphate</trd>
  <trd lng="es">fosfato</trd>
  <trd lng="fa">&a_f;&a_s;&a_f;&a_A;&a_t;</trd>
  <trd lng="fr">phosphate</trd>
  <trd lng="hu">foszf&aacute;t</trd>
  <trd lng="ja">&#x30EA;&#x30F3;&#x9178;&#x5869;  [<ind>&#x30EA;&#x30F3;&#x3055;&#x3093;&#x3048;&#x3093;</ind>]</trd>
  <trd lng="nl">fosfaat</trd>
  <trd lng="pt">fosfato</trd>
  <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_a;&c_t;</trd>
  <trd lng="sv">fosfat</trd>
  <trd lng="uk">&c_f;&#x43E;&c_s;&c_f;&c_a;&c_t;</trd>
</drv>
<drv mrk="fosfat.0aacido">
  <kap><tld/>a acido,
    <var><kap><tld/>acido</kap></var>
  </kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      <ref tip="super" cel="acid.0o">Acido</ref> el
      <ref tip="prt" cel="fosfor.0o">fosforo</ref>, precipe
      <frm>H<sub>3</sub>PO<sub>4</sub></frm>:
      <ekz>
        &ccirc;iu ribozo ligi&gcirc;as kun unu organika bazo kaj anka&ubreve; kun du <tld/>-acidaj grupoj
        <fnt>
          <aut>Ste&ccirc;jo Norvell</aut>
          <vrk><url ref="&Monato;/2000/007157.html">Riboj</url></vrk>
          <bib>Monato</bib>
          <lok>2000/11, p. 16-17</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_f;&c_o;&c_s;&c_f;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</trd>,
    <trd>&c_a;&c_r;&c_t;&c_a;&c_f;&c_o;&c_s;&c_f;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_n;&c_a; &c_k;&c_i;&c_s;&c_je;&c_l;&c_i;&c_n;&c_a;</trd>
  <trd lng="ca">&agrave;cid fosf&ograve;ric</trd>
  <trd lng="cs">kyselina fosfore&ccaron;n&aacute;</trd>
  <trd lng="de">Phosphors&auml;ure</trd>
  <trd lng="en">phosphoric acid</trd>
  <trd lng="fa">&a_A;&a_s;&f_y;&a_d; &a_f;&a_s;&a_f;&a_r;&f_y;&f_k;</trd>
  <trd lng="fr">acide phosphorique</trd>
  <trd lng="hu">foszforsav</trd>
  <trd lng="ja">&#x30EA;&#x30F3;&#x9178;  [<ind>&#x30EA;&#x30F3;&#x3055;&#x3093;</ind>]</trd>
  <trd lng="nl">fosforzuur</trd>
  <trd lng="pt">&aacute;cido fosf&oacute;rico</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_r;&c_t;&c_o;&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>,
    <trd>&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">kyselina fosforov&aacute;</trd>
  <trd lng="sv">fosforsyra</trd>
</drv>
  </art>
