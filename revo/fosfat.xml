<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>fosfat</rad>/o
    </kap>
    <drv mrk="fosfat.0o">
      <kap><tld/>o</kap>
      <uzo tip="fak">KEM</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="sal.0o.KEM">Salo</ref> de <tld/>a acido.  
        </dif>
      </snc>
      <trd lng="be">&c_f;&c_a;&c_s;&c_f;&c_a;&c_t;</trd>
      <trd lng="de">Phosphat</trd>
      <trd lng="es">fosfato</trd>
      <trd lng="fr">phosphate</trd>
      <trd lng="hu">foszf&aacute;t</trd>
      <trd lng="nl">fosfaat</trd>
      <trd lng="ru">&c_f;&c_o;&c_s;&c_f;&c_a;&c_t;</trd>
    </drv>
    <drv mrk="fosfat.0aacido">
      <kap><tld/>a acido</kap>
      <uzo tip="fak">KEM</uzo>
      <snc>
        <dif>
          <ref tip="super" cel="acid.0o">Acido</ref> el
          <ref cel="fosfor.0o">fosforo</ref>, precipe
          <frm>H<sub>3</sub>PO<sub>4</sub></frm>.
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_f;&c_o;&c_s;&c_f;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</trd>,
        <trd>&c_a;&c_r;&c_t;&c_a;&c_f;&c_o;&c_s;&c_f;&c_a;&c_r;&c_n;&c_a;&c_ja; &c_k;&c_ib;&c_s;&c_l;&c_a;&c_t;&c_a;</trd>
      </trdgrp>
      <trd lng="de">Phosphats&auml;ure</trd>
      <trd lng="fr">acide phosphorique</trd>
      <trd lng="hu">foszforsav</trd>
      <trd lng="nl">fosforzuur</trd>
      <trdgrp lng="ru">
 
<trd>&c_o;&c_r;&c_t;&c_o;&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_n;&c_a;&c_ja;
&c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd>,
        <trd>&c_f;&c_o;&c_s;&c_f;&c_o;&c_r;&c_n;&c_a;&c_ja;
&c_k;&c_i;&c_s;&c_l;&c_o;&c_t;&c_a;</trd></trdgrp>
    </drv>
  </art>
