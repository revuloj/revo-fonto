  <fnt><bib>JED</bib></fnt>
</kap>

<drv mrk="afrik.A0o">
  <kap><ofc>8</ofc><tld/>o</kap>
  <snc mrk="afrik.A0o.mondparto">
  <uzo tip="fak">GEOG</uzo>
    <dif>
      La <ref tip="lst" cel="mond.0oparto" lst="voko:mondopartoj">mondoparto</ref>
      sude de E&ubreve;ropo:
      <ekz>
        Aleksandro ekvolis iri Afrikon
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        la palmobran&ccirc;oj de Norda <tld/>o
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maljuna viro la&ubreve;te legadis pri Afriko,
        pri la grandaj arbaroj kaj elefantoj
        <fnt><bib>Fab2</bib>, <lok>Malnova strata lanterno</lok></fnt>;
      </ekz>
      <ekz>
        la cikonio rakontis pri la varma Afriko, pri la piramidoj
        kaj pri la struto kiu galopas tra la dezerto kiel
        sova&gcirc;a &ccirc;evalo
        <fnt><bib>Fab2</bib>, <lok>Dio de dormo</lok></fnt>.
      </ekz>
    </dif>
  <trd lng="th">&#xE17;&#xE27;&#xE35;&#xE1B;&#xE41;&#xE2D;&#xE1F;&#xE23;&#xE34;&#xE01;&#xE32;</trd>
  </snc>
  <snc mrk="afrik.A0o.kontinento">
    <dif>
      La <ref tip="lst" cel="kontin.0o.terkuno" lst="voko:kontinentoj">kontinento</ref>
      sude de E&ubreve;ropo (sen Madagaskaro):
      <ekz>
        pluraj etnoj konsistigas la popolon de la nigra kontinento <tld/>o, kion montras
        anka&ubreve; la diverseco de lingvoj, parolataj tie
        <fnt>
          <bib>Monato</bib>
          <aut>Sagradjelou Kokou</aut>
          <vrk><url ref="&Monato;/2005/008729.php">Lingvoj malpurigitaj</url></vrk>
          <lok>2005</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o estas anka&ubreve; kontinento, kie, la&ubreve; opinio de plimulto da sciencistoj,
        unuafoje naski&gcirc;is la viruso, provokanta &ccirc;e homo la tiel nomatan sindromon de
        imunoperdo, tio estas aidoso
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj aidoso</url></vrk>
          <lok>2004</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="zh">&#x963F;&#x975E;&#x5229;&#x52A0;&#x6D32; <pr>&#x101;f&#x113;il&#xEC;ji&#x101;sh&#x11B;ng</pr></trd>
</snc>
  <trd lng="be">&c_A;&c_f;&c_r;&c_y;&c_k;&c_a;</trd>
  <trd lng="bg">&c_A;&c_f;&c_r;&c_i;&c_k;&c_a;</trd>
  <trd lng="ca">&Agrave;frica</trd>
  <trd lng="de">Afrika</trd>
  <trd lng="en">Africa</trd>
  <trd lng="es">&Aacute;frica</trd>
  <trd lng="fr">Afrique</trd>
  <trd lng="fy">Afrika</trd>
  <trd lng="hu">Afrika</trd>
  <trd lng="id">Afrika</trd>
  <trd lng="io">Afrika</trd>
  <trd lng="it">Africa</trd>
  <trd lng="ja">&#x30A2;&#x30D5;&#x30EA;&#x30AB;  <pr>&#x3042;&#x3075;&#x308A;&#x304B;</pr></trd>
  <trd lng="nl">Afrika</trd>
  <trd lng="no">Afrika</trd>
  <trd lng="pl">Afryka</trd>
  <trd lng="pt">&Aacute;frica</trd>
  <trd lng="ru">&c_A;&c_f;&c_r;&c_i;&c_k;&c_a;</trd>
  <trd lng="tok">ma Apika</trd>
  <trd lng="tr">Afrika</trd>
  <trd lng="sk">Afrika</trd>
  <trd lng="sv">Afrika</trd>
  <trd lng="uk">&c_A;&c_f;&c_r;&c_i;&#x43A;&c_a;</trd>
  <trd lng="vo">Frikop</trd>
  <trd lng="ms">Afrika</trd>
</drv>

<drv mrk="afrik.0ano">
  <kap><tld lit="a"/>ano</kap>
  <snc>
    <dif>
      Lo&gcirc;anto de
      <ref tip="malprt" cel="afrik.A0o.mondparto"><tld/>o</ref>:
      <ekz>
        la <tld/>ano kun la maldelikataj viza&gcirc;trajtoj,
        la dikaj lipoj, la nigraj krispaj haroj, sidas sur la
        marmora &scirc;tuparo de la palaco en la &ccirc;efurbo
        de Portugalujo kaj petas almozojn
        <fnt><bib>Fab3</bib>, <lok>Dorna vojo de la honoro</lok></fnt>;
      </ekz>
      <ekz>
        por perforta kaptado de sklavoj e&ubreve;ropanoj dungis specialajn trupojn
        konsistantajn el <tld lit="a"/>anoj, kiuj penetris multajn regionojn de la
        kontinento
        <fnt>
          <bib>Monato</bib>
          <aut>Vladimir Lemelev</aut>
          <vrk><url ref="&Monato;/2004/008160.html">Afriko kaj aidoso</url></vrk>
          <lok>2004</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u <tld lit="a"/>anoj <klr>[&#x2026;]</klr> ne meritas protekton kaj helpon de UN?
        <fnt><aut>Stefan Maul</aut>:
          <vrk><url ref="&Monato;/2000/006837.php">Neglektata kontinento</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/02, p. 5</lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="vid" cel="negr.0o">negro</ref>
  </snc>
  <trd lng="be">&c_a;&c_f;&c_r;&c_y;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="bg">&c_a;&c_f;&c_r;&c_i;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="ca">afric&agrave;</trd>
  <trd lng="de">Afrikaner</trd>
  <trd lng="en">African</trd>
  <trd lng="es">africano</trd>
  <trd lng="fr">Africain<klr> (habitant)</klr></trd>
  <trd lng="hu">afrikai</trd>
  <trd lng="id"><ind>orang</ind> Afrika</trd>
  <trd lng="io">Afrikano</trd>
  <trd lng="it">africano</trd>
  <trd lng="nl">Afrikaan</trd>
  <trd lng="no">afrikaner</trd>
  <trd lng="ms">orang Afrika</trd>
  <trdgrp lng="pl">
    <trd>Afrykanin</trd>,
    <trd>Afryka&nacute;czyk</trd>,
    <trd>Afro<klr> (pot.)</klr></trd>,
    <trd>Murzyn<klr> (pot., cz&eogonek;sto obra&zacute;l.)</klr></trd>
  </trdgrp>
  <trd lng="pt">africano</trd>
  <trd lng="ru">&c_a;&c_f;&c_r;&c_i;&c_k;&c_a;&c_n;&c_je;&c_c;</trd>
  <trd lng="sk">Afri&ccaron;an</trd>
  <trd lng="sv">afrikan</trd>
  <trd lng="th">&#xE0A;&#xE32;&#xE27;&#xE41;&#xE2D;&#xE1F;&#xE23;&#xE34;&#xE01;&#xE32;</trd>
  <trd lng="tok">jan Apika</trd>
  <trd lng="tr">Afrika'l&inodot;</trd>
  <trd lng="vo">Frikopan</trd>
</drv>

<drv mrk="afrik.CentrA0o">
  <kap>
    <ofc>10</ofc>
    Centr-<tld lit="A"/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">CF</mlg>
  <snc mrk="afrik.CentrA0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="land.0o.POL" lst="voko:&scirc;tatoj">Lando</ref>
      en la centra <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Bango; teritorio de Respubliko Centr-Afriko,
      limanta kun
      <ref tip="vid" cel="cxad.CX0o.GEOG">&Ccirc;ado</ref>,
      <ref tip="vid" cel="kamerun.K0o.GEOG">Kameruno</ref>,
      <ref tip="vid" cel="kong.K0oBrazavila">Kongo Brazavila</ref>,
      <ref tip="vid" cel="kong.K0oKinsxasa.GEOG">Kongo
      Kin&scirc;asa</ref>
      kaj <ref tip="vid" cel="sudan.S0o.GEOG">Sudano</ref>:
      <ekz>
        dekomence iluziaj estis intervenoj en ekzemple Centr-<tld/>o, Kongo kaj Somalio,
        &ccirc;ar tie malamikaj partioj a&ubreve; tute ne a&ubreve; nur surpapere kapablis interkonsenti
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2005/008797.php">Jubileo sen pompo</url></vrk>
          <lok>2005</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#xE1B;&#xE23;&#xE30;&#xE40;&#xE17;&#xE28;&#xE41;&#xE2D;&#xE1F;&#xE23;&#xE34;&#xE01;&#xE32;&#xE01;&#xE25;&#xE32;&#xE07;</trd>
    </snc>
    <trdgrp lng="be">
      <trd>&c_C;&c_e;&c_n;&c_t;&c_r;&c_a;&c_l;&c_mol;&c_n;&c_a;-&c_A;&c_f;&c_r;&c_y;&c_k;&c_a;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_R;&c_e;&c_s;&c_p;&c_u;&c_b;&c_l;&c_ib;&c_k;&c_a;</trd>,
      <trd>&c_C;&c_A;&c_R;</trd>
    </trdgrp>
    <trd lng="ca">Rep&uacute;blica Centreafricana</trd>
    <trdgrp lng="fr">
    <trd>Centrafrique</trd>,
    <trd>R&eacute;publique centrafricaine</trd>
    </trdgrp>
    <trd lng="fy">Sintraal-Afrika</trd>
    <trd lng="id"><ind>Afrika</ind> Tengah</trd>
    <trd lng="io">Centrafrika</trd>
    <trd lng="it">Repubblica <ind>Centrafricana</ind></trd>
    <trd lng="ja">&#x4E2D;&#x592E;&#x30A2;&#x30D5;&#x30EA;&#x30AB;
    <pr>&#x3061;&#x3085;&#x3046;&#x304A;&#x3046;&#x30A2;&#x30D5;&#x30EA;&#x30AB;</pr></trd>
  <trd lng="nl">Centraal-Afrika</trd>
    <trd lng="no">Sentral-Afrika</trd>
    <trd lng="ms">Afrika Tengah</trd>
    <trdgrp lng="pl">
      <trd>Republika <ind>&Sacute;rodkowoafryka&nacute;ska</ind></trd>
    </trdgrp>
    <trd lng="pt">Rep&uacute;blica da &Aacute;frica Central</trd>
    <trd lng="tok">ma Santapiken</trd>
    <trd lng="vo">Z&auml;noda-Frikop</trd>
    <trd lng="zh">&#x4E2D;&#x975E; <pr>zh&#x14D;ngf&#x113;i</pr></trd>
</drv>

  <drv mrk="afrik.OkcidentA0o">
    <kap><ofc>10</ofc>Okcident-<tld/>o</kap>
    <snc mrk="afrik.OkcidentA0o.GEOG">
      <uzo tip="fak">GEOG</uzo>
      <dif>
        Okcidenta parto de <tld/>o, limigita de Atlantiko kaj
        la landoj Ma&ubreve;ritanio, Malio, Ni&gcirc;ero, Ni&gcirc;erio (inkluzive):
        <ekz>
              la usona prezidanto <nom>Barack Obama</nom> vizitis la okcident-<tld lit="a"/>an
              &scirc;taton
              Ganao
              <fnt>
                <bib>Monato</bib>
                <aut>Princo Henriko Oguinye</aut>
                <vrk><url ref="&Monato;/2009/009869.php">Obama: For la fortuloj!</url></vrk>
                <lok>2009</lok>
              </fnt>.
            </ekz>
      </dif>
    </snc>
    <trd lng="id"><ind>Afrika</ind> Barat</trd>
    <trdgrp lng="zh">
    <trd>&#x897F;&#x90E8;&#x975E;&#x6D32; <pr>x&#x12B;b&#xF9;f&#x113;izh&#x14D;u</pr></trd>,
    <trd>&#x897F;&#x975E; <pr>x&#x12B;f&#x113;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="afrik.SudA0o">
  <kap>
    <ofc>10</ofc>
    Sud-<tld lit="A"/>o
    <fnt><bib>RLN</bib></fnt>
  </kap>
  <mlg kod="ISO-3166">ZA</mlg>
  <snc mrk="afrik.SudA0o.GEOG">
    <uzo tip="fak">GEOG</uzo>
    <uzo tip="fak">POL</uzo>
    <dif>
      <ref tip="lst" cel="land.0o.POL" lst="voko:&scirc;tatoj">Lando</ref>
      &ccirc;e la suda ekstremo de <ref tip="malprt" cel="afrik.A0o">Afriko</ref>
      kun &ccirc;efurbo Pretorio.
      Teritorio de Federacia Respubliko Sud-Afriko.
      &Gcirc;i limas kun
      <ref tip="vid" cel="bocvan.B0o.GEOG">Bocvano</ref>,
      <ref tip="vid" cel="lesot.L0o.GEOG">Lesoto</ref>,
      <ref tip="vid" cel="mozambik.M0o.GEOG">Mozambiko</ref>,
      <ref tip="vid" cel="namibi.N0o.GEOG">Namibio</ref>,
      <ref tip="vid" cel="svazi.S0lando.GEOG">Svazilando</ref>
      kaj <ref tip="vid" cel="zimbabv.Z0o.GEOG">Zimbabvo</ref>:
      <ekz>
        reprezentantoj de Sud-<tld/>o
        <fnt><aut>Laimius Stra&zcaron;nickas</aut>:
          <vrk><url ref="&Monato;/2000/007097.php">Per malnovaj a&ubreve;toj &ccirc;irka&ubreve; Balta Maro</url></vrk>,
          <bib>Monato</bib>
          <lok>2000/11, p. 25</lok>
        </fnt>;
      </ekz>
      <ekz>
        &gcirc;i estas donaco de Sud-<tld/>o por danki tiujn, kiuj unue navigis al Kabo de
        Bona Espero
        <fnt>
          <bib>Monato</bib>
          <aut>William Simcock</aut>
          <vrk><url ref="&Monato;/2003/007873.html">Impresoj pri Portugalio</url></vrk>
          <lok>2003</lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="th">&#xE1B;&#xE23;&#xE30;&#xE40;&#xE17;&#xE28;&#xE41;&#xE2D;&#xE1F;&#xE23;&#xE34;&#xE01;&#xE32;&#xE43;&#xE15;&#xE49;</trd>
    </snc>
    <trdgrp lng="be">
      <trd>&c_P;&c_a;&c_w;&c_d;&c_n;&c_jo;&c_v;&c_a;-&c_A;&c_f;&c_r;&c_y;&c_k;&c_a;&c_n;&c_s;&c_k;&c_a;&c_ja; &c_R;&c_e;&c_s;&c_p;&c_u;&c_b;&c_l;&c_ib;&c_k;&c_a;</trd>,
      <trd>&c_P;&c_A;&c_R;</trd>
    </trdgrp>
    <trd lng="ca">Rep&uacute;blica de Sud-&agrave;frica</trd>
    <trd lng="en">South Africa</trd>
    <trd lng="fr">Afrique du Sud</trd>
    <trd lng="fy">S&uacute;d-Afrika</trd>
    <trd lng="id"><ind>Afrika</ind> Selatan</trd>
    <trd lng="io">Sudafrika</trd>
    <trd lng="it">Sudafrica</trd>
    <trd lng="ja">&#x5357;&#x30A2;&#x30D5;&#x30EA;&#x30AB;
    <pr>&#x307F;&#x306A;&#x307F;&#x30A2;&#x30D5;&#x30EA;&#x30AB;</pr></trd>
  <trd lng="nl">Zuid-Afrika</trd>
    <trd lng="no">S&oslash;r-Afrika</trd>
    <trd lng="ms">Afrika Selatan</trd>
    <trdgrp lng="pl">
      <trd>Republika <ind>Po&lstroke;udniowej Afryki</ind></trd>,
      <trd>RPA</trd>
    </trdgrp>
    <trd lng="pt">&Aacute;frica do Sul</trd>
    <trd lng="tok">ma Setapika</trd>
    <trd lng="vo">Sul&uuml;dafrik&auml;n</trd>
    <trdgrp lng="zh">
    <trd>&#x5357;&#x975E; <pr>n&#xE1;nf&#x113;i</pr></trd>,
    <trd>&#x5357;&#x975E;&#x5171;&#x548C;&#x56FD; <pr>n&#xE1;nf&#x113;ig&#xF2;ngh&#xE9;gu&#xF3;</pr></trd>,
    <trd>&#x5357;&#x975E;&#x5171;&#x548C;&#x570B; <pr>n&#xE1;nf&#x113;ig&#xF2;ngh&#xE9;gu&#xF3;</pr></trd>
  </trdgrp>
</drv>
