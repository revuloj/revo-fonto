<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>4</ofc>
  <rad>knar</rad>/i
</kap>
<drv mrk="knar.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Eligi sekan kaj akran bruon de skrapado a&ubreve; frotado:
      <ekz><tld/>as plumo sur papero;</ekz>
      <ekz><tld/>as seka felo<fnt>Z</fnt>,</ekz>
      <ekz>rado<fnt>Z</fnt>,</ekz>
      <ekz>pordo<fnt>Z</fnt>,</ekz>
      <ekz>tondilo<fnt>Z</fnt>,</ekz>
      <ekz>novaj botoj<fnt>Z</fnt>,</ekz>
      <ekz>motoro, ne&gcirc;o sub la piedoj<fnt>Z</fnt>;</ekz>
      <ekz>la malnova planko <tld/>as;</ekz>
      <ekz><tld/>as la vekhorlo&gcirc;o;</ekz>
      <ekz><tld/>anta vo&ccirc;o<fnt>B</fnt>;</ekz>
      <ekz><tld/>antaj mevoj (<ref tip="sin" cel="grak.0i">graki</ref>);</ekz>
      <ekz>la <tld/>ado estas bruo malpli akuta ol la grincado.</ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="grinc.0i">grinci</ref>, 
      <ref cel="krak.0i">kraki</ref>, 
      <ref cel="klak.0i">klaki</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_p;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_r;&c_a;&c_b;&c_u;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trd lng="de">knarren</trd>
  <trdgrp lng="en">
    <trd>grince</trd>,
    <trd>grate</trd>,
    <trd>grind</trd>,
    <trd>scrape</trd>,
    <trd>scratch</trd>,
    <trd>squeak</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kraken</trd>,
    <trd>knarsen</trd>
  </trdgrp>
<trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_r;&c_i;&c_p;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_h;&c_r;&c_u;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="knar.0ilo">
  <kap><tld/>ilo<fnt>Z</fnt></kap>
  <snc>
    <dif>
      Brua ilo, konsistanta el ligna tabuleto, kiu <tld/>as
      turni&gcirc;ante &ccirc;irka&ubreve; la tenilo.  
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_s;&c_k;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="de">Knarre</trd>
  <trd lng="en">clack</trd>
  <trd lng="nl">ratel</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_shch;&c_o;&c_t;&c_k;&c_a;</trd>
</drv>
</art>
