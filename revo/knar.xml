<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: knar.xml,v 1.19 2021/04/30 07:03:18 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>knar</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="knar.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="knar.0i.brui">
    <dif>
      Seke, akre <ref tip="super" cel="bru.0i.soni">brui</ref> pro
      skrapado a&ubreve; frotado, malpli akute ol grincado:
      <ekz>
        <tld/>as plumo sur papero;
      </ekz>
      <ekz>
        <tld/>as seka felo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        rado<fnt>Z</fnt>,
      </ekz>
      <ekz>
        pordo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        tondilo<fnt>Z</fnt>,
      </ekz>
      <ekz>
        novaj botoj<fnt>Z</fnt>,
      </ekz>
      <ekz>
        motoro, ne&gcirc;o sub la piedoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        la malnova planko <tld/>as;
      </ekz>
      <ekz>
        <tld/>as la vekhorlo&gcirc;o;
      </ekz>
      <ekz>
        <tld/>anta vo&ccirc;o<fnt>B</fnt>;
      </ekz>
      <ekz>
        <tld/>antaj mevoj <klr>(<ref cel="grak.0i">graki</ref>)</klr>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grinc.0i">grinci</ref>,
      <ref cel="krak.0i">kraki</ref>.
    </refgrp>
    <trdgrp lng="hu">
      <trd>nyikorog</trd>,
      <trd>ropog</trd>,
      <trd>serceg</trd>
    </trdgrp>
  </snc>
  <snc mrk="knar.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pene funkcii, kun misoj, malakordoj, protestoj:
      <ekz>
        la nova maniero administri la bienon <tld/>is kiel ne&scirc;mirita
        rado
        <fnt><bib>PatrojFiloj</bib>, <lok>&ccirc;apitro 8a, p.
        36a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">nyikorog</trd>    
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_p;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_r;&c_a;&c_b;&c_u;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hrkat</trd>,
    <trd>hr&ccaron;et</trd>,
    <trd>rachotit</trd>,
    <trd>sk&rcaron;&iacute;pat</trd>,
    <trd>vrzat</trd>
  </trdgrp>
  <trd lng="de">knarren</trd>
  <trdgrp lng="en">
    <trd>grince</trd>,
    <trd>grate</trd>,
    <trd>grind</trd>,
    <trd>scrape</trd>,
    <trd>scratch</trd>,
    <trd>squeak</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>crisser<klr tip="ind"> (en &eacute;mettant un son plut&ocirc;t
    bas)</klr></trd>,
    <trd>couiner<klr tip="ind"> (en &eacute;mettant un son plut&ocirc;t
    bas)</klr></trd>,
    <trd>gratter<klr tip="ind"> (&eacute;mettre un bruit de
    grattement)</klr></trd>,
    <trd>grincer<klr tip="ind"> (en &eacute;mettant un son plut&ocirc;t
    bas)</klr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kraken</trd>,
    <trd>knarsen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>chrobota&cacute;</trd>,
    <trd>chrupa&cacute;</trd>,
    <trd>skrzypie&cacute;</trd>,
    <trd>trzeszcze&cacute;</trd>,
    <trd>zgrzyta&cacute;</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>cr&abreve;p&abreve;tur&abreve;</trd>,
    <trd>scr&acirc;&#537;ni</trd>,
    <trd>zg&acirc;ria</trd>
  </trdgrp> 
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_r;&c_i;&c_p;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_h;&c_r;&c_u;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hr&ccaron;a&tcaron;</trd>,
    <trd>rapka&tcaron;</trd>,
    <trd>v&racute;zga&tcaron;</trd>,
    <trd>&scaron;kre&ccaron;a&tcaron;</trd>
  </trdgrp>
</drv>

<drv mrk="knar.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="knar.0.SON">
    <dif>
      Skrapa, frota a&ubreve; kvaza&ubreve; &gcirc;ema
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">sono</ref>:
      <ekz>
        la palisoj de la barilo<klr>...</klr>
        <tld/>is malgaje: <ctl><tld/>', <tld/>',
        <tld/>'</ctl>
        <fnt>
          <bib>Fab3</bib>,
          <vrk>Lino</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_p;</trd>
  <trd lng="fr">iii<klr> (grincement)</klr></trd>
</drv>


<drv mrk="knar.0ilo">
  <kap><tld/>ilo<fnt>Z</fnt></kap>
  <snc mrk="knar.0ilo.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Bruilo konsistanta el ligna tabuleto, kiu <tld/>as
      turni&gcirc;ante &ccirc;irka&ubreve; tenilhava akso.
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_s;&c_k;&c_o;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>hrka&ccaron;ka</trd>,
    <trd>&rcaron;ehta&ccaron;ka</trd>
  </trdgrp>
  <trd lng="de">Knarre</trd>
  <trd lng="en">clack</trd>
  <trd lng="fr">cr&eacute;celle</trd>
  <trd lng="hu">kerepl&odblac;</trd>
  <trd lng="nl">ratel</trd>
  <trdgrp lng="pl">
    <trd>grzechotka</trd>,
    <trd>terkotka</trd>
  </trdgrp> 
  <trd lng="ro">mori&#537;c&abreve;</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_shch;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="sk">rapk&aacute;&ccaron;</trd>
</drv>
</art>
