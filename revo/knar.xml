<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: knar.xml,v 1.26 2023/12/03 09:58:51 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>knar</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="knar.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="knar.0i.brui">
    <dif>
      Seke, akre <ref tip="super" cel="bru.0i.soni">brui</ref> pro
      <ref cel="skrap.0i.SON">skrapado</ref> a&ubreve; <ref cel="frot.0i">frotado</ref>,
      malpli akute ol grincado:
      <ekz>
        ek<tld/>is la duonfermita pordo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        la seka felo en la sako la&ubreve;te ek<tld/>is
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        liaj botoj <tld/>is terure la&ubreve;te
        <fnt>
          <bib>Fab2</bib>
          <lok>Ne&gcirc;a re&gcirc;ino</lok>
        </fnt>;
      </ekz>
      <ekz>
        la ne&gcirc;o <tld/>is sub la piedoj, kiel se oni &ccirc;iam havus sur si novajn botojn
        <fnt>
          <bib>Fab2</bib>
          <lok>Sambuka virineto</lok>
        </fnt>;
      </ekz>
      <ekz>
        la planko <tld/>is
        <fnt>
          <bib>Fab3</bib>
          <lok>Johan&ccirc;jo-malsa&gcirc;ulo</lok>
        </fnt>;
      </ekz>
      <ekz>
        la levma&scirc;ino <tld/>is kaj la siteloj dancadis en la aero
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>is per plumoj skribistoj, eluzante malhelan lumon de kandeloj
        <fnt>
          <bib>Metrop</bib>
        </fnt>;
      </ekz>
      <ekz>
        rado malbona <tld/>as plej multe
        <fnt>
          <bib>PrV</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="grinc.0i">grinci</ref>,
      <ref cel="krak.0i">kraki</ref>
    </refgrp>
    <trdgrp lng="hu">
      <trd>nyikorog</trd>,
      <trd>ropog</trd>,
      <trd>serceg</trd>
    </trdgrp>
  </snc>
  <snc mrk="knar.0i.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Pene funkcii, kun misoj, malakordoj, protestoj:
      <ekz>
        la nova maniero administri la bienon <tld/>is kiel ne&scirc;mirita
        rado
        <fnt><bib>PatrojFiloj</bib> <lok>&ccirc;apitro 8a, p.
        36a</lok></fnt>.
      </ekz>
    </dif>
    <trd lng="hu">nyikorog</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_p;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_r;&c_y;&c_p;&c_a;&c_c;&c_mol;</trd>,
    <trd>&c_h;&c_r;&c_a;&c_b;&c_u;&c_s;&c_mol;&c_c;&c_je;&c_c;&c_mol;</trd>,
    <trd>&c_l;&c_u;&c_s;&c_k;&c_a;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>hrkat</trd>,
    <trd>hr&ccaron;et</trd>,
    <trd>rachotit</trd>,
    <trd>sk&rcaron;&iacute;pat</trd>,
    <trd>vrzat</trd>
  </trdgrp>
  <trd lng="de">knarren</trd>
  <trdgrp lng="en">
    <trd>grince</trd>,
    <trd>grate</trd>,
    <trd>grind</trd>,
    <trd>scrape</trd>,
    <trd>scratch</trd>,
    <trd>squeak</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>crisser<klr tip="ind"> (en &eacute;mettant un son plut&ocirc;t
    bas)</klr></trd>,
    <trd>couiner<klr tip="ind"> (en &eacute;mettant un son plut&ocirc;t
    bas)</klr></trd>,
    <trd>gratter<klr tip="ind"> (&eacute;mettre un bruit de
    grattement)</klr></trd>,
    <trd>grincer<klr tip="ind"> (en &eacute;mettant un son plut&ocirc;t
    bas)</klr></trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x304D;&#x3057;&#x308B;</trd>,
    <trd>&#x30AD;&#x30A4;&#x3068;&#x97F3;&#x3092;&#x7ACB;&#x3066;&#x308B;  <pr>&#x30AD;&#x30A4;&#x3068;&#x304A;&#x3068;&#x3092;&#x305F;&#x3066;&#x308B;</pr></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>kraken</trd>,
    <trd>knarsen</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>chrobota&cacute;</trd>,
    <trd>chrupa&cacute;</trd>,
    <trd>skrzypie&cacute;</trd>,
    <trd>trzeszcze&cacute;</trd>,
    <trd>zgrzyta&cacute;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>cr&abreve;p&abreve;tur&abreve;</trd>,
    <trd>scr&acirc;&#x219;ni</trd>,
    <trd>zg&acirc;ria</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_k;&c_r;&c_i;&c_p;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_h;&c_r;&c_u;&c_s;&c_t;&c_je;&c_t;&c_mol;</trd>,
    <trd>&c_t;&c_r;&c_je;&c_shch;&c_a;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>hr&ccaron;a&tcaron;</trd>,
    <trd>rapka&tcaron;</trd>,
    <trd>v&racute;zga&tcaron;</trd>,
    <trd>&scaron;kre&ccaron;a&tcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_s;&c_k;&c_r;&c_i;&c_p;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_r;&c_i;&c_p;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_h;&c_r;&c_u;&c_s;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_r;&c_ib;&c_shch;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_r;&c_ib;&c_s;&c_k;&c_o;&c_t;&c_ib;&c_t;&c_i;</trd>,
    <trd>&c_t;&c_o;&c_r;&c_o;&c_h;&c_t;&c_ib;&c_t;&c_i;</trd>
  </trdgrp>
  <trd lng="zh">&#x54AF;&#x5431; <pr>g&#x113;zh&#x12B;</pr></trd>
</drv>

<drv mrk="knar.0">
  <kap><tld/></kap>
  <gra><vspec>sonimito</vspec></gra>
  <snc mrk="knar.0.SON">
    <dif>
      Skrapa, frota a&ubreve; kvaza&ubreve; &gcirc;ema
      <ref tip="lst" cel="son.0o" lst="voko:sonoj">sono</ref>:
      <ekz>
        la palisoj de la barilo <klr>(&#x2026;)</klr>
        <tld/>is malgaje: <ctl><tld/>', <tld/>',
        <tld/>'</ctl>
        <fnt>
          <bib>Fab3</bib>
          <vrk>Lino</vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_y;&c_p;</trd>
  <trd lng="de">knarr!</trd>
  <trd lng="fr">iii<klr> (grincement)</klr></trd>
  <trd lng="zh">&#x54AF;&#x5431; <pr>g&#x113;zh&#x12B;</pr></trd>
</drv>

<drv mrk="knar.0o">
  <kap>
    <tld/>o,
    <var><kap><tld/>ado</kap></var>
  </kap>
  <snc mrk="knar.0o.SON">
    <dif>
      Bruo de tio, kio <tld/>as:
      <ekz>
        ili silentis&#x2026; a&ubreve;di&gcirc;is nur la <tld/>ado de la plumo de l&#x2019; juna skribisto, sidanta
        &ccirc;e la apuda tablo
        <fnt>
          <aut>A. Aharonjan, trad. S. Ha&jcirc;jan (S. Mkrt&ccirc;jan)</aut>
          <vrk>La patro</vrk>
          <lok>en: Orienta Almanako, 1913</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_y;&c_p;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>,
    <trd>&c_h;&c_r;&c_a;&c_b;&c_u;&c_s;&c_mol;&c_c;&c_je;&c_n;&c_mol;&c_n;&c_je;</trd>
  </trdgrp>
<trdgrp lng="fr">
  <trd>crissement</trd>,
  <trd>grincement</trd>
</trdgrp>
  <trd lng="zh">&#x560E;&#x5431; <pr>g&#x101;zh&#x12B;</pr></trd>
</drv>

<drv mrk="knar.0ilo">
  <kap><tld/>ilo</kap>
  <snc mrk="knar.0ilo.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      Bruilo konsistanta el ligna tabuleto, kiu <tld/>as
      turni&gcirc;ante &ccirc;irka&ubreve; tenilhava akso:
      <ekz>
        li pretigis <tld/>ilon, kiam oni &gcirc;in turnis, tiam sonis &ccirc;iuj valsoj, polkoj kaj
        mazurkoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_l;&c_ja;&c_s;&c_k;&c_o;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="cs">
    <trd>hrka&ccaron;ka</trd>,
    <trd>&rcaron;ehta&ccaron;ka</trd>
  </trdgrp>
  <trd lng="de">Knarre <klr>(Musik)</klr></trd>
  <trd lng="en">clack</trd>
  <trd lng="fr">cr&eacute;celle</trd>
  <trd lng="hu">kerepl&odblac;</trd>
  <trd lng="ja">&#x304C;&#x3089;&#x304C;&#x3089;</trd>
  <trd lng="nl">ratel</trd>
  <trdgrp lng="pl">
    <trd>grzechotka</trd>,
    <trd>terkotka</trd>
  </trdgrp>
  <trd lng="ro">mori&#x219;c&abreve;</trd>
  <trd lng="ru">&c_t;&c_r;&c_je;&c_shch;&c_o;&c_t;&c_k;&c_a;</trd>
  <trd lng="sk">rapk&aacute;&ccaron;</trd>
  <trd lng="uk">&c_t;&c_r;&c_ib;&c_s;&c_k;&c_a;&c_ch;&c_k;&c_a;</trd>
</drv>
</art>
