<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: interd.xml,v 1.19 2023/04/30 13:26:20 revo Exp $">
<kap>
  <rad>interdikt</rad>/o
  <fnt>Z</fnt>
</kap>
<drv mrk="interd.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KAT</uzo>
  <snc>
    <dif>
      Eklezia <ref tip="super" cel="pun.0o">puno</ref>,
      kiu malpermesas al pastro doni la sakramentojn kaj
      fari la meson:
      <ekz>
        &ccirc;ar <nom>Hus</nom> da&ubreve;rigis siajn agadojn kontra&ubreve; simonio, la papo proklamis <tld/>on super
        Prago, dum <nom>Hus</nom> tie &ccirc;eestas
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>
            <vrk><url ref="&Monato;/2000/007031.php">Johano Hus &ndash; &ccirc;u ne plu herezulo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 19</lok>
          </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ib;&c_n;&c_t;&c_e;&c_r;&c_d;&c_y;&c_k;&c_t;</trd>
  <trdgrp lng="cs">
    <trd>interdikt</trd>,
    <trd>z&aacute;kaz</trd>
  </trdgrp>
  <trd lng="de">Interdikt</trd>
  <trd lng="en">interdict <klr tip="amb">(Catholic)</klr></trd>
  <trd lng="fr">interdit<klr tip="ind"> (sentence eccl&eacute;siastique)
  </klr></trd>
  <trdgrp lng="hu">
    <trd>interdiktum</trd>,
    <trd>egyh&aacute;zi <ind>tilalom</ind></trd></trdgrp>
  <trd lng="it"><ind>sospensione</ind> a divinis</trd>
  <trd lng="nl">interdict</trd>
  <trd lng="pl">interdykt</trd>
  <trd lng="pt">interdito<klr tip="amb"> (catolicismo)</klr></trd>
  <trd lng="ru">&c_i;&c_n;&c_t;&c_je;&c_r;&c_d;&c_i;&c_k;&c_t;</trd>
  <trdgrp lng="sk">
    <trd>interdikt</trd>,
    <trd>z&aacute;kaz</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_z;&c_a;&c_b;&#x43E;&c_r;&#x43E;&#x43D;&c_a;</trd>,
    <trd>&c_ib;&#x43D;&c_t;&c_je;&c_r;&c_d;&c_i;&#x43A;&c_t;</trd>
  </trdgrp>
</drv>
</art>
