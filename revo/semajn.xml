  <trd lng="hr">tjedan</trd>
  <trd lng="hu">h&eacute;t<klr tip="ind"> (h&eacute;t nap)</klr></trd>
  <trd lng="hy">&#x577;&#x561;&#x562;&#x561;&#x569;</trd>
  <trdgrp lng="id">
    <trd>minggu</trd>,
    <trd>pekan</trd>
  </trdgrp>
  <trd lng="is">viku</trd>
  <trd lng="it">settimana</trd>
