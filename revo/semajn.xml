<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>semajn</rad>/o
</kap>
<drv mrk="semajn.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KAL</uzo>
  <snc mrk="semajn.0o.noma">
    <dif>
      Periodo de sep tagoj la&ubreve;orde nomataj
      <ref tip="prt" cel="dimanc.0o">diman&ccirc;o</ref>,
      <ref tip="prt" cel="lund.0o">lundo</ref>,
      <ref tip="prt" cel="mard.0o">mardo</ref>,
      <ref tip="prt" cel="merkre.0o">merkredo</ref>,
      <ref tip="prt" cel="jxauxd.0o">&jcirc;a&ubreve;do</ref>,
      <ref tip="prt" cel="vendre.0o">vendredo</ref>,
      <ref tip="prt" cel="sabat.0o">sabato</ref>:
      <ekz>la sepan tagon de la <tld/>o Dio elektis, ke &gcirc;i estu pli sankta, ol la ses unuaj tagoj
      <fnt><bib>F</bib>, <lok>&FE; 12</lok></fnt>.</ekz>
    </dif>
  </snc>
  <snc mrk="semajn.0o.tagsepo">
    <dif>
      Ajna sinsekvo de sep tagoj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_y;&c_d;&c_z;&c_je;&c_n;&c_mol;</trd>
  <trd lng="cs">t&yacute;den</trd>
  <trd lng="de">Woche</trd>
  <trd lng="en">week</trd>
  <trd lng="es">semana</trd>
  <trd lng="fr">semaine</trd>
  <trd lng="he">&shin;&bet;&vav;&ayin;</trd>
  <trd lng="hu">h&eacute;t<klr tip="ind"> (h&eacute;t nap)</klr></trd>
  <trd lng="it">settimana</trd>
  <trd lng="nl">week</trd>
  <trd lng="ru">&c_n;&c_je;&c_d;&c_je;&c_l;&c_ja;</trd>
  <trd lng="sk">t&yacute;&zcaron;de&ncaron;</trd>
  <trd lng="sv">vecka</trd>
</drv>
</art>
