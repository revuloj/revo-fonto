<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: kvaker.xml,v 1.22 2024/02/28 09:23:59 revo Exp $">
<kap>
  <ofc>9</ofc><rad>kvaker</rad>/o <fnt>Z</fnt> <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="kvaker.0o">
  <kap><ofc>9</ofc><tld/>o</kap>
  <snc mrk="kvaker.0o.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Ano de <ref tip="vid" cel="kvaker.0ismo.REL"><tld/>ismo</ref>:
      <ekz>
        mi ricevis ofte vizitojn de anglaj <tld/>oj
        <fnt>
          <bib>Aventuroj</bib>,
          <lok>&ccirc;apitro 43a, p. 100a</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_v;&c_a;&c_k;&c_je;&c_r;</trd>
  <trd lng="ca">qu&agrave;quer</trd>
  <trd lng="de">Qu&auml;ker</trd>
  <trd lng="en">Quaker</trd>
  <trd lng="es">cu&aacute;quero</trd>
  <trd lng="fr">quaker</trd>
  <trd lng="hu">kv&eacute;ker</trd>
  <trd lng="ja">&#x30AF;&#x30A8;&#x30FC;&#x30AB;&#x30FC;&#25945;&#x5F92; 
  <pr>&#x30AF;&#x30A8;&#x30FC;&#x30AB;&#x30FC;&#x304D;&#12423;&#12358;&#12392;</pr></trd>
  <trd lng="nl">Quaker</trd>
  <trd lng="pl">kwakier</trd>
  <trdgrp lng="pt">
    <trd>quacre</trd>,
    <trd>qu&aacute;quer</trd>,
    <trd>qu&aacute;quero</trd>
  </trdgrp>
  <trd lng="ro">quaker</trd>
  <trd lng="ru">&c_k;&c_v;&c_a;&c_k;&c_je;&c_r;</trd>
  <trd lng="uk">&#x43A;&c_v;&c_a;&#x43A;&c_je;&c_r;</trd>
  <trdgrp lng="zh">
    <trd>&#25945;&#x4F1A;&#x6D3E;&#x4FE1;&#x5F92; <pr>ji&#xE0;ohu&#xEC;p&#xE0;ix&#xEC;nt&#xFA;</pr></trd>,
    <trd>&#25945;&#26371;&#x6D3E;&#x4FE1;&#x5F92; <pr>ji&#xE0;ohu&#xEC;p&#xE0;ix&#xEC;nt&#xFA;</pr></trd>,
    <trd>&#x8CB4;&#x683C;&#26371; <pr>gu&#xEC;g&#xE9;hu&#xEC;</pr></trd>,
    <trd>&#x8D35;&#x683C;&#x4F1A; <pr>gu&#xEC;g&#xE9;hu&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kvaker.0ismo">
  <kap><tld/>ismo</kap>
  <snc mrk="kvaker.0ismo.REL">
    <uzo tip="fak">REL</uzo>
    <dif>
      Doktrino kaj praktiko de
      <ref tip="vid" cel="kvaker.0o.REL"><tld/>oj</ref>,
      religia grupo kun kristana bazo
      fondita en 1652 de Georgo Fox,
      kaj predikanta fratecon kaj internacian paci&gcirc;on;
      oficiale <ctl>Religia Societo de Amikoj</ctl>:
      <ekz>
        <tld/>ismo nepre estas praktika <ref tip="lst" cel="religi.0o.REL"
        lst="voko:religioj">religio</ref> kiu tu&scirc;as
        la tutan
        &ccirc;iutagan vivmanieron
        <fnt>
          <aut>Kvakeroj  en  Hastings</aut>,
          <vrk><url ref="http://www.hastings-quakers.org.uk/enkonduko.htm">
          Enkonduko:  Kiuj  Estas  la  Kvakeroj?</url></vrk>,
          <lok>[vidita en 2010]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_v;&c_a;&c_k;&c_je;&c_r;&c_s;&c_t;&c_v;&c_a;</trd>,
    <trd>&c_k;&c_v;&c_a;&c_k;&c_je;&c_r;&c_s;&c_k;&c_a;&c_ja; &c_d;&c_a;&c_k;&c_t;&c_r;&c_y;&c_n;&c_a;</trd>
  </trdgrp>
  <trd lng="cs">kvakerizmus</trd>
  <trd lng="fr">quakerisme</trd>
  <trd lng="ja">&#x30AF;&#x30A8;&#x30FC;&#x30AB;&#x30FC;&#25945;&#x5F92;&#x306E;&#x4FE1;&#26465; 
  <pr>&#x30AF;&#x30A8;&#x30FC;&#x30AB;&#x30FC;&#x304D;&#12423;&#12358;&#12392;&#x306E;&#12375;&#12435;&#12376;&#12423;&#12358;</pr></trd>
  <trd lng="pl">doktryna kwakr&oacute;w <klr>(Religijne Towarzystwo
  Przyjaci&oacute;&lstroke;)</klr></trd>
  <trd lng="ro">Quaker&uuml;</trd>
  <trd lng="sk">kvakerizmus</trd>
  <trd lng="uk">&#x43A;&c_v;&c_a;&#x43A;&c_je;&c_r;&c_s;&c_t;&c_v;&#x43E;</trd>
  <trd lng="zh">&#25945;&#x53CB;&#x6D3E; <pr>Ji&#xE0;oy&#x1D2;up&#xE0;i</pr></trd>
</drv>
</art>
