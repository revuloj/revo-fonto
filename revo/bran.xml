<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: bran.xml,v 1.22 2023/08/31 08:48:53 revo Exp $">
  <kap>
  <ofc>*</ofc>
    <rad>bran</rad>/o
  </kap>
  
<drv mrk="bran.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Gren&scirc;elo apartigita de la faruno per muelado:
      <ekz>
        se vi sidos en <tld/>oj, vin man&gcirc;os la porkoj
        <fnt>
          <bib>PrV</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>[&#x2026;]</klr> estas grava nutra&jcirc;o, uzata &ccirc;efe por bestoj, ri&ccirc;a je albumino kaj amelo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=16562">Brano</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>pano</ind> havas pli da sanigaj efikoj ol blanka pano
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=483022">Branpano</url></lok>
        </fnt>.
        <trd lng="de">Vollkornbrot</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gri.0o">grio</ref>,
      <ref cel="sxrot.0o">&scirc;roto</ref>,
      <ref cel="farun.0o">faruno</ref>,
      <ref cel="flok.0o">floko</ref>
    </refgrp>
    <trd lng="be">&c_v;&c_o;&c_t;&c_r;&c_u;&c_b;'&c_je;</trd>
    <trd lng="br">brenn</trd>
    <trdgrp lng="ca">
      <trd>seg&oacute;</trd>,
      <trd>bren</trd>
    </trdgrp>
    <trd lng="de">Kleie</trd>
    <trd lng="en">bran</trd>
    <trd lng="es">salvado</trd>
    <trdgrp lng="fr">
      <trd>son<klr tip="ind"> (de c&eacute;r&eacute;ale)</klr></trd>,
      <trd>bran</trd>,
      <trd>bren</trd></trdgrp> 
    <trd lng="hu">korpa</trd>
    <trd lng="nl">zemelen</trd>
    <trd lng="pl">otr&eogonek;by<klr> {pl.}</klr></trd>
    <trd lng="pt">farelo</trd>
    <trd lng="ru">&c_o;&c_t;&c_r;&c_u;&c_b;&c_i;</trd>
    <trd lng="sk">otruby</trd>
  </snc>
  <trd lng="cs">otruby</trd>
  <trd lng="he">&#x5DC;&#x5D7;&#x5DD; &#x5E1;&#x5D5;&#x5D1;&#x5D9;&#x5DF;</trd>
  <trdgrp lng="ja">
    <trd>&#x3075;&#x3059;&#x307E;</trd>,
    <trd>&#x306C;&#x304B;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x438;&#x441;&#x456;&#x432;&#x43A;&#x438;</trd>,
    <trd>&#x491;&#x440;&#x438;&#x441;</trd>
  </trdgrp>
</drv>
</art>
