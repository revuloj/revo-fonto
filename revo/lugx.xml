<vortaro>
  <art mrk="$Id: lugx.xml,v 1.4 2023/09/02 05:33:11 afido Exp $">
    <kap>
      <rad>lu&gcirc;</rad>/o
    </kap>

<drv mrk="lugx.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SPO</uzo>
  <snc>
    <dif>
      Malgranda sporta <ref tip="super" cel="vetur.glit0ilo">glitveturilo</ref> por unu a&ubreve; du
      personoj,
      kiun oni stiras ku&scirc;anta dorse i.a. fleksante la glitilojn per la kruro:
      <ekz>
        &scirc;i glisis al la tero, tiu movi&gcirc;ado, simila al deglito sur ia aera <tld/>o, faris
        al &scirc;i la plej grandan plezuron
        <fnt>
          <bib>MkM</bib>
          <lok>21</lok>
        </fnt>;
      </ekz>
      <ekz>
        enhala na&gcirc;ejo kaj anka&ubreve; sub&ccirc;iela banejo, instala&jcirc;oj por <ind><tld/>i</ind>, tenisejoj
        <klr>[&#x2026;]</klr> estas uzeblaj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=178227">Kronach</url></lok>
        </fnt>.
        <trd lng="de">rennrodeln</trd>
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/1/1f/Natural_luge_double.jpg/640px-Natural_luge_double.jpg" prm="CC BY-SA 4.0"><tld/>o por du
        <fnt>
          <aut>Rodelfreak, propra verko</aut>
          <lok><url ref="&WCW;/File:Natural_luge_double.jpg">&WCW;/File:Natural_luge_double.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="be">&c_s;&c_a;&c_n;&c_ib; <klr>(&c_s;&c_p;&c_a;&c_r;&c_t;&c_o;&c_v;&c_y;&c_ja;)</klr></trd>
    <trd lng="de">Rennrodel</trd>
    <trd lng="en">luge</trd>
    <trd lng="fr">luge</trd>
  </snc>
  <trd lng="ja">&#x30EA;&#x30E5;&#x30FC;&#x30B8;&#x30E5;</trd>
</drv>

  </art>

