<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: virulent.xml,v 1.10 2025/04/28 18:38:15 revo Exp $">
<kap>
  <rad>virulent</rad>/a <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="virulent.0a">
  <kap><tld/>a</kap>
  <uzo tip="stl">RAR</uzo>
  <snc mrk="virulent.0a.MED">
    <uzo tip="fak">MED</uzo>
    <uzo tip="klr">(pri malsano, mikrobo)</uzo>
    <dif>
      <ref tip="dif" cel="akut.0a.malsano">Akuta<sncref/></ref>:
      <ekz>
        anta&ubreve;-inokulado de malforte <tld/>aj &gcirc;ermoj
        <fnt>
         <aut>M. Hesse, C. Werner, M. S&uuml;sse</aut>,
         <vrk><url ref="&STEB;kemio/substancoj/substancoj.htm">
         Kelkaj aspektoj de la kemio de naturaj substancoj</url></vrk>,
         <lok>Scienca Revuo, 1992:1 (43), p. 32a-37a</lok>
        </fnt>.
      </ekz>
    </dif>
   <trd lng="en">virulent <klr>(medicine)</klr></trd>
   <trd lng="fr">aigu<klr> (maladie)</klr></trd>
  </snc>
  <snc mrk="virulent.0a.FIG">
    <uzo tip="stl">FIG</uzo>
    <dif>
      Vigla kaj malutila:
      <ekz>
        Usono enfalis en la embuskon de la <tld/>a franca
        kolonidefenda histerio
        <fnt>
          <aut>rh</aut>,
          <vrk><url ref="https://scienca-revuo.info/article/viewFile/1087/1066">
          Dan Diner: la jarcenton kompreni</url></vrk>,
          <lok>Scienca Revuo, 1999:2 (50)</lok>
        </fnt>.
      </ekz>
    </dif>
   <trd lng="en">virulent <klr>(figurative)</klr></trd>
   <trd lng="fr">virulent</trd>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ib;&c_r;&c_u;&c_l;&c_je;&c_n;&c_t;&c_n;&c_y;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_r;&c_o;&c_b;&c_n;&c_y;</trd>,
    <trd>&c_h;&c_v;&c_a;&c_r;&c_o;&c_b;&c_a;&c_t;&c_v;&c_o;&c_r;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="ja">
    <trd>&#30149;&#21407;&#24615;&#12398;  <pr>&#12403;&#12423;&#12358;&#12370;&#12435;&#12379;&#12356;&#12398;</pr></trd>,
    <trd>&#27602;&#24615;&#12398;  <pr>&#12393;&#12367;&#12379;&#12356;&#12398;</pr></trd>
  </trdgrp>
  <trd lng="pl">jadowity</trd>
  <trdgrp lng="uk">
    <trd>&c_h;&c_v;&c_o;&c_r;&c_o;&c_b;&c_o;&c_t;&c_v;&c_o;&c_r;&c_n;&c_i;&c_j;</trd>,
    <trd>&c_v;&c_ib;&c_r;&c_u;&c_l;&c_je;&c_n;&c_t;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

