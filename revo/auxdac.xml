<vortaro>
  <art mrk="$Id: auxdac.xml,v 1.27 2022/03/12 15:33:56 revo Exp $">
<kap>
  <ofc>8</ofc>
  <rad>a&ubreve;dac</rad>/i <fnt><bib>PV</bib></fnt>
</kap>

<drv mrk="auxdac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc mrk="auxdac.0i.maltimi">
    <dif>
      <ref tip="super" cel="risk.0i.fari">Riski</ref> fari ion, 
      malgra&ubreve; la dan&gcirc;eroj a&ubreve; malhelpoj;
      <ref tip="sin" cel="tim.mal0i">maltimi</ref>:
      <ekz>
        la du unuaj, kiuj <tld/>is eksciti la popolon kontra&ubreve; Cezaro, estas kaptitaj
        <fnt>
          <bib>MkM</bib>
        </fnt>;
      </ekz>
      <ekz>
        li <tld/>is la aventuron el&scirc;ipi&gcirc;i
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
          <lok>&Ccirc;apitro 24</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u mi <tld/>u peti iun knabinon al dancado
        <fnt>
          <bib>FSI</bib>
        </fnt>?
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        verti&gcirc;odona centra alta&jcirc;o, kiun tempo ne <tld/>is tu&scirc;i, kronita de pre&gcirc;ejo
        <fnt>
          <bib>Sol</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd><ind>k&uuml;hn</ind> sein</trd>,
      <trd><ind>unerschrocken</ind> sein</trd>,
      <trd><ind>wagemutig</ind> sein</trd>,
      <trd>sich <ind>trauen</ind></trd>
    </trdgrp>
  </snc>
  <snc mrk="auxdac.0i.kontrauxdece">
    <dif>
      <ref tip="super" cel="risk.0i.fari">Riski</ref> fari ion 
      spitante la decon a&ubreve; kutimojn:
      <ekz>
        ili estas nur servistoj, mi tute ne povis kompreni, kiel ili <tld/>is proponi al
        mi la fruktojn de sia mastro
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        mi estis konsternita, kiel li <tld/>as tion aserti, mi ja tamen pli bone scias
        <fnt>
          <bib>VaK</bib>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;iuj surprizi&gcirc;is pri lia senemocia diro tiurilate, sed neniu <tld/>is fari
        demandojn
        <fnt>
          <bib>ChB</bib>
        </fnt>;
      </ekz>
      <ekz>
        kiel diable vi <tld/>as akuzi min pri &scirc;telo
        <fnt>
          <bib>ChV</bib>
        </fnt>?
      </ekz>
      <ekz>
        foje li <tld/>is deklari, ke la kanjono de Kolorado
        estas pli bela ol la valo de la Tajo en Hispanio
        <fnt><aut>Szathmari</aut>: <vrk>La guarbo</vrk></fnt>;
      </ekz>
      <ekz>
        kiu <tld/>as opinii libere pri la manifesto, la
        kooperativo a&ubreve; la grafika aspekto de la revuo,
        tiu punende pekas
        <fnt><aut>Jorge Camacho</aut>:
          <vrk>La liturgio de l' foiro</vrk>.
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>wagen</trd>,
      <trd>riskieren</trd>,
      <trd>sich <ind>erdreisten</ind></trd>,
      <trd>sich <ind>erk&uuml;hnen</ind></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_s;&c_m;&c_je;&c_t;&c_mol;</trd>,
      <trd>&c_p;&c_o;&c_s;&c_m;&c_je;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_a;&c_s;&c_m;&c_je;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_s;&c_m;&c_je;&c_ja;</trd>,
    <trd>&c_o;&c_s;&c_m;&c_je;&c_l;&c_ja;&c_v;&c_a;&c_m; &c_s;&c_je;</trd>
  </trdgrp>
  <trd lng="en">have the audacity (to)</trd>
  <trd lng="fr">oser</trd>
  <trdgrp lng="he">
    <trd>&lamed;&he;&ayin;&zayin;</trd>,
    <trd>&lamed;&nun;&he;&vav;&gimel; &bet;&alef;&vav;&mem;&ftsadi;</trd>
  </trdgrp>
  <trd lng="hu">mer&eacute;szel</trd>
  <trd lng="it">osare</trd>
  <trd lng="nl">wagen</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_je;&c_sh;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_s;&c_m;&c_je;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_s;&c_m;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_z;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>mie&cacute;
    <ind>&sacute;mia&lstroke;o&sacute;&cacute;</ind></trd>,
    <trd>mie&cacute; <ind>czelno&sacute;&cacute;</ind></trd>,
    <trd>o&sacute;miela&cacute; si&eogonek;</trd>
  </trdgrp>
  <trd lng="sk">odv&aacute;&zcaron;i&tcaron; sa <klr>(osmeli&tcaron;
  sa)</klr></trd>
  <trdgrp lng="sv">
    <trd>vara <ind>dj&auml;rv</ind></trd>,
    <trd>vara <ind>v&aring;ghalsig</ind></trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x437;&#x432;&#x430;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x43D;&#x430;&#x432;&#x430;&#x436;&#x443;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x43D;&#x430;&#x441;&#x43C;&#x456;&#x43B;&#x44E;&#x432;&#x430;&#x442;&#x438;&#x441;&#x44F;</trd>,
    <trd>&#x43A;&#x43D;&#x438;&#x436;&#x43D;.&#xA0;&#x434;&#x435;&#x440;&#x437;&#x430;&#x442;&#x438;</trd>
  </trdgrp>
</drv>


<drv mrk="auxdac.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="A"/>oplena, riskemega,
      <ref tip="sin" cel="tim.mal0a">maltima</ref>:
      <ekz>
        <tld/>a pamfleto, revo, paradokso,
        koketeco, entrepreno, mieno, &scirc;telisto;
      </ekz>
      <ekz>
        neniu el la knaboj estis pli <tld/>a kaj bubeca ol &scirc;i
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;dacaj gepatroj <klr>[&#x2026;]</klr> ne hezitis enkonduki <klr>[tiun lingvon]</klr>
        e&ccirc; en la familian vivon
        <fnt>
          <bib>Ret</bib>
        </fnt>;
      </ekz>
      <ekz>
        liaj versoj per <tld/>aj ritmo&scirc;an&gcirc;oj substrekas la disvolvi&gcirc;on de lia inspiro
        <fnt>
          <bib>EeP</bib>
        </fnt>;
      </ekz>
      <ekz>
        sian iom hispanecan ekstera&jcirc;on <klr>[&#x2026;]</klr> li substrekis per agrabla, tamen
        iom <tld/>a vesta&jcirc;o
        <fnt>
          <bib>KKr</bib>
        </fnt>;
      </ekz>
      <ekz>
        stelulino, tutmonde konata pro siaj <tld/>aj dekolta&jcirc;oj
        <fnt>
          <bib>KrB</bib>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a &scirc;telo okazis hiera&ubreve; nokte, unu horon post la fermo de la foiro
        <fnt>
          <bib>KrM</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kuragx.0a">kura&gcirc;a</ref>,
      <ref cel="risk.0ema">riskema</ref>
    </refgrp>
  </snc>
  <rim>
    Pri la sinonimoj vd
    <ref cel="kuragx.0a.RIM">rimarkon &ccirc;e
    &leftquot;kura&gcirc;a&rightquot;</ref>.
  </rim>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>, 
    <trd>&c_s;&c_mol;&c_m;&c_je;&c_l;&c_y;</trd>, 
    <trd>&c_d;&c_z;&c_jo;&c_r;&c_z;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="bg">
    <trd>&c_s;&c_m;&c_je;&c_l;</trd>, 
    <trd>&c_d;&c_malmol;&c_r;&c_z;&c_malmol;&c_k;</trd>, 
    <trd>&c_b;&c_je;&c_z;&c_o;&c_ch;&c_l;&c_i;&c_v;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>dreist</trd>,
    <trd>wagemutig</trd>,
    <trd>waghalsig</trd>,
    <trd>k&uuml;hn</trd>,
    <trd>draufg&auml;ngerisch</trd>
  </trdgrp>
  <trd lng="en">audacious</trd>
  <trd lng="es">audaz</trd>
  <trd lng="fr">audacieux</trd>
  <trdgrp lng="he">
    <trd>&alef;&mem;&yod;&ftsadi;</trd>,
    <trd>&het;&vav;&tsadi;&pe;&fnun;</trd>
  </trdgrp>
  <trd lng="hu">mer&eacute;sz</trd>
  <trd lng="it">audace</trd>
  <trdgrp lng="nl">
    <trd>waaghalsig</trd>, 
    <trd>dapper</trd>, 
    <trd>roekeloos</trd>
  </trdgrp>
  <trd lng="pl">zuchwa&lstroke;y</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_z;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_m;&c_je;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">odv&aacute;&zcaron;ny</trd>
  <trdgrp lng="sv">
    <trd>dj&auml;rv</trd>,
    <trd>v&aring;ghalsig</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x434;&#x432;&#x430;&#x436;&#x43D;&#x438;&#x439;</trd>,
    <trd>&#x437;&#x443;&#x445;&#x432;&#x430;&#x43B;&#x438;&#x439;</trd>,
    <trd>&#x441;&#x43C;&#x456;&#x43B;&#x438;&#x432;&#x438;&#x439;</trd>,
    <trd>&#x432;&#x456;&#x434;&#x447;&#x430;&#x439;&#x434;&#x443;&#x448;&#x43D;&#x438;&#x439;</trd>
  </trdgrp>
</drv>

<drv mrk="auxdac.0o">
  <kap><tld/>o</kap>
  <snc mrk="auxdac.0o.braveco">
    <dif>
      Ago a&ubreve; sinteno de iu, kiu
      <ref cel="auxdac.0i.maltimi"><tld/>as<sncref/></ref>:
      <ekz>
        mi descendis de la tablo kun la <tld/>o de la profetoj, kaj kunplektinte miajn
        brakojn sur mia brusto mi firme okulen rigardis la kirli&gcirc;antan amason de la
        frenezuloj
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_o;&c_t;&c_v;&c_a;&c_g;&c_a;</trd>,
      <trd>&c_s;&c_m;&c_je;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_d;&c_je;&c_r;&c_z;&c_n;&c_o;&c_v;&c_je;&c_n;&c_i;&c_je;</trd>
    </trdgrp>
  </snc>
  <snc mrk="auxdac.0o.aroganteco">
    <dif>
      Ago a&ubreve; sinteno de iu, kiu
      <ref cel="auxdac.0i.kontrauxdece"><tld/>as<sncref/></ref>:
      <ekz>
        mi konsterni&gcirc;is pro la <tld/>o, ke li povas senkonscience kaj cinike kalumnii
        nian socion
        <fnt>
          <bib>VaK</bib>
        </fnt>.
      </ekz>
    </dif>
    <trdgrp lng="ru">
      <trd>&c_d;&c_je;&c_r;&c_z;&c_o;&c_s;&c_t;&c_mol;</trd>,
      <trd>&c_n;&c_a;&c_g;&c_l;&c_o;&c_s;&c_t;&c_mol;</trd>
    </trdgrp>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_v;&c_a;&c_g;&c_a;</trd>
  <trd lng="bg">&c_d;&c_malmol;&c_r;&c_z;&c_o;&c_s;&c_t;</trd>
  <trdgrp lng="de">
    <trd>Wagemut</trd>,
    <trd>Waghalsigkeit</trd>,
    <trd>K&uuml;hnheit</trd>,
    <trd>Draufg&auml;ngertum</trd>
  </trdgrp>
  <trd lng="en">audacity</trd>
  <trd lng="es">audacia</trd>
  <trd lng="fr">audace</trd>
  <trd lng="he">&het;&vav;&#x5BC;&tsadi;&pe;&#x5BC;&#x5B8;&he;</trd>
  <trd lng="hu">mer&eacute;szs&eacute;g</trd>
  <trdgrp lng="sv">
    <trd>dj&auml;rvhet</trd>,
    <trd>v&aring;ghalsighet</trd>
  </trdgrp>
  <trd lng="it">audacia</trd>
  <trd lng="nl">roekeloosheid</trd>
  <trdgrp lng="pl">
    <trd>&sacute;mia&lstroke;o&sacute;&cacute;</trd>,
    <trd>zuchwa&lstroke;o&sacute;&cacute;</trd>,
    <trd>czelno&sacute;&cacute;</trd>
  </trdgrp>
  <trd lng="sk">odvaha</trd>
  <trdgrp lng="uk">
    <trd>&#x432;&#x456;&#x434;&#x432;&#x430;&#x433;&#x430;</trd>,
    <trd>&#x440;&#x456;&#x448;&#x443;&#x447;&#x456;&#x441;&#x442;&#x44C;</trd>,
    <trd>&#x434;&#x435;&#x440;&#x437;&#x430;&#x43D;&#x43D;&#x44F;</trd>
  </trdgrp>
</drv>


<drv mrk="auxdac.mal0a">
  <kap>mal<tld/>a</kap>
  <snc>
    <dif>
      Embarasi&gcirc;ema, &gcirc;eni&gcirc;ema, al kiu mankas memfido, kiu
      anta&ubreve;timas malobei decon a&ubreve; kutimon:
      <ekz>
        li estis tro mal<tld/>a por demandi pri tio;
	  </ekz><ekz>
        mal<tld/>a rigardo, rideto;
      </ekz><ekz>
        &ccirc;u &gcirc;i <klr>(<nom>MOK</nom>)</klr> rajtas enkonduki hierarkiojn en la
        internacian juron, kiam tiu juro mem estas &ccirc;i-rilate tiom
        mal<tld/>a
        <fnt><lok>Le Monde diplomatique en Esperanto</lok></fnt>?
      </ekz>
    </dif>
  </snc>
  <ref tip="sin" cel="timid.0a">timida</ref>
  <refgrp tip="vid">
    <ref cel="hont.0ema">hontema</ref>,
    <ref cel="gxen.sin0a.hontema">sin&gcirc;ena<sncref/></ref>,
    <ref cel="modest.0a.konduto">modesta<sncref/></ref>,
    <ref cel="tim.0ema">timema</ref>
  </refgrp>
  <trdgrp lng="en">
    <trd>timid</trd>,
    <trd>shy</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_r;&c_o;&c_b;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_z;&c_a;&c_s;&c_t;&c_je;&c_n;&c_ch;&c_i;&c_v;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_m;&c_je;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
</drv>
