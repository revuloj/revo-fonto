<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>8</ofc>
  <rad>a&ubreve;dac</rad>/i
</kap>
<drv mrk="auxdac.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      Riski fari ion, malgra&ubreve; la dan&gcirc;eroj a&ubreve; malhelpoj;
      maltimi:
      <ekz>
        li <tld/>is eniri (en) la sie&gcirc;atan urbon.
      </ekz>
    </dif>
    <trdgrp lng="de">
       <trd>k&uuml;hn sein</trd>,
       <trd>unerschrocken sein</trd>,
       <trd>wagemutig sein</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Riski fari ion spitante la deca&jcirc;ojn:
      <ekz>
        &Ccirc;u vi <tld/>os
        respondi al la admono de via patro?
      </ekz>
      <ekz>
        la malkoncentri&gcirc;o de nia organizado ebligis &ccirc;iajn
        <tld/>ojn al niaj kontra&ubreve;uloj.
      </ekz>
    </dif>
    <trdgrp lng="de">
      <trd>wagen</trd>,
      <trd>riskieren</trd>,
      <trd>sich <ind>erdreisten</ind></trd>,
      <trd>sich <ind>erk&uuml;hnen</ind></trd>
    </trdgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>, 
    <trd>&c_a;&c_s;&c_m;&c_je;&c_l;&c_mol;&c_v;&c_a;&c_c;&c_c;&c_a;</trd>
  </trdgrp>  
  <trd lng="en">have the audacity (to)</trd>
  <trd lng="fr">oser</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_je;&c_sh;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_r;&c_je;&c_sh;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
 
<trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
 
<trd>&c_o;&c_s;&c_m;&c_je;&c_l;&c_i;&c_v;&c_a;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_o;&c_s;&c_m;&c_je;&c_l;&c_i;&c_t;&c_mol;&c_s;&c_ja;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_z;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_z;&c_n;&c_u;&c_t;&c_mol;</trd>
  </trdgrp>
  <trd lng="sk">odv&aacute;&zcaron;i&tcaron; sa <klr>(osmeli&tcaron; sa)</klr></trd>
  <trdgrp lng="sv">
    <trd>vara <ind>dj&auml;rv</ind></trd>,
    <trd>vara <ind>v&aring;ghalsig</ind></trd>
  </trdgrp>
</drv>

<drv mrk="auxdac.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Ago a&ubreve; sinteno de iu, kiu <tld/>as.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_v;&c_a;&c_g;&c_a;</trd>
  <trdgrp lng="de">
      <trd>Wagemut</trd>,
      <trd>Waghalsigkeit</trd>,
      <trd>K&uuml;hnheit</trd>,
      <trd>Draufg&auml;ngertum</trd>
  </trdgrp>
  <trd lng="en">audacity</trd>
  <trd lng="es">audacia</trd>
  <trd lng="fr">audace</trd>
  <trdgrp lng="sv">
    <trd>dj&auml;rvhet</trd>,
    <trd>v&aring;ghalsighet</trd>
  </trdgrp>
  <trd lng="ru">&c_o;&c_t;&c_v;&c_a;&c_g;&c_a;</trd>
  <trd lng="sk">odvaha</trd>
</drv>

<drv mrk="auxdac.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      <tld lit="A"/>oplena, riskemega, maltima:
      <ekz>
        <tld/>a pamfleto, revo, paradokso,
        koketeco, entrepreno, mieno, &scirc;telisto.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kuragx.0a">kura&gcirc;a</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_d;&c_v;&c_a;&c_zh;&c_n;&c_y;</trd>, 
    <trd>&c_s;&c_mol;&c_m;&c_je;&c_l;&c_y;</trd>, 
    <trd>&c_d;&c_z;&c_jo;&c_r;&c_z;&c_k;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="de">
       <trd>dreist</trd>,
       <trd>wagemutig</trd>,
       <trd>waghalsig</trd>,
       <trd>k&uuml;hn</trd>,
       <trd>draufg&auml;ngerisch</trd>
  </trdgrp>
  <trd lng="en">audacious</trd>
  <trd lng="es">audaz</trd>
  <trd lng="fr">audacieux</trd>
  <trdgrp lng="ru">
    <trd>&c_o;&c_t;&c_v;&c_a;&c_zh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_d;&c_je;&c_r;&c_z;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_s;&c_m;&c_je;&c_l;&c_y;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">odv&aacute;&zcaron;ny</trd>
  <trdgrp lng="sv">
    <trd>dj&auml;rv</trd>,
    <trd>v&aring;ghalsig</trd>
  </trdgrp>
</drv>
</art>
