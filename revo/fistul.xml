<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: fistul.xml,v 1.23 2025/04/21 22:30:50 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>fistul</rad>/o
<fnt><bib>UV</bib></fnt></kap>
<drv mrk="fistul.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Da&ubreve;ra <ref tip="lst" cel="absces.0o" lst="voko:malsanoj">absceso</ref> kun konstante fluanta
      puskanaleto:
      <ekz>
        fistulo estas <klr>[&#8230;]</klr> pro ia malsana proceso ekestinta tubo- a&ubreve;
        tubaroforma ligo inter du internaj organoj a&ubreve; inter unu interna organo kaj la
        ha&ubreve;ta surfaco, sekve en la medicina terminaro diferenci&gcirc;as &leftquot;interna fistulo&rightquot;
        <klr>[&#8230;]</klr> kaj &leftquot;ekstera fistulo&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=294955">Fistulo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_s;&c_mol;&c_v;&c_ib;&c_sh;&c_ch;</trd>,
    <trd>&c_f;&c_ib;&c_s;&c_t;&c_u;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_f;&c_i;&c_s;&c_t;&c_u;&c_l;&c_a;</trd>
  <trd lng="ca">f&iacute;stula</trd>
  <trdgrp lng="cs">
    <trd>fistula</trd>,
    <trd>p&iacute;&scaron;t&ecaron;l</trd>
  </trdgrp>
  <trd lng="de">Fistel</trd>
  <trd lng="es">f&iacute;stula</trd>
  <trd lng="fr">fistule</trd>
  <trdgrp lng="hu">
    <trd>sipoly</trd>,
    <trd>fisztula</trd>
  </trdgrp>
  <trd lng="ja">&#x763B;&#x5B54;  <pr>&#x308D;&#12358;&#12371;&#12358;</pr></trd>
  <trd lng="la">fistula</trd>
  <trd lng="nl">fistel</trd>
  <trdgrp lng="pl">
    <trd>fistu&lstroke;a</trd>,
    <trd>przetoka</trd>
  </trdgrp>
  <trd lng="pt">f&iacute;stula</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_v;&c_i;&c_shch;</trd>,
    <trd>&c_f;&c_i;&c_s;&c_t;&c_u;&c_l;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">fistula</trd>
  <trdgrp lng="uk">
    <trd>&c_s;&c_v;&c_i;&c_shch;</trd>,
    <trd>&#x43D;&#x43E;&c_r;&c_i;&c_c;&#x44F;</trd>,
    <trd>&c_f;&c_ib;&c_s;&c_t;&c_u;&#x43B;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x763A; <pr>l&#xF2;u</pr></trd>,
    <trd>&#30232; <pr>l&#xF2;u</pr></trd>
  </trdgrp>
</drv>
</art>
