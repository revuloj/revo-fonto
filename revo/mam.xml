<drv mrk="mam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mam.0o.ina">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Elstara parto de la brusto &ccirc;e virinoj a&ubreve;
      de la ventro &ccirc;e bestinoj, kiu entenas la laktajn glandojn.
    </dif>
    <trdgrp lng="en">
      <trd>breast<klr tip="ind"> (of a woman)</klr></trd>,
      <trd>boob</trd>,
      <trd>tit</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>mama</trd>,
      <trd>seno</trd>
    </trdgrp>
    <trd lng="fa">&f_p;&a_s;&a_t;&a_A;&a_n;</trd>
    <trdgrp lng="hu">
      <trd>mell</trd>,
      <trd>eml&odblac;</trd>
      <trd>csecs</trd>
    </trdgrp>
    <trd lng="id">payudara</trd>
    <trd lng="iu">&#x1403;&#x1555;&#x140A;&#x158F;&#x1585;</trd>
    <trdgrp lng="it">
      <trd>mammella</trd>,
      <trd>seno</trd>,
      <trd>poppa</trd>,
      <trd>tetta</trd>
    </trdgrp>
    <trd lng="pl">pier&sacute;</trd>
    <trdgrp lng="pt">
      <trd>mama</trd>,
      <trd>peito</trd>,
      <trd>teta</trd>,
      <trd>&uacute;bere</trd>,
      <trd>ubre</trd>
    </trdgrp>
    <trd lng="tr">meme</trd>
  </snc>
  <snc mrk="mam.0o.vira">
    <dif>
      Responda parto de la brusto &ccirc;e viro.
    </dif>
    <trdgrp lng="en">
      <trd>chest</trd>
      <trd>breast<klr tip="ind"> (chest)</klr></trd>
    </trdgrp>
    <trd lng="fa">&a_s;&f_y;&a_n;&a_h;</trd>
    <trd lng="hu">mell</trd>
    <trd lng="it">petto</trd>
    <trd lng="pl">pier&sacute;</trd>
  </snc>
  <trdgrp lng="ja">
    <trd>&#x4E73;&#x623F;  <pr>&#x3061;&#x3076;&#x3055;</pr></trd>,
    <trd>&#x3061;&#x3076;&#x3055;</trd>
  </trdgrp>
  <trd lng="lat">mamma, ae</trd>
  <trdgrp lng="be">
    <trd>&c_g;&c_r;&c_u;&c_d;&c_z;&c_ib;</trd>,
    <trd>&c_v;&c_y;&c_m;&c_ja;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_d;&c_n;&c_a;&c_ja; &c_z;&c_a;&c_l;&c_o;&c_z;&c_a;</trd>,
    <trd>&c_c;&c_y;&c_c;&c_k;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_d;&c_z;&c_mol;</trd>
  </trdgrp>
  <trd lng="bg">&c_g;&c_malmol;&c_r;&c_d;&c_a;</trd>
  <trd lng="br">bronn</trd>
  <trdgrp lng="ca">
    <trd>pit</trd>,
    <trd>mamella</trd>
  </trdgrp>
  <trd lng="cs">prs</trd>
  <trd lng="de">Brust</trd>
  <trdgrp lng="fr">
    <trd>mamelle</trd>,
    <trd>sein<klr> (mamelle)</klr></trd>
  </trdgrp>
  <trd lng="he">&shin;&dalet;</trd>
  <trd lng="io">mamo</trd>
  <trdgrp lng="nl">
    <trd>borst<klr tip="ind"> (van vrouw)</klr></trd>,
    <trd>uier</trd>,
    <trd>tiet</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pier&sacute;</trd>,
    <trd>biust</trd>,
    <trd>sutek</trd>,
    <trd>wymi&eogonek;</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>piept</trd>,
    <trd>s&acirc;n</trd>,
    <trd>sf&acirc;rc</trd>,
    <trd>uger</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_d;&c_mol;
    <klr>(&c_zh;&c_je;&c_n;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_a;&c_ja;
    &c_zh;&c_je;&c_l;&c_je;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">prsn&iacute;k</trd>
  <trd lng="sv">br&ouml;st</trd>
  <trdgrp lng="uk">
    <trd>&c_zh;&c_ib;&#x43D;&#x43E;&c_ch;&c_ib; &c_g;&c_r;&c_u;&c_d;&c_i;</trd>,
    <trd>&#x43F;&c_je;&c_r;&c_s;&c_a;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_d;&#x44C;</trd>,
    <trd>&c_v;&c_i;&#x43C;&#x2019;&#x44F;</trd>,
    <trd>&c_g;&c_r;&c_u;&c_d;&#x43D;&c_a; &c_z;&c_a;&#x43B;&#x43E;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="vo">bl&ouml;t</trd>
  <trdgrp lng="zh">
    <trd>&#x80F8;&#x90E8; <pr>xi&#x14D;ngb&#xF9;</pr></trd>,
    <trd>&#x6000; <pr>hu&#xE1;i</pr></trd>,
    <trd>&#x61F7; <pr>hu&#xE1;i</pr></trd>,
    <trd>&#x5976;&#x5B50; <pr>n&#x1CE;izi</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mam.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="mam.0eto.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Malgranda mamo.
    </dif>
  </snc>
  <trd lng="be">&c_c;&c_y;&c_c;&c_a;&c_ch;&c_k;&c_a;</trd>
  <trd lng="io">mameto</trd>
  <trdgrp lng="nl">
    <trd>borstje</trd>,
    <trd>uiertje</trd>,
    <trd>tietje</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>brodawka</trd>,
    <trd>piersi&aogonek;tko</trd>,
    <trd>cycek</trd>
  </trdgrp>
<trdgrp lng="ro">
    <trd>&#x21B;&acirc;&#x21B;&abreve;</trd>,
    <trd>mamelon</trd>,
    <trd>g&acirc;lm&abreve;</trd>
  </trdgrp>
  <trd lng="vo">bl&ouml;til</trd>
</drv>

<drv mrk="mam.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="mam.0ingo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Parto de
      <ref tip="malprt" cel="zon.mam0o.TEKS">mamzono</ref>,
      kiu kovras unu mamon:
      <ekz>
        bikino havas mamzonon kun &scirc;elketoj
        kaj rondaj <tld/>ingoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="&Viki;/Bikino">
          bikino</url>, 2013-10-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_a;&c_sh;&c_k;&c_a; &c_b;&c_ju;&c_s;&c_t;&c_g;&c_a;&c_l;&c_mol;&c_t;&c_a;&c_r;&c_a;</trd>
  <trd lng="bg">&c_ch;&c_a;&c_sh;&c_k;&c_a; &c_n;&c_a;
  &c_s;&c_u;&c_t;&c_i;&c_je;&c_n;</trd>
  <trd lng="cs">podprsenka</trd>
  <trd lng="fr">bonnet de soutien-gorge</trd>
  <trd lng="pl">biustonosz</trd>
  <trdgrp lng="ro">
    <trd>brasier&abreve;</trd>,
    <trd>sutien</trd>
  </trdgrp>
  <trd lng="sk">podprsenka</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&c_ib;&c_f;</trd>,
    <trd>&c_b;&#x44E;&c_s;&c_t;&c_g;&c_a;&#x43B;&#x44C;&c_t;&c_je;&c_r;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x6587;&#x80F8; <pr>w&#xE9;nxi&#x14D;ng</pr></trd>,
    <trd>&#x80F8;&#x7F69; <pr>xi&#x14D;ngzh&#xE0;o</pr></trd>
  </trdgrp>
</drv>

