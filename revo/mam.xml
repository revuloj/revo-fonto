<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>mam</rad>/o
</kap>
<drv mrk="mam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Elstara parto de la brusto &ccirc;e virinoj a&ubreve;
      de la ventro &ccirc;e bestinoj, kiu entenas la laktajn glandojn.
    </dif>
    <trdgrp lng="fr"><trd>sein</trd>, <trd>mamelle</trd></trdgrp>
    <trd lng="tr">meme</trd>
  </snc>
  <snc>
    <dif>
      Responda parto de la brusto &ccirc;e viro.
    </dif>
  </snc>
  <trd lng="de">Brust</trd>
  <trd lng="de">borst</trd>
  <trd lng="lat">mamma, ae</trd>
  <trdgrp lng="nl">
    <trd>borst<klr tip="ind"> (v.vrouw)</klr></trd>,
    <trd>uier</trd>
  </trdgrp>
<trd lng="sv">br&ouml;st</trd>
</drv>
<drv mrk="mam.0eto0pinto">
  <kap><tld/>eto, <tld/>pinto</kap>
  <snc>
    <dif>
      Pinto de la <tld/>oj, el kiu eli&gcirc;as la lakto.
    </dif>
  <ref tip="sin" cel="cic.0o">cico</ref>
  </snc>
  <trd lng="de">Brustwarze</trd>
  <trd lng="fr">mamelon</trd>
  <trd lng="lat">papilla, ae</trd>
  <trd lng="nl">tepel</trd>
  <trd lng="tr">meme ba&scedil;&inodot;</trd>
  <trd lng="sv">br&ouml;stv&aring;rta</trd>
</drv>
<drv mrk="mam.0uloj">
  <kap><tld/>uloj, <tld/>bestoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Klaso de <ref tip="super" cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Mammalia</trd>), kies idoj estas nutrataj per
      su&ccirc;ado de patrina lakto.
    </dif>
  </snc>
  <trd lng="de">S&auml;ugetiere</trd>
  <trd lng="nl">zoogdieren</trd>
  <trd lng="tr">memeliler</trd>
</drv>
<drv mrk="mam.0nutri">
  <kap><tld/>nutri</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Nutri per <tld/>o.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>s&auml;ugen</trd>, <trd>stillen</trd>
  </trdgrp>
  <trd lng="nl">zogen</trd>
  <trd lng="tr">emzirmek</trd>
</drv>
<drv mrk="mam.0sucxi">
  <kap><tld/>su&ccirc;i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Sin nutri su&ccirc;ante <tld/>on.
    </dif>
  </snc>
  <trd lng="fr">t&eacute;ter</trd>
  <trdgrp lng="lat"><trd>lacteo, ere</trd>, <trd>sugo, ere</trd></trdgrp>
  <trd lng="nl">zuigen<klr tip="ind"> (a.d.borst)</klr></trd>
</drv>
<drv mrk="mam.de0igi">
  <kap>de<tld/>igi</kap>
  <snc>
    <dif>
      Senigi idon je <tld/>nutrado por alkutimigi &gcirc;in al malflua
      nutra&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">abstillen</trd>
  <trd lng="fr">sevrer</trd>
  <trdgrp lng="lat"><trd>a matre <ind>depello, ere</ind></trd>, <trd>a mamma <ind>disiungo, ere</ind></trd>, <trd>ablacto, are</trd></trdgrp>
<trd lng="nl">spenen</trd>
</drv>
</art>
