<drv mrk="mam.0uloj">
  <kap><tld/>uloj</kap>
  <snc mrk="mam.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Animaloj kies idoj estas nutrataj per
      su&ccirc;ado de patrina lakto;
      <ref tip="lst" cel="klas.0o.BIO" lst="voko:zoologiaj_klasoj">klaso</ref> de
      <ref tip="malprt" cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Mammalia</trd>);
      <ref tip="sin" cel="best.mam0oj.ZOO"><tld/>bestoj</ref>:
      <ekz>
        la granda pando <klr>...</klr> estas &ccirc;arma
        <tld/>ulo vivanta libere nur en &Ccirc;inio
        <fnt>
          <aut>Peng Jianjun</aut>:
          <vrk>Kloni grandan pandon</vrk>,
          <bib>Monato</bib>,
          <lok>2001:4, p. 25a</lok></fnt>.
      </ekz>
    </dif>
    <lstref lst="voko:mamuloj"><tld/>uloj</lstref>
  </snc>
  <trd lng="be">&c_s;&c_y;&c_s;&c_u;&c_n;&c_y;</trd>
  <trd lng="bg">&c_b;&c_o;&c_z;&c_a;&c_j;&c_n;&c_i;&c_c;&c_i;</trd>
  <trd lng="br">bronneged</trd>
  <trd lng="ca">mam&iacute;fers</trd>
  <trd lng="cs">savce</trd>
  <trd lng="de">S&auml;ugetiere</trd>
  <trd lng="en">mammals</trd>
  <trd lng="es">mam&iacute;feros</trd>
  <trd lng="fa">&f_p;&a_s;&a_t;&a_A;&a_n;&a_d;&a_A;&a_r;&a_A;&a_n;</trd>
  <trd lng="fr">mammif&egrave;res</trd>
  <trd lng="fy">s&ucirc;chdieren</trd>
  <trd lng="io">mamiferi</trd>
  <trd lng="he">&yod;&vav;&nun;&qof;&yod;&fmem;</trd>
  <trd lng="hu">eml&odblac;s&ouml;k</trd>
  <trd lng="id">mamalia</trd>
  <trd lng="it">mammiferi</trd>
  <trd lng="ja">&#x54FA;&#x4E73;&#x985E;  <pr>&#x307B;&#x306B;&#x3085;&#x3046;&#x308B;&#x3044;</pr></trd>
  <trd lng="nl">zoogdieren</trd>
  <trd lng="pl">ssaki</trd>
  <trd lng="pt">mam&iacute;feros</trd>
  <trd lng="ro">mamifere</trd>
  <trd lng="ru">&c_m;&c_l;&c_je;&c_k;&c_o;&c_p;&c_i;&c_t;&c_a;&c_ju;&c_shch;&c_i;&c_je;</trd>
  <trd lng="sk">cicavce</trd>
  <trd lng="tr">memeliler</trd>
  <trd lng="uk">&c_s;&c_s;&c_a;&c_v;&c_c;&c_ib;</trd>
  <trdgrp lng="zh">
    <trd>&#x54FA;&#x4E73;&#x7C7B;&#x52A8;&#x7269; <pr>b&#xFA;r&#x1D4;l&#xE8;id&#xF2;ngwu</pr></trd>,
    <trd>&#x54FA;&#x4E73;&#x985E;&#x52D5;&#x7269; <pr>b&#xFA;r&#x1D4;l&#xE8;id&#xF2;ngwu</pr></trd>,
    <trd>&#x54FA;&#x4E73;&#x7C7B;&#x52A8;&#x7269; <pr>b&#x1D4;r&#x1D4;l&#xE8;id&#xF2;ngw&#xF9;</pr></trd>,
    <trd>&#x54FA;&#x4E73;&#x985E;&#x52D5;&#x7269; <pr>b&#x1D4;r&#x1D4;l&#xE8;id&#xF2;ngw&#xF9;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mam.de0igi">
  <kap>de<tld/>igi</kap>
  <snc mrk="mam.de0igi.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Senigi idon je <tld/>nutrado
      por alkutimigi &gcirc;in al malflua
      nutra&jcirc;o.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_d;&c_v;&c_u;&c_ch;&c_v;&c_a;&c_c;&c_mol; &c_a;&c_d; <ind>&c_g;&c_r;&c_u;&c_d;&c_z;&c_je;&c_j;</ind></trd>
  <trd lng="bg">&c_o;&c_t;&c_b;&c_i;&c_v;&c_a;&c_m; (&c_o;&c_t;
  &c_k;&c_malmol;&c_r;&c_m;&c_je;&c_n;&c_je;)</trd>
  <trd lng="ja">&#x96E2;&#x4E73;&#x3055;&#x305B;&#x308B; 
  <pr>&#x308A;&#x306B;&#x3085;&#x3046;&#x3055;&#x305B;&#x308B;</pr></trd>
  <trdgrp lng="lat">
    <trd>a matre <ind>depello, ere</ind></trd>,
    <trd>a mamma <ind>disiungo, ere</ind></trd>,
    <trd>ablacto, are</trd>
  </trdgrp>
  <trd lng="br">dizona&ntilde;</trd>
  <trd lng="ca">deslletar</trd>
  <trd lng="cs">odstavit kojence</trd>
  <trd lng="de">abstillen</trd>
  <trdgrp lng="en">
    <trd>wean</trd>,
    <trd>ablactate</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_z; <ind>&a_sh;&f_y;&a_r;</ind>
  &f_g;&a_r;&a_f;&a_t;&a_n;</trd>
  <trd lng="fr">sevrer</trd>
  <trd lng="he">&lamed;<ind>&#x2137;&mem;&vav;&lamed;</ind>
  <klr>(&tav;&yod;&nun;&vav;&qof;
  &mem;&yod;&nun;&yod;&qof;&he;)</klr></trd>
  <trd lng="hu">elv&aacute;laszt<klr tip="amb">
  (csecsem&odblac;t)</klr></trd>
  <trd lng="id"><baz>sapih</baz>menyapih</trd>
  <trd lng="it">svezzare</trd>
  <trd lng="nl">spenen</trd>
  <trd lng="pl">odstawi&cacute; od <ind>piersi</ind></trd>
  <trd lng="pt">desmamar</trd>
  <trd lng="ro">&icirc;n&#x21B;&abreve;rca&#x21B;i din <ind>s&acirc;n</ind></trd>
  <trd lng="ru">&c_o;&c_t;&c_n;&c_ja;&c_t;&c_mol; &c_o;&c_t;
  &c_g;&c_r;&c_u;&c_d;&c_i;</trd>
  <trd lng="sk">odstavi&tcaron; doj&ccaron;a</trd>
  <trd lng="uk">&c_v;&c_ib;&c_d;&#x43B;&c_u;&c_ch;&c_a;&c_t;&c_i; &c_v;&c_ib;&c_d; &c_g;&c_r;&c_u;&c_d;&c_je;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#x65AD;&#x5976; <pr>du&#xE0;nn&#x1CE;i</pr></trd>,
    <trd>&#x65B7;&#x5976; <pr>du&#xE0;nn&#x1CE;i</pr></trd>
  </trdgrp>
</drv>

<drv mrk="mam.pra0ulo">
  <kap>pra<tld/>ulo</kap>
