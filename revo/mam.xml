<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: mam.xml,v 1.57 2020/10/25 18:10:50 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>mam</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="mam.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="mam.0o.ina">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Elstara parto de la brusto &ccirc;e virinoj a&ubreve;
      de la ventro &ccirc;e bestinoj, kiu entenas la laktajn glandojn.
    </dif>
    <trdgrp lng="en">
      <trd>breast<klr tip="ind"> (of a woman)</klr></trd>,
      <trd>boob</trd>,
      <trd>tit</trd>
    </trdgrp>
    <trdgrp lng="es">
      <trd>mama</trd>,
      <trd>seno</trd>
    </trdgrp>
    <trd lng="fa">&f_p;&a_s;&a_t;&a_A;&a_n;</trd>
    <trdgrp lng="hu">
      <trd>mell</trd>,
      <trd>eml&odblac;</trd>
      <trd>csecs</trd>
    </trdgrp>
    <trd lng="id">payudara</trd>
    <trd lng="iu">&#5123;&#5461;&#5130;&#5519;&#5509;</trd>
    <trdgrp lng="it">
      <trd>mammella</trd>,
      <trd>seno</trd>,
      <trd>poppa</trd>,
      <trd>tetta</trd>
    </trdgrp>
    <trd lng="pl">pier&sacute;</trd>
    <trdgrp lng="pt">
      <trd>mama</trd>,
      <trd>peito</trd>,
      <trd>teta</trd>,
      <trd>&uacute;bere</trd>,
      <trd>ubre</trd>
    </trdgrp>
    <trd lng="tr">meme</trd>
  </snc>
  <snc mrk="mam.0o.vira">
    <dif>
      Responda parto de la brusto &ccirc;e viro.
    </dif>
    <trdgrp lng="en">
      <trd>chest</trd>
      <trd>breast<klr tip="ind"> (chest)</klr></trd>
    </trdgrp>
    <trd lng="fa">&a_s;&f_y;&a_n;&a_h;</trd>
    <trd lng="hu">mell</trd>
    <trd lng="it">petto</trd>
    <trd lng="pl">pier&sacute;</trd>
  </snc>
  <trd lng="lat">mamma, ae</trd>
  <trd lng="bg">&c_g;&c_malmol;&c_r;&c_d;&c_a;</trd>
  <trd lng="br">bronn</trd>
  <trdgrp lng="ca">
    <trd>pit</trd>,
    <trd>mamella</trd>
  </trdgrp>
  <trd lng="cs">prs</trd>
  <trd lng="de">Brust</trd>
  <trdgrp lng="fr">
    <trd>mamelle</trd>,
    <trd>sein<klr> (mamelle)</klr></trd>
  </trdgrp>
  <trd lng="he">&shin;&dalet;</trd>
  <trd lng="io">mamo</trd>
  <trdgrp lng="nl">
    <trd>borst<klr tip="ind"> (van vrouw)</klr></trd>,
    <trd>uier</trd>,
    <trd>tiet</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>pier&sacute;</trd>,
    <trd>biust</trd>,
    <trd>sutek</trd>,
    <trd>wymi&eogonek;</trd>
  </trdgrp>   
  <trdgrp lng="ro">
    <trd>piept</trd>,
    <trd>s&acirc;n</trd>,
    <trd>sf&acirc;rc</trd>,
    <trd>uger</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_g;&c_r;&c_u;&c_d;&c_mol;
    <klr>(&c_zh;&c_je;&c_n;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>,
    <trd>&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_a;&c_ja;
    &c_zh;&c_je;&c_l;&c_je;&c_z;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">prsn&iacute;k</trd>
  <trd lng="sv">br&ouml;st</trd>
  <trd lng="vo">bl&ouml;t</trd>
</drv>

<drv mrk="mam.0eto">
  <kap><tld/>eto</kap>
  <snc mrk="mam.0eto.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Malgranda mamo.
    </dif>
  </snc>
  <trd lng="io">mameto</trd>
  <trdgrp lng="nl">
    <trd>borstje</trd>,
    <trd>uiertje</trd>,
    <trd>tietje</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>brodawka</trd>,
    <trd>piersi&aogonek;tko</trd>,
    <trd>cycek</trd>
  </trdgrp>   
<trdgrp lng="ro">
    <trd>&#539;&acirc;&#539;&abreve;</trd>,
    <trd>mamelon</trd>,
    <trd>g&acirc;lm&abreve;</trd>
  </trdgrp>
  <trd lng="vo">bl&ouml;til</trd>
</drv>

<drv mrk="mam.0ingo">
  <kap><tld/>ingo</kap>
  <snc mrk="mam.0ingo.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      Parto de
      <ref tip="malprt" cel="zon.mam0o.TEKS">mamzono</ref>,
      kiu kovras unu mamon:
      <ekz>
        bikino havas mamzonon kun &scirc;elketoj
        kaj rondaj <tld/>ingoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url ref="http://eo.wikipedia.org/wiki/Bikino">
          bikino</url>, 2013-10-29</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="bg">&c_ch;&c_a;&c_sh;&c_k;&c_a; &c_n;&c_a;
  &c_s;&c_u;&c_t;&c_i;&c_je;&c_n;</trd>
  <trd lng="cs">podprsenka</trd>
  <trd lng="fr">bonnet de soutien-gorge</trd>
  <trd lng="pl">biustonosz</trd>
  <trdgrp lng="ro">
    <trd>brasier&abreve;</trd>,
    <trd>sutien</trd>
  </trdgrp>
  <trd lng="sk">podprsenka</trd>
</drv>

<drv mrk="mam.0ulo">
  <kap><tld/>ulo</kap>
  <snc mrk="mam.0ulo.ZOO">
    <dif>
      Iu el <ref tip="malprt" cel="mam.0uloj"><tld/>uloj</ref>.
    </dif>
  </snc>
  <trd lng="bg">&c_b;&c_o;&c_z;&c_a;&c_j;&c_n;&c_i;&c_k;</trd>
  <trd lng="cs">savec</trd>
  <trd lng="en">mammal</trd>
  <trd lng="es">mam&iacute;fero</trd>
  <trd lng="fi">nis&auml;k&auml;s</trd>
  <trd lng="fy">s&ucirc;chdier</trd>
  <trd lng="io">mamifero</trd>
  <trd lng="nl">zoogdier</trd>
  <trd lng="pl">ssak</trd>
  <trd lng="ro">mamifer</trd>
  <trd lng="sk">cicavec</trd>
</drv>

<drv mrk="mam.0uloj">
  <kap><tld/>uloj</kap>
  <snc mrk="mam.0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Animaloj kies idoj estas nutrataj per
      su&ccirc;ado de patrina lakto;
      <ref tip="lst" cel="klas.0o.BIO"
      lst="voko:zoologiaj_klasoj">klaso</ref> de
      <ref tip="malprt" cel="verteb.0uloj">vertebruloj</ref>
      (<trd lng="la">Mammalia</trd>);
      <ref tip="sin" cel="best.mam0oj.ZOO"><tld/>bestoj</ref>:
      <ekz>
        la granda pando <klr>...</klr> estas &ccirc;arma
        <tld/>ulo vivanta libere nur en &Ccirc;inio
        <fnt>
          <aut>Peng Jianjun</aut>:
          <vrk>Kloni grandan pandon</vrk>,
          <bib>Monato</bib>,
          <lok>2001:4, p. 25a</lok></fnt>.
      </ekz>
    </dif>
    <lstref lst="voko:mamuloj"><tld/>uloj</lstref>
  </snc>
  <trd lng="bg">&c_b;&c_o;&c_z;&c_a;&c_j;&c_n;&c_i;&c_c;&c_i;</trd>
  <trd lng="br">bronneged</trd>
  <trd lng="ca">mam&iacute;fers</trd>
  <trd lng="cs">savce</trd>
  <trd lng="de">S&auml;ugetiere</trd>
  <trd lng="en">mammals</trd>
  <trd lng="es">mam&iacute;feros</trd>
  <trd lng="fa">&f_p;&a_s;&a_t;&a_A;&a_n;&a_d;&a_A;&a_r;&a_A;&a_n;</trd>
  <trd lng="fr">mammif&egrave;res</trd>
  <trd lng="fy">s&ucirc;chdieren</trd>
  <trd lng="io">mamiferi</trd>
  <trd lng="he">&yod;&vav;&nun;&qof;&yod;&fmem;</trd>
  <trd lng="hu">eml&odblac;s&ouml;k</trd>
  <trd lng="id">mamalia</trd>
  <trd lng="it">mammiferi</trd>
  <trd lng="nl">zoogdieren</trd>
  <trd lng="pl">ssaki</trd>
  <trd lng="pt">mam&iacute;feros</trd>
  <trd lng="ro">mamifere</trd>
  <trd lng="ru">&c_m;&c_l;&c_je;&c_k;&c_o;&c_p;&c_i;&c_t;&c_a;&c_ju;&c_shch;&c_i;&c_je;</trd>
  <trd lng="sk">cicavce</trd>
  <trd lng="tr">memeliler</trd>
</drv>

<drv mrk="mam.de0igi">
  <kap>de<tld/>igi</kap>
  <snc mrk="mam.de0igi.PED">
    <uzo tip="fak">PED</uzo>
    <dif>
      Senigi idon je <tld/>nutrado
      por alkutimigi &gcirc;in al malflua
      nutra&jcirc;o.
    </dif>
  </snc>
  <trd lng="bg">&c_o;&c_t;&c_b;&c_i;&c_v;&c_a;&c_m; (&c_o;&c_t;
  &c_k;&c_malmol;&c_r;&c_m;&c_je;&c_n;&c_je;)</trd>
  <trdgrp lng="lat">
    <trd>a matre <ind>depello, ere</ind></trd>,
    <trd>a mamma <ind>disiungo, ere</ind></trd>,
    <trd>ablacto, are</trd>
  </trdgrp>
  <trd lng="br">dizona&ntilde;</trd>
  <trd lng="ca">deslletar</trd>
  <trd lng="cs">odstavit kojence</trd>
  <trd lng="de">abstillen</trd>
  <trdgrp lng="en">
    <trd>wean</trd>,
    <trd>ablactate</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_z; <ind>&a_sh;&f_y;&a_r;</ind>
  &f_g;&a_r;&a_f;&a_t;&a_n;</trd>
  <trd lng="fr">sevrer</trd>
  <trd lng="he">&lamed;<ind>&#8503;&mem;&vav;&lamed;</ind>
  <klr>(&tav;&yod;&nun;&vav;&qof;
  &mem;&yod;&nun;&yod;&qof;&he;)</klr></trd>
  <trd lng="hu">elv&aacute;laszt<klr tip="amb">
  (csecsem&odblac;t)</klr></trd>
  <trd lng="id"><baz>sapih</baz>menyapih</trd>
  <trd lng="it">svezzare</trd>
  <trd lng="nl">spenen</trd>
  <trd lng="pl">odstawi&cacute; od <ind>piersi</ind></trd>
  <trd lng="pt">desmamar</trd>
  <trd lng="ro">&icirc;n&#539;&abreve;rca&#539;i din <ind>s&acirc;n</ind></trd>
  <trd lng="ru">&c_o;&c_t;&c_n;&c_ja;&c_t;&c_mol; &c_o;&c_t;
  &c_g;&c_r;&c_u;&c_d;&c_i;</trd>
  <trd lng="sk">odstavi&tcaron; doj&ccaron;a</trd>
</drv>

<drv mrk="mam.pra0ulo">
  <kap>pra<tld/>ulo</kap>
  <snc>
    <dif>
      Iu el <ref tip="malprt" cel="mam.pra0uloj">pra<tld/>uloj</ref>.
    </dif>
  </snc>
  <trd lng="pl">prassak</trd>
  <trd lng="ro">monotrem</trd>
</drv>

<drv mrk="mam.pra0uloj">
  <kap>pra<tld/>uloj</kap>
  <snc mrk="mam.pra0uloj.ZOO">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Subklaso de <ref tip="malprt" cel="mam.0uloj"><tld/>uloj</ref>
      (<trd lng="la">Protheria</trd>) kun la sola ordo unutruuloj
      (<trd lng="la">Monotremata</trd>), kies anoj demetas ovojn.
    </dif>
    <refgrp tip="prt">
      <ref cel="erinac.posx0o">po&scirc;erinaco</ref>,
      <ref cel="ornito1.0o">ornitorinko</ref>
    </refgrp>
  </snc>
  <trd lng="de">Urs&auml;uger</trd>
  <trd lng="pl">prassaki</trd>
  <trd lng="ro">monotreme</trd>
</drv>

<drv mrk="mam.nud0a">
  <kap>nud<tld/>a</kap>
  <snc>
    <dif>
      Havanta nudajn <tld/>ojn:
      <ekz>
        nud<tld/>a feministo estis arestita ekster Vatikano
        <fnt>
          <aut>Objectivesea</aut>
          <vrk><url
          ref="https://tatoeba.org/epo/sentences/show/5401757">
          Tatoeba, frazo n-ro 5401757</url></vrk>
          <lok>2016-09-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="da">topl&oslash;s</trd>
  <trdgrp lng="en">
    <trd>bare-breasted</trd>,
    <trd>topless</trd>
  </trdgrp>
  <trd lng="fr">topless</trd>
  <trd lng="nl">topless</trd>
  <trd lng="pl">topless</trd>
  <trdgrp lng="ro">
    <trd>topless</trd>,
    <trd>f&abreve;r&abreve; top</trd>
  </trdgrp>
</drv>

</art>
