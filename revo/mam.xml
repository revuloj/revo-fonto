<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <ofc>*</ofc>
      <rad>mam</rad>/o
    </kap>

    <drv mrk="mam.0o">
      <kap><ofc>*</ofc><tld/>o</kap>
      <uzo tip="fak">ANA</uzo>
      <snc mrk="mam.0o.ina">
        <dif>
          Elstara parto de la brusto &ccirc;e virinoj a&ubreve;
          de la ventro &ccirc;e bestinoj, kiu entenas la laktajn glandojn.
        </dif>
        <trdgrp lng="fr">
          <trd>mamelle</trd>, <trd>sein</trd>
        </trdgrp>
        <trdgrp lng="hu">
          <trd>mell</trd>,
          <trd>eml&odblac;</trd>
          <trd>csecs</trd>
        </trdgrp>
        <trdgrp lng="pt">
          <trd>mama</trd>,
          <trd>peito</trd>,
          <trd>teta</trd>,        
          <trd>&uacute;bere</trd>,
          <trd>ubre</trd>    
        </trdgrp>
        <trd lng="tr">meme</trd>
      </snc>
      <snc mrk="mam.0o.vira">
        <dif>
          Responda parto de la brusto &ccirc;e viro.
        </dif>
        <trdgrp lng="en">
          <trd>chest</trd>,
          <trd>breast</trd>
        </trdgrp>
        <trd lng="hu">mell</trd>
      </snc>
      <trd lng="br">bronn</trd>
      <trdgrp lng="ca">
        <trd>pit</trd>,
        <trd>mamella</trd>
      </trdgrp>
      <trd lng="de">Brust</trd>
      <trdgrp lng="en">
        <trd>breast</trd>,
        <trd>teat</trd>
      </trdgrp>
      <trdgrp lng="fr">
        <trd>sein<klr> (mamelle)</klr></trd>,
        <trd>mamelle</trd>
      </trdgrp>
      <trd lng="he">&shin;&dalet;</trd>
      <trd lng="lat">mamma, ae</trd>
      <trdgrp lng="nl">
        <trd>borst<klr tip="ind"> (v. vrouw)</klr></trd>,
        <trd>uier</trd>
      </trdgrp>
      <trd lng="pl">pier&sacute;</trd>   
      <trdgrp lng="ru">
        <trd>&c_g;&c_r;&c_u;&c_d;&c_mol; 
          <klr>(&c_zh;&c_je;&c_n;&c_s;&c_k;&c_a;&c_ja;)</klr></trd>,
        <trd>&c_m;&c_o;&c_l;&c_o;&c_ch;&c_n;&c_a;&c_ja; 
          &c_zh;&c_je;&c_l;&c_je;&c_z;&c_a;</trd>
      </trdgrp>
      <trd lng="sv">br&ouml;st</trd>
    </drv>

    <drv mrk="mam.0eto">
      <kap><tld/>eto</kap>
      <snc>
        <ref tip="dif" cel="mam.0pinto">mampinto</ref>
      </snc>
      <trd lng="br">beg ar vronn</trd>
      <trd lng="ca">mugr&oacute;</trd>
      <trd lng="de">Brustwarze</trd>
      <trd lng="en">nipple</trd>
      <trdgrp lng="fr">
        <trd>mamelon</trd>,
        <trd>t&ecirc;ton</trd>
      </trdgrp>
      <trd lng="he">&pe;&tet;&mem;&he;</trd>
      <trd lng="hu">mellbimb&oacute;</trd>
      <trd lng="lat">papilla, ae</trd>
      <trd lng="nl">tepel</trd>
      <trd lng="pl">brodawka</trd>   
      <trd lng="pt">mamilo;</trd>
      <trd lng="ru">&c_s;&c_o;&c_s;&c_o;&c_k;</trd>
      <trd lng="sv">br&ouml;stv&aring;rta</trd>
      <trd lng="tr">meme ba&scedil;&inodot;</trd>
    </drv>

    <drv mrk="mam.0uloj">
      <kap><tld/>uloj</kap>
      <uzo tip="fak">ZOO</uzo>
      <snc>
        <dif>
          Klaso de <ref tip="super" cel="verteb.0uloj">vertebruloj</ref>
          (<trd lng="la">Mammalia</trd>), kies idoj estas nutrataj per
          su&ccirc;ado de patrina lakto:
          <ekz>
            la granda pando <ctl>Ailuropoda melanoleuca</ctl> estas &ccirc;arma
            <tld/>ulo vivanta libere nur en &Ccirc;inio
            <fnt><aut>Peng Jianjun</aut>:
              <vrk>Kloni grandan pandon</vrk>,
              <bib>Monato</bib><lok>jaro 2001a,
                numero 4a, p. 25a</lok></fnt>.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="mam.0bestoj"><tld/>bestoj</ref>
        </refgrp>
      </snc>
      <trd lng="br">bronneged</trd>
      <trd lng="ca">mam&iacute;fers</trd>
      <trd lng="de">S&auml;ugetiere</trd>
      <trd lng="en">mammals</trd>
      <trd lng="fr">mammif&egrave;res</trd>
      <trd lng="he">&yod;&vav;&nun;&qof;&yod;&fmem;</trd>
      <trd lng="hu">eml&odblac;s&ouml;k</trd>
      <trd lng="nl">zoogdieren</trd>
      <trd lng="pl">ssaki</trd>   
      <trd lng="pt">mam&iacute;feros</trd>
      <trd 
        lng="ru">&c_m;&c_l;&c_je;&c_k;&c_o;&c_p;&c_i;&c_t;&c_a;&c_ju;&c_shch;&c_i;&c_je;</trd>
      <trd lng="tr">memeliler</trd>
    </drv>

    <drv mrk="mam.0bestoj">
      <kap><tld/>bestoj</kap>
      <uzo tip="fak">ZOO</uzo>
      <snc>
        <ref tip="dif" cel="mam.0uloj"><tld/>uloj</ref>
        <ekz>
          la ovo kovi&gcirc;as en la organismo de la 
          <tld/>besto
          <fnt><aut>Gilbert R. Ledon</aut>: <vrk>Ovo kaj kokino</vrk>,
            <bib>Monato</bib>
            <lok>jaro 2001a, numero 11a, p. 5a</lok>
          </fnt>.
        </ekz>
      </snc>
      <trd lng="br">bronneged</trd> 
      <trd lng="ca">mam&iacute;fers</trd>
      <trd lng="de">S&auml;ugetiere</trd>
      <trd lng="en">mammals</trd>
      <trd lng="fr">mammif&egrave;res</trd>
      <trd lng="he">&yod;&vav;&nun;&qof;&yod;&fmem;</trd>
      <trd lng="hu">eml&odblac;s&aacute;llatok</trd>
      <trd lng="nl">zoogdieren</trd>
      <trd lng="pl">ssaki</trd>   
      <trd 
        lng="ru">&c_m;&c_l;&c_je;&c_k;&c_o;&c_p;&c_i;&c_t;&c_a;&c_ju;&c_shch;&c_i;&c_je;</trd>
      <trd lng="tr">memeliler</trd>
    </drv>

    <drv mrk="mam.0nutri">
      <kap><tld/>nutri</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Nutri per <tld/>o.
        </dif>
      </snc>
      <trd lng="br">bronna&ntilde;</trd>
      <trd lng="ca">alletar</trd>
      <trdgrp lng="de">
        <trd>s&auml;ugen</trd>,
        <trd>stillen</trd>
      </trdgrp>
      <trdgrp lng="en">
        <trd>nurse</trd>,
        <trd>suckle</trd>
      </trdgrp>
      <trd lng="fr">allaiter</trd>
      <trd lng="he">&lamed;<ind>&he;&yod;&nun;&yod;&qof;</ind></trd>
      <trd lng="hu">szoptat</trd>
      <trd lng="nl">zogen</trd>
      <trd lng="pl">karmi&cacute; piersi&aogonek;</trd>   
      <trd lng="pt">amamentar</trd>
      <trd lng="ru">&c_k;&c_o;&c_r;&c_m;&c_i;&c_t;&c_mol; 
        &c_g;&c_r;&c_u;&c_d;&c_mol;&c_ju;</trd>
      <trd lng="tr">emzirmek</trd>
    </drv>

    <drv mrk="mam.0nutristino">
      <kap><tld/>nutristino</kap>
      <snc>
        <dif>
          Virino regule <tld/>nutranta fremdan bebon (precipe pro
          dungo a&ubreve; alia komisio):
          <ekz>
            Lina konas lin de &ccirc;iam: &scirc;i estis lia <tld/>nutristino
            <fnt>
              <aut>Raymond SCHWARTZ</aut>:
              <vrk>&Ccirc;e l' entombigo de l' povra Aleksandro</vrk>.
              <lok>La Nica literatura revuo, 1:4</lok>
            </fnt>;
          </ekz><ekz>
            <uzo tip="stl">FIG</uzo>
            A&ubreve;skuitu do, Azio, <tld/>nutristino nia,
            Afriko sfinkse muta, tribar' Oceania <klr>...</klr>
            <fnt>
              <aut>N. &Hcirc;o&hcirc;lov</aut>: <vrk>E&ubreve;ropo</vrk>.
              <lok>La Tajdo, 1928</lok>
            </fnt>.
          </ekz>
        </dif>
      </snc>
      <ref tip="sin" cel="nutr.0istino">nutristino</ref>
      <trd lng="ru">&c_k;&c_o;&c_r;&c_m;&c_i;&c_l;&c_i;&c_c;&c_a;</trd>
    </drv>

    <drv mrk="mam.0pinto">
      <kap><tld/>pinto</kap>
      <snc>
        <dif>
          Pinto de la <ref tip="malprt" cel="mam.0o.ina"><tld/>oj</ref>, el
          kiu eli&gcirc;as la lakto:
          <ekz>
            la trabluze videblaj <tld/>pintoj de Helga
            <fnt><aut>Pejno Simono</aut>:
              <vrk>La lernemulo</vrk>,
              <bib>Monato</bib><lok>jaro 1999a, numero 6a, p. 23a</lok>
            </fnt>.
          </ekz>
        </dif>
        <refgrp tip="sin">
          <ref cel="cic.0o">cico</ref>,
          <ref cel="mam.0eto">mameto</ref>
        </refgrp>
      </snc>
      <trd lng="br">beg ar vronn</trd>
      <trd lng="ca">mugr&oacute;</trd>
      <trd lng="de">Brustwarze</trd>
      <trd lng="en">nipple</trd>
      <trdgrp lng="fr">
        <trd>mamelon</trd>,
        <trd>t&ecirc;ton</trd>
      </trdgrp>
      <trd lng="he">&pe;&tet;&mem;&he;</trd>
      <trd lng="hu">mellbimb&oacute;</trd>
      <trd lng="lat">papilla, ae</trd>
      <trd lng="nl">tepel</trd>
      <trd lng="pl">brodawka</trd>   
      <trd lng="pt">mamilo</trd>
      <trd lng="ru">&c_s;&c_o;&c_s;&c_o;&c_k;</trd>
      <trd lng="sv">br&ouml;stv&aring;rta</trd>
      <trd lng="tr">meme ba&scedil;&inodot;</trd>
    </drv>

    <drv mrk="mam.0sucxi">
      <kap><tld/>su&ccirc;i</kap>
      <gra><vspec>tr</vspec></gra>
      <snc>
        <dif>
          Sin nutri su&ccirc;ante <tld/>on.
        </dif>
      </snc>
      <trd lng="br">dena&ntilde;</trd>
      <trd lng="ca">mamar</trd>
      <trdgrp lng="en">
        <trd>suck</trd>,
        <trd>suckle</trd>
      </trdgrp>
      <trd lng="fr">t&eacute;ter</trd>
      <trd lng="he">&lamed;<ind>&yod;&nun;&vav;&qof;</ind></trd>
      <trd lng="hu">szopik</trd>
      <trdgrp lng="lat">
        <trd>lacteo, ere</trd>,
        <trd>sugo, ere</trd>
      </trdgrp>
      <trd lng="nl">zuigen<klr tip="ind"> (a.d.borst)</klr></trd>
      <trd lng="pl">ssa&cacute; pier&sacute;</trd>   
      <trd lng="pt">mamar</trd>
      <trd lng="ru">&c_s;&c_o;&c_s;&c_a;&c_t;&c_mol; 
        &c_g;&c_r;&c_u;&c_d;&c_mol;</trd>
    </drv>

    <drv mrk="mam.de0igi">
      <kap>de<tld/>igi</kap>
      <snc>
        <dif>
          Senigi idon je <tld/>nutrado por alkutimigi &gcirc;in al malflua
          nutra&jcirc;o.
        </dif>
      </snc>
      <trd lng="br">dizona&ntilde;</trd>
      <trd lng="ca">deslletar</trd>
      <trd lng="de">abstillen</trd>
      <trdgrp lng="en">
        <trd>wean</trd>,
        <trd>ablactate</trd>
      </trdgrp>
      <trd lng="fr">sevrer</trd>
      <trd lng="he">&lamed;<ind>&gimel;&mem;&vav;&lamed;</ind> <klr>(&tav;&yod;&nun;&vav;&qof;
&mem;&yod;&nun;&yod;&qof;&he;)</klr></trd>
      <trd lng="hu">elv&aacute;laszt<klr tip="amb"> (csecsem&odblac;t)</klr></trd>
      <trdgrp lng="lat">
        <trd>a matre <ind>depello, ere</ind></trd>,
        <trd>a mamma <ind>disiungo, ere</ind></trd>,
        <trd>ablacto, are</trd>
      </trdgrp>
      <trd lng="nl">spenen</trd>
      <trd lng="pl">odstawi&cacute; od piersi</trd>   
      <trd lng="pt">desmamar</trd>
      <trd lng="ru">&c_o;&c_t;&c_n;&c_ja;&c_t;&c_mol; &c_o;&c_t; 
        &c_g;&c_r;&c_u;&c_d;&c_i;</trd>
    </drv>

  </art>
