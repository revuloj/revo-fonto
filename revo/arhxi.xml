<drv mrk="arhxi.0">
  <kap><tld/>-, <var><kap><tld var="k"/>-</kap></var></kap>
  <uzo tip="stl">RAR</uzo>
  <snc>
    <dif>
      De supera rango, <ref tip="dif" cel="cxef.0">&ccirc;ef-</ref>:
      <ekz>
        <tld/>friponoj, protokanajloj
        <fnt>
          <bib>Revizoro</bib>
          <lok>Akto kvina</lok>
        </fnt>!
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="arkidu.0o">ar&hcirc;iduko</ref>,
      <ref cel="arhxiepiskop.0o">ar&hcirc;iepiskopo</ref>
    </refgrp>
    <trd lng="de">Erz-</trd>
    <trd lng="en">arch-</trd>
    <trd lng="fr">arch-</trd>
  </snc>
</drv>

  </art>

