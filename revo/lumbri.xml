<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: lumbri.xml,v 1.23 2020/02/19 23:10:36 revo Exp $">
<kap>
  <ofc>4</ofc>
  <rad>lumbrik</rad>/o
  <fnt><bib>Fab2</bib></fnt>
</kap>
<drv mrk="lumbri.0o">
  <kap>
    <tld/>o
  </kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      <ref tip="lst" lst="voko:zoologiaj_genroj"
      cel="genr.0o.BIO">Genro</ref>
      el familio <ref tip="super" cel="lumbri1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Lumbricus</trd>), relative granda vermo vivanta en
      tero, surtere videbla post pluvo:
      <ekz>
        unu el miaj konataj <tld/>oj
        <fnt>
          <bib>Fab2</bib>
          <lok>Monteto de elfoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        la <tld/>o elrampis el la malseka tero
        <fnt>
          <bib>Fab4</bib>
          <lok>Filino de la mar&ccirc;a re&gcirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        kompo&scirc;tado a&ubreve; humigado (kreado de humo) estas metodo, per kiu
        oni pretigas el
        verdaj &gcirc;ardenaj, agrikulturaj a&ubreve; kuirejaj defala&jcirc;oj kompo&scirc;ton
        per helpo de
        mikroorganismoj, vermoj (<tld/>oj) kaj insektoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=49111"
              >Kompo&scirc;tado</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <ref tip="sin" cel="verm.ter0o">tervermo</ref>
    <bld
      lok="&WCU;/thumb/0/06/Lumbricus_terrestris_01_by-dpc.jpg/640px-Lumbricus_terrestris_01_by-dpc.jpg"
      prm="CC BY 3.0"
      ><tld/>o (Lumbricus terrestris) &ccirc;e Villan&oacute;far, Hispanujo
        <fnt>
          <aut>David Perez, propra verko</aut>
          <lok><url ref="&WCW;/File:Lumbricus_terrestris_01_by-dpc.jpg"
              >https://commons.wikimedia.org/wiki/File:Lumbricus_terrestris_01_by-dpc.jpg</url></lok>
        </fnt>
    </bld>
  </snc>
  <trd lng="be">&c_d;&c_a;&c_zh;&c_d;&c_zh;&c_a;&c_v;&c_y; &c_ch;&c_a;&c_r;&c_v;&c_ja;&c_k;</trd>
  <trd lng="bg">&c_d;&c_malmol;&c_zh;&c_d;&c_o;&c_v;&c_je;&c_n; &c_ch;&c_je;&c_r;&c_v;&c_je;&c_j;</trd>
  <trdgrp lng="ca">
    <trd><ind>cuc</ind> de terra</trd>,
    <trd>llombric</trd>
  </trdgrp>
  <trd lng="de">Regenwurm</trd>
  <trd lng="el">&alfa;&sigma;&kappa;&alfa;&rho;&jota_ton;&sigma_fina;</trd>
  <trd lng="en">earthworm</trd>
  <trdgrp lng="fr">
    <trd>ver de terre</trd>,
    <trd>lombric</trd>
  </trdgrp>
  <trd lng="hu">f&ouml;ldigiliszta</trd>
  <trd lng="nl">regenworm</trd>
  <trd lng="pl">d&zdot;d&zdot;ownica</trd> 
  <trd lng="pt">lombriga</trd>
  <trd lng="ro">r&acirc;m&abreve;</trd>
  <trd lng="ru">&c_d;&c_o;&c_zh;&c_d;&c_je;&c_v;&c_o;&c_j; &c_ch;&c_je;&c_r;&c_v;&c_mol;</trd>
  <trd lng="sv">daggmask</trd>
</drv>
</art>
