<vortaro>
<art mrk="$Id$">
<kap>
<ofc>4</ofc>
<rad>lumbrik</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="lumbri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lumbri1.0oj"><tld/>edoj</ref>
      (<trd lng="la">Lumbricus</trd>), relative granda vermo vivanta en tero,
      surtere videbla post pluvo.
    </dif>
    <ref tip="sin" cel="verm.ter0o">tervermo</ref>
  </snc>
  <trd lng="de">Regenwurm</trd>
  <trd lng="nl">regenworm</trd>
</drv>

</art>
