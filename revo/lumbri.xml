<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>lumbrik</rad>/o
<fnt>Z</fnt>
</kap>
<drv mrk="lumbri.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="lumbri.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Lumbricus</trd>), relative granda vermo vivanta en tero,
      surtere videbla post pluvo.
    </dif>
    <ref tip="sin" cel="verm.ter0o">tervermo</ref>
  </snc>
  <trd lng="de">Regenwurm</trd>
</drv>
<drv mrk="lumbri.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super"
      cel="oligohx.0oj">oligo&hcirc;etoj</ref>
      (<trd lng="la">Lumbricidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Regenw&uuml;rmer <klr tip="ind">(Familie)</klr></trd>
</drv>
</art>
