<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: turism.xml,v 1.30 2022/11/28 13:35:32 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>turism</rad>/o <fnt><bib>Paroloj</bib></fnt>
</kap>
<drv mrk="turism.0o">
  <kap><tld/>o</kap>
  <snc mrk="turism.0o.TUR">
    <dif>
      Voja&gcirc;ado a&ubreve; ekskursado por plezuro a&ubreve; sporto:
      <ekz>
        ne vidama <tld/>o en&scirc;ipigis nin
        <fnt>
          <bib>Paroloj</bib>
          <vrk>Parolado anta&ubreve; la Sesa Kongreso Esperantista en Washington en la 15a de a&ubreve;gusto
          1910</vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a duoninsulo kun belegaj strandoj, klifoj, fi&scirc;havenoj
        <fnt>
          <bib>Monato</bib>
          <aut>Josette Ducloyer</aut>
          <vrk><url ref="&Steloj;monato2/006851.html">Francio: Quimper</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a industrio
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="&Steloj;monato2/006859.html">Seborgo: Urbeto a&ubreve; sendependa
          lando?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>o-agentejo</ind>
        <fnt>
          <bib>Monato</bib>
          <aut>Hori Jasuo</aut>
          <vrk><url ref="&Monato;/2006/008864.php">Kiam beboj fari&gcirc;as malbeboj</url></vrk>
          <lok>2006</lok>
        </fnt>;
        <trd lng="de">Reiseb&uuml;ro</trd>
      </ekz>
      <ekz>
        verki <klr>[&#8230;]</klr> turisman gvidilon pri Santiago
        <fnt>
          <bib>Monato</bib>
          <aut>Rafaela Urue&ntilde;a</aut>
          <vrk><url ref="&Steloj;monato2/006809.html">Hispanio: Vojoj al la sankta urbo</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[la]</klr> insulo Komodo estas jam unu el la &ccirc;efaj <tld/>a&jcirc;oj de Indonezio
        <fnt>
          <bib>Monato</bib>
          <aut>Izabel C. O. Santiago</aut>
          <vrk><url ref="&Steloj;monato2/006900.html">Fa&ubreve;no: La &leftquot;drakoj&rightquot; de Komodo: Gigantaj bestoj
          sur izola insulo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="turist.0o">turisto</ref>,
       <ref cel="vid.0indajxo">vidinda&jcirc;o</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">turisme</trd>
  <trdgrp lng="cs">
    <trd>cizineck&yacute; ruch</trd>,
    <trd>turistika</trd>,
    <trd>turizmus</trd>
  </trdgrp>
  <trd lng="de">Tourismus</trd>
  <trd lng="en">tourism</trd>
  <trd lng="es">turismo</trd>
  <trd lng="fr">tourisme</trd>
  <trdgrp lng="hu">
    <trd>idegenforgalom</trd>,
    <trd>turizmus</trd>
  </trdgrp>
  <trd lng="it">turismo</trd>
  <trd lng="nl">toerisme</trd>
  <trd lng="pl">turystyka</trd>
  <trd lng="pt">turismo</trd>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_i;&c_z;&c_m;</trd>
  <trdgrp lng="sk">
    <trd>cestovn&yacute; ruch</trd>,
    <trd>turistika</trd>
  </trdgrp>
  <trd lng="sv">turism</trd>
  <trd lng="uk">&c_t;&c_u;&c_r;&c_i;&c_z;&c_m;</trd>
</drv>

<drv mrk="turism.0i">
  <kap>
    <tld/>i
  </kap>
  <snc>
    <dif>
      <ref tip="super" cel="vojagx.0i">Voja&gcirc;i</ref> pro plezuro a&ubreve; sporto:
      <ekz>
        &ccirc;iujare centmiloj da germanoj <tld/>as en Italio
        <fnt>
          <bib>Monato</bib>
          <aut>Stefan Maul</aut>
          <vrk><url ref="https://www.monato.be/2003/008168.html"
              >EU-reformeto</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        ni <tld/>os en historia palaco, la budhisma mona&hcirc;ejo <nom>Rumtek</nom> kaj &ccirc;e la belega
        lago <nom>Cangu</nom>
        <fnt>
          <bib>Kon11</bib>
          <lok>Internacia Ekskurso &ccirc;e Katmando</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[krom]</klr> viziti la gepatrojn <klr>[&#8230;]</klr>
        ni do <tld/>as nur iomete, flanke, gvidate de &ccirc;i tiuj plej a&ubreve;tentaj barcelonanoj
        <fnt>
          <bib>Kon11</bib>
          <lok>Tiel la mondo...</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="vojagx.rond0i">rondvoja&gcirc;i</ref>
    </refgrp>
  </snc>
  <trd lng="de"><ind>Tourismus</ind> betreiben</trd>
</drv>


<drv mrk="turism.agro0o">
  <kap>agro<tld/>o</kap>
  <snc mrk="turism.agro0o.TUR">
    <dif>
      <ref tip="vid" cel="feri.0i">Feriado</ref> en 
      <ref tip="vid" cel="bien.0o.kampara">farmbieno</ref>.
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_g;&c_r;&c_a;&c_t;&c_u;&c_r;&c_y;&c_z;&c_m;</trd>
  <trd lng="ca">agro<ind>turisme</ind></trd>
  <trd lng="cs">agroturistika</trd>
  <trd lng="de"><ind>Ferien</ind> auf dem Bauernhof</trd>
  <trd lng="en">farm holidays</trd>
  <trd lng="es">turismo rural</trd>
  <trd lng="fr">vacances &agrave; la ferme</trd>
  <trd lng="hu">falusi <ind>turizmus</ind></trd>
  <trd lng="it">agriturismo</trd>
  <trd lng="pl">agroturystyka</trd>
  <trd lng="sk">agroturistika</trd>
</drv>

<drv mrk="turism.san0o">
  <kap>san<tld/>o</kap>
  <snc mrk="turism.san0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Vizito al alia lando por profiti &gcirc;iajn
      pli bonajn sanservojn, malsanulejojn...:
      <ekz>
        en la hispana gazetaro okazas populisma
        diskuto pri la san<tld/>o
        <fnt>
          <aut>A. Kuenzli</aut>,
          <vrk>en <url
          ref="http://www.liberafolio.org/2013/ribelo-skuas-civitan-sidejon#1378372617651258">
          Ribelo skuas Civitan sidejon</url></vrk>,
          <lok>libera folio, 2013-09-05</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_m;&c_e;&c_d;&c_y;&c_c;&c_y;&c_n;&c_s;&c_k;&c_ib; <ind>&c_t;&c_u;&c_r;&c_y;&c_z;&c_m;</ind></trd>,
    <trd>&c_a;&c_z;&c_d;&c_a;&c_r;&c_a;&c_w;&c_l;&c_je;&c_n;&c_ch;&c_y; <ind>&c_t;&c_u;&c_r;&c_y;&c_z;&c_m;</ind></trd>
  </trdgrp>
  <trd lng="de">Gesundheitstourismus</trd>
  <trdgrp lng="fr">
    <trd>tourisme m&eacute;dical</trd>,
    <trd>tourisme sanitaire</trd>
  </trdgrp>
</drv>

<drv mrk="turism.seks0o">
  <kap>seks<tld/>o</kap>
  <snc mrk="turism.seks0o.TUR">
    <dif>
      Turismado en lando
      por profiti tiean prostituadon:
      <ekz>
        &leftquot;<nac>I'm not in Thailand only for ruins</nac>&rightquot;,
        flustras en lia orelo kvindekjara turisto,
        fiera subtenanto de seks<tld/>o
        <fnt>
          <aut>S. Bellenger, trad. B.-A. Legault</aut>,
          <vrk><url ref="http://eo.hades-presse.com/n2/tajlando.shtml">
          Tajlanda turismo por vakeroj</url></vrk>,
          <lok>Hades, [2009?], (2)</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_e;&c_k;&c_s;-&c_t;&c_u;&c_r;&c_y;&c_z;&c_m;</trd>
  <trd lng="de">Sextourismus</trd>
  <trd lng="fr">tourisme sexuel</trd>
</drv>


<drv mrk="turism.Monda_Organizajxo_pri_T0o">
  <kap>Monda Organiza&jcirc;o pri <tld lit="T"/>o</kap>
  <mlg>MOT</mlg>
  <fnt><bib>ManPol</bib></fnt>
  <snc mrk="turism.Monda_Organizajxo_pri_T0o.POL">
    <uzo tip="fak">POL</uzo>
    <dif>
      Speciala <ref tip="lst" cel="organi2.0ajxo" lst="voko:organiza&jcirc;oj">organiza&jcirc;o</ref> de
      <ref tip="malprt" cel="naci.Unuigxintaj_0oj">Unui&gcirc;intaj
Nacioj</ref>
      por akceli inter&scirc;tatan <tld/>on:
      <ekz>
        fakuloj de Monda Organiza&jcirc;o pri <tld lit="T"/>o (MOT)
        la&ubreve;dis Latvion kaj Estonion pro progreso, atingita en
        turismo.
        <fnt><aut>LAST</aut>,
        <vrk><url ref="&Monato;/007005.html">Pozitiva
evoluo</url></vrk>,
        <bib>Monato</bib>,
        <lok>2000:08</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_S;&c_u;&c_s;&c_mol;&c_v;&c_je;&c_t;&c_n;&c_a;&c_ja; <ind>&c_t;&c_u;&c_r;&c_y;&c_s;&c_t;&c_y;&c_ch;&c_n;&c_a;&c_ja;</ind> &c_a;&c_r;&c_g;&c_a;&c_n;&c_ib;&c_z;&c_a;&c_c;&c_y;&c_ja;</trd>
  <trdgrp lng="ca">
    <trd>Organitzaci&oacute; Mundial del Turisme</trd>,
    <trd>OMT</trd>
  </trdgrp>
  <trd lng="de">Weltorganisation f&uuml;r Tourismus</trd>
  <trdgrp lng="en">
    <trd>World <ind>Tourism</ind> Organisation</trd>,
    <trd>UNWTO</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>Organizaci&oacute;n Mundial del <ind>Turismo</ind></trd>,
    <trd>OMT</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>Organisation Mondiale du <ind>Tourisme</ind></trd>,
    <trd>OMT</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>Idegenforgalmi Vil&aacute;gsz&ouml;vets&eacute;g</trd>,
    <trd>IVSZ</trd>
  </trdgrp>  
  <trdgrp lng="it">
    <trd>Organizatione Mondiale del <ind>Turismo</ind></trd>,
    <trd>OMT</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>Wereldorganisatie voor <ind>Toerisme</ind></trd>,
    <trd>WOT</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>&Sacute;wiatowa Organizacja Turystyki</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>Organiza&ccedil;&atilde;o Mundial do Turismo</trd>,
    <trd>OMT</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_V;&c_s;&c_je;&c_m;&c_i;&c_r;&c_n;&c_a;&c_ja; <ind>&c_t;&c_u;&c_r;&c_i;&c_s;&c_t;&c_s;&c_k;&c_a;&c_ja;</ind> &c_o;&c_r;&c_g;&c_a;&c_n;&c_i;&c_z;&c_a;&c_c;&c_i;&c_ja;</trd>,
    <trd>&c_V;&c_T;&c_O;</trd>
  </trdgrp>
</drv>

</art>
