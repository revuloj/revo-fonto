<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>sa&hcirc;aroz</rad>/o <fnt><bib></bib></fnt>
</kap>
<drv mrk="sahxaroz.0o">
  <kap><tld/>o</kap>
  <snc mrk="sahxaroz.0o.KEM">
    <uzo tip="fak">BIO</uzo>
    <uzo tip="fak">KEM</uzo>
    <dif>
      Ordinara <ref tip="super" cel="suker.0o.KUI">sukero</ref>,
      tre dol&ccirc;a, kiun oni eltiras el sukerkano kaj sukerbeto;
      <ref tip="sin" cel="sakaroz.0o">sakarozo</ref>;
      (C<sub>12</sub>H<sub>22</sub>O<sub>11</sub>):
      <ekz>
        dum la kuirado <klr>...</klr> la relativa kvanto de fruktozo 
        kaj <tld/>o kreskas kompare al la krudaj fruktoj
        <fnt>
          <bib>Viki</bib>,
          <lok><url
          ref="https://eo.wikipedia.org/wiki/Physalis_philadelphica">
          Physalis philadelphica</url>, 2015-12-23</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="fr">saccarose</trd>
</drv>
</art>

<!--
$Log$
-->
</vortaro>