<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>koklu&scirc;</rad>/o
</kap>
<drv mrk="koklus.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">MED</uzo>
  <snc>
    <dif>
      Infekta malsano, karakterizata de konvulsia tuso.  
    </dif>
  </snc>
  <trd lng="de">Keuchhusten</trd>
  <trdgrp lng="en">
    <trd>whooping cough</trd>,
    <trd>pertussis</trd>
  </trdgrp>
  <trd lng="nl">kinkhoest</trd>
  <trd lng="ru">&c_k;&c_o;&c_k;&c_l;&c_ju;&c_sh;</trd>
</drv>
</art>
