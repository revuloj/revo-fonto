<vortaro>
<art mrk="$Id: cxili.xml,v 1.16 2023/10/31 13:37:04 revo Exp $">
<kap><rad>&Ccirc;ili</rad>/o</kap>
<drv mrk="cxili.CX0o">
  <kap><tld/>o</kap>
  <snc>
  <uzo tip="fak">GEOG</uzo>
  <uzo tip="fak">POL</uzo>
    <dif>
      Sudamerika <ref tip="super" cel="sxtat.0o">&scirc;tato</ref>, limanta
      kun <ref tip="vid" cel="argentin.A0o">Argentino</ref>,
      <ref tip="vid" cel="bolivi.B0o">Bolivio</ref>,
      <ref tip="vid" cel="peru.P0o">Peruo</ref> kaj anka&ubreve; al
      <ref tip="vid" cel="pacifik.P0o">Pacifiko</ref>:
      <ekz>
        post 1973, kiam la registaro de <nom>Allende</nom> en <tld/>o
        estis perforte faligita de
        dekstrulaj armeanoj, kelkaj <tld lit="&ccirc;"/>aj politikaj
        rifu&gcirc;intoj trovis
        azilon en Irlando
        <fnt>
          <aut>Garbhan MacAoidh</aut>
            <vrk><url ref="&Monato;/2000/007030.php">Ser&ccirc;ata: justa solvo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 13</lok>
          </fnt>;
      </ekz>
      <ekz>
        la &ccirc;efa pinto, kiun la indi&gcirc;enoj nomis Anon, signas la landlimon
        inter Argentino
        kaj <tld/>o
        <fnt>
          <bib>Monato</bib>
          <aut>Werner SCHAD</aut>
          <vrk><url ref="&Monato;/2003/007824.html">Tondrulo montego de la Andoj</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        sufi&ccirc;as memori, ke usonaj militistoj instruis al &ccirc;iuj siatempaj
        latin-amerikaj
        diktatoroj, en Argentino, en Gvatemalo, en la pino&ccirc;eta <tld/>o,
        kiel torture
        malaperigi opoziciulojn
        <fnt>
          <bib>Monato</bib>
          <aut>Juan J. Schmitter-soto</aut>
          <vrk><url ref="&Monato;/2004/008272.html">&Ccirc;u esperantistoj kapablus altnivele diskuti?</url></vrk>
        </fnt>;
      </ekz>
      <ekz>
        Japanio, Indonezio, <tld/>o tremadas da&ubreve;re, se ne &ccirc;iutage
        <fnt>
          <bib>Monato</bib>
          <aut>Roberto Pigro</aut>
          <vrk><url ref="&Monato;/2013/010777.php">&Gcirc;is la tero tremas</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  <trd lng="be">&c_Ch;&c_y;&c_l;&c_ib;</trd>
  <trd lng="bg">&c_Ch;&c_i;&c_l;&c_i;</trd>
  <trd lng="da">Chile</trd>
  <trd lng="de">Chile</trd>
  <trd lng="en">Chile</trd>
  <trd lng="es">Chile</trd>
  <trd lng="fi">Chile</trd>
  <trdgrp lng="fr">
    <trd>Chili</trd>,
    <trd>R&eacute;publique du Chili</trd>
  </trdgrp>
  <trd lng="he">&tsadi;'&yod;&lamed;&he;</trd>
  <trd lng="hu">Chile</trd>
  <trd lng="ja">&#x30C1;&#x30EA; <pr>&#x3061;&#x308A;</pr></trd>
  <trd lng="id">Cile</trd>
  <trd lng="it">Cile</trd>
  <trd lng="nl">Chili</trd>
  <trd lng="pl">Chile</trd>
  <trd lng="pt">Chile</trd>
  <trd lng="ru">&c_Ch;&c_i;&c_l;&c_i;</trd>
  <trd lng="sl">&Ccaron;ile</trd>
  <trd lng="tok">ma Sile</trd>
  <trd lng="uk">&c_Ch;&c_i;&c_l;&c_ib;</trd>
  </snc>
  <trd lng="zh">&#x667A;&#x5229; <pr>zh&#xEC;l&#xEC;</pr></trd>
</drv>
