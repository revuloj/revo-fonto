<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: unut.xml,v 1.7 2016/10/31 20:10:20 revo Exp $">
<kap>
   <rad>unut</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="unut.0o">
   <kap><tld/>o</kap>
   <uzo tip="fak">MUZ</uzo>
   <snc mrk="unut.0o.intervalo">
     <dif>
       Nula <ref tip="super" cel="interv.0o.MUZ">intervalo</ref>
       inter du samtonaj sonoj:
        <ekz>
          la <tld/>o, okto, kvinto kaj kvarto estas konsiderataj "perfektaj intervaloj" kaj tial la plej konsonancaj
          <fnt>
            <bib>Viki</bib>
            <lok><url ref='https://eo.wikipedia.org/?curid=34770'>Intervalo (muziko)</url></lok>
          </fnt>.
        </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="unison.0o">unisono</ref>
     </refgrp>
   </snc>
   <snc mrk="unut.0o.de_toniko">
     <dif>
       Unua, baza tono de gamo.
     </dif>
   </snc>
  <trd lng="be">&c_u;&c_n;&c_ib;&c_s;&c_o;&c_n;</trd>
  <trd lng="cs">nulov&yacute; hudebn&iacute; interval</trd>
  <trdgrp lng="de">
     <trd>Unisono</trd>
   </trdgrp>
  <trd 
lng="el">&omikron;&my;&omikron_ton;&phi;&omega;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">unison</trd>
  <trd lng="fi">priimi</trd>
  <trd lng="fr">unisson</trd>
  <trd lng="hu">pr&iacute;m</trd>
  <trd lng="nl">unisono</trd>
  <trd lng="pt">un&iacute;ssono</trd>
  <trd lng="ru">&c_u;&c_n;&c_i;&c_s;&c_o;&c_n;</trd>
  <trd lng="sk">nulov&yacute; hudobn&yacute; interval</trd>
</drv>
</art>

