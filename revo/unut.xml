<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>unut</rad>/o <fnt><bib>MuzTerm</bib></fnt>
</kap>
<drv mrk="unut.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MUZ</uzo>
  <snc mrk="unut.0o.intervalo">
    <dif>
      Nula <ref tip="super" cel="interv.0o.MUZ">intervalo</ref>
      inter du samtonaj sonoj.
    </dif>
    <refgrp tip="vid">
      <ref cel="unison.0o">unisono</ref>
    </refgrp>
  </snc>
  <snc mrk="unut.0o.de.toniko">
    <dif>
      Unua, baza tono de gamo.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Unisono</trd>
  </trdgrp>
  <trd lng="el">&omikron;&my;&omikron_ton;&phi;&omega;&ny;&omikron;&sigma_fina;</trd>
  <trd lng="en">unison</trd>
  <trd lng="fr">unisson</trd>
  <trd lng="nl">unisono</trd>
  <trd lng="pt">un&iacute;ssono</trd>
  <trd lng="ru">&c_u;&c_n;&c_i;&c_s;&c_o;&c_n;</trd>
</drv>
</art>

