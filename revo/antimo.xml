<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>antimon</rad>/o
</kap>
<drv mrk="antimo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 51, atompezo 121,75, simbolo Sb. Blanka blueta metalo,
      de lamena strukturo, uzata en industrio kaj medicino.
    </dif>
  </snc>
  <trd lng="de">Antimon</trd>
  <trd lng="fr">antimoine</trd>
  <trd lng="la">Stibium</trd>
  <trd lng="ru">&c_s;&c_u;&c_r;&c_mol;&c_m;&c_a;</trd>
  <trd lng="tr">antimon</trd>
</drv>
</art>
