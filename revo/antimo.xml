<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>antimon</rad>/o
</kap>
<drv mrk="antimo.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <mlg kod="IUPAC">Sb</mlg>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="lst" cel="elemen.0o.KEM"
      lst="voko:&hcirc;emiaj_elementoj">elemento</ref> kun
      atomnumero 51, atompezo 121,75; blanka
      blueta 
      <ref tip="super" cel="metal.0o">metalo</ref>,
      de lamena strukturo, uzata en industrio kaj medicino.
    </dif>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_r;&c_m;&c_a;</trd>
  <trd lng="br">stibiom</trd>
  <trd lng="ca">antimoni</trd>
  <trd lng="de">Antimon</trd>
  <trd lng="en">antimony</trd>
  <trd lng="es">antimonio</trd>
  <trd lng="fr">antimoine</trd>
  <trd lng="hu">antimon</trd>
  <trd lng="it">antimonio</trd>
  <trd lng="la">Stibium</trd>
  <trd lng="nl">antimoon</trd>
  <trd lng="pl">antymon</trd>
  <trd lng="pt">antim&ograve;nio</trd>
  <trd lng="ru">&c_s;&c_u;&c_r;&c_mol;&c_m;&c_a;</trd>
  <trd lng="sk">antim&oacute;n</trd>
  <trd lng="sv">antimon</trd>
  <trd lng="tr">antimon</trd>
</drv>
</art>
