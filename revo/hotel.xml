<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>hotel</rad>/o
</kap>
<drv mrk="hotel.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Domo, kie la voja&gcirc;antoj povas pagante lo&gcirc;i kaj plejofte anka&ubreve; man&gcirc;i;
      gastejo. 
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Hotel</trd>,
     <trd>Gasthaus</trd>,
     <trd>Herberge</trd></trdgrp>
  <trd lng="en">hotel</trd>
  <trd lng="nl">hotel</trd>
  <trdgrp lng="ru">
    <trd>&c_g;&c_o;&c_s;&c_t;&c_i;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_o;&c_t;&c_je;&c_l;&c_mol;</trd>
  </trdgrp>
</drv>
<drv mrk="hotel.0isto0mastro">
  <kap><tld/>isto, <tld/>mastro</kap>
  <snc>
    <dif>
      Posedanto a&ubreve; administranto de <tld/>o.  
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Hotelier</trd>,
     <trd>Hotelbesitzer</trd>,
     <trd>Gastwirt</trd>,
     <trd>Herbergsvater</trd></trdgrp>
   <trdgrp lng="nl">
     <trd>hotelhouder</trd>,
     <trd>h&ocirc;telier</trd>
   </trdgrp>
</drv>
</art>
