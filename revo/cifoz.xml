<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: cifoz.xml,v 1.6 2022/04/12 08:44:45 revo Exp $">
<kap>
  <rad>cifoz</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="cifoz.0o">
  <kap><tld/>o</kap>
  <snc mrk="cifoz.0o.ANA">
    <uzo tip="fak">ANA</uzo>
    <dif>
      Aldorsa kurbo de la spino:
      <ekz>
        <tld/>o <klr>...</klr> estas normala, same kiel lordozo
        <fnt>
          <aut>H. Schicke</aut>,
          <vrk><url ref="http://53913456.swh.strato-hosting.eu/ina/la_korpo_de_homo5.html">
          La Korpo de la Homo</url></vrk>,
          <lok>1995</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="gxib.0o.ANA">&gcirc;ibo</ref>,
      <ref cel="lordoz.0o.ANA">lordozo</ref>,
      <ref cel="skolioz.0o.ANA">skoliozo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_ib;&c_f;&c_o;&c_z;</trd>
  <trd lng="de">Kyphose</trd>
  <trd lng="en">kyphosis</trd>
  <trd lng="fr">cyphose</trd>
  <trdgrp lng="he">
    <trd>&#x5E7;&#x5D9;&#x5E4;&#x5D5;&#x5D6;&#x5D9;&#x5E1;</trd>,
    <trd>&#x5D2;&#x5D1;&#x5E0;&#x5D5;&#x5DF;</trd>
  </trdgrp>
  <trd lng="pl">kifoza</trd>
  <trdgrp lng="uk">
    <trd>&#x43A;&c_ib;&c_f;&#x43E;&c_z;</trd>,
    <trd>&c_g;&#x43E;&c_r;&c_b;&c_a;&c_t;&c_ib;&c_s;&c_t;&#x44C;</trd>
  </trdgrp>
</drv>
</art>

