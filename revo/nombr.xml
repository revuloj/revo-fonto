    </dif>
  </snc>
  <trd lng="be">&c_n;&c_je;&c_z;&c_mol;&c_l;&c_ib;&c_ch;&c_o;&c_n;&c_y;</trd>
  <trd lng="fr">innombrable</trd>
  <trd lng="hu">sz&aacute;mtalan</trd>
  <trd lng="nl">talloos</trd>
  <trd lng="pt">in&uacute;meros</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_je;&c_s;&c_ch;&c_i;&c_s;&c_l;&c_je;&c_n;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_ch;&c_jo;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_s;&c_m;&c_je;&c_t;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_n;&c_je;&c_i;&c_s;&c_ch;&c_i;&c_s;&c_l;&c_i;&c_m;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

