      <ekz>
        la deka, <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 21:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        krizopraso <klr>[&#x2026;]</klr> enhavas kvanteton da <ref cel="nikel.0o">nikelo</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=198144">Krizopraso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fb/Chrysoprase-sample2.jpg/320px-Chrysoprase-sample2.jpg" prm="CC BY 2.0"><tld/>o, A&ubreve;stralio
        <fnt>
