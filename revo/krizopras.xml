<vortaro>
  <art mrk="$Id: krizopras.xml,v 1.2 2022/05/01 08:09:25 afido Exp $">
    <kap>
      <rad>krizopras</rad>/o <fnt><bib>NT</bib></fnt>
    </kap>

<drv mrk="krizopras.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MIN</uzo>
  <snc mrk="krizopras.0o.MIN">
    <dif>
      Verdeca varia&jcirc;o de <ref tip="super" cel="kalcedon.0o">kalcedono</ref>,
      t.e. <ref tip="super" cel="kvarc.0o">kvarco</ref>, kutime uzata por gemoj:
      <ekz>
        la deka, <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>Apokalipso 21:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        krizopraso <klr>[&#8230;]</klr> enhavas kvanteton da <ref cel="nikel.0o">nikelo</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=198144"
              >Krizopraso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld
      lok="&WCU;/thumb/f/fb/Chrysoprase-sample2.jpg/320px-Chrysoprase-sample2.jpg"
      prm="CC BY 2.0"
      ><tld/>o, A&ubreve;stralio
        <fnt>
          <aut>Jarno el Roterdamo</aut>
          <lok><url ref="&WCW;/File:Chrysoprase-sample2.jpg"
              >&WCW;/File:Chrysoprase-sample2.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Chrysopras</trd>
    <trd lng="en">Chrysoprase</trd>
    <trd lng="fr">Chrysoprase</trd>
  </snc>
</drv>

  </art>

