<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
  <art mrk="$Id: krizopras.xml,v 1.6 2024/12/17 08:37:05 afido Exp $">
    <kap>
      <rad>krizopras</rad>/o <fnt><bib>NT</bib></fnt>,
      <var><kap><rad var="z">krizopraz</rad>/o <fnt><bib>PIV1</bib></fnt></kap></var>
    </kap>

<drv mrk="krizopras.0o">
  <kap><tld/>o,
  <var><kap><tld var="z"/>o</kap></var></kap>
  <snc mrk="krizopras.0o.MIN">
    <uzo tip="fak">MIN</uzo>
    <dif>
      Verdeca varia&jcirc;o de <ref tip="super" cel="kalcedon.0o">kalcedono</ref>,
      t.e. <ref tip="super" cel="kvarc.0o">kvarco</ref>, kutime uzata por gemoj:
      <ekz>
        la deka, <tld/>o
        <fnt>
          <bib>NT</bib>
          <lok>&Apo; 21:20</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>o <klr>[&#8230;]</klr> enhavas kvanteton da <ref cel="nikel.0o">nikelo</ref>
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=198144">Krizopraso</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <bld lok="&WCU;/thumb/f/fb/Chrysoprase-sample2.jpg/320px-Chrysoprase-sample2.jpg" prm="CC BY
    2.0"><tld/>o, A&ubreve;stralio
        <fnt>
          <aut>Jarno el Roterdamo</aut>
          <lok><url
          ref="&WCW;/File:Chrysoprase-sample2.jpg">&WCW;/File:Chrysoprase-sample2.jpg</url></lok>
        </fnt>
    </bld>
    <trd lng="de">Chrysopras</trd>
    <trd lng="en">chrysoprase</trd>
    <trdgrp lng="zh">
    <trd>&#x7DA0;&#29577;&#x9AD3; <pr>l&#xFC;y&#xF9;su&#x1D0;</pr></trd>,
    <trd>&#x7EFF;&#29577;&#x9AD3; <pr>l&#xFC;y&#xF9;su&#x1D0;</pr></trd>,
    <trd>&#x7DA0;&#29577;&#x9AD3; <pr>l&#xF9;:y&#xF9;su&#x1D0;</pr></trd>,
    <trd>&#x7EFF;&#29577;&#x9AD3; <pr>l&#xF9;:y&#xF9;su&#x1D0;</pr></trd>,
    <trd>&#x7DA0;&#x746A;&#29785; <pr>l&#xF9;:m&#x1CE;n&#x1CE;o</pr></trd>,
    <trd>&#x7EFF;&#x739B;&#29785; <pr>l&#xF9;:m&#x1CE;n&#x1CE;o</pr></trd>
  </trdgrp>
  </snc>
  <trd lng="be">&c_h;&c_r;&c_y;&c_z;&c_a;&c_p;&c_r;&c_a;&c_z;</trd>
  <trd lng="fr">chrysoprase</trd>
</drv>
