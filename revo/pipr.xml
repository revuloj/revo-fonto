<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>pipr</rad>/o
</kap>
<drv mrk="pipr.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc>
    <dif>
      Sekigita frukto de
      <ref tip="malprt" cel="pipr.0ujo"><tld/>ujo</ref>,
      havanta pikan guston kaj uzata kiel spica&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Pfeffer</trd>
  <trd lng="fr">poivre</trd>
  <trd lng="en"><klr tip="ind">(black)</klr> pepper</trd>
</drv>
<drv mrk="pipr.0arbeto">
  <kap><tld/>arbeto</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="pipr.0ujo.genro">Piprujo
      <sncref ref="pipr.0ujo.genro"/>
      </ref>.
    </dif>
  </snc>
</drv>
<drv mrk="pipr.0ujo">
  <kap><tld/>ujo</kap>
  <snc mrk="pipr.0ujo.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="pipr.0ujacoj"><tld/>ujacoj</ref>
      (<trd lng="la">Piper</trd>), vegeta&jcirc;o produktanta la <tld/>on.
    </dif>
    <trd lng="de">Pfeffer</trd>
    <trd lng="fr">poivrier</trd>
    <trd lng="en">pepper tree</trd>
  </snc>
  <snc mrk="pipr.0ujo.mangxilo">
    <dif>
      Vazeto entenanta la <tld/>on por la man&gcirc;otablo.
    </dif>
  </snc>
  <trd lng="de">Pfefferstreuer</trd>
  <trd lng="fr">poivri&egrave;re</trd>
  <trdgrp lng="en">
    <trd>pepper shaker</trd>,
    <trd>pepper cellar</trd>
  </trdgrp>
</drv>
<drv mrk="pipr.0ujacoj">
  <kap><tld/>ujacoj</kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      Familio de <ref tip="super" cel="kotile.du0aj">dukotiledonaj</ref>
      plantoj (<trd lng="la">Piperaceae</trd>).
    </dif>
  </snc>
</drv>
<drv mrk="pipr.0i">
  <kap><tld/>i</kap>
  <uzo tip="fak">KUI</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Spici per <tld/>o.
    </dif>
    <refgrp tip="sin">
      <ref cel="paprik.0o">papriko</ref>.
    </refgrp>
  </snc>
  <trd lng="fr">poivrer</trd>
  <trd lng="en"><klr tip="ind">(to)</klr> pepper</trd>
</drv>
</art>
