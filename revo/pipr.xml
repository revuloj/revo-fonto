<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: pipr.xml,v 1.36 2024/02/17 06:43:08 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>pipr</rad>/o
</kap>

<drv mrk="pipr.0o">
  <kap>
  <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Sekigita frukto de
      <ref tip="malprt" cel="pipr.0ujo.genro"><tld/>ujo</ref>,
      havanta pikan guston kaj uzata kiel <ref tip="lst" cel="spic.0o" lst="voko:spicoj">spico</ref>.
      <ekz>
        tricent barelegoj da <tld/>o
        <fnt>
          <bib>FK</bib>
        </fnt>;
      </ekz>
      <ekz>
        akva supo, spicita per <tld/>o kaj ranca oleo
        <fnt>
          <bib>Fab1</bib>
          <lok>Galo&scirc;oj de feli&ccirc;o</lok>
        </fnt>;
      </ekz>
      <ekz>
        ili vendadis plej diversajn spicojn, kiel ekzemple safranon, 
        anizon, zingibron kaj precipe <tld/>on <klr>[&#x2026;]</klr>
        kaj pro tio la germanaj komizoj en Danujo ricevis la nomon
        <tld/>istoj
        <fnt>
          <bib>Fab3</bib>
          <lok>Nokta &ccirc;apo de fra&ubreve;lo</lok>
        </fnt>;
      </ekz>
      <ekz>
        li estis <klr>[&#x2026;]</klr> tiel koleri&gcirc;ema, kvaza&ubreve; li havus en si
        <tld/>on
        <fnt>
          <bib>Fab1</bib>
          <lok>Malgranda Niko kaj granda Niko</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>a muelilo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=1123">Erika
          Mann</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>                      
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_c;</trd>
  <trd lng="bg">&c_p;&c_i;&c_p;&c_je;&c_r;</trd>
  <trd lng="br">pebr</trd>
  <trd lng="de">Pfeffer</trd>
  <trd lng="es">pimienta</trd>
  <trd lng="fr">poivre</trd>
  <trd lng="en"><klr tip="ind">(black)</klr> pepper</trd>
  <trd lng="hu">bors<klr tip="amb"> (f&udblac;szer)</klr></trd>
  <trdgrp lng="id">
    <trd>lada</trd>,
    <trd>merica</trd>
  </trdgrp>
  <trd lng="ja">&#x3053;&#x3057;&#x3087;&#x3046;</trd>
  <trd lng="nl">peper</trd>
  <trd lng="pl">pieprz</trd>
  <trd lng="pt">pimenta</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_c;</trd>
  <trd lng="uk">&c_p;&c_je;&c_r;&c_je;&c_c;&c_mol;</trd>
  <trdgrp lng="zh">
    <trd>&#x6912; <pr>ji&#x101;o</pr></trd>,
    <trd>&#x80E1;&#x6912; <pr>h&#xFA;ji&#x101;o</pr></trd>,
    <trd>&#x9ED1;&#x80E1;&#x6912; <pr>h&#x113;ih&#xFA;ji&#x101;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="pipr.0arbeto">
  <kap>
  <tld/>arbeto
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="pipr.0ujo.genro">Piprujo
      <sncref ref="pipr.0ujo.genro"/>
      </ref>.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_c;
  <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd>  
  <trd lng="br">pebrenn</trd>
  <trd lng="de">Pfefferstrauch</trd>
  <trdgrp lng="en">
    <trd>black pepper plant</trd>,
    <trd>peppercorn plant</trd>,
    <trd>pepper tree</trd>
  </trdgrp>
  <trd lng="es">pimentero</trd>
  <trd lng="ja">&#x30B3;&#x30B7;&#x30E7;&#x30A6;</trd>
  <trd lng="nl">peperstruik</trd>
  <trd lng="hu">bors<klr tip="amb"> (cserje)</klr></trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_c; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  <trd lng="zh">&#x80E1;&#x6912; <pr>h&#xFA;ji&#x101;o</pr></trd>
</drv>

<drv mrk="pipr.0ujo">
  <kap>
  <tld/>ujo
  </kap>
  <snc mrk="pipr.0ujo.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      <ref tip="lst" cel="genr.0o" lst="voko:botanikaj_genroj">Genro</ref> el familio <ref tip="malprt" cel="pipruj.0oj"><tld/>ujacoj</ref>
      (<trd lng="la">Piper</trd>), vegeta&jcirc;o produktanta la
      <tld/>on.
    </dif>
    <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_c;
    <klr>(&c_r;&c_a;&c_s;&c_mol;&c_l;&c_ib;&c_n;&c_a;)</klr></trd> 
    <trd lng="br">pebrenn</trd>
    <trd lng="de">Pfefferstrauch</trd>
    <trd lng="es">pimentero</trd>
    <trd lng="fr">poivrier</trd>
    <trd lng="en">pepper tree</trd>
    <trd lng="hu">bors<klr tip="amb"> (cserje)</klr></trd>
    <trd lng="nl">peperboom</trd>
    <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_c; <klr tip="amb">(&c_r;&c_a;&c_s;&c_t;&c_je;&c_n;&c_i;&c_je;)</klr></trd>
  </snc>
  <snc mrk="pipr.0ujo.mangxilo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      <ref tip="super" cel="vaz.0o">Vazeto</ref>
      entenanta la <tld/>on por la man&gcirc;otablo:
      <ekz>
        salujo <klr>[&#x2026;]</klr> ofte <klr>[&#x2026;]</klr> formas paron
        kun la <tld/>ujo, el kiu &gcirc;i povas diferenci&gcirc;i nur la&ubreve; 
        nombro a&ubreve; diametro de la eliraj truoj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=505960">Salujo</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="de">Pfefferstreuer</trd>
    <trd lng="es">pimentero</trd>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_a;&c_ch;&c_n;&c_ib;&c_c;&c_a;</trd>  
  
  <trd lng="br">pebrer</trd>
  
  <trdgrp lng="en">
    <trd>pepper shaker</trd>,
    <trd>pepper cellar</trd>,
    <trd>black pepper plant</trd>,
    <trd>peppercorn plant</trd>
  </trdgrp>
  <trd lng="es">pimentero</trd>
  <trd lng="fr">poivri&egrave;re</trd>
  <trdgrp lng="hu">
    <trd>borshint&odblac;</trd>,
    <trd>borstart&oacute;</trd>
  </trdgrp>
  <trd lng="ja">&#x3053;&#x3057;&#x3087;&#x3046;&#x5165;&#x308C;  <pr>&#x3053;&#x3057;&#x3087;&#x3046;&#x3044;&#x308C;</pr></trd>
  <trd lng="nl">peperbus</trd>
  <trd lng="pl">pieprzniczka</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_i;&c_c;&c_a;</trd>
  <trd lng="uk">&c_p;&c_je;&c_r;&c_je;&c_ch;&c_n;&c_i;&c_c;&c_ja;</trd>
</drv>

<drv mrk="pipr.0i">
  <kap>
  <tld/>i
  </kap>
  <uzo tip="fak">KUI</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="spic.0i.mangxon">Spici</ref> per <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_je;&c_r;&c_ch;&c_y;&c_c;&c_mol;</trd>  
  <trd lng="br">pebra&ntilde;</trd>
  <trd lng="de">pfeffern</trd>
  <trd lng="en"><klr tip="ind">(to)</klr> pepper</trd>
  <trd lng="es">salpimentar</trd>
  <trd lng="fr">poivrer</trd>
  <trd lng="hu">megborsoz</trd>
  <trd lng="id">me<ind>lada</ind>i</trd>
  <trd lng="ja">&#x3053;&#x3057;&#x3087;&#x3046;&#x3092;&#x304D;&#x304B;&#x305B;&#x308B;</trd>
  <trd lng="nl">peperen</trd>
  <trd lng="pl">pieprzy&cacute;</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_ch;&c_i;&c_t;&c_mol;</trd>
  <trd lng="uk">&c_p;&c_je;&c_r;&c_ch;&c_i;&c_t;&c_i;</trd>
</drv>

<drv mrk="pipr.rugxa0o">
  <kap>
    ru&gcirc;a <tld/>o,
    <var><kap>kajena <tld/>o</kap></var>
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="paprik.0o.pulvoro">Papriko</ref>:
      <ekz>
        kajena <tld/>o estas muelita sekigita kapsiketo <klr>[&#x2026;]</klr> 
        nomita la&ubreve; la &ccirc;efurbo de Franca Gvajano, Kajeno, fakte ne
        temas pri <tld/>o sed pri akra kapsiko
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=307383">Kajena
          pipro</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_ch;&c_y;&c_r;&c_v;&c_o;&c_n;&c_y;
  <ind>&c_p;&c_je;&c_r;&c_a;&c_c;</ind></trd>  
  <trd lng="br">paprika</trd>
  <trd lng="de">Paprikagew&uuml;rz</trd>
  <trdgrp lng="en">
    <trd>red pepper</trd>,
    <trd>Cayenne pepper</trd>,
    <trd>paprika</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>piment&oacute;n</trd>,
    <trd>paprika</trd>
  </trdgrp>
  <trd lng="hu">paprika<klr tip="amb"> (por)</klr></trd>
  <trd lng="id">paprika</trd>
  <trd lng="ja">&#x5510;&#x8F9B;&#x5B50;  <pr>&#x3068;&#x3046;&#x304C;&#x3089;&#x3057;</pr></trd>
  <trd lng="pl">papryka<klr> (zmielona przyprawa)</klr></trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_s;&c_n;&c_y;&c_j;
  &c_p;&c_je;&c_r;&c_je;&c_c;</trd>
  <trdgrp lng="zh">
    <trd>&#x725B;&#x89D2;&#x6912; <pr>ni&#xFA;ji&#x1CE;oji&#x101;o</pr></trd>,
    <trd>&#x7D05;&#x8FA3;&#x6912;&#x7C89; <pr>h&#xF3;ngl&#xE0;ji&#x101;of&#x11B;n</pr></trd>,
    <trd>&#x7EA2;&#x8FA3;&#x6912;&#x7C89; <pr>h&#xF3;ngl&#xE0;ji&#x101;of&#x11B;n</pr></trd>,
    <trd>&#x8FA3;&#x5B50; <pr>l&#xE0;zi</pr></trd>
  </trdgrp>
</drv>
</art>
