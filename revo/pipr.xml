<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>pipr</rad>/o
</kap>
<drv mrk="pipr.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <snc>
    <dif>
      Sekigita frukto de
      <ref tip="malprt" cel="pipr.0ujo.genro"><tld/>ujo</ref>,
      havanta pikan guston kaj uzata kiel spica&jcirc;o.
    </dif>
  </snc>
  <trd lng="de">Pfeffer</trd>
  <trd lng="fr">poivre</trd>
  <trd lng="en"><klr tip="ind">(black)</klr> pepper</trd>
  <trd lng="nl">peper</trd>
</drv>

<drv mrk="pipr.0arbeto">
  <kap>
    <tld/>arbeto
  </kap>
  <uzo tip="fak">BOT</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="pipr.0ujo.genro">Piprujo
      <sncref ref="pipr.0ujo.genro"/>
      </ref>.
    </dif>
  </snc>
  <trd lng="nl">peperstruik</trd>
</drv>

<drv mrk="pipr.0ujo">
  <kap>
    <tld/>ujo
  </kap>
  <snc mrk="pipr.0ujo.genro">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="pipruj.0oj"><tld/>ujacoj</ref>
      (<trd lng="la">Piper</trd>), vegeta&jcirc;o produktanta la <tld/>on.
    </dif>
    <trd lng="de">Pfeffer</trd>
    <trd lng="fr">poivrier</trd>
    <trd lng="en">pepper tree</trd>
    <trd lng="nl">peperboom</trd>
  </snc>
  <snc mrk="pipr.0ujo.mangxilo">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Vazeto entenanta la <tld/>on por la man&gcirc;otablo.
    </dif>
  </snc>
  <trd lng="de">Pfefferstreuer</trd>
  <trd lng="fr">poivri&egrave;re</trd>
  <trdgrp lng="en">
    <trd>pepper shaker</trd>,
    <trd>pepper cellar</trd>
  </trdgrp>
  <trd lng="nl">peperbus</trd>
</drv>

<drv mrk="pipr.0i">
  <kap>
    <tld/>i
  </kap>
  <uzo tip="fak">KUI</uzo>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Spici per <tld/>o.
    </dif>
  </snc>
  <trd lng="fr">poivrer</trd>
  <trd lng="en"><klr tip="ind">(to)</klr> pepper</trd>
  <trd lng="nl">peperen</trd>
</drv>

<drv mrk="pipr.rugxa0o">
  <kap>
    ru&gcirc;a <tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <ref tip="dif" cel="paprik.0o.pulvoro">Papriko</ref>.
    </dif>
  </snc>
</drv>
</art>
