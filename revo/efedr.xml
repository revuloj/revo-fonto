<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: efedr.xml,v 1.7 2023/02/22 06:35:18 revo Exp $">
<kap>
  <rad>efedr</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="efedr.0o">
  <kap><tld/>o</kap>
  <snc mrk="efedr.0o.BOT">
    <uzo tip="fak">BOT</uzo>
    <dif>
      Arbeto kreskanta en mezvarmaj landoj de Ameriko, E&ubreve;ropo, Azio, en 
      malsekaj lokoj, &ccirc;e bordoj,
      genro de familio efedracoj (<trd lng="la">Ephedra</trd>):
      <ekz>
        <tld/>o kiel <klr>...</klr> medicina&jcirc;o estas &scirc;vitiga kaj 
        mildigas astmon
        <fnt>
          <vrk><url ref="http://www.chinareport.com.cn/11s/11s-2m/kejiao2-6a.htm">Konsisto
          de recepto</url></vrk>,
          <lok>[vidita je 2006-08-22]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_e;&c_f;&c_e;&c_d;&c_r;&c_a;</trd>
  <trd lng="ca">efedra</trd>
  <trd lng="de">Meertr&auml;ubel</trd>
  <trd lng="fr">ephedra</trd>
  <trdgrp lng="nl">
    <trd>ephedra</trd>,
    <trd>zeedruif</trd>
  </trdgrp>
  <trd lng="pt">&eacute;fedra</trd>
  <trd lng="uk">&c_je;&c_f;&c_je;&c_d;&c_r;&c_a;</trd>
</drv>
</art>

