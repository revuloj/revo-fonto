<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>de&ccirc;ifr</rad>/i
</kap>
<drv mrk="decxif.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">KOMP</uzo>
    <ref tip="dif" cel="cxifr.mal0i">mal&ccirc;ifri.</ref>
    <trdgrp lng="de">
      <trd>entschl&uuml;sseln</trd>,
      <trd>dechiffrieren</trd>
    </trdgrp>
  </snc>
  <snc>
    <dif>
      Pene legi malbone skribitan tekston a&ubreve; malklare esprimitan penson:
      <ekz>
        <tld/>i papiruson;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        sur lia indiferenta viza&gcirc;o &scirc;i penis <tld/>i lian deziron.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="diven.0i">diveni</ref>,
      <ref cel="konjek.0i">konjekti</ref>
    </refgrp>
    <trdgrp lng="de">
      <trd>entziffern</trd>
    </trdgrp>
  </snc>
  <trd lng="fr">d&eacute;chiffrer</trd>
  <trd lng="ru">&c_r;&c_a;&c_s;&c_sh;&c_i;&c_f;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
