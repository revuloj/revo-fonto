<drv mrk="kojl.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ANA</uzo>
  <snc>
    <dif>
      Parto de la <ref tip="malprt" cel="intest.dika0o">dika intesto</ref>
      malanta&ubreve; la cekumo:
      <ekz>
        ar&hcirc;ioj <klr>[&#x2026;]</klr> estas anka&ubreve; parto de la homa mikrobaro, kiu trovi&gcirc;as en
        <tld/>o, bu&scirc;o kaj ha&ubreve;to
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=68">Ar&hcirc;ioj</url></lok>
        </fnt>.
      </ekz>  
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_b;&c_v;&c_o;&c_d;&c_n;&c_a;&c_ja; <ind>&c_k;&c_ib;&c_sh;&c_k;&c_a;</ind></trd>,
    <trd>&c_a;&c_b;&c_v;&c_o;&c_d;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bg">&c_o;&c_b;&c_o;&c_d;&c_n;&c_o; &c_ch;&c_je;&c_r;&c_v;&c_o;</trd>
  <trd lng="ca">c&ograve;lon</trd>
  <trdgrp lng="cs">
    <trd>dutina b&rcaron;i&scaron;n&iacute;</trd>,
    <trd>tra&ccaron;n&iacute;k</trd>
  </trdgrp>
  <trdgrp lng="de">
     <trd>Kolon</trd>,
     <trd>Grimmdarm</trd>
  </trdgrp>
  <trd lng="en">colon</trd>
  <trd lng="es">colon</trd>
  <trd lng="fr">c&ocirc;lon</trd>
  <trd lng="hu">vastagb&eacute;l</trd>
  <trd lng="it">colon</trd>
  <trd lng="ja">&#x7D50;&#x8178;  <pr>&#x3051;&#x3063;&#x3061;&#x3087;&#x3046;</pr></trd>
  <trdgrp lng="nl">
    <trd>karteldarm</trd>,
   <trd>colon<klr tip="ind"> (karteldarm)</klr></trd>
  </trdgrp>
  <trd lng="pl">okr&eogonek;&zdot;nica</trd>
  <trd lng="ru">&c_o;&c_b;&c_o;&c_d;&c_o;&c_ch;&c_n;&c_a;&c_ja; &c_k;&c_i;&c_sh;&c_k;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>bru&scaron;n&aacute; dutina</trd>,
    <trd>tra&ccaron;n&iacute;k</trd>
  </trdgrp>
  <trd lng="uk">&#x43E;&c_b;&#x43E;&c_d;&#x43E;&c_v;&c_a; &#x43A;&c_i;&c_sh;&#x43A;&c_a;</trd>
