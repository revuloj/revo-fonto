  <trd lng="hu">k&aacute;rpitoz</trd>
  <trd lng="nl">vullen<klr tip="ind"> (v.kussen)</klr></trd>
  <trd lng="ru">&c_n;&c_a;&c_b;&c_i;&c_v;&c_a;&c_t;&c_mol; <klr tip="amb">(&c_m;&c_je;&c_b;&c_je;&c_l;&c_mol;, &c_t;&c_ju;&c_f;&c_ja;&c_k;, &c_p;&c_o;&c_d;&c_u;&c_sh;&c_k;&c_u;)</klr></trd>
