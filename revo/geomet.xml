<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>7</ofc>
  <rad>geometr</rad>/o
</kap>
<drv mrk="geomet.0o">
  <kap>
    <tld/>o
  </kap>
  <snc mrk="geomet.0o.sciencisto">
    <ref tip="dif" cel="geomet1.0isto">geometriisto</ref>
    <trd lng="ru">&c_g;&c_je;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  <trd lng="nl">meetkundige</trd>
  </snc>
  <snc mrk="geomet.0o.termezuristo">
    <dif>
      Tiu, kies profesio estas mezuri la kampojn (= kampomezuristo).
    </dif>
    <adm>
      Anstatataux la dubindan referencon al neekzistanta &leftquot;geodeziisto&rightquot;,
      indus meti ref al la sinonima &leftquot;termezuristo&rightquot; (kreota) kaj modernigi
      la difinon. <aut>MB</aut>
    </adm>
    <refgrp tip="sin">
      <ref cel="geodez">geodeziisto</ref>.
    </refgrp>
    <trdgrp lng="de">
      <trd>Landmesser</trd>,
      <trd>Feldmesser</trd>,
      <trd>Geod&auml;t</trd>
    </trdgrp>
    <trd lng="nl">landmeter</trd>
    <trd lng="ru">&c_z;&c_je;&c_m;&c_l;&c_je;&c_m;&c_je;&c_r;</trd>
  </snc>
  <snc mrk="geomet.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="geomet2.0oj"><tld/>edoj</ref>
      (<trd lng="la">Geometra</trd>), nokta papilio.
    </dif>
  </snc>
  <trdgrp lng="fr">
    <trd>g&eacute;om&egrave;tre<klr tip="ind"> (t.s.)</klr></trd>,
    <trd>arpenteur</trd>,
    <trd>chenille arpenteuse</trd>
  </trdgrp>
  <trd lng="nl">spanrups</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_ja;&c_d;&c_je;&c_n;&c_i;&c_c;&c_a;</trd>,
    <trd>&c_z;&c_je;&c_m;&c_l;&c_je;&c_m;&c_je;&c_r; <klr tip="amb">(&c_b;&c_a;&c_b;&c_o;&c_ch;&c_k;&c_a;)</klr></trd>
  </trdgrp>
</drv>
</art>
