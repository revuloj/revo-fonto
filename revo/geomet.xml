<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>7</ofc>
  <rad>geometr</rad>/o
</kap>
<drv mrk="geomet.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">SCI</uzo>
  <snc mrk="geomet.0o.sciencisto">
    <dif>
      Sciencisto studanta la geometrion; specialisto en la geometrio.
    </dif>
    <trd lng="ru">&c_g;&c_je;&c_o;&c_m;&c_je;&c_t;&c_r;</trd>
  </snc>
  <snc mrk="geomet.0o.termezuristo">
    <dif>
      Tiu, kies profesio estas mezuri la kampojn (= kampomezuristo).
    </dif>
    <refgrp tip="sin">
      <ref cel="geodez">geodeziisto</ref>.
    </refgrp>
    <trdgrp lng="de">
     <trd>Landmesser</trd>,
     <trd>Feldmesser</trd>,
     <trd>Geod&auml;t</trd></trdgrp>
    <trd lng="ru">&c_z;&c_je;&c_m;&c_l;&c_je;&c_m;&c_je;&c_r;</trd>
  </snc>
  <snc mrk="geomet.0o.genro">
    <uzo tip="fak">ZOO</uzo>
    <dif>
      Genro el familio <ref tip="super" cel="geomet2.0oj"><tld/>edoj</ref>
      (<trd lng="la">Geometra</trd>), nokta papilio.
    </dif>
  </snc>
</drv>

</art>

