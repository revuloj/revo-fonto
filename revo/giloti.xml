<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: giloti.xml,v 1.22 2023/10/15 18:38:19 revo Exp $">
<kap>
  <ofc>1</ofc>
  <rad>gilotin</rad>/o <fnt><bib>PV</bib></fnt>
</kap>
<drv mrk="giloti.0o">
  <kap><tld/>o</kap>
  <snc mrk="giloti.0o.JUR">
    <dif>
      Ma&scirc;ino por senkapigi mortkondamnitojn:
      <ekz>
        la postan tagon mi trankvile klinus
        jam mian kapon sub la <tld/>on
        <fnt>
          <aut>I. Mad&aacute;ch, trad. K. Kalocsay</aut>, 
          <vrk>La Tragedio de l' Homo</vrk>, 
          <lok>1965</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_l;&c_mol;&c_ja;&c_t;&c_y;&c_n;&c_a;</trd>
  <trd lng="bg">&c_g;&c_i;&c_l;&c_o;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="cs">gilotina</trd>
  <trdgrp lng="de">
    <trd>Guillotine</trd>,
    <trd>Fallbeil</trd>
  </trdgrp>
  <trd lng="en">guillotine</trd>
  <trd lng="es">guillotina</trd>
  <trd lng="fa">&f_g;&f_y;&a_w;&a_t;&f_y;&a_n;</trd>
  <trd lng="fr">guillotine</trd>
  <trdgrp lng="hu">
    <trd>nyaktil&oacute;</trd>,
    <trd>guillotine</trd></trdgrp>
  <trdgrp lng="ja">
    <trd>&#x30AE;&#x30ED;&#x30C1;&#x30F3;</trd>,
    <trd>&#x65AD;&#x982D;&#x53F0;  <pr>&#x3060;&#x3093;&#x3068;&#x3046;&#x3060;&#x3044;</pr></trd>
  </trdgrp>
  <trd lng="nl">guillotine</trd>
  <trd lng="pt">guilhotina</trd>
  <trd lng="ru">&c_g;&c_i;&c_l;&c_mol;&c_o;&c_t;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">gilot&#xED;na</trd>
  <trd lng="tr">giyotin</trd>
  <trd lng="uk">&#x491;&#x456;&#x43B;&#x44C;&#x439;&#x43E;&#x442;&#x438;&#x43D;&#x430;</trd>
</drv>

<drv mrk="giloti.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="giloti.0i.JUR">
    <uzo tip="fak">JUR</uzo>
    <dif>
      <ref tip="super" cel="ekzeku.0i.JUR">Ekzekuti</ref>,
      senkapigante per <tld/>o. 
    </dif>
  </snc>
  <trd lng="be">&c_g;&c_ib;&c_l;&c_mol;&c_ja;&c_t;&c_y;&c_n;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="cs">
    <trd>gilotinovat</trd>,
    <trd>popravit gilotinou</trd>
  </trdgrp>
  <trd lng="de">guillotinieren</trd>
  <trd lng="es">guillotinar</trd>
  <trd lng="fr">guillotiner</trd>
  <trd lng="hu">lenyakaz<klr tip="ind"> (guillotine-nal)</klr></trd>
  <trd lng="ja">&#x30AE;&#x30ED;&#x30C1;&#x30F3;&#x306B;&#x304B;&#x3051;&#x308B;</trd>
  <trd lng="nl">onthoofden</trd>
  <trd lng="pt">guilhotinar</trd>
  <trd lng="ru">&c_g;&c_i;&c_l;&c_mol;&c_o;&c_t;&c_i;&c_n;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">popravi&#x165; gilot&#xED;nou</trd>
  <trd lng="tr">giyotinle kafa u&ccedil;urma</trd>
  <trd lng="uk">&#x491;&#x456;&#x43B;&#x44C;&#x439;&#x43E;&#x442;&#x438;&#x43D;&#x443;&#x432;&#x430;&#x442;&#x438;</trd>
</drv>
</art>
