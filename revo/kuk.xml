<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>kuk</rad>/o
</kap>
<drv mrk="kuk.0o">
  <kap>
    <ofc>*</ofc><tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Franda baka&jcirc;o el diversspeca pasto kun sukero:
      <ekz>tablon ornamas ne tuko sed <tld/>o<fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>de pli da suko ne malboni&gcirc;as la <tld/>o
        <klr>(tro da bono ne &ccirc;agrenas)</klr><fnt><bib>PrV</bib></fnt>;</ekz>
      <ekz>
        e&ccirc; per dol&ccirc;a <tld/>o vi min ne allogus<fnt>Z</fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="en">cake</trd>
  <trd lng="fr">g&acirc;teau</trd>
  <trdgrp lng="ru">
    <trd>&c_p;&c_i;&c_r;&c_o;&c_g; <klr>(&c_s;&c_l;&c_a;&c_d;&c_k;&c_i;&c_j;)</klr></trd>,
    <trd>&c_p;&c_i;&c_r;&c_o;&c_zh;&c_o;&c_k;</trd>,
    <trd>&c_l;&c_je;&c_p;&c_jo;&c_sh;&c_k;&c_a;</trd>,
    <trd>&c_k;&c_je;&c_k;&c_s;</trd>,
    <trd>&c_p;&c_i;&c_r;&c_o;&c_zh;&c_n;&c_o;&c_je;</trd>,
    <trd>&c_k;&c_o;&c_n;&c_d;&c_i;&c_t;&c_je;&c_r;&c_s;&c_k;&c_o;&c_je; &c_i;&c_z;&c_d;&c_je;&c_l;&c_i;&c_je; <klr>&c_i; &c_t;.&c_p;.</klr></trd>
  </trdgrp>
  <trd lng="sv">kaka</trd>
</drv>

<drv mrk="kuk.0ejo">
  <kap>
    <tld/>ejo
  </kap>
  <snc>
    <dif>
      Loko, kie oni bakas kaj vendas <tld/>ojn, konfita&jcirc;ojn ktp.
    </dif>
    <refgrp tip="sin">
      <ref cel="konfit">konfitejo</ref>.
    </refgrp>
  </snc>
  <trd lng="en">bakery</trd>
  <trd lng="fr">p&acirc;tisserie</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_u;&c_l;&c_o;&c_ch;&c_n;&c_a;&c_ja;</trd>,
    <trd>&c_p;&c_je;&c_k;&c_a;&c_r;&c_n;&c_ja;</trd>
  </trdgrp>
</drv>

<drv mrk="kuk.tavol0o">
  <kap>
    tavol<tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      <tld lit="K"/>o kiu tavoli&gcirc;as &ccirc;e bakado; pastofolia <tld/>o.
    </dif>
  </snc>
  <trd lng="en">layer cake</trd>
  <trd lng="fr">millefeuille</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_l;&c_o;&c_jo;&c_n;&c_y;&c_j; &c_p;&c_i;&c_r;&c_o;&c_zh;&c_o;&c_k;</trd>,
    <trd>&c_s;&c_l;&c_o;&c_j;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="kuk.pat0o">
  <kap>
    pat<tld/>o
  </kap>
  <uzo tip="fak">KUI</uzo>
  <snc>
    <dif>
      Maldika <tld/>o kuirita en pato;
      <ref tip="sin" cel="kresp.0o">krespo</ref>.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>pancake</trd>,
    <trd>griddle cake</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_o;&c_l;&c_a;&c_d;&c_mol;&c_ja;</trd>
  </trdgrp>
</drv>
</art>
