<drv mrk="kuk.mol0o">
  <kap>
    mol<tld/>o, <var><kap>spongo<tld/>o</kap></var>
  </kap>
  <snc>
    <dif>
      Tre mola, spongeca <tld/>o el flua pasto de faruno, graso, ovo, sukero, lakto,
      eventuale amelo, bakpulvoro:
      <ekz>
        &ccirc;okolada mol<tld/>o, <klr>[&#8230;]</klr> citrona spongo<tld/>o (savoj<tld/>o)
        <fnt>
          <vrk><url ref="https://esperanto-gironde.fr/wp-content/uploads/LEGI158.pdf"
              >Kunveno en Cenon Palmer la 21an de Marto 2019</url></vrk>
          <lok>La Esperanto-Gironde-Informilo, 04/2019 p.8</lok>
        </fnt>;
      </ekz>
      <ekz>
        Nigra-arbara &ccirc;eriz<tld/>o <klr>[el]</klr> du tavoloj de &ccirc;okoladaj spongo<tld/>oj
        gustigitaj per &ccirc;erizobrando, aroma konfita&jcirc;o el erigitaj &ccirc;erizoj
        <klr>[&#8230;]</klr> batita kremo <klr>[&#8230;]</klr> kaj raspita &ccirc;okolado
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=670454"
              >Nigra-arbara &ccirc;erizkuko</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_je;&c_k;&c_s;</trd>
  <trd lng="de">R&uuml;hrkuchen</trd>
  <trd lng="pl">keks</trd>
  <trd lng="ru">&c_k;&c_je;&c_k;&c_s;</trd>
  <trd lng="tr">rikwato</trd>
</drv>

<drv mrk="kuk.pat0o">
  <kap>pat<tld/>o</kap>
  <snc mrk="kuk.pat0o.KUI">
    <uzo tip="fak">KUI</uzo>
    <dif>
      Maldika <ref tip="super" cel="kuk.0o.KUI"><tld/>o</ref> kuirita
      en pato:
      <ekz>
        ni scias, kiu havos dol&ccirc;an supon kaj pat<tld/>on
        <fnt>
          <bib>Fab2</bib>
          <lok>Pa&scirc;tisto de porkoj</lok>
        </fnt>!
      </ekz>
      <ekz>
        la nederlanda pat<tld/>o vere estas farita en pato, kaj estas
        granda, plata kaj ronda
        <fnt>
          <aut>P. Peeraerts</aut>,
          <vrk>Re: Ich bin ein Berliner</vrk>,
          <lok><url ref="&SCE;">soc.culture.esperanto</url>,
          2003-09-24</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="kresp.0o">krespo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_b;&c_l;&c_ib;&c_n;&c_je;&c_c;</trd>,
    <trd>&c_a;&c_l;&c_a;&c_d;&c_k;&c_a;</trd>,
    <trd>&c_s;&c_k;&c_a;&c_v;&c_a;&c_r;&c_o;&c_d;&c_n;&c_ib;&c_k;</trd>
  </trdgrp>
  <trdgrp lng="cs">
    <trd>omeleta</trd>,
    <trd>pala&ccaron;inka</trd>,
    <trd>sv&iacute;tek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Pfannkuchen<klr tip="amb"> (flach)</klr></trd>,
    <trd>Eierkuchen</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>pancake</trd>,
    <trd>griddle cake</trd>
  </trdgrp>
  <trd lng="fr">cr&ecirc;pe</trd>
  <trd lng="hu">palacsinta</trd>
  <trdgrp lng="id">
    <trd>krep</trd>,
    <trd>krepes</trd>
  </trdgrp>
  <trd lng="ja">&#12507;&#12483;&#12488;&#12465;&#12540;&#12461;</trd>
  <trd lng="nl">pannenkoek</trd>
  <trd lng="pl">nale&sacute;nik</trd>
  <trdgrp lng="ru">
    <trd>&c_b;&c_l;&c_i;&c_n;</trd>,
    <trd>&c_o;&c_l;&c_a;&c_d;&c_mol;&c_ja;</trd>
  </trdgrp>
  <trd lng="sk">palacinka</trd>
  <trd lng="sv">pannkaka</trd>
  <trdgrp lng="uk">
    <trd>&c_n;&c_a;&c_l;&c_i;&c_s;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_m;&c_l;&c_i;&c_n;&c_je;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#34180;&#28900;&#39173; <pr>b&oacute;k&#462;ob&#464;ng</pr></trd>,
    <trd>&#34180;&#28900;&#39292; <pr>b&oacute;k&#462;ob&#464;ng</pr></trd>,
    <trd>&#27963;&#21147;&#34507;&#39173; <pr>hu&oacute;l&igrave;d&agrave;nb&#464;ng</pr></trd>
  </trdgrp>
</drv>

<drv mrk="kuk.plat0o">
  <kap>
    plat<tld/>o
  </kap>
  <snc>
    <dif>
      Plata <tld/>o, bakita sur &scirc;tono, bakpleto k.s.:
      <ekz>
        hordeaj plat<tld/>oj
        <fnt>
          <bib>MT</bib>
          <lok>Je&hcirc;ezkel 4:12</lok>
        </fnt>;
      </ekz>
      <ekz>
        mi plore rifuzadis man&gcirc;i plat<tld/>ojn
        <fnt>
          <bib>SkandalJozef</bib>
          <lok>Plendoj de Jozefo arestita</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="flan.0o">flano</ref>,
      <ref cel="pic.0o">pico</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_k;&c_u;&c_h;&c_a;&c_n;</trd>
  <trdgrp lng="de">
    <trd>Blechkuchen</trd>,
    <trd>Plattenkuchen</trd>,
    <trd>Fladen</trd>
  </trdgrp>
  <trd lng="en">tray bake</trd>
</drv>

