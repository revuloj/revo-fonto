<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>volfram</rad>/o
</kap>
<drv mrk="volfram.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">KEM</uzo>
  <snc>
    <dif>
      &Hcirc;emia <ref tip="super" cel="elemen.0o.KEM">elemento</ref> kun
      atomnumero 74, atompezo 183,85, simbolo W. <ref tip="super"
      cel="metal.0o">Metalo</ref> malmola, griza, uzata por
      hardi &scirc;talon, tre alta boltemperaturo, pro tio uzata por fabriki
      filamentojn al inkandeskaj kaj elektronaj lampoj. Eksa nomo estis
      <refgrp tip="sin">
        <ref cel="tungst.0o">tungsteno</ref>.
      </refgrp>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_a;&c_l;&c_mol;&c_f;&c_r;&c_a;&c_m;</trd>
  <trd lng="de">Wolfram</trd>
  <trd lng="fr">wolfram</trd>
  <trd lng="hu">volfr&aacute;m</trd>
  <trd lng="nl">wolfraam</trd>
  <trd lng="ru">&c_v;&c_o;&c_l;&c_mol;&c_f;&c_r;&c_a;&c_m;</trd>
</drv>
</art>
