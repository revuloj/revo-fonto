<art mrk="$Id: polied.xml,v 1.9 2022/03/26 12:56:09 revo Exp $">
<kap>
  <rad>poliedr</rad>/o
</kap>
<drv mrk="polied.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="edr.plur0o">Pluredro</ref>:
      <ekz>
        kristalo <klr>[&#x2026;]</klr> aperas sub la formo de regula a&ubreve; simetria <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8171">Kristalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">pol&iacute;edre</trd>
  <trdgrp lng="cs">
    <trd>mnohost&ecaron;n</trd>,
    <trd>polyedr</trd>
  </trdgrp>
  <trd lng="nl">veelvlak<klr tip="ind"> (arch.)</klr></trd>
  <trd lng="pt">poliedro</trd>
  <trd lng="sk">mnohosten</trd>
  <trdgrp lng="uk">
    <trd>&#x431;&#x430;&#x433;&#x430;&#x442;&#x43E;&#x433;&#x440;&#x430;&#x43D;&#x43D;&#x438;&#x43A;</trd>,
    <trd>&#x43F;&#x43E;&#x43B;&#x456;&#x435;&#x434;&#x440;</trd>
  </trdgrp>
</drv>
</art>

