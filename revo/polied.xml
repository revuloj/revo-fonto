<art mrk="$Id: polied.xml,v 1.11 2022/04/16 22:29:57 revo Exp $">
<kap>
  <rad>poliedr</rad>/o
</kap>
<drv mrk="polied.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">MAT</uzo>
  <uzo tip="stl">ARK</uzo>
  <fnt><bib>MatTerm</bib><lok>p. 30</lok></fnt>
  <snc>
    <dif>
      <ref tip="dif" cel="edr.plur0o">Pluredro</ref>:
      <ekz>
        kristalo <klr>[&#8230;]</klr> aperas sub la formo de regula a&ubreve; simetria <tld/>o
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=8171">Kristalo</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">pol&iacute;edre</trd>
  <trdgrp lng="cs">
    <trd>mnohost&ecaron;n</trd>,
    <trd>polyedr</trd>
  </trdgrp>
  <trd lng="en">polyhedron</trd>
  <trd lng="nl">veelvlak<klr tip="ind"> (arch.)</klr></trd>
  <trd lng="pl">wielo&sacute;cian</trd>
  <trd lng="pt">poliedro</trd>
  <trd lng="ro">poliedru</trd>
  <trd lng="sk">mnohosten</trd>
  <trdgrp lng="uk">
    <trd>&c_b;&c_a;&c_g;&c_a;&c_t;&c_o;&c_g;&c_r;&c_a;&c_n;&c_n;&c_i;&c_k;</trd>,
    <trd>&c_p;&c_o;&c_l;&c_ib;&c_je;&c_d;&c_r;</trd>
  </trdgrp>
</drv>
</art>

