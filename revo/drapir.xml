<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: drapir.xml,v 1.26 2023/11/05 18:30:01 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>drapir</rad>/i
  <fnt><bib>UV</bib></fnt>
</kap>

<drv mrk="drapir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.0i">Ornami</ref>,
      <ref tip="super" cel="kovr.0i">kovri</ref> per pendanta,
      multfalda &scirc;tofo:
      <ekz>
        granda, lukse <tld/>ita halego
        <fnt>
          <bib>KrM</bib>
          <lok>14. Sperto Sa&gcirc;on Akrigas.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la blanka rajdmantelo <klr>[&#8230;]</klr> enkovris liajn graciajn membrojn &scirc;ajne
        senzorge kaj tamen montris plej zorgan <tld/>adon
        <fnt>
          <bib>RBa</bib>
          <lok>&Ccirc;apitro III</lok>
        </fnt>;
      </ekz>
      <ekz>
        &scirc;ia stando estis bele <tld/>ita, kaj la parfumo estis en belformaj flakonoj
        <fnt>
          <bib>KrM</bib>
          <lok>20. Kontra&ubreve; &Ccirc;iu Malfacilo Ekzistas Konsilo.</lok>
        </fnt>;
      </ekz>
      <ekz>
        la maljunulo <klr>[&#8230;]</klr> prenis du kapu&ccirc;ajn mantelojn, unu el kiu li surmetis
        kaj la alian li <tld/>is &ccirc;irka&ubreve; la oldan virinon
        <fnt>
          <bib>Sol</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dekora.0i">dekori, dekoracii</ref>,
      <ref cel="garn.0i.ornami_iu">garni</ref>,
      <ref cel="tapet.0i">tapeti</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_p;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_r;&c_a;&c_p;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>pot&aacute;hnout z&rcaron;asenou l&aacute;tkou</trd>,
    <trd>zahalit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>drapieren</trd>,
    <trd>k&uuml;nstlerisch <ind>beh&auml;ngen</ind></trd>
  </trdgrp>
  <trd lng="en">drape</trd>
  <trd lng="fr">draper</trd>
  <trdgrp lng="ja">
    <trd>&#12402;&#12384;&#x306E;&#12354;&#x308B;&#x5E03;&#12391;&#x304A;&#x304A;&#12358;
    <pr>&#12402;&#12384;&#x306E;&#12354;&#x308B;&#x306C;&#x306E;&#12391;&#x304A;&#x304A;&#12358;</pr></trd>,
    <trd>&#22402;&#x308C;&#x5E55;&#12391;&#x98FE;&#x308B;
    <pr>&#x305F;&#x308C;&#x307E;&#x304F;&#12391;&#x304B;&#12374;&#x308B;</pr></trd>,
    <trd>&#12402;&#12384;&#12434;&#12388;&#12369;&#x308B;</trd>
  </trdgrp>
  <trd lng="nl">draperen</trd>
  <trd lng="pl">drapowa&cacute;</trd>
  <trd lng="pt">drapejar</trd>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">ozdobi&tcaron; drap&eacute;riou</trd>
  <trd lng="uk">&c_d;&c_r;&c_a;&c_p;&c_ib;&c_r;&c_u;&c_v;&c_a;&c_t;&c_i;</trd>
</drv>

<drv mrk="drapir.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="drapir.0ajxo.BELA">
  <uzo tip="fak">BELA</uzo>
    <dif>
      Multfalda penda&jcirc;o:
      <ekz>
        oni vidis balkonojn kun belaj <tld/>a&jcirc;oj
        <fnt>
          <bib>I&scirc;tar</bib>
          <lok>&Ccirc;apitro X</lok>
        </fnt>;
      </ekz>
      <ekz>
        li vokis <nom>Eunice</nom>n, kiu eniris, vestita per blanka <tld/>a&jcirc;o
        <fnt>
          <bib>QuV</bib>
          <lok>&Ccirc;apitro XXIX</lok>
        </fnt>;
      </ekz>
      <ekz>
        kial Via <tld/>a&jcirc;o estas ru&gcirc;a, kaj Viaj vestoj kiel &ccirc;e tretanto en vinpremejo
        <fnt>
          <bib>MT</bib>
          <lok>Jesaja 63:2</lok>
        </fnt>?
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        monta grundo, kie sinsekvas klifoj kaj cirkoj, rozkoloraj kaj flavaj <tld/>a&jcirc;oj,
        izolantaj la verdan areon
        <fnt>
          <bib>Monato</bib>
          <aut>Robert Roze</aut>
          <vrk><url ref="http://steloj.de/esperanto/monato2/006945.html">Francio: Flagrantaj
          koloroj</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Drapierung</trd>,
    <trd>Stoffbehang</trd>,
    <trd>Faltenwurf</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>drapery</trd>,
    <trd>draping</trd>
  </trdgrp>
  <trd lng="fr">drap&eacute;<klr> (subst.)</klr></trd>
  <trdgrp lng="ja">
    <trd>&#x639B;&#12369;&#x5E03;  <pr>&#x304B;&#12369;&#12405;</pr></trd>,
    <trd>&#22402;&#x308C;&#x5E55;  <pr>&#x305F;&#x308C;&#x307E;&#x304F;</pr></trd>,
    <trd>&#12402;&#12384;</trd>,
    <trd>&#x30C9;&#x30EC;&#x30FC;&#x30D7;</trd>
  </trdgrp>
  <trd lng="nl">draperie</trd>
  <trd lng="pl">draperia</trd>
  <trdgrp lng="uk">
    <trd>&c_d;&c_r;&c_a;&c_p;&c_ib;&c_r;&c_u;&c_v;&c_a;&c_n;&c_n;&c_ja;</trd>,
    <trd>&c_d;&c_r;&c_a;&c_p;&c_r;&c_ib;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x5E10;&#x5E18; <pr>zh&#xE0;ngli&#xE1;n</pr></trd>,
    <trd>&#x5E54; <pr>m&#xE0;n</pr></trd>
  </trdgrp>
</drv>
</art>
