<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<ofc>*</ofc>
<rad>drapir</rad>/i
</kap>
<drv mrk="drapir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.0i">Ornami</ref>,
      <ref tip="super" cel="kovr.0i">kovri</ref> per pendanta,
      multfalda &scirc;tofo:
      <ekz>
        <tld/>i liton, fenestron;
      </ekz>
      <ekz>
        la <tld/>a&jcirc;oj de solena estrado;
      </ekz>
      <ekz>
        la grekaj statuoj estas admirinde <tld/>itaj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &ccirc;ielo subite <tld/>i&gcirc;is per malhelaj nuboj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tapet.0i">tapeti</ref>,
      <ref cel="dekor">dekori</ref>,
      <ref cel="garn.0i.ornami_iu">garni</ref>.
    </refgrp>
  </snc>
  <trd lng="be">&c_d;&c_r;&c_a;&c_p;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trdgrp lng="de">
    <trd>drapieren</trd>,
    <trd>k&uuml;nstlerisch <ind>beh&auml;ngen</ind></trd>
  </trdgrp>
  <trd lng="en">drape</trd>
  <trd lng="fr">draper</trd>
  <trd lng="ru">&c_d;&c_r;&c_a;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
</drv>
</art>
