<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id: drapir.xml,v 1.19 2015/07/04 08:37:32 revo Exp $">
<kap>
<ofc>*</ofc>
<rad>drapir</rad>/i
</kap>

<drv mrk="drapir.0i">
  <kap><ofc>*</ofc><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="ornam.0i">Ornami</ref>,
      <ref tip="super" cel="kovr.0i">kovri</ref> per pendanta,
      multfalda &scirc;tofo:
      <ekz>
        <tld/>i liton, fenestron;
      </ekz>
      <ekz>
        la grekaj statuoj estas admirinde <tld/>itaj;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        la &ccirc;ielo subite <tld/>i&gcirc;is per malhelaj nuboj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="dekor">dekori</ref>,
      <ref cel="garn.0i.ornami_iu">garni</ref>,
      <ref cel="tapet.0i">tapeti</ref>
    </refgrp>
  </snc>
  <trd
  lng="be">&c_d;&c_r;&c_a;&c_p;&c_ib;&c_r;&c_a;&c_v;&c_a;&c_c;&c_mol;</trd>
  <trd lng="bg">&c_d;&c_r;&c_a;&c_p;&c_i;&c_r;&c_a;&c_m;</trd>
  <trdgrp lng="cs">
    <trd>potáhnout zřasenou látkou</trd>,
    <trd>zahalit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>drapieren</trd>,
    <trd>k&uuml;nstlerisch <ind>beh&auml;ngen</ind></trd>
  </trdgrp>
  <trd lng="en">drape</trd>
  <trd lng="fr">draper</trd>
  <trd lng="nl">draperen</trd>
  <trd lng="pl">drapowa&cacute;</trd>
  <trd lng="pt">drapejar</trd>
  <trd
  lng="ru">&c_d;&c_r;&c_a;&c_p;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sk">ozdobiť drapériou</trd>
</drv>

<drv mrk="drapir.0ajxo">
  <kap><tld/>a&jcirc;o</kap>
  <snc mrk="drapir.0ajxo.BELA">
  <uzo tip="fak">BELA</uzo>
    <dif>
      Multfalda penda&jcirc;o:
      <ekz>
        la <tld/>a&jcirc;oj de solena estrado;
      </ekz>
      <ekz>
        tiu pentra&jcirc;o vidigas per <tld/>a&jcirc;o la sintenon de
        subvesta
        korpo.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>drapery</trd>,
    <trd>draping</trd>
  </trdgrp>
  <trd lng="fr">drap&eacute;<klr> (subst.)</klr></trd>
  <trd lng="nl">draperie</trd>
  <trd lng="pl">draperia</trd>
</drv>
</art>
