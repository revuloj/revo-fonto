<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>prizon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="prizon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Oficiala <ref tip="super" cel="liber.mal0ejo">malliberejo</ref> por
      gardado de arestitoj a&ubreve; kondamnitoj.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_u;&c_r;&c_m;&c_a;</trd>,
    <trd>&c_v;&c_ja;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="de">Gef&auml;ngnis</trd>
  <trd lng="fr">prison</trd>
  <trd lng="hu">b&ouml;rt&ouml;n</trd>
  <trd lng="nl">gevangenis</trd>
  <trd lng="ru">&c_t;&c_ju;&c_r;&c_mol;&c_m;&c_a;</trd>
</drv>

<drv mrk="prizon.el0igi">
  <kap>el<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Ellasi el <tld/>o.
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_z; &c_t;&c_u;&c_r;&c_m;&c_y;)</klr></trd>
  <trd lng="fr">lib&eacute;rer<klr> (de prison)</klr></trd>
  <trd lng="de">freilassen (aus Gef&auml;ngnis)</trd>
  <trdgrp lng="hu">
    <trd>kienged a <ind>b&ouml;rt&ouml;nb&odblac;l</ind></trd>,
    <trd>elbocs&aacute;t a <ind>b&ouml;rt&ouml;nb&odblac;l</ind></trd>
  </trdgrp>
  <trd lng="nl">vrijlaten</trd>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_i;&c_z; &c_t;&c_ju;&c_r;&c_mol;&c_m;&c_y;)</klr></trd>
</drv>

<drv mrk="prizon.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Konduki al <tld/>o kaj enfermi en &gcirc;i.
      <ekz>
        oni en<tld/>igis lin kiel &leftquot;demokraton&rightquot;, kromnomo, pri kiu li
        fieregis
        <fnt><aut>Bardhyl Selimi</aut>: <vrk>Viro &ccirc;e la landlimo</vrk>,
        <bib>Monato</bib>, <lok>jaro 2002a, numero 10a, p.
        21a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_u; &c_t;&c_u;&c_r;&c_m;&c_u;)</klr></trd>
  <trd lng="de">einsperren (in Gef&auml;ngnis)</trd>
  <trd lng="fr">emprisonner</trd>
  <trdgrp lng="hu">
    <trd>beb&ouml;rt&ouml;n&ouml;z</trd>,
    <trd>b&ouml;rt&ouml;nbe z&aacute;r</trd>
  </trdgrp>
  <trd lng="nl">gevangen nemen</trd>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_v; &c_t;&c_ju;&c_r;&c_mol;&c_m;&c_u;)</klr></trd>
</drv>
</art>

