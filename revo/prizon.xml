<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: prizon.xml,v 1.22 2024/02/17 07:52:36 revo Exp $">
<kap>
  <rad>prizon</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>

<drv mrk="prizon.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">JUR</uzo>
  <snc>
    <dif>
      Oficiala <ref tip="super" cel="liber.mal0ejo">malliberejo</ref>
      por gardado de arestitoj a&ubreve; kondamnitoj:
      <ekz>
        dum tiuj ekleziuloj diversmaniere amuzi&gcirc;is kaj dibo&ccirc;is, <nom>Hus</nom> senkulpe ku&scirc;is en
        tenebra <tld/>o, kie li dufoje grave malsani&gcirc;is
        <fnt>
          <aut>Ji&rcaron;&iacute; Patera</aut>
            <vrk><url ref="&Monato;/2000/007031.php">Johano Hus &ndash; &ccirc;u ne plu herezulo?</url></vrk>
            <bib>Monato</bib>
            <lok>2000/08, p. 19</lok>
          </fnt>;
      </ekz>
      <ekz>
        kiel homoj forkurintaj el <tld/>o kaj rifu&gcirc;antaj en templojn ili &gcirc;ojplene
        eskapas la metiojn kaj kuras al filozofio
        <fnt>
          <bib>Rsp</bib>
        </fnt>;
      </ekz>
      <ekz>
        la manuskripto ekestis en la londona <tld/>a tempo inter 1923 kaj 1924
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        a&ubreve;dienco &ccirc;e la <tld/>a pastro
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="arest.0ejo">arestejo</ref>,
       <ref cel="karcer.0o.malliberejo">karcero</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_t;&c_u;&c_r;&c_m;&c_a;</trd>,
    <trd>&c_v;&c_ja;&c_z;&c_mol;&c_n;&c_ib;&c_c;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&t_ba;&t_tsa;&t_naro;&t_na;&t_tsheg;&t_kha;&t_nga;&t_tsheg;</trd>
  <trd lng="ca">pres&oacute;</trd>
  <trdgrp lng="cs">
    <trd>festunk</trd>,
    <trd>festu&ncaron;k</trd>,
    <trd>v&ecaron;zen&iacute;</trd>,
    <trd>v&ecaron;znice</trd>,
    <trd>&zcaron;al&aacute;&rcaron;</trd>
  </trdgrp>
  <trd lng="de">Gef&auml;ngnis</trd>
  <trd lng="en">prison</trd>
  <trd lng="fa">&a_z;&a_n;&a_d;&a_A;&a_n;</trd>
  <trd lng="fr">prison</trd>
  <trd lng="hu">b&ouml;rt&ouml;n</trd>
  <trdgrp lng="it">
    <trd>prigione</trd>,   
    <trd>carcere</trd>
  </trdgrp>
  <trd lng="nl">gevangenis</trd>
  <trd lng="pl">wi&eogonek;zienie</trd>
  <trdgrp lng="pt">
    <trd>pris&atilde;o</trd>,
    <trd>pres&iacute;dio</trd>,    
    <trd>c&aacute;rcere</trd>
  </trdgrp>
  <trd lng="ru">&c_t;&c_ju;&c_r;&c_mol;&c_m;&c_a;</trd>
  <trd lng="sk">v&auml;znica</trd>
  <trdgrp lng="uk">
    <trd>&c_v;&#x2019;&#x44F;&c_z;&#x43D;&c_i;&c_c;&#x44F;</trd>,
    <trd>&c_t;&#x44E;&c_r;&#x43C;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x56DA;&#x7981; <pr>qi&#xFA;j&#xEC;n</pr></trd>,
    <trd>&#x76D1;&#x72F1; <pr>ji&#x101;ny&#xF9;</pr></trd>,
    <trd>&#x76E3;&#x7344; <pr>ji&#x101;ny&#xF9;</pr></trd>,
    <trd>&#x72F1; <pr>y&#xF9;</pr></trd>,
    <trd>&#x7344; <pr>y&#xF9;</pr></trd>,
    <trd>&#x7262;&#x623F; <pr>l&#xE1;of&#xE1;ng</pr></trd>,
    <trd>&#x5927;&#x7262; <pr>d&#xE0;l&#xE1;o</pr></trd>,
    <trd>&#x5927;&#x72F1; <pr>d&#xE0;y&#xF9;</pr></trd>,
    <trd>&#x5927;&#x7344; <pr>d&#xE0;y&#xF9;</pr></trd>,
    <trd>&#x72B4; <pr>&#xE0;n</pr></trd>,
    <trd>&#x7262;&#x72F1; <pr>l&#xE1;oy&#xF9;</pr></trd>,
    <trd>&#x7262;&#x7344; <pr>l&#xE1;oy&#xF9;</pr></trd>,
    <trd>&#x8C7B; <pr>&#xE0;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="prizon.0ulo">
  <kap>
    <tld/>ulo
  </kap>
  <snc>
    <dif>
      Iu malliberigita en <tld/>o:
      <ekz>
        fu&gcirc;inta <tld/>ulo
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>;
      </ekz>
      <ekz>
        li, estante prizonulo de la cara re&gcirc;imo, senemocie studis lernolibron de
        Esperanto, dum liaj samsortanoj estis mortpafataj
        <fnt>
          <bib>EeP</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_v;&c_ja;&c_z;&c_je;&c_n;&c_mol;</trd>,
    <trd>&c_z;&c_mol;&c_n;&c_ja;&c_v;&c_o;&c_l;&c_je;&c_n;&c_y;</trd>
  </trdgrp>
  <trd lng="de">H&auml;ftling</trd>
  <trd lng="en">prisoner</trd>
  <trd lng="nl">gevangene</trd>
  <trd lng="pl">wi&eogonek;zie&nacute;</trd>
  <trdgrp lng="zh">
    <trd>&#x56DA; <pr>qi&#xFA;</pr></trd>,
    <trd>&#x56DA;&#x72AF; <pr>qi&#xFA;f&#xE0;n</pr></trd>,
    <trd>&#x72AF;&#x4EBA; <pr>f&#xE0;nr&#xE9;n</pr></trd>,
    <trd>&#x62D8;&#x56DA; <pr>j&#x16B;qi&#xFA;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="prizon.el0igi">
  <kap>el<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="las.el0i">Ellasi</ref> el <tld/>o:
      <ekz>
        la ju&gcirc;isto el<tld/>igis arkitekton kondamnitan al kvar jaroj en malliberejo pro
        trafik-akcidento
        <fnt>
          <aut>Garbhan MacAoidh/pg</aut>
            <vrk><url ref="&Monato;/2000/007134.php">Malbona investo</url></vrk>
            <bib>Monato</bib>
            <lok>2000/11, p. 12</lok>
          </fnt>;
      </ekz>
      <ekz>
        iujn <klr>[oni]</klr> fojfoje el<tld/>igas kontra&ubreve; granda monsumo
        <fnt>
          <bib>Monato</bib>
          <aut>Bardhyl Selimi</aut>
          <vrk><url ref="&Monato;/2003/007925.html">Voja&gcirc;impresoj, somero 2002: Kosovo
          anta&ubreve;eniras</url></vrk>
          <lok>2003</lok>
        </fnt>;
      </ekz>
      <ekz>
        krimulo kondamnita al 10-jara enkarcerigo &gcirc;enerale estas el<tld/>igata post ses
        jaroj
        <fnt>
          <bib>Monato</bib>
          <aut>Astrid Millet</aut>
          <vrk><url ref="&Monato;/2011/010322.php">Kial la kolero?</url></vrk>
          <lok>2011</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_v;&c_y;&c_p;&c_u;&c_s;&c_mol;&c_c;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_z;
  &c_t;&c_u;&c_r;&c_m;&c_y;)</klr></trd>
  <trdgrp lng="ca">
    <trd>alliberar</trd>,
    <trd>soltar de la pres&oacute;</trd>
  </trdgrp>
  <trd lng="fa">&a_A;&a_z; <ind>&a_z;&a_n;&a_d;&a_A;&a_n;</ind> &a_A_madda;&a_z;&a_A;&a_d; &f_k;&a_r;&a_d;&a_n;</trd>
  <trd lng="fr">lib&eacute;rer<klr> (de prison)</klr></trd>
  <trd lng="de">freilassen (aus Gef&auml;ngnis)</trd>
  <trdgrp lng="hu">
    <trd>kienged a <ind>b&ouml;rt&ouml;nb&odblac;l</ind></trd>,
    <trd>elbocs&aacute;t a <ind>b&ouml;rt&ouml;nb&odblac;l</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>liberare <klr> di prigione</klr></trd>,   
    <trd>scarcerare</trd>
  </trdgrp>
  <trd lng="nl">vrijlaten</trd>
  <trd lng="pl">zwolni&cacute; z  <ind>wi&eogonek;zienia</ind></trd>
  <trd lng="ru">&c_v;&c_y;&c_p;&c_u;&c_s;&c_t;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_i;&c_z;
  &c_t;&c_ju;&c_r;&c_mol;&c_m;&c_y;)</klr></trd>
  <trd lng="zh">&#x653E;&#x51FA;&#x53BB; <pr>f&#xE0;ngch&#x16B;q&#xF9;</pr></trd>
</drv>

<drv mrk="prizon.en0igi">
  <kap>en<tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Konduki al <tld/>o kaj <ref tip="super" cel="ferm.en0i">enfermi</ref> en &gcirc;i:
      <ekz>
        oni en<tld/>igis lin kiel &leftquot;demokraton&rightquot;, kromnomo, pri kiu li
        fieregis
        <fnt><aut>Bardhyl Selimi</aut>: <vrk>Viro &ccirc;e la landlimo</vrk>,
        <bib>Monato</bib>, <lok>2002/10, p.
        21</lok></fnt>;
      </ekz>
      <ekz>
        unua tago: en<tld/>i&gcirc;o, banado, kontrolo de la saneco, eldono de la tola&jcirc;o,
        disdono de &ccirc;eloj
        <fnt>
          <bib>Mortul&Scirc;ip</bib>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_a;&c_d;&c_z;&c_ib;&c_c;&c_mol; <klr tip="amb">(&c_u;
  &c_t;&c_u;&c_r;&c_m;&c_u;)</klr></trd>
  <trd lng="ca">empresonar</trd>
  <trd lng="cs">uv&ecaron;znit</trd>
  <trd lng="de">einsperren (in Gef&auml;ngnis)</trd>
  <trd lng="en">jail</trd>
  <trdgrp lng="fa">
    <trd>&a_z;&a_n;&a_d;&a_A;&a_n;&f_y; &f_k;&a_r;&a_d;&a_n;</trd>,
    <trd>&a_b;&a_h; <ind>&a_z;&a_n;&a_d;&a_A;&a_n;</ind> &a_A;&a_n;&a_d;&a_A;&a_kh;&a_t;&a_n;</trd>
  </trdgrp>
  <trd lng="fr">emprisonner</trd>
  <trdgrp lng="hu">
    <trd>beb&ouml;rt&ouml;n&ouml;z</trd>,
    <trd>b&ouml;rt&ouml;nbe z&aacute;r</trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>imprigionare</trd>, 
    <trd>mettere in <ind>prigione</ind></trd>, 
    <trd>incarcerare</trd>
  </trdgrp>
  <trd lng="nl">gevangen nemen</trd>
  <trdgrp lng="pl">
    <trd>uwi&eogonek;zi&cacute;</trd>,
    <trd>wsadzi&cacute; do  <ind>wi&eogonek;zienia</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_p;&c_o;&c_s;&c_a;&c_d;&c_i;&c_t;&c_mol;<klr tip="amb"> (&c_v;
  &c_t;&c_ju;&c_r;&c_mol;&c_m;&c_u;)</klr></trd>
  <trd lng="sk">uv&auml;zni&tcaron;</trd>
  <trdgrp lng="uk">
    <trd>&c_u;&c_v;&#x2019;&#x44F;&c_z;&#x43D;&#x44E;&c_v;&c_a;&c_t;&c_i;</trd>,
    <trd>&c_s;&c_a;&c_d;&c_zh;&c_a;&c_t;&c_i;/&#x43A;&c_i;&c_d;&c_a;&c_t;&c_i; &c_u; &c_v;&#x2019;&#x44F;&c_z;&#x43D;&c_i;&c_c;&#x44E;/&c_t;&#x44E;&c_r;&#x43C;&c_u;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x76D1;&#x7981; <pr>ji&#x101;nj&#xEC;n</pr></trd>,
    <trd>&#x76E3;&#x7981; <pr>ji&#x101;nj&#xEC;n</pr></trd>,
    <trd>&#x5704; <pr>y&#x1D4;</pr></trd>
  </trdgrp>
</drv>
</art>

