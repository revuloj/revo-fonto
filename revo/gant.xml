<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>gant</rad>/o
</kap>
<drv mrk="gant.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Vesto por la mano:
      <ekz>
        <uzo tip="stl">FIG</uzo>
        &jcirc;eti al iu la <tld/>on<fnt>B</fnt>
        <klr>(alvoki al batalo, defii)</klr>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        levi la <tld/>on<fnt>B</fnt>
        <klr>(akcepti la batalon, la defion)</klr>.
      </ekz> 
    </dif>
    <refgrp tip="vid">
      <ref cel="muf.0o">mufo</ref>.
    </refgrp>
  </snc>
  <trd lng="de">Handschuh</trd>
  <trd lng="hu">keszty&udblac;</trd>
  <trd lng="fr">gant</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>
  <trdgrp lng="sv">
    <trd>handske</trd>,
    <trd>vante</trd>
  </trdgrp>
</drv>
<drv mrk="gant.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Surmeti <tld/>on al iu: <ekz>sin <tld/>i.</ekz>
    </dif>
  </snc>
  <trd lng="fr">ganter</trd>
</drv>
<drv mrk="gant.fer0o">
  <kap>fer<tld/>o</kap>
  <uzo tip="fak">MIL</uzo>
  <uzo tip="fak">HIS</uzo>
  <snc>
    <dif>
      <tld lit="G"/>o el ferlamenoj, uzata de mezepokaj kavaliroj. 
    </dif>
  </snc>
  <trd lng="fr">gantelet</trd>
</drv>
<drv mrk="gant.duon0o">
  <kap>duon<tld/>o</kap>
  <uzo tip="fak">TEKS</uzo>
  <snc>
    <dif>
      Speco de <tld/>o kovranta la manon nur &gcirc;is la dua falango
      de la fingroj.  
    </dif>
  </snc>
  <trd lng="fr">mitaine</trd>
</drv>
</art>
