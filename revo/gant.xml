<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: gant.xml,v 1.42 2023/11/11 10:00:39 revo Exp $">
<kap>
  <ofc>*</ofc>
  <rad>gant</rad>/o
</kap>

<drv mrk="gant.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <snc mrk="gant.0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="lst" lst="voko:vesta&jcirc;oj" cel="vest.0o">Vesto</ref> por la mano:
      <ekz>
        li detiris siajn &scirc;amajn <tld/>ojn
        <fnt><bib>FK</bib>, <lok>3, anekdotoj</lok></fnt>;
      </ekz>
      <ekz>
        <klr>[ili]</klr> surtiris delikatajn <tld/>ojn sur &scirc;iajn bruligitajn fingrojn
        <fnt>
          <bib>Fab1</bib>
          <lok>Sova&gcirc;aj cignoj</lok>
        </fnt>;
      </ekz>
      <ekz>
        <klr>[li]</klr> lernis kudradon de <tld/>oj
        <fnt>
          <bib>Fab1</bib>
          <lok>Kupra porko</lok>
        </fnt>;
      </ekz>
      <ekz>
        de la mateno ili estis okupitaj per briktransportado, kaj Pa&ubreve;lo blasfemis pro
        siaj preska&ubreve; detruitaj led<tld/>oj
        <fnt>
          <bib>InfanTorent2</bib>
          <lok>Dua Parto</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        Monato fakte signifis defion en la panoramo de la tiutempa Esperanto-gazetaro,
        <nom>Stefan Maul</nom> <ind>&jcirc;etis <tld/>on</ind> <klr>(alvokis
        al <ref cel="defi.0i">defio</ref>)</klr>, kiun levis <nom>Torben Kehlet</nom>
        <fnt>
          <bib>Monato</bib>
          <vrk><url ref="https://www.monato.be/2004/008242.html">Gravaj opinioj</url></vrk>
          <lok>2004</lok>
        </fnt>;
        <trd lng="ca">llan&ccedil;ar el guant a alg&uacute;</trd>
        <trd lng="fr">jeter le gant &agrave; qn</trd>
        <trd lng="nl">de <ind>handschoen</ind> toewerpen</trd>
        <trd lng="hu">keszty&udblac;t dob</trd>
      </ekz>
      <ekz>
        <uzo tip="stl">FIG</uzo>
        <ind>levi la <tld/>on</ind>
        <klr>(akcepti la batalon, la defion)</klr>.
        <fnt><bib>VdE</bib>, <lok/></fnt>.
        <trd lng="ca">recollir el guant</trd>
        <trd lng="fr">relever le gant</trd>
        <trd lng="hu">felveszi a <ind>keszty&udblac;t</ind></trd>
        <trd lng="nl">de <ind>handschoen</ind> opnemen</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="muf.0o">mufo</ref>
    </refgrp>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>
  <trd lng="bg">&c_r;&c_malmol;&c_k;&c_a;&c_v;&c_i;&c_c;&c_a;</trd>
  <trd lng="ca">guant</trd>
  <trd lng="cs">rukavice prstov&aacute;</trd>
  <trd lng="de">Handschuh</trd>
  <trd lng="el">&gamma;&alfa_ton;&ny;&tau;&jota;</trd>
  <trd lng="en">glove</trd>
  <trd lng="es">guante</trd>
  <trd lng="fr">gant</trd>
  <trd lng="hu">keszty&udblac;</trd>
  <trd lng="it">guanto</trd>
  <trdgrp lng="ja">
    <trd>&#x624B;&#x888B;  <pr>&#x3066;&#x3076;&#x304F;&#x308D;</pr></trd>,
    <trd>&#x30B0;&#x30ED;&#x30FC;&#x30D6;  <pr>&#x3050;&#x308D;&#x30FC;&#x3076;</pr></trd>
  </trdgrp>
  <trd lng="nl">handschoen</trd>
  <trd lng="pt">luva</trd>
  <trd lng="ru">&c_p;&c_je;&c_r;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>
  <trd lng="sk">rukavica prstov&aacute;</trd>
  <trdgrp lng="sv">
    <trd>handske</trd>,
    <trd>vante</trd>
  </trdgrp>
  <trd lng="tr">eldiven</trd>
  <trd lng="uk">&c_r;&c_u;&c_k;&c_a;&c_v;&c_i;&c_ch;&c_k;&c_a;</trd>
  <trdgrp lng="zh">
    <trd>&#x624B;&#x7B20; <pr>sh&#x1D2;ul&#xEC;</pr></trd>,
    <trd>&#x624B;&#x5957; <pr>sh&#x1D2;ut&#xE0;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="gant.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc mrk="gant.0i.TEKS">
    <dif>
      Surmeti <tld/>on al iu:
      <ekz>
        sin <tld/>i.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_n;&c_a;&c_d;&c_z;&c_je;&c_c;&c_mol;
  <ind>&c_p;&c_a;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_k;&c_u;</ind></trd>
  <trd lng="ca">enguantar</trd>
  <trd lng="cs">obl&eacute;knout rukavice</trd>
  <trd lng="de">Handschuhe <ind>anziehen</ind></trd>
  <trd lng="fr">ganter</trd>
  <trd lng="hu">keszty&udblac;t h&uacute;z</trd>
  <trdgrp lng="it">
    <trd>inguantare</trd>,
    <trd>mettere i guanti a</trd>
  </trdgrp>
  <trd lng="ja">&#x624B;&#x888B;&#x3092;&#x306F;&#x3081;&#x308B; 
  <pr>&#x3066;&#x3076;&#x304F;&#x308D;&#x3092;&#x306F;&#x3081;&#x308B;</pr></trd>
  <trd lng="nl">de <ind>handschoen(en)</ind> aantrekken</trd>
  <trd lng="pt">cal&ccedil;ar <ind>luvas</ind> a</trd>
  <trd lng="ru">&c_n;&c_a;&c_d;&c_je;&c_t;&c_mol;
  <ind>&c_p;&c_je;&c_r;&c_ch;&c_a;&c_t;&c_k;&c_u;</ind></trd>
  <trd lng="sk">natiahnu&tcaron; rukavice niekomu</trd>
  <trd lng="uk">&c_n;&c_a;&c_d;&c_ib;&c_v;&c_a;&c_t;&c_i; &c_r;&c_u;&c_k;&c_a;&c_v;&c_i;&c_ch;&c_k;&c_i;/&c_r;&c_u;&c_k;&c_a;&c_v;&c_i;&c_c;&c_ib;</trd>
  <trd lng="zh">&#x6234;&#x624B;&#x5957; <pr>d&#xE0;ish&#x1D2;ut&#xE0;o</pr></trd>
</drv>

<drv mrk="gant.0isto">
  <kap><tld/>isto</kap>
  <snc mrk="gant.0isto.EKON">
    <dif>
      Faristo a&ubreve; vendisto de <tld/>oj:
      <ekz>
        tio estis metiejo de <tld/>isto; ili eniris
        <fnt><bib>Fab1</bib>, <lok>kupra porko</lok></fnt>;
      </ekz>
      <ekz>
        brava homo, metiisto, svati&gcirc;as al vi, la <tld/>isto <nom>Erik</nom>
        <fnt>
          <bib>Fab3</bib>
          <lok>&Scirc;i estis tute senta&ubreve;ga</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni trovas stratojn de <tld/>isto, kombilfaristo, linteksisto, &ccirc;apelfaristo
        <fnt>
          <bib>Monato</bib>
          <aut>Saliko</aut>
          <vrk><url ref="https://www.monato.be/2014/011095.php">Tammisaari, urbeto &ccirc;e Balta
          Maro</url></vrk>
          <lok>2014</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_n;&c_ib;&c_k;</trd>
  <trd lng="ca">guanter</trd>
  <trd lng="cs">rukavi&ccaron;k&aacute;&rcaron;</trd>
  <trd lng="de">Handschuhmacher</trd>
  <trd lng="fr">gantier</trd>
  <trd lng="it">guantaio</trd>
  <trd lng="nl">handschoenmaker</trd>
  <trdgrp lng="pt">
   <trd>fabricante de <ind>luvas</ind></trd>,
   <trd>vendedor de <ind>luvas</ind></trd>
  </trdgrp>
  <trd lng="sk">rukavi&ccaron;k&aacute;r</trd>
  <trd lng="tr">eldivenci <klr>(eldiven yapan yada satan)</klr></trd>
  <trd lng="uk">&c_r;&c_u;&c_k;&c_a;&c_v;&c_i;&c_ch;&c_n;&c_i;&c_k;</trd>
</drv>

<drv mrk="gant.fer0o">
  <kap>fer<tld/>o</kap>
  <snc mrk="gant.fer0o.MIL">
    <uzo tip="fak">MIL</uzo>
    <uzo tip="fak">HIS</uzo>
    <dif>
      <tld lit="G"/>o el ferlamenoj, uzata de mezepokaj kavaliroj.
    </dif>
  </snc>
  <trd lng="be">&c_zh;&c_a;&c_l;&c_je;&c_z;&c_n;&c_a;&c_ja; <ind>&c_p;&c_a;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_k;&c_a;</ind></trd>
  <trd lng="ca">manyopla<klr> (arm.)</klr></trd>
  <trd lng="cs">&zcaron;elezn&aacute; rukavice</trd>
  <trdgrp lng="de">
    <trd>Kampfhandschuh</trd>,
    <trd>Panzerhandschuh</trd>,
    <trd>Eisenhandschuh</trd>
  </trdgrp>
  <trd lng="fr">gantelet</trd>
  <trd lng="hu">vaskeszty&udblac;</trd>
  <trdgrp lng="it">
    <trd>manopola<klr> (guanto di ferro)</klr></trd>,
    <trd><ind>guanto</ind> di ferro</trd>
  </trdgrp>
  <trd lng="ja">&#x3053;&#x3066;</trd>
  <trdgrp lng="nl">
    <trd>pantserhandschoen</trd>,
    <trd>mali&euml;nhandschoen</trd>
  </trdgrp>
  <trd lng="pt">guante</trd>
  <trd lng="ru">&c_l;&c_a;&c_t;&c_n;&c_a;&c_ja; &c_p;&c_je;&c_r;&c_ch;&c_a;&c_t;&c_k;&c_a;</trd>
  <trd lng="sk">&zcaron;elezn&aacute; rukavica</trd>
  <trd lng="uk">&c_z;&c_a;&c_l;&c_ib;&c_z;&c_n;&c_a; &c_r;&c_u;&c_k;&c_a;&c_v;&c_i;&c_c;&c_ja; (&c_ch;&c_a;&c_s;&c_t;&c_i;&c_n;&c_a;
  &c_s;&c_je;&c_r;&c_je;&c_d;&c_n;&c_mol;&c_o;&c_v;&c_ib;&c_ch;&c_n;&c_o;&c_g;&c_o; &c_o;&c_b;&c_l;&c_a;&c_d;&c_u;&c_n;&c_k;&c_u;)</trd>
</drv>

<drv mrk="gant.duon0o">
  <kap>duon<tld/>o</kap>
  <snc mrk="gant.duon0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="gant.0o.TEKS"><tld lit="G"/>o</ref> kovranta
      la manon nur &gcirc;is la dua falango de la fingroj, lasanta la pintojn
      liberaj:
      <ekz>
        Klaro &gcirc;oje kisis &scirc;ian manon en lana duon<tld/>o
        <fnt>
          <bib>IK</bib>
          <lok>&Ccirc;apitro II</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_u;&c_l;&c_mol;&c_s;&c_e;&c_t;&c_k;&c_a; <klr>(&c_p;&c_a;&c_w;&c_p;&c_a;&c_l;&c_mol;&c_ch;&c_a;&c_t;&c_k;&c_a;)</klr></trd>
  <trd lng="ca">mitena</trd>
  <trd lng="cs">rukavice bez prst&uring;</trd>
  <trdgrp lng="de">
    <trd>Halbhandschuh</trd>,
    <trd><ind>fingerfrei</ind>er Handschuh</trd>
  </trdgrp>
  <trd lng="fr">mitaine</trd>
  <trdgrp lng="hu">
    <trd>f&eacute;lkeszty&udblac;</trd>,
    <trd>ujjatlan <ind>keszty&udblac;</ind></trd>
  </trdgrp>
  <trdgrp lng="it">
    <trd>mitena</trd>,
    <trd>mezzo<ind>guanto</ind></trd>
  </trdgrp>
  <trd lng="ja">&#x6307;&#x5148;&#x306E;&#x306A;&#x3044;&#x624B;&#x888B; 
  <pr>&#x3086;&#x3073;&#x3055;&#x304D;&#x306E;&#x306A;&#x3044;&#x3066;&#x3076;&#x304F;&#x308D;</pr></trd>
  <trd lng="nl">handschoen zonder vingers</trd>
  <trdgrp lng="pt">
    <trd>meia-luva</trd>,
    <trd>mitene</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_p;&c_je;&c_r;&c_ch;&c_a;&c_t;&c_k;&c_a; &c_b;&c_je;&c_z;
    &c_p;&c_a;&c_l;&c_mol;&c_c;&c_je;&c_v;</trd>,
    <trd>&c_m;&c_i;&c_t;&c_je;&c_n;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="sk">rukavice bez prstov</trd>
  <trdgrp lng="uk">
    <trd>&c_m;&c_ib;&c_t;&c_je;&c_n;&c_k;&c_a;</trd>,
    <trd>&c_p;&c_ib;&c_v;&c_r;&c_u;&c_k;&c_a;&c_v;&c_i;&c_ch;&c_k;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="gant.pugno0o">
  <kap>pugno<tld/>o,
  <var><kap>muf<tld/>o</kap></var></kap>
  <snc mrk="gant.pugno0o.TEKS">
    <uzo tip="fak">TEKS</uzo>
    <dif>
      <ref tip="super" cel="gant.0o.TEKS"><tld lit="G"/>o</ref> kovranta
      unupece la manon, kaj aparte nur la dikan fingron;
      <ref tip="sin" cel="muf.0o.TEKS_ganto">mufo<sncref/></ref>:
      <ekz>
        la vila&gcirc;ano kun grandaj pugno<tld/>oj sur la manoj sidis en sia
        glitveturilo kaj batadis per la brakoj sian korpon, por forigi
        la malvarmon
        <fnt><bib>Fab3</bib>, <lok>historio de la jaro</lok></fnt>;
      </ekz>
      <ekz>
        mi prenis la mantelon kaj la muf<tld/>ojn, fermis la kurtenon
        de la lito de sinjoro <nom>Gulden</nom> kaj eliris,
        la &scirc;losilaron en mia po&scirc;o
        <fnt>
          <aut>Erckmann-Chatrian, trad. P. J. Doumen</aut>
          <vrk><url ref="http://esperanto-forum.org/doc/Romanoj/soldatodenapoleono.pdf">Soldato de
          Napoleono</url></vrk>
          <lok>[vidita en 2015]</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_r;&c_u;&c_k;&c_a;&c_v;&c_ib;&c_c;&c_a;</trd>,
    <trd>&c_s;&c_p;&c_o;&c_d;&c_k;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">manyopla</trd>
  <trd lng="de">F&auml;ustling</trd>
  <trdgrp lng="fr">
    <trd>gant de boxe</trd>,
    <trd>moufle</trd>
  </trdgrp>
  <trd lng="it">muffola</trd>
  <trd lng="ja">&#x30DF;&#x30C8;&#x30F3;</trd>
  <trd lng="nl">bokshandschoen</trd>
  <trd lng="tr">boks eldiveni</trd>
  <trd lng="uk">&c_m;&c_u;&c_f;&c_t;&c_a;</trd>
</drv>
</art>
