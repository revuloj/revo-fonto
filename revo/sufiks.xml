<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>*</ofc>
  <rad>sufiks</rad>/o
</kap>
<drv mrk="sufiks.0o">
  <kap><ofc>*</ofc><tld/>o</kap>
  <uzo tip="fak">GRA</uzo>
  <snc>
    <dif>
      Vortero metata post la radiko por &scirc;an&gcirc;i &gcirc;ian sencon (<ref tip="malprt" cel="deriv.0i">derivi</ref> novan vorton).
    </dif>
    <ref tip="vid" cel="fin.0ajxo.GRA">fina&jcirc;o</ref>
  </snc>
  <trd lng="be">&c_s;&c_u;&c_f;&c_ib;&c_k;&c_s;</trd>
  <trd lng="cs">p&rcaron;&iacute;pona</trd>
  <trd lng="de">Suffix</trd>
  <trd lng="en">suffix</trd>
  <trd lng="es">sufijo</trd>
  <trd lng="fr">suffixe</trd>
  <trdgrp lng="nl">
    <trd>achtervoegsel</trd>,
    <trd>suffix</trd>
  </trdgrp>
  <trd lng="ru">&c_s;&c_u;&c_f;&c_f;&c_i;&c_k;&c_s;</trd>
</drv>
</art>
