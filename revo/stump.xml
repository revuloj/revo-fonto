<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: stump.xml,v 1.24 2024/03/25 07:28:37 revo Exp $">
<kap>
  <ofc>3</ofc>
  <rad>stump</rad>/o
</kap>
<drv mrk="stump.0o">
  <kap><tld/>o</kap>
  <snc mrk="stump.0o.MED">
    <uzo tip="fak">MED</uzo>
    <dif>
      Nedifektita <ref tip="super" cel="rest.0ajxo">resta&jcirc;o</ref>
      el fortran&ccirc;ita <ref cel="membr.0o.ANA">membro</ref>:
      <ekz>
        <tld/>o de amputita brako;
      </ekz>
      <ekz>
        <tld/>&scirc;trumpo estas &scirc;trumpo, kiun amputitaj uloj portas sur <tld/>o
        <fnt>
          <vrk><url ref="&Viki;/Stump%C5%9Dtrumpo">Stump&scirc;trumpo</url></vrk>
        </fnt>.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_b;&c_r;&c_u;&c_b;&c_a;&c_k;</trd>
    <trd lng="ca">mony&oacute;</trd>
    <trd lng="de">Stumpf</trd>
    <trd lng="en">stump</trd>
    <trd lng="es">mu&ntilde;&oacute;n</trd>
    <trd lng="fr">moignon</trd>
    <trd lng="ru">&c_o;&c_b;&c_r;&c_u;&c_b;&c_o;&c_k;</trd>
  </snc>
  <snc mrk="stump.0o.FIG">
    <dif>
      Mallonga <ref tip="super" cel="part.0o.KOMUNE">parto</ref>
      restanta el io, kies plej grandan parton oni
      fortran&ccirc;is a&ubreve; fordetruis:
      <ekz>
        <tld/>o de trunko, de bran&ccirc;o;
      </ekz>
      <ekz>
        kelkloke elstaris ankora&ubreve; la <tld/>oj de malmultaj arbgigantoj, seki&gcirc;ante
        kaduki&gcirc;ante kiel la turoj de sablokasteloj
        <fnt>
          <aut>Michael Ende, trad. Wolfram Diestel</aut>
          <vrk>La Sen&ccirc;esa Rakonto</vrk>
          <lok>Goabo, la dezerto de la Koloroj</lok>
        </fnt>;
      </ekz>
      <ekz>
        fasko da vakskandelaj <tld/>etoj
        <fnt>
          <bib>Fab2</bib>
          <lok>Malnova strata lanterno</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind><tld/>eto</ind> de forbrulinta alumeto
        <fnt>
          <bib>Fab2</bib>
          <lok>Knabineto kun alumetoj</lok>
        </fnt>;
        <trd lng="de">Stummel</trd>
      </ekz>
      <ekz>
        <tld/>eto de cigaredo
        <fnt>
          <bib>Monato</bib>
          <aut>Albisturo Kvinke</aut>
          <vrk><url ref="&Monato;/2006/008788.php">Tenebro</url></vrk>
          <lok>2006</lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="stopl.0o">stoplo</ref>
    </refgrp>

   </snc>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_r;&c_u;&c_b;&c_a;&c_k;</trd>,
      <trd>&c_p;&c_a;&c_r;&c_e;&c_sh;&c_t;&c_k;&c_ib;</trd>,
      <trd>&c_r;&c_e;&c_sh;&c_t;&c_k;&c_a;</trd>,
      <trd>&c_a;&c_b;&c_l;&c_o;&c_m;&c_a;&c_k;</trd>,
      <trd>&c_n;&c_je;&c_d;&c_a;&c_p;&c_a;&c_l;&c_a;&c_k;</trd>,
      <trd>&c_a;&c_g;&c_r;&c_y;&c_z;&c_a;&c_k;</trd>,
      <trd>&c_a;&c_g;&c_a;&c_r;&c_a;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="cs">
        <trd>pah&yacute;l</trd>,
        <trd>pa&rcaron;ez</trd>,
        <trd>zbytek</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Stumpf</trd>,
      <trd>Stummel</trd>,
      <trd>Strunk</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>stump</trd>,
      <trd>butt <klr>(precipe se p.p. cigaredresta&jcirc;o)</klr></trd>
    </trdgrp>
    <trdgrp lng="fr">
      <trd>chicot</trd>,
      <trd>m&eacute;got</trd>,
      <trd>reste<klr> (d'un objet arrach&eacute;)</klr></trd>,
      <trd>souche</trd>,
      <trd>trognon</trd>,
      <trd>talon<klr> (de billet, de ch&egrave;que)</klr></trd>
    </trdgrp>
    <trd lng="hu">csonk</trd>
    <trdgrp lng="ja">
      <trd>&#x5207;&#x682A;  <pr>&#x304D;&#x308A;&#x304B;&#x3076;</pr></trd>,
      <trd>&#x5207;&#x6B8B;&#x308A;  <pr>&#x305B;&#x3064;&#x306E;&#x3053;&#x308A;</pr></trd>,
      <trd>&#x5207;&#x308C;&#x7AEF;  <pr>&#x304D;&#x308C;&#x306F;&#x3057;</pr></trd>,
      <trd>&#x534A;&#x5238;  <pr>&#x306F;&#x3093;&#x3051;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">stomp</trd>
  <trdgrp lng="pl">
      <trd>kikut</trd>,
      <trd>niedopa&lstroke;ek</trd>,
      <trd>ogarek</trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_o;&c_m;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_u;&c_b;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_g;&c_a;&c_r;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_k;&c_u;&c_r;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_g;&c_r;&c_y;&c_z;&c_o;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="sk">
      <trd>k&yacute;pe&tcaron;</trd>,
      <trd>pah&yacute;&lcaron;</trd>,
      <trd>zostatok</trd>
    </trdgrp>
    <trdgrp lng="uk">
      <trd>&c_u;&#x43B;&c_a;&#x43C;&#x43E;&#x43A;</trd>,
      <trd>&#x43E;&c_b;&c_r;&c_u;&c_b;&#x43E;&#x43A;</trd>,
      <trd>&#x43E;&c_c;&c_u;&#x43F;&#x43E;&#x43A;</trd>,
      <trd>&c_v;&c_ib;&c_d;&c_r;&c_ib;&c_z;&#x43E;&#x43A;</trd>,
      <trd>&c_v;&c_ib;&c_d;&c_t;&c_i;&#x43D;&#x43E;&#x43A;</trd>,
      <trd>&#x43D;&c_je;&c_d;&#x43E;&c_g;&c_r;&c_i;&c_z;&#x43E;&#x43A;</trd>,
      <trd>&#x43D;&c_je;&c_d;&#x43E;&#x43A;&c_u;&c_r;&#x43E;&#x43A;</trd>,
      <trd>&#x43D;&c_je;&c_d;&#x43E;&#x43F;&c_a;&#x43B;&#x43E;&#x43A;</trd>,
      <trd>&#x43D;&c_je;&c_d;&#x43E;&c_g;&c_a;&c_r;&#x43E;&#x43A;</trd>
    </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x70DF;&#x5934; <pr>y&#x101;nt&#xF3;u</pr></trd>,
    <trd>&#x7159;&#x982D; <pr>y&#x101;nt&#xF3;u</pr></trd>,
    <trd>&#x6811;&#x6869; <pr>sh&#xF9;zhu&#x101;ng</pr></trd>,
    <trd>&#x6A39;&#x6A01; <pr>sh&#xF9;zhu&#x101;ng</pr></trd>,
    <trd>&#x5C3E;&#x6839; <pr>w&#x11B;ig&#x113;n</pr></trd>,
    <trd>&#x6B8B;&#x6839; <pr>c&#xE1;ng&#x113;n</pr></trd>,
    <trd>&#x6B98;&#x6839; <pr>c&#xE1;ng&#x113;n</pr></trd>,
    <trd>&#x6B8B;&#x6BB5; <pr>c&#xE1;ndu&#xE0;n</pr></trd>,
    <trd>&#x6B98;&#x6BB5; <pr>c&#xE1;ndu&#xE0;n</pr></trd>,
    <trd>&#x6B8B;&#x7AEF; <pr>c&#xE1;ndu&#x101;n</pr></trd>,
    <trd>&#x6B98;&#x7AEF; <pr>c&#xE1;ndu&#x101;n</pr></trd>,
    <trd>&#x6B8B;&#x80A2; <pr>c&#xE1;nzh&#x12B;</pr></trd>,
    <trd>&#x6B98;&#x80A2; <pr>c&#xE1;nzh&#x12B;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="stump.0igi">
  <kap><tld/>igi</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Grandparte forigi, lasante nur etan, bazan resta&jcirc;on:
      <ekz>
        la Arbara Le&gcirc;o de 1878 <klr>[&#x2026;]</klr> <tld/>igis la <klr>[&#x2026;]</klr> tradician uzon
        de komunumoj de iliaj arbaroj
        <fnt>
          <vrk><url ref="https://epo.wikitrans.net/Indian_Forest_Acts,_1865_and_1878">Hinda Forest
          Acts, 1865 kaj 1878</url></vrk>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="ca">amputar</trd>
  <trdgrp lng="cs">
    <trd>odd&ecaron;lit</trd>,
    <trd>otupit</trd>,
    <trd>vytvo&rcaron;it pah&yacute;l</trd>,
    <trd>zkr&aacute;tit</trd>
  </trdgrp>
  <trd lng="de">verst&uuml;mmeln</trd>
  <trd lng="es">amputar</trd>
  <trd lng="fr">amputer</trd>
  <trdgrp lng="hu">
      <trd>csonkol</trd>,
      <trd>amput&aacute;l</trd>
    </trdgrp>
  <trdgrp lng="ja">
    <trd>&#x5207;&#x65AD;&#x3059;&#x308B;  <pr>&#x305B;&#x3064;&#x3060;&#x3093;&#x3059;&#x308B;</pr></trd>,
    <trd>&#x5207;&#x308A;&#x843D;&#x3068;&#x3059;  <pr>&#x304D;&#x308A;&#x304A;&#x3068;&#x3059;</pr></trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>okaleczy&cacute;</trd>,
    <trd>przycina&cacute;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>otupi&tcaron;</trd>,
    <trd>skr&aacute;ti&tcaron;</trd>,
    <trd>vytvori&tcaron; pah&yacute;&lcaron;</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&c_u;&#x43A;&#x43E;&c_r;&#x43E;&c_t;&c_i;&c_t;&c_i;</trd>,
    <trd>&c_u;&c_r;&c_ib;&c_z;&c_a;&c_t;&c_i;</trd>,
    <trd>&#x43E;&c_b;&#x43B;&c_a;&#x43C;&c_a;&c_t;&c_i;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x4F7F;&#x4E0D;&#x5B8C;&#x6574; <pr>sh&#x1D0;b&#xF9;w&#xE1;nzh&#x11B;ng</pr></trd>,
    <trd>&#x4F7F;&#x6B8B;&#x5E9F; <pr>sh&#x1D0;c&#xE1;nf&#xE8;i</pr></trd>,
    <trd>&#x4F7F;&#x6B98;&#x5EE2; <pr>sh&#x1D0;c&#xE1;nf&#xE8;i</pr></trd>
  </trdgrp>
</drv>
