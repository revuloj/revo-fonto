<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>stump</rad>/o
</kap>
<drv mrk="stump.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Nedifektita resta&jcirc;o el fortran&ccirc;ita membro:
      <ekz>
        <tld/>o de amputita brako.
      </ekz>
    </dif>
    <trd lng="be">&c_a;&c_b;&c_r;&c_u;&c_b;&c_a;&c_k;</trd>
    <trd lng="de">Stumpf</trd>
    <trd lng="en">stump</trd>
    <trd lng="fr">moignon</trd>
    <trd lng="ru">&c_o;&c_b;&c_r;&c_u;&c_b;&c_o;&c_k;</trd>
  </snc>
  <snc>
    <dif>
      Mallonga parto restanta el io, kies plej grandan parton oni
      fortran&ccirc;is a&ubreve; fordetruis:
      <ekz>
        <tld/>o de kandelo<fnt>Z</fnt>;
      </ekz>
      <ekz>
        cigar<tld/>o;
      </ekz>
      <ekz>
        <tld/>o de trunko, de bran&ccirc;o;
      </ekz>
      <ekz>
        <tld/>ita vorto, <tld/>vorto;
      </ekz>
      <ekz>
        vakskandelaj <tld/>etoj<fnt>Z</fnt>;
      </ekz>
      <ekz>
        <tld/>eto de forbrulinta alumeto<fnt>Z</fnt>.
      </ekz>
    </dif>
    <refgrp tip="sin">
      <ref cel="stopl.0o">stoplo</ref>,
      <ref cel="sxtip.0o">&scirc;tipo</ref>,
      <ref cel="trunk.0o">trunko</ref>,
      <ref cel="kripl.0a">kripla</ref>,
      <ref cel="invali">invalida</ref>.
    </refgrp>
    <trdgrp lng="be">
      <trd>&c_a;&c_b;&c_r;&c_u;&c_b;&c_a;&c_k;</trd>, 
      <trd>&c_p;&c_a;&c_r;&c_e;&c_sh;&c_t;&c_k;&c_ib;</trd>, 
      <trd>&c_r;&c_e;&c_sh;&c_t;&c_k;&c_a;</trd>, 
      <trd>&c_a;&c_b;&c_l;&c_o;&c_m;&c_a;&c_k;</trd>, 
      <trd>&c_n;&c_je;&c_d;&c_a;&c_p;&c_a;&c_l;&c_a;&c_k;</trd>, 
      <trd>&c_a;&c_g;&c_r;&c_y;&c_z;&c_a;&c_k;</trd>, 
      <trd>&c_a;&c_g;&c_a;&c_r;&c_a;&c_k;</trd>
    </trdgrp>
    <trdgrp lng="de">
      <trd>Stumpf</trd>,
      <trd>Stummel</trd>,
      <trd>Strunk</trd>
    </trdgrp>
    <trdgrp lng="en">
      <trd>stump</trd>,
      <trd>butt <klr>(precipe se p.p. cigaredrestajxo)</klr></trd>
    </trdgrp>
    <trdgrp lng="ru">
      <trd>&c_o;&c_s;&c_t;&c_a;&c_t;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_b;&c_l;&c_o;&c_m;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_b;&c_r;&c_u;&c_b;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_g;&c_a;&c_r;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_k;&c_u;&c_r;&c_o;&c_k;</trd>,
      <trd>&c_o;&c_g;&c_r;&c_y;&c_z;&c_o;&c_k;</trd>
    </trdgrp>
  </snc>
  <trd lng="nl">stomp</trd>
</drv>
</art>
