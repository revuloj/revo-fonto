<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>budh</rad>/o
    </kap>
    <drv mrk="budh.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          <ref tip="dif" cel="buda.0o">Budao</ref>.
        </dif>
      </snc>
    <trd lng="en">buddha</trd>
    </drv>
    <drv mrk="budh.0ismo">
      <kap><tld/>ismo</kap>
      <snc>
        <dif>
          <ref tip="dif" cel="buda.0ismo">Budaismo</ref>.
        </dif>
      </snc>
    <trd lng="en">buddhism</trd>
    </drv>
    <drv mrk="budh.0isto">
      <kap><tld/>isto, <tld/>ano</kap>
      <snc>
        <dif>
          <ref tip="dif" cel="buda.0isto">Budaisto</ref>.
        </dif>
      </snc>
    <trd lng="en">buddhist</trd>
    </drv>
  </art>

