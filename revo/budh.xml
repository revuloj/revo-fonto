<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
  <art mrk="$Id$">
    <kap>
      <rad>budh</rad>/o
    </kap>
    <drv mrk="budh.0o">
      <kap><tld/>o</kap>
      <snc>
        <dif>
          <ref tip="dif" cel="buda.0o">Budao</ref>.
        </dif>
      </snc>
      <trdgrp lng="be">
        <trd>&c_b;&c_u;&c_d;&c_a;</trd>,
        <trd>&c_B;&c_u;&c_d;&c_a;</trd>
      </trdgrp>
      <trd lng="ca">Buda</trd>
    <trd lng="en">buddha</trd>
      <trd lng="es">Buda</trd>
    </drv>
    <drv mrk="budh.0ismo">
      <kap><tld/>ismo</kap>
      <snc>
        <dif>
          <ref tip="dif" cel="buda.0ismo">Budaismo</ref>.
        </dif>
      </snc>
      <trd lng="be">&c_b;&c_u;&c_d;&c_y;&c_z;&c_m;</trd>
      <trd lng="ca">budisme</trd>
    <trd lng="en">buddhism</trd>
      <trd lng="es">budismo</trd>
    </drv>
    <drv mrk="budh.0isto">
      <kap><tld/>isto, <tld/>ano</kap>
      <snc>
        <dif>
          <ref tip="dif" cel="buda.0isto">Budaisto</ref>.
        </dif>
      </snc>
      <trd lng="be">&c_b;&c_u;&c_d;&c_y;&c_s;&c_t;</trd>
      <trd lng="ca">budista</trd>
    <trd lng="en">buddhist</trd>
      <trd lng="es">budista</trd>
    </drv>
  </art>

