<drv mrk="impost.0o">
  <kap><tld/>o</kap>
  <fnt><bib>IKEV</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>BEE</bib></fnt>
  <snc mrk="impost.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Deviga kontribuo de regnanoj al la &gcirc;eneralaj bezonoj de
      &scirc;tato, urbo ktp:
      <ekz>
        pagi pezajn <tld/>ojn;
      </ekz>
      <ekz>
        pagi sian <tld/>on per mono, per natura&jcirc;oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akciz.0o">akcizo</ref>,
      <ref cel="pag.de0o">depago</ref>,
      <ref cel="dogan.0o">dogano</ref>,
      <ref cel="tribut.0o">tributo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="cs">
    <trd>daň</trd>,
    <trd>někoho)</trd>,
    <trd>poplatek <klr>( z něčeho)</klr></trd>,
    <trd>zdanění (něčeho</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Abgabe</trd>,
    <trd>Steuer</trd>
  </trdgrp>
  <trd lng="en">tax</trd>
  <trd lng="es">impuesto</trd>
  <trd lng="fr">imp&ocirc;t</trd>
  <trdgrp lng="hu">
    <trd>ad&oacute;</trd>,
    <trd>illet&eacute;k</trd>
  </trdgrp>
  <trd lng="id">pajak</trd>
  <trd lng="nl">belasting</trd>
  <trd lng="pl">podatek</trd>
  <trd lng="pt">imposto<klr tip="amb"> (economia, com&eacute;rcio)</klr>
  </trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_l;&c_o;&c_g;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_sh;&c_l;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>daň</trd>,
    <trd>poplatok</trd>
  </trdgrp>
</drv>

<drv mrk="impost.0a">
  <kap><tld/>a</kap>
  <snc mrk="impost.0a.EKON">
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        donacantoj al la asocio &gcirc;uas
        <ind><tld/>an avanta&gcirc;on</ind>
        <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_v;&c_u;&c_ju; &c_l;&c_mol;&c_g;&c_o;&c_t;&c_u;</trd>
        <fnt>
          <aut>K. Schl&uuml;ter</aut>:
          <vrk>Kulturtago de EMAS</vrk>.
          <bib>LOdE</bib>, <lok>2003:12 (110)</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u venos fino de la <tld/>aj paradizoj?
        <fnt><bib>Monato</bib></fnt>;
      </ekz>
      <ekz>
        certigi, ke la <ind><tld/>a pezo</ind> distribui&gcirc;os juste
        <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_v;&c_a;&c_ja; &c_n;&c_a;&c_g;&c_r;&c_u;&c_z;&c_k;&c_a;</trd>
        <fnt><bib>Monato</bib></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>tax<klr> (attr.)</klr></trd>,
    <trd>fiscal</trd>
  </trdgrp>
  <trd lng="fr">fiscal</trd>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="impost.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <fnt><bib>CCT</bib></fnt>
  <fnt><bib>IKEV</bib></fnt>
  <snc mrk="impost.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Le&gcirc;e meti <tld/>on pri iu a&ubreve; io:
      <ekz>
        <tld/>i vinon, alkohola&jcirc;ojn;
      </ekz>
      <ekz>
        <tld/>i fortan depagon al provinco, lando.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>zdanit</trd>,
    <trd>zpoplatnit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>besteuern</trd>,
    <trd>Steuer <ind>erheben</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>charge a tax</trd>,
    <trd>levy a <ind>tax</ind></trd>,
    <trd>tax</trd>,
    <trd>assess</trd>
  </trdgrp>
  <trd lng="es">gravar con <ind>impuesto</ind></trd>
  <trdgrp lng="fr">
    <trd>imposer<klr tip="ind"> (fin.)</klr></trd>,
    <trd>taxer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megad&oacute;ztat</trd>,
    <trd>ad&oacute;t vet ki</trd>
  </trdgrp>
  <trd lng="nl">belasten<klr tip="ind"> (belasting
  heffen)</klr></trd>
  <trdgrp lng="pl">
    <trd>opodatkowa&cacute;</trd>,
    <trd>nak&lstroke;ada&cacute; <ind>podatek</ind></trd>,
    <trd>ok&lstroke;ada&cacute; <ind>podatkiem</ind></trd>
  </trdgrp>
  <trd lng="pt">onerar com <ind>imposto</ind></trd>
  <trd lng="ru">&c_o;&c_b;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;
    <ind>&c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_m;</ind></trd>
  <trd lng="sk">zdaniť</trd>
</drv>

