<drv mrk="impost.lim0o">
  <kap>lim<tld/>o</kap>
  <fnt><bib>BEE</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="impost.lim0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <ref tip="dif" cel="dogan.0o.imposto">dogano<sncref/></ref>
  </snc>
  <trd lng="cs">clo</trd>
  <trdgrp lng="de">
    <trd>Zollgeb&uuml;hr</trd>,
    <trd>Zoll</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>droit de <ind>douane</ind></trd>,
    <trd>douane<klr tip="ind"> (droit de douane)</klr></trd>
  </trdgrp>
  <trd lng="hu">v&aacute;millet&eacute;k</trd>
  <trd lng="nl">tol</trd>
  <trdgrp lng="pl">
    <trd>c&lstroke;o</trd>,
    <trd>podatek graniczny</trd>
  </trdgrp>
  <trd lng="pt">imposto aduaneiro</trd>
  <trd lng="ru">&c_t;&c_a;&c_m;&c_o;&c_zh;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_sh;&c_l;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>clo</trd>,
    <trd>hraničný poplatok</trd>,
    <trd>mýtne</trd>
  </trdgrp>
</drv>

<drv mrk="impost.pont0o">
  <kap>pont<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="impost.pont0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="I"/>o pagenda por la uzo de
      <ref tip="dif" cel="pont.0o">ponto</ref>.
    </dif>
  </snc>
  <trd lng="cs">mostné</trd>
  <trdgrp lng="de">
    <trd>Br&uuml;ckenzoll</trd>
  </trdgrp>
  <trd lng="fr">p&eacute;age de pont</trd>
  <trd lng="hu">h&iacute;dv&aacute;m</trd>
  <trd lng="nl">bruggentol</trd>
  <trdgrp lng="pl">
    <trd>podatek mostowy</trd>,
    <trd>myto</trd>
  </trdgrp>
  <trd lng="ru">&c_m;&c_o;&c_s;&c_t;&c_o;&c_v;&c_o;&c_j; &c_s;&c_b;&c_o;&c_r;</trd>
  <trd lng="sk">mostné</trd>
</drv>

<drv mrk="impost.sal0o">
  <kap>sal<tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="impost.sal0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="I"/>o pro la vendo de <ref tip="dif"
      cel="sal.0o">salo</ref>.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>daň z dovozu soli</trd>,
    <trd>solná daň</trd>
  </trdgrp>
  <trd lng="de">Salzsteuer</trd>
  <trd lng="en">salt duty</trd>
  <trd lng="fr">gabelle</trd>
  <trd lng="hu">s&oacute;ad&oacute;</trd>
  <trd lng="nl">zoutbelasting</trd>
  <trd lng="pl">podatek solny</trd>
  <trd lng="ru">&c_s;&c_o;&c_l;&c_ja;&c_n;&c_o;&c_j; &c_n;&c_a;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">soľná daň</trd>
</drv>

<drv mrk="impost.sang0o">
  <kap>sang<tld/>o</kap>
  <uzo tip="stl">FRAZ</uzo>
  <snc mrk="impost.sang0o.militservado">
    <dif>
      Devigo de la milita servado.
    </dif>
  </snc>
  <adm>
    La koncerna metaforo estas jam tre ar&hcirc;aika en la franca.
    Kiel estas en Esperanto?
    <aut>MB</aut>
  </adm>
  <trd lng="de">Wehrpflicht</trd>
  <trd lng="fr">imp&ocirc;t du sang</trd>
  <trd lng="hu">k&ouml;telez&odblac; <ind>katonai</ind>
    szolg&aacute;lat</trd>
  <trd lng="nl">dienstplicht<klr tip="ind"> (in tijde van
      oorlog)</klr></trd>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_g; &c_k;&c_r;&c_o;&c_v;&c_mol;&c_ju;</trd>
  <trd lng="sk">krvná daň</trd>
  <trd lng="cs">krvavá daň</trd>
</drv>

<drv mrk="impost.sen0a">
  <kap>sen<tld/>a</kap>
  <fnt><bib>BEE</bib></fnt>
  <snc mrk="impost.sen0a.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Libera de imposto.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>bezcelní <klr>(zóna)</klr></trd>,
    <trd>daněprostý</trd>,
    <trd>prostý daně</trd>,
    <trd>svobodný (přístav)</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>steuerfrei</trd>,
    <trd>abgabenfrei</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>duty free</trd>,
    <trd>tax free</trd>
  </trdgrp>
  <trd lng="fr">hors taxe</trd>
  <trd lng="hu">ad&oacute;mentes</trd>
  <trdgrp lng="nl">
    <trd>tolvrij</trd>,
    <trd>belastingvrij</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bezc&lstroke;owy</trd>,
    <trd>nieopodatkowany</trd>,
    <trd>zwolniony z podatku</trd>
  </trdgrp>
  <trd lng="pt">livre de <ind>imposto</ind><klr tip="ind">
      (adj.)</klr></trd>
  <trd lng="ru">&c_b;&c_je;&c_s;&c_p;&c_o;&c_sh;&c_l;&c_i;&c_n;&c_n;&c_y;&c_j;</trd>
  <trdgrp lng="sk">
    <trd>nezdaniteľný</trd>,
    <trd>oslobodený od dane</trd>
  </trdgrp>
</drv>

<drv mrk="impost.spekul0o">
  <kap>spekul<tld/>o</kap>
  <snc mrk="impost.spekul0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="I"/>o pro bil-profitoj, se tiuj estas realigitaj anta&ubreve;
      la fino de la spekultempa intervalo difinita de la le&gcirc;o.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>short-term capital gains tax</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Spekulationssteuer</trd>
  </trdgrp>
  <trd lng="fr">taxe sur la plus-value</trd>
  <trd lng="hu">spekul&aacute;ci&oacute;s <ind>ad&oacute;</ind></trd>
  <trd lng="nl">speculatiebelasting</trd>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_g; &c_n;&c_a; &c_p;&c_r;&c_i;&c_b;&c_y;&c_l;&c_mol;  &c_o;&c_t; &c_s;&c_p;&c_je;&c_k;&c_u;&c_l;&c_ja;&c_t;&c_i;&c_v;&c_n;&c_y;&c_h; &c_s;&c_d;&c_je;&c_l;&c_o;&c_k;</trd>
</drv>

<drv mrk="impost.stamp0o">
  <kap>stamp<tld/>o</kap>
  <fnt><bib>IKEV</bib></fnt>
  <snc mrk="impost.stamp0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="I"/>o pagenda kontra&ubreve; administra
      provizo de dokumento, registrado...
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kolkovné</trd>,
    <trd>správní poplatek</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stempelsteuer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>stamp duty</trd>,
    <trd>stamp tax</trd>
  </trdgrp>
  <trd lng="fr">droit d'enregistrement<klr> (timbre fiscal)</klr></trd>
  <trd lng="hu">b&eacute;lyegad&oacute;</trd>
  <trd lng="ru">&c_g;&c_je;&c_r;&c_b;&c_o;&c_v;&c_y;&c_j; &c_s;&c_b;&c_o;&c_r;</trd>
  <trd lng="sk">správny poplatok</trd>
</drv>

<drv mrk="impost.super0i">
  <kap>super<tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <fnt><bib>PIV1</bib></fnt>
  <snc mrk="impost.super0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Meti kroman <tld/>on pri io a&ubreve; iu.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>mehrfach <ind>besteuern</ind></trd>,
    <trd>doppelt besteuern</trd>,
    <trd>Steuer mehrfach <ind>erheben</ind></trd>
  </trdgrp>
  <trd lng="fr">surimposer</trd>
  <trd lng="hu">p&oacute;tad&oacute;t vet ki</trd>
  <trdgrp lng="pl">
    <trd>nak&lstroke;ada&cacute; dodatkowy <ind>podatek</ind></trd>,
    <trd>ok&lstroke;ada&cacute; dodatkowym <ind>podatkiem</ind></trd>
  </trdgrp>
  <trd lng="pt">onerar com <ind>imposto</ind> adicional</trd>
</drv>

<drv mrk="impost.ter0o">
  <kap>ter<tld/>o</kap>
  <fnt><bib>BEE</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="I"/>o pagenda pro la posedo de tereno.
    </dif>
  </snc>
  <trd lng="cs">pozemková daň</trd>
  <trdgrp lng="de">
    <trd>Grundsteuer</trd>
  </trdgrp>
  <trd lng="fr">taxe fonci&egrave;re</trd>
  <trd lng="hu">telekad&oacute;</trd>
  <trd lng="nl">grondbelasting</trd>
  <trdgrp lng="pl">
    <trd>podatek od nieruchomo&sacute;ci</trd>,
    <trd>podatek katastralny</trd>,
    <trd>kataster<klr> (pot.)</klr></trd>
  </trdgrp>
  <trd lng="pt">imposto territorial</trd>
  <trd lng="ru">&c_p;&c_o;&c_z;&c_je;&c_m;&c_je;&c_l;&c_mol;&c_n;&c_y;&c_j; &c_n;&c_a;&c_l;&c_o;&c_g;</trd>
  <trd lng="sk">pozemková daň</trd>
</drv>

<drv mrk="impost.vend0o">
  <kap>vend<tld/>o</kap>
  <fnt><bib>IKEV</bib></fnt>
  <snc mrk="impost.vend0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="I"/>o surbaze de la vendoprezo je la vendo al
      konsumanto;
      vendo-<tld/>o, spezo<tld/>o.
    </dif>
  </snc>
  <trd lng="cs">daň z prodeje</trd>
  <trdgrp lng="de">
    <trd>Umsatzsteuer</trd>,
    <trd>USt</trd>,
    <trd>Warenumsatzsteuer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>sales tax</trd>,
    <trd>turnover tax</trd>
  </trdgrp>
  <trd lng="hu">forgalmi <ind>ad&oacute;</ind></trd>
  <trd lng="nl">omzetbelasting</trd>
  <trdgrp lng="pt">
    <trd>imposto sobre circula&ccedil;&atilde;o de mercadoria, ICM</trd>
  </trdgrp>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_g; &c_s; &c_p;&c_r;&c_o;&c_d;&c_a;&c_zh;</trd>
  <trd lng="sk">daň z predaja</trd>
</drv>

<drv mrk="impost.voj0o">
  <kap>voj<tld/>o</kap>
  <fnt><bib>BEE</bib></fnt>
  <snc mrk="impost.voj0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      <tld lit="I"/>o pagenda pro la uzo de <ref tip="dif"
      cel="strat.0o">strato</ref>,
      <ref tip="dif" cel="voj.0o">vojo</ref> ktp.
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>mýtné</trd>,
    <trd>silniční daň</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Benutzungsgeb&uuml;hr</trd>,
    <trd>Mautgeb&uuml;hr</trd>,
    <trd>Maut</trd>,
    <trd>Wegesteuer</trd>,
    <trd>Stra&szlig;engeb&uuml;hr</trd>,
    <trd>Stra&szlig;ensteuer</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>toll</trd>
  </trdgrp>
  <trd lng="fr">p&eacute;age</trd>
  <trdgrp lng="hu">
    <trd>&uacute;tad&oacute;</trd>,
    <trd>&uacute;thaszn&aacute;lati <ind>ad&oacute;</ind></trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tol<klr tip="ind"> (wegenbelasting)</klr></trd>,
    <trd>wegenbelasting</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podatek drogowy</trd>,
    <trd>myto<klr> (dawn.)</klr></trd>
  </trdgrp>
  <trd lng="pt">ped&aacute;gio</trd>
  <trd lng="ru">&c_p;&c_o;&c_d;&c_o;&c_r;&c_o;&c_zh;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_sh;&c_l;&c_i;&c_n;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>cestná daň</trd>,
    <trd>mýtne</trd>
  </trdgrp>
</drv>
