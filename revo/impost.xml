    <trd>&#x8CA1;&#x653F;&#x7A05;&#x52D9;&#x6A5F;&#x95DC; <pr>c&#xE1;izh&#xE8;ngshu&#xEC;w&#xF9;j&#x12B;gu&#x101;n</pr></trd>
  </trdgrp>
</drv>

<drv mrk="impost.0isto">
  <kap><tld/>isto <fnt><bib>MT</bib></fnt></kap>
  <snc mrk="impost.0isto.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Persono kolektanta <tld/>ojn:
      <ekz>
        jen viro, nomata Zak&hcirc;eo, kaj li estis &ccirc;ef<tld/>isto, kaj li estis ri&ccirc;a
        <fnt>
          <bib>NT</bib>
          <lok>Luko 19:2</lok>
        </fnt>;
      </ekz>
      <ekz>
        jen man&gcirc;egulo kaj vindrinkulo, amiko de <tld/>istoj kaj pekuloj
        <fnt>
          <bib>NT</bib>
          <lok>Luko 7:34</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_z;&c_b;&c_o;&c_r;&c_sh;&c_ch;&c_y;&c_k; <ind>&c_p;&c_a;&c_d;&c_a;&c_t;&c_k;&c_a;&c_w;</ind></trd>
  <trd lng="cs">v&yacute;b&ecaron;r&ccaron;&iacute; dan&iacute;</trd>
  <trdgrp lng="de">
    <trd>Steuereintreiber</trd>,
    <trd>Z&ouml;llner</trd>
  </trdgrp>
  <trd lng="en">tax collector</trd>
  <trd lng="fr">percepteur<klr> (fin.)</klr></trd>
  <trd lng="hu">ad&oacute;szed&odblac;</trd>
