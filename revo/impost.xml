<drv mrk="impost.pont0o">
  <kap>pont<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Imposto pagenda por la uzo de ponto.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Br&uuml;ckenzoll</trd>
  </trdgrp>
</drv>

<drv mrk="impost.sal0o">
  <kap>sal<tld/>o</kap>
