<drv mrk="impost.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>BEE</bib></fnt>
  <snc>
    <dif>
      Deviga kontribuo de regnanoj al la &gcirc;eneralaj bezonoj de
      &scirc;tato, urbo ktp:
      <ekz>
        pagi pezajn <tld/>ojn;
      </ekz>
      <ekz>
        pagi sian <tld/>on per mono, per natura&jcirc;oj.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="tribut.0o">tributo</ref>,
      <ref cel="akciz.0o">akcizo</ref>,
      <ref cel="dogan.0o">dogano</ref>,
      <ref cel="pag.de0o">depago</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="de">
    <trd>Abgabe</trd>,
    <trd>Steuer</trd>
  </trdgrp>
  <trd lng="en">tax</trd>
  <trd lng="es">impuesto</trd>
  <trd lng="fr">imp&ocirc;t</trd>
  <trdgrp lng="hu">
    <trd>ad&oacute;</trd>,
    <trd>illet&eacute;k</trd>
  </trdgrp>
  <trd lng="nl">belasting</trd>
  <trd lng="pl">podatek</trd>
  <trd lng="pt">imposto<klr tip="amb"> (economia, com&eacute;rcio)</klr>
  </trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_l;&c_o;&c_g;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_sh;&c_l;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
</drv>

<drv mrk="impost.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>CCT</bib></fnt>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Le&gcirc;e meti <tld/>on sur iun a&ubreve; ion:
      <ekz>
        <tld/>i vinon, alkohola&jcirc;ojn;
      </ekz>
      <ekz>
        <tld/>i fortan depagon al provinco, lando.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>charge a tax</trd>,
    <trd>levy a <ind>tax</ind></trd>,
    <trd>tax</trd>,
    <trd>assess</trd>
  </trdgrp>
  <trd lng="es">gravar con <ind>impuesto</ind></trd>
  <trdgrp lng="de">
    <trd>besteuern</trd>,
    <trd>Steuer <ind>erheben</ind></trd>
  </trdgrp>
  <trd lng="fr">imposer<klr tip="ind"> (fin.)</klr></trd>
  <trdgrp lng="hu">
    <trd>megad&oacute;ztat</trd>,
    <trd>ad&oacute;t vet ki</trd>
  </trdgrp>
  <trd lng="nl">belasten<klr tip="ind"> (belasting heffen)</klr></trd>
  <trdgrp lng="pl">
    <trd>opodatkowa&cacute;</trd>,
    <trd>nak&lstroke;ada&cacute; <ind>podatek</ind></trd>,
    <trd>ok&lstroke;ada&cacute; <ind>podatkiem</ind></trd>
  </trdgrp>
  <trd lng="pt">onerar com <ind>imposto</ind></trd>
  <trd lng="ru">&c_o;&c_b;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;
  <ind>&c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_m;</ind></trd>
</drv>

