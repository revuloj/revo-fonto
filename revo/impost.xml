  <snc>
    <dif>
      Libera de imposto.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>steuerfrei</trd>,
    <trd>abgabenfrei</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>duty free</trd>,
    <trd>tax free</trd>
  </trdgrp>
</drv>

<drv mrk="impost.spekul0o">
  <kap>spekul<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
