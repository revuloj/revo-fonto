    <trd>toll</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Benutzungsgeb&uuml;hr</trd>,
    <trd>Mautgeb&uuml;hr</trd>,
    <trd>Maut</trd>,
    <trd>Wegesteuer</trd>,
    <trd>Stra&szlig;engeb&uuml;hr</trd>,
    <trd>Stra&szlig;ensteuer</trd>
  </trdgrp>
</drv>

