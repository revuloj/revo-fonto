<drv mrk="impost.0o.AVI">
  <kap>aldonvalora <tld/>o</kap>
  <mlg>AVI</mlg>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      &Gcirc;enerala vendo-<tld/>o nete kalkulata surbaze de la
      valorkreska&jcirc;o sur &ccirc;iu unuopa produkto- a&ubreve;
      komerco-nivelo:
      <ekz>
        en 1986 &gcirc;i enkondukis aldonvaloran <tld/>on je &ccirc;iuj
        enlandaj konsumoj
        <fnt>
        <aut>Donald Rogers</aut>:
        <vrk>
        <url ref="&Monato;/005411.html">Ekonomia miraklo a&ubreve;
        malsano?</url>
        </vrk>,
        <bib>Monato</bib>,
        <lok>n-ro 02/1997</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Mehrwertsteuer</trd>,
    <trd>MWSt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>value added <ind>tax</ind></trd>,
    <trd>VAT</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>impuesto sobre el valor a&ntilde;adido</trd>,
    <trd>impuesto al valor agregado</trd>,
    <trd>IVA</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>taxe sur la valeur ajout&eacute;e</trd>,
    <trd>TVA</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rt&eacute;kt&ouml;bbletad&oacute;</trd>,
    <trd>&aacute;ltal&aacute;nos forgalmi <ind>ad&oacute;</ind></trd>,
    <trd>&aacute;fa</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>belasting over de toegevoegde waarde</trd>,
    <trd>BTW</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podatek od warto&sacute;ci dodanej</trd>,
    <trd>VAT</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>imposto sobre circulac&atilde;o de mercadorias</trd>,
    <trd>ICM</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>
    &c_n;&c_a;&c_l;&c_o;&c_g; &c_n;&c_a;
    &c_d;&c_o;&c_b;&c_a;&c_v;&c_l;&c_je;&c_n;&c_n;&c_u;&c_ju;
    &c_s;&c_t;&c_o;&c_i;&c_m;&c_o;&c_s;&c_t;&c_mol;
    </trd>,
    <trd>&c_N;&c_D;&c_S;</trd>
  </trdgrp>
</drv>

