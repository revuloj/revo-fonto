<drv mrk="impost.vend0o">
  <kap>vend<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
      Imposto surbaze de la vendoprezo je la vendo al konsumanto.
      Vendo-imposto, spezoimposto.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>sales tax</trd>,
    <trd>turnover tax</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Umsatzsteuer</trd>,
    <trd>USt</trd>,
    <trd>Warenumsatzsteuer</trd>
  </trdgrp>
  <trd lng="nl">omzetbelasting</trd>
</drv>

<drv mrk="impost.voj0o">
  <kap>voj<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>BEE</bib></fnt>
  <snc>
    <dif>
      Imposto pagenda por la uzo de <ref tip="dif"
      cel="strat.0o">strato</ref>,
      <ref tip="dif" cel="voj.0o">vojo</ref> ktp.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>toll</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Benutzungsgeb&uuml;hr</trd>,
    <trd>Mautgeb&uuml;hr</trd>,
    <trd>Maut</trd>,
    <trd>Wegesteuer</trd>,
    <trd>Stra&szlig;engeb&uuml;hr</trd>,
    <trd>Stra&szlig;ensteuer</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tol<klr tip="ind"> (wegenbelasting)</klr></trd>,
    <trd>wegenbelasting</trd>
  </trdgrp>
</drv>
</art>
