  </trdgrp>
  <trdgrp lng="fr">
    <trd>droit de <ind>douane</ind></trd>,
    <trd>douane<klr tip="ind"> (droit de douane)</klr></trd>
  </trdgrp>
  <trd lng="nl">tol</trd>
  <trd lng="ru">&c_t;&c_a;&c_m;&c_o;&c_zh;&c_je;&c_n;&c_n;&c_a;&c_ja; &c_p;&c_o;&c_sh;&c_l;&c_i;&c_n;&c_a;</trd>
</drv>

<drv mrk="impost.pont0o">
  <kap>pont<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Imposto pagenda por la uzo de <ref tip="dif"
      cel="pont.0o">ponto</ref>.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Br&uuml;ckenzoll</trd>
  </trdgrp>
  <trd lng="nl">bruggentol</trd>
</drv>

<drv mrk="impost.sal0o">
  <kap>sal<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
      Imposto sur la vendo de <ref tip="dif" cel="sal.0o">salo</ref>.
    </dif>
  </snc>
  <trd lng="de">Salzsteuer</trd>
  <trd lng="en">salt duty</trd>
  <trd lng="nl">zoutbelasting</trd>
</drv>

<drv mrk="impost.sang0o">
  <kap>sang<tld/>o</kap>
  <snc>
    <dif>
      Devigo de la milita servado.
    </dif>
  </snc>
  <adm>
    La koncerna metaforo estas jam tre ar&hcirc;aika en la franca.
    Kiel estas en Esperanto?
    <aut>MB</aut>
  </adm>
  <trd lng="de">Wehrpflicht</trd>
  <trd lng="fr">imp&ocirc;t du sang</trd>
  <trd lng="nl">dienstplicht<klr tip="ind"> (in tijde van
  oorlog)</klr></trd>
</drv>

<drv mrk="impost.sen0a">
  <kap>sen<tld/>a</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>BEE</bib></fnt>
  <snc>
    <dif>
      Libera de imposto.
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>steuerfrei</trd>,
    <trd>abgabenfrei</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>duty free</trd>,
    <trd>tax free</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>tolvrij</trd>,
    <trd>belastingvrij</trd>
  </trdgrp>
</drv>

<drv mrk="impost.spekul0o">
  <kap>spekul<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <snc>
    <dif>
      Imposto sur bil-profitoj, se tiuj estas realigitaj anta&ubreve; la fino de
      la spekultempa intervalo difinita de la le&gcirc;o.
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>short-term capital gains tax</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Spekulationssteuer</trd>
  </trdgrp>
  <trd lng="nl">speculatiebelasting</trd>
</drv>

<drv mrk="impost.stamp0o">
  <kap>stamp<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>IKEV</bib></fnt>
  <snc>
    <dif>
    </dif>
  </snc>
  <trdgrp lng="en">
    <trd>stamp duty</trd>,
    <trd>stamp tax</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Stempelsteuer</trd>
  </trdgrp>
</drv>

<drv mrk="impost.ter0o">
  <kap>ter<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>BEE</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <dif>
