<drv mrk="impost.0o">
  <kap><tld/>o</kap>
  <fnt><bib>IKEV</bib> </fnt>
  <fnt><bib>PIV1</bib> </fnt>
  <fnt><bib>BEE</bib></fnt>
  <snc mrk="impost.0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Deviga <ref cel="kontri.0i">kontribu</ref>o de regnanoj al la &gcirc;eneralaj bezonoj de
      <ref tip="malprt" cel="sxtat.0o">&scirc;tato</ref>, urbo ktp:
      <ekz>
        pagi sian <tld/>on per mono, per natura&jcirc;oj;
      </ekz>
      <ekz>
        la re&gcirc;o Salomono prenis <tld/>on de la tuta <nom>Izrael</nom>, la <tld/>o estis tridek mil
        viroj.
        <fnt>
          <bib>MT</bib>
          <lok>I. Re&gcirc;oj 5:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        li surmetis tro pezajn <tld/>ojn
        <fnt>
          <bib>Fab4</bib>
          <lok>Sonorila profundo</lok>
        </fnt>;
      </ekz>
      <ekz>
        se tiu urbo estos konstruita kaj la muregoj estos finitaj, tiam ili ne donos
        tributon nek <tld/>ojn nek jarpagon
        <fnt>
          <bib>MT</bib>
          <lok>Ezra 4:13</lok>
        </fnt>;
      </ekz>
      <ekz>
        la kamparanojn &scirc;ar&gcirc;u per nova <tld/>o, &ccirc;iun po unu dra&hcirc;mo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XVI</lok>
        </fnt>;
      </ekz>
      <ekz>
        malpli da <tld/>oj alfluas al la trezorejo
        <fnt>
          <bib>Far3</bib>
          <lok>&Ccirc;apitro XI</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj el produktiva laboro
        <fnt>
          
          <aut>Bertil ENGLUND</aut>
          <vrk><url ref="&Monato;/2000/007035.php">Elefanto en
          porcelanbutiko</url></vrk>
          <bib>Monato</bib>
          <lok>2000/07, p. 4</lok>
        </fnt>;
      </ekz>
      <ekz>
        oni devas pagi jaran <tld/>on al la urba administracio nur pro tio, ke oni la
        domon posedas
        <fnt>
          <aut>Remigiusz Mielcarek</aut>
          
          <vrk><url ref="&Monato;/1994/A940101.php">Enmigrintoj en ora ka&gcirc;o</url></vrk>
          <bib>Monato</bib>
          <lok>1994/01, p. 15</lok>
        </fnt>.
      </ekz>
      <ekz>
        plialtigi <tld/>ojn por socia asekuro
        <fnt>
          
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2000/006806.php">Litovio: &Ccirc;iea &scirc;parado</url></vrk>
          <bib>Monato</bib>
          <lok>2000/05, p. 7</lok>
        </fnt>;
      </ekz>
      <ekz>
        <tld/>oj pro cigaredoj en Britio atingas 80 procentojn de la prezo a&ubreve; e&ccirc; pli
        <fnt>
          <bib>Monato</bib>
          <aut>Laimius Stra&zcaron;nickas</aut>
          <vrk><url ref="&Monato;/2000/006830.php">LITOVIO -- BRITIO:
          Kontrabanditaj cigaredoj invadas Brition</url></vrk>
          <lok>2000/04, p. 13</lok>
        </fnt>;
      </ekz>
      <ekz>
        <uzo tip="fak">EKON</uzo>
        <ind><tld/>evitulo</ind>
        <klr>(iu ne paganta la fiksitan <tld/>on)</klr>
        <fnt><bib>BEE</bib></fnt>;
        <trdgrp lng="de">
          <trd>Steuerhinterzieher</trd>
        </trdgrp>
        <trd lng="en">tax dodger</trd>
        <trd lng="es">evasor de <ind>impuestos</ind></trd>
        <trd lng="fr">fraudeur fiscal</trd>
        <trd lng="hu">ad&oacute;csal&oacute;</trd>
        <trd lng="nl">belastingontduiker</trd>
        <trd lng="pt">sonegador de <ind>impostos</ind></trd>
        <trdgrp lng="ru">
          <trd>&c_n;&c_je;&c_p;&c_l;&c_a;&c_t;&c_je;&c_l;&c_mol;&c_shch;&c_i;&c_k;</trd>,
          <trd>&c_l;&c_i;&c_c;&c_o;, &c_u;&c_k;&c_l;&c_o;&c_n;&c_ja;&c_ju;&c_shch;&c_je;&c_je;&c_s;&c_ja; &c_o;&c_t; &c_u;&c_p;&c_l;&c_a;&c_t;&c_y; &c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_v;</trd>
        </trdgrp>
      </ekz>
      <ekz>
        <tld/>o-kvoto
        <fnt>
          
          <aut>Hektor Alos i Font</aut>
          <vrk><url ref="&Monato;/2006/008812.php">Survoje al pli granda memregado</url></vrk>
          <bib>Monato</bib>
          <lok>2006</lok>
        </fnt>;
      </ekz>
      <ekz>
        <ind>entreprena <tld/>o</ind>
        <fnt>
          
          <aut>Josef Mendl</aut>
          <vrk><url ref="&Monato;/2004/008326.html">Kien evoluas la mondo?</url></vrk>
          <bib>Monato</bib>
          <lok>2004</lok>
        </fnt>;
        <trd lng="de">Unternehmenssteuer</trd>
      </ekz>
      <ekz>
        solidar-<tld/>o
        <fnt>
         
          <aut>Stefan Maul</aut>
          <vrk><url ref="&Monato;/2004/008450.html">Ne nur protesto</url></vrk>
           <bib>Monato</bib>
          <lok>2004</lok>
        </fnt>;
        <trd lng="de">Solidarit&auml;tszuschlag</trd>
      </ekz>
      <ekz>
        <ind>sang<tld/>o</ind>
        <klr>(deviga militservo)</klr>;
        <trd lng="de">Blutsteuer</trd>
        <trd lng="fr">imp&ocirc;t du sang</trd>
        <trd lng="hu">k&ouml;telez&odblac; <ind>katonai</ind>
          szolg&aacute;lat</trd>
        <trd lng="nl">dienstplicht<klr tip="ind"> (in tijde van
            oorlog)</klr></trd>
        <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_g; &c_k;&c_r;&c_o;&c_v;&c_mol;&c_ju;</trd>
        <trd lng="sk">krvn&aacute; da&ncaron;</trd>
        <trd lng="cs">krvav&aacute; da&ncaron;</trd>
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="akciz.0o">akcizo</ref>,
      <ref cel="pag.de0o">depago</ref>,
      <ref cel="dogan.0o">dogano</ref>,
      <ref cel="tribut.0o">tributo</ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_a;&c_t;&c_a;&c_k;</trd>,
    <trd>&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_z;&c_b;&c_o;&c_r; <klr>(&c_f;&c_ib;&c_s;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;)</klr></trd>,
    <trd>&c_p;&c_o;&c_sh;&c_l;&c_ib;&c_n;&c_a;</trd>,
    <trd>&c_m;&c_y;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="bo">&#xF41;&#xFB2;&#xF63;&#xF0B;</trd>
  <trdgrp lng="cs">
    <trd>da&ncaron;</trd>,
    <trd>n&ecaron;koho)</trd>,
    <trd>poplatek <klr>( z n&ecaron;&ccaron;eho)</klr></trd>,
    <trd>zdan&ecaron;n&iacute; (n&ecaron;&ccaron;eho</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Abgabe</trd>,
    <trd>Steuer</trd>,
    <trd>Zoll</trd>
  </trdgrp>
  <trd lng="en">tax</trd>
  <trd lng="es">impuesto</trd>
  <trd lng="fr">imp&ocirc;t</trd>
  <trdgrp lng="hu">
    <trd>ad&oacute;</trd>,
    <trd>illet&eacute;k</trd>
  </trdgrp>
  <trd lng="id">pajak</trd>
  <trdgrp lng="ja">
    <trd>&#x7A0E;  <pr>&#x305C;&#x3044;</pr></trd>,
    <trd>&#x7A0E;&#x91D1;  <pr>&#x305C;&#x3044;&#x304D;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">belasting</trd>
  <trd lng="pl">podatek</trd>
  <trd lng="pt">imposto<klr tip="amb"> (economia, com&eacute;rcio)</klr>
  </trd>
  <trdgrp lng="ru">
    <trd>&c_n;&c_a;&c_l;&c_o;&c_g;</trd>,
    <trd>&c_p;&c_o;&c_d;&c_a;&c_t;&c_mol;</trd>,
    <trd>&c_d;&c_a;&c_n;&c_mol;</trd>,
    <trd>&c_p;&c_o;&c_sh;&c_l;&c_i;&c_n;&c_a;</trd>
  </trdgrp>
  <trdgrp lng="sk">
    <trd>da&ncaron;</trd>,
    <trd>poplatok</trd>
  </trdgrp>
  <trdgrp lng="uk">
    <trd>&#x43F;&#x43E;&c_d;&c_a;&c_t;&#x43E;&#x43A; &#x43D;&c_a; &c_d;&#x43E;&c_d;&c_a;&#x43D;&c_u; &c_v;&c_a;&c_r;&c_t;&c_ib;&c_s;&c_t;&#x44C;</trd>,
    <trd>&#x41F;&c_D;&c_V;</trd>
  </trdgrp>
  <trdgrp lng="zh">
    <trd>&#x7A05;&#x8CA0; <pr>shu&#xEC;f&#xF9;</pr></trd>,
    <trd>&#x7A05;&#x91D1; <pr>shu&#xEC;j&#x12B;n</pr></trd>,
    <trd>&#x6D77;&#x5173; <pr>h&#x1CE;igu&#x101;n</pr></trd>,
    <trd>&#x7A05; <pr>shu&#xEC;</pr></trd>,
    <trd>&#x6D77;&#x5173;&#x7A0E;&#x5B98; <pr>h&#x1CE;igu&#x101;nshu&#xEC;gu&#x101;n</pr></trd>,
    <trd>&#x4E00;&#x540B;&#x5BBD; <pr>y&#x12B;c&#xF9;nku&#x101;n</pr></trd>,
    <trd>&#x4E00;&#x540B;&#x9AD8; <pr>y&#x12B;c&#xF9;ng&#x101;o</pr></trd>,
    <trd>&#x4E00;&#x82F1;&#x5BF8;&#x5BBD; <pr>y&#x12B;y&#x12B;ngc&#xF9;nku&#x101;n</pr></trd>,
    <trd>&#x4E00;&#x82F1;&#x5BF8;&#x9AD8; <pr>y&#x12B;y&#x12B;ngc&#xF9;ng&#x101;o</pr></trd>
  </trdgrp>
</drv>

<drv mrk="impost.0a">
  <kap><tld/>a</kap>
  <snc mrk="impost.0a.EKON">
    <dif>
      Rilata al <tld/>oj:
      <ekz>
        donacantoj al la asocio &gcirc;uas
        <ind><tld/>an avanta&gcirc;on</ind>
        <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_v;&c_u;&c_ju; &c_l;&c_mol;&c_g;&c_o;&c_t;&c_u;</trd>
        <fnt>
          <aut>K. Schl&uuml;ter</aut>:
          <vrk>Kulturtago de EMAS</vrk>.
          <bib>LOdE</bib>, <lok>2003:12 (110)</lok>
        </fnt>;
      </ekz>
      <ekz>
        &ccirc;u venos fino de la <tld/>aj paradizoj?
        <fnt><vrk><url ref="&Monato;/2000/007128.html">&Ccirc;u venos fino de la impostaj
        paradizoj?</url></vrk>
        <bib>Monato</bib>
        <lok>2000/10, p. 10</lok></fnt>;
      </ekz>
      <ekz>
        certigi, ke la <ind><tld/>a pezo</ind> distribui&gcirc;os juste
        <trd lng="nl">belastingdruk</trd>
        <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_v;&c_a;&c_ja; &c_n;&c_a;&c_g;&c_r;&c_u;&c_z;&c_k;&c_a;</trd>
        <fnt><vrk><url ref="&Monato;/2000/007128.html">&Ccirc;u venos fino de la impostaj
        paradizoj?</url></vrk>
        <bib>Monato</bib>
        <lok>2000/10, p. 10</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y;</trd>,
    <trd>&c_f;&c_ib;&c_s;&c_k;&c_a;&c_l;&c_mol;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>tax<klr> (attr.)</klr></trd>,
    <trd>fiscal</trd>
  </trdgrp>
  <trd lng="fr">fiscal</trd>
  <trd lng="nl">fiscaal</trd>
  <trd lng="ru">&c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_v;&c_y;&c_j;</trd>
</drv>

<drv mrk="impost.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <fnt><bib>CCT</bib> </fnt>
  <fnt><bib>IKEV</bib></fnt>
  <snc mrk="impost.0i.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      Le&gcirc;e meti <tld/>on pri iu a&ubreve; io:
      <ekz>
        <tld/>i vinon, alkohola&jcirc;ojn;
      </ekz>
      <ekz>
        <tld/>i fortan depagon al provinco, lando;
      </ekz>
      <ekz>
        en Britio <nom>Thomas Paine</nom> proponis detalan planon por <tld/>i posedantojn por pagi
        por la bezonoj de la senhavuloj
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=4075">Socialismo</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        komunumoj havas rajton <tld/>i siajn lo&gcirc;antojn (kaj entreprenojn), sed en multaj komunumoj
        la 
        plej signifa financa fonto estas subvencioj de la &scirc;tato
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=140930">Komunumo (Finnlando)</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_a;&c_b;&c_k;&c_l;&c_a;&c_d;&c_a;&c_c;&c_mol; <ind>&c_p;&c_a;&c_d;&c_a;&c_t;&c_k;&c_a;&c_m;</ind></trd>
  <trdgrp lng="cs">
    <trd>zdanit</trd>,
    <trd>zpoplatnit</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>besteuern</trd>,
    <trd>Steuer <ind>erheben</ind></trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>charge a tax</trd>,
    <trd>levy a <ind>tax</ind></trd>,
    <trd>tax</trd>,
    <trd>assess</trd>
  </trdgrp>
  <trd lng="es">gravar con <ind>impuesto</ind></trd>
  <trdgrp lng="fr">
    <trd>imposer<klr tip="ind"> (fin.)</klr></trd>,
    <trd>taxer</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>megad&oacute;ztat</trd>,
    <trd>ad&oacute;t vet ki</trd>
  </trdgrp>
  <trd lng="ja">&#x8AB2;&#x7A0E;&#x3059;&#x308B;  <pr>&#x304B;&#x305C;&#x3044;&#x3059;&#x308B;</pr></trd>
  <trd lng="nl">belasten<klr tip="ind"> (belasting
  heffen)</klr></trd>
  <trdgrp lng="pl">
    <trd>opodatkowa&cacute;</trd>,
    <trd>nak&lstroke;ada&cacute; <ind>podatek</ind></trd>,
    <trd>ok&lstroke;ada&cacute; <ind>podatkiem</ind></trd>
  </trdgrp>
  <trd lng="pt">onerar com <ind>imposto</ind></trd>
  <trd lng="ru">&c_o;&c_b;&c_l;&c_a;&c_g;&c_a;&c_t;&c_mol;
    <ind>&c_n;&c_a;&c_l;&c_o;&c_g;&c_o;&c_m;</ind></trd>
  <trd lng="sk">zdani&tcaron;</trd>
  <trd lng="uk">&#x43E;&c_b;&#x43A;&#x43B;&c_a;&c_d;&c_a;&c_t;&c_i; &#x43F;&#x43E;&c_d;&c_a;&c_t;&#x43A;&#x43E;&#x43C;</trd>
  <trdgrp lng="zh">
    <trd>&#x62BD;&#x7A05; <pr>ch&#x14D;ushu&#xEC;</pr></trd>,
    <trd>&#x6350; <pr>ju&#x101;n</pr></trd>,
    <trd>&#x5F81;&#x7A0E; <pr>zh&#x113;ngshu&#xEC;</pr></trd>,
    <trd>&#x5F81;&#x7A05; <pr>zh&#x113;ngshu&#xEC;</pr></trd>
  </trdgrp>
</drv>

<drv mrk="impost.0ejo">
  <kap>
    <tld/>ejo
</kap>
  <snc mrk="impost.0ejo.loko">
    <dif>
      Loko, oficejo, kie oni pagas <tld/>ojn:
      <ekz>
        li eliris, kaj vidis <tld/>iston, nomatan Levi, sidantan &ccirc;e la <tld/>ejo
        <fnt>
          <bib>NT</bib>
          <lok>Luko 5:27</lok>
        </fnt>;
      </ekz>
      <ekz>
        Turo de <nom>Belem</nom> <klr>[...]</klr> utilis kiel <tld/>ejo por eniri la urbon
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=217874">Turo de Belem</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_p;&c_a;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_y; &c_v;&c_o;&c_r;&c_g;&c_a;&c_n;</trd>,
    <trd>&c_p;&c_a;&c_d;&c_a;&c_t;&c_k;&c_o;&c_v;&c_a;&c_ja; &c_ib;&c_n;&c_s;&c_p;&c_e;&c_k;&c_c;&c_y;&c_ja;</trd>
  </trdgrp>
