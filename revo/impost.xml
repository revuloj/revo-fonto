  <trdgrp lng="en">
     <trd>anchorage</trd>,
     <trd>harbour dues</trd>,
     <trd>port dues</trd>,
     <trd>port <ind>charges</ind></trd>
  </trdgrp>
</drv>

<drv mrk="impost.hund0o">
  <kap>hund<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
