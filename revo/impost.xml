  <snc>
    <dif>
     Imposto pagenda pro la posedo de hundo.
    </dif>
  </snc>
  <trd lng="de">Hundesteuer</trd>
</drv>

<drv mrk="impost.kap0o">
  <kap>kap<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>BEE</bib></fnt>
  <snc>
    <dif>
    </dif>
  </snc>
  <trdgrp lng="de">
     <trd>Kopfsteuer</trd>,
     <trd>B&uuml;rgersteuer</trd>,
     <trd>Wahlsteuer</trd>
  </trdgrp>
</drv>

<drv mrk="impost.lim0o">
  <kap>lim<tld/>o</kap>
  <uzo tip="fak">EKON</uzo>
  <fnt><bib>BEE</bib></fnt>
  <fnt><bib>PIV1</bib></fnt>
  <snc>
    <ref tip="dif" cel="dogan.0o.imposto">dogano <sncref/></ref>
