  <fnt><bib>IKEV</bib></fnt>
  <snc mrk="impost.aldonvalora0o.EKON">
    <uzo tip="fak">EKON</uzo>
    <dif>
      &Gcirc;enerala vendo-<tld/>o nete kalkulata surbaze de la
      valorkreska&jcirc;o en &ccirc;iu unuopa produkto- a&ubreve;
      komerco-nivelo:
      <ekz>
        en 1986 &gcirc;i enkondukis aldonvaloran <tld/>on je &ccirc;iuj
        enlandaj konsumoj
        <fnt>
          <aut>D. Rogers</aut>:
          <vrk>
            <url ref="&Monato;/005411.html">Ekonomia miraklo a&ubreve;
              malsano?</url>
          </vrk>,
          <bib>Monato</bib>,
          <lok>1997:02</lok>.
        </fnt>
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="de">
    <trd>Mehrwertsteuer</trd>,
    <trd>MWSt</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>value added <ind>tax</ind></trd>,
    <trd>VAT</trd>
  </trdgrp>
  <trdgrp lng="es">
    <trd>impuesto sobre el valor a&ntilde;adido</trd>,
    <trd>impuesto al valor agregado</trd>,
    <trd>IVA</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>taxe sur la valeur ajout&eacute;e</trd>,
    <trd>TVA</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>&eacute;rt&eacute;kt&ouml;bbletad&oacute;</trd>,
    <trd>&aacute;ltal&aacute;nos forgalmi <ind>ad&oacute;</ind></trd>,
    <trd>&aacute;fa</trd>
  </trdgrp>
  <trd lng="id"><ind>pajak</ind> pertambahan nilai</trd>
  <trdgrp lng="nl">
    <trd>belasting over de toegevoegde waarde</trd>,
    <trd>BTW</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>podatek od warto&sacute;ci dodanej</trd>,
    <trd>VAT</trd>
  </trdgrp>
  <trdgrp lng="pt">
    <trd>imposto sobre circulac&atilde;o de mercadorias</trd>,
    <trd>ICM</trd>
  </trdgrp>
  <trdgrp lng="ru">
