<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
   <rad>livid</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="livid.0o">
   <kap><tld/>o</kap>
   <snc mrk="livid.0o.MED">
     <uzo tip="fak">MED</uzo>
     <dif>
       Viole blua ha&ubreve;ta makulo pro interna sangado:
       <ekz>
         e&ccirc; se mi volus forgesi pri la vergoj ricevitaj <klr>...</klr>,
         ilin memorigus al mi la <tld/>oj <klr>...</klr> sur miaj ripoj
         <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 3a, p.
         411a</lok></fnt>
       </ekz>
     </dif>
   </snc>
   <trdgrp lng="fr">
     <trd>bleu<klr> (h&eacute;matome)</klr></trd>,
     <trd>h&eacute;matome</trd>,
     <trd>liv&eacute;do</trd>
   </trdgrp>
   <trd lng="hu">livedo <klr tip="amb">(k&eacute;kesv&ouml;r&ouml;s b&odblac;rfolt)</klr></trd>
   <trd lng="pt">hematoma</trd>
   <trdgrp lng="ru">
     <trd>&c_l;&c_i;&c_v;&c_je;&c_d;&c_o;</trd>,
     <trd>&c_s;&c_i;&c_n;&c_ju;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
   </trdgrp>
</drv>

<drv mrk="livid.0a">
   <kap><tld/>a</kap>
   <snc mrk="livid.0a.MED">
     <dif>
       Pale blua a&ubreve; violkolora, simile al <tld/>o.
     </dif>
   </snc>
   <trd lng="fr">livide<klr> (violac&eacute;)</klr></trd>
  <trdgrp lng="hu">
    <trd>szederjes</trd>,
    <trd>k&eacute;kesv&ouml;r&ouml;s</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_n;&c_ju;&c_sh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_je;&c_r;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;-&c_s;&c_i;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

