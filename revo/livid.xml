<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: livid.xml,v 1.15 2024/03/11 09:22:20 revo Exp $">
<kap>
   <rad>livid</rad>/o <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="livid.0o">
   <kap><tld/>o</kap>
   <snc mrk="livid.0o.MED">
     <uzo tip="fak">MED</uzo>
     <dif>
       Viole bluaj ha&ubreve;taj makuloj pro perturbo de la sangocirkulado:
       <ekz>
         e&ccirc; se mi volus forgesi pri la vergoj ricevitaj <klr>[&#8230;]</klr>,
         ilin memorigus al mi la <tld/>oj <klr>[&#8230;]</klr> sur miaj ripoj
         <fnt><bib>DonKihxoto</bib>, <lok>dua parto, &ccirc;apitro 3a, p.
         411a</lok></fnt>
       </ekz>
     </dif>
     <refgrp tip="vid">
       <ref cel="sang.0umo">sangumo</ref>,
       <ref cel="ehximoz.0o">e&hcirc;imozo</ref>
     </refgrp>
   </snc>
   <trd lng="be">&c_s;&c_ib;&c_n;&c_ju;&c_sh;&c_n;&c_a;&c_s;&c_c;&c_mol; <klr>(&c_m;&c_e;&c_d;.)</klr></trd>
   <trdgrp lng="de">
     <trd>Livedo</trd>,
     <trd>Marmorierung <klr>(Haut)</klr></trd>
   </trdgrp>
   <trdgrp lng="fr">
     <trd>liv&eacute;do</trd>,
     <trd>livedo</trd>,
     <trd>marbrure</trd>
   </trdgrp>
   <trd lng="hu">livedo <klr tip="amb">(k&eacute;kesv&ouml;r&ouml;s
   b&odblac;rfolt)</klr></trd>
   <trdgrp lng="pl">
    <trd>martwica sk&oacute;ry</trd>,
    <trd>siniak</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>necroza pielii</trd>,
    <trd>v&acirc;n&abreve;taie</trd>
  </trdgrp>
   <trdgrp lng="ru">
     <trd>&c_l;&c_i;&c_v;&c_je;&c_d;&c_o;</trd>,
     <trd>&c_s;&c_i;&c_n;&c_ju;&c_sh;&c_n;&c_o;&c_s;&c_t;&c_mol;</trd>
   </trdgrp>
</drv>

<drv mrk="livid.0a">
   <kap><tld/>a</kap>
   <snc mrk="livid.0a.MED">
     <dif>
       Pale blua a&ubreve; violkolora, simile al <tld/>o.
     </dif>
   </snc>
   <trd lng="be">&c_s;&c_ib;&c_n;&c_ju;&c_sh;&c_n;&c_y;</trd>
  <trd lng="cs">lividn&iacute;</trd>
  <trd lng="de">marmoriert <klr>(Haut)</klr></trd>
  <trd lng="fr">livide</trd>
  <trdgrp lng="hu">
    <trd>szederjes</trd>,
    <trd>k&eacute;kesv&ouml;r&ouml;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>lijkbleek</trd>,
    <trd>vaal</trd>
  </trdgrp>
  <trdgrp lng="pl">
    <trd>bladoniebieski</trd>,
    <trd>purpurowy</trd>
  </trdgrp>
  <trdgrp lng="ro">
    <trd>albastru pal</trd>,
    <trd>purpuriu</trd>
  </trdgrp>
  <trdgrp lng="ru">
    <trd>&c_s;&c_i;&c_n;&c_ju;&c_sh;&c_n;&c_y;&c_j;</trd>,
    <trd>&c_m;&c_je;&c_r;&c_t;&c_v;&c_je;&c_n;&c_n;&c_o;-&c_s;&c_i;&c_n;&c_i;&c_j;</trd>
  </trdgrp>
  <trd lng="sk">lividn&yacute;</trd>
  <trdgrp lng="uk">
    <trd>&c_M;&c_je;&c_r;&c_t;&c_v;&c_o;&c_t;&c_n;&c_o;-&c_b;&c_l;&c_ib;&c_d;&c_i;&c_j;</trd>,
    <trd>&c_p;&c_o;&c_s;&c_i;&c_n;&c_ib;&c_l;&c_i;&c_j;</trd>
  </trdgrp>
</drv>
</art>

