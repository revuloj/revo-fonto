  <trd lng="nl">herleidbaar</trd>
  <trd lng="uk">&c_s;&c_k;&c_o;&c_r;&c_o;&c_t;&c_n;&c_i;&c_j;</trd>
  <trdgrp lng="zh">
    <trd>&#27425;&#24207;&#20851;&#31995; <pr>c&igrave;x&ugrave;gu&amacron;nxi</pr></trd>,
    <trd>&#27425;&#24207;&#38364;&#31995; <pr>c&igrave;x&ugrave;gu&amacron;nxi</pr></trd>
