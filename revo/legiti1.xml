<kap>
  <rad>legitimaci</rad>/o
</kap>
<drv mrk="legiti1.0o">
  <kap>
    <tld/>o
  </kap>
  <snc>
  <uzo tip="fak">JUR</uzo>
    <dif>
      Legitimado a&ubreve; <ref tip="dif" cel="legiti.0ajxo">legitima&jcirc;o,
      legitimilo</ref>:
      <ekz>
        <nom>Czes&lstroke;aw Mi&lstroke;osz</nom> <klr>[&#x2026;]</klr> ekpartoprenis en Asocio
        de Polaj Verkistoj (li
        ricevis la <tld/>on de la numero 1)
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=214895">Czes&lstroke;aw Mi&lstroke;osz</url></lok>
        </fnt>;
      </ekz>
      <ekz>
        kontra&ubreve;uloj de <nac>Nakba</nac> asertas ke la Tago de
        <nac>Nakba</nac> estas &leftquot;<tld/>o de ilia (araba)
        rajto je ribelo a&ubreve; a&ubreve;tonomeco, kaj anka&ubreve; faktoro, kiu kreas
        malamon koncerne
        hebreojn&rightquot;
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=343532">La Nakba Tago</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_p;&c_a;&c_s;&c_mol;&c_v;&c_je;&c_d;&c_ch;&c_a;&c_n;&c_mol;&c_n;&c_je;</trd>
  <trdgrp lng="de">
     <trd>Legitimierung</trd>,
     <trd>Legitimation</trd>,
     <trd>Ausweis</trd>,
     <trd>Beglaubigung</trd>
  </trdgrp>
  <trd lng="fr">l&eacute;gitimation</trd>
  <trdgrp lng="hu">
     <trd>legitim&aacute;ci&oacute;</trd>,
     <trd>igazol&aacute;s</trd>,
     <trd>jogos&iacute;t&aacute;s</trd>
  </trdgrp>  
  <trd lng="nl">legitimatie</trd>
  <trd lng="pl">legitymacja</trd>
  <trd lng="pt">legitima&ccedil;&atilde;o</trd>
  <trdgrp lng="ro">
    <trd>document de identitate</trd>,
    <trd>card</trd>
  </trdgrp>
  <trd lng="ru">&c_u;&c_d;&c_o;&c_s;&c_t;&c_o;&c_v;&c_je;&c_r;&c_je;&c_n;&c_i;&c_je;</trd>
  <trdgrp lng="uk">
    <trd>&#x43B;&#x435;&#x491;&#x456;&#x442;&#x438;&#x43C;&#x430;&#x446;&#x456;&#x44F;</trd>,
    <trd>&#x443;&#x437;&#x430;&#x43A;&#x43E;&#x43D;&#x44E;&#x44E;&#x447;&#x438;&#x439; &#x434;&#x43E;&#x43A;&#x443;&#x43C;&#x435;&#x43D;&#x442;</trd>
  </trdgrp>
</drv>
