<art mrk="$Id: kviz.xml,v 1.10 2018/11/01 21:10:14 revo Exp $">
<kap>
  <rad>kviz</rad>/o <fnt><bib>NePiv</bib></fnt>
</kap>
<drv mrk="kviz.0o">
  <kap><tld/>o</kap>
  <snc mrk="kviz.0o.KOMUNE">
    <uzo tip="fak">PSI</uzo>
    <uzo tip="fak">SPO</uzo>
    <dif>
      <ref tip="super" cel="demand.0aro.KOMUNE">Demandaro</ref>
      elprovanta
      la sciaron de respondantoj:
      <ekz>
        jen malgranda <tld/>o, mi priskribos realan situacion, kaj vi
        devas diveni pri kio temas
        <fnt>
        <aut>W. Slootmans</aut>,
        <vrk>kvizeto</vrk>,
        <lok>soc.culture.esperanto, 1992-06-17</lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trdgrp lng="cs">
    <trd>kviz</trd>,
    <trd>kv&iacute;z</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Quiz</trd>
  </trdgrp>
  <trd lng="es">cuestionario</trd>
  <trdgrp lng="fr">
    <trd>questionnaire<klr> (quiz)</klr></trd>,
    <trd>quiz</trd>
  </trdgrp>
  <trdgrp lng="hu">
    <trd>vet&eacute;lked&odblac;</trd>,
    <trd>tud&aacute;spr&oacute;ba</trd>,
    <trd>kv&iacute;z</trd>
  </trdgrp>
  <trd lng="nl">quiz</trd>
  <trdgrp lng="pl">
    <trd>quiz</trd>,
    <trd>kwiz</trd>
  </trdgrp>   
  <trd lng="pt">question&aacute;rio</trd>
  <trd lng="ro">chestionare</trd>
  <trd lng="ru">&c_v;&c_i;&c_k;&c_t;&c_o;&c_r;&c_i;&c_n;&c_a;</trd>
  <trd lng="sk">kv&iacute;z</trd>
</drv>
