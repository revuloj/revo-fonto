<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>3</ofc>
  <rad>stenograf</rad>/i
</kap>
<drv mrk="stenog.0i">
  <kap><tld/>i</kap>
  <gra><vspec>tr</vspec></gra>
  <snc>
    <dif>
      Uzi specialajn konvenciajn tre simplajn signojn por skribi same
      rapide, kiel oni parolas.
    </dif>
  </snc>
  <trd lng="de">stenografieren</trd>
  <trd lng="nl">stenograferen</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_r;&c_o;&c_v;&c_a;&c_t;&c_mol;</trd>
  <trd lng="sv">stenografera</trd>
</drv>

<drv mrk="stenog.0o">
  <kap><tld/>o</kap>
  <snc>
    <dif>
      Stenografiisto.
    </dif>
  </snc>
  <trd lng="de">Stenograf</trd>
  <trd lng="nl">stenograaf</trd>
  <trd lng="ru">&c_s;&c_t;&c_je;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;</trd>
</drv>

<drv mrk="stenog.0ado">
  <kap><tld/>ado</kap>
  <snc>
    <dif>
      Ago <tld/>i a&ubreve; <tld/>arto.
    </dif>
  </snc>
  <trd lng="de">Stenografie</trd>
  <trd lng="nl">stenografie</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_t;&c_je;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_ja;</trd>,
    <trd>&c_s;&c_t;&c_je;&c_n;&c_o;&c_g;&c_r;&c_a;&c_f;&c_i;&c_r;&c_o;&c_v;&c_a;&c_n;&c_i;&c_je;</trd>
  </trdgrp>
  <trd lng="sv">stenografi</trd>
</drv>
</art>
