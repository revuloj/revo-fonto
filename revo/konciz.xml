<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <ofc>2</ofc>
  <rad>konciz</rad>/a
</kap>
<drv mrk="konciz.0a">
  <kap><tld/>a</kap>
  <snc>
    <dif>
      Esprimanta la ideojn per plej malmultaj vortoj:
      <ekz>
        <tld/>a stilo, verkisto, lingvo, raporto.
      </ekz>
    </dif>
    <refgrp tip="vid">
      <ref cel="lakon.0a">lakona</ref>.
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_o;&c_t;&c_k;&c_ib;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_l;&c_y;</trd>,
    <trd>&c_l;&c_ja;&c_k;&c_a;&c_n;&c_ib;&c_ch;&c_n;&c_y;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>kurz</trd>,
    <trd>gedr&auml;ngt</trd>,
    <trd>b&uuml;ndig</trd>,
    <trd>knapp</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>concise</trd>,
    <trd>brief and comprehensive</trd>,
    <trd>laconic</trd>,
    <trd>pithy</trd>,
    <trd>succinct</trd>,
    <trd>terse</trd>
  </trdgrp>
  <trdgrp lng="fr">
    <trd>concis</trd>,
    <trd>bref<klr tip="ind"> (condens&eacute;)</klr></trd>,
    <trd>condens&eacute;<klr tip="ind"> (succinct)</klr></trd>,
    <trd>ramass&eacute;<klr tip="ind"> (succinct)</klr></trd>,
    <trd>sobre<klr tip="ind"> (succinct)</klr></trd>,
    <trd>succinct</trd>
  </trdgrp>
  <trd lng="nl">beknopt</trd>
  <trdgrp lng="ru">
    <trd>&c_s;&c_zh;&c_a;&c_t;&c_y;&c_j;</trd>,
    <trd>&c_k;&c_r;&c_a;&c_t;&c_k;&c_i;&c_j;</trd>,
    <trd>&c_l;&c_a;&c_k;&c_o;&c_n;&c_i;&c_ch;&c_n;&c_y;&c_j;</trd>
  </trdgrp>
</drv>

<drv mrk="konciz.0eco">
  <kap><tld/>eco</kap>
  <snc>
    <dif>
      Eco, de io <tld/>a.
    </dif>
  </snc>
  <trdgrp lng="be">
    <trd>&c_k;&c_a;&c_r;&c_o;&c_t;&c_k;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>,
    <trd>&c_s;&c_mol;&c_c;&c_ib;&c_s;&c_l;&c_a;&c_s;&c_mol;&c_c;&c_mol;</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>K&uuml;rze</trd>,
    <trd>B&uuml;ndigkeit</trd>,
    <trd>Knappheit</trd>
  </trdgrp>
  <trd lng="en">conciseness</trd>
  <trdgrp lng="fr">
    <trd>concision</trd>,
    <trd>bri&egrave;vet&eacute;<klr tip="ind"> (concision)</klr></trd>,
    <trd>sobri&eacute;t&eacute;<klr tip="ind"> (concision)</klr></trd>,
  </trdgrp>
  <trd lng="nl">beknoptheid</trd>
  <trd lng="ru">&c_k;&c_r;&c_a;&c_t;&c_k;&c_o;&c_s;&c_t;&c_mol;</trd>
</drv>
</art>
