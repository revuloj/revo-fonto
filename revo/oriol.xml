<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
<rad>oriol</rad>/o
</kap>
<drv mrk="oriol.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Genro el familio <ref tip="super" cel="oriol.0edoj"><tld/>edoj</ref>
      (<trd lng="la">Oriolus</trd>), kun flava nigra plumaro kaj kun agrabla   
      kanto.
    </dif>
  </snc>
  <trd lng="cs">&zcaron;luva</trd>
  <trd lng="de">Pirol</trd>
  <trd lng="en">oriole</trd>
  <trd lng="hu">s&aacute;rgarig&oacute;</trd>
  <trd lng="pl">wilga</trd>
  <trd lng="ru">&c_i;&c_v;&c_o;&c_l;&c_g;&c_a;</trd>
</drv>
<drv mrk="oriol.0edoj">
  <kap><tld/>edoj</kap>
  <uzo tip="fak">ZOO</uzo>
  <snc>
    <dif>
      Familio el la ordo <ref tip="super" cel="paser.0oformaj">paseroformaj
      birdoj</ref> (<trd lng="la">Oriolidae</trd>).
    </dif>
  </snc>
  <trd lng="de">Pirole <klr tip="ind">(Familie)</klr></trd>
  <trd lng="en">orioles <klr tip="ind">(family)</klr></trd>
  <trd lng="fr">loriots</trd>
</drv>
</art>
