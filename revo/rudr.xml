<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>rudr</rad>/o <fnt><bib>SPV</bib></fnt>
</kap>
<drv mrk="rudr.0o">
  <kap><tld/>o</kap>
  <snc mrk="rudr.0o.MAR">
    <uzo tip="fak">MAR</uzo>
    <dif>
      <ref tip="super" cel="stir.0ilo">Stirilo</ref> de <ref tip="malprt"
      cel="sxip.0o">&scirc;ipo</ref>, kies direktilo movas enakvan platon:
      <ekz>
        en la alia &scirc;ipo oni faris abruptan <tld/>omanovron
        <fnt><bib>DonKihxoto</bib>, <lok>parto 1a, &ccirc;apitro 41a, p.
        317a</lok></fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="de">Ruder</trd>
  <trd lng="fr">gouvernail</trd>
  <trdgrp lng="hu">
    <trd>lap&aacute;tkorm&aacute;ny</trd>,
    <trd>evez&odblac;korm&aacute;ny</trd>
  </trdgrp>
  <trd lng="pt">leme</trd>
  <trdgrp lng="ru">
    <trd>&c_r;&c_u;&c_l;&c_mol;<klr tip="amb"> (&c_u; &c_l;&c_o;&c_d;&c_k;&c_i;, &c_k;&c_o;&c_r;&c_a;&c_b;&c_l;&c_ja;)</klr></trd>,
    <trd>&c_k;&c_o;&c_r;&c_m;&c_i;&c_l;&c_o;</trd>
  </trdgrp>
</drv>
</art>

