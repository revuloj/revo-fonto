<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">
<vortaro>
<art mrk="$Id: anuitat.xml,v 1.16 2021/03/02 06:50:09 revo Exp $">
<kap>
  <rad>anuitat</rad>/o
</kap>
<drv mrk="anuitat.0o">
  <kap><tld/>o</kap>
  <fnt><bib>PIV1</bib></fnt>
  <fnt><bib>IKEV</bib></fnt>
  <snc mrk="anuitat.0o.EKON">
  <uzo tip="fak">EKON</uzo>
    <dif>
      Fiksita <ref tip="super" cel="sum.mon0o">monsumo</ref> kun 
      <ref tip="prt" cel="rent.0o">rento</ref> pagenda post regulaj intervaloj, ekz-e
      &ccirc;iujare, por <ref cel="amorti.0i">amortizi</ref> &scirc;uldon:
      <ekz>
        en 1522, li ricevis papan <tld/>on pro sia dekanejo en Kolonjo
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=502949">Paul Ziegler</url></lok>
        </fnt>.
      </ekz>
    </dif>
    <refgrp tip="vid">
       <ref cel="kvot.0o">kvoto</ref>,
       <ref cel="procen.0o.jara">procento<sncref/></ref>
    </refgrp>
  </snc>
  <trdgrp lng="be">
    <trd>&c_a;&c_n;&c_u;&c_ib;&c_t;&c_e;&c_t;</trd>,
    <trd>&c_sh;&c_t;&c_o;&c_g;&c_a;&c_d;&c_o;&c_v;&c_a;&c_ja; &c_v;&c_y;&c_p;&c_l;&c_a;&c_t;&c_a;</trd>
  </trdgrp>
  <trd lng="ca">anualitat</trd>
  <trdgrp lng="cs">
    <trd>anuita</trd>,
    <trd>spl&aacute;tka</trd>
  </trdgrp>
  <trdgrp lng="de">
    <trd>Jahresrate</trd>,
    <trd>Annuit&auml;t</trd>
  </trdgrp>
  <trdgrp lng="en">
    <trd>annual instalment</trd>,
    <trd>annuity</trd>
  </trdgrp>
  <trd lng="es">anualidad</trd>
  <trd lng="fr">annuit&eacute;</trd>
  <trd lng="he">&tav;&shin;&lamed;&vav;&fmem; (&mem;&tav;&vav;&fkaf;)</trd>
  <trdgrp lng="hu">
    <trd>&eacute;vi <ind>t&ouml;rleszt&eacute;s</ind></trd>,
    <trd>annuit&aacute;s</trd>
  </trdgrp>
  <trdgrp lng="nl">
    <trd>annu&iuml;teit</trd>,
    <trd>jaarlijkse <ind>uitkering</ind></trd>,
    <trd>jaarlijkse <ind>afbetaling</ind></trd>
  </trdgrp>
  <trd lng="ru">&c_je;&c_zh;&c_je;&c_g;&c_o;&c_d;&c_n;&c_a;&c_ja; &c_r;&c_je;&c_n;&c_t;&c_a;</trd>
  <trdgrp lng="sk">
    <trd>anuita</trd>,
    <trd>spl&aacute;tka</trd>
  </trdgrp>
</drv>
</art>
