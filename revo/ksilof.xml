<art mrk="$Id: ksilof.xml,v 1.25 2023/12/03 09:59:02 revo Exp $">
<kap>
  <ofc>2</ofc>
  <rad>ksilofon</rad>/o
</kap>
<drv mrk="ksilof.0o">
  <kap><tld/>o</kap>
  <snc mrk="ksilof.0o.MUZ">
    <uzo tip="fak">MUZ</uzo>
    <dif>
      <ref tip="lst" cel="instru2.frap0o" lst="voko:frapinstrumentoj">Frapinstrumento</ref>
      konsistanta el paralelaj lignaj tubetoj de neegala longeco, kiujn
      oni frapas per du marteletoj:
      <ekz>
        <nom>Jason Webley</nom> <klr>[&#x2026;]</klr> blekas plivole ol kantas akompanita de &ccirc;u akordiono,
        &ccirc;u gitaro, &ccirc;u <tld/>o a&ubreve; improvizitaj instrumentoj a&ubreve; iloj kiel tiu botelo de
        vodko kiun li plenigis per tutmonddevenaj moneroj kaj skuas la&ubreve;ritme
        <fnt>
          <bib>Viki</bib>
          <lok><url ref="https://eo.wikipedia.org/?curid=263350">Jason Webley</url></lok>
        </fnt>.
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_k;&c_s;&c_ib;&c_l;&c_a;&c_f;&c_o;&c_n;</trd>
  <trd lng="cs">xylofon</trd>
  <trd lng="de">Xylophon</trd>
  <trd lng="en">xylophone</trd>
  <trd lng="fr">xylophone</trd>
  <trdgrp lng="hu">
    <trd>xilofon</trd>,
    <trd>facimbalom</trd>
  </trdgrp>
  <trd lng="id">xilofon</trd>
  <trdgrp lng="ja">
    <trd>&#x30B7;&#x30ED;&#x30DB;&#x30F3;</trd>,
    <trd>&#x6728;&#x7434;  <pr>&#x3082;&#x3063;&#x304D;&#x3093;</pr></trd>
  </trdgrp>
  <trd lng="nl">xylofoon</trd>
  <trdgrp lng="pl">
    <trd>ksylofon</trd>,
    <trd>cymba&lstroke;y</trd>
  </trdgrp>
  <trd lng="pt">xilofone</trd>
  <trd lng="ru">&c_k;&c_s;&c_i;&c_l;&c_o;&c_f;&c_o;&c_n;</trd>
  <trd lng="sk">xylof&oacute;n</trd>
  <trd lng="sv">xylofon</trd>
  <trd lng="uk">&#x43A;&#x441;&#x438;&#x43B;&#x43E;&#x444;&#x43E;&#x43D; (&#x43C;&#x443;&#x437;. &#x456;&#x43D;&#x441;&#x442;&#x440;&#x443;&#x43C;&#x435;&#x43D;&#x442;)</trd>
  <trd lng="zh">&#x6728;&#x7434; <pr>m&#xF9;q&#xED;n</pr></trd>
</drv>
</art>
