<?xml version="1.0"?>
<!DOCTYPE vortaro SYSTEM "../dtd/vokoxml.dtd">

<vortaro>
<art mrk="$Id$">
<kap>
  <rad>turne</rad>/i <fnt><bib>PIV1</bib></fnt>
</kap>
<drv mrk="turne.0i">
  <kap><tld/>i</kap>
  <gra><vspec>ntr</vspec></gra>
  <snc>
    <dif>
      <ref tip="super" cel="vojagx.0i">Voja&gcirc;i</ref> a&ubreve; rondvoja&gcirc;i
      de loko al loko prezentante spekta&jcirc;ojn, arta&jcirc;ojn:
      <ekz>
        unu el la Moskvaj teatroj <tld/>anta en Leningrado
        <fnt><bib>MkM</bib>, <lok>&ccirc;apitro 27a, p. 289a</lok></fnt>;
      </ekz>
    </dif>
  </snc>
  <trd lng="be">&c_r;&c_a;&c_b;&c_ib;&c_c;&c_mol; <ind>&c_t;&c_u;&c_r;&c_n;&c_e;</ind></trd>
  <trd lng="fr">faire une <ind>tourn&eacute;e</ind><klr> (spectacles)</klr></trd>
  <trd lng="ru">&c_d;&c_je;&c_l;&c_a;&c_t;&c_mol; <ind>&c_t;&c_u;&c_r;&c_n;&c_je;</ind></trd>
</drv>

<drv mrk="turne.0o">
  <kap><tld/>o</kap>
  <uzo tip="fak">TEA</uzo>
  <uzo tip="fak">MUZ</uzo>
  <snc>
    <dif>
      Voja&gcirc;o de artista grupo por prezenti spekta&jcirc;ojn en diversaj lokoj.
    </dif>
  </snc>
  <trd lng="be">&c_t;&c_u;&c_r;&c_n;&c_e;</trd>
  <trd lng="fr">tourn&eacute;e<klr> (spectacles)</klr></trd>
  <trd lng="ru">&c_t;&c_u;&c_r;&c_n;&c_je;</trd>
</drv>
</art>
